stylesheet "1.0";
output method="xml" indent="yes";

tpl [*] {
  $n = `name (.)`;
  <$n {
    for-each [@*] {
      $attr = `name (.)`;
      choose {
        when [$attr = "pid"] { }
        when [$attr = "relnr"] { }
        when [$attr = "redefnr"] { }
        when [$attr = "line"] { }
        when [$attr = "col"] { }
        when [$attr = "x"] { }
        when [$attr = "y"] { }
        when [$attr = "mizfiles"] { }
        otherwise {
          copy-of `.`;
        }
      }
    }
  }
}
