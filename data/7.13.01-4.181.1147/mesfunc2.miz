theorem
L164: (for R1 being non  empty set holds (for R6 being (SigmaField of R1) holds (for R4 being (PartFunc of R1 , ( ExtREAL )) holds (for R5 being (PartFunc of R1 , ( ExtREAL )) holds (for R13 being (Element of R6) holds ((R4 is  real-valued & R5 is  real-valued & R4 is_measurable_on R13 & R5 is_measurable_on R13) implies ( R4 + R5 ) is_measurable_on R13))))))
proof
let R1 being non  empty set;
let R6 being (SigmaField of R1);
let R4 being (PartFunc of R1 , ( ExtREAL ));
let R5 being (PartFunc of R1 , ( ExtREAL ));
let R13 being (Element of R6);
assume that
L165: (R4 is  real-valued & R5 is  real-valued)
and
L166: (R4 is_measurable_on R13 & R5 is_measurable_on R13);
L167: (for B15 being  real number holds ( R13 /\ ( less_dom (( R4 + R5 ) , ( R_EAL B15 )) ) ) in R6)
proof
let C19 being  real number;
