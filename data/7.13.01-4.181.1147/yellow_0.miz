scheme RelStrEx { F1() -> non  empty set , P1[set , set] } : (ex B1 being non  empty  strict RelStr st ((the carrier of B1) = F1() & (for B2 , B3 being (Element of B1) holds (B2 <= B3 iff P1[ B2 , B3 ]))))
proof
consider C1 being (Relation of F1()) such that L1: (for B4 , B5 being (Element of F1()) holds ([ B4 , B5 ] in C1 iff P1[ B4 , B5 ])) from RELSET_1:sch 2;
take D1 = RelStr (# F1() , C1 #);
thus L2: (the carrier of D1) = F1();
let C2 , C3 being (Element of D1);
L3: (C2 <= C3 iff [ C2 , C3 ] in C1) by ORDERS_2:def 5;
thus L4: thesis by L3 , L1;
end;
definition
let C4 being non  empty RelStr;
