begin
definition
let R1 being RealLinearSpace;
let R11 being (Subset of R1);
attr R11 is  linearly-closed
means
:L1: ((for R7 being (VECTOR of R1) holds (for R4 being (VECTOR of R1) holds ((R7 in R11 & R4 in R11) implies ( R7 + R4 ) in R11))) & (for R10 being Real holds (for R7 being (VECTOR of R1) holds (R7 in R11 implies ( R10 * R7 ) in R11))));
end;
theorem
L3: (for R1 being RealLinearSpace holds (for R11 being (Subset of R1) holds ((R11 <> ( {} ) & R11 is  linearly-closed) implies ( 0. R1 ) in R11)))
proof
let R1 being RealLinearSpace;
let R11 being (Subset of R1);
assume that
L4: R11 <> ( {} )
and
L5: R11 is  linearly-closed;
set D1 = the (Element of R11);
reconsider D2 = D1 as (Element of R1) by L4 , TARSKI:def 3;
L6: ( ( 0 ) * D2 ) in R11 by L4 , L5 , L1;
thus L7: thesis by L6 , RLVECT_1:10;
end;
theorem
L8: (for R1 being RealLinearSpace holds (for R11 being (Subset of R1) holds (R11 is  linearly-closed implies (for R7 being (VECTOR of R1) holds (R7 in R11 implies ( - R7 ) in R11)))))
proof
let R1 being RealLinearSpace;
let R11 being (Subset of R1);
assume L9: R11 is  linearly-closed;
let R7 being (VECTOR of R1);
assume L10: R7 in R11;
L11: ( ( - 1 ) * R7 ) in R11 by L10 , L9 , L1;
thus L12: thesis by L11 , RLVECT_1:16;
end;
theorem
L13: (for R1 being RealLinearSpace holds (for R11 being (Subset of R1) holds (R11 is  linearly-closed implies (for R7 being (VECTOR of R1) holds (for R4 being (VECTOR of R1) holds ((R7 in R11 & R4 in R11) implies ( R7 - R4 ) in R11))))))
proof
let R1 being RealLinearSpace;
let R11 being (Subset of R1);
assume L14: R11 is  linearly-closed;
let R7 being (VECTOR of R1);
let R4 being (VECTOR of R1);
assume that
L15: R7 in R11
and
L16: R4 in R11;
L17: ( - R4 ) in R11 by L14 , L16 , L8;
thus L18: thesis by L17 , L14 , L15 , L1;
end;
theorem
L19: (for R1 being RealLinearSpace holds { ( 0. R1 ) } is  linearly-closed)
proof
let R1 being RealLinearSpace;
thus L20: (for R7 being (VECTOR of R1) holds (for R4 being (VECTOR of R1) holds ((R7 in { ( 0. R1 ) } & R4 in { ( 0. R1 ) }) implies ( R7 + R4 ) in { ( 0. R1 ) })))
proof
let R7 being (VECTOR of R1);
let R4 being (VECTOR of R1);
assume L21: (R7 in { ( 0. R1 ) } & R4 in { ( 0. R1 ) });
L22: (R7 = ( 0. R1 ) & R4 = ( 0. R1 )) by L21 , TARSKI:def 1;
L23: ( R7 + R4 ) = ( 0. R1 ) by L22 , RLVECT_1:4;
thus L24: thesis by L23 , TARSKI:def 1;
end;

let R10 being Real;
let R7 being (VECTOR of R1);
assume L25: R7 in { ( 0. R1 ) };
L26: R7 = ( 0. R1 ) by L25 , TARSKI:def 1;
thus L27: thesis by L26 , L25 , RLVECT_1:10;
end;
theorem
L28: (for R1 being RealLinearSpace holds (for R11 being (Subset of R1) holds ((the carrier of R1) = R11 implies R11 is  linearly-closed)))
proof
let R1 being RealLinearSpace;
let R11 being (Subset of R1);
assume L29: (the carrier of R1) = R11;
thus L30: (for R7 being (VECTOR of R1) holds (for R4 being (VECTOR of R1) holds ((R7 in R11 & R4 in R11) implies ( R7 + R4 ) in R11))) by L29;
let R10 being Real;
let R7 being (VECTOR of R1);
assume L31: R7 in R11;
thus L32: thesis by L29;
end;
theorem
L33: (for R1 being RealLinearSpace holds (for R11 being (Subset of R1) holds (for R12 being (Subset of R1) holds (for R13 being (Subset of R1) holds ((R11 is  linearly-closed & R12 is  linearly-closed & R13 = { ( R7 + R4 ) where R7 is (VECTOR of R1) , R4 is (VECTOR of R1) : (R7 in R11 & R4 in R12) }) implies R13 is  linearly-closed)))))
proof
let R1 being RealLinearSpace;
let R11 being (Subset of R1);
let R12 being (Subset of R1);
let R13 being (Subset of R1);
assume that
L34: (R11 is  linearly-closed & R12 is  linearly-closed)
and
L35: R13 = { ( R7 + R4 ) where R7 is (VECTOR of R1) , R4 is (VECTOR of R1) : (R7 in R11 & R4 in R12) };
thus L36: (for R7 being (VECTOR of R1) holds (for R4 being (VECTOR of R1) holds ((R7 in R13 & R4 in R13) implies ( R7 + R4 ) in R13)))
proof
let R7 being (VECTOR of R1);
let R4 being (VECTOR of R1);
assume that
L37: R7 in R13
and
L38: R4 in R13;
consider R8 being (VECTOR of R1), R9 being (VECTOR of R1) such that L39: R7 = ( R8 + R9 ) and L40: (R8 in R11 & R9 in R12) by L35 , L37;
consider R5 being (VECTOR of R1), R6 being (VECTOR of R1) such that L41: R4 = ( R5 + R6 ) and L42: (R5 in R11 & R6 in R12) by L35 , L38;
L43: ( R7 + R4 ) = ( ( ( R8 + R9 ) + R5 ) + R6 ) by L39 , L41 , RLVECT_1:def 3
.= ( ( ( R8 + R5 ) + R9 ) + R6 ) by RLVECT_1:def 3
.= ( ( R8 + R5 ) + ( R9 + R6 ) ) by RLVECT_1:def 3;
L44: (( R8 + R5 ) in R11 & ( R9 + R6 ) in R12) by L34 , L40 , L42 , L1;
thus L45: thesis by L44 , L35 , L43;
end;

let R10 being Real;
let R7 being (VECTOR of R1);
assume L46: R7 in R13;
consider R8 being (VECTOR of R1), R9 being (VECTOR of R1) such that L47: R7 = ( R8 + R9 ) and L48: (R8 in R11 & R9 in R12) by L46 , L35;
L49: ( R10 * R7 ) = ( ( R10 * R8 ) + ( R10 * R9 ) ) by L47 , RLVECT_1:def 5;
L50: (( R10 * R8 ) in R11 & ( R10 * R9 ) in R12) by L34 , L48 , L1;
thus L51: thesis by L50 , L35 , L49;
end;
theorem
L52: (for R1 being RealLinearSpace holds (for R11 being (Subset of R1) holds (for R12 being (Subset of R1) holds ((R11 is  linearly-closed & R12 is  linearly-closed) implies ( R11 /\ R12 ) is  linearly-closed))))
proof
let R1 being RealLinearSpace;
let R11 being (Subset of R1);
let R12 being (Subset of R1);
assume that
L53: R11 is  linearly-closed
and
L54: R12 is  linearly-closed;
thus L55: (for R7 being (VECTOR of R1) holds (for R4 being (VECTOR of R1) holds ((R7 in ( R11 /\ R12 ) & R4 in ( R11 /\ R12 )) implies ( R7 + R4 ) in ( R11 /\ R12 ))))
proof
let R7 being (VECTOR of R1);
let R4 being (VECTOR of R1);
assume L56: (R7 in ( R11 /\ R12 ) & R4 in ( R11 /\ R12 ));
L57: (R7 in R12 & R4 in R12) by L56 , XBOOLE_0:def 4;
L58: ( R7 + R4 ) in R12 by L57 , L54 , L1;
L59: (R7 in R11 & R4 in R11) by L56 , XBOOLE_0:def 4;
L60: ( R7 + R4 ) in R11 by L59 , L53 , L1;
thus L61: thesis by L60 , L58 , XBOOLE_0:def 4;
end;

let R10 being Real;
let R7 being (VECTOR of R1);
assume L62: R7 in ( R11 /\ R12 );
L63: R7 in R12 by L62 , XBOOLE_0:def 4;
L64: ( R10 * R7 ) in R12 by L63 , L54 , L1;
L65: R7 in R11 by L62 , XBOOLE_0:def 4;
L66: ( R10 * R7 ) in R11 by L65 , L53 , L1;
thus L67: thesis by L66 , L64 , XBOOLE_0:def 4;
end;
definition
let R1 being RealLinearSpace;
mode Subspace of R1
 -> RealLinearSpace
means :L68: ((the carrier of it) c= (the carrier of R1) & ( 0. it ) = ( 0. R1 ) & (the addF of it) = ( (the addF of R1) || (the carrier of it) ) & (the Mult of it) = ( (the Mult of R1) | [: ( REAL ) , (the carrier of it) :] ));
existence
proof
L69: ((the addF of R1) = ( (the addF of R1) || (the carrier of R1) ) & (the Mult of R1) = ( (the Mult of R1) | [: ( REAL ) , (the carrier of R1) :] )) by RELSET_1:19;
thus L70: thesis by L69;
end;
end;
theorem
L72: (for R1 being RealLinearSpace holds (for R14 being set holds (for R16 being (Subspace of R1) holds (for R17 being (Subspace of R1) holds ((R14 in R16 & R16 is (Subspace of R17)) implies R14 in R17)))))
proof
let R1 being RealLinearSpace;
let R14 being set;
let R16 being (Subspace of R1);
let R17 being (Subspace of R1);
assume L73: (R14 in R16 & R16 is (Subspace of R17));
L74: (R14 in (the carrier of R16) & (the carrier of R16) c= (the carrier of R17)) by L73 , L68 , STRUCT_0:def 5;
thus L75: thesis by L74 , STRUCT_0:def 5;
end;
theorem
L76: (for R1 being RealLinearSpace holds (for R14 being set holds (for R15 being (Subspace of R1) holds (R14 in R15 implies R14 in R1))))
proof
let R1 being RealLinearSpace;
let R14 being set;
let R15 being (Subspace of R1);
assume L77: R14 in R15;
L78: R14 in (the carrier of R15) by L77 , STRUCT_0:def 5;
L79: (the carrier of R15) c= (the carrier of R1) by L68;
thus L80: thesis by L79 , L78 , STRUCT_0:def 5;
end;
theorem
L81: (for R1 being RealLinearSpace holds (for R15 being (Subspace of R1) holds (for R18 being (VECTOR of R15) holds R18 is (VECTOR of R1))))
proof
let R1 being RealLinearSpace;
let R15 being (Subspace of R1);
let R18 being (VECTOR of R15);
L82: R18 in R1 by L76 , RLVECT_1:1;
thus L83: thesis by L82 , STRUCT_0:def 5;
end;
theorem
L84: (for R1 being RealLinearSpace holds (for R15 being (Subspace of R1) holds ( 0. R15 ) = ( 0. R1 ))) by L68;
theorem
L85: (for R1 being RealLinearSpace holds (for R16 being (Subspace of R1) holds (for R17 being (Subspace of R1) holds ( 0. R16 ) = ( 0. R17 ))))
proof
let R1 being RealLinearSpace;
let R16 being (Subspace of R1);
let R17 being (Subspace of R1);
thus L86: ( 0. R16 ) = ( 0. R1 ) by L68
.= ( 0. R17 ) by L68;
end;
theorem
L87: (for R1 being RealLinearSpace holds (for R4 being (VECTOR of R1) holds (for R7 being (VECTOR of R1) holds (for R15 being (Subspace of R1) holds (for R19 being (VECTOR of R15) holds (for R20 being (VECTOR of R15) holds ((R19 = R7 & R20 = R4) implies ( R19 + R20 ) = ( R7 + R4 ))))))))
proof
let R1 being RealLinearSpace;
let R4 being (VECTOR of R1);
let R7 being (VECTOR of R1);
let R15 being (Subspace of R1);
let R19 being (VECTOR of R15);
let R20 being (VECTOR of R15);
assume L88: (R7 = R19 & R4 = R20);
L89: ( R19 + R20 ) = ( ( (the addF of R1) || (the carrier of R15) ) . [ R19 , R20 ] ) by L68;
thus L90: thesis by L89 , L88 , FUNCT_1:49;
end;
theorem
L91: (for R1 being RealLinearSpace holds (for R7 being (VECTOR of R1) holds (for R10 being Real holds (for R15 being (Subspace of R1) holds (for R18 being (VECTOR of R15) holds (R18 = R7 implies ( R10 * R18 ) = ( R10 * R7 )))))))
proof
let R1 being RealLinearSpace;
let R7 being (VECTOR of R1);
let R10 being Real;
let R15 being (Subspace of R1);
let R18 being (VECTOR of R15);
assume L92: R18 = R7;
L93: ( R10 * R18 ) = ( ( (the Mult of R1) | [: ( REAL ) , (the carrier of R15) :] ) . [ R10 , R18 ] ) by L68;
thus L94: thesis by L93 , L92 , FUNCT_1:49;
end;
theorem
L95: (for R1 being RealLinearSpace holds (for R7 being (VECTOR of R1) holds (for R15 being (Subspace of R1) holds (for R18 being (VECTOR of R15) holds (R18 = R7 implies ( - R7 ) = ( - R18 ))))))
proof
let R1 being RealLinearSpace;
let R7 being (VECTOR of R1);
let R15 being (Subspace of R1);
let R18 being (VECTOR of R15);
L96: (( - R7 ) = ( ( - 1 ) * R7 ) & ( - R18 ) = ( ( - 1 ) * R18 )) by RLVECT_1:16;
assume L97: R18 = R7;
thus L98: thesis by L97 , L96 , L91;
end;
theorem
L99: (for R1 being RealLinearSpace holds (for R4 being (VECTOR of R1) holds (for R7 being (VECTOR of R1) holds (for R15 being (Subspace of R1) holds (for R19 being (VECTOR of R15) holds (for R20 being (VECTOR of R15) holds ((R19 = R7 & R20 = R4) implies ( R19 - R20 ) = ( R7 - R4 ))))))))
proof
let R1 being RealLinearSpace;
let R4 being (VECTOR of R1);
let R7 being (VECTOR of R1);
let R15 being (Subspace of R1);
let R19 being (VECTOR of R15);
let R20 being (VECTOR of R15);
assume that
L100: R19 = R7
and
L101: R20 = R4;
L102: ( - R20 ) = ( - R4 ) by L101 , L95;
thus L103: thesis by L102 , L100 , L87;
end;
L104: (for R1 being RealLinearSpace holds (for R11 being (Subset of R1) holds (for R15 being (Subspace of R1) holds ((the carrier of R15) = R11 implies R11 is  linearly-closed))))
proof
let R1 being RealLinearSpace;
let R11 being (Subset of R1);
let R15 being (Subspace of R1);
set D3 = (the carrier of R15);
reconsider D4 = R15 as RealLinearSpace;
assume L105: (the carrier of R15) = R11;
thus L106: (for R7 being (VECTOR of R1) holds (for R4 being (VECTOR of R1) holds ((R7 in R11 & R4 in R11) implies ( R7 + R4 ) in R11)))
proof
let R7 being (VECTOR of R1);
let R4 being (VECTOR of R1);
assume L107: (R7 in R11 & R4 in R11);
reconsider D5 = R7 , D6 = R4 as (VECTOR of D4) by L107 , L105;
reconsider D7 = ( D5 + D6 ) as (Element of D3);
L108: D7 in R11 by L105;
thus L109: thesis by L108 , L87;
end;

let R10 being Real;
let R7 being (VECTOR of R1);
assume L110: R7 in R11;
reconsider D8 = R7 as (VECTOR of D4) by L110 , L105;
reconsider D9 = ( R10 * D8 ) as (Element of D3);
L111: D9 in R11 by L105;
thus L112: thesis by L111 , L91;
end;
theorem
L113: (for R1 being RealLinearSpace holds (for R15 being (Subspace of R1) holds ( 0. R1 ) in R15))
proof
let R1 being RealLinearSpace;
let R15 being (Subspace of R1);
L114: ( 0. R15 ) in R15 by RLVECT_1:1;
thus L115: thesis by L114 , L68;
end;
theorem
L116: (for R1 being RealLinearSpace holds (for R16 being (Subspace of R1) holds (for R17 being (Subspace of R1) holds ( 0. R16 ) in R17)))
proof
let R1 being RealLinearSpace;
let R16 being (Subspace of R1);
let R17 being (Subspace of R1);
L117: ( 0. R16 ) = ( 0. R1 ) by L68;
thus L118: thesis by L117 , L113;
end;
theorem
L119: (for R1 being RealLinearSpace holds (for R15 being (Subspace of R1) holds ( 0. R15 ) in R1)) by L76 , RLVECT_1:1;
theorem
L120: (for R1 being RealLinearSpace holds (for R4 being (VECTOR of R1) holds (for R7 being (VECTOR of R1) holds (for R15 being (Subspace of R1) holds ((R4 in R15 & R7 in R15) implies ( R4 + R7 ) in R15)))))
proof
let R1 being RealLinearSpace;
let R4 being (VECTOR of R1);
let R7 being (VECTOR of R1);
let R15 being (Subspace of R1);
reconsider D10 = (the carrier of R15) as (Subset of R1) by L68;
assume L121: (R4 in R15 & R7 in R15);
L122: (R4 in (the carrier of R15) & R7 in (the carrier of R15)) by L121 , STRUCT_0:def 5;
L123: D10 is  linearly-closed by L104;
L124: ( R4 + R7 ) in (the carrier of R15) by L123 , L122 , L1;
thus L125: thesis by L124 , STRUCT_0:def 5;
end;
theorem
L126: (for R1 being RealLinearSpace holds (for R7 being (VECTOR of R1) holds (for R10 being Real holds (for R15 being (Subspace of R1) holds (R7 in R15 implies ( R10 * R7 ) in R15)))))
proof
let R1 being RealLinearSpace;
let R7 being (VECTOR of R1);
let R10 being Real;
let R15 being (Subspace of R1);
reconsider D11 = (the carrier of R15) as (Subset of R1) by L68;
assume L127: R7 in R15;
L128: R7 in (the carrier of R15) by L127 , STRUCT_0:def 5;
L129: D11 is  linearly-closed by L104;
L130: ( R10 * R7 ) in (the carrier of R15) by L129 , L128 , L1;
thus L131: thesis by L130 , STRUCT_0:def 5;
end;
theorem
L132: (for R1 being RealLinearSpace holds (for R7 being (VECTOR of R1) holds (for R15 being (Subspace of R1) holds (R7 in R15 implies ( - R7 ) in R15))))
proof
let R1 being RealLinearSpace;
let R7 being (VECTOR of R1);
let R15 being (Subspace of R1);
assume L133: R7 in R15;
L134: ( ( - 1 ) * R7 ) in R15 by L133 , L126;
thus L135: thesis by L134 , RLVECT_1:16;
end;
theorem
L136: (for R1 being RealLinearSpace holds (for R4 being (VECTOR of R1) holds (for R7 being (VECTOR of R1) holds (for R15 being (Subspace of R1) holds ((R4 in R15 & R7 in R15) implies ( R4 - R7 ) in R15)))))
proof
let R1 being RealLinearSpace;
let R4 being (VECTOR of R1);
let R7 being (VECTOR of R1);
let R15 being (Subspace of R1);
assume that
L137: R4 in R15
and
L138: R7 in R15;
L139: ( - R7 ) in R15 by L138 , L132;
thus L140: thesis by L139 , L137 , L120;
end;
theorem
L141: (for R1 being RealLinearSpace holds (for R11 being (Subset of R1) holds (for R21 being non  empty set holds (for R22 being (Element of R21) holds (for R23 being (BinOp of R21) holds (for R24 being (Function of [: ( REAL ) , R21 :] , R21) holds ((R11 = R21 & R22 = ( 0. R1 ) & R23 = ( (the addF of R1) || R11 ) & R24 = ( (the Mult of R1) | [: ( REAL ) , R11 :] )) implies RLSStruct (# R21 , R22 , R23 , R24 #) is (Subspace of R1))))))))
proof
let R1 being RealLinearSpace;
let R11 being (Subset of R1);
let R21 being non  empty set;
let R22 being (Element of R21);
let R23 being (BinOp of R21);
let R24 being (Function of [: ( REAL ) , R21 :] , R21);
assume that
L142: R11 = R21
and
L143: R22 = ( 0. R1 )
and
L144: R23 = ( (the addF of R1) || R11 )
and
L145: R24 = ( (the Mult of R1) | [: ( REAL ) , R11 :] );
set D12 = RLSStruct (# R21 , R22 , R23 , R24 #);
L146: (for R10 being Real holds (for B1 being (VECTOR of D12) holds ( R10 * B1 ) = ( (the Mult of R1) . (R10 , B1) )))
proof
let R10 being Real;
let C1 being (VECTOR of D12);
thus L147: ( R10 * C1 ) = ( (the Mult of R1) . [ R10 , C1 ] ) by L142 , L145 , FUNCT_1:49
.= ( (the Mult of R1) . (R10 , C1) );
end;
L148: (for B2 , B3 being (VECTOR of D12) holds ( B2 + B3 ) = ( (the addF of R1) . (B2 , B3) ))
proof
let C2 , C3 being (VECTOR of D12);
thus L149: ( C2 + C3 ) = ( (the addF of R1) . [ C2 , C3 ] ) by L142 , L144 , FUNCT_1:49
.= ( (the addF of R1) . (C2 , C3) );
end;
L150: D12 is  Abelian  add-associative  right_zeroed  right_complementable  vector-distributive  scalar-distributive  scalar-associative  scalar-unital
proof
set D13 = (the Mult of R1);
set D14 = (the addF of R1);
thus L151: D12 is  Abelian
proof
let C4 , C5 being (VECTOR of D12);
reconsider D15 = C4 , D16 = C5 as (VECTOR of R1) by L142 , TARSKI:def 3;
thus L152: ( C4 + C5 ) = ( D15 + D16 ) by L148
.= ( D16 + D15 )
.= ( C5 + C4 ) by L148;
end;

thus L153: D12 is  add-associative
proof
let C6 , C7 , C8 being (VECTOR of D12);
reconsider D17 = C6 , D18 = C7 , D19 = C8 as (VECTOR of R1) by L142 , TARSKI:def 3;
thus L154: ( ( C6 + C7 ) + C8 ) = ( D14 . (( C6 + C7 ) , D19) ) by L148
.= ( ( D17 + D18 ) + D19 ) by L148
