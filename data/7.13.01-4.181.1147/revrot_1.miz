definition
let C1 being non  empty set;
let C2 being (FinSequence of C1);
redefine attr C2 is  constant
means
:L1: (for R5 being (Element of ( NAT )) holds (for R4 being (Element of ( NAT )) holds ((R5 in ( dom C2 ) & R4 in ( dom C2 )) implies ( C2 /. R5 ) = ( C2 /. R4 ))));
compatibility
proof
thus L2:now
assume L3: C2 is  constant;
let R5 being (Element of ( NAT ));
let R4 being (Element of ( NAT ));
assume that
L4: R5 in ( dom C2 )
and
L5: R4 in ( dom C2 );
thus L6: ( C2 /. R5 ) = ( C2 . R5 ) by L4 , PARTFUN1:def 6
.= ( C2 . R4 ) by L3 , L4 , L5 , SEQM_3:def 10
.= ( C2 /. R4 ) by L5 , PARTFUN1:def 6;
end;
assume L7: (for R5 being (Element of ( NAT )) holds (for R4 being (Element of ( NAT )) holds ((R5 in ( dom C2 ) & R4 in ( dom C2 )) implies ( C2 /. R5 ) = ( C2 /. R4 ))));
let R5 being (Element of ( NAT ));
let R4 being (Element of ( NAT ));
assume that
L8: R5 in ( dom C2 )
and
L9: R4 in ( dom C2 );
thus L10: ( C2 . R5 ) = ( C2 /. R5 ) by L8 , PARTFUN1:def 6
.= ( C2 /. R4 ) by L7 , L8 , L9
.= ( C2 . R4 ) by L9 , PARTFUN1:def 6;
end;
end;
theorem
L12: (for B1 being non  empty set holds (for B2 being (FinSequence of B1) holds (B2 just_once_values ( B2 /. ( len B2 ) ) implies ( ( B2 /. ( len B2 ) ) .. B2 ) = ( len B2 ))))
proof
let C3 being non  empty set;
let C4 being (FinSequence of C3);
assume L13: C4 just_once_values ( C4 /. ( len C4 ) );
reconsider D1 = C4 as non  empty (FinSequence of C3) by L13 , FINSEQ_4:5 , RELAT_1:38;
L14: ( ( ( C4 /. ( len C4 ) ) .. C4 ) + 1 ) = ( ( ( C4 /. ( len C4 ) ) .. C4 ) + ( ( ( Rev D1 ) /. 1 ) .. ( Rev D1 ) ) ) by FINSEQ_6:43
.= ( ( ( C4 /. ( len C4 ) ) .. C4 ) + ( ( C4 /. ( len C4 ) ) .. ( Rev C4 ) ) ) by FINSEQ_5:65
.= ( ( len C4 ) + 1 ) by L13 , FINSEQ_6:37;
thus L15: thesis by L14;
end;
