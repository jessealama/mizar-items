begin
definition
let C1 being RealUnitarySpace;
let C2 , C3 being (Subspace of C1);
func C2 + C3 ->  strict (Subspace of C1) means 
:L1: (the carrier of it) = { ( B1 + B2 ) where B1 , B2 is (VECTOR of C1) : (B1 in C2 & B2 in C3) };
existence
proof
reconsider D1 = (the carrier of C2) , D2 = (the carrier of C3) as (Subset of C1) by RUSUB_1:def 1;
set D3 = { ( B3 + B4 ) where B3 , B4 is (VECTOR of C1) : (B3 in C2 & B4 in C3) };
L2: D3 c= (the carrier of C1)
proof
let C4 being set;
assume L3: C4 in D3;
L4: (ex B5 , B6 being (VECTOR of C1) st (C4 = ( B5 + B6 ) & B5 in C2 & B6 in C3)) by L3;
thus L5: thesis by L4;
end;
reconsider D4 = D3 as (Subset of C1) by L2;
L6: ( 0. C1 ) = ( ( 0. C1 ) + ( 0. C1 ) ) by RLVECT_1:4;
L7: (( 0. C1 ) in C2 & ( 0. C1 ) in C3) by RUSUB_1:11;
L8: ( 0. C1 ) in D4 by L7 , L6;
L9: D4 = { ( B7 + B8 ) where B7 , B8 is (VECTOR of C1) : (B7 in D1 & B8 in D2) }
proof
thus L10: D4 c= { ( B9 + B10 ) where B9 , B10 is (VECTOR of C1) : (B9 in D1 & B10 in D2) }
proof
let C5 being set;
assume L11: C5 in D4;
consider C6 , C7 being (VECTOR of C1) such that L12: C5 = ( C6 + C7 ) and L13: (C6 in C2 & C7 in C3) by L11;
L14: (C6 in D1 & C7 in D2) by L13 , STRUCT_0:def 5;
thus L15: thesis by L14 , L12;
end;

let C8 being set;
assume L16: C8 in { ( B11 + B12 ) where B11 , B12 is (VECTOR of C1) : (B11 in D1 & B12 in D2) };
consider C9 , C10 being (VECTOR of C1) such that L17: C8 = ( C9 + C10 ) and L18: (C9 in D1 & C10 in D2) by L16;
L19: (C9 in C2 & C10 in C3) by L18 , STRUCT_0:def 5;
thus L20: thesis by L19 , L17;
end;
L21: (D1 is  linearly-closed & D2 is  linearly-closed) by RUSUB_1:28;
thus L22: thesis by L21 , L8 , L9 , RLSUB_1:6 , RUSUB_1:29;
end;
uniqueness by RUSUB_1:24;
end;
definition
let C11 being RealUnitarySpace;
let C12 , C13 being (Subspace of C11);
func C12 /\ C13 ->  strict (Subspace of C11) means 
:L24: (the carrier of it) = ( (the carrier of C12) /\ (the carrier of C13) );
existence
proof
set D5 = (the carrier of C13);
set D6 = (the carrier of C12);
set D7 = (the carrier of C11);
L25: ( 0. C11 ) in C13 by RUSUB_1:11;
L26: ( 0. C11 ) in D5 by L25 , STRUCT_0:def 5;
L27: (D6 c= D7 & D5 c= D7) by RUSUB_1:def 1;
L28: ( D6 /\ D5 ) c= ( D7 /\ D7 ) by L27 , XBOOLE_1:27;
reconsider D8 = D6 , D9 = D5 , D10 = ( D6 /\ D5 ) as (Subset of C11) by L28 , RUSUB_1:def 1;
L29: (D8 is  linearly-closed & D9 is  linearly-closed) by RUSUB_1:28;
L30: D10 is  linearly-closed by L29 , RLSUB_1:7;
L31: ( 0. C11 ) in C12 by RUSUB_1:11;
L32: ( 0. C11 ) in D6 by L31 , STRUCT_0:def 5;
L33: ( D6 /\ D5 ) <> ( {} ) by L32 , L26 , XBOOLE_0:def 4;
thus L34: thesis by L33 , L30 , RUSUB_1:29;
end;
uniqueness by RUSUB_1:24;
end;
begin
theorem
L36: (for B13 being RealUnitarySpace holds (for B14 , B15 being (Subspace of B13) holds (for B16 being set holds (B16 in ( B14 + B15 ) iff (ex B17 , B18 being (VECTOR of B13) st (B17 in B14 & B18 in B15 & B16 = ( B17 + B18 )))))))
proof
let C14 being RealUnitarySpace;
let C15 , C16 being (Subspace of C14);
let C17 being set;
thus L37: (C17 in ( C15 + C16 ) implies (ex B19 , B20 being (VECTOR of C14) st (B19 in C15 & B20 in C16 & C17 = ( B19 + B20 ))))
proof
assume L38: C17 in ( C15 + C16 );
L39: C17 in (the carrier of ( C15 + C16 )) by L38 , STRUCT_0:def 5;
L40: C17 in { ( B21 + B22 ) where B21 , B22 is (VECTOR of C14) : (B21 in C15 & B22 in C16) } by L39 , L1;
consider C18 , C19 being (VECTOR of C14) such that L41: (C17 = ( C18 + C19 ) & C18 in C15 & C19 in C16) by L40;
take C18;
take C19;
thus L42: thesis by L41;
end;

given C20 , C21 being (VECTOR of C14) such that
L43: (C20 in C15 & C21 in C16 & C17 = ( C20 + C21 ));

L44: C17 in { ( B23 + B24 ) where B23 , B24 is (VECTOR of C14) : (B23 in C15 & B24 in C16) } by L43;
L45: C17 in (the carrier of ( C15 + C16 )) by L44 , L1;
thus L46: thesis by L45 , STRUCT_0:def 5;
end;
theorem
L47: (for B25 being RealUnitarySpace holds (for B26 , B27 being (Subspace of B25) holds (for B28 being (VECTOR of B25) holds ((B28 in B26 or B28 in B27) implies B28 in ( B26 + B27 )))))
proof
let C22 being RealUnitarySpace;
let C23 , C24 being (Subspace of C22);
let C25 being (VECTOR of C22);
assume L48: (C25 in C23 or C25 in C24);
L49:
now
per cases  by L48;
suppose L50: C25 in C23;

L51: (C25 = ( C25 + ( 0. C22 ) ) & ( 0. C22 ) in C24) by RLVECT_1:4 , RUSUB_1:11;
thus L52: thesis by L51 , L50 , L36;
end;
suppose L53: C25 in C24;

L54: (C25 = ( ( 0. C22 ) + C25 ) & ( 0. C22 ) in C23) by RLVECT_1:4 , RUSUB_1:11;
thus L55: thesis by L54 , L53 , L36;
end;
end;
thus L57: thesis by L49;
end;
theorem
L58: (for B29 being RealUnitarySpace holds (for B30 , B31 being (Subspace of B29) holds (for B32 being set holds (B32 in ( B30 /\ B31 ) iff (B32 in B30 & B32 in B31)))))
proof
let C26 being RealUnitarySpace;
let C27 , C28 being (Subspace of C26);
let C29 being set;
L59: (C29 in ( C27 /\ C28 ) iff C29 in (the carrier of ( C27 /\ C28 ))) by STRUCT_0:def 5;
L60: (C29 in ( C27 /\ C28 ) iff C29 in ( (the carrier of C27) /\ (the carrier of C28) )) by L59 , L24;
L61: (C29 in ( C27 /\ C28 ) iff (C29 in (the carrier of C27) & C29 in (the carrier of C28))) by L60 , XBOOLE_0:def 4;
thus L62: thesis by L61 , STRUCT_0:def 5;
end;
L63: (for B33 being RealUnitarySpace holds (for B34 , B35 being (Subspace of B33) holds ( B34 + B35 ) = ( B35 + B34 )))
proof
let C30 being RealUnitarySpace;
let C31 , C32 being (Subspace of C30);
set D11 = { ( B36 + B37 ) where B36 , B37 is (VECTOR of C30) : (B36 in C31 & B37 in C32) };
set D12 = { ( B38 + B39 ) where B38 , B39 is (VECTOR of C30) : (B38 in C32 & B39 in C31) };
L64: D12 c= D11
proof
let C33 being set;
assume L65: C33 in D12;
L66: (ex B40 , B41 being (VECTOR of C30) st (C33 = ( B40 + B41 ) & B40 in C32 & B41 in C31)) by L65;
thus L67: thesis by L66;
end;
L68: (the carrier of ( C31 + C32 )) = D11 by L1;
L69: D11 c= D12
proof
let C34 being set;
assume L70: C34 in D11;
L71: (ex B42 , B43 being (VECTOR of C30) st (C34 = ( B42 + B43 ) & B42 in C31 & B43 in C32)) by L70;
thus L72: thesis by L71;
end;
L73: D11 = D12 by L69 , L64 , XBOOLE_0:def 10;
thus L74: thesis by L73 , L68 , L1;
end;
L75: (for B44 being RealUnitarySpace holds (for B45 , B46 being (Subspace of B44) holds (the carrier of B45) c= (the carrier of ( B45 + B46 ))))
proof
let C35 being RealUnitarySpace;
let C36 , C37 being (Subspace of C35);
let C38 being set;
set D13 = { ( B47 + B48 ) where B47 , B48 is (VECTOR of C35) : (B47 in C36 & B48 in C37) };
assume L76: C38 in (the carrier of C36);
reconsider D14 = C38 as (Element of C36) by L76;
reconsider D15 = D14 as (VECTOR of C35) by RUSUB_1:3;
L77: D15 = ( D15 + ( 0. C35 ) ) by RLVECT_1:4;
L78: (D15 in C36 & ( 0. C35 ) in C37) by RUSUB_1:11 , STRUCT_0:def 5;
L79: C38 in D13 by L78 , L77;
thus L80: thesis by L79 , L1;
end;
L81: (for B49 being RealUnitarySpace holds (for B50 being (Subspace of B49) holds (for B51 being  strict (Subspace of B49) holds ((the carrier of B50) c= (the carrier of B51) implies ( B50 + B51 ) = B51))))
proof
let C39 being RealUnitarySpace;
let C40 being (Subspace of C39);
let C41 being  strict (Subspace of C39);
assume L82: (the carrier of C40) c= (the carrier of C41);
L83: (the carrier of ( C40 + C41 )) c= (the carrier of C41)
proof
let C42 being set;
assume L84: C42 in (the carrier of ( C40 + C41 ));
L85: C42 in { ( B52 + B53 ) where B52 , B53 is (VECTOR of C39) : (B52 in C40 & B53 in C41) } by L84 , L1;
consider C43 , C44 being (VECTOR of C39) such that L86: C42 = ( C43 + C44 ) and L87: C43 in C40 and L88: C44 in C41 by L85;
L89: C40 is (Subspace of C41) by L82 , RUSUB_1:22;
L90: C43 in C41 by L89 , L87 , RUSUB_1:1;
L91: ( C43 + C44 ) in C41 by L90 , L88 , RUSUB_1:14;
thus L92: thesis by L91 , L86 , STRUCT_0:def 5;
end;
L93: ( C40 + C41 ) = ( C41 + C40 ) by L63;
L94: (the carrier of C41) c= (the carrier of ( C40 + C41 )) by L93 , L75;
L95: (the carrier of ( C40 + C41 )) = (the carrier of C41) by L94 , L83 , XBOOLE_0:def 10;
thus L96: thesis by L95 , RUSUB_1:24;
end;
theorem
L97: (for B54 being RealUnitarySpace holds (for B55 being  strict (Subspace of B54) holds ( B55 + B55 ) = B55)) by L81;
theorem
L98: (for B56 being RealUnitarySpace holds (for B57 , B58 being (Subspace of B56) holds ( B57 + B58 ) = ( B58 + B57 ))) by L63;
theorem
L99: (for B59 being RealUnitarySpace holds (for B60 , B61 , B62 being (Subspace of B59) holds ( B60 + ( B61 + B62 ) ) = ( ( B60 + B61 ) + B62 )))
proof
let C45 being RealUnitarySpace;
let C46 , C47 , C48 being (Subspace of C45);
set D16 = { ( B63 + B64 ) where B63 , B64 is (VECTOR of C45) : (B63 in C46 & B64 in C47) };
set D17 = { ( B65 + B66 ) where B65 , B66 is (VECTOR of C45) : (B65 in C47 & B66 in C48) };
set D18 = { ( B67 + B68 ) where B67 , B68 is (VECTOR of C45) : (B67 in ( C46 + C47 ) & B68 in C48) };
set D19 = { ( B69 + B70 ) where B69 , B70 is (VECTOR of C45) : (B69 in C46 & B70 in ( C47 + C48 )) };
L100: (the carrier of ( C46 + ( C47 + C48 ) )) = D19 by L1;
L101: D18 c= D19
proof
let C49 being set;
assume L102: C49 in D18;
consider C50 , C51 being (VECTOR of C45) such that L103: C49 = ( C50 + C51 ) and L104: C50 in ( C46 + C47 ) and L105: C51 in C48 by L102;
L106: C50 in (the carrier of ( C46 + C47 )) by L104 , STRUCT_0:def 5;
L107: C50 in D16 by L106 , L1;
consider C52 , C53 being (VECTOR of C45) such that L108: C50 = ( C52 + C53 ) and L109: C52 in C46 and L110: C53 in C47 by L107;
L111: ( C53 + C51 ) in D17 by L105 , L110;
L112: ( C53 + C51 ) in (the carrier of ( C47 + C48 )) by L111 , L1;
L113: ( C53 + C51 ) in ( C47 + C48 ) by L112 , STRUCT_0:def 5;
L114: ( C50 + C51 ) = ( C52 + ( C53 + C51 ) ) by L108 , RLVECT_1:def 3;
thus L115: thesis by L114 , L103 , L109 , L113;
end;
L116: D19 c= D18
proof
let C54 being set;
assume L117: C54 in D19;
consider C55 , C56 being (VECTOR of C45) such that L118: C54 = ( C55 + C56 ) and L119: C55 in C46 and L120: C56 in ( C47 + C48 ) by L117;
L121: C56 in (the carrier of ( C47 + C48 )) by L120 , STRUCT_0:def 5;
L122: C56 in D17 by L121 , L1;
consider C57 , C58 being (VECTOR of C45) such that L123: C56 = ( C57 + C58 ) and L124: C57 in C47 and L125: C58 in C48 by L122;
L126: ( C55 + C57 ) in D16 by L119 , L124;
L127: ( C55 + C57 ) in (the carrier of ( C46 + C47 )) by L126 , L1;
L128: ( C55 + C57 ) in ( C46 + C47 ) by L127 , STRUCT_0:def 5;
L129: ( C55 + C56 ) = ( ( C55 + C57 ) + C58 ) by L123 , RLVECT_1:def 3;
thus L130: thesis by L129 , L118 , L125 , L128;
end;
L131: D19 = D18 by L116 , L101 , XBOOLE_0:def 10;
thus L132: thesis by L131 , L100 , L1;
end;
theorem
L133: (for B71 being RealUnitarySpace holds (for B72 , B73 being (Subspace of B71) holds (B72 is (Subspace of ( B72 + B73 )) & B73 is (Subspace of ( B72 + B73 )))))
proof
let C59 being RealUnitarySpace;
let C60 , C61 being (Subspace of C59);
L134: (the carrier of C60) c= (the carrier of ( C60 + C61 )) by L75;
thus L135: C60 is (Subspace of ( C60 + C61 )) by L134 , RUSUB_1:22;
L136: (the carrier of C61) c= (the carrier of ( C61 + C60 )) by L75;
L137: (the carrier of C61) c= (the carrier of ( C60 + C61 )) by L136 , L63;
thus L138: thesis by L137 , RUSUB_1:22;
end;
theorem
L139: (for B74 being RealUnitarySpace holds (for B75 being (Subspace of B74) holds (for B76 being  strict (Subspace of B74) holds (B75 is (Subspace of B76) iff ( B75 + B76 ) = B76))))
proof
let C62 being RealUnitarySpace;
let C63 being (Subspace of C62);
let C64 being  strict (Subspace of C62);
thus L140: (C63 is (Subspace of C64) implies ( C63 + C64 ) = C64)
proof
assume L141: C63 is (Subspace of C64);
L142: (the carrier of C63) c= (the carrier of C64) by L141 , RUSUB_1:def 1;
thus L143: thesis by L142 , L81;
end;

thus L144: thesis by L133;
end;
theorem
L145: (for B77 being RealUnitarySpace holds (for B78 being  strict (Subspace of B77) holds (( ( (0). B77 ) + B78 ) = B78 & ( B78 + ( (0). B77 ) ) = B78)))
proof
let C65 being RealUnitarySpace;
let C66 being  strict (Subspace of C65);
L146: ( (0). C65 ) is (Subspace of C66) by RUSUB_1:33;
L147: (the carrier of ( (0). C65 )) c= (the carrier of C66) by L146 , RUSUB_1:def 1;
thus L148: ( ( (0). C65 ) + C66 ) = C66 by L147 , L81;
thus L149: thesis by L148 , L63;
end;
theorem
L150: (for B79 being RealUnitarySpace holds (( ( (0). B79 ) + ( (Omega). B79 ) ) = ( the UNITSTR of B79 ) & ( ( (Omega). B79 ) + ( (0). B79 ) ) = ( the UNITSTR of B79 )))
proof
let C67 being RealUnitarySpace;
thus L151: ( ( (0). C67 ) + ( (Omega). C67 ) ) = ( (Omega). C67 ) by L145
.= ( the UNITSTR of C67 ) by RUSUB_1:def 3;
thus L152: thesis by L151 , L63;
end;
theorem
L153: (for B80 being RealUnitarySpace holds (for B81 being (Subspace of B80) holds (( ( (Omega). B80 ) + B81 ) = ( the UNITSTR of B80 ) & ( B81 + ( (Omega). B80 ) ) = ( the UNITSTR of B80 ))))
proof
let C68 being RealUnitarySpace;
let C69 being (Subspace of C68);
L154: ((the carrier of C68) = (the carrier of ( the UNITSTR of C68 )) & (the carrier of C69) c= (the carrier of C68)) by RUSUB_1:def 1;
L155: (the carrier of C69) c= (the carrier of ( (Omega). C68 )) by L154 , RUSUB_1:def 3;
L156: ( C69 + ( (Omega). C68 ) ) = ( (Omega). C68 ) by L155 , L81
.= ( the UNITSTR of C68 ) by RUSUB_1:def 3;
thus L157: thesis by L156 , L63;
end;
theorem
L158: (for B82 being  strict RealUnitarySpace holds ( ( (Omega). B82 ) + ( (Omega). B82 ) ) = B82) by L153;
theorem
L159: (for B83 being RealUnitarySpace holds (for B84 being  strict (Subspace of B83) holds ( B84 /\ B84 ) = B84))
proof
let C70 being RealUnitarySpace;
let C71 being  strict (Subspace of C70);
L160: (the carrier of C71) = ( (the carrier of C71) /\ (the carrier of C71) );
thus L161: thesis by L160 , L24;
end;
theorem
L162: (for B85 being RealUnitarySpace holds (for B86 , B87 being (Subspace of B85) holds ( B86 /\ B87 ) = ( B87 /\ B86 )))
proof
let C72 being RealUnitarySpace;
let C73 , C74 being (Subspace of C72);
L163: (the carrier of ( C73 /\ C74 )) = ( (the carrier of C74) /\ (the carrier of C73) ) by L24;
thus L164: thesis by L163 , L24;
end;
theorem
L165: (for B88 being RealUnitarySpace holds (for B89 , B90 , B91 being (Subspace of B88) holds ( B89 /\ ( B90 /\ B91 ) ) = ( ( B89 /\ B90 ) /\ B91 )))
proof
let C75 being RealUnitarySpace;
let C76 , C77 , C78 being (Subspace of C75);
set D20 = (the carrier of C76);
set D21 = (the carrier of C77);
set D22 = (the carrier of C78);
L166: (the carrier of ( C76 /\ ( C77 /\ C78 ) )) = ( D20 /\ (the carrier of ( C77 /\ C78 )) ) by L24
.= ( D20 /\ ( D21 /\ D22 ) ) by L24
.= ( ( D20 /\ D21 ) /\ D22 ) by XBOOLE_1:16
.= ( (the carrier of ( C76 /\ C77 )) /\ D22 ) by L24;
thus L167: thesis by L166 , L24;
end;
L168: (for B92 being RealUnitarySpace holds (for B93 , B94 being (Subspace of B92) holds (the carrier of ( B93 /\ B94 )) c= (the carrier of B93)))
proof
let C79 being RealUnitarySpace;
let C80 , C81 being (Subspace of C79);
L169: (the carrier of ( C80 /\ C81 )) = ( (the carrier of C80) /\ (the carrier of C81) ) by L24;
thus L170: thesis by L169 , XBOOLE_1:17;
end;
theorem
L171: (for B95 being RealUnitarySpace holds (for B96 , B97 being (Subspace of B95) holds (( B96 /\ B97 ) is (Subspace of B96) & ( B96 /\ B97 ) is (Subspace of B97))))
proof
let C82 being RealUnitarySpace;
let C83 , C84 being (Subspace of C82);
L172: (the carrier of ( C83 /\ C84 )) c= (the carrier of C83) by L168;
thus L173: ( C83 /\ C84 ) is (Subspace of C83) by L172 , RUSUB_1:22;
L174: (the carrier of ( C84 /\ C83 )) c= (the carrier of C84) by L168;
L175: (the carrier of ( C83 /\ C84 )) c= (the carrier of C84) by L174 , L162;
thus L176: thesis by L175 , RUSUB_1:22;
end;
theorem
L177: (for B98 being RealUnitarySpace holds (for B99 being (Subspace of B98) holds (for B100 being  strict (Subspace of B98) holds (B100 is (Subspace of B99) iff ( B100 /\ B99 ) = B100))))
proof
let C85 being RealUnitarySpace;
let C86 being (Subspace of C85);
let C87 being  strict (Subspace of C85);
thus L178: (C87 is (Subspace of C86) implies ( C87 /\ C86 ) = C87)
proof
assume L179: C87 is (Subspace of C86);
L180: (the carrier of C87) c= (the carrier of C86) by L179 , RUSUB_1:def 1;
L181: (the carrier of ( C87 /\ C86 )) = ( (the carrier of C87) /\ (the carrier of C86) ) by L24;
thus L182: thesis by L181 , L180 , RUSUB_1:24 , XBOOLE_1:28;
end;

thus L183: thesis by L171;
end;
theorem
L184: (for B101 being RealUnitarySpace holds (for B102 being (Subspace of B101) holds (( ( (0). B101 ) /\ B102 ) = ( (0). B101 ) & ( B102 /\ ( (0). B101 ) ) = ( (0). B101 ))))
proof
let C88 being RealUnitarySpace;
let C89 being (Subspace of C88);
L185: ( 0. C88 ) in C89 by RUSUB_1:11;
L186: ( 0. C88 ) in (the carrier of C89) by L185 , STRUCT_0:def 5;
