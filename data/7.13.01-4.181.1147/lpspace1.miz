theorem
L3: (for B3 being RealLinearSpace holds (for B4 being (Subset of B3) holds (B4 is  linearly-closed iff B4 is  add-closed  multi-closed)))
proof
let C3 being RealLinearSpace;
let C4 being (Subset of C3);
thus L4:now
assume L5: C4 is  linearly-closed;
L6: ((for B5 , B6 being (VECTOR of C3) holds ((B5 in C4 & B6 in C4) implies ( B5 + B6 ) in C4)) & (for B7 being Real holds (for B8 being (VECTOR of C3) holds (B8 in C4 implies ( B7 * B8 ) in C4)))) by L5 , RLSUB_1:def 1;
thus L7: (C4 is  add-closed & C4 is  multi-closed) by L6 , L1 , IDEAL_1:def 1;
end;
assume L8: (C4 is  add-closed & C4 is  multi-closed);
L9: ((for B9 , B10 being (Element of C3) holds ((B9 in C4 & B10 in C4) implies ( B9 + B10 ) in C4)) & (for B11 being Real holds (for B12 being (VECTOR of C3) holds (B12 in C4 implies ( B11 * B12 ) in C4)))) by L8 , L1 , IDEAL_1:def 1;
thus L10: thesis by L9 , RLSUB_1:def 1;
end;
registration
let C5 being non  empty RLSStruct;
cluster  add-closed  multi-closed non  empty for (Subset of C5);
existence
proof
set D1 = (the carrier of C5);
L11: (for B13 being set holds (B13 in D1 implies B13 in (the carrier of C5)));
reconsider D2 = D1 as (Subset of C5) by L11 , TARSKI:def 3;
reconsider D3 = D2 as non  empty (Subset of C5);
take D3;
L12: ((for B14 being Real holds (for B15 being (VECTOR of C5) holds (B15 in D3 implies ( B14 * B15 ) in D3))) & (for B16 , B17 being (Element of C5) holds ((B16 in D3 & B17 in D3) implies ( B16 + B17 ) in D3)));
thus L13: thesis by L12 , L1 , IDEAL_1:def 1;
end;
end;
definition
let C6 being non  empty RLSStruct;
let C7 being  multi-closed non  empty (Subset of C6);
func Mult_ C7 -> (Function of [: ( REAL ) , C7 :] , C7) equals 
( (the Mult of C6) | [: ( REAL ) , C7 :] );
correctness
proof
L15: ([: ( REAL ) , C7 :] c= [: ( REAL ) , (the carrier of C6) :] & ( dom (the Mult of C6) ) = [: ( REAL ) , (the carrier of C6) :]) by FUNCT_2:def 1 , ZFMISC_1:95;
L16:
now
let C8 being set;
assume L17: C8 in [: ( REAL ) , C7 :];
consider C9 , C10 being set such that L18: C9 in ( REAL ) and L19: C10 in C7 and L20: C8 = [ C9 , C10 ] by L17 , ZFMISC_1:def 2;
reconsider D4 = C9 as Real by L18;
reconsider D5 = C10 as (VECTOR of C6) by L19;
L21: [ D4 , C10 ] in ( dom ( (the Mult of C6) | [: ( REAL ) , C7 :] ) ) by L15 , L17 , L20 , RELAT_1:62;
L22: ( ( (the Mult of C6) | [: ( REAL ) , C7 :] ) . C8 ) = ( D4 * D5 ) by L21 , L20 , FUNCT_1:47;
thus L23: ( ( (the Mult of C6) | [: ( REAL ) , C7 :] ) . C8 ) in C7 by L22 , L19 , L1;
end;
L24: ( dom ( (the Mult of C6) | [: ( REAL ) , C7 :] ) ) = [: ( REAL ) , C7 :] by L15 , RELAT_1:62;
thus L25: thesis by L24 , L16 , FUNCT_2:3;
end;
end;
theorem
L27: (for B18 being  Abelian  add-associative  right_zeroed  vector-distributive  scalar-distributive  scalar-associative  scalar-unital non  empty RLSStruct holds (for B19 being non  empty (Subset of B18) holds (for B20 being (Element of B19) holds (for B21 being (BinOp of B19) holds (for B22 being (Function of [: ( REAL ) , B19 :] , B19) holds ((B20 = ( 0. B18 ) & B21 = ( (the addF of B18) || B19 ) & B22 = ( (the Mult of B18) | [: ( REAL ) , B19 :] )) implies RLSStruct (# B19 , B20 , B21 , B22 #) is  Abelian  add-associative  right_zeroed  vector-distributive  scalar-distributive  scalar-associative  scalar-unital))))))
proof
let C11 being  Abelian  add-associative  right_zeroed  vector-distributive  scalar-distributive  scalar-associative  scalar-unital non  empty RLSStruct;
let C12 being non  empty (Subset of C11);
let C13 being (Element of C12);
let C14 being (BinOp of C12);
let C15 being (Function of [: ( REAL ) , C12 :] , C12);
set D6 = C12;
assume that
L28: C13 = ( 0. C11 )
and
L29: C14 = ( (the addF of C11) || C12 )
and
L30: C15 = ( (the Mult of C11) | [: ( REAL ) , C12 :] );
set D7 = RLSStruct (# D6 , C13 , C14 , C15 #);
L31:
now
let R1 being Real;
let C16 being (VECTOR of D7);
thus L32: ( R1 * C16 ) = ( (the Mult of C11) . [ R1 , C16 ] ) by L30 , FUNCT_1:49
.= ( (the Mult of C11) . (R1 , C16) );
end;
L33:
now
let C17 , C18 being (VECTOR of D7);
thus L34: ( C17 + C18 ) = ( (the addF of C11) . [ C17 , C18 ] ) by L29 , FUNCT_1:49
.= ( (the addF of C11) . (C17 , C18) );
end;
L35: D7 is  Abelian  add-associative  right_zeroed  vector-distributive  scalar-distributive  scalar-associative  scalar-unital
proof
set D8 = (the Mult of C11);
set D9 = (the addF of C11);
thus L36:now
let C19 , C20 being (VECTOR of D7);
reconsider D10 = C19 , D11 = C20 as (VECTOR of C11) by TARSKI:def 3;
thus L37: ( C19 + C20 ) = ( D10 + D11 ) by L33
.= ( D11 + D10 )
.= ( C20 + C19 ) by L33;
end;
L38:
now
let C21 , C22 , C23 being (VECTOR of D7);
reconsider D12 = C21 , D13 = C22 , D14 = C23 as (VECTOR of C11) by TARSKI:def 3;
thus L39: ( ( C21 + C22 ) + C23 ) = ( D9 . (( C21 + C22 ) , D14) ) by L33
.= ( ( D12 + D13 ) + D14 ) by L33
.= ( D12 + ( D13 + D14 ) ) by RLVECT_1:def 3
.= ( D9 . (D12 , ( C22 + C23 )) ) by L33
.= ( C21 + ( C22 + C23 ) ) by L33;
end;
thus L40: D7 is  add-associative by L38 , RLVECT_1:def 3;
L41:
now
let C24 being (VECTOR of D7);
reconsider D15 = C24 as (VECTOR of C11) by TARSKI:def 3;
thus L42: ( C24 + ( 0. D7 ) ) = ( D15 + ( 0. C11 ) ) by L28 , L33
.= C24 by RLVECT_1:def 4;
end;
thus L43: D7 is  right_zeroed by L41 , RLVECT_1:def 4;
thus L44:now
let C25 being  real number;
let C26 , C27 being (VECTOR of D7);
reconsider D16 = C25 as Real by XREAL_0:def 1;
reconsider D17 = C26 , D18 = C27 as (VECTOR of C11) by TARSKI:def 3;
L45: ( D16 * ( C26 + C27 ) ) = ( D8 . (D16 , ( C26 + C27 )) ) by L31
.= ( D16 * ( D17 + D18 ) ) by L33
.= ( ( D16 * D17 ) + ( D16 * D18 ) ) by RLVECT_1:def 5
.= ( D9 . (( D8 . (D16 , D17) ) , ( D16 * C27 )) ) by L31
.= ( D9 . (( D16 * C26 ) , ( D16 * C27 )) ) by L31
.= ( ( D16 * C26 ) + ( D16 * C27 ) ) by L33;
thus L46: ( C25 * ( C26 + C27 ) ) = ( ( C25 * C26 ) + ( C25 * C27 ) ) by L45;
end;
thus L47:now
let C28 , C29 being  real number;
let C30 being (VECTOR of D7);
reconsider D19 = C28 , D20 = C29 as Real by XREAL_0:def 1;
reconsider D21 = C30 as (VECTOR of C11) by TARSKI:def 3;
L48: ( ( D19 + D20 ) * C30 ) = ( ( D19 + D20 ) * D21 ) by L31
.= ( ( D19 * D21 ) + ( D20 * D21 ) ) by RLVECT_1:def 6
.= ( D9 . (( D8 . (D19 , D21) ) , ( D20 * C30 )) ) by L31
.= ( D9 . (( D19 * C30 ) , ( D20 * C30 )) ) by L31
.= ( ( D19 * C30 ) + ( D20 * C30 ) ) by L33;
thus L49: ( ( C28 + C29 ) * C30 ) = ( ( C28 * C30 ) + ( C29 * C30 ) ) by L48;
end;
thus L50:now
let C31 , C32 being  real number;
let C33 being (VECTOR of D7);
reconsider D22 = C31 , D23 = C32 as Real by XREAL_0:def 1;
reconsider D24 = C33 as (VECTOR of C11) by TARSKI:def 3;
L51: ( ( D22 * D23 ) * C33 ) = ( ( D22 * D23 ) * D24 ) by L31
.= ( D22 * ( D23 * D24 ) ) by RLVECT_1:def 7
.= ( D8 . (D22 , ( D23 * C33 )) ) by L31
.= ( D22 * ( D23 * C33 ) ) by L31;
thus L52: ( ( C31 * C32 ) * C33 ) = ( C31 * ( C32 * C33 ) ) by L51;
end;
let C34 being (VECTOR of D7);
reconsider D25 = C34 as (VECTOR of C11) by TARSKI:def 3;
thus L53: ( 1 * C34 ) = ( 1 * D25 ) by L31
.= C34 by RLVECT_1:def 8;
end;
thus L54: thesis by L35;
end;
theorem
L55: (for B23 being  Abelian  add-associative  right_zeroed  vector-distributive  scalar-distributive  scalar-associative  scalar-unital non  empty RLSStruct holds (for B24 being  add-closed  multi-closed non  empty (Subset of B23) holds (( 0. B23 ) in B24 implies RLSStruct (# B24 , ( In (( 0. B23 ) , B24) ) , ( add| (B24 , B23) ) , ( Mult_ B24 ) #) is  Abelian  add-associative  right_zeroed  vector-distributive  scalar-distributive  scalar-associative  scalar-unital)))
