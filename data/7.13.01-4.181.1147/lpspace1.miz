theorem
L63: (for B31 being non  empty RLSStruct holds (for B32 being  add-closed  multi-closed non  empty (Subset of B31) holds (for B33 being Real holds (for B34 being (VECTOR of B31) holds (for B35 being (VECTOR of RLSStruct (# B32 , ( In (( 0. B31 ) , B32) ) , ( add| (B32 , B31) ) , ( Mult_ B32 ) #)) holds (B35 = B34 implies ( B33 * B35 ) = ( B33 * B34 )))))))
proof
let C43 being non  empty RLSStruct;
let C44 being  add-closed  multi-closed non  empty (Subset of C43);
let C45 being Real;
let C46 being (VECTOR of C43);
let C47 being (VECTOR of RLSStruct (# C44 , ( In (( 0. C43 ) , C44) ) , ( add| (C44 , C43) ) , ( Mult_ C44 ) #));
assume L64: C47 = C46;
L65: [ C45 , C46 ] in [: ( REAL ) , C44 :] by L64 , ZFMISC_1:87;
thus L66: thesis by L65 , L64 , FUNCT_1:49;
end;
