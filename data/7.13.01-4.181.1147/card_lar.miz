begin
registration
cluster  cardinal  infinite ->  limit_ordinal for Ordinal;
coherence;
end;
registration
cluster non  empty  limit_ordinal ->  infinite for Ordinal;
coherence
proof
let C1 being Ordinal;
assume that
L2: C1 is non  empty  limit_ordinal;
assume L3: C1 is  finite;
L4: C1 in ( omega ) by L3 , CARD_1:61;
L5: ( {} ) in C1 by L2 , ORDINAL1:16 , XBOOLE_1:3;
L6: ( omega ) c= C1 by L5 , L2 , ORDINAL1:def 11;
L7: C1 in C1 by L6 , L4;
thus L8: contradiction by L7;
end;
end;
registration
cluster non  limit_cardinal -> non  countable for Aleph;
coherence
proof
let C2 being Aleph;
assume that
L10: C2 is non  limit_cardinal;
assume L11: C2 is  countable;
L12: ( card C2 ) c= ( omega ) by L11 , CARD_3:def 14;
L13: ( card C2 ) = ( omega )
proof
assume L14: ( card C2 ) <> ( omega );
L15: ( card C2 ) in ( omega ) by L14 , L12 , CARD_1:3;
thus L16: contradiction by L15;
end;
thus L17: contradiction by L13 , L10 , CARD_1:def 2;
end;
end;
registration
cluster  regular non  countable for Aleph;
existence
proof
set D1 = the non  limit_cardinal Aleph;
take D1;
thus L19: thesis;
end;
end;
definition
let R1 being  limit_ordinal  infinite Ordinal;
let R10 being set;
pred R10 is_unbounded_in R1
means
:L21: (R10 c= R1 & ( sup R10 ) = R1)
;pred R10 is_closed_in R1
means
:L22: (R10 c= R1 & (for R2 being  limit_ordinal  infinite Ordinal holds (R2 in R1 implies (( sup ( R10 /\ R2 ) ) = R2 implies R2 in R10))))
;end;
definition
let R1 being  limit_ordinal  infinite Ordinal;
let R10 being set;
pred R10 is_club_in R1
means
:L24: (R10 is_closed_in R1 & R10 is_unbounded_in R1)
;end;
definition
let R1 being  limit_ordinal  infinite Ordinal;
let R11 being (Subset of R1);
attr R11 is  unbounded
means
:L26: ( sup R11 ) = R1;
attr R11 is  closed
means
:L27: (for R2 being  limit_ordinal  infinite Ordinal holds (R2 in R1 implies (( sup ( R11 /\ R2 ) ) = R2 implies R2 in R11)));
end;
notation
let R1 being  limit_ordinal  infinite Ordinal;
let R11 being (Subset of R1);
antonym R11 is  bounded for R11 is  unbounded;
end;
theorem
L30: (for R1 being  limit_ordinal  infinite Ordinal holds (for R11 being (Subset of R1) holds (R11 is_club_in R1 iff R11 is  closed  unbounded)))
proof
let R1 being  limit_ordinal  infinite Ordinal;
let R11 being (Subset of R1);
thus L31: (R11 is_club_in R1 implies R11 is  closed  unbounded)
proof
assume L32: R11 is_club_in R1;
L33: R11 is_unbounded_in R1 by L32 , L24;
L34: ( sup R11 ) = R1 by L33 , L21;
L35: R11 is_closed_in R1 by L32 , L24;
L36: (for R2 being  limit_ordinal  infinite Ordinal holds (R2 in R1 implies (( sup ( R11 /\ R2 ) ) = R2 implies R2 in R11))) by L35 , L22;
thus L37: thesis by L36 , L34 , L26 , L27;
end;

assume L38: R11 is  closed  unbounded;
L39: ( sup R11 ) = R1 by L38 , L26;
L40: R11 is_unbounded_in R1 by L39 , L21;
L41: (for R2 being  limit_ordinal  infinite Ordinal holds (R2 in R1 implies (( sup ( R11 /\ R2 ) ) = R2 implies R2 in R11))) by L38 , L27;
L42: R11 is_closed_in R1 by L41 , L22;
thus L43: thesis by L42 , L40 , L24;
end;
theorem
L44: (for R1 being  limit_ordinal  infinite Ordinal holds (for R11 being (Subset of R1) holds R11 c= ( sup R11 )))
proof
let R1 being  limit_ordinal  infinite Ordinal;
let R11 being (Subset of R1);
let C3 being set;
assume L45: C3 in R11;
thus L46: thesis by L45 , ORDINAL2:19;
end;
theorem
L47: (for R1 being  limit_ordinal  infinite Ordinal holds (for R11 being (Subset of R1) holds ((R11 is non  empty & (for R3 being Ordinal holds (R3 in R11 implies (ex R4 being Ordinal st (R4 in R11 & R3 in R4))))) implies ( sup R11 ) is  limit_ordinal  infinite  limit_ordinal  infinite  limit_ordinal  infinite  limit_ordinal  infinite Ordinal)))
proof
let R1 being  limit_ordinal  infinite Ordinal;
let R11 being (Subset of R1);
assume L48: R11 is non  empty;
L49: (ex B1 being set st B1 in R11) by L48 , XBOOLE_0:def 1;
assume L50: (for R3 being Ordinal holds (R3 in R11 implies (ex R4 being Ordinal st (R4 in R11 & R3 in R4))));
L51: (for R9 being Ordinal holds (R9 in ( sup R11 ) implies ( succ R9 ) in ( sup R11 )))
proof
let R9 being Ordinal;
assume L52: R9 in ( sup R11 );
consider R5 being Ordinal such that L53: R5 in R11 and L54: R9 c= R5 by L52 , ORDINAL2:21;
consider R4 being Ordinal such that L55: R4 in R11 and L56: R5 in R4 by L50 , L53;
L57: R9 in R4 by L54 , L56 , ORDINAL1:12;
L58: ( succ R9 ) c= R4 by L57 , ORDINAL1:21;
L59: R4 in ( sup R11 ) by L55 , ORDINAL2:19;
thus L60: thesis by L59 , L58 , ORDINAL1:12;
end;
L61: R11 c= ( sup R11 ) by L44;
reconsider D2 = ( sup R11 ) as non  empty  limit_ordinal Ordinal by L61 , L51 , L49 , ORDINAL1:28;
L62: D2 is  limit_ordinal  infinite  limit_ordinal  infinite  limit_ordinal  infinite  limit_ordinal  infinite Ordinal;
thus L63: thesis by L62;
end;
theorem
L64: (for R1 being  limit_ordinal  infinite Ordinal holds (for R11 being (Subset of R1) holds (R11 is  bounded iff (ex R3 being Ordinal st (R3 in R1 & R11 c= R3)))))
proof
let R1 being  limit_ordinal  infinite Ordinal;
let R11 being (Subset of R1);
L65: (( sup R11 ) c< R1 iff (( sup R11 ) c= R1 & ( sup R11 ) <> R1)) by XBOOLE_0:def 8;
L66: R11 c= ( sup R11 ) by L44;
L67: R1 = ( sup R1 ) by ORDINAL2:18;
L68: (( sup R11 ) <> R1 iff ( sup R11 ) in R1) by L67 , L65 , ORDINAL1:11 , ORDINAL2:22;
thus L69: (R11 is  bounded implies (ex R3 being Ordinal st (R3 in R1 & R11 c= R3))) by L68 , L66 , L26;
given R3 being Ordinal such that
L70: R3 in R1
and
L71: R11 c= R3;

L72: (R11 is  bounded iff ( sup R11 ) <> R1) by L26;
L73: ( sup R11 ) c= ( sup R3 ) by L71 , ORDINAL2:22;
L74: ( sup R11 ) c= R3 by L73 , ORDINAL2:18;
thus L75: thesis by L74 , L72 , L70 , ORDINAL1:12;
end;
theorem
L76: (for R1 being  limit_ordinal  infinite Ordinal holds (for R2 being  limit_ordinal  infinite Ordinal holds (for R11 being (Subset of R1) holds ((not ( sup ( R11 /\ R2 ) ) = R2) implies (ex R3 being Ordinal st (R3 in R2 & ( R11 /\ R2 ) c= R3))))))
proof
let R1 being  limit_ordinal  infinite Ordinal;
let R2 being  limit_ordinal  infinite Ordinal;
let R11 being (Subset of R1);
reconsider D3 = ( R11 /\ R2 ) as (Subset of R2) by XBOOLE_1:17;
assume L77: (not ( sup ( R11 /\ R2 ) ) = R2);
L78: D3 is  bounded by L77 , L26;
consider R3 being Ordinal such that L79: (R3 in R2 & D3 c= R3) by L78 , L64;
take R3;
thus L80: thesis by L79;
end;
theorem
L81: (for R1 being  limit_ordinal  infinite Ordinal holds (for R11 being (Subset of R1) holds (R11 is  unbounded iff (for R3 being Ordinal holds (R3 in R1 implies (ex R9 being Ordinal st (R9 in R11 & R3 c= R9)))))))
proof
let R1 being  limit_ordinal  infinite Ordinal;
let R11 being (Subset of R1);
thus L82: (R11 is  unbounded implies (for R3 being Ordinal holds (R3 in R1 implies (ex R9 being Ordinal st (R9 in R11 & R3 c= R9)))))
proof
assume L83: R11 is  unbounded;
let R3 being Ordinal;
assume L84: R3 in R1;
L85: (not R11 c= R3) by L84 , L83 , L64;
consider C4 being set such that L86: C4 in R11 and L87: (not C4 in R3) by L85 , TARSKI:def 3;
reconsider D4 = C4 as (Element of R1) by L86;
take D4;
thus L88: D4 in R11 by L86;
thus L89: thesis by L87 , ORDINAL1:16;
end;

assume L90: (for R3 being Ordinal holds (R3 in R1 implies (ex R9 being Ordinal st (R9 in R11 & R3 c= R9))));
assume L91: R11 is  bounded;
consider R3 being Ordinal such that L92: R3 in R1 and L93: R11 c= R3 by L91 , L64;
consider R9 being Ordinal such that L94: R9 in R11 and L95: R3 c= R9 by L90 , L92;
L96: R11 c= R9 by L93 , L95 , XBOOLE_1:1;
L97: R9 in R9 by L96 , L94;
thus L98: contradiction by L97;
end;
theorem
L99: (for R1 being  limit_ordinal  infinite Ordinal holds (for R11 being (Subset of R1) holds (R11 is  unbounded implies R11 is non  empty)))
proof
let R1 being  limit_ordinal  infinite Ordinal;
let R11 being (Subset of R1);
set D5 = the (Element of R1);
assume L100: R11 is  unbounded;
L101: (ex R9 being Ordinal st (R9 in R11 & D5 c= R9)) by L100 , L81;
thus L102: thesis by L101;
end;
theorem
L103: (for R1 being  limit_ordinal  infinite Ordinal holds (for R3 being Ordinal holds (for R11 being (Subset of R1) holds ((R11 is  unbounded & R3 in R1) implies (ex B2 being (Element of R1) st B2 in { B3 where B3 is (Element of R1) : (B3 in R11 & R3 in B3) })))))
proof
let R1 being  limit_ordinal  infinite Ordinal;
let R3 being Ordinal;
let R11 being (Subset of R1);
assume L104: R11 is  unbounded;
assume L105: R3 in R1;
L106: ( succ R3 ) in R1 by L105 , ORDINAL1:28;
consider R5 being Ordinal such that L107: R5 in R11 and L108: ( succ R3 ) c= R5 by L106 , L104 , L81;
reconsider D6 = R5 as (Element of R1) by L107;
take D6;
L109: R3 in R5 by L108 , ORDINAL1:21;
thus L110: thesis by L109 , L107;
end;
definition
let R1 being  limit_ordinal  infinite Ordinal;
let R11 being (Subset of R1);
let R3 being Ordinal;
assume L111: R11 is  unbounded;
assume L112: R3 in R1;
func LBound (R3 , R11) -> (Element of R11) equals 
:L113: ( inf { B4 where B4 is (Element of R1) : (B4 in R11 & R3 in B4) } );
coherence
proof
defpred S1[ set ] means ($1 in R11 & R3 in $1);
set D7 = { B5 where B5 is (Element of R1) : S1[ B5 ] };
L114: (ex B6 being (Element of R1) st B6 in D7) by L111 , L112 , L103;
L115: ( inf D7 ) in D7 by L114 , ORDINAL2:17;
L116: S1[ ( inf D7 ) ] from CARD_FIL:sch 1(L115);
thus L117: thesis by L116;
end;
end;
theorem
L119: (for R1 being  limit_ordinal  infinite Ordinal holds (for R3 being Ordinal holds (for R11 being (Subset of R1) holds ((R11 is  unbounded & R3 in R1) implies (( LBound (R3 , R11) ) in R11 & R3 in ( LBound (R3 , R11) ))))))
proof
let R1 being  limit_ordinal  infinite Ordinal;
let R3 being Ordinal;
let R11 being (Subset of R1);
assume L120: R11 is  unbounded;
defpred S2[ set ] means ($1 in R11 & R3 in $1);
set D8 = { B7 where B7 is (Element of R1) : S2[ B7 ] };
L121: (for B8 being set holds (B8 in D8 implies R3 in B8))
proof
let C5 being set;
assume L122: C5 in D8;
L123: S2[ C5 ] from CARD_FIL:sch 1(L122);
thus L124: thesis by L123;
end;
L125: D8 is (Subset of R1) from DOMAIN_1:sch 7;
L126: (( inf D8 ) = ( meet ( On D8 ) ) & ( On D8 ) = D8) by L125 , ORDINAL2:def 2 , ORDINAL3:6;
assume L127: R3 in R1;
L128: R11 is non  empty by L120 , L99;
thus L129: ( LBound (R3 , R11) ) in R11 by L128;
L130: (ex B9 being (Element of R1) st B9 in D8) by L120 , L127 , L103;
L131: R3 in ( meet D8 ) by L130 , L121 , SETFAM_1:def 1;
thus L132: thesis by L131 , L120 , L127 , L126 , L113;
end;
theorem
L133: (for R1 being  limit_ordinal  infinite Ordinal holds ( [#] R1 ) is  closed  unbounded)
proof
let R1 being  limit_ordinal  infinite Ordinal;
thus L134: ( [#] R1 ) is  closed
proof
let R2 being  limit_ordinal  infinite Ordinal;
assume that
L135: R2 in R1;
assume L136: ( sup ( ( [#] R1 ) /\ R2 ) ) = R2;
thus L137: thesis by L135;
end;

L138: ( sup ( [#] R1 ) ) = R1 by ORDINAL2:18;
thus L139: thesis by L138 , L26;
end;
theorem
L140: (for R1 being  limit_ordinal  infinite Ordinal holds (for R3 being Ordinal holds (for R11 being (Subset of R1) holds ((R3 in R1 & R11 is  closed  unbounded) implies ( R11 \ R3 ) is  closed  unbounded))))
proof
let R1 being  limit_ordinal  infinite Ordinal;
let R3 being Ordinal;
let R11 being (Subset of R1);
assume L141: R3 in R1;
assume L142: R11 is  closed  unbounded;
thus L143: ( R11 \ R3 ) is  closed
proof
let R2 being  limit_ordinal  infinite Ordinal;
assume that
L144: R2 in R1;
assume L145: ( sup ( ( R11 \ R3 ) /\ R2 ) ) = R2;
L146: ( sup ( R11 /\ R2 ) ) c= ( sup R2 ) by ORDINAL2:22 , XBOOLE_1:17;
L147: ( sup ( R11 /\ R2 ) ) c= R2 by L146 , ORDINAL2:18;
L148: ( ( R11 \ R3 ) /\ R2 ) c= ( R11 /\ R2 ) by XBOOLE_1:26 , XBOOLE_1:36;
L149: R2 c= ( sup ( R11 /\ R2 ) ) by L148 , L145 , ORDINAL2:22;
L150: ( sup ( R11 /\ R2 ) ) = R2 by L149 , L147 , XBOOLE_0:def 10;
L151: R2 in R11 by L150 , L142 , L144 , L27;
assume L152: (not R2 in ( R11 \ R3 ));
L153: R2 in R3 by L152 , L151 , XBOOLE_0:def 5;
L154: R2 c= R3 by L153 , ORDINAL1:def 2;
L155: ( R11 \ R2 ) misses R2 by XBOOLE_1:79;
L156: ( R11 \ R3 ) misses R2 by L155 , L154 , XBOOLE_1:34 , XBOOLE_1:63;
L157: ( ( R11 \ R3 ) /\ R2 ) = ( {} ) by L156 , XBOOLE_0:def 7;
thus L158: contradiction by L157 , L145 , ORDINAL2:18;
end;

L159: (for R4 being Ordinal holds (R4 in R1 implies (ex R9 being Ordinal st (R9 in ( R11 \ R3 ) & R4 c= R9))))
proof
let R4 being Ordinal;
assume that
L160: R4 in R1;
per cases  by ORDINAL1:16;
suppose L161: R3 in R4;

consider R8 being Ordinal such that L162: R8 in R11 and L163: R4 c= R8 by L142 , L160 , L81;
take R8;
L164: R3 in R8 by L161 , L163;
thus L165: R8 in ( R11 \ R3 ) by L164 , L162 , XBOOLE_0:def 5;
thus L166: thesis by L163;
end;
suppose L167: R4 c= R3;

consider R8 being Ordinal such that L168: R8 in R11 and L169: R3 c= R8 by L141 , L142 , L81;
take R8;
L170: (not R8 in R3) by L169 , ORDINAL1:5;
thus L171: R8 in ( R11 \ R3 ) by L170 , L168 , XBOOLE_0:def 5;
thus L172: thesis by L167 , L169 , XBOOLE_1:1;
end;
end;
thus L174: thesis by L159 , L81;
end;
theorem
L175: (for R1 being  limit_ordinal  infinite Ordinal holds (for R3 being Ordinal holds (R3 in R1 implies ( R1 \ R3 ) is  closed  unbounded)))
proof
let R1 being  limit_ordinal  infinite Ordinal;
let R3 being Ordinal;
assume L176: R3 in R1;
L177: ( [#] R1 ) is  closed  unbounded by L133;
thus L178: thesis by L177 , L176 , L140;
end;
definition
let R1 being  limit_ordinal  infinite Ordinal;
let R11 being (Subset of R1);
attr R11 is  stationary
means
:L179: (for B10 being (Subset of R1) holds (B10 is  closed  unbounded implies ( R11 /\ B10 ) is non  empty));
end;
theorem
L181: (for R1 being  limit_ordinal  infinite Ordinal holds (for B11 , B12 being (Subset of R1) holds ((B11 is  stationary & B11 c= B12) implies B12 is  stationary)))
proof
let R1 being  limit_ordinal  infinite Ordinal;
let C6 , C7 being (Subset of R1);
assume L182: C6 is  stationary;
assume L183: C6 c= C7;
let C8 being (Subset of R1);
assume L184: C8 is  closed  unbounded;
L185: ( C6 /\ C8 ) is non  empty by L184 , L182 , L179;
L186: (ex B13 being set st B13 in ( C6 /\ C8 )) by L185 , XBOOLE_0:def 1;
L187: ( C6 /\ C8 ) c= ( C7 /\ C8 ) by L183 , XBOOLE_1:26;
thus L188: thesis by L187 , L186;
end;
definition
let R1 being  limit_ordinal  infinite Ordinal;
let C9 being set;
pred C9 is_stationary_in R1
means
:L189: (C9 c= R1 & (for B14 being (Subset of R1) holds (B14 is  closed  unbounded implies ( C9 /\ B14 ) is non  empty)))
;end;
theorem
L191: (for R1 being  limit_ordinal  infinite Ordinal holds (for B15 , B16 being set holds ((B15 is_stationary_in R1 & B15 c= B16 & B16 c= R1) implies B16 is_stationary_in R1)))
proof
let R1 being  limit_ordinal  infinite Ordinal;
let C10 , C11 being set;
assume L192: C10 is_stationary_in R1;
reconsider D9 = C10 as (Subset of R1) by L192 , L189;
assume L193: C10 c= C11;
assume L194: C11 c= R1;
reconsider D10 = C11 as (Subset of R1) by L194;
L195: (for B17 being (Subset of R1) holds (B17 is  closed  unbounded implies ( C10 /\ B17 ) is non  empty)) by L192 , L189;
L196: D9 is  stationary by L195 , L179;
L197: D10 is  stationary by L196 , L193 , L181;
L198: (for B18 being (Subset of R1) holds (B18 is  closed  unbounded implies ( D10 /\ B18 ) is non  empty)) by L197 , L179;
thus L199: thesis by L198 , L189;
end;
definition
let C12 being set;
let C13 being (Subset-Family of C12);
redefine mode Element of C13
 -> (Subset of C12);
coherence
proof
let C14 being (Element of C13);
per cases ;
suppose L200: C13 is  empty;

L201: C14 is  empty by L200 , SUBSET_1:def 1;
thus L202: thesis by L201 , SUBSET_1:1;
end;
suppose L203: C13 is non  empty;

L204: C14 in C13 by L203;
thus L205: thesis by L204;
end;
end;
end;
theorem
L208: (for R1 being  limit_ordinal  infinite Ordinal holds (for R11 being (Subset of R1) holds (R11 is  stationary implies R11 is  unbounded)))
proof
let R1 being  limit_ordinal  infinite Ordinal;
let R11 being (Subset of R1);
assume L209: R11 is  stationary;
assume L210: R11 is  bounded;
consider R3 being Ordinal such that L211: R3 in R1 and L212: R11 c= R3 by L210 , L64;
L213: ( R1 \ R3 ) misses R3 by XBOOLE_1:79;
L214: ( R1 \ R3 ) misses R11 by L213 , L212 , XBOOLE_1:63;
L215: ( ( R1 \ R3 ) /\ R11 ) = ( {} ) by L214 , XBOOLE_0:def 7;
L216: ( R1 \ R3 ) is  closed  unbounded by L211 , L175;
thus L217: contradiction by L216 , L209 , L215 , L179;
end;
definition
let R1 being  limit_ordinal  infinite Ordinal;
let R11 being (Subset of R1);
func limpoints R11 -> (Subset of R1) equals 
{ B19 where B19 is (Element of R1) : (B19 is  infinite  limit_ordinal & ( sup ( R11 /\ B19 ) ) = B19) };
coherence
proof
defpred S3[ set ] means ($1 is  infinite  limit_ordinal & ( sup ( R11 /\ $1 ) ) = $1);
thus L218: { B20 where B20 is (Element of R1) : S3[ B20 ] } is (Subset of R1) from DOMAIN_1:sch 7;
end;
end;
theorem
L220: (for R1 being  limit_ordinal  infinite Ordinal holds (for R3 being Ordinal holds (for R5 being Ordinal holds (for R11 being (Subset of R1) holds (( R11 /\ R5 ) c= R3 implies ( R5 /\ ( limpoints R11 ) ) c= ( succ R3 ))))))
proof
let R1 being  limit_ordinal  infinite Ordinal;
