theorem
L55: (for B9 , B10 being (PartFunc of ( REAL ) , ( REAL )) holds (for B11 being non  empty (Subset of ( REAL )) holds ( ( B9 || B11 ) (#) ( B10 || B11 ) ) = ( ( B9 (#) B10 ) || B11 )))
proof
let C13 , C14 being (PartFunc of ( REAL ) , ( REAL ));
let C15 being non  empty (Subset of ( REAL ));
L56: ( dom ( ( C13 (#) C14 ) || C15 ) ) = ( ( dom ( C13 (#) C14 ) ) /\ C15 ) by RELAT_1:61
.= ( ( ( dom C13 ) /\ ( dom C14 ) ) /\ C15 ) by VALUED_1:def 4;
L57: ( dom ( ( C13 || C15 ) (#) ( C14 || C15 ) ) ) = ( ( dom ( C13 | C15 ) ) /\ ( dom ( C14 || C15 ) ) ) by VALUED_1:def 4
.= ( ( ( dom C13 ) /\ C15 ) /\ ( dom ( C14 | C15 ) ) ) by RELAT_1:61
.= ( ( ( dom C13 ) /\ C15 ) /\ ( ( dom C14 ) /\ C15 ) ) by RELAT_1:61
.= ( ( ( ( dom C13 ) /\ C15 ) /\ C15 ) /\ ( dom C14 ) ) by XBOOLE_1:16
.= ( ( ( dom C13 ) /\ ( C15 /\ C15 ) ) /\ ( dom C14 ) ) by XBOOLE_1:16
.= ( ( ( dom C13 ) /\ C15 ) /\ ( dom C14 ) );
L58: ( dom ( ( C13 || C15 ) (#) ( C14 || C15 ) ) ) = ( dom ( ( C13 (#) C14 ) || C15 ) ) by L57 , L56 , XBOOLE_1:16;
L59: (for B12 being (Element of C15) holds (B12 in ( dom ( ( C13 || C15 ) (#) ( C14 || C15 ) ) ) implies ( ( ( C13 || C15 ) (#) ( C14 || C15 ) ) . B12 ) = ( ( ( C13 (#) C14 ) || C15 ) . B12 )))
proof
let C16 being (Element of C15);
L60: ( ( ( C13 || C15 ) (#) ( C14 || C15 ) ) . C16 ) = ( ( ( C13 | C15 ) . C16 ) * ( ( C14 | C15 ) . C16 ) ) by VALUED_1:5;
assume L61: C16 in ( dom ( ( C13 || C15 ) (#) ( C14 || C15 ) ) );
L62: C16 in ( ( dom ( C13 || C15 ) ) /\ ( dom ( C14 || C15 ) ) ) by L61 , VALUED_1:def 4;
L63: C16 in ( dom ( C13 || C15 ) ) by L62 , XBOOLE_0:def 4;
L64: C16 in ( dom ( C14 || C15 ) ) by L62 , XBOOLE_0:def 4;
L65: ( ( ( C13 (#) C14 ) || C15 ) . C16 ) = ( ( C13 (#) C14 ) . C16 ) by L58 , L61 , FUNCT_1:47
.= ( ( C13 . C16 ) * ( C14 . C16 ) ) by VALUED_1:5
.= ( ( ( C13 | C15 ) . C16 ) * ( C14 . C16 ) ) by L63 , FUNCT_1:47;
thus L66: thesis by L65 , L64 , L60 , FUNCT_1:47;
end;
thus L67: thesis by L59 , L57 , L56 , PARTFUN1:5 , XBOOLE_1:16;
end;
theorem
L68: (for B13 , B14 being (PartFunc of ( REAL ) , ( REAL )) holds (for B15 being non  empty (Subset of ( REAL )) holds ( ( B13 + B14 ) || B15 ) = ( ( B13 || B15 ) + ( B14 || B15 ) )))
proof
let C17 , C18 being (PartFunc of ( REAL ) , ( REAL ));
let C19 being non  empty (Subset of ( REAL ));
L69: ( dom ( ( C17 || C19 ) + ( C18 || C19 ) ) ) = ( ( dom ( C17 | C19 ) ) /\ ( dom ( C18 || C19 ) ) ) by VALUED_1:def 1
.= ( ( ( dom C17 ) /\ C19 ) /\ ( dom ( C18 | C19 ) ) ) by RELAT_1:61
.= ( ( ( dom C17 ) /\ C19 ) /\ ( ( dom C18 ) /\ C19 ) ) by RELAT_1:61
.= ( ( dom C17 ) /\ ( C19 /\ ( ( dom C18 ) /\ C19 ) ) ) by XBOOLE_1:16
.= ( ( dom C17 ) /\ ( ( dom C18 ) /\ ( C19 /\ C19 ) ) ) by XBOOLE_1:16
.= ( ( dom C17 ) /\ ( ( dom C18 ) /\ C19 ) );
L70: ( dom ( ( C17 + C18 ) || C19 ) ) = ( ( dom ( C17 + C18 ) ) /\ C19 ) by RELAT_1:61
.= ( ( ( dom C17 ) /\ ( dom C18 ) ) /\ C19 ) by VALUED_1:def 1;
L71: ( dom ( ( C17 + C18 ) || C19 ) ) = ( dom ( ( C17 || C19 ) + ( C18 || C19 ) ) ) by L70 , L69 , XBOOLE_1:16;
L72: (for B16 being (Element of C19) holds (B16 in ( dom ( ( C17 + C18 ) || C19 ) ) implies ( ( ( C17 + C18 ) || C19 ) . B16 ) = ( ( ( C17 || C19 ) + ( C18 || C19 ) ) . B16 )))
proof
let C20 being (Element of C19);
assume L73: C20 in ( dom ( ( C17 + C18 ) || C19 ) );
L74: C20 in ( ( dom ( C17 + C18 ) ) /\ C19 ) by L73 , RELAT_1:61;
L75: C20 in ( dom ( C17 + C18 ) ) by L74 , XBOOLE_0:def 4;
L76: C20 in ( ( dom ( C17 || C19 ) ) /\ ( dom ( C18 || C19 ) ) ) by L71 , L73 , VALUED_1:def 1;
L77: C20 in ( dom ( C17 || C19 ) ) by L76 , XBOOLE_0:def 4;
L78: ( ( ( C17 + C18 ) || C19 ) . C20 ) = ( ( C17 + C18 ) . C20 ) by L73 , FUNCT_1:47
.= ( ( C17 . C20 ) + ( C18 . C20 ) ) by L75 , VALUED_1:def 1;
L79: C20 in ( dom ( C18 || C19 ) ) by L76 , XBOOLE_0:def 4;
L80: ( ( ( C17 || C19 ) + ( C18 || C19 ) ) . C20 ) = ( ( ( C17 | C19 ) . C20 ) + ( ( C18 || C19 ) . C20 ) ) by L71 , L73 , VALUED_1:def 1
.= ( ( C17 . C20 ) + ( ( C18 | C19 ) . C20 ) ) by L77 , FUNCT_1:47;
thus L81: thesis by L80 , L78 , L79 , FUNCT_1:47;
end;
thus L82: thesis by L72 , L70 , L69 , PARTFUN1:5 , XBOOLE_1:16;
end;
