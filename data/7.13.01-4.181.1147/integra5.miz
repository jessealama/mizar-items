theorem
L1: (for B1 , B2 , B3 being (FinSequence of ( REAL )) holds (for R8 being Real holds (for R9 being Real holds (((B2 = ( <* R8 *> ^ B1 ) or B2 = ( B1 ^ <* R8 *> )) & (B3 = ( <* R9 *> ^ B1 ) or B3 = ( B1 ^ <* R9 *> ))) implies ( Sum ( B2 - B3 ) ) = ( R8 - R9 )))))
proof
let C1 , C2 , C3 being (FinSequence of ( REAL ));
let R8 being Real;
let R9 being Real;
assume that
L2: (C2 = ( <* R8 *> ^ C1 ) or C2 = ( C1 ^ <* R8 *> ))
and
L3: (C3 = ( <* R9 *> ^ C1 ) or C3 = ( C1 ^ <* R9 *> ));
L4: ( len C2 ) = ( ( len C1 ) + ( len <* R8 *> ) ) by L2 , FINSEQ_1:22;
L5: ( len C2 ) = ( ( len C1 ) + 1 ) by L4 , FINSEQ_1:39;
L6: ( len C3 ) = ( ( len <* R9 *> ) + ( len C1 ) ) by L3 , FINSEQ_1:22;
L7: ( len C3 ) = ( 1 + ( len C1 ) ) by L6 , FINSEQ_1:39;
L8: ( C2 - C3 ) = ( ( diffreal ) .: (C2 , C3) ) by RVSUM_1:def 6;
L9: ( len C2 ) = ( len ( C2 - C3 ) ) by L8 , L5 , L7 , FINSEQ_2:72;
L10: (for R2 being (Element of ( NAT )) holds (R2 in ( dom C2 ) implies ( ( C2 - C3 ) . R2 ) = ( ( C2 /. R2 ) - ( C3 /. R2 ) )))
proof
let R2 being (Element of ( NAT ));
assume L11: R2 in ( dom C2 );
L12: ( C2 . R2 ) = ( C2 /. R2 ) by L11 , PARTFUN1:def 6;
L13: R2 in ( Seg ( len C2 ) ) by L11 , FINSEQ_1:def 3;
L14: R2 in ( dom C3 ) by L13 , L5 , L7 , FINSEQ_1:def 3;
L15: ( C3 . R2 ) = ( C3 /. R2 ) by L14 , PARTFUN1:def 6;
L16: R2 in ( dom ( C2 - C3 ) ) by L9 , L13 , FINSEQ_1:def 3;
thus L17: thesis by L16 , L12 , L15 , VALUED_1:13;
end;
L18: ( Sum ( C2 - C3 ) ) = ( ( Sum C2 ) - ( Sum C3 ) ) by L10 , L5 , L7 , L9 , INTEGRA1:22;
L19: (( Sum C2 ) = ( R8 + ( Sum C1 ) ) & ( Sum C3 ) = ( ( Sum C1 ) + R9 )) by L2 , L3 , RVSUM_1:74 , RVSUM_1:76;
thus L20: thesis by L19 , L18;
end;
theorem
L21: (for B4 , B5 being (FinSequence of ( REAL )) holds (( len B4 ) = ( len B5 ) implies (( len ( B4 + B5 ) ) = ( len B4 ) & ( len ( B4 - B5 ) ) = ( len B4 ) & ( Sum ( B4 + B5 ) ) = ( ( Sum B4 ) + ( Sum B5 ) ) & ( Sum ( B4 - B5 ) ) = ( ( Sum B4 ) - ( Sum B5 ) ))))
proof
let C4 , C5 being (FinSequence of ( REAL ));
assume L22: ( len C4 ) = ( len C5 );
L23: ( C4 + C5 ) = ( ( addreal ) .: (C4 , C5) ) by RVSUM_1:def 4;
thus L24: ( len C4 ) = ( len ( C4 + C5 ) ) by L23 , L22 , FINSEQ_2:72;
L25: ( C4 - C5 ) = ( ( diffreal ) .: (C4 , C5) ) by RVSUM_1:def 6;
thus L26: ( len C4 ) = ( len ( C4 - C5 ) ) by L25 , L22 , FINSEQ_2:72;
L27: (for R2 being (Element of ( NAT )) holds (R2 in ( dom C4 ) implies ( ( C4 + C5 ) . R2 ) = ( ( C4 /. R2 ) + ( C5 /. R2 ) )))
proof
let R2 being (Element of ( NAT ));
assume L28: R2 in ( dom C4 );
L29: ( C4 . R2 ) = ( C4 /. R2 ) by L28 , PARTFUN1:def 6;
L30: R2 in ( Seg ( len C4 ) ) by L28 , FINSEQ_1:def 3;
L31: R2 in ( dom C5 ) by L30 , L22 , FINSEQ_1:def 3;
L32: ( C5 . R2 ) = ( C5 /. R2 ) by L31 , PARTFUN1:def 6;
L33: R2 in ( dom ( C4 + C5 ) ) by L24 , L30 , FINSEQ_1:def 3;
thus L34: thesis by L33 , L29 , L32 , VALUED_1:def 1;
end;
thus L35: ( Sum ( C4 + C5 ) ) = ( ( Sum C4 ) + ( Sum C5 ) ) by L27 , L22 , L24 , INTEGRA1:21;
L36: (for R2 being (Element of ( NAT )) holds (R2 in ( dom C4 ) implies ( ( C4 - C5 ) . R2 ) = ( ( C4 /. R2 ) - ( C5 /. R2 ) )))
proof
let R2 being (Element of ( NAT ));
assume L37: R2 in ( dom C4 );
L38: ( C4 . R2 ) = ( C4 /. R2 ) by L37 , PARTFUN1:def 6;
L39: R2 in ( Seg ( len C4 ) ) by L37 , FINSEQ_1:def 3;
L40: R2 in ( dom C5 ) by L39 , L22 , FINSEQ_1:def 3;
L41: ( C5 . R2 ) = ( C5 /. R2 ) by L40 , PARTFUN1:def 6;
L42: R2 in ( dom ( C4 - C5 ) ) by L26 , L39 , FINSEQ_1:def 3;
thus L43: thesis by L42 , L38 , L41 , VALUED_1:13;
end;
thus L44: thesis by L36 , L22 , L26 , INTEGRA1:22;
end;
theorem
L45: (for B6 , B7 being (FinSequence of ( REAL )) holds ((( len B6 ) = ( len B7 ) & (for R1 being (Element of ( NAT )) holds (R1 in ( dom B6 ) implies ( B6 . R1 ) <= ( B7 . R1 )))) implies ( Sum B6 ) <= ( Sum B7 )))
proof
let C6 , C7 being (FinSequence of ( REAL ));
assume that
L46: ( len C6 ) = ( len C7 )
and
L47: (for R1 being (Element of ( NAT )) holds (R1 in ( dom C6 ) implies ( C6 . R1 ) <= ( C7 . R1 )));
reconsider D1 = C6 as (Element of ( ( len C6 ) -tuples_on ( REAL ) )) by FINSEQ_2:92;
reconsider D2 = C7 as (Element of ( ( len C6 ) -tuples_on ( REAL ) )) by L46 , FINSEQ_2:92;
L48: (for B8 being Nat holds (B8 in ( Seg ( len C6 ) ) implies ( D1 . B8 ) <= ( D2 . B8 )))
proof
let C8 being Nat;
assume L49: C8 in ( Seg ( len C6 ) );
L50: C8 in ( dom C6 ) by L49 , FINSEQ_1:def 3;
thus L51: thesis by L50 , L47;
end;
thus L52: thesis by L48 , RVSUM_1:82;
end;
