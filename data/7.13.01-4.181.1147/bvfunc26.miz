begin
definition
let C1 , C2 being  boolean-valued Function;
func C1 'nand' C2 -> Function means 
:L1: (( dom it ) = ( ( dom C1 ) /\ ( dom C2 ) ) & (for B1 being set holds (B1 in ( dom it ) implies ( it . B1 ) = ( ( C1 . B1 ) 'nand' ( C2 . B1 ) ))));
existence
proof
deffunc H1(set) = ( ( C1 . $1 ) 'nand' ( C2 . $1 ) );
consider C3 being Function such that L2: (( dom C3 ) = ( ( dom C1 ) /\ ( dom C2 ) ) & (for B2 being set holds (B2 in ( ( dom C1 ) /\ ( dom C2 ) ) implies ( C3 . B2 ) = H1(B2)))) from FUNCT_1:sch 3;
take C3;
thus L3: thesis by L2;
end;
uniqueness
proof
let C4 , C5 being Function;
assume that
L4: ( dom C4 ) = ( ( dom C1 ) /\ ( dom C2 ) )
and
L5: (for B3 being set holds (B3 in ( dom C4 ) implies ( C4 . B3 ) = ( ( C1 . B3 ) 'nand' ( C2 . B3 ) )))
and
L6: ( dom C5 ) = ( ( dom C1 ) /\ ( dom C2 ) )
and
L7: (for B4 being set holds (B4 in ( dom C5 ) implies ( C5 . B4 ) = ( ( C1 . B4 ) 'nand' ( C2 . B4 ) )));
L8: (for B5 being set holds (B5 in ( dom C4 ) implies ( C4 . B5 ) = ( C5 . B5 )))
proof
let C6 being set;
assume L9: C6 in ( dom C4 );
L10: ( C4 . C6 ) = ( ( C1 . C6 ) 'nand' ( C2 . C6 ) ) by L9 , L5;
thus L11: thesis by L10 , L4 , L6 , L7 , L9;
end;
thus L12: thesis by L8 , L4 , L6 , FUNCT_1:2;
end;
commutativity
;
func C1 'nor' C2 -> Function means 
:L13: (( dom it ) = ( ( dom C1 ) /\ ( dom C2 ) ) & (for B6 being set holds (B6 in ( dom it ) implies ( it . B6 ) = ( ( C1 . B6 ) 'nor' ( C2 . B6 ) ))));
existence
proof
deffunc H2(set) = ( ( C1 . $1 ) 'nor' ( C2 . $1 ) );
consider C7 being Function such that L14: (( dom C7 ) = ( ( dom C1 ) /\ ( dom C2 ) ) & (for B7 being set holds (B7 in ( ( dom C1 ) /\ ( dom C2 ) ) implies ( C7 . B7 ) = H2(B7)))) from FUNCT_1:sch 3;
take C7;
thus L15: thesis by L14;
end;
uniqueness
proof
let C8 , C9 being Function;
assume that
L16: ( dom C8 ) = ( ( dom C1 ) /\ ( dom C2 ) )
and
L17: (for B8 being set holds (B8 in ( dom C8 ) implies ( C8 . B8 ) = ( ( C1 . B8 ) 'nor' ( C2 . B8 ) )))
and
L18: ( dom C9 ) = ( ( dom C1 ) /\ ( dom C2 ) )
and
L19: (for B9 being set holds (B9 in ( dom C9 ) implies ( C9 . B9 ) = ( ( C1 . B9 ) 'nor' ( C2 . B9 ) )));
L20: (for B10 being set holds (B10 in ( dom C8 ) implies ( C8 . B10 ) = ( C9 . B10 )))
proof
let C10 being set;
assume L21: C10 in ( dom C8 );
L22: ( C8 . C10 ) = ( ( C1 . C10 ) 'nor' ( C2 . C10 ) ) by L21 , L17;
thus L23: thesis by L22 , L16 , L18 , L19 , L21;
end;
thus L24: thesis by L20 , L16 , L18 , FUNCT_1:2;
end;
commutativity
;
end;
registration
let C11 , C12 being  boolean-valued Function;
cluster ( C11 'nand' C12 ) ->  boolean-valued;
coherence
proof
let C13 being set;
assume L26: C13 in ( rng ( C11 'nand' C12 ) );
consider C14 being set such that L27: (C14 in ( dom ( C11 'nand' C12 ) ) & C13 = ( ( C11 'nand' C12 ) . C14 )) by L26 , FUNCT_1:def 3;
L28: C13 = ( ( C11 . C14 ) 'nand' ( C12 . C14 ) ) by L27 , L1;
L29: (C13 = ( FALSE ) or C13 = ( TRUE )) by L28 , XBOOLEAN:def 3;
thus L30: thesis by L29;
end;
cluster ( C11 'nor' C12 ) ->  boolean-valued;
coherence
proof
let C15 being set;
assume L31: C15 in ( rng ( C11 'nor' C12 ) );
consider C16 being set such that L32: (C16 in ( dom ( C11 'nor' C12 ) ) & C15 = ( ( C11 'nor' C12 ) . C16 )) by L31 , FUNCT_1:def 3;
L33: C15 = ( ( C11 . C16 ) 'nor' ( C12 . C16 ) ) by L32 , L13;
L34: (C15 = ( FALSE ) or C15 = ( TRUE )) by L33 , XBOOLEAN:def 3;
thus L35: thesis by L34;
end;
end;
definition
let C17 being non  empty set;
let C18 , C19 being (Function of C17 , ( BOOLEAN ));
redefine func C18 'nand' C19 -> (Function of C17 , ( BOOLEAN )) means 
:L37: (for B11 being (Element of C17) holds ( it . B11 ) = ( ( C18 . B11 ) 'nand' ( C19 . B11 ) ));
coherence
proof
L38: (( dom C18 ) = C17 & ( dom C19 ) = C17) by PARTFUN1:def 2;
L39: ( dom ( C18 'nand' C19 ) ) = ( C17 /\ C17 ) by L38 , L1
.= C17;
L40: ( rng ( C18 'nand' C19 ) ) c= ( BOOLEAN ) by MARGREL1:def 16;
thus L41: thesis by L40 , L39 , FUNCT_2:2;
end;
compatibility
proof
let C20 being (Function of C17 , ( BOOLEAN ));
L42: ( dom C20 ) = C17 by FUNCT_2:def 1;
thus L43:now
assume L44: C20 = ( C18 'nand' C19 );
let C21 being (Element of C17);
L45: (( dom C18 ) = C17 & ( dom C19 ) = C17) by FUNCT_2:def 1;
L46: ( dom ( C18 'nand' C19 ) ) = ( C17 /\ C17 ) by L45 , L1
.= C17;
thus L47: ( C20 . C21 ) = ( ( C18 . C21 ) 'nand' ( C19 . C21 ) ) by L46 , L44 , L1;
end;
L48: ( dom C19 ) = C17 by FUNCT_2:def 1;
L49: ( dom C20 ) = ( C17 /\ C17 ) by FUNCT_2:def 1
.= ( ( dom C18 ) /\ ( dom C19 ) ) by L48 , FUNCT_2:def 1;
assume L50: (for B12 being (Element of C17) holds ( C20 . B12 ) = ( ( C18 . B12 ) 'nand' ( C19 . B12 ) ));
L51: (for B13 being set holds (B13 in ( dom C20 ) implies ( C20 . B13 ) = ( ( C18 . B13 ) 'nand' ( C19 . B13 ) ))) by L50 , L42;
thus L52: thesis by L51 , L49 , L1;
end;
redefine func C18 'nor' C19 -> (Function of C17 , ( BOOLEAN )) means 
:L53: (for B14 being (Element of C17) holds ( it . B14 ) = ( ( C18 . B14 ) 'nor' ( C19 . B14 ) ));
coherence
proof
L54: (( dom C18 ) = C17 & ( dom C19 ) = C17) by PARTFUN1:def 2;
L55: ( dom ( C18 'nor' C19 ) ) = ( C17 /\ C17 ) by L54 , L13
.= C17;
L56: ( rng ( C18 'nor' C19 ) ) c= ( BOOLEAN ) by MARGREL1:def 16;
thus L57: thesis by L56 , L55 , FUNCT_2:2;
end;
compatibility
proof
let C22 being (Function of C17 , ( BOOLEAN ));
L58: ( dom C22 ) = C17 by FUNCT_2:def 1;
thus L59:now
assume L60: C22 = ( C18 'nor' C19 );
let C23 being (Element of C17);
L61: (( dom C18 ) = C17 & ( dom C19 ) = C17) by FUNCT_2:def 1;
L62: ( dom ( C18 'nor' C19 ) ) = ( C17 /\ C17 ) by L61 , L13
.= C17;
thus L63: ( C22 . C23 ) = ( ( C18 . C23 ) 'nor' ( C19 . C23 ) ) by L62 , L60 , L13;
end;
L64: ( dom C19 ) = C17 by FUNCT_2:def 1;
L65: ( dom C22 ) = ( C17 /\ C17 ) by FUNCT_2:def 1
.= ( ( dom C18 ) /\ ( dom C19 ) ) by L64 , FUNCT_2:def 1;
assume L66: (for B15 being (Element of C17) holds ( C22 . B15 ) = ( ( C18 . B15 ) 'nor' ( C19 . B15 ) ));
L67: (for B16 being set holds (B16 in ( dom C22 ) implies ( C22 . B16 ) = ( ( C18 . B16 ) 'nor' ( C19 . B16 ) ))) by L66 , L58;
thus L68: thesis by L67 , L65 , L13;
end;
end;
definition
let R1 being non  empty set;
let C24 , C25 being (Function of R1 , ( BOOLEAN ));
redefine func C24 'nand' C25 -> (Function of R1 , ( BOOLEAN ));

coherence
proof
L70: ( C24 'nand' C25 ) is (Function of R1 , ( BOOLEAN ));
thus L71: thesis by L70;
end;
redefine func C24 'nor' C25 -> (Function of R1 , ( BOOLEAN ));

coherence
proof
L72: ( C24 'nor' C25 ) is (Function of R1 , ( BOOLEAN ));
thus L73: thesis by L72;
end;
end;
theorem
L75: (for R1 being non  empty set holds (for R2 being (Function of R1 , ( BOOLEAN )) holds (for R3 being (Function of R1 , ( BOOLEAN )) holds ( R2 'nand' R3 ) = ( 'not' ( R2 '&' R3 ) ))))
proof
let R1 being non  empty set;
let R2 being (Function of R1 , ( BOOLEAN ));
let R3 being (Function of R1 , ( BOOLEAN ));
let C26 being (Element of R1);
thus L76: ( ( 'not' ( R2 '&' R3 ) ) . C26 ) = ( 'not' ( ( R2 '&' R3 ) . C26 ) ) by MARGREL1:def 19
.= ( 'not' ( ( R2 . C26 ) '&' ( R3 . C26 ) ) ) by MARGREL1:def 20
.= ( 'not' ( 'not' ( ( R2 . C26 ) 'nand' ( R3 . C26 ) ) ) ) by BVFUNC_1:46
.= ( ( R2 'nand' R3 ) . C26 ) by L37;
end;
theorem
L77: (for R1 being non  empty set holds (for R2 being (Function of R1 , ( BOOLEAN )) holds (for R3 being (Function of R1 , ( BOOLEAN )) holds ( R2 'nor' R3 ) = ( 'not' ( R2 'or' R3 ) ))))
proof
let R1 being non  empty set;
let R2 being (Function of R1 , ( BOOLEAN ));
let R3 being (Function of R1 , ( BOOLEAN ));
let C27 being (Element of R1);
thus L78: ( ( 'not' ( R2 'or' R3 ) ) . C27 ) = ( 'not' ( ( R2 'or' R3 ) . C27 ) ) by MARGREL1:def 19
.= ( 'not' ( ( R2 . C27 ) 'or' ( R3 . C27 ) ) ) by BVFUNC_1:def 4
.= ( 'not' ( 'not' ( ( R2 . C27 ) 'nor' ( R3 . C27 ) ) ) ) by BVFUNC_1:53
.= ( ( R2 'nor' R3 ) . C27 ) by L53;
end;
theorem
L79: (for R1 being non  empty set holds (for R2 being (Function of R1 , ( BOOLEAN )) holds ( ( I_el R1 ) 'nand' R2 ) = ( 'not' R2 )))
proof
let R1 being non  empty set;
let R2 being (Function of R1 , ( BOOLEAN ));
L80: ( ( I_el R1 ) 'nand' R2 ) = ( 'not' ( ( I_el R1 ) '&' R2 ) ) by L75;
thus L81: thesis by L80 , BVFUNC_1:6;
end;
theorem
L82: (for R1 being non  empty set holds (for R2 being (Function of R1 , ( BOOLEAN )) holds ( ( O_el R1 ) 'nand' R2 ) = ( I_el R1 )))
proof
let R1 being non  empty set;
let R2 being (Function of R1 , ( BOOLEAN ));
L83: (( ( O_el R1 ) 'nand' R2 ) = ( 'not' ( ( O_el R1 ) '&' R2 ) ) & ( ( O_el R1 ) '&' R2 ) = ( O_el R1 )) by L75 , BVFUNC_1:5;
thus L84: thesis by L83 , BVFUNC_1:2;
end;
theorem
L85: (for R1 being non  empty set holds (( ( O_el R1 ) 'nand' ( O_el R1 ) ) = ( I_el R1 ) & ( ( O_el R1 ) 'nand' ( I_el R1 ) ) = ( I_el R1 ) & ( ( I_el R1 ) 'nand' ( I_el R1 ) ) = ( O_el R1 )))
proof
let R1 being non  empty set;
thus L86: ( ( O_el R1 ) 'nand' ( O_el R1 ) ) = ( I_el R1 ) by L82;
thus L87: ( ( O_el R1 ) 'nand' ( I_el R1 ) ) = ( I_el R1 ) by L82;
thus L88: ( ( I_el R1 ) 'nand' ( I_el R1 ) ) = ( 'not' ( I_el R1 ) ) by L79
.= ( O_el R1 ) by BVFUNC_1:2;
end;
theorem
L89: (for R1 being non  empty set holds (for R2 being (Function of R1 , ( BOOLEAN )) holds (( R2 'nand' R2 ) = ( 'not' R2 ) & ( 'not' ( R2 'nand' R2 ) ) = R2)))
proof
let R1 being non  empty set;
let R2 being (Function of R1 , ( BOOLEAN ));
L90: ( R2 'nand' R2 ) = ( 'not' ( R2 '&' R2 ) ) by L75
.= ( 'not' R2 );
thus L91: thesis by L90;
end;
theorem
L92: (for R1 being non  empty set holds (for R2 being (Function of R1 , ( BOOLEAN )) holds (for R3 being (Function of R1 , ( BOOLEAN )) holds ( 'not' ( R2 'nand' R3 ) ) = ( R2 '&' R3 ))))
proof
let R1 being non  empty set;
let R2 being (Function of R1 , ( BOOLEAN ));
let R3 being (Function of R1 , ( BOOLEAN ));
L93: ( 'not' ( R2 'nand' R3 ) ) = ( 'not' ( 'not' ( R2 '&' R3 ) ) ) by L75;
thus L94: thesis by L93;
end;
theorem
L95: (for R1 being non  empty set holds (for R2 being (Function of R1 , ( BOOLEAN )) holds (( R2 'nand' ( 'not' R2 ) ) = ( I_el R1 ) & ( 'not' ( R2 'nand' ( 'not' R2 ) ) ) = ( O_el R1 ))))
proof
let R1 being non  empty set;
let R2 being (Function of R1 , ( BOOLEAN ));
L96: ( R2 'nand' ( 'not' R2 ) ) = ( 'not' ( R2 '&' ( 'not' R2 ) ) ) by L75
.= ( 'not' ( O_el R1 ) ) by BVFUNC_4:5
.= ( I_el R1 ) by BVFUNC_1:2;
thus L97: thesis by L96 , BVFUNC_1:2;
end;
theorem
L98: (for R1 being non  empty set holds (for R2 being (Function of R1 , ( BOOLEAN )) holds (for R3 being (Function of R1 , ( BOOLEAN )) holds (for R4 being (Function of R1 , ( BOOLEAN )) holds ( R2 'nand' ( R3 '&' R4 ) ) = ( 'not' ( ( R2 '&' R3 ) '&' R4 ) )))))
proof
let R1 being non  empty set;
let R2 being (Function of R1 , ( BOOLEAN ));
let R3 being (Function of R1 , ( BOOLEAN ));
let R4 being (Function of R1 , ( BOOLEAN ));
L99: ( R2 'nand' ( R3 '&' R4 ) ) = ( 'not' ( R2 '&' ( R3 '&' R4 ) ) ) by L75;
thus L100: thesis by L99 , BVFUNC_1:4;
end;
theorem
L101: (for R1 being non  empty set holds (for R2 being (Function of R1 , ( BOOLEAN )) holds (for R3 being (Function of R1 , ( BOOLEAN )) holds (for R4 being (Function of R1 , ( BOOLEAN )) holds ( R2 'nand' ( R3 '&' R4 ) ) = ( ( R2 '&' R3 ) 'nand' R4 )))))
proof
let R1 being non  empty set;
let R2 being (Function of R1 , ( BOOLEAN ));
let R3 being (Function of R1 , ( BOOLEAN ));
let R4 being (Function of R1 , ( BOOLEAN ));
L102: ( ( R2 '&' R3 ) 'nand' R4 ) = ( 'not' ( ( R2 '&' R3 ) '&' R4 ) ) by L75;
thus L103: thesis by L102 , L98;
end;
theorem
L104: (for R1 being non  empty set holds (for R2 being (Function of R1 , ( BOOLEAN )) holds (for R3 being (Function of R1 , ( BOOLEAN )) holds (for R4 being (Function of R1 , ( BOOLEAN )) holds ( R2 'nand' ( R3 'or' R4 ) ) = ( ( 'not' ( R2 '&' R3 ) ) '&' ( 'not' ( R2 '&' R4 ) ) )))))
proof
let R1 being non  empty set;
let R2 being (Function of R1 , ( BOOLEAN ));
let R3 being (Function of R1 , ( BOOLEAN ));
let R4 being (Function of R1 , ( BOOLEAN ));
thus L105: ( R2 'nand' ( R3 'or' R4 ) ) = ( 'not' ( R2 '&' ( R3 'or' R4 ) ) ) by L75
.= ( 'not' ( ( R2 '&' R3 ) 'or' ( R2 '&' R4 ) ) ) by BVFUNC_1:12
.= ( ( 'not' ( R2 '&' R3 ) ) '&' ( 'not' ( R2 '&' R4 ) ) ) by BVFUNC_1:13;
end;
theorem
L106: (for R1 being non  empty set holds (for R2 being (Function of R1 , ( BOOLEAN )) holds (for R3 being (Function of R1 , ( BOOLEAN )) holds (for R4 being (Function of R1 , ( BOOLEAN )) holds ( R2 'nand' ( R3 'xor' R4 ) ) = ( ( R2 '&' R3 ) 'eqv' ( R2 '&' R4 ) )))))
proof
let R1 being non  empty set;
let R2 being (Function of R1 , ( BOOLEAN ));
let R3 being (Function of R1 , ( BOOLEAN ));
let R4 being (Function of R1 , ( BOOLEAN ));
thus L107: ( R2 'nand' ( R3 'xor' R4 ) ) = ( 'not' ( R2 '&' ( R3 'xor' R4 ) ) ) by L75
.= ( 'not' ( ( R2 '&' R3 ) 'xor' ( R2 '&' R4 ) ) ) by BVFUNC25:11
.= ( 'not' ( 'not' ( ( R2 '&' R3 ) 'eqv' ( R2 '&' R4 ) ) ) ) by BVFUNC_8:12
.= ( ( R2 '&' R3 ) 'eqv' ( R2 '&' R4 ) );
end;
theorem
L108: (for R1 being non  empty set holds (for R2 being (Function of R1 , ( BOOLEAN )) holds (for R3 being (Function of R1 , ( BOOLEAN )) holds (for R4 being (Function of R1 , ( BOOLEAN )) holds (( R2 'nand' ( R3 'nand' R4 ) ) = ( ( 'not' R2 ) 'or' ( R3 '&' R4 ) ) & ( R2 'nand' ( R3 'nand' R4 ) ) = ( R2 'imp' ( R3 '&' R4 ) ))))))
proof
let R1 being non  empty set;
let R2 being (Function of R1 , ( BOOLEAN ));
let R3 being (Function of R1 , ( BOOLEAN ));
let R4 being (Function of R1 , ( BOOLEAN ));
L109: ( R2 'nand' ( R3 'nand' R4 ) ) = ( 'not' ( R2 '&' ( R3 'nand' R4 ) ) ) by L75
.= ( 'not' ( R2 '&' ( 'not' ( R3 '&' R4 ) ) ) ) by L75
.= ( ( 'not' R2 ) 'or' ( 'not' ( 'not' ( R3 '&' R4 ) ) ) ) by BVFUNC_1:14
.= ( ( 'not' R2 ) 'or' ( R3 '&' R4 ) );
thus L110: thesis by L109 , BVFUNC_4:8;
end;
theorem
L111: (for R1 being non  empty set holds (for R2 being (Function of R1 , ( BOOLEAN )) holds (for R3 being (Function of R1 , ( BOOLEAN )) holds (for R4 being (Function of R1 , ( BOOLEAN )) holds (( R2 'nand' ( R3 'nor' R4 ) ) = ( ( ( 'not' R2 ) 'or' R3 ) 'or' R4 ) & ( R2 'nand' ( R3 'nor' R4 ) ) = ( R2 'imp' ( R3 'or' R4 ) ))))))
proof
let R1 being non  empty set;
let R2 being (Function of R1 , ( BOOLEAN ));
let R3 being (Function of R1 , ( BOOLEAN ));
let R4 being (Function of R1 , ( BOOLEAN ));
L112: ( R2 'nand' ( R3 'nor' R4 ) ) = ( 'not' ( R2 '&' ( R3 'nor' R4 ) ) ) by L75
.= ( 'not' ( R2 '&' ( 'not' ( R3 'or' R4 ) ) ) ) by L77
.= ( ( 'not' R2 ) 'or' ( 'not' ( 'not' ( R3 'or' R4 ) ) ) ) by BVFUNC_1:14
.= ( ( ( 'not' R2 ) 'or' R3 ) 'or' R4 ) by BVFUNC_1:8;
L113: ( R2 'nand' ( R3 'nor' R4 ) ) = ( ( 'not' R2 ) 'or' ( R3 'or' R4 ) ) by L112 , BVFUNC_1:8;
thus L114: thesis by L113 , L112 , BVFUNC_4:8;
end;
theorem
L115: (for R1 being non  empty set holds (for R2 being (Function of R1 , ( BOOLEAN )) holds (for R3 being (Function of R1 , ( BOOLEAN )) holds (for R4 being (Function of R1 , ( BOOLEAN )) holds ( R2 'nand' ( R3 'eqv' R4 ) ) = ( R2 'imp' ( R3 'xor' R4 ) )))))
proof
let R1 being non  empty set;
let R2 being (Function of R1 , ( BOOLEAN ));
let R3 being (Function of R1 , ( BOOLEAN ));
let R4 being (Function of R1 , ( BOOLEAN ));
L116: ( R2 'nand' ( R3 'eqv' R4 ) ) = ( 'not' ( R2 '&' ( R3 'eqv' R4 ) ) ) by L75
.= ( ( 'not' R2 ) 'or' ( 'not' ( R3 'eqv' R4 ) ) ) by BVFUNC_1:14
.= ( ( 'not' R2 ) 'or' ( 'not' ( 'not' ( R3 'xor' R4 ) ) ) ) by BVFUNC25:12
.= ( ( 'not' R2 ) 'or' ( R3 'xor' R4 ) );
thus L117: thesis by L116 , BVFUNC_4:8;
end;
theorem
L118: (for R1 being non  empty set holds (for R2 being (Function of R1 , ( BOOLEAN )) holds (for R3 being (Function of R1 , ( BOOLEAN )) holds ( R2 'nand' ( R2 '&' R3 ) ) = ( R2 'nand' R3 ))))
proof
let R1 being non  empty set;
let R2 being (Function of R1 , ( BOOLEAN ));
let R3 being (Function of R1 , ( BOOLEAN ));
L119: ( R2 'nand' ( R2 '&' R3 ) ) = ( ( R2 '&' R2 ) 'nand' R3 ) by L101;
thus L120: thesis by L119;
end;
theorem
L121: (for R1 being non  empty set holds (for R2 being (Function of R1 , ( BOOLEAN )) holds (for R3 being (Function of R1 , ( BOOLEAN )) holds ( R2 'nand' ( R2 'or' R3 ) ) = ( ( 'not' R2 ) '&' ( 'not' ( R2 '&' R3 ) ) ))))
proof
let R1 being non  empty set;
let R2 being (Function of R1 , ( BOOLEAN ));
let R3 being (Function of R1 , ( BOOLEAN ));
thus L122: ( R2 'nand' ( R2 'or' R3 ) ) = ( ( 'not' ( R2 '&' R2 ) ) '&' ( 'not' ( R2 '&' R3 ) ) ) by L104
.= ( ( 'not' R2 ) '&' ( 'not' ( R2 '&' R3 ) ) );
