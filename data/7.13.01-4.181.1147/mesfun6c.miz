theorem
L1137: (for R1 being non  empty set holds (for R3 being (SigmaField of R1) holds (for R10 being (Element of R3) holds (for R12 being (PartFunc of R1 , ( REAL )) holds (for R13 being Real holds ( R10 /\ ( eq_dom (R12 , R13) ) ) = ( ( R10 /\ ( great_eq_dom (R12 , R13) ) ) /\ ( less_eq_dom (R12 , R13) ) ))))))
proof
let R1 being non  empty set;
let R3 being (SigmaField of R1);
let R10 being (Element of R3);
let R12 being (PartFunc of R1 , ( REAL ));
let R13 being Real;
L1138:
now
