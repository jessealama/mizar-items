theorem
L32: (for R9 being  complex number holds (for R10 being  complex number holds (for R12 being  complex-valued Function holds (for R13 being  complex-valued Function holds ((R9 <> ( 0 ) & R10 <> ( 0 )) implies ( ( R12 (/) R9 ) - ( R13 (/) R10 ) ) = ( ( ( R12 (#) R10 ) - ( R13 (#) R9 ) ) (/) ( R9 * R10 ) ))))))
proof
let R9 being  complex number;
let R10 being  complex number;
let R12 being  complex-valued Function;
let R13 being  complex-valued Function;
assume L33: (R9 <> ( 0 ) & R10 <> ( 0 ));
L34: ( dom ( R12 (/) R9 ) ) = ( dom R12 ) by VALUED_2:28;
L35: ( dom ( R13 (/) R10 ) ) = ( dom R13 ) by VALUED_2:28;
L36: ( dom ( R12 (#) R10 ) ) = ( dom R12 ) by VALUED_1:def 5;
L37: ( dom ( R13 (#) R9 ) ) = ( dom R13 ) by VALUED_1:def 5;
L38: ( dom ( ( R12 (/) R9 ) - ( R13 (/) R10 ) ) ) = ( ( dom ( R12 (/) R9 ) ) /\ ( dom ( R13 (/) R10 ) ) ) by VALUED_1:12;
L39: ( dom ( ( R12 (#) R10 ) - ( R13 (#) R9 ) ) ) = ( ( dom ( R12 (#) R10 ) ) /\ ( dom ( R13 (#) R9 ) ) ) by VALUED_1:12;
thus L40: ( dom ( ( R12 (/) R9 ) - ( R13 (/) R10 ) ) ) = ( dom ( ( ( R12 (#) R10 ) - ( R13 (#) R9 ) ) (/) ( R9 * R10 ) ) ) by L39 , L34 , L35 , L36 , L37 , L38 , VALUED_2:28;
let R1 being set;
assume L41: R1 in ( dom ( ( R12 (/) R9 ) - ( R13 (/) R10 ) ) );
thus L42: ( ( ( R12 (/) R9 ) - ( R13 (/) R10 ) ) . R1 ) = ( ( ( R12 (/) R9 ) . R1 ) - ( ( R13 (/) R10 ) . R1 ) ) by L41 , VALUED_1:13
.= ( ( ( R12 . R1 ) / R9 ) - ( ( R13 (/) R10 ) . R1 ) ) by VALUED_2:29
.= ( ( ( R12 . R1 ) / R9 ) - ( ( R13 . R1 ) / R10 ) ) by VALUED_2:29
.= ( ( ( ( R12 . R1 ) * R10 ) - ( ( R13 . R1 ) * R9 ) ) / ( R9 * R10 ) ) by L33 , XCMPLX_1:130
.= ( ( ( ( R12 . R1 ) * R10 ) - ( ( R13 (#) R9 ) . R1 ) ) / ( R9 * R10 ) ) by VALUED_1:6
.= ( ( ( ( R12 (#) R10 ) . R1 ) - ( ( R13 (#) R9 ) . R1 ) ) / ( R9 * R10 ) ) by VALUED_1:6
.= ( ( ( ( R12 (#) R10 ) - ( R13 (#) R9 ) ) . R1 ) / ( R9 * R10 ) ) by L34 , L35 , L36 , L37 , L38 , L39 , L41 , VALUED_1:13
.= ( ( ( ( R12 (#) R10 ) - ( R13 (#) R9 ) ) (/) ( R9 * R10 ) ) . R1 ) by VALUED_2:29;
end;
theorem
L43: (for R8 being  complex number holds (for R12 being  complex-valued Function holds (for R13 being  complex-valued Function holds (R8 <> ( 0 ) implies ( ( R12 (/) R8 ) - R13 ) = ( ( R12 - ( R8 (#) R13 ) ) (/) R8 )))))
proof
let R8 being  complex number;
let R12 being  complex-valued Function;
let R13 being  complex-valued Function;
assume L44: R8 <> ( 0 );
L45: ( dom ( ( R12 (/) R8 ) - R13 ) ) = ( ( dom ( R12 (/) R8 ) ) /\ ( dom R13 ) ) by VALUED_1:12
.= ( ( dom R12 ) /\ ( dom R13 ) ) by VALUED_2:28;
L46: ( dom ( R12 - ( R8 (#) R13 ) ) ) = ( ( dom R12 ) /\ ( dom ( R8 (#) R13 ) ) ) by VALUED_1:12
.= ( ( dom R12 ) /\ ( dom R13 ) ) by VALUED_1:def 5;
thus L47: ( dom ( ( R12 (/) R8 ) - R13 ) ) = ( dom ( ( R12 - ( R8 (#) R13 ) ) (/) R8 ) ) by L46 , L45 , VALUED_2:28;
let C6 being set;
assume L48: C6 in ( dom ( ( R12 (/) R8 ) - R13 ) );
thus L49: ( ( ( R12 (/) R8 ) - R13 ) . C6 ) = ( ( ( R12 (/) R8 ) . C6 ) - ( R13 . C6 ) ) by L48 , VALUED_1:13
.= ( ( ( R12 . C6 ) / R8 ) - ( R13 . C6 ) ) by VALUED_2:29
.= ( ( ( R12 . C6 ) / R8 ) - ( ( R8 * ( R13 . C6 ) ) / R8 ) ) by L44 , XCMPLX_1:89
.= ( ( ( R12 . C6 ) / R8 ) - ( ( ( R8 (#) R13 ) . C6 ) / R8 ) ) by VALUED_1:6
.= ( ( ( R12 . C6 ) - ( ( R8 (#) R13 ) . C6 ) ) / R8 )
.= ( ( ( R12 - ( R8 (#) R13 ) ) . C6 ) / R8 ) by L45 , L46 , L48 , VALUED_1:13
.= ( ( ( R12 - ( R8 (#) R13 ) ) (/) R8 ) . C6 ) by VALUED_2:29;
end;
theorem
L50: (for R8 being  complex number holds (for R11 being  complex number holds (for R12 being  complex-valued Function holds ( ( R8 - R11 ) (#) R12 ) = ( ( R8 (#) R12 ) - ( R11 (#) R12 ) ))))
proof
let R8 being  complex number;
let R11 being  complex number;
let R12 being  complex-valued Function;
L51: ( dom ( ( R8 (#) R12 ) - ( R11 (#) R12 ) ) ) = ( ( dom ( R8 (#) R12 ) ) /\ ( dom ( R11 (#) R12 ) ) ) by VALUED_1:12
.= ( ( dom R12 ) /\ ( dom ( R11 (#) R12 ) ) ) by VALUED_1:def 5
.= ( ( dom R12 ) /\ ( dom R12 ) ) by VALUED_1:def 5;
thus L52: ( dom ( ( R8 - R11 ) (#) R12 ) ) = ( dom ( ( R8 (#) R12 ) - ( R11 (#) R12 ) ) ) by L51 , VALUED_1:def 5;
let C7 being set;
assume L53: C7 in ( dom ( ( R8 - R11 ) (#) R12 ) );
thus L54: ( ( ( R8 - R11 ) (#) R12 ) . C7 ) = ( ( R8 - R11 ) * ( R12 . C7 ) ) by VALUED_1:6
.= ( ( R8 * ( R12 . C7 ) ) - ( R11 * ( R12 . C7 ) ) )
.= ( ( R8 * ( R12 . C7 ) ) - ( ( R11 (#) R12 ) . C7 ) ) by VALUED_1:6
.= ( ( ( R8 (#) R12 ) . C7 ) - ( ( R11 (#) R12 ) . C7 ) ) by VALUED_1:6
.= ( ( ( R8 (#) R12 ) - ( R11 (#) R12 ) ) . C7 ) by L52 , L53 , VALUED_1:13;
end;
theorem
L55: (for R12 being  complex-valued Function holds (for R13 being  complex-valued Function holds ( ( R12 - R13 ) ^2 ) = ( ( R13 - R12 ) ^2 )))
proof
let R12 being  complex-valued Function;
let R13 being  complex-valued Function;
L56: ( dom ( R12 - R13 ) ) = ( ( dom R12 ) /\ ( dom R13 ) ) by VALUED_1:12;
L57: ( dom ( R13 - R12 ) ) = ( ( dom R13 ) /\ ( dom R12 ) ) by VALUED_1:12;
L58: ( dom ( ( R12 - R13 ) ^2 ) ) = ( dom ( R12 - R13 ) ) by VALUED_1:11;
thus L59: ( dom ( ( R12 - R13 ) ^2 ) ) = ( dom ( ( R13 - R12 ) ^2 ) ) by L58 , L56 , L57 , VALUED_1:11;
let C8 being set;
assume L60: C8 in ( dom ( ( R12 - R13 ) ^2 ) );
L61: ( ( R12 - R13 ) . C8 ) = ( ( R12 . C8 ) - ( R13 . C8 ) ) by L60 , L58 , VALUED_1:13;
L62: ( ( R13 - R12 ) . C8 ) = ( ( R13 . C8 ) - ( R12 . C8 ) ) by L56 , L58 , L60 , L57 , VALUED_1:13;
thus L63: ( ( ( R12 - R13 ) ^2 ) . C8 ) = ( ( ( R12 - R13 ) . C8 ) * ( ( R12 - R13 ) . C8 ) ) by VALUED_1:5
.= ( ( ( R13 - R12 ) . C8 ) * ( ( R13 - R12 ) . C8 ) ) by L61 , L62
.= ( ( ( R13 - R12 ) ^2 ) . C8 ) by VALUED_1:5;
end;
theorem
L64: (for R8 being  complex number holds (for R12 being  complex-valued Function holds ( ( R12 (/) R8 ) ^2 ) = ( ( R12 ^2 ) (/) ( R8 ^2 ) )))
