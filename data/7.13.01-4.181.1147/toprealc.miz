theorem
L68: (for R4 being Nat holds (for R6 being  real number holds (for R7 being  real number holds |. ( ( R4 |-> R6 ) - ( R4 |-> R7 ) ) .| = ( ( sqrt R4 ) * ( abs ( R6 - R7 ) ) ))))
proof
let R4 being Nat;
let R6 being  real number;
let R7 being  real number;
thus L69: |. ( ( R4 |-> R6 ) - ( R4 |-> R7 ) ) .| = ( sqrt ( Sum ( sqr ( R4 |-> ( R6 - R7 ) ) ) ) ) by RVSUM_1:30
.= ( sqrt ( Sum ( R4 |-> ( ( R6 - R7 ) ^2 ) ) ) ) by RVSUM_1:56
.= ( sqrt ( R4 * ( ( R6 - R7 ) ^2 ) ) ) by RVSUM_1:80
.= ( ( sqrt R4 ) * ( sqrt ( ( R6 - R7 ) ^2 ) ) ) by SQUARE_1:29
.= ( ( sqrt R4 ) * ( abs ( R6 - R7 ) ) ) by COMPLEX1:72;
end;
registration
let R12 being  complex-valued Function;
let R1 being set;
let R8 being  complex number;
cluster ( R12 +* (R1 , R8) ) ->  complex-valued;
coherence
proof
set D1 = ( R12 +* (R1 , R8) );
let C10 being set;
assume that
L70: C10 in ( dom D1 );
per cases ;
suppose L71: (R1 in ( dom R12 ) & R1 = C10);

thus L72: thesis by L71 , FUNCT_7:31;
end;
suppose L73: (R1 in ( dom R12 ) & R1 <> C10);

L74: ( D1 . C10 ) = ( R12 . C10 ) by L73 , FUNCT_7:32;
thus L75: thesis by L74;
end;
suppose L76: (not R1 in ( dom R12 ));

L77: ( D1 . C10 ) = ( R12 . C10 ) by L76 , FUNCT_7:def 3;
thus L78: thesis by L77;
end;
end;
end;
theorem
L81: (for R1 being set holds (for R4 being Nat holds (for R8 being  complex number holds ( ( ( 0* R4 ) +* (R1 , R8) ) ^2 ) = ( ( 0* R4 ) +* (R1 , ( R8 ^2 )) ))))
proof
let R1 being set;
let R4 being Nat;
let R8 being  complex number;
set D2 = ( ( 0* R4 ) +* (R1 , R8) );
set D3 = ( ( 0* R4 ) +* (R1 , ( R8 ^2 )) );
L82: ( dom D2 ) = ( dom ( 0* R4 ) ) by FUNCT_7:30;
L83: ( dom D3 ) = ( dom ( 0* R4 ) ) by FUNCT_7:30;
L84: ( dom ( D2 ^2 ) ) = ( dom D2 ) by VALUED_1:11;
thus L85: ( dom ( D2 ^2 ) ) = ( dom D3 ) by L82 , L83 , VALUED_1:11;
let C11 being set;
assume L86: C11 in ( dom ( D2 ^2 ) );
L87: ( ( D2 ^2 ) . C11 ) = ( ( D2 . C11 ) ^2 ) by VALUED_1:11;
per cases ;
suppose L88: C11 = R1;

L89: ( D2 . C11 ) = R8 by L88 , L82 , L84 , L86 , FUNCT_7:31;
thus L90: thesis by L89 , L88 , L82 , L84 , L86 , L87 , FUNCT_7:31;
end;
suppose L91: C11 <> R1;

L92: ( D2 . C11 ) = ( ( R4 |-> ( 0 ) ) . C11 ) by L91 , FUNCT_7:32
.= ( ( {} ) . R1 );
L93: ( D3 . C11 ) = ( ( R4 |-> ( 0 ) ) . C11 ) by L91 , FUNCT_7:32
.= ( ( {} ) . R1 );
thus L94: thesis by L93 , L87 , L92;
end;
end;
theorem
L96: (for R1 being set holds (for R4 being Nat holds (for R6 being  real number holds (R1 in ( Seg R4 ) implies |. ( ( 0* R4 ) +* (R1 , R6) ) .| = ( abs R6 )))))
proof
let R1 being set;
let R4 being Nat;
let R6 being  real number;
set D4 = ( ( 0* R4 ) +* (R1 , R6) );
L97: ( R6 ^2 ) in ( REAL ) by XREAL_0:def 1;
L98: R4 in ( NAT ) by ORDINAL1:def 12;
assume L99: R1 in ( Seg R4 );
L100: ( D4 ^2 ) = ( ( 0* R4 ) +* (R1 , ( R6 ^2 )) ) by L81;
L101: ( Sum ( D4 ^2 ) ) = ( R6 ^2 ) by L100 , L99 , L97 , L98 , JORDAN2B:10;
thus L102: thesis by L101 , COMPLEX1:72;
end;
theorem
L103: (for R1 being set holds (for R4 being Nat holds ( ( 0.REAL R4 ) +* (R1 , ( 0 )) ) = ( 0.REAL R4 )))
proof
