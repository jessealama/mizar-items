theorem
L1: (for B1 being  trivial set holds (for B2 being set holds (B1 , B2 are_equipotent  implies B2 is  trivial)))
proof
let C1 being  trivial set;
let C2 being set;
assume that
L2: C1 , C2 are_equipotent ;
L3: ( card C1 ) < 2 by NAT_D:60;
L4: C2 is  finite by L2 , CARD_1:38;
L5: ( card C1 ) = ( card C2 ) by L2 , CARD_1:5;
thus L6: thesis by L5 , L3 , L4 , NAT_D:60;
end;
registration
let C3 being  real number;
cluster ( C3 ^2 ) -> non  negative;
coherence;
end;
registration
let C4 being  positive  real number;
cluster ( C4 ^2 ) ->  positive;
coherence;
end;
registration
cluster ( sqrt ( 0 ) ) ->  zero;
coherence by SQUARE_1:17;
end;
registration
let C5 being  empty set;
cluster ( sqr C5 ) ->  empty;
coherence;
cluster |. C5 .| ->  zero;
coherence by RVSUM_1:72;
end;
theorem
L11: (for R9 being  complex number holds (for R10 being  complex number holds (for R12 being  complex-valued Function holds ( R12 (#) ( R9 + R10 ) ) = ( ( R12 (#) R9 ) + ( R12 (#) R10 ) ))))
proof
let R9 being  complex number;
let R10 being  complex number;
let R12 being  complex-valued Function;
L12: ( dom ( R12 (#) ( R9 + R10 ) ) ) = ( dom R12 ) by VALUED_1:def 5;
L13: ( dom ( R12 (#) R9 ) ) = ( dom R12 ) by VALUED_1:def 5;
L14: ( dom ( R12 (#) R10 ) ) = ( dom R12 ) by VALUED_1:def 5;
L15: ( dom ( ( R12 (#) R9 ) + ( R12 (#) R10 ) ) ) = ( ( dom ( R12 (#) R9 ) ) /\ ( dom ( R12 (#) R10 ) ) ) by VALUED_1:def 1;
thus L16: ( dom ( R12 (#) ( R9 + R10 ) ) ) = ( dom ( ( R12 (#) R9 ) + ( R12 (#) R10 ) ) ) by L15 , L13 , L14 , VALUED_1:def 5;
let R1 being set;
assume L17: R1 in ( dom ( R12 (#) ( R9 + R10 ) ) );
thus L18: ( ( R12 (#) ( R9 + R10 ) ) . R1 ) = ( ( R12 . R1 ) * ( R9 + R10 ) ) by L17 , VALUED_1:def 5
.= ( ( ( R12 . R1 ) * R9 ) + ( ( R12 . R1 ) * R10 ) )
.= ( ( ( R12 (#) R9 ) . R1 ) + ( ( R12 . R1 ) * R10 ) ) by L12 , L13 , L17 , VALUED_1:def 5
.= ( ( ( R12 (#) R9 ) . R1 ) + ( ( R12 (#) R10 ) . R1 ) ) by L12 , L14 , L17 , VALUED_1:def 5
.= ( ( ( R12 (#) R9 ) + ( R12 (#) R10 ) ) . R1 ) by L12 , L13 , L14 , L15 , L17 , VALUED_1:def 1;
end;
theorem
L19: (for R9 being  complex number holds (for R10 being  complex number holds (for R12 being  complex-valued Function holds ( R12 (#) ( R9 - R10 ) ) = ( ( R12 (#) R9 ) - ( R12 (#) R10 ) ))))
proof
