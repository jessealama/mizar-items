begin
definition
let C1 , C2 being non  empty doubleLoopStr;
let C3 being (Function of C1 , C2);
attr C3 is  linear
means
:L1: C3 is  additive  multiplicative  unity-preserving;
end;
registration
let C4 , C5 being non  empty doubleLoopStr;
cluster  linear ->  additive  multiplicative  unity-preserving for (Function of C4 , C5);
coherence by L1;
cluster  additive  multiplicative  unity-preserving ->  linear for (Function of C4 , C5);
coherence by L1;
end;
theorem
L4: (for B1 , B2 , B3 being non  empty doubleLoopStr holds (for B4 being (Function of B1 , B2) holds (for B5 being (Function of B2 , B3) holds ((B4 is  linear & B5 is  linear) implies ( B5 * B4 ) is  linear))))
proof
let C6 , C7 , C8 being non  empty doubleLoopStr;
let C9 being (Function of C6 , C7);
let C10 being (Function of C7 , C8);
assume that
L5: C9 is  additive  multiplicative  unity-preserving
and
L6: C10 is  additive  multiplicative  unity-preserving;
set D1 = ( C10 * C9 );
thus L7: D1 is  additive
proof
let C11 , C12 being (Scalar of C6);
L8: (( C10 . ( C9 . C11 ) ) = ( D1 . C11 ) & ( C10 . ( C9 . C12 ) ) = ( D1 . C12 )) by FUNCT_2:15;
thus L9: ( D1 . ( C11 + C12 ) ) = ( C10 . ( C9 . ( C11 + C12 ) ) ) by FUNCT_2:15
.= ( C10 . ( ( C9 . C11 ) + ( C9 . C12 ) ) ) by L5 , VECTSP_1:def 20
.= ( ( D1 . C11 ) + ( D1 . C12 ) ) by L6 , L8 , VECTSP_1:def 20;
end;

thus L10: D1 is  multiplicative
proof
let C13 , C14 being (Scalar of C6);
L11: (( C10 . ( C9 . C13 ) ) = ( D1 . C13 ) & ( C10 . ( C9 . C14 ) ) = ( D1 . C14 )) by FUNCT_2:15;
thus L12: ( D1 . ( C13 * C14 ) ) = ( C10 . ( C9 . ( C13 * C14 ) ) ) by FUNCT_2:15
.= ( C10 . ( ( C9 . C13 ) * ( C9 . C14 ) ) ) by L5 , GROUP_6:def 6
.= ( ( D1 . C13 ) * ( D1 . C14 ) ) by L6 , L11 , GROUP_6:def 6;
end;

thus L13: ( D1 . ( 1_ C6 ) ) = ( C10 . ( C9 . ( 1_ C6 ) ) ) by FUNCT_2:15
.= ( C10 . ( 1_ C7 ) ) by L5 , GROUP_1:def 13
.= ( 1_ C8 ) by L6 , GROUP_1:def 13;
end;
definition
struct RingMorphismStr(# Dom , Cod -> Ring , Fun -> (Function of the Dom , the Cod) #);
end;
definition
let R5 being RingMorphismStr;
func dom R5 -> Ring equals 
(the Dom of R5);
coherence;
func cod R5 -> Ring equals 
(the Cod of R5);
coherence;
func fun R5 -> (Function of (the Dom of R5) , (the Cod of R5)) equals 
(the Fun of R5);
coherence;
end;
L16: (for B6 being non  empty doubleLoopStr holds ( id B6 ) is  linear)
proof
let C15 being non  empty doubleLoopStr;
set D2 = ( id C15 );
thus L17: D2 is  additive;
thus L18: D2 is  multiplicative
proof
let C16 , C17 being (Scalar of C15);
L19: (( D2 . ( C16 * C17 ) ) = ( C16 * C17 ) & ( D2 . C16 ) = C16) by FUNCT_1:18;
thus L20: thesis by L19 , FUNCT_1:18;
end;

thus L21: ( D2 . ( 1_ C15 ) ) = ( 1_ C15 ) by FUNCT_1:18;
end;
registration
let C18 being non  empty doubleLoopStr;
cluster ( id C18 ) ->  linear;
coherence by L16;
end;
definition
let C19 being RingMorphismStr;
attr C19 is  RingMorphism-like
means
:L23: ( fun C19 ) is  linear;
end;
registration
cluster  strict  RingMorphism-like for RingMorphismStr;
existence
proof
set D3 = the Ring;
set D4 = RingMorphismStr (# D3 , D3 , ( id D3 ) #);
L25: ( fun D4 ) = ( id D3 );
thus L26: thesis by L25 , L23;
end;
end;
definition
mode RingMorphism
 is  RingMorphism-like RingMorphismStr;
end;
definition
let R6 being Ring;
func ID R6 -> RingMorphism equals 
RingMorphismStr (# R6 , R6 , ( id R6 ) #);
coherence
proof
set D5 = RingMorphismStr (# R6 , R6 , ( id R6 ) #);
L29: ( fun D5 ) = ( id R6 );
thus L30: thesis by L29 , L23;
end;
end;
registration
let R6 being Ring;
cluster ( ID R6 ) ->  strict;
coherence;
end;
definition
let R6 being Ring;
let R7 being Ring;
pred R6 <= R7
means
:L33: (ex B7 being RingMorphism st (( dom B7 ) = R6 & ( cod B7 ) = R7))
;reflexivity
proof
let R6 being Ring;
set D6 = ( ID R6 );
take D6;
thus L34: thesis;
end;
end;
L36: (for R12 being RingMorphism holds ( the RingMorphismStr of R12 ) is  RingMorphism-like)
proof
let R12 being RingMorphism;
set D7 = ( the RingMorphismStr of R12 );
L37: ( fun R12 ) is  linear by L23;
thus L38: (for B8 , B9 being (Scalar of (the Dom of D7)) holds ( ( fun D7 ) . ( B8 + B9 ) ) = ( ( ( fun D7 ) . B8 ) + ( ( fun D7 ) . B9 ) )) by L37 , VECTSP_1:def 20;
thus L39: (for B10 , B11 being (Scalar of (the Dom of D7)) holds ( ( fun D7 ) . ( B10 * B11 ) ) = ( ( ( fun D7 ) . B10 ) * ( ( fun D7 ) . B11 ) )) by L37 , GROUP_6:def 6;
thus L40: thesis by L37;
end;
definition
let R6 being Ring;
let R7 being Ring;
assume L41: R6 <= R7;
mode Morphism of R6 , R7
 ->  strict RingMorphism
means :L42: (( dom it ) = R6 & ( cod it ) = R7);
existence
proof
consider C20 being RingMorphism such that L43: (( dom C20 ) = R6 & ( cod C20 ) = R7) by L41 , L33;
set D8 = ( the RingMorphismStr of C20 );
L44: (( dom D8 ) = R6 & ( cod D8 ) = R7) by L43;
L45: D8 is RingMorphism by L36;
thus L46: thesis by L45 , L44;
end;
end;
registration
let R6 being Ring;
let R7 being Ring;
cluster  strict for (Morphism of R6 , R7);
existence
proof
set D9 = the (Morphism of R6 , R7);
L48: D9 is RingMorphism;
thus L49: thesis by L48;
end;
end;
definition
let R6 being Ring;
redefine func ID R6 ->  strict (Morphism of R6 , R6);

coherence
proof
set D10 = ( ID R6 );
L51: (( dom D10 ) = R6 & ( cod D10 ) = R6);
thus L52: thesis by L51 , L42;
end;
end;
L54: (for R12 being RingMorphism holds (the Fun of R12) is  linear)
proof
let R12 being RingMorphism;
L55: (the Fun of R12) = ( fun R12 );
thus L56: thesis by L55 , L23;
end;
L57: (for R6 being Ring holds (for R7 being Ring holds (for B12 being  strict RingMorphismStr holds ((( dom B12 ) = R6 & ( cod B12 ) = R7 & ( fun B12 ) is  linear) implies B12 is (Morphism of R6 , R7)))))
proof
let R6 being Ring;
let R7 being Ring;
let C21 being  strict RingMorphismStr;
assume that
L58: ( dom C21 ) = R6
and
L59: ( cod C21 ) = R7
and
L60: ( fun C21 ) is  linear;
reconsider D11 = C21 as RingMorphism by L60 , L23;
L61: ( dom D11 ) = R6 by L58;
L62: R6 <= R7 by L61 , L59 , L33;
L63: D11 is (Morphism of R6 , R7) by L62 , L58 , L59 , L42;
thus L64: thesis by L63;
end;
L65: (for R6 being Ring holds (for R7 being Ring holds (for B13 being (Function of R6 , R7) holds (B13 is  linear implies RingMorphismStr (# R6 , R7 , B13 #) is (Morphism of R6 , R7)))))
proof
let R6 being Ring;
let R7 being Ring;
let C22 being (Function of R6 , R7);
assume that
L66: C22 is  linear;
set D12 = RingMorphismStr (# R6 , R7 , C22 #);
L67: ( fun D12 ) = C22;
L68: (( dom D12 ) = R6 & ( cod D12 ) = R7);
thus L69: thesis by L68 , L66 , L67 , L57;
end;
L70: (for B14 being RingMorphism holds (ex R6 being Ring st (ex R7 being Ring st (R6 <= R7 & ( dom B14 ) = R6 & ( cod B14 ) = R7 & ( the RingMorphismStr of B14 ) is (Morphism of R6 , R7)))))
proof
let C23 being RingMorphism;
reconsider D13 = ( the RingMorphismStr of C23 ) as RingMorphism by L36;
set D14 = (the Dom of C23);
set D15 = (the Cod of C23);
take D14;
take D15;
L71: (( dom C23 ) = D14 & ( cod C23 ) = D15);
L72: D14 <= D15 by L71 , L33;
L73: (( dom D13 ) = D14 & ( cod D13 ) = D15);
thus L74: thesis by L73 , L72 , L42;
end;
theorem
L75: (for B15 , B16 being RingMorphism holds (( dom B15 ) = ( cod B16 ) implies (ex R8 being Ring st (ex R9 being Ring st (ex R10 being Ring st (R8 <= R9 & R9 <= R10 & ( the RingMorphismStr of B15 ) is (Morphism of R9 , R10) & ( the RingMorphismStr of B16 ) is (Morphism of R8 , R9)))))))
proof
defpred S1[ RingMorphism , RingMorphism ] means ( dom $1 ) = ( cod $2 );
let C24 , C25 being RingMorphism;
assume that
L76: S1[ C24 , C25 ];
L77: ((ex R9 being Ring st (ex R10 being Ring st (R9 <= R10 & ( dom C24 ) = R9 & ( cod C24 ) = R10 & ( the RingMorphismStr of C24 ) is (Morphism of R9 , R10)))) & (ex B17 , B18 being Ring st (B17 <= B18 & ( dom C25 ) = B17 & ( cod C25 ) = B18 & ( the RingMorphismStr of C25 ) is (Morphism of B17 , B18)))) by L70;
thus L78: thesis by L77 , L76;
end;
theorem
L79: (for B19 being  strict RingMorphism holds (B19 is (Morphism of ( dom B19 ) , ( cod B19 )) & ( dom B19 ) <= ( cod B19 )))
proof
let C26 being  strict RingMorphism;
L80: (ex R6 being Ring st (ex R7 being Ring st (R6 <= R7 & ( dom C26 ) = R6 & ( cod C26 ) = R7 & C26 is (Morphism of R6 , R7)))) by L70;
thus L81: thesis by L80;
end;
L82: (for R6 being Ring holds (for R7 being Ring holds (for B20 being (Morphism of R6 , R7) holds (R6 <= R7 implies (ex B21 being (Function of R6 , R7) st (B20 = RingMorphismStr (# R6 , R7 , B21 #) & B21 is  linear))))))
proof
let R6 being Ring;
let R7 being Ring;
let C27 being (Morphism of R6 , R7);
assume that
L83: R6 <= R7;
L84: (the Cod of C27) = ( cod C27 )
.= R7 by L83 , L42;
L85: (the Dom of C27) = ( dom C27 )
.= R6 by L83 , L42;
reconsider D16 = (the Fun of C27) as (Function of R6 , R7) by L85 , L84;
take D16;
thus L86: thesis by L85 , L84 , L54;
end;
L87: (for R6 being Ring holds (for R7 being Ring holds (for B22 being (Morphism of R6 , R7) holds (R6 <= R7 implies (ex B23 being (Function of R6 , R7) st B22 = RingMorphismStr (# R6 , R7 , B23 #))))))
proof
let R6 being Ring;
let R7 being Ring;
let C28 being (Morphism of R6 , R7);
assume L88: R6 <= R7;
consider C29 being (Function of R6 , R7) such that L89: C28 = RingMorphismStr (# R6 , R7 , C29 #) and L90: C29 is  linear by L88 , L82;
take C29;
thus L91: thesis by L89;
end;
theorem
L92: (for B24 being  strict RingMorphism holds (ex R6 being Ring st (ex R7 being Ring st (ex B25 being (Function of R6 , R7) st (B24 is (Morphism of R6 , R7) & B24 = RingMorphismStr (# R6 , R7 , B25 #) & B25 is  linear)))))
proof
let C30 being  strict RingMorphism;
consider R6 being Ring, R7 being Ring such that L93: R6 <= R7 and L94: ( dom C30 ) = R6 and L95: ( cod C30 ) = R7 and L96: C30 is (Morphism of R6 , R7) by L70;
reconsider D17 = C30 as (Morphism of R6 , R7) by L96;
consider C31 being (Function of R6 , R7) such that L97: (D17 = RingMorphismStr (# R6 , R7 , C31 #) & C31 is  linear) by L93 , L82;
take R6;
take R7;
take C31;
thus L98: thesis by L97;
end;
definition
let C32 , C33 being RingMorphism;
assume L99: ( dom C32 ) = ( cod C33 );
func C32 * C33 ->  strict RingMorphism means 
:L100: (for R8 being Ring holds (for R9 being Ring holds (for R10 being Ring holds (for B26 being (Function of R9 , R10) holds (for B27 being (Function of R8 , R9) holds ((( the RingMorphismStr of C32 ) = RingMorphismStr (# R9 , R10 , B26 #) & ( the RingMorphismStr of C33 ) = RingMorphismStr (# R8 , R9 , B27 #)) implies it = RingMorphismStr (# R8 , R10 , ( B26 * B27 ) #)))))));
existence
proof
consider C34 , C35 , C36 being Ring such that L101: C34 <= C35 and L102: (C35 <= C36 & ( the RingMorphismStr of C32 ) is (Morphism of C35 , C36)) and L103: ( the RingMorphismStr of C33 ) is (Morphism of C34 , C35) by L99 , L75;
consider C37 being (Function of C35 , C36) such that L104: ( the RingMorphismStr of C32 ) = RingMorphismStr (# C35 , C36 , C37 #) and L105: C37 is  linear by L102 , L82;
consider C38 being (Function of C34 , C35) such that L106: ( the RingMorphismStr of C33 ) = RingMorphismStr (# C34 , C35 , C38 #) and L107: C38 is  linear by L101 , L103 , L82;
L108: ( C37 * C38 ) is  linear by L105 , L107 , L4;
reconsider D18 = RingMorphismStr (# C34 , C36 , ( C37 * C38 ) #) as  strict RingMorphism by L108 , L65;
take D18;
thus L109: thesis by L104 , L106;
end;
uniqueness
proof
consider C39 , C40 being Ring such that L110: C39 <= C40 and L111: ( dom C33 ) = C39 and L112: ( cod C33 ) = C40 and L113: ( the RingMorphismStr of C33 ) is (Morphism of C39 , C40) by L70;
reconsider D19 = ( the RingMorphismStr of C33 ) as (Morphism of C39 , C40) by L113;
let C41 , C42 being  strict RingMorphism;
assume that
L114: (for R8 being Ring holds (for R9 being Ring holds (for R10 being Ring holds (for B28 being (Function of R9 , R10) holds (for B29 being (Function of R8 , R9) holds ((( the RingMorphismStr of C32 ) = RingMorphismStr (# R9 , R10 , B28 #) & ( the RingMorphismStr of C33 ) = RingMorphismStr (# R8 , R9 , B29 #)) implies C41 = RingMorphismStr (# R8 , R10 , ( B28 * B29 ) #)))))))
and
L115: (for R8 being Ring holds (for R9 being Ring holds (for R10 being Ring holds (for B30 being (Function of R9 , R10) holds (for B31 being (Function of R8 , R9) holds ((( the RingMorphismStr of C32 ) = RingMorphismStr (# R9 , R10 , B30 #) & ( the RingMorphismStr of C33 ) = RingMorphismStr (# R8 , R9 , B31 #)) implies C42 = RingMorphismStr (# R8 , R10 , ( B30 * B31 ) #)))))));
consider C43 , C44 being Ring such that L116: C43 <= C44 and L117: ( dom C32 ) = C43 and L118: ( cod C32 ) = C44 and L119: ( the RingMorphismStr of C32 ) is (Morphism of C43 , C44) by L70;
reconsider D20 = D19 as (Morphism of C39 , C43) by L99 , L117 , L112;
consider C45 being (Function of C39 , C43) such that L120: D20 = RingMorphismStr (# C39 , C43 , C45 #) by L99 , L117 , L111;
reconsider D21 = ( the RingMorphismStr of C32 ) as (Morphism of C43 , C44) by L119;
consider C46 being (Function of C43 , C44) such that L121: D21 = RingMorphismStr (# C43 , C44 , C46 #) by L116 , L87;
thus L122: C41 = RingMorphismStr (# C39 , C44 , ( C46 * C45 ) #) by L114 , L121 , L120
.= C42 by L115 , L121 , L120;
end;
end;
theorem
L124: (for R8 being Ring holds (for R9 being Ring holds (for R10 being Ring holds ((R8 <= R9 & R9 <= R10) implies R8 <= R10))))
proof
let R8 being Ring;
let R9 being Ring;
let R10 being Ring;
assume that
L125: R8 <= R9
and
L126: R9 <= R10;
consider C47 being RingMorphism such that L127: ( dom C47 ) = R8 and L128: ( cod C47 ) = R9 by L125 , L33;
reconsider D22 = ( the RingMorphismStr of C47 ) as RingMorphism by L36;
L129: (( dom D22 ) = R8 & ( cod D22 ) = R9) by L127 , L128;
reconsider D23 = D22 as (Morphism of R8 , R9) by L129 , L125 , L42;
consider C48 being (Function of R8 , R9) such that L130: D23 = RingMorphismStr (# R8 , R9 , C48 #) by L125 , L87;
consider C49 being RingMorphism such that L131: ( dom C49 ) = R9 and L132: ( cod C49 ) = R10 by L126 , L33;
reconsider D24 = ( the RingMorphismStr of C49 ) as RingMorphism by L36;
L133: (( dom D24 ) = R9 & ( cod D24 ) = R10) by L131 , L132;
reconsider D25 = D24 as (Morphism of R9 , R10) by L133 , L126 , L42;
consider C50 being (Function of R9 , R10) such that L134: D25 = RingMorphismStr (# R9 , R10 , C50 #) by L126 , L87;
L135: ( dom D24 ) = ( cod D22 ) by L128 , L131;
L136: ( D24 * D22 ) = RingMorphismStr (# R8 , R10 , ( C50 * C48 ) #) by L135 , L134 , L130 , L100;
L137: (( dom ( D24 * D22 ) ) = R8 & ( cod ( D24 * D22 ) ) = R10) by L136;
thus L138: thesis by L137 , L33;
end;
theorem
L139: (for R8 being Ring holds (for R9 being Ring holds (for R10 being Ring holds (for B32 being (Morphism of R9 , R10) holds (for B33 being (Morphism of R8 , R9) holds ((R8 <= R9 & R9 <= R10) implies ( B32 * B33 ) is (Morphism of R8 , R10)))))))
proof
let R8 being Ring;
let R9 being Ring;
let R10 being Ring;
let C51 being (Morphism of R9 , R10);
let C52 being (Morphism of R8 , R9);
assume that
L140: R8 <= R9
and
L141: R9 <= R10;
consider C53 being (Function of R9 , R10) such that L142: C51 = RingMorphismStr (# R9 , R10 , C53 #) by L141 , L87;
consider C54 being (Function of R8 , R9) such that L143: C52 = RingMorphismStr (# R8 , R9 , C54 #) by L140 , L87;
L144: ( dom C51 ) = R9 by L141 , L42
.= ( cod C52 ) by L140 , L42;
L145: ( C51 * C52 ) = RingMorphismStr (# R8 , R10 , ( C53 * C54 ) #) by L144 , L142 , L143 , L100;
L146: (( dom ( C51 * C52 ) ) = R8 & ( cod ( C51 * C52 ) ) = R10) by L145;
L147: R8 <= R10 by L140 , L141 , L124;
thus L148: thesis by L147 , L146 , L42;
end;
definition
let R8 being Ring;
let R9 being Ring;
let R10 being Ring;
let C55 being (Morphism of R9 , R10);
let C56 being (Morphism of R8 , R9);
assume L149: (R8 <= R9 & R9 <= R10);
func C55 *' C56 ->  strict (Morphism of R8 , R10) equals 
:L150: ( C55 * C56 );
coherence by L149 , L139;
end;
theorem
L152: (for B34 , B35 being  strict RingMorphism holds (( dom B35 ) = ( cod B34 ) implies (ex R8 being Ring st (ex R9 being Ring st (ex R10 being Ring st (ex B36 being (Function of R8 , R9) st (ex B37 being (Function of R9 , R10) st (B34 = RingMorphismStr (# R8 , R9 , B36 #) & B35 = RingMorphismStr (# R9 , R10 , B37 #) & ( B35 * B34 ) = RingMorphismStr (# R8 , R10 , ( B37 * B36 ) #)))))))))
proof
let C57 , C58 being  strict RingMorphism;
assume that
L153: ( dom C58 ) = ( cod C57 );
set D26 = ( dom C57 );
set D27 = ( cod C57 );
set D28 = ( cod C58 );
reconsider D29 = C58 as (Morphism of D27 , D28) by L153 , L79;
consider C59 being (Function of D27 , D28) such that L154: D29 = RingMorphismStr (# D27 , D28 , C59 #) by L153;
reconsider D30 = C57 as (Morphism of D26 , D27) by L79;
consider C60 being (Function of D26 , D27) such that L155: D30 = RingMorphismStr (# D26 , D27 , C60 #);
take D26;
take D27;
take D28;
take C60;
take C59;
thus L156: thesis by L153 , L155 , L154 , L100;
end;
theorem
L157: (for B38 , B39 being  strict RingMorphism holds (( dom B39 ) = ( cod B38 ) implies (( dom ( B39 * B38 ) ) = ( dom B38 ) & ( cod ( B39 * B38 ) ) = ( cod B39 ))))
proof
let C61 , C62 being  strict RingMorphism;
assume L158: ( dom C62 ) = ( cod C61 );
L159: (ex B40 , B41 , B42 being Ring st (ex B43 being (Function of B40 , B41) st (ex B44 being (Function of B41 , B42) st (C61 = RingMorphismStr (# B40 , B41 , B43 #) & C62 = RingMorphismStr (# B41 , B42 , B44 #) & ( C62 * C61 ) = RingMorphismStr (# B40 , B42 , ( B44 * B43 ) #))))) by L158 , L152;
thus L160: ( dom ( C62 * C61 ) ) = ( dom C61 ) by L159;
thus L161: thesis by L159;
end;
theorem
L162: (for R8 being Ring holds (for R9 being Ring holds (for R10 being Ring holds (for R11 being Ring holds (for B45 being (Morphism of R8 , R9) holds (for B46 being (Morphism of R9 , R10) holds (for B47 being (Morphism of R10 , R11) holds ((R8 <= R9 & R9 <= R10 & R10 <= R11) implies ( B47 * ( B46 * B45 ) ) = ( ( B47 * B46 ) * B45 )))))))))
proof
let R8 being Ring;
let R9 being Ring;
let R10 being Ring;
let R11 being Ring;
