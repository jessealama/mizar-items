definition
let R1 being RealLinearSpace;
let R3 being (Subspace of R1);
let R4 being (Subspace of R1);
func R3 + R4 ->  strict (Subspace of R1) means
:L1: (the carrier of it) = { ( R9 + R6 ) where R9 is (VECTOR of R1) , R6 is (VECTOR of R1) : (R9 in R3 & R6 in R4) };
existence
proof
reconsider D1 = (the carrier of R3) , D2 = (the carrier of R4) as (Subset of R1) by RLSUB_1:def 2;
set D3 = { ( R9 + R6 ) where R9 is (VECTOR of R1) , R6 is (VECTOR of R1) : (R9 in R3 & R6 in R4) };
L2: D3 c= (the carrier of R1)
proof
let C1 being set;
assume L3: C1 in D3;
L4: (ex R10 being (VECTOR of R1) st (ex R11 being (VECTOR of R1) st (C1 = ( R10 + R11 ) & R10 in R3 & R11 in R4))) by L3;
thus L5: thesis by L4;
end;
reconsider D4 = D3 as (Subset of R1) by L2;
L6: ( 0. R1 ) = ( ( 0. R1 ) + ( 0. R1 ) ) by RLVECT_1:4;
L7: (( 0. R1 ) in R3 & ( 0. R1 ) in R4) by RLSUB_1:17;
L8: ( 0. R1 ) in D4 by L7 , L6;
L9: D4 = { ( R9 + R6 ) where R9 is (VECTOR of R1) , R6 is (VECTOR of R1) : (R9 in D1 & R6 in D2) }
proof
thus L10: D4 c= { ( R9 + R6 ) where R9 is (VECTOR of R1) , R6 is (VECTOR of R1) : (R9 in D1 & R6 in D2) }
proof
let C2 being set;
assume L11: C2 in D4;
consider R9 being (VECTOR of R1), R6 being (VECTOR of R1) such that L12: C2 = ( R9 + R6 ) and L13: (R9 in R3 & R6 in R4) by L11;
L14: (R9 in D1 & R6 in D2) by L13 , STRUCT_0:def 5;
thus L15: thesis by L14 , L12;
end;

let C3 being set;
assume L16: C3 in { ( R9 + R6 ) where R9 is (VECTOR of R1) , R6 is (VECTOR of R1) : (R9 in D1 & R6 in D2) };
consider R9 being (VECTOR of R1), R6 being (VECTOR of R1) such that L17: C3 = ( R9 + R6 ) and L18: (R9 in D1 & R6 in D2) by L16;
L19: (R9 in R3 & R6 in R4) by L18 , STRUCT_0:def 5;
thus L20: thesis by L19 , L17;
end;
L21: (D1 is  linearly-closed & D2 is  linearly-closed) by RLSUB_1:34;
thus L22: thesis by L21 , L8 , L9 , RLSUB_1:6 , RLSUB_1:35;
end;
uniqueness by RLSUB_1:30;
end;
definition
let R1 being RealLinearSpace;
let R3 being (Subspace of R1);
let R4 being (Subspace of R1);
func R3 /\ R4 ->  strict (Subspace of R1) means
:L24: (the carrier of it) = ( (the carrier of R3) /\ (the carrier of R4) );
existence
proof
set D5 = (the carrier of R4);
set D6 = (the carrier of R3);
set D7 = (the carrier of R1);
L25: ( 0. R1 ) in R4 by RLSUB_1:17;
L26: ( 0. R1 ) in D5 by L25 , STRUCT_0:def 5;
L27: (D6 c= D7 & D5 c= D7) by RLSUB_1:def 2;
L28: ( D6 /\ D5 ) c= ( D7 /\ D7 ) by L27 , XBOOLE_1:27;
reconsider D8 = D6 , D9 = D5 , D10 = ( D6 /\ D5 ) as (Subset of (the carrier of R1)) by L28 , RLSUB_1:def 2;
L29: (D8 is  linearly-closed & D9 is  linearly-closed) by RLSUB_1:34;
L30: D10 is  linearly-closed by L29 , RLSUB_1:7;
L31: ( 0. R1 ) in R3 by RLSUB_1:17;
L32: ( 0. R1 ) in D6 by L31 , STRUCT_0:def 5;
L33: ( D6 /\ D5 ) <> ( {} ) by L32 , L26 , XBOOLE_0:def 4;
thus L34: thesis by L33 , L30 , RLSUB_1:35;
end;
uniqueness by RLSUB_1:30;
end;
theorem
L36: (for R1 being RealLinearSpace holds (for R3 being (Subspace of R1) holds (for R4 being (Subspace of R1) holds (for R17 being set holds (R17 in ( R3 + R4 ) iff (ex R10 being (VECTOR of R1) st (ex R11 being (VECTOR of R1) st (R10 in R3 & R11 in R4 & R17 = ( R10 + R11 )))))))))
proof
let R1 being RealLinearSpace;
let R3 being (Subspace of R1);
let R4 being (Subspace of R1);
let R17 being set;
thus L37: (R17 in ( R3 + R4 ) implies (ex R10 being (VECTOR of R1) st (ex R11 being (VECTOR of R1) st (R10 in R3 & R11 in R4 & R17 = ( R10 + R11 )))))
proof
assume L38: R17 in ( R3 + R4 );
L39: R17 in (the carrier of ( R3 + R4 )) by L38 , STRUCT_0:def 5;
L40: R17 in { ( R9 + R6 ) where R9 is (VECTOR of R1) , R6 is (VECTOR of R1) : (R9 in R3 & R6 in R4) } by L39 , L1;
consider R10 being (VECTOR of R1), R11 being (VECTOR of R1) such that L41: (R17 = ( R10 + R11 ) & R10 in R3 & R11 in R4) by L40;
take R10;
take R11;
thus L42: thesis by L41;
end;

given R10 being (VECTOR of R1) , R11 being (VECTOR of R1) such that
L43: (R10 in R3 & R11 in R4 & R17 = ( R10 + R11 ));

L44: R17 in { ( R9 + R6 ) where R9 is (VECTOR of R1) , R6 is (VECTOR of R1) : (R9 in R3 & R6 in R4) } by L43;
L45: R17 in (the carrier of ( R3 + R4 )) by L44 , L1;
thus L46: thesis by L45 , STRUCT_0:def 5;
end;
theorem
L47: (for R1 being RealLinearSpace holds (for R3 being (Subspace of R1) holds (for R4 being (Subspace of R1) holds (for R9 being (VECTOR of R1) holds ((R9 in R3 or R9 in R4) implies R9 in ( R3 + R4 ))))))
proof
let R1 being RealLinearSpace;
let R3 being (Subspace of R1);
let R4 being (Subspace of R1);
let R9 being (VECTOR of R1);
assume L48: (R9 in R3 or R9 in R4);
L49:
now
per cases  by L48;
suppose L50: R9 in R3;

L51: (R9 = ( R9 + ( 0. R1 ) ) & ( 0. R1 ) in R4) by RLSUB_1:17 , RLVECT_1:4;
thus L52: thesis by L51 , L50 , L36;
end;
suppose L53: R9 in R4;

L54: (R9 = ( ( 0. R1 ) + R9 ) & ( 0. R1 ) in R3) by RLSUB_1:17 , RLVECT_1:4;
thus L55: thesis by L54 , L53 , L36;
end;
end;
thus L57: thesis by L49;
end;
theorem
L58: (for R1 being RealLinearSpace holds (for R3 being (Subspace of R1) holds (for R4 being (Subspace of R1) holds (for R17 being set holds (R17 in ( R3 /\ R4 ) iff (R17 in R3 & R17 in R4))))))
proof
let R1 being RealLinearSpace;
let R3 being (Subspace of R1);
let R4 being (Subspace of R1);
let R17 being set;
L59: (R17 in ( R3 /\ R4 ) iff R17 in (the carrier of ( R3 /\ R4 ))) by STRUCT_0:def 5;
L60: (R17 in ( R3 /\ R4 ) iff R17 in ( (the carrier of R3) /\ (the carrier of R4) )) by L59 , L24;
L61: (R17 in ( R3 /\ R4 ) iff (R17 in (the carrier of R3) & R17 in (the carrier of R4))) by L60 , XBOOLE_0:def 4;
thus L62: thesis by L61 , STRUCT_0:def 5;
end;
L63: (for R1 being RealLinearSpace holds (for R3 being (Subspace of R1) holds (for R4 being (Subspace of R1) holds ( R3 + R4 ) = ( R4 + R3 ))))
proof
let R1 being RealLinearSpace;
let R3 being (Subspace of R1);
let R4 being (Subspace of R1);
set D11 = { ( R9 + R6 ) where R9 is (VECTOR of R1) , R6 is (VECTOR of R1) : (R9 in R3 & R6 in R4) };
set D12 = { ( R9 + R6 ) where R9 is (VECTOR of R1) , R6 is (VECTOR of R1) : (R9 in R4 & R6 in R3) };
L64: D12 c= D11
proof
let C4 being set;
assume L65: C4 in D12;
L66: (ex R9 being (VECTOR of R1) st (ex R6 being (VECTOR of R1) st (C4 = ( R9 + R6 ) & R9 in R4 & R6 in R3))) by L65;
thus L67: thesis by L66;
end;
L68: (the carrier of ( R3 + R4 )) = D11 by L1;
L69: D11 c= D12
proof
let C5 being set;
assume L70: C5 in D11;
L71: (ex R9 being (VECTOR of R1) st (ex R6 being (VECTOR of R1) st (C5 = ( R9 + R6 ) & R9 in R3 & R6 in R4))) by L70;
thus L72: thesis by L71;
end;
L73: D11 = D12 by L69 , L64 , XBOOLE_0:def 10;
thus L74: thesis by L73 , L68 , L1;
end;
L75: (for R1 being RealLinearSpace holds (for R3 being (Subspace of R1) holds (for R4 being (Subspace of R1) holds (the carrier of R3) c= (the carrier of ( R3 + R4 )))))
proof
let R1 being RealLinearSpace;
let R3 being (Subspace of R1);
let R4 being (Subspace of R1);
let C6 being set;
set D13 = { ( R9 + R6 ) where R9 is (VECTOR of R1) , R6 is (VECTOR of R1) : (R9 in R3 & R6 in R4) };
assume L76: C6 in (the carrier of R3);
reconsider D14 = C6 as (Element of R3) by L76;
reconsider D15 = D14 as (VECTOR of R1) by RLSUB_1:10;
L77: D15 = ( D15 + ( 0. R1 ) ) by RLVECT_1:4;
L78: (D15 in R3 & ( 0. R1 ) in R4) by RLSUB_1:17 , STRUCT_0:def 5;
L79: C6 in D13 by L78 , L77;
thus L80: thesis by L79 , L1;
end;
L81: (for R1 being RealLinearSpace holds (for R3 being (Subspace of R1) holds (for B1 being  strict (Subspace of R1) holds ((the carrier of R3) c= (the carrier of B1) implies ( R3 + B1 ) = B1))))
proof
let R1 being RealLinearSpace;
let R3 being (Subspace of R1);
let C7 being  strict (Subspace of R1);
assume L82: (the carrier of R3) c= (the carrier of C7);
L83: (the carrier of ( R3 + C7 )) c= (the carrier of C7)
proof
let C8 being set;
assume L84: C8 in (the carrier of ( R3 + C7 ));
L85: C8 in { ( R9 + R6 ) where R9 is (VECTOR of R1) , R6 is (VECTOR of R1) : (R9 in R3 & R6 in C7) } by L84 , L1;
consider R9 being (VECTOR of R1), R6 being (VECTOR of R1) such that L86: C8 = ( R9 + R6 ) and L87: R9 in R3 and L88: R6 in C7 by L85;
L89: R3 is (Subspace of C7) by L82 , RLSUB_1:28;
L90: R9 in C7 by L89 , L87 , RLSUB_1:8;
L91: ( R9 + R6 ) in C7 by L90 , L88 , RLSUB_1:20;
thus L92: thesis by L91 , L86 , STRUCT_0:def 5;
end;
L93: ( R3 + C7 ) = ( C7 + R3 ) by L63;
L94: (the carrier of C7) c= (the carrier of ( R3 + C7 )) by L93 , L75;
L95: (the carrier of ( R3 + C7 )) = (the carrier of C7) by L94 , L83 , XBOOLE_0:def 10;
thus L96: thesis by L95 , RLSUB_1:30;
end;
theorem
L97: (for R1 being RealLinearSpace holds (for B2 being  strict (Subspace of R1) holds ( B2 + B2 ) = B2)) by L81;
theorem
L98: (for R1 being RealLinearSpace holds (for R3 being (Subspace of R1) holds (for R4 being (Subspace of R1) holds ( R3 + R4 ) = ( R4 + R3 )))) by L63;
theorem
L99: (for R1 being RealLinearSpace holds (for R3 being (Subspace of R1) holds (for R4 being (Subspace of R1) holds (for R5 being (Subspace of R1) holds ( R3 + ( R4 + R5 ) ) = ( ( R3 + R4 ) + R5 )))))
proof
let R1 being RealLinearSpace;
let R3 being (Subspace of R1);
let R4 being (Subspace of R1);
let R5 being (Subspace of R1);
set D16 = { ( R9 + R6 ) where R9 is (VECTOR of R1) , R6 is (VECTOR of R1) : (R9 in R3 & R6 in R4) };
set D17 = { ( R9 + R6 ) where R9 is (VECTOR of R1) , R6 is (VECTOR of R1) : (R9 in R4 & R6 in R5) };
set D18 = { ( R9 + R6 ) where R9 is (VECTOR of R1) , R6 is (VECTOR of R1) : (R9 in ( R3 + R4 ) & R6 in R5) };
set D19 = { ( R9 + R6 ) where R9 is (VECTOR of R1) , R6 is (VECTOR of R1) : (R9 in R3 & R6 in ( R4 + R5 )) };
L100: (the carrier of ( R3 + ( R4 + R5 ) )) = D19 by L1;
L101: D18 c= D19
proof
let C9 being set;
assume L102: C9 in D18;
consider R9 being (VECTOR of R1), R6 being (VECTOR of R1) such that L103: C9 = ( R9 + R6 ) and L104: R9 in ( R3 + R4 ) and L105: R6 in R5 by L102;
L106: R9 in (the carrier of ( R3 + R4 )) by L104 , STRUCT_0:def 5;
L107: R9 in D16 by L106 , L1;
consider R7 being (VECTOR of R1), R8 being (VECTOR of R1) such that L108: R9 = ( R7 + R8 ) and L109: R7 in R3 and L110: R8 in R4 by L107;
L111: ( R8 + R6 ) in D17 by L105 , L110;
L112: ( R8 + R6 ) in (the carrier of ( R4 + R5 )) by L111 , L1;
L113: ( R8 + R6 ) in ( R4 + R5 ) by L112 , STRUCT_0:def 5;
L114: ( R9 + R6 ) = ( R7 + ( R8 + R6 ) ) by L108 , RLVECT_1:def 3;
thus L115: thesis by L114 , L103 , L109 , L113;
end;
L116: D19 c= D18
proof
let C10 being set;
assume L117: C10 in D19;
consider R9 being (VECTOR of R1), R6 being (VECTOR of R1) such that L118: C10 = ( R9 + R6 ) and L119: R9 in R3 and L120: R6 in ( R4 + R5 ) by L117;
L121: R6 in (the carrier of ( R4 + R5 )) by L120 , STRUCT_0:def 5;
L122: R6 in D17 by L121 , L1;
consider R7 being (VECTOR of R1), R8 being (VECTOR of R1) such that L123: R6 = ( R7 + R8 ) and L124: R7 in R4 and L125: R8 in R5 by L122;
L126: ( R9 + R7 ) in D16 by L119 , L124;
L127: ( R9 + R7 ) in (the carrier of ( R3 + R4 )) by L126 , L1;
L128: ( R9 + R7 ) in ( R3 + R4 ) by L127 , STRUCT_0:def 5;
L129: ( R9 + R6 ) = ( ( R9 + R7 ) + R8 ) by L123 , RLVECT_1:def 3;
thus L130: thesis by L129 , L118 , L125 , L128;
end;
L131: D19 = D18 by L116 , L101 , XBOOLE_0:def 10;
thus L132: thesis by L131 , L100 , L1;
end;
theorem
L133: (for R1 being RealLinearSpace holds (for R3 being (Subspace of R1) holds (for R4 being (Subspace of R1) holds (R3 is (Subspace of ( R3 + R4 )) & R4 is (Subspace of ( R3 + R4 ))))))
proof
let R1 being RealLinearSpace;
let R3 being (Subspace of R1);
let R4 being (Subspace of R1);
L134: (the carrier of R3) c= (the carrier of ( R3 + R4 )) by L75;
thus L135: R3 is (Subspace of ( R3 + R4 )) by L134 , RLSUB_1:28;
L136: (the carrier of R4) c= (the carrier of ( R4 + R3 )) by L75;
L137: (the carrier of R4) c= (the carrier of ( R3 + R4 )) by L136 , L63;
thus L138: thesis by L137 , RLSUB_1:28;
end;
theorem
L139: (for R1 being RealLinearSpace holds (for R3 being (Subspace of R1) holds (for B3 being  strict (Subspace of R1) holds (R3 is (Subspace of B3) iff ( R3 + B3 ) = B3))))
proof
let R1 being RealLinearSpace;
let R3 being (Subspace of R1);
let C11 being  strict (Subspace of R1);
thus L140: (R3 is (Subspace of C11) implies ( R3 + C11 ) = C11)
proof
assume L141: R3 is (Subspace of C11);
L142: (the carrier of R3) c= (the carrier of C11) by L141 , RLSUB_1:def 2;
thus L143: thesis by L142 , L81;
end;

thus L144: thesis by L133;
end;
theorem
L145: (for R1 being RealLinearSpace holds (for B4 being  strict (Subspace of R1) holds (( ( (0). R1 ) + B4 ) = B4 & ( B4 + ( (0). R1 ) ) = B4)))
proof
let R1 being RealLinearSpace;
let C12 being  strict (Subspace of R1);
L146: ( (0). R1 ) is (Subspace of C12) by RLSUB_1:39;
L147: (the carrier of ( (0). R1 )) c= (the carrier of C12) by L146 , RLSUB_1:def 2;
thus L148: ( ( (0). R1 ) + C12 ) = C12 by L147 , L81;
thus L149: thesis by L148 , L63;
end;
theorem
L150: (for R1 being RealLinearSpace holds (( ( (0). R1 ) + ( (Omega). R1 ) ) = ( the RLSStruct of R1 ) & ( ( (Omega). R1 ) + ( (0). R1 ) ) = ( the RLSStruct of R1 ))) by L145;
theorem
L151: (for R1 being RealLinearSpace holds (for R2 being (Subspace of R1) holds (( ( (Omega). R1 ) + R2 ) = ( the RLSStruct of R1 ) & ( R2 + ( (Omega). R1 ) ) = ( the RLSStruct of R1 ))))
proof
let R1 being RealLinearSpace;
let R2 being (Subspace of R1);
L152: (the carrier of R2) c= (the carrier of R1) by RLSUB_1:def 2;
L153: ( R2 + ( (Omega). R1 ) ) = ( the RLSStruct of R1 ) by L152 , L81;
thus L154: thesis by L153 , L63;
end;
theorem
L155: (for B5 being  strict RealLinearSpace holds ( ( (Omega). B5 ) + ( (Omega). B5 ) ) = B5) by L151;
theorem
L156: (for R1 being RealLinearSpace holds (for B6 being  strict (Subspace of R1) holds ( B6 /\ B6 ) = B6))
proof
let R1 being RealLinearSpace;
let C13 being  strict (Subspace of R1);
L157: (the carrier of C13) = ( (the carrier of C13) /\ (the carrier of C13) );
thus L158: thesis by L157 , L24;
end;
theorem
L159: (for R1 being RealLinearSpace holds (for R3 being (Subspace of R1) holds (for R4 being (Subspace of R1) holds ( R3 /\ R4 ) = ( R4 /\ R3 ))))
proof
let R1 being RealLinearSpace;
let R3 being (Subspace of R1);
let R4 being (Subspace of R1);
L160: (the carrier of ( R3 /\ R4 )) = ( (the carrier of R4) /\ (the carrier of R3) ) by L24;
thus L161: thesis by L160 , L24;
end;
theorem
L162: (for R1 being RealLinearSpace holds (for R3 being (Subspace of R1) holds (for R4 being (Subspace of R1) holds (for R5 being (Subspace of R1) holds ( R3 /\ ( R4 /\ R5 ) ) = ( ( R3 /\ R4 ) /\ R5 )))))
proof
let R1 being RealLinearSpace;
let R3 being (Subspace of R1);
let R4 being (Subspace of R1);
let R5 being (Subspace of R1);
set D20 = (the carrier of R3);
set D21 = (the carrier of R4);
set D22 = (the carrier of R5);
L163: (the carrier of ( R3 /\ ( R4 /\ R5 ) )) = ( D20 /\ (the carrier of ( R4 /\ R5 )) ) by L24
.= ( D20 /\ ( D21 /\ D22 ) ) by L24
.= ( ( D20 /\ D21 ) /\ D22 ) by XBOOLE_1:16
.= ( (the carrier of ( R3 /\ R4 )) /\ D22 ) by L24;
thus L164: thesis by L163 , L24;
end;
L165: (for R1 being RealLinearSpace holds (for R3 being (Subspace of R1) holds (for R4 being (Subspace of R1) holds (the carrier of ( R3 /\ R4 )) c= (the carrier of R3))))
proof
let R1 being RealLinearSpace;
let R3 being (Subspace of R1);
let R4 being (Subspace of R1);
L166: (the carrier of ( R3 /\ R4 )) = ( (the carrier of R3) /\ (the carrier of R4) ) by L24;
thus L167: thesis by L166 , XBOOLE_1:17;
end;
theorem
L168: (for R1 being RealLinearSpace holds (for R3 being (Subspace of R1) holds (for R4 being (Subspace of R1) holds (( R3 /\ R4 ) is (Subspace of R3) & ( R3 /\ R4 ) is (Subspace of R4)))))
proof
let R1 being RealLinearSpace;
let R3 being (Subspace of R1);
let R4 being (Subspace of R1);
L169: (the carrier of ( R3 /\ R4 )) c= (the carrier of R3) by L165;
thus L170: ( R3 /\ R4 ) is (Subspace of R3) by L169 , RLSUB_1:28;
L171: (the carrier of ( R4 /\ R3 )) c= (the carrier of R4) by L165;
L172: (the carrier of ( R3 /\ R4 )) c= (the carrier of R4) by L171 , L159;
thus L173: thesis by L172 , RLSUB_1:28;
end;
theorem
L174: (for R1 being RealLinearSpace holds (for R4 being (Subspace of R1) holds (for B7 being  strict (Subspace of R1) holds (B7 is (Subspace of R4) iff ( B7 /\ R4 ) = B7))))
proof
let R1 being RealLinearSpace;
let R4 being (Subspace of R1);
let C14 being  strict (Subspace of R1);
thus L175: (C14 is (Subspace of R4) implies ( C14 /\ R4 ) = C14)
proof
