theorem
L1: (for R1 being non  empty set holds (for R2 being (ManySortedSet of R1) holds ( id R2 ) is (Equivalence_Relation of R2)))
proof
let R1 being non  empty set;
let R2 being (ManySortedSet of R1);
set D1 = ( id R2 );
L2: (for B1 being set holds (B1 in R1 implies ( D1 . B1 ) is (Relation of ( R2 . B1 ))))
proof
let C1 being set;
assume L3: C1 in R1;
L4: ( D1 . C1 ) = ( id ( R2 . C1 ) ) by L3 , MSUALG_3:def 1;
thus L5: thesis by L4;
end;
reconsider D2 = D1 as (ManySortedRelation of R2) by L2 , MSUALG_4:def 1;
L6: (for B2 being set holds (for B3 being (Relation of ( R2 . B2 )) holds ((B2 in R1 & ( D2 . B2 ) = B3) implies B3 is (Equivalence_Relation of ( R2 . B2 )))))
proof
let C2 being set;
let C3 being (Relation of ( R2 . C2 ));
assume that
L7: C2 in R1
and
L8: ( D2 . C2 ) = C3;
L9: ( D2 . C2 ) = ( id ( R2 . C2 ) ) by L7 , MSUALG_3:def 1;
thus L10: thesis by L9 , L8 , EQREL_1:3;
end;
thus L11: thesis by L6 , MSUALG_4:def 2;
end;
theorem
L12: (for R1 being non  empty set holds (for R2 being (ManySortedSet of R1) holds [| R2 , R2 |] is (Equivalence_Relation of R2)))
proof
