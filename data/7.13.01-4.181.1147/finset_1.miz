registration
let C43 being  finite set;
cluster  ->  finite for (Subset of C43);
coherence
proof
let C44 being (Subset of C43);
per cases ;
suppose L91: C44 is  empty;

thus L92: thesis by L91;
end;
suppose L93: C44 is non  empty;

consider C45 being set such that L94: C45 in C44 by L93 , XBOOLE_0:def 1;
consider C46 being Function such that L95: ( rng C46 ) = C43 and L96: ( dom C46 ) in ( omega ) by L1;
deffunc H3(set) = ( C46 . $1 );
deffunc H4(set) = C45;
defpred S2[ set ] means ( C46 . $1 ) in C44;
consider C47 being Function such that L97: ( dom C47 ) = ( dom C46 ) and L98: (for R6 being set holds (R6 in ( dom C46 ) implies ((S2[ R6 ] implies ( C47 . R6 ) = H3(R6)) & ((not S2[ R6 ]) implies ( C47 . R6 ) = H4(R6))))) from PARTFUN1:sch 1;
L99:
now
let R7 being set;
thus L100: (R7 in C44 implies (ex R6 being set st (R6 in ( dom C47 ) & R7 = ( C47 . R6 ))))
proof
assume L101: R7 in C44;
consider R6 being set such that L102: R6 in ( dom C46 ) and L103: ( C46 . R6 ) = R7 by L101 , L95 , FUNCT_1:def 3;
take R6;
thus L104: R6 in ( dom C47 ) by L97 , L102;
thus L105: thesis by L98 , L101 , L102 , L103;
end;

given R6 being set such that
L106: R6 in ( dom C47 )
and
L107: R7 = ( C47 . R6 );

per cases ;
suppose L108: ( C46 . R6 ) in C44;

thus L109: R7 in C44 by L108 , L97 , L98 , L106 , L107;
end;
suppose L110: (not ( C46 . R6 ) in C44);

thus L111: R7 in C44 by L110 , L94 , L97 , L98 , L106 , L107;
end;
end;
L113: ( rng C47 ) = C44 by L99 , FUNCT_1:def 3;
thus L114: thesis by L113 , L96 , L97 , L1;
end;
end;
end;
