scheme OLambdaC { F1() -> set , P1[set] , F2(set) -> set , F3(set) -> set } : (ex B3 being Function st (( dom B3 ) = F1() & (for B4 being Ordinal holds (B4 in F1() implies ((P1[ B4 ] implies ( B3 . B4 ) = F2(B4)) & ((not P1[ B4 ]) implies ( B3 . B4 ) = F3(B4)))))))
proof
consider C4 being Function such that L25: (( dom C4 ) = F1() & (for B5 being set holds (B5 in F1() implies ((P1[ B5 ] implies ( C4 . B5 ) = F2(B5)) & ((not P1[ B5 ]) implies ( C4 . B5 ) = F3(B5)))))) from PARTFUN1:sch 1;
take C4;
thus L26: thesis by L25;
end;
L27: (for R1 being set holds (for R2 being set holds ((R1 is  finite & R2 is  finite) implies ( R1 \/ R2 ) is  finite)))
proof
let R1 being set;
let R2 being set;
given C5 being Function such that
L28: ( rng C5 ) = R1
and
L29: ( dom C5 ) in ( omega );

given C6 being Function such that
L30: ( rng C6 ) = R2
and
L31: ( dom C6 ) in ( omega );

reconsider D2 = ( dom C5 ) , D3 = ( dom C6 ) as Ordinal by L29 , L31;
deffunc H1(Ordinal) = ( C5 . $1 );
deffunc H2(Ordinal) = ( C6 . ( $1 -^ D2 ) );
defpred S1[ set ] means $1 in D2;
consider R8 being Function such that L32: ( dom R8 ) = ( D2 +^ D3 ) and L33: (for B6 being Ordinal holds (B6 in ( D2 +^ D3 ) implies ((S1[ B6 ] implies ( R8 . B6 ) = H1(B6)) & ((not S1[ B6 ]) implies ( R8 . B6 ) = H2(B6))))) from OLambdaC;
take R8;
reconsider D4 = D2 , D5 = D3 as  natural Ordinal by L29 , L31;
thus L34: ( rng R8 ) = ( R1 \/ R2 )
proof
thus L35: ( rng R8 ) c= ( R1 \/ R2 )
proof
let R7 being set;
assume L36: R7 in ( rng R8 );
consider R6 being set such that L37: R6 in ( dom R8 ) and L38: R7 = ( R8 . R6 ) by L36 , FUNCT_1:def 3;
reconsider D6 = R6 as Ordinal by L32 , L37;
per cases ;
suppose L39: D6 in D4;

L40: R7 = ( C5 . D6 ) by L39 , L32 , L33 , L37 , L38;
L41: ( C5 . D6 ) in ( rng C5 ) by L39 , FUNCT_1:def 3;
thus L42: thesis by L41 , L28 , L40 , XBOOLE_0:def 3;
end;
suppose L43: (not D6 in D4);

L44: D4 c= D6 by L43 , ORDINAL1:16;
L45: R7 = ( C6 . ( D6 -^ D4 ) ) by L32 , L33 , L37 , L38 , L43;
L46: ( D4 +^ ( D6 -^ D4 ) ) = D6 by L44 , ORDINAL3:def 5;
L47: ( D6 -^ D4 ) in ( dom C6 ) by L46 , L32 , L37 , ORDINAL3:22;
L48: R7 in R2 by L47 , L30 , L45 , FUNCT_1:def 3;
thus L49: thesis by L48 , XBOOLE_0:def 3;
end;
end;

let R6 being set;
assume L51: R6 in ( R1 \/ R2 );
per cases  by L28 , L30 , L51 , XBOOLE_0:def 3;
suppose L52: R6 in ( rng C5 );

consider R7 being set such that L53: R7 in ( dom C5 ) and L54: R6 = ( C5 . R7 ) by L52 , FUNCT_1:def 3;
L55: ( dom C5 ) c= ( dom R8 ) by L32 , ORDINAL3:24;
L56: R6 = ( R8 . R7 ) by L55 , L32 , L33 , L53 , L54;
thus L57: thesis by L56 , L53 , L55 , FUNCT_1:def 3;
end;
suppose L58: R6 in ( rng C6 );

consider R7 being set such that L59: R7 in D5 and L60: R6 = ( C6 . R7 ) by L58 , FUNCT_1:def 3;
reconsider D7 = R7 as Ordinal by L59;
set D8 = ( D4 +^ D7 );
L61: D4 c= D8 by ORDINAL3:24;
L62: (not D8 in D4) by L61 , ORDINAL1:5;
L63: D8 in ( dom R8 ) by L32 , L59 , ORDINAL3:17;
L64: R6 = ( C6 . ( D8 -^ D4 ) ) by L60 , ORDINAL3:52
.= ( R8 . D8 ) by L32 , L33 , L62 , L63;
thus L65: thesis by L64 , L63 , FUNCT_1:def 3;
end;
end;

L67: ( D4 +^ D5 ) is  natural;
thus L68: thesis by L67 , L32 , ORDINAL1:def 12;
end;
