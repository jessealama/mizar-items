theorem
L133: (for R1 being set holds (R1 is  finite implies (for B7 being (Subset-Family of R1) holds (B7 <> ( {} ) implies (ex B8 being set st (B8 in B7 & (for B9 being set holds (B9 in B7 implies (B8 c= B9 implies B9 = B8)))))))))
proof
let R1 being set;
assume L134: R1 is  finite;
let C57 being (Subset-Family of R1);
assume that
L135: C57 <> ( {} );
consider C58 being Function such that L136: ( rng C58 ) = R1 and L137: ( dom C58 ) in ( omega ) by L134 , L1;
defpred S3[ set ] means ( C58 .: $1 ) in C57;
consider C59 being set such that L138: (for R6 being set holds (R6 in C59 iff (R6 in ( bool ( dom C58 ) ) & S3[ R6 ]))) from XBOOLE_0:sch 1;
L139: C59 c= ( bool ( dom C58 ) )
proof
let R6 being set;
assume L140: R6 in C59;
thus L141: thesis by L140 , L138;
end;
reconsider D10 = C59 as (Subset-Family of ( dom C58 )) by L139;
set D11 = the (Element of C57);
L142: D11 is (Subset of R1) by L135 , TARSKI:def 3;
L143: ( C58 .: ( C58 " D11 ) ) = D11 by L142 , L136 , FUNCT_1:77;
L144: ( C58 " D11 ) c= ( dom C58 ) by RELAT_1:132;
L145: D10 <> ( {} ) by L144 , L135 , L138 , L143;
defpred S4[ set ] means ($1 in ( omega ) implies (for B10 being (Subset-Family of $1) holds (B10 <> ( {} ) implies (ex B11 being set st (B11 in B10 & (for B12 being set holds (B12 in B10 implies (B11 c= B12 implies B12 = B11))))))));
L146: S4[ ( {} ) ]
proof
assume L147: ( {} ) in ( omega );
let C60 being (Subset-Family of ( {} ));
assume L148: C60 <> ( {} );
L149: C60 = { ( {} ) } by L148 , ZFMISC_1:1 , ZFMISC_1:33;
take ( {} );
thus L150: thesis by L149 , TARSKI:def 1;
end;
L151: (for R9 being Ordinal holds (S4[ R9 ] implies S4[ ( succ R9 ) ]))
proof
let R9 being Ordinal;
assume L152: (R9 in ( omega ) implies (for B13 being (Subset-Family of R9) holds (B13 <> ( {} ) implies (ex B14 being set st (B14 in B13 & (for B15 being set holds (B15 in B13 implies (B14 c= B15 implies B15 = B14))))))));
thus L153: (( succ R9 ) in ( omega ) implies (for B16 being (Subset-Family of ( succ R9 )) holds (B16 <> ( {} ) implies (ex B17 being set st (B17 in B16 & (for B18 being set holds (B18 in B16 implies (B17 c= B18 implies B18 = B17))))))))
proof
assume L154: ( succ R9 ) in ( omega );
L155: ( succ R9 ) c= ( omega ) by L154 , ORDINAL1:def 2;
let C61 being (Subset-Family of ( succ R9 ));
assume that
L156: C61 <> ( {} );
defpred S5[ set ] means (ex R1 being set st (R1 in C61 & $1 = ( R1 \ { R9 } )));
consider C62 being set such that L157: (for R6 being set holds (R6 in C62 iff (R6 in ( bool R9 ) & S5[ R6 ]))) from XBOOLE_0:sch 1;
L158: C62 c= ( bool R9 )
proof
let R6 being set;
assume L159: R6 in C62;
thus L160: thesis by L159 , L157;
end;
reconsider D12 = C62 as (Subset-Family of R9) by L158;
set D13 = the (Element of C61);
L161: ( succ R9 ) = ( R9 \/ { R9 } ) by ORDINAL1:def 1;
L162: D13 is (Subset of ( succ R9 )) by L156 , TARSKI:def 3;
L163: ( D13 \ { R9 } ) c= ( ( R9 \/ { R9 } ) \ { R9 } ) by L162 , L161 , XBOOLE_1:33;
L164: ( D13 \ { R9 } ) c= ( R9 \ { R9 } ) by L163 , XBOOLE_1:40;
L165: R9 in ( succ R9 ) by ORDINAL1:6;
L166: (not R9 in R9);
L167: ( D13 \ { R9 } ) c= R9 by L166 , L164 , ZFMISC_1:57;
L168: D12 <> ( {} ) by L167 , L156 , L157;
consider R5 being set such that L169: R5 in D12 and L170: (for B19 being set holds (B19 in D12 implies (R5 c= B19 implies B19 = R5))) by L168 , L152 , L155 , L165;
consider C63 being set such that L171: C63 in C61 and L172: R5 = ( C63 \ { R9 } ) by L157 , L169;
L173: R9 in { R9 } by TARSKI:def 1;
L174: (not R9 in R5) by L173 , L172 , XBOOLE_0:def 5;
L175:
now
assume L176: ( R5 \/ { R9 } ) in C61;
take D14 = ( R5 \/ { R9 } );
L177: (for B20 being set holds (B20 in C61 implies (D14 c= B20 implies B20 = D14)))
proof
let R2 being set;
assume that
L178: R2 in C61;
assume L179: D14 c= R2;
L180:
now
assume L181: R9 in R2;
set D15 = ( R2 \ { R9 } );
L182: { R9 } c= R2 by L181 , ZFMISC_1:31;
L183: R2 = ( D15 \/ { R9 } ) by L182 , XBOOLE_1:45;
L184: ( D14 \ { R9 } ) c= D15 by L179 , XBOOLE_1:33;
L185: ( R5 \ { R9 } ) c= D15 by L184 , XBOOLE_1:40;
L186: (not R9 in R5) by L172 , L173 , XBOOLE_0:def 5;
L187: R5 c= D15 by L186 , L185 , ZFMISC_1:57;
L188: D15 c= ( ( R9 \/ { R9 } ) \ { R9 } ) by L161 , L178 , XBOOLE_1:33;
L189: D15 c= ( R9 \ { R9 } ) by L188 , XBOOLE_1:40;
L190: D15 c= R9 by L189 , L166 , ZFMISC_1:57;
L191: D15 in D12 by L190 , L157 , L178;
thus L192: thesis by L191 , L170 , L183 , L187;
end;
L193:
now
assume L194: (not R9 in R2);
L195: R9 in { R9 } by TARSKI:def 1;
L196: R9 in D14 by L195 , XBOOLE_0:def 3;
thus L197: contradiction by L196 , L179 , L194;
end;
thus L198: thesis by L193 , L180;
end;
thus L199: thesis by L177 , L176;
end;
L200:
now
assume L201: (not ( R5 \/ { R9 } ) in C61);
take D16 = R5;
L202:
now
assume L203: R9 in C63;
L204: C63 = ( C63 \/ { R9 } ) by L203 , ZFMISC_1:40
.= ( R5 \/ { R9 } ) by L172 , XBOOLE_1:39;
thus L205: contradiction by L204 , L171 , L201;
end;
L206: D16 in C61 by L202 , L171 , L172 , ZFMISC_1:57;
L207: (for B21 being set holds (B21 in C61 implies (D16 c= B21 implies B21 = D16)))
proof
let R2 being set;
assume L208: R2 in C61;
L209: ( R2 \ { R9 } ) c= ( ( R9 \/ { R9 } ) \ { R9 } ) by L208 , L161 , XBOOLE_1:33;
L210: ( R2 \ { R9 } ) c= ( R9 \ { R9 } ) by L209 , XBOOLE_1:40;
L211: ( R2 \ { R9 } ) c= R9 by L210 , L166 , ZFMISC_1:57;
L212: ( R2 \ { R9 } ) in D12 by L211 , L157 , L208;
assume L213: D16 c= R2;
L214: ( D16 \ { R9 } ) c= ( R2 \ { R9 } ) by L213 , XBOOLE_1:33;
L215: D16 c= ( R2 \ { R9 } ) by L214 , L174 , ZFMISC_1:57;
L216:
now
assume L217: R9 in R2;
L218: ( D16 \/ { R9 } ) = ( ( R2 \ { R9 } ) \/ { R9 } ) by L170 , L212 , L215
.= ( R2 \/ { R9 } ) by XBOOLE_1:39
.= R2 by L217 , ZFMISC_1:40;
thus L219: contradiction by L218 , L201 , L208;
end;
L220: R2 c= R9
proof
let R6 being set;
assume that
L221: R6 in R2;
L222: (R6 in R9 or R6 in { R9 }) by L161 , L208 , L221 , XBOOLE_0:def 3;
thus L223: thesis by L222 , L216 , L221 , TARSKI:def 1;
end;
L224: ( R2 \ { R9 } ) = R2 by L216 , ZFMISC_1:57;
L225: R2 in D12 by L224 , L157 , L208 , L220;
thus L226: thesis by L225 , L170 , L213;
end;
thus L227: thesis by L207 , L206;
end;
thus L228: thesis by L200 , L175;
end;

end;
L154: (for R9 being Ordinal holds ((R9 <> ( {} ) & R9 is  limit_ordinal & (for B22 being Ordinal holds (B22 in R9 implies S4[ B22 ]))) implies S4[ R9 ]))
proof
let R9 being Ordinal;
assume that
