cluster  empty ->  finite for set;
coherence
proof
let C3 being set;
assume L21: C3 is  empty;
take ( {} );
thus L22: ( rng ( {} ) ) = C3 by L21;
thus L23: thesis by ORDINAL1:def 11;
end;
