theorem
L1: (for B1 being  add-associative  right_zeroed  right_complementable non  empty addLoopStr holds (for B2 , B3 being (Element of B1) holds ( ( B2 - B3 ) + B3 ) = B2))
proof
let C1 being  add-associative  right_zeroed  right_complementable non  empty addLoopStr;
let C2 , C3 being (Element of C1);
thus L2: ( ( C2 - C3 ) + C3 ) = ( C2 + ( ( - C3 ) + C3 ) ) by RLVECT_1:def 3
.= ( C2 + ( 0. C1 ) ) by RLVECT_1:5
.= C2 by RLVECT_1:def 4;
end;
theorem
L3: (for B4 being  add-associative  right_zeroed  right_complementable  Abelian non  empty addLoopStr holds (for B5 , B6 being (Element of B4) holds B6 = ( B5 - ( B5 - B6 ) )))
proof
let C4 being  add-associative  right_zeroed  right_complementable  Abelian non  empty addLoopStr;
let C5 , C6 being (Element of C4);
set D1 = ( C5 - C6 );
L4: ( ( D1 + C6 ) - D1 ) = ( ( C6 - D1 ) + D1 ) by RLVECT_1:28
.= C6 by L1;
thus L5: thesis by L4 , L1;
end;
theorem
L6: (for B7 being  add-associative  right_zeroed  right_complementable  Abelian non  empty addLoopStr holds (for B8 , B9 , B10 being (Element of B7) holds ( ( B8 - B9 ) - ( B10 - B9 ) ) = ( B8 - B10 )))
proof
let C7 being  add-associative  right_zeroed  right_complementable  Abelian non  empty addLoopStr;
let C8 , C9 , C10 being (Element of C7);
thus L7: ( ( C8 - C9 ) - ( C10 - C9 ) ) = ( ( ( C8 - C9 ) - C10 ) + C9 ) by RLVECT_1:29
.= ( ( ( C8 - C9 ) + C9 ) - C10 ) by RLVECT_1:28
.= ( ( C8 - ( C9 - C9 ) ) - C10 ) by RLVECT_1:29
.= ( ( C8 - ( 0. C7 ) ) - C10 ) by RLVECT_1:15
.= ( C8 - C10 ) by RLVECT_1:13;
end;
