L1: (for B1 being (Subset of ( REAL )) holds (B1 is  bounded_below implies ( -- B1 ) is  bounded_above))
proof
let C1 being (Subset of ( REAL ));
given R2 being  real number such that
L2: R2 is (LowerBound of C1);

take ( - R2 );
let C2 being  ext-real number;
assume L3: C2 in ( -- C1 );
L4: C2 in { ( - B2 ) where B2 is (Element of ( COMPLEX )) : B2 in C1 } by L3 , MEMBER_1:def 2;
consider C3 being (Element of ( COMPLEX )) such that L5: C2 = ( - C3 ) and L6: C3 in C1 by L4;
reconsider D1 = C3 as Real by L6;
L7: D1 >= R2 by L2 , L6 , XXREAL_2:def 2;
thus L8: thesis by L7 , L5 , XREAL_1:24;
end;
L9: (for B3 being non  empty set holds (for B4 being (Function of B3 , ( REAL )) holds (B4 is  with_min implies ( - B4 ) is  with_max)))
proof
let C4 being non  empty set;
let C5 being (Function of C4 , ( REAL ));
assume that
L10: ( C5 .: C4 ) is  bounded_below
and
L11: ( lower_bound ( C5 .: C4 ) ) in ( C5 .: C4 );
L12: ( -- ( C5 .: C4 ) ) = ( ( - C5 ) .: C4 ) by MEASURE6:65;
thus L13: ( ( - C5 ) .: C4 ) is  bounded_above by L12 , L10 , L1;
L14: ( upper_bound ( ( - C5 ) .: C4 ) ) = ( - ( lower_bound ( -- ( -- ( C5 .: C4 ) ) ) ) ) by L13 , L12 , MEASURE6:44
.= ( - ( lower_bound ( C5 .: C4 ) ) );
L15: ( - ( lower_bound ( C5 .: C4 ) ) ) in ( -- ( C5 .: C4 ) ) by L11 , MEMBER_1:11;
thus L16: thesis by L15 , L14 , MEASURE6:65;
end;
