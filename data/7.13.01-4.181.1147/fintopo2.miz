theorem
L39: (for R1 being non  empty RelStr holds (for R2 being (Subset of R1) holds (( R2 ` ) is  open implies R2 is  closed)))
proof
let R1 being non  empty RelStr;
let R2 being (Subset of R1);
assume L40: ( R2 ` ) is  open;
L41: ( R2 ` ) = ( ( R2 ` ) ^i ) by L40 , FIN_TOPO:def 14;
L42: ( ( R2 ` ) ^i ) = ( ( ( ( R2 ` ) ` ) ^b ) ` ) by FIN_TOPO:17
.= ( ( R2 ^b ) ` );
L43: R2 = ( ( ( R2 ^b ) ` ) ` ) by L42 , L41
.= ( R2 ^b );
thus L44: thesis by L43 , FIN_TOPO:def 15;
end;
theorem
L45: (for R1 being non  empty RelStr holds (for R2 being (Subset of R1) holds (( R2 ` ) is  closed implies R2 is  open)))
proof
