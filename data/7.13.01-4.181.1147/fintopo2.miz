theorem
L1: (for B1 being non  empty RelStr holds (for B2 , B3 being (Subset of B1) holds (B2 c= B3 implies ( B2 ^i ) c= ( B3 ^i ))))
proof
let C1 being non  empty RelStr;
let C2 , C3 being (Subset of C1);
assume L2: C2 c= C3;
let C4 being set;
assume L3: C4 in ( C2 ^i );
reconsider D1 = C4 as (Element of C1) by L3;
L4: ( U_FT D1 ) c= C2 by L3 , FIN_TOPO:7;
L5: (for B4 being (Element of C1) holds (B4 in ( U_FT D1 ) implies B4 in C3))
proof
let C5 being (Element of C1);
assume L6: C5 in ( U_FT D1 );
L7: C5 in C2 by L6 , L4;
thus L8: thesis by L7 , L2;
end;
L9: ( U_FT D1 ) c= C3 by L5 , SUBSET_1:2;
thus L10: thesis by L9;
end;
theorem
L11: (for R1 being non  empty RelStr holds (for R2 being (Subset of R1) holds ( R2 ^delta ) = ( ( R2 ^b ) /\ ( ( R2 ^i ) ` ) )))
proof
let R1 being non  empty RelStr;
let R2 being (Subset of R1);
L12: (for B5 being set holds (B5 in ( R2 ^delta ) iff B5 in ( ( R2 ^b ) /\ ( ( R2 ^i ) ` ) )))
proof
let C6 being set;
thus L13: (C6 in ( R2 ^delta ) implies C6 in ( ( R2 ^b ) /\ ( ( R2 ^i ) ` ) ))
proof
assume L14: C6 in ( R2 ^delta );
reconsider D2 = C6 as (Element of R1) by L14;
L15: ( U_FT D2 ) meets ( R2 ` ) by L14 , FIN_TOPO:5;
L16: D2 in ( ( ( ( R2 ` ) ^b ) ` ) ` ) by L15;
L17: D2 in ( ( R2 ^i ) ` ) by L16 , FIN_TOPO:17;
L18: ( U_FT D2 ) meets R2 by L14 , FIN_TOPO:5;
L19: D2 in ( R2 ^b ) by L18;
thus L20: thesis by L19 , L17 , XBOOLE_0:def 4;
end;

assume L21: C6 in ( ( R2 ^b ) /\ ( ( R2 ^i ) ` ) );
reconsider D3 = C6 as (Element of R1) by L21;
L22: C6 in ( ( R2 ^i ) ` ) by L21 , XBOOLE_0:def 4;
L23: C6 in ( ( ( ( R2 ` ) ^b ) ` ) ` ) by L22 , FIN_TOPO:17;
L24: ( U_FT D3 ) meets ( R2 ` ) by L23 , FIN_TOPO:8;
L25: C6 in ( R2 ^b ) by L21 , XBOOLE_0:def 4;
L26: ( U_FT D3 ) meets R2 by L25 , FIN_TOPO:8;
thus L27: thesis by L26 , L24;
end;
thus L28: thesis by L12 , TARSKI:1;
end;
