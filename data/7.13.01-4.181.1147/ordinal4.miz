theorem
L5: (for R2 being Ordinal-Sequence holds (for R4 being Ordinal holds (( dom R2 ) = ( succ R4 ) implies (( last R2 ) is_limes_of R2 & ( lim R2 ) = ( last R2 )))))
proof
let R2 being Ordinal-Sequence;
let R4 being Ordinal;
assume L6: ( dom R2 ) = ( succ R4 );
L7: ( last R2 ) = ( R2 . R4 ) by L6 , ORDINAL2:6;
thus L8: ( last R2 ) is_limes_of R2
proof
per cases ;
case L9: ( last R2 ) = ( {} );
take R4;
thus L10: R4 in ( dom R2 ) by L6 , ORDINAL1:6;
let R6 being Ordinal;
assume that
L11: R4 c= R6
and
L12: R6 in ( dom R2 );
L13: R6 c= R4 by L6 , L12 , ORDINAL1:22;
thus L14: thesis by L13 , L7 , L9 , L11 , XBOOLE_0:def 10;
end;
case L15: ( last R2 ) <> ( {} );
let R6 being Ordinal;
let R7 being Ordinal;
assume that
L16: R6 in ( last R2 )
and
L17: ( last R2 ) in R7;
take R4;
thus L18: R4 in ( dom R2 ) by L6 , ORDINAL1:6;
let R8 being Ordinal;
assume that
L19: R4 c= R8
and
L20: R8 in ( dom R2 );
L21: R8 c= R4 by L6 , L20 , ORDINAL1:22;
thus L22: thesis by L21 , L7 , L16 , L17 , L19 , XBOOLE_0:def 10;
end;
end;

thus L24: thesis by L8 , ORDINAL2:def 10;
end;
