let R2 being (Subset of R1);
let R3 being (Subset of R1);
assume L16: ((R2 is  everywhere_dense & R3 is  dense) or (R2 is  dense & R3 is  everywhere_dense));
L17: ( R2 /\ R3 ) <> ( {} ) by L16 , TOPS_3:17 , TOPS_3:45;
thus L18: thesis by L17 , XBOOLE_0:def 7;
end;
