theorem
L1: (for R1 being non  empty TopSpace holds (for R2 being (Subset of R1) holds (for R3 being (Subset of R1) holds (R2 , R3 constitute_a_decomposition  implies (R2 is non  empty iff R3 is  proper)))))
proof
let R1 being non  empty TopSpace;
let R2 being (Subset of R1);
let R3 being (Subset of R1);
assume L2: R2 , R3 constitute_a_decomposition ;
L3: R3 = ( R2 ` ) by L2 , TSEP_2:3;
thus L4: (R2 is non  empty implies R3 is  proper)
proof
assume L5: R2 is non  empty;
L6: R3 <> (the carrier of R1) by L5 , L3 , TOPS_3:1;
thus L7: thesis by L6 , SUBSET_1:def 6;
end;

assume L8: R3 is  proper;
L9: R2 = ( R3 ` ) by L2 , TSEP_2:3;
thus L10: thesis by L9 , L8;
end;
L11: (for R1 being non  empty TopSpace holds (for R2 being (Subset of R1) holds (for R3 being (Subset of R1) holds ((R2 is  everywhere_dense & R3 is  everywhere_dense) implies R2 meets R3))))
proof
let R1 being non  empty TopSpace;
let R2 being (Subset of R1);
let R3 being (Subset of R1);
assume L12: (R2 is  everywhere_dense & R3 is  everywhere_dense);
L13: ( R2 /\ R3 ) <> ( {} ) by L12 , TOPS_3:34 , TOPS_3:44;
thus L14: thesis by L13 , XBOOLE_0:def 7;
end;
L15: (for R1 being non  empty TopSpace holds (for R2 being (Subset of R1) holds (for R3 being (Subset of R1) holds (((R2 is  everywhere_dense & R3 is  dense) or (R2 is  dense & R3 is  everywhere_dense)) implies R2 meets R3))))
proof
