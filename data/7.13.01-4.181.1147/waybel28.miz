theorem
L1: (for B1 being  complete LATTICE holds (for B2 being (net of B1) holds ( inf B2 ) <= ( lim_inf B2 )))
proof
let C1 being  complete LATTICE;
let C2 being (net of C1);
set D1 = { ( "/\" ({ ( C2 . B4 ) where B4 is (Element of C2) : B4 >= B3 } , C1) ) where B3 is (Element of C2) : (not contradiction) };
L2: D1 c= (the carrier of C1)
proof
let C3 being set;
assume L3: C3 in D1;
L4: (ex B5 being (Element of C2) st C3 = ( "/\" ({ ( C2 . B6 ) where B6 is (Element of C2) : B6 >= B5 } , C1) )) by L3;
thus L5: thesis by L4;
end;
reconsider D2 = D1 as (Subset of C1) by L2;
set D3 = the (Element of C2);
L6: { ( C2 . B7 ) where B7 is (Element of C2) : B7 >= D3 } c= ( rng (the mapping of C2) )
proof
let C4 being set;
L7: ( dom (the mapping of C2) ) = (the carrier of C2) by FUNCT_2:def 1;
assume L8: C4 in { ( C2 . B8 ) where B8 is (Element of C2) : B8 >= D3 };
consider C5 being (Element of C2) such that L9: C4 = ( C2 . C5 ) and L10: C5 >= D3 by L8;
L11: C4 = ( (the mapping of C2) . C5 ) by L9 , WAYBEL_0:def 8;
thus L12: thesis by L11 , L7 , FUNCT_1:def 3;
end;
reconsider D4 = D2 as (Subset of C1);
set D5 = ( "/\" ({ ( C2 . B9 ) where B9 is (Element of C2) : B9 >= D3 } , C1) );
L13: ( ex_inf_of { ( C2 . B10 ) where B10 is (Element of C2) : B10 >= D3 } , C1 &  ex_inf_of ( rng (the mapping of C2) ) , C1) by YELLOW_0:17;
L14: ( "/\" ({ ( C2 . B11 ) where B11 is (Element of C2) : B11 >= D3 } , C1) ) >= ( "/\" (( rng (the mapping of C2) ) , C1) ) by L13 , L6 , YELLOW_0:35;
L15: D5 >= ( Inf (the mapping of C2) ) by L14 , YELLOW_2:def 6;
L16: ( inf C2 ) <= D5 by L15 , WAYBEL_9:def 2;
L17:  ex_sup_of D4 , C1 by YELLOW_0:17;
L18: (D5 in D4 & D4 is_<=_than ( "\/" (D4 , C1) )) by L17 , YELLOW_0:def 9;
L19: D5 <= ( "\/" (D4 , C1) ) by L18 , LATTICE3:def 9;
thus L20: thesis by L19 , L16 , YELLOW_0:def 2;
end;
theorem
L21: (for B12 being  complete LATTICE holds (for B13 being (net of B12) holds (for B14 being (Element of B12) holds ((for B15 being (subnet of B13) holds B14 = ( lim_inf B15 )) implies (B14 = ( lim_inf B13 ) & (for B16 being (subnet of B13) holds B14 >= ( inf B16 )))))))
proof
let C6 being  complete LATTICE;
let C7 being (net of C6);
let C8 being (Element of C6);
assume L22: (for B17 being (subnet of C7) holds C8 = ( lim_inf B17 ));
L23: C7 is (subnet of C7) by YELLOW_6:14;
thus L24: C8 = ( lim_inf C7 ) by L23 , L22;
let C9 being (subnet of C7);
L25: C8 = ( lim_inf C9 ) by L22;
thus L26: thesis by L25 , L1;
end;
theorem
L27: (for B18 being  complete LATTICE holds (for B19 being (net of B18) holds (for B20 being (Element of B18) holds (B19 in ( NetUniv B18 ) implies ((for B21 being (subnet of B19) holds (B21 in ( NetUniv B18 ) implies B20 = ( lim_inf B21 ))) implies (B20 = ( lim_inf B19 ) & (for B22 being (subnet of B19) holds (B22 in ( NetUniv B18 ) implies B20 >= ( inf B22 )))))))))
proof
let C10 being  complete LATTICE;
let C11 being (net of C10);
let C12 being (Element of C10);
assume L28: C11 in ( NetUniv C10 );
assume L29: (for B23 being (subnet of C11) holds (B23 in ( NetUniv C10 ) implies C12 = ( lim_inf B23 )));
L30: C11 is (subnet of C11) by YELLOW_6:14;
thus L31: C12 = ( lim_inf C11 ) by L30 , L28 , L29;
let C13 being (subnet of C11);
assume L32: C13 in ( NetUniv C10 );
L33: C12 = ( lim_inf C13 ) by L32 , L29;
thus L34: thesis by L33 , L1;
end;
