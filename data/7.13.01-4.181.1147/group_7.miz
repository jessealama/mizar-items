definition
let C1 being Relation;
attr C1 is  multMagma-yielding
means
:L1: (for B1 being set holds (B1 in ( rng C1 ) implies B1 is non  empty non  empty non  empty non  empty multMagma));
end;
registration
cluster  multMagma-yielding ->  1-sorted-yielding for Function;
coherence
proof
let C2 being Function;
assume that
L3: C2 is  multMagma-yielding;
let C3 being set;
assume L4: C3 in ( dom C2 );
L5: ( C2 . C3 ) in ( rng C2 ) by L4 , FUNCT_1:def 3;
thus L6: thesis by L5 , L3 , L1;
end;
end;
registration
let C4 being set;
