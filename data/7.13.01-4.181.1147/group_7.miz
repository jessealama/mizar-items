existence
proof
set D1 = the non  empty multMagma;
set D2 = ( C4 --> D1 );
take D2;
let C5 being set;
assume L8: C5 in ( rng D2 );
L9: (ex B2 being set st (B2 in ( dom D2 ) & C5 = ( D2 . B2 ))) by L8 , FUNCT_1:def 3;
thus L10: thesis by L9 , FUNCOP_1:7;
end;
end;
