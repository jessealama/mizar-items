theorem
L108: (for B16 , B17 being Lattice holds (( LattPOSet B16 ) = ( LattPOSet B17 ) implies ( the LattStr of B16 ) = ( the LattStr of B17 )))
proof
let C29 , C30 being Lattice;
assume that
L109: ( LattPOSet C29 ) = ( LattPOSet C30 );
reconsider D22 = H2(C30) , D23 = H3(C30) as (BinOp of H1(C29)) by L109;
L110:
now
let C31 , C32 being (Element of C29);
reconsider D24 = C31 , D25 = C32 , D26 = ( C31 "\/" C32 ) as (Element of C30) by L109;
reconsider D27 = ( D24 "\/" D25 ) as (Element of C29) by L109;
L111: C31 [= ( C31 "\/" C32 ) by LATTICES:5;
L112: C32 [= ( C32 "\/" C31 ) by LATTICES:5;
L113: D24 [= ( D24 "\/" D25 ) by LATTICES:5;
L114: D25 [= ( D25 "\/" D24 ) by LATTICES:5;
L115: [ D24 , D26 ] in ( LattRel C30 ) by L109 , L111 , FILTER_1:31;
L116: [ D25 , D26 ] in ( LattRel C30 ) by L109 , L112 , FILTER_1:31;
L117: [ C31 , D27 ] in ( LattRel C29 ) by L109 , L113 , FILTER_1:31;
L118: [ C32 , D27 ] in ( LattRel C29 ) by L109 , L114 , FILTER_1:31;
L119: C31 [= D27 by L117 , FILTER_1:31;
L120: C32 [= D27 by L118 , FILTER_1:31;
L121: D24 [= D26 by L115 , FILTER_1:31;
L122: D25 [= D26 by L116 , FILTER_1:31;
L123: ( C31 "\/" C32 ) [= D27 by L119 , L120 , FILTER_0:6;
L124: ( D24 "\/" D25 ) [= D26 by L121 , L122 , FILTER_0:6;
L125: [ D27 , ( C31 "\/" C32 ) ] in ( LattRel C29 ) by L124 , L109 , FILTER_1:31;
L126: D27 [= ( C31 "\/" C32 ) by L125 , FILTER_1:31;
thus L127: ( H2(C29) . (C31 , C32) ) = ( D22 . (C31 , C32) ) by L126 , L123 , LATTICES:8;
end;
L128: H2(C29) = D22 by L110 , BINOP_1:2;
L129:
now
let C33 , C34 being (Element of C29);
reconsider D28 = C33 , D29 = C34 , D30 = ( C33 "/\" C34 ) as (Element of C30) by L109;
reconsider D31 = ( D28 "/\" D29 ) as (Element of C29) by L109;
L130: ( C33 "/\" C34 ) [= C33 by LATTICES:6;
L131: ( C34 "/\" C33 ) [= C34 by LATTICES:6;
L132: ( D28 "/\" D29 ) [= D28 by LATTICES:6;
L133: ( D29 "/\" D28 ) [= D29 by LATTICES:6;
L134: [ D30 , D28 ] in ( LattRel C30 ) by L109 , L130 , FILTER_1:31;
L135: [ D30 , D29 ] in ( LattRel C30 ) by L109 , L131 , FILTER_1:31;
L136: [ D31 , C33 ] in ( LattRel C29 ) by L109 , L132 , FILTER_1:31;
L137: [ D31 , C34 ] in ( LattRel C29 ) by L109 , L133 , FILTER_1:31;
L138: D31 [= C33 by L136 , FILTER_1:31;
L139: D31 [= C34 by L137 , FILTER_1:31;
L140: D30 [= D28 by L134 , FILTER_1:31;
L141: D30 [= D29 by L135 , FILTER_1:31;
L142: D31 [= ( C33 "/\" C34 ) by L138 , L139 , FILTER_0:7;
L143: D30 [= ( D28 "/\" D29 ) by L140 , L141 , FILTER_0:7;
L144: [ ( C33 "/\" C34 ) , D31 ] in ( LattRel C29 ) by L143 , L109 , FILTER_1:31;
L145: ( C33 "/\" C34 ) [= D31 by L144 , FILTER_1:31;
thus L146: ( H3(C29) . (C33 , C34) ) = ( D23 . (C33 , C34) ) by L145 , L142 , LATTICES:8;
end;
thus L147: thesis by L129 , L109 , L128 , BINOP_1:2;
end;
