begin
deffunc H1(LattStr) = (the carrier of $1);
deffunc H2(LattStr) = (the L_join of $1);
deffunc H3(LattStr) = (the L_meet of $1);
definition
let C1 being set;
func BooleLatt C1 ->  strict LattStr means 
:L1: ((the carrier of it) = ( bool C1 ) & (for B1 , B2 being (Subset of C1) holds (( (the L_join of it) . (B1 , B2) ) = ( B1 \/ B2 ) & ( (the L_meet of it) . (B1 , B2) ) = ( B1 /\ B2 ))));
existence
proof
deffunc H4((Subset of C1) , (Subset of C1)) = ( $1 \/ $2 );
consider C2 being (BinOp of ( bool C1 )) such that L2: (for B3 , B4 being (Subset of C1) holds ( C2 . (B3 , B4) ) = H4(B3 , B4)) from BINOP_1:sch 4;
deffunc H5((Subset of C1) , (Subset of C1)) = ( $1 /\ $2 );
consider C3 being (BinOp of ( bool C1 )) such that L3: (for B5 , B6 being (Subset of C1) holds ( C3 . (B5 , B6) ) = H5(B5 , B6)) from BINOP_1:sch 4;
take LattStr (# ( bool C1 ) , C2 , C3 #);
thus L4: thesis by L2 , L3;
end;
uniqueness
proof
let C4 , C5 being  strict LattStr;
assume that
L5: (the carrier of C4) = ( bool C1 )
and
L6: (for B7 , B8 being (Subset of C1) holds (( H2(C4) . (B7 , B8) ) = ( B7 \/ B8 ) & ( H3(C4) . (B7 , B8) ) = ( B7 /\ B8 )))
and
L7: (the carrier of C5) = ( bool C1 )
and
L8: (for B9 , B10 being (Subset of C1) holds (( H2(C5) . (B9 , B10) ) = ( B9 \/ B10 ) & ( H3(C5) . (B9 , B10) ) = ( B9 /\ B10 )));
reconsider D1 = H2(C4) , D2 = H2(C5) , D3 = H3(C4) , D4 = H3(C5) as (BinOp of ( bool C1 )) by L5 , L7;
L9:
now
let C6 , C7 being (Subset of C1);
thus L10: ( D1 . (C6 , C7) ) = ( C6 \/ C7 ) by L6
.= ( D2 . (C6 , C7) ) by L8;
end;
L11: D1 = D2 by L9 , BINOP_1:2;
L12:
now
let C8 , C9 being (Subset of C1);
thus L13: ( D3 . (C8 , C9) ) = ( C8 /\ C9 ) by L6
.= ( D4 . (C8 , C9) ) by L8;
end;
thus L14: thesis by L12 , L5 , L7 , L11 , BINOP_1:2;
end;
end;
registration
let C10 being set;
cluster ( BooleLatt C10 ) -> non  empty;
coherence
proof
L16: (the carrier of ( BooleLatt C10 )) = ( bool C10 ) by L1;
thus L17: (the carrier of ( BooleLatt C10 )) is non  empty by L16;
end;
end;
registration
let R1 being set;
let R2 being (Element of ( BooleLatt R1 ));
let R3 being (Element of ( BooleLatt R1 ));
let C11 , C12 being set;
identify R2 "\/" R3 with C11 \/ C12 when R2 = C11 , R3 = C12;
compatibility
proof
reconsider D5 = R2 , D6 = R3 as (Subset of R1) by L1;
assume that
L19: R2 = C11
and
L20: R3 = C12;
thus L21: ( R2 "\/" R3 ) = ( H2(( BooleLatt R1 )) . (D5 , D6) )
.= ( C11 \/ C12 ) by L19 , L20 , L1;
end;
identify R2 "/\" R3 with C11 /\ C12 when R2 = C11 , R3 = C12;
compatibility
proof
reconsider D7 = R2 , D8 = R3 as (Subset of R1) by L1;
assume that
L22: R2 = C11
and
L23: R3 = C12;
thus L24: ( R2 "/\" R3 ) = ( H3(( BooleLatt R1 )) . (D7 , D8) )
.= ( C11 /\ C12 ) by L22 , L23 , L1;
end;
end;
theorem
L26: (for R1 being set holds (for R2 being (Element of ( BooleLatt R1 )) holds (for R3 being (Element of ( BooleLatt R1 )) holds (( R2 "\/" R3 ) = ( R2 \/ R3 ) & ( R2 "/\" R3 ) = ( R2 /\ R3 )))));
theorem
L27: (for R1 being set holds (for R2 being (Element of ( BooleLatt R1 )) holds (for R3 being (Element of ( BooleLatt R1 )) holds (R2 [= R3 iff R2 c= R3))))
proof
let R1 being set;
let R2 being (Element of ( BooleLatt R1 ));
let R3 being (Element of ( BooleLatt R1 ));
L28: (R2 [= R3 iff ( R2 "\/" R3 ) = R3) by LATTICES:def 3;
thus L29: thesis by L28 , XBOOLE_1:7 , XBOOLE_1:12;
end;
registration
let R1 being set;
cluster ( BooleLatt R1 ) ->  Lattice-like;
coherence
proof
L30: (for R2 being (Element of ( BooleLatt R1 )) holds (for R3 being (Element of ( BooleLatt R1 )) holds ( R2 "\/" R3 ) = ( R3 "\/" R2 )));
L31: (for R2 being (Element of ( BooleLatt R1 )) holds (for R3 being (Element of ( BooleLatt R1 )) holds (for R4 being (Element of ( BooleLatt R1 )) holds ( R2 "\/" ( R3 "\/" R4 ) ) = ( ( R2 "\/" R3 ) "\/" R4 )))) by XBOOLE_1:4;
L32: (for R2 being (Element of ( BooleLatt R1 )) holds (for R3 being (Element of ( BooleLatt R1 )) holds ( ( R2 "/\" R3 ) "\/" R3 ) = R3)) by XBOOLE_1:22;
L33: (for R2 being (Element of ( BooleLatt R1 )) holds (for R3 being (Element of ( BooleLatt R1 )) holds ( R2 "/\" R3 ) = ( R3 "/\" R2 )));
L34: (for R2 being (Element of ( BooleLatt R1 )) holds (for R3 being (Element of ( BooleLatt R1 )) holds (for R4 being (Element of ( BooleLatt R1 )) holds ( R2 "/\" ( R3 "/\" R4 ) ) = ( ( R2 "/\" R3 ) "/\" R4 )))) by XBOOLE_1:16;
L35: (for R2 being (Element of ( BooleLatt R1 )) holds (for R3 being (Element of ( BooleLatt R1 )) holds ( R2 "/\" ( R2 "\/" R3 ) ) = R2)) by XBOOLE_1:21;
L36: ( BooleLatt R1 ) is  join-commutative  join-associative  meet-absorbing  meet-commutative  meet-associative  join-absorbing by L35 , L30 , L31 , L32 , L33 , L34 , LATTICES:def 4 , LATTICES:def 5 , LATTICES:def 6 , LATTICES:def 7 , LATTICES:def 8 , LATTICES:def 9;
thus L37: thesis by L36;
end;
end;
theorem
L39: (for R1 being set holds (( BooleLatt R1 ) is  lower-bounded & ( Bottom ( BooleLatt R1 ) ) = ( {} )))
proof
let R1 being set;
L40: ( {} ) c= R1 by XBOOLE_1:2;
reconsider D9 = ( {} ) as (Element of ( BooleLatt R1 )) by L40 , L1;
L41: (for R6 being (Element of ( BooleLatt R1 )) holds ( D9 "/\" R6 ) = D9);
L42: (for R6 being (Element of ( BooleLatt R1 )) holds ( R6 "/\" D9 ) = D9);
thus L43: ( BooleLatt R1 ) is  lower-bounded
proof
take D9;
thus L44: thesis;
end;

thus L45: thesis by L43 , L41 , L42 , LATTICES:def 16;
end;
theorem
L46: (for R1 being set holds (( BooleLatt R1 ) is  upper-bounded & ( Top ( BooleLatt R1 ) ) = R1))
proof
let R1 being set;
L47: ( bool R1 ) = H1(( BooleLatt R1 )) by L1;
reconsider D10 = R1 as (Element of ( BooleLatt R1 )) by L47 , ZFMISC_1:def 1;
L48: (for R6 being (Element of ( BooleLatt R1 )) holds ( D10 "\/" R6 ) = D10) by L47 , XBOOLE_1:12;
L49: (for R6 being (Element of ( BooleLatt R1 )) holds ( R6 "\/" D10 ) = D10) by L47 , XBOOLE_1:12;
thus L50: ( BooleLatt R1 ) is  upper-bounded
proof
take D10;
thus L51: thesis by L47 , XBOOLE_1:12;
end;

thus L52: thesis by L50 , L48 , L49 , LATTICES:def 17;
end;
registration
let R1 being set;
cluster ( BooleLatt R1 ) ->  Boolean;
coherence
proof
set D11 = ( BooleLatt R1 );
L53: D11 is 0_Lattice by L39;
L54: D11 is 1_Lattice by L46;
reconsider D12 = D11 as 01_Lattice by L54 , L53;
L55: D12 is  complemented
proof
let C13 being (Element of D12);
L56: H1(D12) = ( bool R1 ) by L1;
reconsider D13 = ( R1 \ C13 ) as (Element of D12) by L1;
take D13;
thus L57: ( D13 "\/" C13 ) = ( D13 \/ C13 ) by L26
.= ( R1 \/ C13 ) by XBOOLE_1:39
.= R1 by L56 , XBOOLE_1:12
.= ( Top D12 ) by L46;
thus L58: ( C13 "\/" D13 ) = ( Top D12 ) by L57;
L59: D13 misses C13 by XBOOLE_1:79;
thus L60: ( D13 "/\" C13 ) = ( D13 /\ C13 ) by L26
.= ( {} ) by L59 , XBOOLE_0:def 7
.= ( Bottom D12 ) by L39;
thus L61: thesis by L60;
end;
L62: D12 is  distributive
proof
let C14 , C15 , C16 being (Element of D12);
thus L63: ( C14 "/\" ( C15 "\/" C16 ) ) = ( C14 /\ ( C15 "\/" C16 ) ) by L26
.= ( C14 /\ ( C15 \/ C16 ) ) by L26
.= ( ( C14 /\ C15 ) \/ ( C14 /\ C16 ) ) by XBOOLE_1:23
.= ( ( C14 "/\" C15 ) \/ ( C14 /\ C16 ) ) by L26
.= ( ( C14 "/\" C15 ) \/ ( C14 "/\" C16 ) ) by L26
.= ( ( C14 "/\" C15 ) "\/" ( C14 "/\" C16 ) ) by L26;
end;
thus L64: thesis by L62 , L55;
end;
end;
theorem
L66: (for R1 being set holds (for B11 being (Element of ( BooleLatt R1 )) holds ( B11 ` ) = ( R1 \ B11 )))
proof
let R1 being set;
set D14 = ( BooleLatt R1 );
let C17 being (Element of D14);
L67: ( ( C17 ` ) "/\" C17 ) = ( Bottom D14 ) by LATTICES:20;
L68: ( C17 "\/" ( C17 ` ) ) = ( Top D14 ) by LATTICES:21;
L69: ( Bottom D14 ) = ( {} ) by L39;
L70: ( Top D14 ) = R1 by L46;
L71: ( C17 ` ) misses C17 by L67 , L69 , XBOOLE_0:def 7;
L72: ( R1 \ C17 ) = ( ( C17 \ C17 ) \/ ( ( C17 ` ) \ C17 ) ) by L68 , L70 , XBOOLE_1:42;
L73: ( C17 \ C17 ) = ( {} ) by XBOOLE_1:37;
thus L74: thesis by L73 , L71 , L72 , XBOOLE_1:83;
end;
begin
definition
let C18 being Lattice;
redefine func LattRel C18 -> (Order of (the carrier of C18));

coherence
proof
L75: ( LattRel C18 ) = { [ B12 , B13 ] where B12 is (Element of C18) , B13 is (Element of C18) : B12 [= B13 } by FILTER_1:def 8;
L76: ( LattRel C18 ) c= [: H1(C18) , H1(C18) :]
proof
let C19 , C20 being set;
assume L77: [ C19 , C20 ] in ( LattRel C18 );
L78: (ex B14 , B15 being (Element of C18) st ([ C19 , C20 ] = [ B14 , B15 ] & B14 [= B15)) by L77 , L75;
thus L79: thesis by L78 , ZFMISC_1:87;
end;
reconsider D15 = ( LattRel C18 ) as (Relation of H1(C18)) by L76;
L80: D15 is_reflexive_in H1(C18)
proof
let C21 being set;
assume L81: C21 in H1(C18);
reconsider D16 = C21 as (Element of C18) by L81;
L82: D16 [= D16;
thus L83: thesis by L82 , FILTER_1:31;
end;
L84: D15 is_antisymmetric_in H1(C18)
proof
let C22 , C23 being set;
assume that
L85: C22 in H1(C18)
and
L86: C23 in H1(C18);
reconsider D17 = C22 , D18 = C23 as (Element of C18) by L85 , L86;
assume that
L87: [ C22 , C23 ] in D15
and
L88: [ C23 , C22 ] in D15;
L89: D17 [= D18 by L87 , FILTER_1:31;
L90: D18 [= D17 by L88 , FILTER_1:31;
thus L91: thesis by L90 , L89 , LATTICES:8;
end;
L92: D15 is_transitive_in H1(C18)
proof
let C24 , C25 , C26 being set;
assume that
L93: C24 in H1(C18)
and
L94: C25 in H1(C18)
and
L95: C26 in H1(C18);
reconsider D19 = C24 , D20 = C25 , D21 = C26 as (Element of C18) by L93 , L94 , L95;
assume that
L96: [ C24 , C25 ] in D15
and
L97: [ C25 , C26 ] in D15;
L98: D19 [= D20 by L96 , FILTER_1:31;
L99: D20 [= D21 by L97 , FILTER_1:31;
L100: D19 [= D21 by L99 , L98 , LATTICES:7;
thus L101: thesis by L100 , FILTER_1:31;
end;
L102: ( dom D15 ) = H1(C18) by L80 , ORDERS_1:13;
L103: ( field D15 ) = H1(C18) by L80 , ORDERS_1:13;
thus L104: thesis by L103 , L80 , L84 , L92 , L102 , PARTFUN1:def 2 , RELAT_2:def 9 , RELAT_2:def 12 , RELAT_2:def 16;
end;
end;
