assume L8: (for B13 , B14 being (Element of C4) holds (B13 <= B14 implies ( C6 . B13 ) <= ( C6 . B14 )));
thus L9: (for B15 , B16 being (Element of C4) holds (B15 <= B16 implies (for B17 , B18 being (Element of C5) holds ((B17 = ( C6 . B15 ) & B18 = ( C6 . B16 )) implies B17 <= B18)))) by L8;
end;
