let R2 being (Element of R1);
let R4 being (Element of R1);
let R6 being (Element of R1);
let R3 being (Element of R1);
let R5 being (Element of R1);
let R7 being (Element of R1);
assume that
L32: R20 is  being_line
and
L33: R21 is  being_line
and
