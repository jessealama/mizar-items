theorem
L121: (for R6 being Field holds (for B19 , B20 being (FinSequence of R6) holds (for B21 being (Element of R6) holds (( len B19 ) = ( len B20 ) implies (( mlt (( B21 * B19 ) , B20) ) = ( B21 * ( mlt (B19 , B20) ) ) & ( mlt (B19 , ( B21 * B20 )) ) = ( B21 * ( mlt (B19 , B20) ) ))))))
proof
let R6 being Field;
let C22 , C23 being (FinSequence of R6);
let C24 being (Element of R6);
assume L122: ( len C22 ) = ( len C23 );
reconsider D13 = C23 as (Element of ( ( len C22 ) -tuples_on (the carrier of R6) )) by L122 , L3;
reconsider D14 = C22 as (Element of ( ( len C22 ) -tuples_on (the carrier of R6) )) by L3;
thus L123: ( mlt (( C24 * C22 ) , C23) ) = ( C24 * ( mlt (D14 , D13) ) ) by FVSUM_1:69
.= ( C24 * ( mlt (C22 , C23) ) );
thus L124: ( mlt (C22 , ( C24 * C23 )) ) = ( C24 * ( mlt (D14 , D13) ) ) by FVSUM_1:69
.= ( C24 * ( mlt (C22 , C23) ) );
end;
