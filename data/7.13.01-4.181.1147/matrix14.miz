theorem
L3: (for R7 being non  empty addLoopStr holds (for B1 being (FinSequence of R7) holds B1 is (Element of ( ( len B1 ) -tuples_on (the carrier of R7) ))))
proof
let R7 being non  empty addLoopStr;
let C3 being (FinSequence of R7);
L4: C3 is (Element of ( (the carrier of R7) * )) by FINSEQ_1:def 11;
L5: C3 in { B2 where B2 is (Element of ( (the carrier of R7) * )) : ( len B2 ) = ( len C3 ) } by L4;
thus L6: thesis by L5 , FINSEQ_2:def 4;
end;
theorem
L7: (for R7 being non  empty addLoopStr holds (for B3 , B4 being (FinSequence of R7) holds (( len B3 ) = ( len B4 ) implies ( len ( B3 + B4 ) ) = ( len B3 ))))
proof
let R7 being non  empty addLoopStr;
let C4 , C5 being (FinSequence of R7);
set D1 = ( len C4 );
reconsider D2 = C4 as (Element of ( ( len C4 ) -tuples_on (the carrier of R7) )) by FINSEQ_2:92;
assume L8: ( len C4 ) = ( len C5 );
reconsider D3 = C5 as (Element of ( D1 -tuples_on (the carrier of R7) )) by L8 , FINSEQ_2:92;
L9: ( dom ( D2 + D3 ) ) = ( Seg D1 ) by FINSEQ_2:124;
thus L10: thesis by L9 , FINSEQ_1:def 3;
end;
theorem
L11: (for R7 being non  empty addLoopStr holds (for B5 , B6 being (FinSequence of R7) holds (( len B5 ) = ( len B6 ) implies ( len ( B5 - B6 ) ) = ( len B5 ))))
proof
let R7 being non  empty addLoopStr;
let C6 , C7 being (FinSequence of R7);
set D4 = ( len C6 );
reconsider D5 = C6 as (Element of ( ( len C6 ) -tuples_on (the carrier of R7) )) by FINSEQ_2:92;
assume L12: ( len C6 ) = ( len C7 );
reconsider D6 = C7 as (Element of ( D4 -tuples_on (the carrier of R7) )) by L12 , FINSEQ_2:92;
L13: ( dom ( D5 - D6 ) ) = ( Seg D4 ) by FINSEQ_2:124;
thus L14: thesis by L13 , FINSEQ_1:def 3;
end;
theorem
L15: (for R8 being non  empty multLoopStr holds (for B7 , B8 being (FinSequence of R8) holds (for R4 being (Element of ( NAT )) holds (R4 in ( dom ( mlt (B7 , B8) ) ) implies (( ( mlt (B7 , B8) ) . R4 ) = ( ( B7 /. R4 ) * ( B8 /. R4 ) ) & ( ( mlt (B7 , B8) ) /. R4 ) = ( ( B7 /. R4 ) * ( B8 /. R4 ) ))))))
proof
let R8 being non  empty multLoopStr;
let C8 , C9 being (FinSequence of R8);
let R4 being (Element of ( NAT ));
assume L16: R4 in ( dom ( mlt (C8 , C9) ) );
L17: ( mlt (C8 , C9) ) = ( (the multF of R8) .: (C8 , C9) ) by FVSUM_1:def 7;
L18: ( rng C9 ) c= (the carrier of R8) by FINSEQ_1:def 4;
L19: (( dom (the multF of R8) ) = [: (the carrier of R8) , (the carrier of R8) :] & ( rng C8 ) c= (the carrier of R8)) by FINSEQ_1:def 4 , FUNCT_2:def 1;
L20: [: ( rng C8 ) , ( rng C9 ) :] c= ( dom (the multF of R8) ) by L19 , L18 , ZFMISC_1:96;
L21: ( dom ( mlt (C8 , C9) ) ) = ( ( dom C8 ) /\ ( dom C9 ) ) by L20 , L17 , FUNCOP_1:69;
L22: R4 in ( dom C9 ) by L21 , L16 , XBOOLE_0:def 4;
L23: ( C9 /. R4 ) = ( C9 . R4 ) by L22 , PARTFUN1:def 6;
L24: R4 in ( dom C8 ) by L16 , L21 , XBOOLE_0:def 4;
L25: ( C8 /. R4 ) = ( C8 . R4 ) by L24 , PARTFUN1:def 6;
thus L26: ( ( mlt (C8 , C9) ) . R4 ) = ( ( C8 /. R4 ) * ( C9 /. R4 ) ) by L25 , L16 , L23 , FVSUM_1:60;
thus L27: thesis by L26 , L16 , PARTFUN1:def 6;
end;
theorem
L28: (for R7 being non  empty addLoopStr holds (for B9 , B10 being (FinSequence of R7) holds (for B11 being Nat holds ((( len B9 ) = ( len B10 ) & 1 <= B11 & B11 <= ( len B9 )) implies (( ( B9 + B10 ) . B11 ) = ( ( B9 /. B11 ) + ( B10 /. B11 ) ) & ( ( B9 - B10 ) . B11 ) = ( ( B9 /. B11 ) - ( B10 /. B11 ) ))))))
proof
let R7 being non  empty addLoopStr;
let C10 , C11 being (FinSequence of R7);
let C12 being Nat;
assume that
L29: ( len C10 ) = ( len C11 )
and
L30: (1 <= C12 & C12 <= ( len C10 ));
reconsider D7 = C10 , D8 = C11 as (Element of ( ( len C10 ) -tuples_on (the carrier of R7) )) by L29 , L3;
L31: (( D7 /. C12 ) = ( D7 . C12 ) & ( D8 /. C12 ) = ( D8 . C12 )) by L29 , L30 , FINSEQ_4:15;
L32: C12 in ( Seg ( len C10 ) ) by L30 , FINSEQ_1:1;
L33: C12 in ( Seg ( len ( C10 + C11 ) ) ) by L32 , L29 , L7;
L34: C12 in ( dom ( C10 + C11 ) ) by L33 , FINSEQ_1:def 3;
thus L35: ( ( C10 + C11 ) . C12 ) = ( ( C10 /. C12 ) + ( C11 /. C12 ) ) by L34 , L31 , FVSUM_1:17;
L36: C12 in ( Seg ( len C10 ) ) by L30 , FINSEQ_1:1;
L37: C12 in ( Seg ( len ( C10 - C11 ) ) ) by L36 , L29 , L11;
L38: C12 in ( dom ( C10 - C11 ) ) by L37 , FINSEQ_1:def 3;
thus L39: thesis by L38 , L31 , FVSUM_1:32;
end;
theorem
L40: (for R6 being Field holds (for B12 being (Element of R6) holds (for B13 being (FinSequence of R6) holds (( - ( B12 * B13 ) ) = ( ( - B12 ) * B13 ) & ( - ( B12 * B13 ) ) = ( B12 * ( - B13 ) )))))
proof
let R6 being Field;
let C13 being (Element of R6);
let C14 being (FinSequence of R6);
set D9 = ( len C14 );
reconsider D10 = C14 as (Element of ( D9 -tuples_on (the carrier of R6) )) by L3;
reconsider D11 = ( C13 * D10 ) as (Element of ( D9 -tuples_on (the carrier of R6) ));
thus L41: ( - ( C13 * C14 ) ) = ( ( - ( 1_ R6 ) ) * D11 ) by FVSUM_1:59
.= ( ( ( - ( 1_ R6 ) ) * C13 ) * D10 ) by FVSUM_1:54
.= ( ( - ( ( 1_ R6 ) * C13 ) ) * D10 ) by VECTSP_1:8
.= ( ( - C13 ) * C14 ) by VECTSP_1:def 8;
thus L42: ( - ( C13 * C14 ) ) = ( ( - ( 1_ R6 ) ) * D11 ) by FVSUM_1:59
.= ( ( ( - ( 1_ R6 ) ) * C13 ) * D10 ) by FVSUM_1:54
.= ( C13 * ( ( - ( 1_ R6 ) ) * D10 ) ) by FVSUM_1:54
.= ( C13 * ( - C14 ) ) by FVSUM_1:59;
end;
theorem
L43: (for R8 being non  empty multLoopStr holds (for B14 , B15 , B16 , B17 being (FinSequence of R8) holds ((( len B14 ) = ( len B15 ) & ( len B16 ) = ( len B17 )) implies ( mlt (( B14 ^ B16 ) , ( B15 ^ B17 )) ) = ( ( mlt (B14 , B15) ) ^ ( mlt (B16 , B17) ) ))))
proof
let R8 being non  empty multLoopStr;
let C15 , C16 , C17 , C18 being (FinSequence of R8);
assume that
L44: ( len C15 ) = ( len C16 )
and
L45: ( len C17 ) = ( len C18 );
L46: ( (the multF of R8) .: (( C15 ^ C17 ) , ( C16 ^ C18 )) ) = ( (the multF of R8) * <: ( C15 ^ C17 ) , ( C16 ^ C18 ) :> ) by FUNCOP_1:def 3;
L47: ( dom ( C15 ^ C17 ) ) = ( Seg ( len ( C15 ^ C17 ) ) ) by FINSEQ_1:def 3;
L48: ( rng ( C16 ^ C18 ) ) c= (the carrier of R8) by FINSEQ_1:def 4;
L49: (( dom (the multF of R8) ) = [: (the carrier of R8) , (the carrier of R8) :] & ( rng ( C15 ^ C17 ) ) c= (the carrier of R8)) by FINSEQ_1:def 4 , FUNCT_2:def 1;
L50: [: ( rng ( C15 ^ C17 ) ) , ( rng ( C16 ^ C18 ) ) :] c= ( dom (the multF of R8) ) by L49 , L48 , ZFMISC_1:96;
L51: ( dom ( (the multF of R8) * <: ( C15 ^ C17 ) , ( C16 ^ C18 ) :> ) ) = ( ( dom ( C15 ^ C17 ) ) /\ ( dom ( C16 ^ C18 ) ) ) by L50 , L46 , FUNCOP_1:69;
L52: ( len ( C16 ^ C18 ) ) = ( ( len C16 ) + ( len C18 ) ) by FINSEQ_1:22;
L53: ( dom ( C16 ^ C18 ) ) = ( Seg ( len ( C16 ^ C18 ) ) ) by FINSEQ_1:def 3;
L54: ( dom ( C15 ^ C17 ) ) = ( dom ( C16 ^ C18 ) ) by L53 , L44 , L45 , L52 , L47 , FINSEQ_1:22;
L55: ( dom ( mlt (( C15 ^ C17 ) , ( C16 ^ C18 )) ) ) = ( dom ( C15 ^ C17 ) ) by L54 , L46 , L51 , FVSUM_1:def 7;
L56: ( (the multF of R8) .: (C17 , C18) ) = ( (the multF of R8) * <: C17 , C18 :> ) by FUNCOP_1:def 3;
L57: ( dom (the multF of R8) ) = [: (the carrier of R8) , (the carrier of R8) :] by FUNCT_2:def 1;
L58: (( rng C17 ) c= (the carrier of R8) & ( rng C18 ) c= (the carrier of R8)) by FINSEQ_1:def 4;
L59: [: ( rng C17 ) , ( rng C18 ) :] c= ( dom (the multF of R8) ) by L58 , L57 , ZFMISC_1:96;
L60: ( dom ( (the multF of R8) * <: C17 , C18 :> ) ) = ( ( dom C17 ) /\ ( dom C18 ) ) by L59 , L56 , FUNCOP_1:69;
L61: ( dom C18 ) = ( Seg ( len C17 ) ) by L45 , FINSEQ_1:def 3
.= ( dom C17 ) by FINSEQ_1:def 3;
L62: ( dom ( mlt (C17 , C18) ) ) = ( dom C17 ) by L61 , L56 , L60 , FVSUM_1:def 7;
L63: ( dom ( mlt (C17 , C18) ) ) = ( Seg ( len C17 ) ) by L62 , FINSEQ_1:def 3;
L64: ( len ( mlt (C17 , C18) ) ) = ( len C17 ) by L63 , FINSEQ_1:def 3;
L65: ( (the multF of R8) .: (C15 , C16) ) = ( (the multF of R8) * <: C15 , C16 :> ) by FUNCOP_1:def 3;
L66: (( rng C15 ) c= (the carrier of R8) & ( rng C16 ) c= (the carrier of R8)) by FINSEQ_1:def 4;
L67: [: ( rng C15 ) , ( rng C16 ) :] c= ( dom (the multF of R8) ) by L66 , L57 , ZFMISC_1:96;
L68: ( dom ( (the multF of R8) * <: C15 , C16 :> ) ) = ( ( dom C15 ) /\ ( dom C16 ) ) by L67 , L65 , FUNCOP_1:69;
L69: ( len ( C15 ^ C17 ) ) = ( ( len C15 ) + ( len C17 ) ) by FINSEQ_1:22;
L70: ( dom C16 ) = ( Seg ( len C15 ) ) by L44 , FINSEQ_1:def 3
.= ( dom C15 ) by FINSEQ_1:def 3;
L71: ( dom ( mlt (C15 , C16) ) ) = ( dom C15 ) by L70 , L65 , L68 , FVSUM_1:def 7;
L72: ( dom ( mlt (C15 , C16) ) ) = ( Seg ( len C15 ) ) by L71 , FINSEQ_1:def 3;
L73: (for B18 being Nat holds ((1 <= B18 & B18 <= ( len ( mlt (( C15 ^ C17 ) , ( C16 ^ C18 )) ) )) implies ( ( mlt (( C15 ^ C17 ) , ( C16 ^ C18 )) ) . B18 ) = ( ( ( mlt (C15 , C16) ) ^ ( mlt (C17 , C18) ) ) . B18 )))
proof
let C19 being Nat;
assume that
L74: 1 <= C19
and
L75: C19 <= ( len ( mlt (( C15 ^ C17 ) , ( C16 ^ C18 )) ) );
L76: C19 in ( Seg ( len ( mlt (( C15 ^ C17 ) , ( C16 ^ C18 )) ) ) ) by L74 , L75 , FINSEQ_1:1;
L77: C19 in ( dom ( mlt (( C15 ^ C17 ) , ( C16 ^ C18 )) ) ) by L76 , FINSEQ_1:def 3;
L78: C19 <= ( len ( C15 ^ C17 ) ) by L77 , L47 , L55 , FINSEQ_1:1;
L79: ( ( C15 ^ C17 ) /. C19 ) = ( ( C15 ^ C17 ) . C19 ) by L78 , L74 , FINSEQ_4:15;
L80: C19 <= ( len ( C16 ^ C18 ) ) by L44 , L45 , L69 , L52 , L47 , L55 , L77 , FINSEQ_1:1;
L81: ( ( C16 ^ C18 ) /. C19 ) = ( ( C16 ^ C18 ) . C19 ) by L80 , L74 , FINSEQ_4:15;
L82: C19 <= ( ( len C15 ) + ( len C17 ) ) by L69 , L47 , L55 , L77 , FINSEQ_1:1;
L83:
now
per cases ;
suppose L84: C19 <= ( len C15 );

L85: C19 in ( Seg ( len C15 ) ) by L84 , L74 , FINSEQ_1:1;
L86: C19 in ( dom C15 ) by L85 , FINSEQ_1:def 3;
L87: C19 in ( dom C16 ) by L44 , L85 , FINSEQ_1:def 3;
L88: ( ( C16 ^ C18 ) . C19 ) = ( C16 . C19 ) by L87 , FINSEQ_1:def 7;
L89: C19 in ( dom ( mlt (C15 , C16) ) ) by L71 , L85 , FINSEQ_1:def 3;
L90: ( ( ( mlt (C15 , C16) ) ^ ( mlt (C17 , C18) ) ) . C19 ) = ( ( mlt (C15 , C16) ) . C19 ) by L89 , FINSEQ_1:def 7
.= ( ( C15 /. C19 ) * ( C16 /. C19 ) ) by L89 , L15;
L91: (( C15 /. C19 ) = ( C15 . C19 ) & ( C16 /. C19 ) = ( C16 . C19 )) by L44 , L74 , L84 , FINSEQ_4:15;
thus L92: ( ( ( C15 ^ C17 ) /. C19 ) * ( ( C16 ^ C18 ) /. C19 ) ) = ( ( ( mlt (C15 , C16) ) ^ ( mlt (C17 , C18) ) ) . C19 ) by L91 , L79 , L81 , L86 , L88 , L90 , FINSEQ_1:def 7;
end;
suppose L93: C19 > ( len C15 );

L94: C19 <= ( len ( C16 ^ C18 ) ) by L44 , L45 , L69 , L52 , L47 , L55 , L77 , FINSEQ_1:1;
L95: ( ( C16 ^ C18 ) /. C19 ) = ( ( C16 ^ C18 ) . C19 ) by L94 , L74 , FINSEQ_4:15;
L96: C19 <= ( len ( C15 ^ C17 ) ) by L47 , L55 , L77 , FINSEQ_1:1;
L97: ( ( C15 ^ C17 ) /. C19 ) = ( ( C15 ^ C17 ) . C19 ) by L96 , L74 , FINSEQ_4:15;
set D12 = ( C19 -' ( len C15 ) );
L98: D12 = ( C19 - ( len C15 ) ) by L93 , XREAL_1:233;
L99: C19 = ( ( len C15 ) + D12 ) by L98;
L100: ( C19 - ( len C15 ) ) <= ( ( ( len C15 ) + ( len C17 ) ) - ( len C15 ) ) by L82 , XREAL_1:13;
L101: D12 <= ( len C17 ) by L100 , L93 , XREAL_1:233;
L102: D12 > ( 0 ) by L93 , L98 , XREAL_1:50;
L103: ( ( 0 ) + 1 ) <= D12 by L102 , NAT_1:13;
L104: D12 in ( Seg ( len C17 ) ) by L103 , L101 , FINSEQ_1:1;
L105: D12 in ( dom ( mlt (C17 , C18) ) ) by L104 , L62 , FINSEQ_1:def 3;
L106: C19 = ( ( len ( mlt (C15 , C16) ) ) + D12 ) by L72 , L99 , FINSEQ_1:def 3;
L107: ( ( ( mlt (C15 , C16) ) ^ ( mlt (C17 , C18) ) ) . C19 ) = ( ( mlt (C17 , C18) ) . D12 ) by L106 , L105 , FINSEQ_1:def 7
.= ( ( C17 /. D12 ) * ( C18 /. D12 ) ) by L105 , L15;
L108: D12 in ( dom C17 ) by L104 , FINSEQ_1:def 3;
L109: ( ( C15 ^ C17 ) . C19 ) = ( C17 . D12 ) by L108 , L99 , FINSEQ_1:def 7;
L110: D12 in ( Seg ( len C17 ) ) by L103 , L101 , FINSEQ_1:1;
L111: D12 in ( dom C18 ) by L110 , L45 , FINSEQ_1:def 3;
L112: (( C17 /. D12 ) = ( C17 . D12 ) & ( C18 /. D12 ) = ( C18 . D12 )) by L45 , L103 , L101 , FINSEQ_4:15;
thus L113: ( ( ( C15 ^ C17 ) /. C19 ) * ( ( C16 ^ C18 ) /. C19 ) ) = ( ( ( mlt (C15 , C16) ) ^ ( mlt (C17 , C18) ) ) . C19 ) by L112 , L44 , L99 , L111 , L109 , L97 , L95 , L107 , FINSEQ_1:def 7;
end;
end;
thus L115: thesis by L83 , L77 , L15;
end;
L116: ( len ( mlt (( C15 ^ C17 ) , ( C16 ^ C18 )) ) ) = ( len ( C15 ^ C17 ) ) by L47 , L55 , FINSEQ_1:def 3
