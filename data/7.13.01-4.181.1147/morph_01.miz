theorem
L9: (for B7 being RealLinearSpace holds (for B8 , B9 being (Subset of B7) holds (B9 = ( {} ) implies (( B8 (+) B9 ) = B9 & ( B9 (+) B8 ) = B9 & ( B8 (-) B9 ) = (the carrier of B7)))))
proof
let C9 being RealLinearSpace;
let C10 , C11 being (Subset of C9);
assume L10: C11 = ( {} );
thus L11: (( C10 (+) C11 ) = C11 & ( C11 (+) C10 ) = C11) by L10 , RUSUB_5:5;
L12:
now
let C12 being set;
assume L13: C12 in (the carrier of C9);
reconsider D1 = C12 as (Element of C9) by L13;
L14: (for B10 being (Element of C9) holds (B10 in C11 implies ( D1 - B10 ) in C10)) by L10;
thus L15: C12 in ( C10 (-) C11 ) by L14;
end;
L16: (the carrier of C9) c= ( C10 (-) C11 ) by L12 , TARSKI:def 3;
thus L17: (the carrier of C9) = ( C10 (-) C11 ) by L16 , XBOOLE_0:def 10;
end;
theorem
L18: (for B11 being RealLinearSpace holds (for B12 , B13 being (Subset of B11) holds ((B12 <> ( {} ) & B13 = ( {} )) implies ( B13 (-) B12 ) = B13)))
proof
let C13 being RealLinearSpace;
let C14 , C15 being (Subset of C13);
assume L19: (C14 <> ( {} ) & C15 = ( {} ));
consider C16 being set such that L20: C16 in C14 by L19 , XBOOLE_0:def 1;
reconsider D2 = C16 as (Element of C13) by L20;
assume L21: ( C15 (-) C14 ) <> C15;
consider C17 being set such that L22: C17 in ( C15 (-) C14 ) by L21 , L19 , XBOOLE_0:def 1;
consider C18 being (Element of C13) such that L23: (C17 = C18 & (for B14 being (Element of C13) holds (B14 in C14 implies ( C18 - B14 ) in C15))) by L22;
thus L24: contradiction by L19 , L20 , L23;
end;
theorem
L25: (for B15 being RealLinearSpace holds (for B16 , B17 being (Subset of B15) holds ((B17 = (the carrier of B15) & B16 <> ( {} )) implies (( B16 (+) B17 ) = B17 & ( B17 (+) B16 ) = B17))))
proof
let C19 being RealLinearSpace;
let C20 , C21 being (Subset of C19);
assume L26: (C21 = (the carrier of C19) & C20 <> ( {} ));
consider C22 being set such that L27: C22 in C20 by L26 , XBOOLE_0:def 1;
reconsider D3 = C22 as (Element of C19) by L27;
L28:
now
let C23 being set;
assume L29: C23 in (the carrier of C19);
reconsider D4 = C23 as (Element of C19) by L29;
L30: ( D3 + ( D4 - D3 ) ) = D4 by RLVECT_4:1;
thus L31: C23 in ( C20 (+) C21 ) by L30 , L26 , L27;
end;
L32: (the carrier of C19) c= ( C20 (+) C21 ) by L28 , TARSKI:def 3;
thus L33: C21 = ( C20 (+) C21 ) by L32 , L26 , XBOOLE_0:def 10;
thus L34: ( C21 (+) C20 ) = ( C20 + C21 )
.= C21 by L32 , L26 , XBOOLE_0:def 10;
end;
theorem
L35: (for B18 being RealLinearSpace holds (for B19 , B20 being (Subset of B18) holds (B20 = (the carrier of B18) implies ( B20 (-) B19 ) = B20)))
proof
let C24 being RealLinearSpace;
let C25 , C26 being (Subset of C24);
assume L36: C26 = (the carrier of C24);
L37:
now
let C27 being set;
assume L38: C27 in (the carrier of C24);
reconsider D5 = C27 as (Element of C24) by L38;
L39: (for B21 being (Element of C24) holds (B21 in C25 implies ( D5 - B21 ) in C26)) by L36;
thus L40: C27 in ( C26 (-) C25 ) by L39;
end;
L41: (the carrier of C24) c= ( C26 (-) C25 ) by L37 , TARSKI:def 3;
thus L42: C26 = ( C26 (-) C25 ) by L41 , XBOOLE_0:def 10 , L36;
end;
theorem
L43: (for R1 being RealLinearSpace holds (for R2 being (binary-image of R1) holds (for R3 being (binary-image of R1) holds ( R2 (+) R3 ) = ( union { ( B22 + R2 ) where B22 is (Element of R1) : B22 in R3 } ))))
proof
let R1 being RealLinearSpace;
let R2 being (binary-image of R1);
let R3 being (binary-image of R1);
L44:
now
let C28 being set;
assume L45: C28 in ( R2 (+) R3 );
consider C29 , C30 being (Element of R1) such that L46: (C28 = ( C29 + C30 ) & C29 in R2 & C30 in R3) by L45;
L47: C28 in ( C30 + R2 ) by L46;
L48: ( C30 + R2 ) in { ( B23 + R2 ) where B23 is (Element of R1) : B23 in R3 } by L46;
thus L49: C28 in ( union { ( B24 + R2 ) where B24 is (Element of R1) : B24 in R3 } ) by L48 , L47 , TARSKI:def 4;
end;
thus L50: ( R2 (+) R3 ) c= ( union { ( B25 + R2 ) where B25 is (Element of R1) : B25 in R3 } ) by L44 , TARSKI:def 3;
L51:
now
let C31 being set;
assume L52: C31 in ( union { ( B26 + R2 ) where B26 is (Element of R1) : B26 in R3 } );
consider C32 being set such that L53: (C31 in C32 & C32 in { ( B27 + R2 ) where B27 is (Element of R1) : B27 in R3 }) by L52 , TARSKI:def 4;
consider C33 being (Element of R1) such that L54: (C32 = ( C33 + R2 ) & C33 in R3) by L53;
consider C34 being (Element of R1) such that L55: (C31 = ( C33 + C34 ) & C34 in R2) by L54 , L53;
thus L56: C31 in ( R2 (+) R3 ) by L54 , L55;
end;
thus L57: thesis by L51 , TARSKI:def 3;
end;
