definition
let C1 being non  empty RLSStruct;
mode binary-image of C1
 is (Subset of C1);
end;
definition
let C2 being RealLinearSpace;
let C3 , C4 being (binary-image of C2);
func C3 (-) C4 -> (binary-image of C2) equals 
{ B1 where B1 is (Element of C2) : (for B2 being (Element of C2) holds (B2 in C4 implies ( B1 - B2 ) in C3)) };
correctness
proof
L2:
now
let C5 being set;
assume L3: C5 in { B3 where B3 is (Element of C2) : (for B4 being (Element of C2) holds (B4 in C4 implies ( B3 - B4 ) in C3)) };
L4: (ex B5 being (Element of C2) st (C5 = B5 & (for B6 being (Element of C2) holds (B6 in C4 implies ( B5 - B6 ) in C3)))) by L3;
thus L5: C5 in (the carrier of C2) by L4;
end;
thus L6: thesis by L2 , TARSKI:def 3;
end;
end;
