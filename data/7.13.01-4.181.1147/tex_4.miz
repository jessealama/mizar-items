theorem
L210: (for R2 being non  empty TopStruct holds (for R3 being (Point of R2) holds { R3 } = ( meet ( MaxADSF R3 ) )))
proof
let R2 being non  empty TopStruct;
let R3 being (Point of R2);
L211: R3 in { R3 } by TARSKI:def 1;
L212:
now
let C73 being set;
assume L213: C73 in ( MaxADSF R3 );
L214: (ex B46 being (Subset of R2) st (B46 = C73 & B46 is  anti-discrete & R3 in B46)) by L213;
thus L215: { R3 } c= C73 by L214 , ZFMISC_1:31;
end;
L216: { R3 } c= ( meet ( MaxADSF R3 ) ) by L212 , SETFAM_1:5;
L217: { R3 } is  anti-discrete by L154;
