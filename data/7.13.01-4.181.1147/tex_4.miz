let R3 being (Point of R2);
L205:
now
let C72 being (Subset of R2);
assume L206: C72 in ( MaxADSF R3 );
L207: (ex B45 being (Subset of R2) st (B45 = C72 & B45 is  anti-discrete & R3 in B45)) by L206;
