theorem
L169: (for R2 being non  empty TopStruct holds (for B37 being (Subset-Family of R2) holds (B37 is  anti-discrete-set-family implies (( meet B37 ) <> ( {} ) implies ( union B37 ) is  anti-discrete))))
proof
let R2 being non  empty TopStruct;
let C62 being (Subset-Family of R2);
assume L170: C62 is  anti-discrete-set-family;
assume L171: ( meet C62 ) <> ( {} );
L172: (for B38 being (Subset of R2) holds (B38 is  open implies (( union C62 ) misses B38 or ( union C62 ) c= B38)))
proof
let C63 being (Subset of R2);
assume L173: C63 is  open;
assume L174: ( union C62 ) meets C63;
consider C64 being set such that L175: C64 in C62 and L176: C64 meets C63 by L174 , ZFMISC_1:80;
reconsider D11 = C64 as (Subset of R2) by L175;
L177: D11 is  anti-discrete by L170 , L175 , L167;
L178: D11 c= C63 by L177 , L173 , L176 , L103;
L179: ( meet C62 ) c= D11 by L175 , SETFAM_1:3;
L180: ( meet C62 ) c= C63 by L179 , L178 , XBOOLE_1:1;
L181: (for B39 being set holds (B39 in C62 implies B39 c= C63))
proof
let C65 being set;
assume L182: C65 in C62;
reconsider D12 = C65 as (Subset of R2) by L182;
L183: ( meet C62 ) c= D12 by L182 , SETFAM_1:3;
L184: ( D12 /\ C63 ) <> ( {} ) by L183 , L171 , L180 , XBOOLE_1:3 , XBOOLE_1:19;
L185: D12 meets C63 by L184 , XBOOLE_0:def 7;
L186: D12 is  anti-discrete by L170 , L182 , L167;
thus L187: thesis by L186 , L173 , L185 , L103;
end;
thus L188: thesis by L181 , ZFMISC_1:76;
end;
thus L189: thesis by L172 , L103;
end;
theorem
L190: (for R2 being non  empty TopStruct holds (for B40 being (Subset-Family of R2) holds (B40 is  anti-discrete-set-family implies ( meet B40 ) is  anti-discrete)))
proof
let R2 being non  empty TopStruct;
let C66 being (Subset-Family of R2);
assume L191: C66 is  anti-discrete-set-family;
thus L192:now
per cases ;
suppose L193: ( meet C66 ) = ( {} );

thus L194: thesis by L193 , L164;
end;
suppose L195: ( meet C66 ) <> ( {} );

L196: ( meet C66 ) c= ( union C66 ) by SETFAM_1:2;
thus L197: thesis by L196 , L191 , L195 , L142 , L169;
end;
end;
end;
