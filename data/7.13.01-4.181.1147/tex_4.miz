theorem
L9: (for R1 being non  empty TopSpace holds (for B1 being (Subset of R1) holds ( Cl B1 ) = ( meet { B2 where B2 is (Subset of R1) : (B2 is  closed & B1 c= B2) } )))
proof
let R1 being non  empty TopSpace;
let C9 being (Subset of R1);
set D1 = { B3 where B3 is (Subset of R1) : (B3 is  closed & C9 c= B3) };
L10: D1 c= ( bool (the carrier of R1) )
proof
let C10 being set;
assume L11: C10 in D1;
L12: (ex B4 being (Subset of R1) st (C10 = B4 & B4 is  closed & C9 c= B4)) by L11;
thus L13: thesis by L12;
end;
L14: ( [#] R1 ) in D1;
reconsider D2 = D1 as non  empty (Subset-Family of R1) by L14 , L10;
L15:
now
let C11 being set;
assume L16: C11 in D2;
L17: (ex B5 being (Subset of R1) st (B5 = C11 & B5 is  closed & C9 c= B5)) by L16;
thus L18: C9 c= C11 by L17;
end;
L19: C9 c= ( meet D2 ) by L15 , SETFAM_1:5;
L20: C9 c= ( Cl C9 ) by PRE_TOPC:18;
L21: ( Cl C9 ) in D2 by L20;
L22: ( meet D2 ) c= ( Cl C9 ) by L21 , SETFAM_1:3;
L23:
now
let C12 being (Subset of R1);
assume L24: C12 in D2;
L25: (ex B6 being (Subset of R1) st (B6 = C12 & B6 is  closed & C9 c= B6)) by L24;
thus L26: C12 is  closed by L25;
end;
L27: D2 is  closed by L23 , TOPS_2:def 2;
L28: ( Cl C9 ) c= ( meet D2 ) by L27 , L19 , TOPS_1:5 , TOPS_2:22;
thus L29: thesis by L28 , L22 , XBOOLE_0:def 10;
end;
theorem
L30: (for R1 being non  empty TopSpace holds (for B7 being (Point of R1) holds ( Cl { B7 } ) = ( meet { B8 where B8 is (Subset of R1) : (B8 is  closed & B7 in B8) } )))
proof
let R1 being non  empty TopSpace;
let C13 being (Point of R1);
set D3 = { B9 where B9 is (Subset of R1) : (B9 is  closed & C13 in B9) };
set D4 = { B10 where B10 is (Subset of R1) : (B10 is  closed & { C13 } c= B10) };
L31:
now
let C14 being set;
assume L32: C14 in D3;
consider C15 being (Subset of R1) such that L33: C15 = C14 and L34: C15 is  closed and L35: C13 in C15 by L32;
L36: { C13 } c= C15 by L35 , ZFMISC_1:31;
thus L37: C14 in D4 by L36 , L33 , L34;
end;
L38: D3 c= D4 by L31 , TARSKI:def 3;
L39:
now
let C16 being set;
assume L40: C16 in D4;
consider C17 being (Subset of R1) such that L41: C17 = C16 and L42: C17 is  closed and L43: { C13 } c= C17 by L40;
L44: C13 in C17 by L43 , ZFMISC_1:31;
thus L45: C16 in D3 by L44 , L41 , L42;
end;
L46: D4 c= D3 by L39 , TARSKI:def 3;
L47: ( Cl { C13 } ) = ( meet D4 ) by L9;
thus L48: thesis by L47 , L38 , L46 , XBOOLE_0:def 10;
end;
registration
let C18 being non  empty TopSpace;
let C19 being non  proper (Subset of C18);
cluster ( Int C19 ) -> non  proper;
coherence
proof
L49: C19 = ( [#] C18 ) by SUBSET_1:def 6;
thus L50: thesis by L49 , TOPS_1:15;
end;
end;
registration
let C20 being non  empty TopSpace;
let C21 being  proper (Subset of C20);
cluster ( Int C21 ) ->  proper;
coherence
proof
L52:
now
assume L53: ( Int C21 ) is non  proper;
L54: ( Int C21 ) = ( [#] C20 ) by L53 , SUBSET_1:def 6;
L55: ( Int C21 ) c= C21 by TOPS_1:16;
thus L56: contradiction by L55 , L54 , XBOOLE_0:def 10;
end;
thus L57: thesis by L52;
end;
end;
registration
let C22 being non  empty TopSpace;
let C23 being  empty (Subset of C22);
cluster ( Int C23 ) ->  empty;
