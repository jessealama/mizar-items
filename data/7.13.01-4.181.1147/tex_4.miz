end;
theorem
L60: (for R1 being non  empty TopSpace holds (for B11 being (Subset of R1) holds ( Int B11 ) = ( union { B12 where B12 is (Subset of R1) : (B12 is  open & B12 c= B11) } )))
proof
let R1 being non  empty TopSpace;
let C24 being (Subset of R1);
set D5 = { B13 where B13 is (Subset of R1) : (B13 is  open & B13 c= C24) };
L61: D5 c= ( bool (the carrier of R1) )
proof
let C25 being set;
assume L62: C25 in D5;
L63: (ex B14 being (Subset of R1) st (C25 = B14 & B14 is  open & B14 c= C24)) by L62;
thus L64: thesis by L63;
end;
L65: ( {} ) c= C24 by XBOOLE_1:2;
L66: ( {} R1 ) in D5 by L65;
reconsider D6 = D5 as non  empty (Subset-Family of R1) by L66 , L61;
L67:
now
let C26 being set;
assume L68: C26 in D6;
L69: (ex B15 being (Subset of R1) st (B15 = C26 & B15 is  open & B15 c= C24)) by L68;
thus L70: C26 c= C24 by L69;
end;
L71: ( union D6 ) c= C24 by L67 , ZFMISC_1:76;
L72: ( Int C24 ) c= C24 by TOPS_1:16;
L73: ( Int C24 ) in D6 by L72;
L74: ( Int C24 ) c= ( union D6 ) by L73 , ZFMISC_1:74;
L75:
now
let C27 being (Subset of R1);
assume L76: C27 in D6;
L77: (ex B16 being (Subset of R1) st (B16 = C27 & B16 is  open & B16 c= C24)) by L76;
thus L78: C27 is  open by L77;
end;
L79: D6 is  open by L75 , TOPS_2:def 1;
L80: ( union D6 ) c= ( Int C24 ) by L79 , L71 , TOPS_1:24 , TOPS_2:19;
thus L81: thesis by L80 , L74 , XBOOLE_0:def 10;
end;
begin
definition
let C28 being TopStruct;
let C29 being (Subset of C28);
attr C29 is  anti-discrete
means
:L82: (for B17 being (Point of C28) holds (for B18 being (Subset of C28) holds ((B18 is  open & B17 in B18) implies (B17 in C29 implies C29 c= B18))));
end;
definition
let C30 being non  empty TopStruct;
let C31 being (Subset of C30);
redefine attr C31 is  anti-discrete
means
:L84: (for B19 being (Point of C30) holds (for B20 being (Subset of C30) holds ((B20 is  closed & B19 in B20) implies (B19 in C31 implies C31 c= B20))));
compatibility
proof
thus L85:now
assume L86: C31 is  anti-discrete;
let C32 being (Point of C30);
let C33 being (Subset of C30);
set D7 = ( ( [#] C30 ) \ C33 );
L87: ( C31 \ C33 ) c= D7 by XBOOLE_1:33;
assume L88: C33 is  closed;
L89: D7 is  open by L88 , PRE_TOPC:def 3;
assume L90: C32 in C33;
assume L91: C32 in C31;
assume L92: (not C31 c= C33);
L93: ( C31 \ C33 ) <> ( {} ) by L92 , XBOOLE_1:37;
consider C34 being set such that L94: C34 in ( C31 \ C33 ) by L93 , XBOOLE_0:def 1;
L95: C34 in C31 by L94 , XBOOLE_0:def 5;
L96: C31 c= D7 by L95 , L86 , L94 , L87 , L89 , L82;
L97: C31 misses ( D7 ` ) by L96 , SUBSET_1:24;
L98: ( D7 ` ) = ( ( C33 ` ) ` )
.= C33;
L99: ( C31 /\ C33 ) is non  empty by L90 , L91 , XBOOLE_0:def 4;
thus L100: contradiction by L99 , L98 , L97 , XBOOLE_0:def 7;
end;
thus L101:now
assume L102: (for B21 being (Point of C30) holds (for B22 being (Subset of C30) holds ((B22 is  closed & B21 in B22) implies (B21 in C31 implies C31 c= B22))));
L103:
now
let C35 being (Point of C30);
let C36 being (Subset of C30);
set D8 = ( ( [#] C30 ) \ C36 );
L104: ( C31 \ C36 ) c= D8 by XBOOLE_1:33;
L105: C36 = ( ( [#] C30 ) \ D8 ) by PRE_TOPC:3;
assume L106: C36 is  open;
L107: D8 is  closed by L106 , L105 , PRE_TOPC:def 3;
assume L108: C35 in C36;
assume L109: C35 in C31;
assume L110: (not C31 c= C36);
L111: ( C31 \ C36 ) <> ( {} ) by L110 , XBOOLE_1:37;
consider C37 being set such that L112: C37 in ( C31 \ C36 ) by L111 , XBOOLE_0:def 1;
L113: D8 = ( C36 ` );
L114: C37 in C31 by L112 , XBOOLE_0:def 5;
L115: C31 c= D8 by L114 , L102 , L112 , L104 , L107;
L116: C31 misses ( D8 ` ) by L115 , SUBSET_1:24;
thus L117: contradiction by L116 , L108 , L109 , L113 , XBOOLE_0:3;
end;
thus L118: C31 is  anti-discrete by L103 , L82;
end;
end;
end;
definition
let C38 being TopStruct;
let C39 being (Subset of C38);
redefine attr C39 is  anti-discrete
means
:L103: (for B23 being (Subset of C38) holds (B23 is  open implies (C39 misses B23 or C39 c= B23)));
compatibility
proof
thus L104:now
assume L105: C39 is  anti-discrete;
let C40 being (Subset of C38);
assume L106: C40 is  open;
assume L107: C39 meets C40;
consider C41 being set such that L108: C41 in ( C39 /\ C40 ) by L107 , XBOOLE_0:4;
reconsider D9 = C41 as (Point of C38) by L108;
L109: D9 in C40 by L108 , XBOOLE_0:def 4;
L110: D9 in C39 by L108 , XBOOLE_0:def 4;
thus L111: C39 c= C40 by L110 , L105 , L106 , L109 , L82;
end;
assume L112: (for B24 being (Subset of C38) holds (B24 is  open implies (C39 misses B24 or C39 c= B24)));
L113:
now
let C42 being (Point of C38);
let C43 being (Subset of C38);
assume L114: C43 is  open;
assume L115: C42 in C43;
assume L116: C42 in C39;
L117: C39 meets C43 by L116 , L115 , XBOOLE_0:3;
thus L118: C39 c= C43 by L117 , L112 , L114;
end;
thus L119: thesis by L113 , L82;
end;
end;
definition
let C44 being TopStruct;
let C45 being (Subset of C44);
redefine attr C45 is  anti-discrete
means
:L121: (for B25 being (Subset of C44) holds (B25 is  closed implies (C45 misses B25 or C45 c= B25)));
compatibility
proof
thus L122:now
assume L123: C45 is  anti-discrete;
let C46 being (Subset of C44);
assume L124: C46 is  closed;
L125: ( ( [#] C44 ) \ C46 ) is  open by L124 , PRE_TOPC:def 3;
L126: (C45 misses ( C46 ` ) or C45 c= ( C46 ` )) by L125 , L123 , L103;
assume L127: C45 meets C46;
L128: C45 c= ( ( C46 ` ) ` ) by L127 , L126 , SUBSET_1:23;
thus L129: C45 c= C46 by L128;
end;
thus L130:now
assume L131: (for B26 being (Subset of C44) holds (B26 is  closed implies (C45 misses B26 or C45 c= B26)));
L132:
now
let C47 being (Subset of C44);
L133: C47 = ( ( [#] C44 ) \ ( ( [#] C44 ) \ C47 ) ) by PRE_TOPC:3;
assume L134: C47 is  open;
L135: ( ( [#] C44 ) \ C47 ) is  closed by L134 , L133 , PRE_TOPC:def 3;
L136: (C45 misses ( C47 ` ) or C45 c= ( C47 ` )) by L135 , L131;
assume L137: C45 meets C47;
L138: C45 c= ( ( C47 ` ) ` ) by L137 , L136 , SUBSET_1:23;
thus L139: C45 c= C47 by L138;
end;
thus L140: C45 is  anti-discrete by L132 , L103;
end;
end;
end;
theorem
L132: (for B27 , B28 being TopStruct holds (for B29 being (Subset of B27) holds (for B30 being (Subset of B28) holds ((( the TopStruct of B27 ) = ( the TopStruct of B28 ) & B29 = B30) implies (B29 is  anti-discrete implies B30 is  anti-discrete)))))
proof
let C48 , C49 being TopStruct;
let C50 being (Subset of C48);
let C51 being (Subset of C49);
assume L133: ( the TopStruct of C48 ) = ( the TopStruct of C49 );
assume L134: C50 = C51;
assume L135: C50 is  anti-discrete;
L136:
now
let C52 being (Subset of C49);
reconsider D10 = C52 as (Subset of C48) by L133;
assume L137: C52 is  open;
L138: D10 in (the topology of C48) by L137 , L133 , PRE_TOPC:def 2;
L139: D10 is  open by L138 , PRE_TOPC:def 2;
thus L140: (C51 misses C52 or C51 c= C52) by L139 , L134 , L135 , L103;
end;
thus L141: thesis by L136 , L103;
end;
theorem
L142: (for R2 being non  empty TopStruct holds (for B31 , B32 being (Subset of R2) holds (B32 c= B31 implies (B31 is  anti-discrete implies B32 is  anti-discrete))))
proof
let R2 being non  empty TopStruct;
let C53 , C54 being (Subset of R2);
assume L143: C54 c= C53;
assume L144: C53 is  anti-discrete;
L145:
now
let C55 being (Subset of R2);
assume L146: C55 is  open;
assume L147: C54 meets C55;
L148: ( C54 /\ C55 ) <> ( {} ) by L147 , XBOOLE_0:def 7;
L149: ( C53 /\ C55 ) <> ( {} ) by L148 , L143 , XBOOLE_1:3 , XBOOLE_1:26;
L150: C53 meets C55 by L149 , XBOOLE_0:def 7;
L151: C53 c= C55 by L150 , L144 , L146 , L103;
thus L152: C54 c= C55 by L151 , L143 , XBOOLE_1:1;
end;
thus L153: thesis by L145 , L103;
end;
theorem
L154: (for R2 being non  empty TopStruct holds (for B33 being (Point of R2) holds { B33 } is  anti-discrete))
proof
let R2 being non  empty TopStruct;
let C56 being (Point of R2);
L155:
now
let C57 being (Subset of R2);
assume that
L156: C57 is  open;
assume L157: { C56 } meets C57;
consider C58 being set such that L158: C58 in ( { C56 } /\ C57 ) by L157 , XBOOLE_0:4;
L159: C58 in { C56 } by L158 , XBOOLE_0:def 4;
L160: C58 = C56 by L159 , TARSKI:def 1;
L161: C58 in C57 by L158 , XBOOLE_0:def 4;
thus L162: { C56 } c= C57 by L161 , L160 , ZFMISC_1:31;
