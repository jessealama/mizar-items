theorem
L48: (for B18 , B19 , B20 being non  empty RelStr holds (for B21 being (Function of B19 , B20) holds (for B22 , B23 being (Function of B18 , B19) holds ((B22 <= B23 & B21 is  monotone) implies ( B21 * B22 ) <= ( B21 * B23 )))))
proof
let C28 , C29 , C30 being non  empty RelStr;
let C31 being (Function of C29 , C30);
let C32 , C33 being (Function of C28 , C29);
assume that
L49: C32 <= C33
and
L50: (for B24 , B25 being (Element of C29) holds (B24 <= B25 implies ( C31 . B24 ) <= ( C31 . B25 )));
L51: (for B26 being (Element of C28) holds ( ( C31 * C32 ) . B26 ) <= ( ( C31 * C33 ) . B26 ))
proof
let C34 being (Element of C28);
L52: (( ( C31 * C32 ) . C34 ) = ( C31 . ( C32 . C34 ) ) & ( ( C31 * C33 ) . C34 ) = ( C31 . ( C33 . C34 ) )) by FUNCT_2:15;
L53: ( C32 . C34 ) <= ( C33 . C34 ) by L49 , YELLOW_2:9;
thus L54: thesis by L53 , L50 , L52;
end;
thus L55: thesis by L51 , YELLOW_2:9;
end;
theorem
L56: (for B27 being non  empty TopSpace holds (for B28 being non  empty  TopSpace-like TopRelStr holds (for B29 , B30 being (Function of B27 , B28) holds (for B31 , B32 being (Element of ( ContMaps (B27 , B28) )) holds ((B31 = B29 & B32 = B30) implies (B31 <= B32 iff B29 <= B30))))))
proof
