theorem
L8: (for B3 being  with_units non  empty AltCatStr holds (for B4 being (object of B3) holds (( idm B4 ) is  retraction & ( idm B4 ) is  coretraction)))
proof
let C19 being  with_units non  empty AltCatStr;
let C20 being (object of C19);
L9: <^ C20 , C20 ^> <> ( {} ) by ALTCAT_1:19;
L10: ( ( idm C20 ) * ( idm C20 ) ) = ( idm C20 ) by L9 , ALTCAT_1:def 17;
L11: ( idm C20 ) is_left_inverse_of ( idm C20 ) by L10 , L1;
thus L12: thesis by L11 , L4 , L6;
end;
definition
let C21 being category;
let C22 , C23 being (object of C21);
assume that
L13: <^ C22 , C23 ^> <> ( {} )
and
L14: <^ C23 , C22 ^> <> ( {} );
let C24 being (Morphism of C22 , C23);
assume that
L15: C24 is  retraction  coretraction;
func C24 " -> (Morphism of C23 , C22) means 
:L16: (it is_left_inverse_of C24 & it is_right_inverse_of C24);
existence
proof
consider C25 being (Morphism of C23 , C22) such that L17: C25 is_right_inverse_of C24 by L15 , L4;
take C25;
consider C26 being (Morphism of C23 , C22) such that L18: C26 is_left_inverse_of C24 by L15 , L6;
L19: C25 = ( ( idm C22 ) * C25 ) by L14 , ALTCAT_1:20
.= ( ( C26 * C24 ) * C25 ) by L18 , L1
.= ( C26 * ( C24 * C25 ) ) by L13 , L14 , ALTCAT_1:21
.= ( C26 * ( idm C23 ) ) by L17 , L1
.= C26 by L14 , ALTCAT_1:def 17;
thus L20: thesis by L19 , L17 , L18;
end;
uniqueness
proof
let C27 , C28 being (Morphism of C23 , C22);
assume that
L21: C27 is_left_inverse_of C24
and
L22: C27 is_right_inverse_of C24
and
L23: C28 is_left_inverse_of C24
and
L24: C28 is_right_inverse_of C24;
thus L25: C27 = ( C27 * ( idm C23 ) ) by L14 , ALTCAT_1:def 17
.= ( C27 * ( C24 * C28 ) ) by L24 , L1
.= ( ( C27 * C24 ) * C28 ) by L13 , L14 , ALTCAT_1:21
.= ( ( idm C22 ) * C28 ) by L21 , L1
.= C28 by L14 , ALTCAT_1:20;
end;
end;
theorem
L27: (for B5 being category holds (for B6 , B7 being (object of B5) holds ((<^ B6 , B7 ^> <> ( {} ) & <^ B7 , B6 ^> <> ( {} )) implies (for B8 being (Morphism of B6 , B7) holds ((B8 is  retraction & B8 is  coretraction) implies (( ( B8 " ) * B8 ) = ( idm B6 ) & ( B8 * ( B8 " ) ) = ( idm B7 )))))))
