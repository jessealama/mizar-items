L17: (R7 in R4 iff (for R2 being set holds (R2 in R1 implies R7 in R2))) by L14;
thus L18: (R7 in R4 iff R7 in R5) by L17 , L15;
end;
thus L19: R4 = R5 by L16 , TARSKI:1;
end;
