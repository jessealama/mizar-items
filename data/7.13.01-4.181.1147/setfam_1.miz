definition
let R1 being set;
func meet R1 means 
:L1: (for R7 being set holds (R7 in it iff (for R2 being set holds (R2 in R1 implies R7 in R2)))) if R1 <> ( {} ) otherwise it = ( {} );
existence
proof
thus L2: (R1 <> ( {} ) implies (ex R4 being set st (for R7 being set holds (R7 in R4 iff (for R3 being set holds (R3 in R1 implies R7 in R3))))))
proof
defpred S1[ set ] means (for R3 being set holds (R3 in R1 implies $1 in R3));
assume L3: R1 <> ( {} );
consider R2 being set such that L4: (for R7 being set holds (R7 in R2 iff (R7 in ( union R1 ) & S1[ R7 ]))) from XBOOLE_0:sch 1;
take R2;
L5: (for R7 being set holds ((for R3 being set holds (R3 in R1 implies R7 in R3)) implies R7 in R2))
proof
set D1 = the (Element of R1);
let R7 being set;
assume that
L6: (for R3 being set holds (R3 in R1 implies R7 in R3));
L7: R7 in D1 by L3 , L6;
L8: R7 in ( union R1 ) by L7 , L3 , TARSKI:def 4;
thus L9: thesis by L8 , L4 , L6;
end;
thus L10: thesis by L5 , L4;
end;

thus L11: thesis;
end;
