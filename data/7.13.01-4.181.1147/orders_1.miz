assume that
L201: R17 is  reflexive
and
L202: R17 is  transitive
and
L203: R17 is  antisymmetric;
thus L204: (( R17 ~ ) is  reflexive & ( R17 ~ ) is  transitive & ( R17 ~ ) is  antisymmetric) by L201 , L202 , L203;
end;
L205: (for R17 being Relation holds (R17 is  connected implies ( R17 ~ ) is  connected))
proof
let R17 being Relation;
assume L206: (for R24 being set holds (for R25 being set holds ((R24 in ( field R17 ) & R25 in ( field R17 ) & R24 <> R25) implies ([ R24 , R25 ] in R17 or [ R25 , R24 ] in R17))));
let R24 being set;
let R25 being set;
assume that
L207: R24 in ( field ( R17 ~ ) )
and
L208: R25 in ( field ( R17 ~ ) )
and
L209: R24 <> R25;
L210: ( field R17 ) = ( field ( R17 ~ ) ) by RELAT_1:21;
L211: ([ R24 , R25 ] in R17 or [ R25 , R24 ] in R17) by L210 , L206 , L207 , L208 , L209;
thus L212: thesis by L211 , RELAT_1:def 7;
end;
theorem
L213: (for R17 being Relation holds (R17 is  being_linear-order implies ( R17 ~ ) is  being_linear-order))
