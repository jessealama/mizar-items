begin
L1: (for R2 being set holds ((ex R1 being set st (R1 <> ( {} ) & R1 in R2)) iff ( union R2 ) <> ( {} )))
proof
let R2 being set;
thus L2: ((ex R1 being set st (R1 <> ( {} ) & R1 in R2)) implies ( union R2 ) <> ( {} ))
proof
given R1 being set such that
L3: R1 <> ( {} )
and
L4: R1 in R2;

set D1 = the (Element of R1);
L5: D1 in R1 by L3;
thus L6: thesis by L5 , L4 , TARSKI:def 4;
end;

set D2 = the (Element of ( union R2 ));
assume L7: ( union R2 ) <> ( {} );
consider R1 being set such that L8: D2 in R1 and L9: R1 in R2 by L7 , TARSKI:def 4;
take R1;
thus L10: thesis by L8 , L9;
end;
definition
let R13 being non  empty set;
assume L11: (not ( {} ) in R13);
mode Choice_Function of R13
 -> (Function of R13 , ( union R13 ))
means :L12: (for R1 being set holds (R1 in R13 implies ( it . R1 ) in R1));
existence
proof
deffunc H1(set) = ( $1 `2 );
set D3 = the (Element of R13);
deffunc H2(set) = [: { $1 } , $1 :];
consider R10 being Function such that L13: ( dom R10 ) = R13 and L14: (for R1 being set holds (R1 in R13 implies ( R10 . R1 ) = H2(R1))) from FUNCT_1:sch 3;
L15: D3 in R13;
reconsider D4 = ( rng R10 ) as non  empty set by L15 , L13 , FUNCT_1:3;
L16:
now
let R1 being set;
let R2 being set;
assume that
L17: R1 in D4
and
L18: R2 in D4
and
L19: R1 <> R2;
consider R6 being set such that L20: R6 in ( dom R10 ) and L21: ( R10 . R6 ) = R2 by L18 , FUNCT_1:def 3;
L22: R2 = [: { R6 } , R6 :] by L13 , L14 , L20 , L21;
set D5 = the (Element of ( R1 /\ R2 ));
assume L23: R1 meets R2;
L24: ( R1 /\ R2 ) <> ( {} ) by L23 , XBOOLE_0:def 7;
consider R3 being set such that L25: R3 in ( dom R10 ) and L26: ( R10 . R3 ) = R1 by L17 , FUNCT_1:def 3;
L27: R1 = [: { R3 } , R3 :] by L13 , L14 , L25 , L26;
consider R4 being set, R5 being set such that L28: D5 = [ R4 , R5 ] and L29: R4 in ( { R3 } /\ { R6 } ) and L30: R5 in ( R3 /\ R6 ) by L27 , L22 , L24 , ZFMISC_1:85;
L31: R4 in { R3 } by L29 , XBOOLE_0:def 4;
L32: R4 = R3 by L31 , TARSKI:def 1;
L33: R4 in { R6 } by L29 , XBOOLE_0:def 4;
thus L34: contradiction by L33 , L19 , L26 , L21 , L32 , TARSKI:def 1;
end;
L35:
now
let R1 being set;
assume L36: R1 in D4;
consider R6 being set such that L37: R6 in ( dom R10 ) and L38: ( R10 . R6 ) = R1 by L36 , FUNCT_1:def 3;
L39: R6 <> ( {} ) by L11 , L13 , L37;
L40: R1 = [: { R6 } , R6 :] by L13 , L14 , L37 , L38;
thus L41: R1 <> ( {} ) by L40 , L39;
end;
consider C1 being set such that L42: (for R1 being set holds (R1 in D4 implies (ex R3 being set st ( C1 /\ R1 ) = { R3 }))) by L35 , L16 , WELLORD2:18;
defpred S1[ set , set ] means $2 in ( $1 /\ C1 );
L43: (for R1 being set holds (R1 in D4 implies (ex R6 being set st S1[ R1 , R6 ])))
proof
let R1 being set;
assume L44: R1 in D4;
consider R3 being set such that L45: ( C1 /\ R1 ) = { R3 } by L44 , L42;
take R3;
thus L46: thesis by L45 , TARSKI:def 1;
end;
L47: (for R1 being set holds (for R7 being set holds (for R8 being set holds ((R1 in D4 & S1[ R1 , R7 ] & S1[ R1 , R8 ]) implies R7 = R8))))
proof
let R1 being set;
let R7 being set;
let R8 being set;
assume that
L48: R1 in D4
and
L49: R7 in ( R1 /\ C1 )
and
L50: R8 in ( R1 /\ C1 );
consider R3 being set such that L51: { R3 } = ( C1 /\ R1 ) by L42 , L48;
L52: R3 = R7 by L49 , L51 , TARSKI:def 1;
thus L53: thesis by L52 , L50 , L51 , TARSKI:def 1;
end;
consider R11 being Function such that L54: ( dom R11 ) = D4 and L55: (for R1 being set holds (R1 in D4 implies S1[ R1 , ( R11 . R1 ) ])) from FUNCT_1:sch 2(L47 , L43);
L56: ( rng R11 ) c= ( union D4 )
proof
let R3 being set;
assume L57: R3 in ( rng R11 );
consider R6 being set such that L58: R6 in ( dom R11 ) and L59: ( R11 . R6 ) = R3 by L57 , FUNCT_1:def 3;
L60: R3 in ( R6 /\ C1 ) by L54 , L55 , L58 , L59;
L61: R3 in R6 by L60 , XBOOLE_0:def 4;
thus L62: thesis by L61 , L54 , L58 , TARSKI:def 4;
end;
L63: ( R10 . D3 ) in ( rng R10 ) by L13 , FUNCT_1:def 3;
consider R4 being set such that L64: R4 in D4 by L63;
consider R5 being set such that L65: R5 in ( dom R10 ) and L66: ( R10 . R5 ) = R4 by L64 , FUNCT_1:def 3;
L67: R5 <> ( {} ) by L11 , L13 , L65;
reconsider D6 = R10 as (Function of R13 , D4) by L13 , FUNCT_2:def 1 , RELSET_1:4;
consider R12 being Function such that L68: ( dom R12 ) = ( union D4 ) and L69: (for R3 being set holds (R3 in ( union D4 ) implies ( R12 . R3 ) = H1(R3))) from FUNCT_1:sch 3;
L70: ( rng R12 ) c= ( union R13 )
proof
let R3 being set;
assume L71: R3 in ( rng R12 );
consider R6 being set such that L72: R6 in ( dom R12 ) and L73: ( R12 . R6 ) = R3 by L71 , FUNCT_1:def 3;
consider R2 being set such that L74: R6 in R2 and L75: R2 in D4 by L68 , L72 , TARSKI:def 4;
consider R9 being set such that L76: R9 in ( dom D6 ) and L77: ( D6 . R9 ) = R2 by L75 , FUNCT_1:def 3;
L78: R2 = [: { R9 } , R9 :] by L14 , L76 , L77;
consider R4 being set, R5 being set such that L79: R6 = [ R4 , R5 ] by L78 , L74 , RELAT_1:def 1;
L80: R5 in R9 by L74 , L78 , L79 , ZFMISC_1:87;
L81: R3 = ( [ R4 , R5 ] `2 ) by L68 , L69 , L72 , L73 , L79;
L82: R3 in R9 by L81 , L80;
thus L83: thesis by L82 , L76 , TARSKI:def 4;
end;
L84: R4 = [: { R5 } , R5 :] by L13 , L14 , L65 , L66;
L85: ( union D4 ) <> ( {} ) by L84 , L64 , L67 , L1;
reconsider D7 = R11 as (Function of D4 , ( union D4 )) by L85 , L54 , L56 , FUNCT_2:def 1 , RELSET_1:4;
L86: ( union R13 ) <> ( {} ) by L11 , L15 , L1;
reconsider D8 = R12 as (Function of ( union D4 ) , ( union R13 )) by L86 , L68 , L70 , FUNCT_2:def 1 , RELSET_1:4;
L87: ( dom ( D7 * D6 ) ) = R13 by L85 , FUNCT_2:def 1;
reconsider D9 = ( D8 * ( D7 * D6 ) ) as (Function of R13 , ( union R13 )) by L85;
take D9;
let R1 being set;
assume L88: R1 in R13;
L89: ( D6 . R1 ) = [: { R1 } , R1 :] by L88 , L14;
set D10 = ( D6 . R1 );
L90: ( D6 . R1 ) in D4 by L13 , L88 , FUNCT_1:def 3;
L91: ( D7 . D10 ) in ( rng D7 ) by L90 , L54 , FUNCT_1:def 3;
L92: ( D8 . ( D7 . D10 ) ) = ( ( D7 . D10 ) `2 ) by L91 , L69;
L93: ( D7 . D10 ) in ( D10 /\ C1 ) by L55 , L90;
consider R4 being set, R5 being set such that L94: [ R4 , R5 ] = ( D7 . D10 ) by L93 , L89 , RELAT_1:def 1;
L95: ( D7 . D10 ) in D10 by L93 , XBOOLE_0:def 4;
L96: R5 in R1 by L95 , L89 , L94 , ZFMISC_1:87;
L97: ( D8 . ( D7 . D10 ) ) in R1 by L96 , L94 , L92 , MCART_1:7;
L98: ( ( D7 * D6 ) . R1 ) = ( D7 . D10 ) by L13 , L88 , FUNCT_1:13;
thus L99: thesis by L98 , L87 , L88 , L97 , FUNCT_1:13;
end;
end;
definition
let C2 being set;
func BOOL C2 -> set equals 
( ( bool C2 ) \ { ( {} ) } );
coherence;
end;
