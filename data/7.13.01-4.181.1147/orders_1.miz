theorem
L196: (for R17 being Relation holds (R17 is  being_quasi-order implies ( R17 ~ ) is  being_quasi-order))
proof
let R17 being Relation;
assume that
L197: R17 is  reflexive
and
L198: R17 is  transitive;
thus L199: (( R17 ~ ) is  reflexive & ( R17 ~ ) is  transitive) by L197 , L198;
