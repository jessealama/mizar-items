definition
let C1 being Function;
attr C1 is  uncurrying
means
:L1: ((for B1 being set holds (B1 in ( dom C1 ) implies B1 is  Function-yielding  Function-yielding  Function-yielding  Function-yielding Function)) & (for B2 being Function holds (B2 in ( dom C1 ) implies ( C1 . B2 ) = ( uncurry B2 ))));
attr C1 is  currying
means
:L2: ((for B3 being set holds (B3 in ( dom C1 ) implies (B3 is Function & ( proj1 B3 ) is Relation))) & (for B4 being Function holds (B4 in ( dom C1 ) implies ( C1 . B4 ) = ( curry B4 ))));
attr C1 is  commuting
means
:L3: ((for B5 being set holds (B5 in ( dom C1 ) implies B5 is  Function-yielding  Function-yielding  Function-yielding  Function-yielding Function)) & (for B6 being Function holds (B6 in ( dom C1 ) implies ( C1 . B6 ) = ( commute B6 ))));
end;
registration
cluster  empty ->  uncurrying  currying  commuting for Function;
coherence
proof
let C2 being Function;
assume L5: C2 is  empty;
thus L6: (for B7 being set holds (B7 in ( dom C2 ) implies B7 is  Function-yielding  Function-yielding  Function-yielding  Function-yielding Function)) by L5;
thus L7: (for B8 being Function holds (B8 in ( dom C2 ) implies ( C2 . B8 ) = ( uncurry B8 ))) by L5;
thus L8: (for B9 being set holds (B9 in ( dom C2 ) implies (B9 is Function & ( proj1 B9 ) is Relation))) by L5;
thus L9: (for B10 being Function holds (B10 in ( dom C2 ) implies ( C2 . B10 ) = ( curry B10 ))) by L5;
thus L10: (for B11 being set holds (B11 in ( dom C2 ) implies B11 is  Function-yielding  Function-yielding  Function-yielding  Function-yielding Function)) by L5;
thus L11: thesis by L5;
end;
end;
