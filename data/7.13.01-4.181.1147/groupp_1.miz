theorem
L1: (for R5 being Nat holds (for R6 being  prime Nat holds ((for B1 being Nat holds R5 <> ( R6 |^ B1 )) implies (ex B2 being (Element of ( NAT )) st (B2 is  prime & B2 divides R5 & B2 <> R6)))))
proof
let R5 being Nat;
let R6 being  prime Nat;
assume L2: (for B3 being Nat holds R5 <> ( R6 |^ B3 ));
per cases ;
suppose L3: R5 = ( 0 );

per cases ;
suppose L4: R6 = 2;

take D1 = 3;
thus L5: D1 is  prime by PEPIN:41;
thus L6: D1 divides R5 by L3 , INT_2:12;
thus L7: D1 <> R6 by L4;
end;
suppose L8: R6 <> 2;

take D2 = 2;
thus L9: D2 is  prime by INT_2:28;
thus L10: D2 divides R5 by L3 , INT_2:12;
thus L11: D2 <> R6 by L8;
end;
end;
suppose L13: R5 <> ( 0 );

L14: R6 > 1 by INT_2:def 4;
reconsider D3 = ( R6 |-count R5 ) as (Element of ( NAT ));
L15: ( R6 |^ D3 ) divides R5 by L13 , L14 , NAT_3:def 7;
L16: (not ( R6 |^ ( D3 + 1 ) ) divides R5) by L13 , L14 , NAT_3:def 7;
consider C1 being Nat such that L17: R5 = ( ( R6 |^ D3 ) * C1 ) by L15 , NAT_D:def 3;
L18: C1 >= 2
proof
assume L19: (not C1 >= 2);
L20: C1 < ( 1 + 1 ) by L19;
L21: C1 <= 1 by L20 , NAT_1:13;
L22: (C1 = ( 0 ) or C1 = 1) by L21 , NAT_1:25;
thus L23: contradiction by L22 , L13 , L2 , L17;
end;
consider C2 being (Element of ( NAT )) such that L24: C2 is  prime and L25: C2 divides C1 by L18 , INT_2:31;
L26: C1 divides R5 by L17 , NAT_D:def 3;
take C2;
L27: C2 <> R6
proof
assume L28: C2 = R6;
consider C3 being Nat such that L29: C1 = ( R6 * C3 ) by L28 , L25 , NAT_D:def 3;
L30: R5 = ( ( ( R6 |^ D3 ) * R6 ) * C3 ) by L17 , L29
.= ( ( R6 |^ ( D3 + 1 ) ) * C3 ) by NEWTON:6;
thus L31: contradiction by L30 , L16 , NAT_D:def 3;
end;
thus L32: thesis by L27 , L24 , L25 , L26 , NAT_D:4;
end;
end;
