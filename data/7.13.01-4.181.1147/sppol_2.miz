theorem
L2: (for R2 being (FinSequence of ( TOP-REAL 2 )) holds (for B1 , B2 being Nat holds (( B1 + B2 ) = ( len R2 ) implies ( LSeg (R2 , B1) ) = ( LSeg (( Rev R2 ) , B2) ))))
proof
let R2 being (FinSequence of ( TOP-REAL 2 ));
let C1 , C2 being Nat;
assume L3: ( C1 + C2 ) = ( len R2 );
per cases ;
suppose that L4: 1 <= C1
and
L5: ( C1 + 1 ) <= ( len R2 );
L6: ( C1 + ( C2 + 1 ) ) = ( ( len R2 ) + 1 ) by L3;
L7: C1 in ( dom R2 ) by L4 , L5 , SEQ_4:134;
L8: ( C2 + 1 ) in ( dom ( Rev R2 ) ) by L7 , L6 , FINSEQ_5:59;
L9: ( C2 + 1 ) <= ( len ( Rev R2 ) ) by L8 , FINSEQ_3:25;
L10: ( ( C1 + 1 ) + C2 ) = ( ( len R2 ) + 1 ) by L3;
L11: ( C1 + 1 ) in ( dom R2 ) by L4 , L5 , SEQ_4:134;
L12: C2 in ( dom ( Rev R2 ) ) by L11 , L10 , FINSEQ_5:59;
L13: 1 <= C2 by L12 , FINSEQ_3:25;
thus L14: ( LSeg (R2 , C1) ) = ( LSeg (( R2 /. C1 ) , ( R2 /. ( C1 + 1 ) )) ) by L4 , L5 , TOPREAL1:def 3
.= ( LSeg (( ( Rev R2 ) /. ( C2 + 1 ) ) , ( R2 /. ( C1 + 1 ) )) ) by L7 , L6 , FINSEQ_5:66
.= ( LSeg (( ( Rev R2 ) /. C2 ) , ( ( Rev R2 ) /. ( C2 + 1 ) )) ) by L11 , L10 , FINSEQ_5:66
.= ( LSeg (( Rev R2 ) , C2) ) by L9 , L13 , TOPREAL1:def 3;
end;
suppose L15: (not 1 <= C1);

L16: C1 = ( 0 ) by L15 , NAT_1:14;
L17: (not ( C2 + 1 ) <= ( len R2 )) by L16 , L3 , NAT_1:13;
L18: (not ( C2 + 1 ) <= ( len ( Rev R2 ) )) by L17 , FINSEQ_5:def 3;
L19: ( LSeg (R2 , C1) ) = ( {} ) by L15 , TOPREAL1:def 3;
thus L20: thesis by L19 , L18 , TOPREAL1:def 3;
end;
suppose L21: (not ( C1 + 1 ) <= ( len R2 ));

L22: ( LSeg (R2 , C1) ) = ( {} ) by L21 , TOPREAL1:def 3;
L23: C2 < 1 by L3 , L21 , XREAL_1:6;
thus L24: thesis by L23 , L22 , TOPREAL1:def 3;
end;
end;
