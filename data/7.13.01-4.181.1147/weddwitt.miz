theorem
L1: (for B1 being (Element of ( NAT )) holds (for B2 being Real holds ((1 < B2 & ( B2 |^ B1 ) = 1) implies B1 = ( 0 ))))
proof
let C1 being (Element of ( NAT ));
let C2 being Real;
assume that
L2: 1 < C2
and
L3: ( C2 |^ C1 ) = 1
and
L4: C1 <> ( 0 );
L5: C1 < ( 1 + 1 ) by L2 , L3 , PREPOWER:13;
L6: C1 <= ( ( 0 ) + 1 ) by L5 , NAT_1:13;
L7: C1 = 1 by L6 , L4 , NAT_1:8;
L8: ( C2 #Z C1 ) = C2 by L7 , PREPOWER:35;
thus L9: contradiction by L8 , L2 , L3 , PREPOWER:36;
end;
theorem
