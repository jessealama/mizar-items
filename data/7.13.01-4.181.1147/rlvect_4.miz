let R16 being (VECTOR of R13);
let R17 being (VECTOR of R13);
let R19 being (VECTOR of R13);
assume L28: ( R16 - R19 ) = ( R17 - R19 );
L29: ( - ( R16 - R19 ) ) = ( R19 - R17 ) by L28 , RLVECT_1:33;
L30: ( R19 - R16 ) = ( R19 - R17 ) by L29 , RLVECT_1:33;
thus L31: thesis by L30 , RLVECT_1:23;
end;
theorem
L32: (for R2 being Real holds (for R13 being RealLinearSpace holds (for R15 being (VECTOR of R13) holds ( - ( R2 * R15 ) ) = ( ( - R2 ) * R15 ))))
proof
let R2 being Real;
let R13 being RealLinearSpace;
let R15 being (VECTOR of R13);
thus L33: ( - ( R2 * R15 ) ) = ( R2 * ( - R15 ) ) by RLVECT_1:25
.= ( ( - R2 ) * R15 ) by RLVECT_1:24;
end;
theorem
L34: (for R13 being RealLinearSpace holds (for R15 being (VECTOR of R13) holds (for R24 being (Subspace of R13) holds (for R25 being (Subspace of R13) holds (R24 is (Subspace of R25) implies ( R15 + R24 ) c= ( R15 + R25 ))))))
proof
