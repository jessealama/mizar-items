theorem
L1: (for B1 being sup-Semilattice holds (for B2 , B3 being (Element of B1) holds ( "/\" (( ( uparrow B2 ) /\ ( uparrow B3 ) ) , B1) ) = ( B2 "\/" B3 )))
proof
let C1 being sup-Semilattice;
let C2 , C3 being (Element of C1);
L2: ( ( uparrow C2 ) /\ ( uparrow C3 ) ) = ( uparrow ( C2 "\/" C3 ) ) by WAYBEL14:4;
thus L3: thesis by L2 , WAYBEL_0:39;
end;
theorem
L4: (for B4 being Semilattice holds (for B5 , B6 being (Element of B4) holds ( "\/" (( ( downarrow B5 ) /\ ( downarrow B6 ) ) , B4) ) = ( B5 "/\" B6 )))
proof
