let C31 , C32 being (Subset of ( C28 opp ));
assume L69: (C29 = C31 & C30 = C32);
thus L70: ( C29 "/\" C30 ) c= ( C31 "\/" C32 )
proof
let C33 being set;
assume L71: C33 in ( C29 "/\" C30 );
L72: C33 in { ( B31 "/\" B32 ) where B31 , B32 is (Element of C28) : (B31 in C29 & B32 in C30) } by L71 , YELLOW_4:def 4;
consider C34 , C35 being (Element of C28) such that L73: C33 = ( C34 "/\" C35 ) and L74: (C34 in C29 & C35 in C30) by L72;
L75: C33 = ( ( C34 ~ ) "\/" ( C35 ~ ) ) by L73 , YELLOW_7:21;
L76: (( C34 ~ ) in C31 & ( C35 ~ ) in C32) by L69 , L74 , LATTICE3:def 6;
L77: C33 in { ( B33 "\/" B34 ) where B33 , B34 is (Element of ( C28 opp )) : (B33 in C31 & B34 in C32) } by L76 , L75;
thus L78: thesis by L77 , YELLOW_4:def 3;
end;

let C36 being set;
assume L79: C36 in ( C31 "\/" C32 );
L80: C36 in { ( B35 "\/" B36 ) where B35 , B36 is (Element of ( C28 opp )) : (B35 in C31 & B36 in C32) } by L79 , YELLOW_4:def 3;
consider C37 , C38 being (Element of ( C28 opp )) such that L81: C36 = ( C37 "\/" C38 ) and L82: (C37 in C31 & C38 in C32) by L80;
L83: C36 = ( ( ~ C37 ) "/\" ( ~ C38 ) ) by L81 , YELLOW_7:22;
L84: (( ~ C37 ) in C29 & ( ~ C38 ) in C30) by L69 , L82 , LATTICE3:def 7;
L85: C36 in { ( B37 "/\" B38 ) where B37 , B38 is (Element of C28) : (B37 in C29 & B38 in C30) } by L84 , L83;
thus L86: thesis by L85 , YELLOW_4:def 4;
end;
theorem
L87: (for B39 being non  empty  reflexive  transitive RelStr holds ( Filt B39 ) = ( Ids ( B39 opp ) ))
proof
let C39 being non  empty  reflexive  transitive RelStr;
L88: ( Ids ( C39 opp ) ) c= ( Filt C39 )
proof
let C40 being set;
assume L89: C40 in ( Ids ( C39 opp ) );
L90: C40 in { B40 where B40 is (Ideal of ( C39 opp )) : (not contradiction) } by L89 , WAYBEL_0:def 23;
consider C41 being (Ideal of ( C39 opp )) such that L91: C41 = C40 by L90;
L92: (C41 is  upper  upper  upper  upper (Subset of C39) & C41 is  filtered  filtered  filtered  filtered (Subset of C39)) by YELLOW_7:27 , YELLOW_7:29;
L93: C40 in { B41 where B41 is (Filter of C39) : (not contradiction) } by L92 , L91;
thus L94: thesis by L93 , WAYBEL_0:def 24;
end;
L95: ( Filt C39 ) c= ( Ids ( C39 opp ) )
proof
let C42 being set;
assume L96: C42 in ( Filt C39 );
L97: C42 in { B42 where B42 is (Filter of C39) : (not contradiction) } by L96 , WAYBEL_0:def 24;
consider C43 being (Filter of C39) such that L98: C43 = C42 by L97;
L99: (C43 is  lower  lower  lower  lower (Subset of ( C39 opp )) & C43 is  directed  directed  directed  directed (Subset of ( C39 opp ))) by YELLOW_7:27 , YELLOW_7:29;
L100: C42 in { B43 where B43 is (Ideal of ( C39 opp )) : (not contradiction) } by L99 , L98;
thus L101: thesis by L100 , WAYBEL_0:def 23;
end;
thus L102: thesis by L95 , L88 , XBOOLE_0:def 10;
end;
