set D8 = ( InclPoset ( Filt ( BoolePoset C66 ) ) );
let C67 being non  empty (Subset of D8);
L196: ( meet C67 ) is (Filter of ( BoolePoset C66 )) by L165;
L197: ( meet C67 ) in { B67 where B67 is (Filter of ( BoolePoset C66 )) : (not contradiction) } by L196;
