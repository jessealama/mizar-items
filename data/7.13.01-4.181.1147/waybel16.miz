theorem
L49: (for B13 being  with_suprema Poset holds (for B14 , B15 being (Subset of B13) holds (for B16 , B17 being (Subset of ( B13 opp )) holds ((B14 = B16 & B15 = B17) implies ( B14 "\/" B15 ) = ( B16 "/\" B17 )))))
proof
let C17 being  with_suprema Poset;
let C18 , C19 being (Subset of C17);
let C20 , C21 being (Subset of ( C17 opp ));
assume L50: (C18 = C20 & C19 = C21);
thus L51: ( C18 "\/" C19 ) c= ( C20 "/\" C21 )
proof
let C22 being set;
assume L52: C22 in ( C18 "\/" C19 );
L53: C22 in { ( B18 "\/" B19 ) where B18 , B19 is (Element of C17) : (B18 in C18 & B19 in C19) } by L52 , YELLOW_4:def 3;
consider C23 , C24 being (Element of C17) such that L54: C22 = ( C23 "\/" C24 ) and L55: (C23 in C18 & C24 in C19) by L53;
L56: C22 = ( ( C23 ~ ) "/\" ( C24 ~ ) ) by L54 , YELLOW_7:23;
L57: (( C23 ~ ) in C20 & ( C24 ~ ) in C21) by L50 , L55 , LATTICE3:def 6;
L58: C22 in { ( B20 "/\" B21 ) where B20 , B21 is (Element of ( C17 opp )) : (B20 in C20 & B21 in C21) } by L57 , L56;
thus L59: thesis by L58 , YELLOW_4:def 4;
end;

let C25 being set;
assume L60: C25 in ( C20 "/\" C21 );
L61: C25 in { ( B22 "/\" B23 ) where B22 , B23 is (Element of ( C17 opp )) : (B22 in C20 & B23 in C21) } by L60 , YELLOW_4:def 4;
consider C26 , C27 being (Element of ( C17 opp )) such that L62: C25 = ( C26 "/\" C27 ) and L63: (C26 in C20 & C27 in C21) by L61;
L64: C25 = ( ( ~ C26 ) "\/" ( ~ C27 ) ) by L62 , YELLOW_7:24;
L65: (( ~ C26 ) in C18 & ( ~ C27 ) in C19) by L50 , L63 , LATTICE3:def 7;
L66: C25 in { ( B24 "\/" B25 ) where B24 , B25 is (Element of C17) : (B24 in C18 & B25 in C19) } by L65 , L64;
thus L67: thesis by L66 , YELLOW_4:def 3;
end;
theorem
L68: (for B26 being  with_infima Poset holds (for B27 , B28 being (Subset of B26) holds (for B29 , B30 being (Subset of ( B26 opp )) holds ((B27 = B29 & B28 = B30) implies ( B27 "/\" B28 ) = ( B29 "\/" B30 )))))
proof
