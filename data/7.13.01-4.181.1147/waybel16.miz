let C5 , C6 being (Element of C4);
L5: ( ( downarrow C5 ) /\ ( downarrow C6 ) ) = ( downarrow ( C5 "/\" C6 ) ) by WAYBEL14:3;
thus L6: thesis by L5 , WAYBEL_0:34;
end;
theorem
L7: (for B7 being non  empty RelStr holds (for B8 , B9 being (Element of B7) holds (B8 is_maximal_in ( (the carrier of B7) \ ( uparrow B9 ) ) implies ( ( uparrow B8 ) \ { B8 } ) = ( ( uparrow B8 ) /\ ( uparrow B9 ) ))))
proof
let C7 being non  empty RelStr;
let C8 , C9 being (Element of C7);
assume L8: C8 is_maximal_in ( (the carrier of C7) \ ( uparrow C9 ) );
L9: C8 in ( (the carrier of C7) \ ( uparrow C9 ) ) by L8 , WAYBEL_4:55;
L10: (not C8 in ( uparrow C9 )) by L9 , XBOOLE_0:def 5;
L11: (not C9 <= C8) by L10 , WAYBEL_0:18;
thus L12: ( ( uparrow C8 ) \ { C8 } ) c= ( ( uparrow C8 ) /\ ( uparrow C9 ) )
proof
let C10 being set;
assume L13: C10 in ( ( uparrow C8 ) \ { C8 } );
reconsider D1 = C10 as (Element of C7) by L13;
L14: (not C10 in { C8 }) by L13 , XBOOLE_0:def 5;
L15: D1 <> C8 by L14 , TARSKI:def 1;
L16: C10 in ( uparrow C8 ) by L13 , XBOOLE_0:def 5;
L17: C8 <= D1 by L16 , WAYBEL_0:18;
L18: C8 < D1 by L17 , L15 , ORDERS_2:def 6;
L19: (not D1 in ( (the carrier of C7) \ ( uparrow C9 ) )) by L18 , L8 , WAYBEL_4:55;
L20: C10 in ( uparrow C9 ) by L19 , XBOOLE_0:def 5;
thus L21: thesis by L20 , L16 , XBOOLE_0:def 4;
end;

let C11 being set;
assume L22: C11 in ( ( uparrow C8 ) /\ ( uparrow C9 ) );
L23: C11 in ( uparrow C8 ) by L22 , XBOOLE_0:def 4;
reconsider D2 = C11 as (Element of C7) by L22;
L24: C11 in ( uparrow C9 ) by L22 , XBOOLE_0:def 4;
L25: C9 <= D2 by L24 , WAYBEL_0:18;
L26: (not C11 in { C8 }) by L25 , L11 , TARSKI:def 1;
thus L27: thesis by L26 , L23 , XBOOLE_0:def 5;
end;
theorem
L28: (for B10 being non  empty RelStr holds (for B11 , B12 being (Element of B10) holds (B11 is_minimal_in ( (the carrier of B10) \ ( downarrow B12 ) ) implies ( ( downarrow B11 ) \ { B11 } ) = ( ( downarrow B11 ) /\ ( downarrow B12 ) ))))
proof
let C12 being non  empty RelStr;
let C13 , C14 being (Element of C12);
assume L29: C13 is_minimal_in ( (the carrier of C12) \ ( downarrow C14 ) );
L30: C13 in ( (the carrier of C12) \ ( downarrow C14 ) ) by L29 , WAYBEL_4:56;
L31: (not C13 in ( downarrow C14 )) by L30 , XBOOLE_0:def 5;
L32: (not C13 <= C14) by L31 , WAYBEL_0:17;
thus L33: ( ( downarrow C13 ) \ { C13 } ) c= ( ( downarrow C13 ) /\ ( downarrow C14 ) )
proof
let C15 being set;
assume L34: C15 in ( ( downarrow C13 ) \ { C13 } );
reconsider D3 = C15 as (Element of C12) by L34;
L35: (not C15 in { C13 }) by L34 , XBOOLE_0:def 5;
L36: D3 <> C13 by L35 , TARSKI:def 1;
L37: C15 in ( downarrow C13 ) by L34 , XBOOLE_0:def 5;
L38: D3 <= C13 by L37 , WAYBEL_0:17;
L39: D3 < C13 by L38 , L36 , ORDERS_2:def 6;
L40: (not D3 in ( (the carrier of C12) \ ( downarrow C14 ) )) by L39 , L29 , WAYBEL_4:56;
L41: C15 in ( downarrow C14 ) by L40 , XBOOLE_0:def 5;
thus L42: thesis by L41 , L37 , XBOOLE_0:def 4;
end;

let C16 being set;
assume L43: C16 in ( ( downarrow C13 ) /\ ( downarrow C14 ) );
L44: C16 in ( downarrow C13 ) by L43 , XBOOLE_0:def 4;
reconsider D4 = C16 as (Element of C12) by L43;
L45: C16 in ( downarrow C14 ) by L43 , XBOOLE_0:def 4;
L46: D4 <= C14 by L45 , WAYBEL_0:17;
L47: (not C16 in { C13 }) by L46 , L32 , TARSKI:def 1;
thus L48: thesis by L47 , L44 , XBOOLE_0:def 5;
