thus L31: thesis by L30;
end;
thus L32: (ex B10 being (basis of C14) st B10 is  compact) by L28 , L21;
end;
assume L33: (for B11 being (Point of C13) holds (ex B12 being (basis of B11) st B12 is  compact));
let C19 being (Point of C13);
let C20 being (Subset of C13);
assume that
L34: C19 in C20
and
L35: C20 is  open;
