theorem
L152: (for B28 being  Hausdorff non  empty TopSpace holds (for B29 being non  empty (Subset of B28) holds (( B28 | B29 ) is  dense  locally-compact implies ( B28 | B29 ) is  open)))
proof
let C62 being  Hausdorff non  empty TopSpace;
let C63 being non  empty (Subset of C62);
assume that
L153: ( C62 | C63 ) is  dense  locally-compact;
L154: ( [#] ( C62 | C63 ) ) = C63 by PRE_TOPC:8;
L155: ( Int C63 ) = C63
proof
thus L156: ( Int C63 ) c= C63 by TOPS_1:16;
let C64 being set;
assume L157: C64 in C63;
reconsider D18 = C64 as (Point of C62) by L157;
reconsider D19 = D18 as (Point of ( C62 | C63 )) by L157 , PRE_TOPC:8;
set D20 = { B30 where B30 is (Subset of C62) : (B30 is  open & B30 c= C63) };
consider C65 being (a_neighborhood of D19) such that L158: C65 is  compact by L153 , L48;
reconsider D21 = C65 as (Subset of C62) by L154 , XBOOLE_1:1;
L159: C65 c= ( [#] ( C62 | C63 ) );
L160: ( Int C65 ) in (the topology of ( C62 | C63 )) by PRE_TOPC:def 2;
consider C66 being (Subset of C62) such that L161: C66 in (the topology of C62) and L162: ( Int C65 ) = ( C66 /\ C63 ) by L160 , L154 , PRE_TOPC:def 4;
L163: C66 is  open by L161 , PRE_TOPC:def 2;
L164: (for B31 being (Subset of ( C62 | C63 )) holds (B31 = D21 implies B31 is  compact)) by L158;
L165: D21 is  compact by L164 , L159 , COMPTS_1:2;
L166: ( Cl ( C66 /\ C63 ) ) c= D21 by L165 , L162 , TOPS_1:5 , TOPS_1:16;
L167: C63 is  dense by L153 , L154 , TEX_3:def 1;
L168: ( Cl ( C66 /\ C63 ) ) = ( Cl C66 ) by L167 , L163 , TOPS_1:46;
L169: C66 c= ( Cl C66 ) by PRE_TOPC:18;
L170: C66 c= C65 by L169 , L168 , L166 , XBOOLE_1:1;
L171: C66 c= C63 by L170 , L154 , XBOOLE_1:1;
L172: C66 in D20 by L171 , L163;
L173: D19 in ( Int C65 ) by CONNSP_2:def 1;
L174: ( Int C65 ) c= C66 by L162 , XBOOLE_1:17;
L175: C64 in ( union D20 ) by L174 , L173 , L172 , TARSKI:def 4;
thus L176: thesis by L175 , TEX_4:3;
end;
thus L177: thesis by L155 , L154 , TSEP_1:16;
end;
