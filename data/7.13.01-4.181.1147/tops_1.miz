theorem
L55: (for B1 being non  empty TopSpace holds (for B2 being (Subset of B1) holds (for B3 being (Point of B1) holds (B3 in ( Cl B2 ) iff (for B4 being (Subset of B1) holds ((B4 is  open & B3 in B4) implies B2 meets B4))))))
proof
let C12 being non  empty TopSpace;
let C13 being (Subset of C12);
let C14 being (Point of C12);
thus L56:now
assume L57: C14 in ( Cl C13 );
given C15 being (Subset of C12) such that
L58: C15 is  open
and
L59: C14 in C15
and
L60: C13 misses C15;

L61: ( ( C13 ` ) \/ ( C15 ` ) ) = ( ( C13 /\ C15 ) ` ) by XBOOLE_1:54;
L62: ( C13 /\ C15 ) = ( {} C12 ) by L60 , XBOOLE_0:def 7;
L63: C13 c= ( C15 ` )
proof
let R6 being set;
assume L64: R6 in C13;
L65: (R6 in ( C13 ` ) or R6 in ( C15 ` )) by L64 , L61 , L62 , XBOOLE_0:def 3;
thus L66: thesis by L65 , L64 , XBOOLE_0:def 5;
end;
L67: ( Cl ( C15 ` ) ) = ( C15 ` ) by L58 , PRE_TOPC:22;
L68: ( Cl C13 ) c= ( C15 ` ) by L67 , L63 , PRE_TOPC:19;
thus L69: contradiction by L68 , L57 , L59 , XBOOLE_0:def 5;
end;
assume L70: (for B5 being (Subset of C12) holds ((B5 is  open & C14 in B5) implies C13 meets B5));
assume L71: (not C14 in ( Cl C13 ));
L72: C14 in ( ( Cl C13 ) ` ) by L71 , SUBSET_1:29;
L73: C13 meets ( ( Cl C13 ) ` ) by L72 , L70;
L74: C13 misses ( C13 ` ) by XBOOLE_1:79;
L75: ( C13 /\ ( C13 ` ) ) = ( {} ) by L74 , XBOOLE_0:def 7;
L76: C13 c= ( Cl C13 ) by PRE_TOPC:18;
L77: ( ( Cl C13 ) ` ) c= ( C13 ` ) by L76 , SUBSET_1:12;
L78: ( C13 /\ ( ( Cl C13 ) ` ) ) = ( {} ) by L77 , L75 , XBOOLE_1:3 , XBOOLE_1:26;
thus L79: contradiction by L78 , L73 , XBOOLE_0:def 7;
end;
theorem
L80: (for R4 being TopSpace holds (for R8 being (Subset of R4) holds (for R9 being (Subset of R4) holds (R8 is  open implies ( R8 /\ ( Cl R9 ) ) c= ( Cl ( R8 /\ R9 ) )))))
proof
let R4 being TopSpace;
let R8 being (Subset of R4);
let R9 being (Subset of R4);
assume L81: R8 is  open;
let R6 being set;
assume L82: R6 in ( R8 /\ ( Cl R9 ) );
L83: R6 in ( Cl R9 ) by L82 , XBOOLE_0:def 4;
reconsider D1 = R6 as (Point of R4) by L82;
L84: R4 is non  empty by L82;
L85: R6 in R8 by L82 , XBOOLE_0:def 4;
L86: (for B6 being (Subset of R4) holds (B6 is  open implies (D1 in B6 implies ( R8 /\ R9 ) meets B6)))
proof
let C16 being (Subset of R4);
assume L87: C16 is  open;
assume L88: D1 in C16;
L89: D1 in ( C16 /\ R8 ) by L88 , L85 , XBOOLE_0:def 4;
L90: R9 meets ( C16 /\ R8 ) by L89 , L81 , L83 , L84 , L87 , L55;
L91: ( R9 /\ ( C16 /\ R8 ) ) <> ( {} ) by L90 , XBOOLE_0:def 7;
L92: ( R9 /\ ( C16 /\ R8 ) ) = ( ( R8 /\ R9 ) /\ C16 ) by XBOOLE_1:16;
thus L93: thesis by L92 , L91 , XBOOLE_0:def 7;
end;
thus L94: thesis by L86 , L84 , L55;
end;
theorem
L95: (for R4 being TopSpace holds (for R8 being (Subset of R4) holds (for R9 being (Subset of R4) holds (R8 is  open implies ( Cl ( R8 /\ ( Cl R9 ) ) ) = ( Cl ( R8 /\ R9 ) )))))
proof
let R4 being TopSpace;
let R8 being (Subset of R4);
let R9 being (Subset of R4);
L96: ( Cl ( R8 /\ R9 ) ) c= ( Cl ( R8 /\ ( Cl R9 ) ) )
proof
let R6 being set;
assume L97: R6 in ( Cl ( R8 /\ R9 ) );
reconsider D2 = R6 as (Point of R4) by L97;
L98: R4 is non  empty by L97;
L99: (for B7 being (Subset of R4) holds (B7 is  open implies (D2 in B7 implies ( R8 /\ ( Cl R9 ) ) meets B7)))
proof
let C17 being (Subset of R4);
assume L100: C17 is  open;
assume L101: D2 in C17;
L102: ( R8 /\ R9 ) meets C17 by L101 , L97 , L98 , L100 , L55;
L103: ( ( R8 /\ R9 ) /\ C17 ) <> ( {} ) by L102 , XBOOLE_0:def 7;
L104: ( R8 /\ R9 ) c= ( R8 /\ ( Cl R9 ) ) by PRE_TOPC:18 , XBOOLE_1:26;
L105: ( ( R8 /\ ( Cl R9 ) ) /\ C17 ) <> ( {} ) by L104 , L103 , XBOOLE_1:3 , XBOOLE_1:26;
thus L106: thesis by L105 , XBOOLE_0:def 7;
end;
thus L107: thesis by L99 , L98 , L55;
end;
assume L108: R8 is  open;
L109: ( Cl ( R8 /\ ( Cl R9 ) ) ) c= ( Cl ( Cl ( R8 /\ R9 ) ) ) by L108 , L80 , PRE_TOPC:19;
thus L110: thesis by L109 , L96 , XBOOLE_0:def 10;
end;
