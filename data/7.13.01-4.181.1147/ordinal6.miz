theorem
L38: (for R9 being set holds (for R10 being set holds (for B2 being Function holds (B2 is_isomorphism_of ( RelIncl R9 ) , ( RelIncl R10 ) implies (for R11 being set holds (for R12 being set holds ((R11 in R9 & R12 in R9) implies (R11 c= R12 iff ( B2 . R11 ) c= ( B2 . R12 )))))))))
proof
let R9 being set;
let R10 being set;
let C6 being Function;
assume L39: C6 is_isomorphism_of ( RelIncl R9 ) , ( RelIncl R10 );
let R11 being set;
let R12 being set;
assume that
L40: (R11 in R9 & R12 in R9);
L41: (( field ( RelIncl R9 ) ) = R9 & ( field ( RelIncl R10 ) ) = R10) by WELLORD2:def 1;
L42: (( dom C6 ) = R9 & ( rng C6 ) = R10) by L41 , L39 , WELLORD1:def 7;
L43: (( C6 . R11 ) in R10 & ( C6 . R12 ) in R10) by L42 , L40 , FUNCT_1:def 3;
L44: (R11 c= R12 iff [ R11 , R12 ] in ( RelIncl R9 )) by L40 , WELLORD2:def 1;
L45: (R11 c= R12 iff [ ( C6 . R11 ) , ( C6 . R12 ) ] in ( RelIncl R10 )) by L44 , L39 , L40 , L41 , WELLORD1:def 7;
thus L46: thesis by L45 , L43 , WELLORD2:def 1;
end;
theorem
L47: (for B3 , B4 being  ordinal-membered set holds (for B5 being Function holds (B5 is_isomorphism_of ( RelIncl B3 ) , ( RelIncl B4 ) implies (for R11 being set holds (for R12 being set holds ((R11 in B3 & R12 in B3) implies (R11 in R12 iff ( B5 . R11 ) in ( B5 . R12 ))))))))
proof
