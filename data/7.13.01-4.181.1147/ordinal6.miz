L61: ( dom ( C10 ^ C11 ) ) = ( ( dom C10 ) +^ ( dom C11 ) ) by ORDINAL4:def 1;
L62: ( dom C10 ) c= ( dom ( C10 ^ C11 ) ) by L61 , ORDINAL3:24;
L63: (for R11 being set holds (R11 in ( dom C10 ) implies ( C10 . R11 ) = ( ( C10 ^ C11 ) . R11 ))) by ORDINAL4:def 1;
thus L64: C10 c= ( C10 ^ C11 ) by L63 , L62 , GRFUNC_1:2;
end;
theorem
L65: (for B8 , B9 being T-Sequence holds ( rng ( B8 ^ B9 ) ) = ( ( rng B8 ) \/ ( rng B9 ) ))
proof
let C12 , C13 being T-Sequence;
set D6 = ( C12 ^ C13 );
set D7 = ( rng C12 );
set D8 = ( rng C13 );
L66: ( dom ( C12 ^ C13 ) ) = ( ( dom C12 ) +^ ( dom C13 ) ) by ORDINAL4:def 1;
thus L67: ( rng ( C12 ^ C13 ) ) c= ( ( rng C12 ) \/ ( rng C13 ) )
proof
let R12 being set;
assume L68: R12 in ( rng D6 );
consider R11 being set such that L69: R11 in ( dom D6 ) and L70: R12 = ( D6 . R11 ) by L68 , FUNCT_1:def 3;
reconsider D9 = R11 as Ordinal by L69;
per cases ;
suppose L71: D9 in ( dom C12 );

L72: ( C12 . D9 ) in ( rng C12 ) by L71 , FUNCT_1:def 3;
L73: R12 = ( C12 . D9 ) by L70 , L71 , ORDINAL4:def 1;
thus L74: thesis by L73 , L72 , XBOOLE_0:def 3;
end;
suppose L75: (not D9 in ( dom C12 ));

L76: ( dom C12 ) c= D9 by L75 , ORDINAL1:16;
L77: ( ( dom C12 ) +^ ( D9 -^ ( dom C12 ) ) ) = D9 by L76 , ORDINAL3:def 5;
L78: ( D9 -^ ( dom C12 ) ) in ( dom C13 ) by L77 , L66 , L69 , ORDINAL3:22;
L79: R12 = ( C13 . ( D9 -^ ( dom C12 ) ) ) by L78 , L70 , L77 , ORDINAL4:def 1;
L80: R12 in ( rng C13 ) by L79 , L78 , FUNCT_1:def 3;
thus L81: thesis by L80 , XBOOLE_0:def 3;
end;
end;

let R11 being set;
assume L83: R11 in ( ( rng C12 ) \/ ( rng C13 ) );
L84: ( dom C12 ) c= ( ( dom C12 ) +^ ( dom C13 ) ) by ORDINAL3:24;
per cases  by L83 , XBOOLE_0:def 3;
suppose L85: R11 in ( rng C12 );

consider R13 being set such that L86: (R13 in ( dom C12 ) & R11 = ( C12 . R13 )) by L85 , FUNCT_1:def 3;
L87: R11 = ( D6 . R13 ) by L86 , ORDINAL4:def 1;
thus L88: thesis by L87 , L66 , L84 , L86 , FUNCT_1:def 3;
end;
suppose L89: R11 in ( rng C13 );

consider R13 being set such that L90: (R13 in ( dom C13 ) & R11 = ( C13 . R13 )) by L89 , FUNCT_1:def 3;
reconsider D10 = R13 as Ordinal by L90;
L91: ( ( dom C12 ) +^ D10 ) in ( dom D6 ) by L66 , L90 , ORDINAL3:17;
L92: R11 = ( D6 . ( ( dom C12 ) +^ D10 ) ) by L90 , ORDINAL4:def 1;
thus L93: thesis by L92 , L91 , FUNCT_1:def 3;
end;
end;
theorem
L95: (for R1 being  ordinal number holds (for R2 being  ordinal number holds (R1 c= R2 iff ( epsilon_ R1 ) c= ( epsilon_ R2 ))))
proof
let R1 being  ordinal number;
let R2 being  ordinal number;
thus L96:now
assume L97: R1 c= R2;
L98: (R1 = R2 or R1 c< R2) by L97 , XBOOLE_0:def 8;
L99: (R1 = R2 or ( epsilon_ R1 ) in ( epsilon_ R2 )) by L98 , ORDINAL1:11 , ORDINAL5:44;
thus L100: ( epsilon_ R1 ) c= ( epsilon_ R2 ) by L99 , ORDINAL1:def 2;
end;
assume L101: ( epsilon_ R1 ) c= ( epsilon_ R2 );
assume L102: R1 c/= R2;
L103: ( epsilon_ R2 ) in ( epsilon_ R1 ) by L102 , ORDINAL1:16 , ORDINAL5:44;
L104: ( epsilon_ R2 ) in ( epsilon_ R2 ) by L103 , L101;
thus L105: thesis by L104;
end;
