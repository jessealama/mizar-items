coherence
proof
let C17 being Ordinal-Sequence;
assume that
L123: C17 = ( id R1 );
let R2 being  ordinal number;
let R3 being  ordinal number;
assume L124: (R2 in ( dom C17 ) & R2 <> ( {} ) & R2 is  limit_ordinal & R3 = ( C17 . R2 ));
set D11 = ( C17 | R2 );
L125: (( dom C17 ) = R1 & ( dom ( id R2 ) ) = R2) by L123 , RELAT_1:45;
L126: R3 = R2 by L125 , L123 , L124 , FUNCT_1:18;
L127: R2 c= R1 by L124 , L125 , ORDINAL1:def 2;
L128: D11 = ( id R2 ) by L127 , L123 , FUNCT_3:1;
per cases ;
case L129: R3 = ( {} );
thus L130: thesis by L129 , L124 , L123 , L125 , FUNCT_1:18;
end;
case L131: R3 <> ( {} );
let C18 , C19 being Ordinal;
assume L132: (C18 in R3 & R3 in C19);
take D12 = ( succ C18 );
thus L133: D12 in ( dom D11 ) by L124 , L125 , L126 , L128 , L132 , ORDINAL1:28;
