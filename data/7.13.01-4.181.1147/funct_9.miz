definition
let C1 being  real number;
let C2 being Function;
attr C2 is C1 -periodic
means
:L1: (C1 <> ( 0 ) & (for R1 being  real number holds ((R1 in ( dom C2 ) iff ( R1 + C1 ) in ( dom C2 )) & (R1 in ( dom C2 ) implies ( C2 . R1 ) = ( C2 . ( R1 + C1 ) )))));
end;
definition
let C3 being Function;
attr C3 is  periodic
means
:L3: (ex R2 being  real number st C3 is R2 -periodic);
end;
