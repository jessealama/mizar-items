theorem
L1: (for R3 being  real number holds (for R4 being  real number holds (R4 <= R3 implies (R4 <= ( ( R4 + R3 ) / 2 ) & ( ( R4 + R3 ) / 2 ) <= R3))))
proof
let R3 being  real number;
let R4 being  real number;
assume L2: R4 <= R3;
per cases  by L2 , XXREAL_0:1;
suppose L3: R4 < R3;

thus L4: thesis by L3 , XREAL_1:226;
end;
suppose L5: R4 = R3;

thus L6: thesis by L5;
end;
end;
theorem
L8: (for B1 being non  empty TopSpace holds (for B2 being (Subset of B1) holds (for B3 being (Subset of ( B1 | B2 )) holds (for B4 being (Subset of B1) holds ((B4 is  closed & B3 = ( B4 /\ B2 )) implies B3 is  closed)))))
proof
let C1 being non  empty TopSpace;
let C2 being (Subset of C1);
let C3 being (Subset of ( C1 | C2 ));
let C4 being (Subset of C1);
assume that
L9: C4 is  closed
and
L10: C3 = ( C4 /\ C2 );
L11: ( [#] ( C1 | C2 ) ) = C2 by PRE_TOPC:def 5;
thus L12: thesis by L11 , L9 , L10 , PRE_TOPC:13;
end;
theorem
L13: (for B5 , B6 being non  empty TopSpace holds (for B7 being non  empty (Subset of B6) holds (for B8 being (Function of B5 , ( B6 | B7 )) holds (B8 is (Function of B5 , B6) & (for B9 being (Function of B5 , B6) holds ((B9 = B8 & B8 is  continuous) implies B9 is  continuous))))))
proof
let C5 , C6 being non  empty TopSpace;
let C7 being non  empty (Subset of C6);
let C8 being (Function of C5 , ( C6 | C7 ));
L14: (the carrier of ( C6 | C7 )) = ( [#] ( C6 | C7 ) )
.= C7 by PRE_TOPC:def 5;
thus L15: C8 is (Function of C5 , C6) by L14 , FUNCT_2:7;
let C9 being (Function of C5 , C6);
assume that
L16: C9 = C8
and
L17: C8 is  continuous;
let C10 being (Subset of C6);
assume L18: C10 is  closed;
reconsider D1 = ( C10 /\ C7 ) as (Subset of ( C6 | C7 )) by TOPS_2:29;
L19: D1 is  closed by L18 , L8;
L20: ( C9 " C7 ) = (the carrier of C5) by L14 , L16 , FUNCT_2:40
.= ( dom C9 ) by FUNCT_2:def 1;
L21: ( C9 " C10 ) = ( ( C9 " C10 ) /\ ( C9 " C7 ) ) by L20 , RELAT_1:132 , XBOOLE_1:28
.= ( C8 " D1 ) by L16 , FUNCT_1:68;
thus L22: thesis by L21 , L17 , L19 , PRE_TOPC:def 6;
end;
