scheme FraenkelA2 { F1() -> non  empty set , F2(set , set) -> set , P1[set , set] , P2[set , set] } : { F2(B1 , B2) where B1 is (Element of F1()) , B2 is (Element of F1()) : P1[ B1 , B2 ] } is (Subset of F1())
provided
L1: (for B3 being (Element of F1()) holds (for B4 being (Element of F1()) holds F2(B3 , B4) in F1()))
proof
L2: { F2(B5 , B6) where B5 is (Element of F1()) , B6 is (Element of F1()) : P1[ B5 , B6 ] } c= F1()
proof
let C1 being set;
assume L3: C1 in { F2(B7 , B8) where B7 is (Element of F1()) , B8 is (Element of F1()) : P1[ B7 , B8 ] };
L4: (ex B9 , B10 being (Element of F1()) st (C1 = F2(B9 , B10) & P1[ B9 , B10 ])) by L3;
thus L5: thesis by L4 , L1;
end;
thus L6: thesis by L2;
end;
registration
let C2 being RelStr;
let C3 being  empty (Subset of C2);
cluster ( downarrow C3 ) ->  empty;
coherence
proof
assume L7: ( downarrow C3 ) is non  empty;
consider C4 being set such that L8: C4 in ( downarrow C3 ) by L7 , XBOOLE_0:def 1;
reconsider D1 = C4 as (Element of C2) by L8;
L9: (ex B11 being (Element of C2) st (D1 <= B11 & B11 in C3)) by L8 , WAYBEL_0:def 15;
thus L10: contradiction by L9;
end;
cluster ( uparrow C3 ) ->  empty;
