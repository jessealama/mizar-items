definition
let C102 being RealLinearSpace;
let C103 being (Subset of C102);
redefine attr C103 is  convex
means
:L169: (for B122 , B123 being (Point of C102) holds (for B124 being Real holds ((( 0 ) <= B124 & B124 <= 1 & B122 in C103 & B123 in C103) implies ( ( B124 * B122 ) + ( ( 1 - B124 ) * B123 ) ) in C103)));
compatibility
proof
thus L170:now
assume L171: C103 is  convex;
let C104 , C105 being (Point of C102);
let C106 being Real;
assume that
L172: (( 0 ) <= C106 & C106 <= 1)
