let R5 being Group;
let R6 being (Element of R5);
let R7 being (Element of R5);
thus L2: ( ( R6 * R7 ) * ( R7 " ) ) = ( R6 * ( R7 * ( R7 " ) ) ) by GROUP_1:def 3
