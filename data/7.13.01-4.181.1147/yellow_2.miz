theorem
L69: (for B17 being Relation holds (for B18 , B19 being set holds (B18 c= B19 implies ( B17 |_2 B18 ) c= ( B17 |_2 B19 ))))
proof
let C21 being Relation;
let C22 , C23 being set;
assume L70: C22 c= C23;
L71: ( C22 |` C21 ) c= ( C23 |` C21 ) by L70 , RELAT_1:100;
L72: ( ( C22 |` C21 ) | C22 ) c= ( ( C23 |` C21 ) | C22 ) by L71 , RELAT_1:76;
L73: ( ( C23 |` C21 ) | C22 ) c= ( ( C23 |` C21 ) | C23 ) by L70 , RELAT_1:75;
L74: ( ( C22 |` C21 ) | C22 ) c= ( ( C23 |` C21 ) | C23 ) by L73 , L72 , XBOOLE_1:1;
L75: ( C21 |_2 C22 ) c= ( ( C23 |` C21 ) | C23 ) by L74 , WELLORD1:10;
thus L76: thesis by L75 , WELLORD1:10;
end;
