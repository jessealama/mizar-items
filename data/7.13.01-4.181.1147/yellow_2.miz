theorem
L17: (for B9 being  antisymmetric  transitive  with_suprema RelStr holds (for B10 , B11 being set holds (( ex_sup_of B10 , B9 &  ex_sup_of B11 , B9) implies ( ex_sup_of ( B10 \/ B11 ) , B9 & ( "\/" (( B10 \/ B11 ) , B9) ) = ( ( "\/" (B10 , B9) ) "\/" ( "\/" (B11 , B9) ) )))))
proof
let C11 being  antisymmetric  transitive  with_suprema RelStr;
let C12 , C13 being set;
assume that
L18:  ex_sup_of C12 , C11
and
L19:  ex_sup_of C13 , C11;
set D3 = ( ( "\/" (C12 , C11) ) "\/" ( "\/" (C13 , C11) ) );
L20: ( C12 \/ C13 ) is_<=_than D3
proof
let C14 being (Element of C11);
assume L21: C14 in ( C12 \/ C13 );
per cases  by L21 , XBOOLE_0:def 3;
suppose L22: C14 in C12;

L23: C12 is_<=_than ( "\/" (C12 , C11) ) by L18 , YELLOW_0:30;
L24: C14 <= ( "\/" (C12 , C11) ) by L23 , L22 , LATTICE3:def 9;
L25: ( "\/" (C12 , C11) ) <= D3 by YELLOW_0:22;
thus L26: thesis by L25 , L24 , ORDERS_2:3;
end;
suppose L27: C14 in C13;

L28: C13 is_<=_than ( "\/" (C13 , C11) ) by L19 , YELLOW_0:30;
L29: C14 <= ( "\/" (C13 , C11) ) by L28 , L27 , LATTICE3:def 9;
L30: ( "\/" (C13 , C11) ) <= D3 by YELLOW_0:22;
thus L31: thesis by L30 , L29 , ORDERS_2:3;
end;
end;
L33: (for B12 being (Element of C11) holds (( C12 \/ C13 ) is_<=_than B12 implies D3 <= B12))
proof
let C15 being (Element of C11);
assume L34: ( C12 \/ C13 ) is_<=_than C15;
L35: C13 c= ( C12 \/ C13 ) by XBOOLE_1:7;
L36: C13 is_<=_than C15 by L35 , L34 , YELLOW_0:9;
L37: ( "\/" (C13 , C11) ) <= C15 by L36 , L19 , YELLOW_0:30;
L38: C12 c= ( C12 \/ C13 ) by XBOOLE_1:7;
L39: C12 is_<=_than C15 by L38 , L34 , YELLOW_0:9;
L40: ( "\/" (C12 , C11) ) <= C15 by L39 , L18 , YELLOW_0:30;
thus L41: thesis by L40 , L37 , YELLOW_0:22;
end;
thus L42: thesis by L33 , L20 , YELLOW_0:30;
end;
theorem
L43: (for B13 being  antisymmetric  transitive  with_infima RelStr holds (for B14 , B15 being set holds (( ex_inf_of B14 , B13 &  ex_inf_of B15 , B13) implies ( ex_inf_of ( B14 \/ B15 ) , B13 & ( "/\" (( B14 \/ B15 ) , B13) ) = ( ( "/\" (B14 , B13) ) "/\" ( "/\" (B15 , B13) ) )))))
proof
let C16 being  antisymmetric  transitive  with_infima RelStr;
let C17 , C18 being set;
assume that
L44:  ex_inf_of C17 , C16
and
L45:  ex_inf_of C18 , C16;
set D4 = ( ( "/\" (C17 , C16) ) "/\" ( "/\" (C18 , C16) ) );
L46: ( C17 \/ C18 ) is_>=_than D4
proof
let C19 being (Element of C16);
assume L47: C19 in ( C17 \/ C18 );
per cases  by L47 , XBOOLE_0:def 3;
suppose L48: C19 in C17;

L49: C17 is_>=_than ( "/\" (C17 , C16) ) by L44 , YELLOW_0:31;
L50: C19 >= ( "/\" (C17 , C16) ) by L49 , L48 , LATTICE3:def 8;
L51: ( "/\" (C17 , C16) ) >= D4 by YELLOW_0:23;
thus L52: thesis by L51 , L50 , ORDERS_2:3;
end;
suppose L53: C19 in C18;

L54: C18 is_>=_than ( "/\" (C18 , C16) ) by L45 , YELLOW_0:31;
L55: C19 >= ( "/\" (C18 , C16) ) by L54 , L53 , LATTICE3:def 8;
L56: ( "/\" (C18 , C16) ) >= D4 by YELLOW_0:23;
thus L57: thesis by L56 , L55 , ORDERS_2:3;
end;
end;
L59: (for B16 being (Element of C16) holds (( C17 \/ C18 ) is_>=_than B16 implies D4 >= B16))
proof
let C20 being (Element of C16);
assume L60: ( C17 \/ C18 ) is_>=_than C20;
L61: C18 c= ( C17 \/ C18 ) by XBOOLE_1:7;
L62: C18 is_>=_than C20 by L61 , L60 , YELLOW_0:9;
L63: ( "/\" (C18 , C16) ) >= C20 by L62 , L45 , YELLOW_0:31;
L64: C17 c= ( C17 \/ C18 ) by XBOOLE_1:7;
L65: C17 is_>=_than C20 by L64 , L60 , YELLOW_0:9;
L66: ( "/\" (C17 , C16) ) >= C20 by L65 , L44 , YELLOW_0:31;
thus L67: thesis by L66 , L63 , YELLOW_0:23;
end;
thus L68: thesis by L59 , L46 , YELLOW_0:31;
end;
