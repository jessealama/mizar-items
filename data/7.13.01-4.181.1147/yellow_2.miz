theorem
L1: (for B1 being non  empty RelStr holds (for B2 being (Element of B1) holds (for B3 being (Subset of B1) holds (B3 c= ( downarrow B2 ) iff B3 is_<=_than B2))))
proof
let C1 being non  empty RelStr;
let C2 being (Element of C1);
let C3 being (Subset of C1);
thus L2:now
assume L3: C3 c= ( downarrow C2 );
thus L4: C3 is_<=_than C2
proof
let C4 being (Element of C1);
assume L5: C4 in C3;
thus L6: thesis by L5 , L3 , WAYBEL_0:17;
end;

end;
assume L5: (for B4 being (Element of C1) holds (B4 in C3 implies B4 <= C2));
let C5 being set;
assume L6: C5 in C3;
reconsider D1 = C5 as (Element of C1) by L6;
L7: D1 <= C2 by L5 , L6;
thus L8: thesis by L7 , WAYBEL_0:17;
end;
