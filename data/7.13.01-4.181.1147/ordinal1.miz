theorem
L1: (for R1 being set holds (for R2 being set holds (R2 in R1 implies (not R1 c= R2))))
proof
let R1 being set;
let R2 being set;
assume L2: R2 in R1;
assume L3: R1 c= R2;
L4: R2 in R2 by L3 , L2;
thus L5: contradiction by L4;
end;
