theorem
L198: (for R1 being TopSpace holds ( ( PO R1 ) /\ ( D(alpha,p) R1 ) ) = ( R1 ^alpha ))
proof
let R1 being TopSpace;
thus L199: ( ( PO R1 ) /\ ( D(alpha,p) R1 ) ) c= ( R1 ^alpha )
proof
let C24 being set;
assume L200: C24 in ( ( PO R1 ) /\ ( D(alpha,p) R1 ) );
