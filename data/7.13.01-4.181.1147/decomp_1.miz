let C1 being TopStruct;
mode alpha-set of C1
 -> (Subset of C1)
means :L1: it c= ( Int ( Cl ( Int it ) ) );
existence
proof
take ( {} C1 );
thus L2: thesis by XBOOLE_1:2;
end;
let C2 being (Subset of C1);
attr C2 is  semi-open
means
:L3: C2 c= ( Cl ( Int C2 ) );
attr C2 is  pre-open
means
:L4: C2 c= ( Int ( Cl C2 ) );
attr C2 is  pre-semi-open
means
:L5: C2 c= ( Cl ( Int ( Cl C2 ) ) );
attr C2 is  semi-pre-open
means
:L6: C2 c= ( ( Cl ( Int C2 ) ) \/ ( Int ( Cl C2 ) ) );
end;
