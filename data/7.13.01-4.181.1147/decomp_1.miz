theorem
L41: (for R1 being TopSpace holds (for R2 being (Subset of R1) holds (( alphaInt R2 ) = ( pInt R2 ) iff ( sInt R2 ) = ( psInt R2 ))))
proof
let R1 being TopSpace;
let R2 being (Subset of R1);
thus L42:now
L43: ( Cl ( Int R2 ) ) c= ( Cl R2 ) by PRE_TOPC:19 , TOPS_1:16;
L44: ( Int ( Cl ( Int R2 ) ) ) c= ( Int ( Cl R2 ) ) by L43 , TOPS_1:19;
L45: ( Cl ( Int ( Cl ( Int R2 ) ) ) ) c= ( Cl ( Int ( Cl R2 ) ) ) by L44 , PRE_TOPC:19;
L46: ( Cl ( Int R2 ) ) c= ( Cl ( Int ( Cl R2 ) ) ) by L45 , TOPS_1:26;
L47: ( sInt R2 ) c= ( R2 /\ ( Cl ( Int ( Cl R2 ) ) ) ) by L46 , XBOOLE_1:26;
assume L48: ( alphaInt R2 ) = ( pInt R2 );
L49: ( Cl ( R2 /\ ( Int ( Cl R2 ) ) ) ) c= ( Cl ( Int ( Cl ( Int R2 ) ) ) ) by L48 , PRE_TOPC:19 , XBOOLE_1:17;
L50: ( Cl ( R2 /\ ( Int ( Cl R2 ) ) ) ) c= ( Cl ( Int R2 ) ) by L49 , TOPS_1:26;
L51: ( Cl ( R2 /\ ( Int ( Cl R2 ) ) ) ) = ( Cl ( ( Cl R2 ) /\ ( Int ( Cl R2 ) ) ) ) by TOPS_1:14;
L52: ( Cl ( R2 /\ ( Int ( Cl R2 ) ) ) ) = ( Cl ( Int ( Cl R2 ) ) ) by L51 , TOPS_1:16 , XBOOLE_1:28;
L53: ( R2 /\ ( Cl ( Int ( Cl R2 ) ) ) ) c= ( R2 /\ ( Cl ( Int R2 ) ) ) by L52 , L50 , XBOOLE_1:26;
thus L54: ( sInt R2 ) = ( psInt R2 ) by L53 , L47 , XBOOLE_0:def 10;
end;
L55: ( Int ( Cl R2 ) ) c= ( Cl ( Int ( Cl R2 ) ) ) by PRE_TOPC:18;
assume L56: ( psInt R2 ) = ( sInt R2 );
L57: ( psInt R2 ) c= ( Cl ( Int R2 ) ) by L56 , XBOOLE_1:17;
L58: ( R2 /\ ( Int ( Cl R2 ) ) ) c= ( R2 /\ ( Cl ( Int ( Cl R2 ) ) ) ) by PRE_TOPC:18 , XBOOLE_1:26;
L59: ( R2 /\ ( Int ( Cl R2 ) ) ) c= ( Cl ( Int R2 ) ) by L58 , L57 , XBOOLE_1:1;
L60: ( Cl ( R2 /\ ( Int ( Cl R2 ) ) ) ) c= ( Cl ( Cl ( Int R2 ) ) ) by L59 , PRE_TOPC:19;
L61: ( Cl ( R2 /\ ( Int ( Cl R2 ) ) ) ) = ( Cl ( ( Cl R2 ) /\ ( Int ( Cl R2 ) ) ) ) by TOPS_1:14;
L62: ( Cl ( Int ( Cl R2 ) ) ) c= ( Cl ( Int R2 ) ) by L61 , L60 , TOPS_1:16 , XBOOLE_1:28;
L63: ( Int ( Cl R2 ) ) c= ( Cl ( Int R2 ) ) by L62 , L55 , XBOOLE_1:1;
L64: ( Int ( Int ( Cl R2 ) ) ) c= ( Int ( Cl ( Int R2 ) ) ) by L63 , TOPS_1:19;
L65: ( R2 /\ ( Int ( Cl R2 ) ) ) c= ( R2 /\ ( Int ( Cl ( Int R2 ) ) ) ) by L64 , XBOOLE_1:26;
L66: ( Cl ( Int R2 ) ) c= ( Cl R2 ) by PRE_TOPC:19 , TOPS_1:16;
L67: ( Int ( Cl ( Int R2 ) ) ) c= ( Int ( Cl R2 ) ) by L66 , TOPS_1:19;
L68: ( alphaInt R2 ) c= ( R2 /\ ( Int ( Cl R2 ) ) ) by L67 , XBOOLE_1:26;
thus L69: thesis by L68 , L65 , XBOOLE_0:def 10;
end;
theorem
L70: (for R1 being TopSpace holds (for R2 being (Subset of R1) holds (R2 is (alpha-set of R1) iff R2 = ( alphaInt R2 ))))
proof
let R1 being TopSpace;
let R2 being (Subset of R1);
