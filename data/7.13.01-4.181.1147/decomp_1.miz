theorem
L112: (for R1 being TopSpace holds ( ( R1 ^alpha ) /\ ( D(c,alpha) R1 ) ) = (the topology of R1))
proof
let R1 being TopSpace;
thus L113: ( ( R1 ^alpha ) /\ ( D(c,alpha) R1 ) ) c= (the topology of R1)
proof
let C8 being set;
assume L114: C8 in ( ( R1 ^alpha ) /\ ( D(c,alpha) R1 ) );
L115: C8 in ( R1 ^alpha ) by L114 , XBOOLE_0:def 4;
consider C9 being (Subset of R1) such that L116: C8 = C9 and L117: C9 is (alpha-set of R1) by L115;
L118: C8 in ( D(c,alpha) R1 ) by L114 , XBOOLE_0:def 4;
consider C10 being (Subset of R1) such that L119: C8 = C10 and L120: ( Int C10 ) = ( alphaInt C10 ) by L118;
L121: C9 = ( alphaInt C9 ) by L117 , L70;
L122: C10 is  open by L121 , L116 , L119 , L120;
thus L123: thesis by L122 , L119 , PRE_TOPC:def 2;
end;

let C11 being set;
assume L124: C11 in (the topology of R1);
reconsider D1 = C11 as (Subset of R1) by L124;
L125: D1 is  open by L124 , PRE_TOPC:def 2;
L126: D1 = ( Int D1 ) by L125 , TOPS_1:23;
L127: D1 c= ( Int ( Cl ( Int D1 ) ) ) by L126 , PRE_TOPC:18 , TOPS_1:19;
L128: D1 is (alpha-set of R1) by L127 , L1;
L129: ( Int D1 ) = ( alphaInt D1 ) by L128 , L126 , L70;
L130: D1 in { R2 where R2 is (Subset of R1) : ( Int R2 ) = ( alphaInt R2 ) } by L129;
L131: D1 in ( R1 ^alpha ) by L128;
thus L132: thesis by L131 , L130 , XBOOLE_0:def 4;
end;
