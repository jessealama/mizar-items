begin
definition
let C1 being non  empty TopSpace;
let C2 being (Point of C1);
mode a_neighborhood of C2
 -> (Subset of C1)
means :L1: C2 in ( Int it );
existence
proof
take ( [#] C1 );
L2: ( Int ( [#] C1 ) ) = ( [#] C1 ) by TOPS_1:15;
thus L3: thesis by L2;
end;
end;
definition
let C3 being TopSpace;
let C4 being (Subset of C3);
mode a_neighborhood of C4
 -> (Subset of C3)
means :L5: C4 c= ( Int it );
existence
proof
take ( [#] C3 );
L6: ( Int ( [#] C3 ) ) = ( [#] C3 ) by TOPS_1:15;
thus L7: thesis by L6;
end;
end;
theorem
L9: (for R1 being non  empty TopSpace holds (for R2 being (Point of R1) holds (for B1 , B2 being (Subset of R1) holds ((B1 is (a_neighborhood of R2) & B2 is (a_neighborhood of R2)) implies ( B1 \/ B2 ) is (a_neighborhood of R2)))))
proof
let R1 being non  empty TopSpace;
let R2 being (Point of R1);
let C5 , C6 being (Subset of R1);
reconsider D1 = C5 , D2 = C6 as (Subset of R1);
L10: ((D1 is (a_neighborhood of R2) & D2 is (a_neighborhood of R2)) implies ( D1 \/ D2 ) is (a_neighborhood of R2))
proof
assume that
L11: R2 in ( Int D1 )
and
L12: R2 in ( Int D2 );
L13: ( ( Int D1 ) \/ ( Int D2 ) ) c= ( Int ( D1 \/ D2 ) ) by TOPS_1:20;
L14: R2 in ( ( Int D1 ) \/ ( Int D2 ) ) by L11 , XBOOLE_0:def 3;
thus L15: thesis by L14 , L13 , L1;
end;
thus L16: thesis by L10;
end;
theorem
L17: (for R1 being non  empty TopSpace holds (for R2 being (Point of R1) holds (for B3 , B4 being (Subset of R1) holds ((B3 is (a_neighborhood of R2) & B4 is (a_neighborhood of R2)) iff ( B3 /\ B4 ) is (a_neighborhood of R2)))))
proof
let R1 being non  empty TopSpace;
let R2 being (Point of R1);
let C7 , C8 being (Subset of R1);
L18: ((C7 is (a_neighborhood of R2) & C8 is (a_neighborhood of R2)) iff (R2 in ( Int C7 ) & R2 in ( Int C8 ))) by L1;
L19: ((C7 is (a_neighborhood of R2) & C8 is (a_neighborhood of R2)) iff R2 in ( ( Int C7 ) /\ ( Int C8 ) )) by L18 , XBOOLE_0:def 4;
L20: ((C7 is (a_neighborhood of R2) & C8 is (a_neighborhood of R2)) iff R2 in ( Int ( C7 /\ C8 ) )) by L19 , TOPS_1:17;
thus L21: thesis by L20 , L1;
end;
theorem
L22: (for R1 being non  empty TopSpace holds (for B5 being (Subset of R1) holds (for B6 being (Point of R1) holds ((B5 is  open & B6 in B5) implies B5 is (a_neighborhood of B6)))))
proof
let R1 being non  empty TopSpace;
let C9 being (Subset of R1);
let C10 being (Point of R1);
assume L23: (C9 is  open & C10 in C9);
L24: C10 in ( Int C9 ) by L23 , TOPS_1:23;
thus L25: thesis by L24 , L1;
end;
theorem
L26: (for R1 being non  empty TopSpace holds (for B7 being (Subset of R1) holds (for B8 being (Point of R1) holds (B7 is (a_neighborhood of B8) implies B8 in B7))))
proof
let R1 being non  empty TopSpace;
let C11 being (Subset of R1);
let C12 being (Point of R1);
assume L27: C11 is (a_neighborhood of C12);
L28: C12 in ( Int C11 ) by L27 , L1;
L29: ( Int C11 ) c= C11 by TOPS_1:16;
thus L30: thesis by L29 , L28;
end;
theorem
L31: (for R1 being non  empty TopSpace holds (for R2 being (Point of R1) holds (for R3 being (Subset of R1) holds (R3 is (a_neighborhood of R2) implies (ex B9 being non  empty (Subset of R1) st (B9 is (a_neighborhood of R2) & B9 is  open & B9 c= R3))))))
proof
let R1 being non  empty TopSpace;
let R2 being (Point of R1);
let R3 being (Subset of R1);
assume L32: R3 is (a_neighborhood of R2);
L33: R2 in ( Int R3 ) by L32 , L1;
consider C13 being (Subset of R1) such that L34: (C13 is  open & C13 c= R3) and L35: R2 in C13 by L33 , TOPS_1:22;
reconsider D3 = C13 as non  empty (Subset of R1) by L35;
take D3;
thus L36: thesis by L34 , L35 , L22;
end;
theorem
L37: (for R1 being non  empty TopSpace holds (for R2 being (Point of R1) holds (for R3 being (Subset of R1) holds (R3 is (a_neighborhood of R2) iff (ex B10 being (Subset of R1) st (B10 is  open & B10 c= R3 & R2 in B10))))))
proof
let R1 being non  empty TopSpace;
let R2 being (Point of R1);
let R3 being (Subset of R1);
L38:
now
assume L39: R3 is (a_neighborhood of R2);
consider C14 being non  empty (Subset of R1) such that L40: (C14 is (a_neighborhood of R2) & C14 is  open & C14 c= R3) by L39 , L31;
take D4 = C14;
thus L41: (ex B11 being (Subset of R1) st (B11 is  open & B11 c= R3 & R2 in B11)) by L40 , L26;
end;
L42:
now
given C15 being (Subset of R1) such that
L43: (C15 is  open & C15 c= R3 & R2 in C15);

L44: R2 in ( Int R3 ) by L43 , TOPS_1:22;
thus L45: R3 is (a_neighborhood of R2) by L44 , L1;
end;
thus L46: thesis by L42 , L38;
end;
theorem
L47: (for R1 being non  empty TopSpace holds (for B12 being (Subset of R1) holds (B12 is  open iff (for R2 being (Point of R1) holds (R2 in B12 implies (ex B13 being (Subset of R1) st (B13 is (a_neighborhood of R2) & B13 c= B12)))))))
proof
let R1 being non  empty TopSpace;
let C16 being (Subset of R1);
L48:
now
assume L49: (for R2 being (Point of R1) holds (R2 in C16 implies (ex B14 being (Subset of R1) st (B14 is (a_neighborhood of R2) & B14 c= C16))));
L50: (for B15 being set holds (B15 in C16 iff (ex B16 being (Subset of R1) st (B16 is  open & B16 c= C16 & B15 in B16))))
proof
let C17 being set;
thus L51: (C17 in C16 implies (ex B17 being (Subset of R1) st (B17 is  open & B17 c= C16 & C17 in B17)))
proof
assume L52: C17 in C16;
reconsider D5 = C17 as (Point of R1) by L52;
consider C18 being (Subset of R1) such that L53: C18 is (a_neighborhood of D5) and L54: C18 c= C16 by L49 , L52;
consider C19 being (Subset of R1) such that L55: (C19 is  open & C19 c= C18 & D5 in C19) by L53 , L37;
take C19;
thus L56: thesis by L54 , L55 , XBOOLE_1:1;
end;

given C20 being (Subset of R1) such that
L57: C20 is  open
and
L58: (C20 c= C16 & C17 in C20);

thus L59: thesis by L58;
end;
thus L60: C16 is  open by L50 , TOPS_1:25;
end;
thus L61: thesis by L48 , L22;
end;
theorem
L62: (for R1 being non  empty TopSpace holds (for R2 being (Point of R1) holds (for B18 being (Subset of R1) holds (B18 is (a_neighborhood of { R2 }) iff B18 is (a_neighborhood of R2)))))
proof
let R1 being non  empty TopSpace;
let R2 being (Point of R1);
let C21 being (Subset of R1);
thus L63: (C21 is (a_neighborhood of { R2 }) implies C21 is (a_neighborhood of R2))
proof
assume L64: C21 is (a_neighborhood of { R2 });
L65: { R2 } c= ( Int C21 ) by L64 , L5;
L66: R2 in { R2 } by TARSKI:def 1;
thus L67: thesis by L66 , L65 , L1;
end;

assume L68: C21 is (a_neighborhood of R2);
L69: R2 in ( Int C21 ) by L68 , L1;
L70: (for B19 being set holds (B19 in { R2 } implies B19 in ( Int C21 ))) by L69 , TARSKI:def 1;
L71: { R2 } c= ( Int C21 ) by L70 , TARSKI:def 3;
thus L72: thesis by L71 , L5;
end;
theorem
L73: (for R1 being non  empty TopSpace holds (for B20 being non  empty (Subset of R1) holds (for B21 being (Point of ( R1 | B20 )) holds (for B22 being (Subset of ( R1 | B20 )) holds (for B23 being (Subset of R1) holds (for B24 being (Point of R1) holds ((B20 is  open & B22 is (a_neighborhood of B21) & B22 = B23 & B21 = B24) implies B23 is (a_neighborhood of B24))))))))
proof
let R1 being non  empty TopSpace;
let C22 being non  empty (Subset of R1);
let C23 being (Point of ( R1 | C22 ));
let C24 being (Subset of ( R1 | C22 ));
let C25 being (Subset of R1);
let C26 being (Point of R1);
assume that
L74: C22 is  open
and
L75: C24 is (a_neighborhood of C23)
and
L76: (C24 = C25 & C23 = C26);
L77: C23 in ( Int C24 ) by L75 , L1;
consider C27 being (Subset of ( R1 | C22 )) such that L78: C27 is  open and L79: (C27 c= C24 & C23 in C27) by L77 , TOPS_1:22;
L80: C27 in (the topology of ( R1 | C22 )) by L78 , PRE_TOPC:def 2;
consider C28 being (Subset of R1) such that L81: C28 in (the topology of R1) and L82: C27 = ( C28 /\ ( [#] ( R1 | C22 ) ) ) by L80 , PRE_TOPC:def 4;
reconsider D6 = C28 as (Subset of R1);
L83: D6 is  open by L81 , PRE_TOPC:def 2;
L84: ( C28 /\ C22 ) is  open by L83 , L74;
L85: C27 = ( C28 /\ C22 ) by L82 , PRE_TOPC:def 5;
L86: C26 in ( Int C25 ) by L85 , L76 , L79 , L84 , TOPS_1:22;
thus L87: thesis by L86 , L1;
end;
L88: (for R1 being non  empty TopSpace holds (for B25 being (SubSpace of R1) holds (for B26 being (Subset of R1) holds (for B27 being (Subset of B25) holds (B26 = B27 implies ( ( Int B26 ) /\ ( [#] B25 ) ) c= ( Int B27 ))))))
proof
let R1 being non  empty TopSpace;
let C29 being (SubSpace of R1);
let C30 being (Subset of R1);
let C31 being (Subset of C29);
assume L89: C30 = C31;
L90: (for B28 being set holds (B28 in ( ( Int C30 ) /\ ( [#] C29 ) ) implies B28 in ( Int C31 )))
proof
let C32 being set;
assume L91: C32 in ( ( Int C30 ) /\ ( [#] C29 ) );
L92: C32 in ( Int C30 ) by L91 , XBOOLE_0:def 4;
consider C33 being (Subset of R1) such that L93: C33 is  open and L94: (C33 c= C30 & C32 in C33) by L92 , TOPS_1:22;
L95: (ex B29 being (Subset of C29) st (B29 is  open & B29 c= C31 & C32 in B29))
proof
reconsider D7 = ( C33 /\ ( [#] C29 ) ) as (Subset of C29);
take D7;
L96: C33 in (the topology of R1) by L93 , PRE_TOPC:def 2;
L97: (D7 c= C33 & D7 in (the topology of C29)) by L96 , PRE_TOPC:def 4 , XBOOLE_1:17;
thus L98: thesis by L97 , L89 , L91 , L94 , PRE_TOPC:def 2 , XBOOLE_0:def 4 , XBOOLE_1:1;
end;
thus L99: thesis by L95 , TOPS_1:22;
end;
thus L100: thesis by L90 , TARSKI:def 3;
end;
theorem
L101: (for R1 being non  empty TopSpace holds (for B30 being non  empty (Subset of R1) holds (for B31 being (Point of ( R1 | B30 )) holds (for B32 being (Subset of ( R1 | B30 )) holds (for B33 being (Subset of R1) holds (for B34 being (Point of R1) holds ((B33 is (a_neighborhood of B34) & B32 = B33 & B31 = B34) implies B32 is (a_neighborhood of B31))))))))
proof
let R1 being non  empty TopSpace;
let C34 being non  empty (Subset of R1);
let C35 being (Point of ( R1 | C34 ));
let C36 being (Subset of ( R1 | C34 ));
let C37 being (Subset of R1);
let C38 being (Point of R1);
assume that
L102: C37 is (a_neighborhood of C38)
and
L103: C36 = C37
and
L104: C35 = C38;
L105: C38 in ( Int C37 ) by L102 , L1;
L106: C38 in ( ( Int C37 ) /\ ( [#] ( R1 | C34 ) ) ) by L105 , L104 , XBOOLE_0:def 4;
L107: ( ( Int C37 ) /\ ( [#] ( R1 | C34 ) ) ) c= ( Int C36 ) by L103 , L88;
thus L108: thesis by L107 , L104 , L106 , L1;
end;
theorem
L109: (for R1 being non  empty TopSpace holds (for B35 being (Subset of R1) holds (for B36 being (Subset of R1) holds ((B35 is  a_component & B35 c= B36) implies B35 is_a_component_of B36))))
proof
let R1 being non  empty TopSpace;
let C39 being (Subset of R1);
let C40 being (Subset of R1);
assume that
L110: C39 is  a_component
and
L111: C39 c= C40;
L112: C39 is  connected by L110;
L113: (ex B37 being (Subset of ( R1 | C40 )) st (C39 = B37 & B37 is  a_component))
proof
L114: C40 = ( [#] ( R1 | C40 ) ) by PRE_TOPC:def 5;
reconsider D8 = C39 as (Subset of ( R1 | C40 )) by L114 , L111;
take D9 = D8;
L115: (for B38 being (Subset of ( R1 | C40 )) holds (B38 is  connected implies (D9 c= B38 implies D9 = B38)))
proof
let C41 being (Subset of ( R1 | C40 ));
assume that
L116: C41 is  connected;
reconsider D10 = C41 as (Subset of R1) by PRE_TOPC:11;
assume L117: D9 c= C41;
L118: D10 is  connected by L116 , CONNSP_1:23;
thus L119: thesis by L118 , L110 , L117 , CONNSP_1:def 5;
end;
L120: D9 is  connected by L112 , CONNSP_1:23;
thus L121: thesis by L120 , L115 , CONNSP_1:def 5;
end;
thus L122: thesis by L113 , CONNSP_1:def 6;
end;
theorem
L123: (for R1 being non  empty TopSpace holds (for B39 being non  empty (SubSpace of R1) holds (for B40 being (Point of R1) holds (for B41 being (Point of B39) holds (B40 = B41 implies ( Component_of B41 ) c= ( Component_of B40 ))))))
proof
let R1 being non  empty TopSpace;
let C42 being non  empty (SubSpace of R1);
let C43 being (Point of R1);
let C44 being (Point of C42);
consider C45 being (Subset-Family of R1) such that L124: (for B42 being (Subset of R1) holds (B42 in C45 iff (B42 is  connected & C43 in B42))) and L125: ( union C45 ) = ( Component_of C43 ) by CONNSP_1:def 7;
reconsider D11 = ( Component_of C44 ) as (Subset of R1) by PRE_TOPC:11;
L126: (C44 in D11 & D11 is  connected) by CONNSP_1:23 , CONNSP_1:38;
assume L127: C43 = C44;
L128: D11 in C45 by L127 , L124 , L126;
thus L129: thesis by L128 , L125 , ZFMISC_1:74;
end;
definition
let C46 being non  empty TopSpace;
let C47 being (Point of C46);
pred C46 is_locally_connected_in C47
means
:L130: (for B43 being (Subset of C46) holds (B43 is (a_neighborhood of C47) implies (ex B44 being (Subset of C46) st (B44 is (a_neighborhood of C47) & B44 is  connected & B44 c= B43))))
;end;
definition
let C48 being non  empty TopSpace;
attr C48 is  locally_connected
means
:L132: (for B45 being (Point of C48) holds C48 is_locally_connected_in B45);
end;
definition
let C49 being non  empty TopSpace;
let C50 being (Subset of C49);
let C51 being (Point of C49);
pred C50 is_locally_connected_in C51
means
:L134: (for B46 being non  empty (Subset of C49) holds (C50 = B46 implies (ex B47 being (Point of ( C49 | B46 )) st (B47 = C51 & ( C49 | B46 ) is_locally_connected_in B47))))
;end;
definition
let C52 being non  empty TopSpace;
let C53 being non  empty (Subset of C52);
attr C53 is  locally_connected
means
:L136: ( C52 | C53 ) is  locally_connected;
end;
theorem
L138: (for R1 being non  empty TopSpace holds (for R2 being (Point of R1) holds (R1 is_locally_connected_in R2 iff (for B48 , B49 being (Subset of R1) holds ((B48 is (a_neighborhood of R2) & B49 is_a_component_of B48 & R2 in B49) implies B49 is (a_neighborhood of R2))))))
proof
let R1 being non  empty TopSpace;
let R2 being (Point of R1);
thus L139: (R1 is_locally_connected_in R2 implies (for B50 , B51 being (Subset of R1) holds ((B50 is (a_neighborhood of R2) & B51 is_a_component_of B50 & R2 in B51) implies B51 is (a_neighborhood of R2))))
proof
assume L140: R1 is_locally_connected_in R2;
let C54 , C55 being (Subset of R1);
assume that
L141: C54 is (a_neighborhood of R2)
and
L142: C55 is_a_component_of C54
and
L143: R2 in C55;
reconsider D12 = C54 as non  empty (Subset of R1) by L141 , L26;
consider C56 being (Subset of ( R1 | C54 )) such that L144: C56 = C55 and L145: C56 is  a_component by L142 , CONNSP_1:def 6;
consider C57 being (Subset of R1) such that L146: C57 is (a_neighborhood of R2) and L147: C57 is  connected and L148: C57 c= C54 by L140 , L141 , L130;
L149: C57 c= ( [#] ( R1 | C54 ) ) by L148 , PRE_TOPC:def 5;
reconsider D13 = C57 as (Subset of ( R1 | C54 )) by L149;
L150: R2 in ( Int C57 ) by L146 , L1;
L151: D13 is  connected by L147 , CONNSP_1:23;
L152: (D13 misses C56 or D13 c= C56) by L151 , L145 , CONNSP_1:36;
L153: (( D13 /\ C56 ) = ( {} ( R1 | D12 ) ) or D13 c= C56) by L152 , XBOOLE_0:def 7;
L154: R2 in D13 by L146 , L26;
L155: ( Int C57 ) c= ( Int C55 ) by L154 , L143 , L144 , L153 , TOPS_1:19 , XBOOLE_0:def 4;
thus L156: thesis by L155 , L150 , L1;
end;

assume L157: (for B52 , B53 being (Subset of R1) holds ((B52 is (a_neighborhood of R2) & B53 is_a_component_of B52 & R2 in B53) implies B53 is (a_neighborhood of R2)));
L158: (for B54 being (Subset of R1) holds (B54 is (a_neighborhood of R2) implies (ex B55 being (Subset of R1) st (B55 is (a_neighborhood of R2) & B55 is  connected & B55 c= B54))))
proof
let C58 being (Subset of R1);
L159: ( [#] ( R1 | C58 ) ) = C58 by PRE_TOPC:def 5;
assume L160: C58 is (a_neighborhood of R2);
L161: R2 in C58 by L160 , L26;
reconsider D14 = C58 as non  empty (Subset of R1) by L160 , L26;
L162: R2 in ( [#] ( R1 | D14 ) ) by L161 , PRE_TOPC:def 5;
reconsider D15 = R2 as (Point of ( R1 | D14 )) by L162;
set D16 = ( Component_of D15 );
reconsider D17 = D16 as (Subset of R1) by PRE_TOPC:11;
take D17;
L163: D16 is  a_component by CONNSP_1:40;
L164: D17 is_a_component_of D14 by L163 , CONNSP_1:def 6;
L165: (R2 in D17 & D16 is  connected) by CONNSP_1:38;
thus L166: thesis by L165 , L157 , L160 , L159 , L164 , CONNSP_1:23;
end;
thus L167: thesis by L158 , L130;
end;
theorem
L168: (for R1 being non  empty TopSpace holds (for R2 being (Point of R1) holds (R1 is_locally_connected_in R2 iff (for B56 being non  empty (Subset of R1) holds ((B56 is  open & R2 in B56) implies (ex B57 being (Point of ( R1 | B56 )) st (B57 = R2 & R2 in ( Int ( Component_of B57 ) ))))))))
proof
let R1 being non  empty TopSpace;
let R2 being (Point of R1);
L169: (R1 is_locally_connected_in R2 implies (for B58 being non  empty (Subset of R1) holds ((B58 is  open & R2 in B58) implies (ex B59 being (Point of ( R1 | B58 )) st (B59 = R2 & R2 in ( Int ( Component_of B59 ) ))))))
proof
assume L170: R1 is_locally_connected_in R2;
let C59 being non  empty (Subset of R1);
assume that
L171: C59 is  open
and
L172: R2 in C59;
L173: R2 in ( [#] ( R1 | C59 ) ) by L172 , PRE_TOPC:def 5;
reconsider D18 = R2 as (Point of ( R1 | C59 )) by L173;
reconsider D19 = ( Component_of D18 ) as (Subset of ( R1 | C59 ));
take D18;
reconsider D20 = D19 as (Subset of R1) by PRE_TOPC:11;
L174: R2 in D20 by CONNSP_1:38;
L175: D19 is  a_component by CONNSP_1:40;
L176: D20 is_a_component_of C59 by L175 , CONNSP_1:def 6;
L177: C59 is (a_neighborhood of R2) by L171 , L172 , L22;
L178: D20 is (a_neighborhood of R2) by L177 , L170 , L176 , L174 , L138;
L179: D19 is (a_neighborhood of D18) by L178 , L101;
thus L180: thesis by L179 , L1;
end;
L181: ((for B60 being non  empty (Subset of R1) holds ((B60 is  open & R2 in B60) implies (ex B61 being (Point of ( R1 | B60 )) st (B61 = R2 & R2 in ( Int ( Component_of B61 ) ))))) implies R1 is_locally_connected_in R2)
proof
assume L182: (for B62 being non  empty (Subset of R1) holds ((B62 is  open & R2 in B62) implies (ex B63 being (Point of ( R1 | B62 )) st (B63 = R2 & R2 in ( Int ( Component_of B63 ) )))));
