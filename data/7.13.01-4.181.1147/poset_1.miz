L1: (for B1 being Function holds (( rng B1 ) c= ( dom B1 ) implies ( iter (B1 , ( 0 )) ) = ( id ( dom B1 ) )))
proof
let C1 being Function;
assume L2: ( rng C1 ) c= ( dom C1 );
L3: ( field C1 ) = ( dom C1 ) by L2 , XBOOLE_1:12;
thus L4: thesis by L3 , FUNCT_7:68;
end;
L5: (for B2 being Function holds (for B3 being Nat holds (( rng B2 ) c= ( dom B2 ) implies (( dom ( iter (B2 , B3) ) ) = ( dom B2 ) & ( rng ( iter (B2 , B3) ) ) c= ( dom B2 )))))
proof
let C2 being Function;
let C3 being Nat;
defpred S1[ Nat ] means (( dom ( iter (C2 , $1) ) ) = ( dom C2 ) & ( rng ( iter (C2 , $1) ) ) c= ( dom C2 ));
assume L6: ( rng C2 ) c= ( dom C2 );
L7: ( iter (C2 , ( 0 )) ) = ( id ( dom C2 ) ) by L6 , L1;
L8: S1[ ( 0 ) ] by L7 , RELAT_1:45;
L9: (for B4 being Nat holds (S1[ B4 ] implies S1[ ( B4 + 1 ) ]))
proof
let C4 being Nat;
assume L10: (( dom ( iter (C2 , C4) ) ) = ( dom C2 ) & ( rng ( iter (C2 , C4) ) ) c= ( dom C2 ));
L11: (( iter (C2 , ( C4 + 1 )) ) = ( C2 * ( iter (C2 , C4) ) ) & ( iter (C2 , ( C4 + 1 )) ) = ( ( iter (C2 , C4) ) * C2 )) by FUNCT_7:69 , FUNCT_7:71;
L12: (( dom ( iter (C2 , ( C4 + 1 )) ) ) = ( dom ( iter (C2 , C4) ) ) & ( rng ( iter (C2 , ( C4 + 1 )) ) ) c= ( rng ( iter (C2 , C4) ) )) by L11 , L10 , RELAT_1:26 , RELAT_1:27;
thus L13: thesis by L12 , L10 , XBOOLE_1:1;
end;
L14: (for B5 being Nat holds S1[ B5 ]) from NAT_1:sch 2(L8 , L9);
thus L15: thesis by L14;
end;
L16: (for B6 being set holds (for B7 being (Function of B6 , B6) holds (for B8 being Nat holds ( iter (B7 , B8) ) is (Function of B6 , B6))))
proof
let C5 being set;
let C6 being (Function of C5 , C5);
let C7 being Nat;
L17: (C5 = ( {} ) implies C5 = ( {} ));
L18: (( dom C6 ) = C5 & ( rng C6 ) c= C5) by L17 , FUNCT_2:def 1;
L19: (( dom ( iter (C6 , C7) ) ) = C5 & ( rng ( iter (C6 , C7) ) ) c= C5) by L18 , L5;
reconsider D1 = ( iter (C6 , C7) ) as (Relation of C5 , C5) by L19 , RELSET_1:4;
L20: (( dom D1 ) = C5 & ( rng D1 ) c= C5) by L18 , L5;
thus L21: thesis by L20 , L17 , FUNCT_2:def 1;
end;
