definition
let C1 , C2 , C3 being non  empty set;
let C4 being (PartFunc of [: C1 , C2 :] , C3);
redefine func ~ C4 -> (PartFunc of [: C2 , C1 :] , C3);

coherence by FUNCT_4:48;
end;
definition
let R2 being Category;
func R2 opp ->  strict non  empty non  void CatStr equals 
CatStr (# (the carrier of R2) , (the carrier' of R2) , (the Target of R2) , (the Source of R2) , ( ~ (the Comp of R2) ) #);
coherence;
end;
definition
let R2 being Category;
let C5 being (Object of R2);
func C5 opp -> (Object of ( R2 opp )) equals 
C5;
coherence;
end;
registration
let R2 being Category;
cluster ( R2 opp ) ->  Category-like  transitive  associative  reflexive  with_identities;
coherence
proof
set D1 = (the carrier' of R2);
set D2 = (the Target of R2);
set D3 = (the Source of R2);
set D4 = ( ~ (the Comp of R2) );
set D5 = ( R2 opp );
thus L4: D5 is  Category-like
proof
let C6 , C7 being (Morphism of D5);
reconsider D6 = C6 , D7 = C7 as (Morphism of R2);
thus L5: ([ C7 , C6 ] in ( dom (the Comp of D5) ) implies ( dom C7 ) = ( cod C6 ))
proof
assume L6: [ C7 , C6 ] in ( dom (the Comp of D5) );
L7: [ D6 , D7 ] in ( dom (the Comp of R2) ) by L6 , FUNCT_4:42;
L8: ( dom D6 ) = ( cod D7 ) by L7 , CAT_1:def 6;
thus L9: ( dom C7 ) = ( cod C6 ) by L8;
end;

assume L10: ( dom C7 ) = ( cod C6 );
L11: ( cod D7 ) = ( dom D6 ) by L10;
L12: [ D6 , D7 ] in ( dom (the Comp of R2) ) by L11 , CAT_1:def 6;
thus L13: [ C7 , C6 ] in ( dom (the Comp of D5) ) by L12 , FUNCT_4:42;
end;

L14: (for B1 , B2 being (Element of D1) holds (( D2 . B2 ) = ( D3 . B1 ) implies ( D4 . (B2 , B1) ) = ( (the Comp of R2) . (B1 , B2) )))
proof
let C8 , C9 being (Element of D1);
reconsider D8 = C8 , D9 = C9 as (Morphism of D5);
assume L15: ( D2 . C9 ) = ( D3 . C8 );
L16: ( dom D9 ) = ( cod D8 ) by L15;
L17: [ D9 , D8 ] in ( dom D4 ) by L16 , L4 , CAT_1:def 6;
thus L18: thesis by L17 , FUNCT_4:43;
end;
thus L19: D5 is  transitive
proof
let C10 , C11 being (Morphism of D5);
reconsider D10 = C10 , D11 = C11 as (Morphism of R2);
assume L20: ( dom C11 ) = ( cod C10 );
L21: ( cod D11 ) = ( dom D10 ) by L20;
L22: [ D10 , D11 ] in ( dom (the Comp of R2) ) by L21 , CAT_1:def 6;
L23: [ C11 , C10 ] in ( dom (the Comp of D5) ) by L20 , L4 , CAT_1:def 6;
L24: ( C11 (*) C10 ) = ( D4 . (D11 , D10) ) by L23 , CAT_1:def 1
.= ( (the Comp of R2) . (D10 , D11) ) by L14 , L20
.= ( D10 (*) D11 ) by L22 , CAT_1:def 1;
thus L25: ( dom ( C11 (*) C10 ) ) = ( cod ( D10 (*) D11 ) ) by L24
.= ( cod D10 ) by L21 , CAT_1:def 7
.= ( dom C10 );
thus L26: ( cod ( C11 (*) C10 ) ) = ( dom ( D10 (*) D11 ) ) by L24
.= ( dom D11 ) by L21 , CAT_1:def 7
.= ( cod C11 );
end;

thus L27: D5 is  associative
proof
let C12 , C13 , C14 being (Morphism of D5);
reconsider D12 = C12 , D13 = C13 , D14 = C14 as (Morphism of R2);
assume that
L28: ( dom C14 ) = ( cod C13 )
and
L29: ( dom C13 ) = ( cod C12 );
L30: [ D14 , D13 ] in ( dom D4 ) by L4 , CAT_1:def 6 , L28;
L31: ( D4 . (D14 , D13) ) is (Element of D1) by L30 , PARTFUN1:4;
L32: [ D13 , D12 ] in ( dom D4 ) by L4 , CAT_1:def 6 , L29;
L33: ( D4 . (D13 , D12) ) is (Element of D1) by L32 , PARTFUN1:4;
L34: ( D4 . (D14 , D13) ) = ( (the Comp of R2) . (D13 , D14) ) by L14 , L28;
L35: ( D2 . ( D4 . (D14 , D13) ) ) = ( dom ( C14 (*) C13 ) ) by L30 , CAT_1:def 1
.= ( dom C13 ) by L19 , CAT_1:def 7 , L28;
L36: ( D4 . (( D4 . (D14 , D13) ) , D12) ) = ( (the Comp of R2) . (D12 , ( (the Comp of R2) . (D13 , D14) )) ) by L35 , L14 , L29 , L31 , L34;
L37: (( cod D14 ) = ( dom D13 ) & ( cod D13 ) = ( dom D12 )) by L28 , L29;
L38: ( D4 . (D13 , D12) ) = ( (the Comp of R2) . (D12 , D13) ) by L14 , L29;
L39: ( D3 . ( D4 . (D13 , D12) ) ) = ( cod ( C13 (*) C12 ) ) by L32 , CAT_1:def 1
.= ( cod C13 ) by L19 , CAT_1:def 7 , L29;
L40: ( dom ( C14 (*) C13 ) ) = ( dom C13 ) by L19 , CAT_1:def 7 , L28;
L41: [ ( C14 (*) C13 ) , C12 ] in ( dom (the Comp of D5) ) by L40 , L4 , CAT_1:def 6 , L29;
L42: ( cod ( C13 (*) C12 ) ) = ( cod C13 ) by L19 , CAT_1:def 7 , L29;
L43: [ C14 , ( C13 (*) C12 ) ] in ( dom (the Comp of D5) ) by L42 , L4 , CAT_1:def 6 , L28;
L44: [ C14 , C13 ] in ( dom (the Comp of D5) ) by L28 , L4 , CAT_1:def 6;
L45: ( C14 (*) C13 ) = ( D4 . (D14 , D13) ) by L44 , CAT_1:def 1;
L46: ( D12 (*) D13 ) = ( (the Comp of R2) . (D12 , D13) ) by L37 , CAT_1:16;
L47: ( dom ( D12 (*) D13 ) ) = ( dom D13 ) by L37 , CAT_1:def 7;
L48: ( D13 (*) D14 ) = ( (the Comp of R2) . (D13 , D14) ) by L37 , CAT_1:16;
L49: ( cod ( D13 (*) D14 ) ) = ( cod D13 ) by L37 , CAT_1:def 7;
L50: [ C13 , C12 ] in ( dom (the Comp of D5) ) by L29 , L4 , CAT_1:def 6;
L51: ( C13 (*) C12 ) = ( D4 . (D13 , D12) ) by L50 , CAT_1:def 1;
thus L52: ( C14 (*) ( C13 (*) C12 ) ) = ( D4 . (D14 , ( D4 . (D13 , D12) )) ) by L51 , L43 , CAT_1:def 1
.= ( (the Comp of R2) . (( (the Comp of R2) . (D12 , D13) ) , D14) ) by L14 , L28 , L33 , L38 , L39
.= ( ( D12 (*) D13 ) (*) D14 ) by L46 , L37 , L47 , CAT_1:16
.= ( D12 (*) ( D13 (*) D14 ) ) by L37 , CAT_1:def 8
.= ( D4 . (( D4 . (D14 , D13) ) , D12) ) by L36 , L37 , L48 , L49 , CAT_1:16
.= ( ( C14 (*) C13 ) (*) C12 ) by L41 , L45 , CAT_1:def 1;
end;

thus L53: D5 is  reflexive
proof
let C15 being (Object of D5);
reconsider D15 = C15 as (Element of R2);
consider C16 being (Morphism of R2) such that L54: C16 in ( Hom (D15 , D15) ) by SUBSET_1:4;
reconsider D16 = C16 as (Morphism of D5);
L55: ( dom D16 ) = ( cod C16 )
.= C15 by L54 , CAT_1:1;
L56: ( cod D16 ) = ( dom C16 )
.= C15 by L54 , CAT_1:1;
L57: D16 in ( Hom (C15 , C15) ) by L56 , L55;
thus L58: ( Hom (C15 , C15) ) <> ( {} ) by L57;
end;

let C17 being (Element of D5);
reconsider D17 = C17 as (Element of R2);
reconsider D18 = ( id D17 ) as (Morphism of D5);
L59: ( dom D18 ) = ( cod ( id D17 ) )
.= D17;
L60: ( cod D18 ) = ( dom ( id D17 ) )
.= D17;
reconsider D19 = D18 as (Morphism of C17 , C17) by L60 , L59 , CAT_1:4;
take D19;
let C18 being (Element of D5);
reconsider D20 = C18 as (Element of R2);
thus L61: (( Hom (C17 , C18) ) <> ( {} ) implies (for B3 being (Morphism of C17 , C18) holds ( B3 (*) D19 ) = B3))
proof
assume L62: ( Hom (C17 , C18) ) <> ( {} );
let C19 being (Morphism of C17 , C18);
reconsider D21 = C19 as (Morphism of R2);
L63: ( dom D21 ) = ( cod C19 )
.= D20 by L62 , CAT_1:5;
L64: ( cod D21 ) = ( dom C19 )
.= D17 by L62 , CAT_1:5;
L65: ( cod D21 ) = ( dom ( id D17 ) ) by L64;
reconsider D22 = D21 as (Morphism of D20 , D17) by L63 , L64 , CAT_1:4;
L66: ( D3 . D19 ) = D17 by L60
.= ( dom C19 ) by L62 , CAT_1:5
.= ( D2 . C19 );
L67: ( dom C19 ) = ( cod D19 ) by L66;
L68: [ C19 , D19 ] in ( dom (the Comp of D5) ) by L67 , L4 , CAT_1:def 6;
thus L69: ( C19 (*) D19 ) = ( D4 . (C19 , D19) ) by L68 , CAT_1:def 1
.= ( (the Comp of R2) . (D19 , C19) ) by L66 , L14
.= ( ( id D17 ) (*) D22 ) by L65 , CAT_1:16
.= C19 by L64 , CAT_1:21;
end;

assume L70: ( Hom (C18 , C17) ) <> ( {} );
let C20 being (Morphism of C18 , C17);
reconsider D23 = C20 as (Morphism of R2);
L71: ( cod D23 ) = ( dom C20 )
.= D20 by L70 , CAT_1:5;
L72: ( dom D23 ) = ( cod C20 )
.= D17 by L70 , CAT_1:5;
L73: ( dom D23 ) = ( cod ( id D17 ) ) by L72;
reconsider D24 = D23 as (Morphism of D17 , D20) by L71 , L72 , CAT_1:4;
L74: ( D2 . D19 ) = D17 by L59
.= ( cod C20 ) by L70 , CAT_1:5
.= ( D3 . C20 );
L75: ( cod C20 ) = ( dom D19 ) by L74;
L76: [ D19 , C20 ] in ( dom (the Comp of D5) ) by L75 , L4 , CAT_1:def 6;
thus L77: ( D19 (*) C20 ) = ( D4 . (D19 , C20) ) by L76 , CAT_1:def 1
.= ( (the Comp of R2) . (C20 , D19) ) by L74 , L14
.= ( D24 (*) ( id D17 ) ) by L73 , CAT_1:16
.= C20 by L72 , CAT_1:22;
end;
end;
definition
let R2 being Category;
let C21 being (Object of ( R2 opp ));
func opp C21 -> (Object of R2) equals 
( C21 opp );
