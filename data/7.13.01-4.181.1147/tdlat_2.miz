theorem
L1: (for R1 being TopSpace holds (for B1 being (Subset of R1) holds (( Int ( Cl ( Int B1 ) ) ) c= ( Int ( Cl B1 ) ) & ( Int ( Cl ( Int B1 ) ) ) c= ( Cl ( Int B1 ) ))))
proof
let R1 being TopSpace;
let C1 being (Subset of R1);
L2: ( Cl ( Int C1 ) ) c= ( Cl C1 ) by PRE_TOPC:19 , TOPS_1:16;
thus L3: ( Int ( Cl ( Int C1 ) ) ) c= ( Int ( Cl C1 ) ) by L2 , TOPS_1:19;
thus L4: thesis by TOPS_1:16;
end;
theorem
L5: (for R1 being TopSpace holds (for B2 being (Subset of R1) holds (( Cl ( Int B2 ) ) c= ( Cl ( Int ( Cl B2 ) ) ) & ( Int ( Cl B2 ) ) c= ( Cl ( Int ( Cl B2 ) ) ))))
proof
let R1 being TopSpace;
let C2 being (Subset of R1);
L6: ( Int C2 ) c= ( Int ( Cl C2 ) ) by PRE_TOPC:18 , TOPS_1:19;
thus L7: ( Cl ( Int C2 ) ) c= ( Cl ( Int ( Cl C2 ) ) ) by L6 , PRE_TOPC:19;
thus L8: thesis by PRE_TOPC:18;
end;
theorem
L9: (for R1 being TopSpace holds (for B3 , B4 being (Subset of R1) holds (B4 is  closed implies (( Cl ( Int ( B3 /\ B4 ) ) ) = B3 implies B3 c= B4))))
proof
let R1 being TopSpace;
let C3 , C4 being (Subset of R1);
assume L10: C4 is  closed;
L11: ( C3 /\ C4 ) c= C4 by XBOOLE_1:17;
L12: ( Int ( C3 /\ C4 ) ) c= ( C3 /\ C4 ) by TOPS_1:16;
L13: ( Int ( C3 /\ C4 ) ) c= C4 by L12 , L11 , XBOOLE_1:1;
L14: ( Cl ( Int ( C3 /\ C4 ) ) ) c= ( Cl C4 ) by L13 , PRE_TOPC:19;
assume L15: ( Cl ( Int ( C3 /\ C4 ) ) ) = C3;
thus L16: thesis by L15 , L10 , L14 , PRE_TOPC:22;
end;
theorem
L17: (for R1 being TopSpace holds (for B5 , B6 being (Subset of R1) holds (B5 is  open implies (( Int ( Cl ( B5 \/ B6 ) ) ) = B6 implies B5 c= B6))))
proof
let R1 being TopSpace;
let C5 , C6 being (Subset of R1);
assume L18: C5 is  open;
L19: C5 c= ( C5 \/ C6 ) by XBOOLE_1:7;
L20: ( C5 \/ C6 ) c= ( Cl ( C5 \/ C6 ) ) by PRE_TOPC:18;
L21: C5 c= ( Cl ( C5 \/ C6 ) ) by L20 , L19 , XBOOLE_1:1;
L22: ( Int C5 ) c= ( Int ( Cl ( C5 \/ C6 ) ) ) by L21 , TOPS_1:19;
assume L23: ( Int ( Cl ( C5 \/ C6 ) ) ) = C6;
thus L24: thesis by L23 , L18 , L22 , TOPS_1:23;
end;
theorem
L25: (for R1 being TopSpace holds (for B7 being (Subset of R1) holds (B7 c= ( Cl ( Int B7 ) ) implies ( B7 \/ ( Int ( Cl B7 ) ) ) c= ( Cl ( Int ( B7 \/ ( Int ( Cl B7 ) ) ) ) ))))
proof
