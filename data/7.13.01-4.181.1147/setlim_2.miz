L60: R4 in ( Union R8 ) by L59 , PROB_1:12;
thus L61: thesis by L60 , XBOOLE_0:def 3;
end;
end;

let R4 being set;
assume L63: R4 in ( ( Union R7 ) \/ ( Union R8 ) );
