theorem
L1: (for R1 being (Element of ( NAT )) holds (for R5 being set holds (for R7 being (SetSequence of R5) holds ( ( inferior_setsequence R7 ) . R1 ) = ( Intersection ( R7 ^\ R1 ) ))))
proof
let R1 being (Element of ( NAT ));
let R5 being set;
let R7 being (SetSequence of R5);
reconsider D1 = { ( R7 . R3 ) where R3 is (Element of ( NAT )) : R1 <= R3 } as (Subset-Family of R5) by SETLIM_1:28;
L2: ( ( inferior_setsequence R7 ) . R1 ) = ( meet D1 ) by SETLIM_1:def 2
.= ( meet ( rng ( R7 ^\ R1 ) ) ) by SETLIM_1:6;
thus L3: thesis by L2 , SETLIM_1:8;
end;
theorem
L4: (for R1 being (Element of ( NAT )) holds (for R5 being set holds (for R7 being (SetSequence of R5) holds ( ( superior_setsequence R7 ) . R1 ) = ( Union ( R7 ^\ R1 ) ))))
proof
let R1 being (Element of ( NAT ));
let R5 being set;
let R7 being (SetSequence of R5);
reconsider D2 = { ( R7 . R3 ) where R3 is (Element of ( NAT )) : R1 <= R3 } as (Subset-Family of R5) by SETLIM_1:28;
L5: ( ( superior_setsequence R7 ) . R1 ) = ( union D2 ) by SETLIM_1:def 3
.= ( union ( rng ( R7 ^\ R1 ) ) ) by SETLIM_1:6;
thus L6: thesis by L5 , CARD_3:def 4;
end;
