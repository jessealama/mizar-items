theorem
L1: (for R12 being non  empty  closed_interval (Subset of ( REAL )) holds (for B1 being (Division of R12) holds (( vol R12 ) = ( 0 ) implies ( len B1 ) = 1)))
proof
let R12 being non  empty  closed_interval (Subset of ( REAL ));
let C1 being (Division of R12);
assume that
L2: ( vol R12 ) = ( 0 )
and
L3: ( len C1 ) <> 1;
L4: ( rng C1 ) <> ( {} );
L5: 1 in ( dom C1 ) by L4 , FINSEQ_3:32;
L6: 1 <= ( len C1 ) by L5 , FINSEQ_3:25;
L7: ( len C1 ) in ( dom C1 ) by L6 , FINSEQ_3:25;
L8: ( C1 . 1 ) in R12 by L5 , INTEGRA1:6;
L9: ( lower_bound R12 ) <= ( C1 . 1 ) by L8 , INTEGRA2:1;
L10: 1 < ( len C1 ) by L3 , L6 , XXREAL_0:1;
L11: ( C1 . 1 ) < ( C1 . ( len C1 ) ) by L10 , L5 , L7 , SEQM_3:def 1;
L12: ( ( upper_bound R12 ) - ( lower_bound R12 ) ) = ( 0 ) by L2 , INTEGRA1:def 5;
thus L13: contradiction by L12 , L11 , L9 , INTEGRA1:def 2;
end;
theorem
L14: (for R12 being non  empty  closed_interval (Subset of ( REAL )) holds (( chi (R12 , R12) ) is  integrable & ( integral ( chi (R12 , R12) ) ) = ( vol R12 )))
proof
let R12 being non  empty  closed_interval (Subset of ( REAL ));
L15: ( ( divs R12 ) /\ ( dom ( upper_sum_set ( chi (R12 , R12) ) ) ) ) = ( ( divs R12 ) /\ ( divs R12 ) ) by FUNCT_2:def 1;
L16: ( divs R12 ) meets ( dom ( upper_sum_set ( chi (R12 , R12) ) ) ) by L15 , XBOOLE_0:def 7;
