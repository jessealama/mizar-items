theorem
L181: (for B33 being  anti-discrete non  empty TopStruct holds (for B34 being (Point of B33) holds { (the carrier of B33) } is (Basis of B34)))
proof
let C70 being  anti-discrete non  empty TopStruct;
let C71 being (Point of C70);
set D30 = { (the carrier of C70) };
L182: D30 c= ( bool (the carrier of C70) )
proof
let C72 being set;
assume L183: C72 in D30;
L184: C72 = (the carrier of C70) by L183 , TARSKI:def 1;
L185: (the carrier of C70) c= (the carrier of C70);
thus L186: thesis by L185 , L184;
end;
reconsider D31 = D30 as (Subset-Family of C70) by L182;
L187: D31 is (Basis of C71)
proof
L188: D31 is  open
proof
let C73 being (Subset of C70);
assume L189: C73 in D31;
L190: C73 = ( [#] C70 ) by L189 , TARSKI:def 1;
thus L191: thesis by L190;
end;
L192: D31 is C71 -quasi_basis
proof
L193: ( Intersect D31 ) = ( meet D31 ) by SETFAM_1:def 9
.= (the carrier of C70) by SETFAM_1:10;
thus L194: C71 in ( Intersect D31 ) by L193;
let C74 being (Subset of C70);
assume L195: (C74 is  open & C71 in C74);
L196: C74 = (the carrier of C70) by L195 , TDLAT_3:18;
take C74;
thus L197: thesis by L196 , TARSKI:def 1;
end;
thus L198: thesis by L192 , L188;
end;
thus L199: thesis by L187;
end;
theorem
L200: (for B35 being  anti-discrete non  empty TopStruct holds (for B36 being (Point of B35) holds (for B37 being (Basis of B36) holds B37 = { (the carrier of B35) })))
proof
let C75 being  anti-discrete non  empty TopStruct;
let C76 being (Point of C75);
let C77 being (Basis of C76);
