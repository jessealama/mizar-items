let C11 , C12 being (Subset of C10);
assume that
L28: C11 <> ( {} )
and
L29: C12 <> ( {} )
and
L30: C11 is  closed
and
L31: C12 is  closed
and
L32: ( C11 /\ C12 ) = ( {} );
take D2 = C11;
take D3 = C12;
thus L33: (D2 is  open & D3 is  open) by TDLAT_3:15;
thus L34: (C11 c= D2 & C12 c= D3 & ( D2 /\ D3 ) = ( {} )) by L32;
end;
theorem
L35: (for B8 being  discrete non  empty TopSpace holds B8 is  regular)
proof
let C13 being  discrete non  empty TopSpace;
let C14 being (Point of C13);
let C15 being (Subset of C13);
assume that
L36: C15 <> ( {} )
and
L37: C15 is  closed
and
L38: C14 in ( C15 ` );
take D4 = { C14 };
take D5 = C15;
thus L39: (D4 is  open & D5 is  open) by TDLAT_3:15;
L40: (not C14 in C15) by L38 , XBOOLE_0:def 5;
L41: ( D4 /\ D5 ) c= ( {} )
proof
let C16 being set;
assume L42: C16 in ( D4 /\ D5 );
assume L43: (not C16 in ( {} ));
L44: (C16 in D4 & C16 in D5) by L42 , XBOOLE_0:def 4;
thus L45: contradiction by L44 , L40 , TARSKI:def 1;
end;
thus L46: (C14 in D4 & C15 c= D5 & ( D4 /\ D5 ) = ( {} )) by L41 , TARSKI:def 1;
end;
theorem
L47: (for B9 being  discrete non  empty TopSpace holds B9 is  T_2)
proof
let C17 being  discrete non  empty TopSpace;
let C18 , C19 being (Point of C17);
assume that
L48: (not C18 = C19);
take D6 = { C18 };
take D7 = { C19 };
thus L49: (D6 is  open & D7 is  open) by TDLAT_3:15;
L50: ( D6 /\ D7 ) c= ( {} )
proof
