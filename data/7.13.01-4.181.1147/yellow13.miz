L52: C20 in D6 by L51 , XBOOLE_0:def 4;
L53: C20 = C18 by L52 , TARSKI:def 1;
assume L54: (not C20 in ( {} ));
L55: C20 in D7 by L51 , XBOOLE_0:def 4;
thus L56: contradiction by L55 , L48 , L53 , TARSKI:def 1;
end;
thus L57: (C18 in D6 & C19 in D7 & ( D6 /\ D7 ) = ( {} )) by L50 , TARSKI:def 1;
end;
