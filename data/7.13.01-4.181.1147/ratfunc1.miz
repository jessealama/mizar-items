proof
let C1 being  add-associative  right_zeroed  right_complementable  right-distributive non  empty doubleLoopStr;
let C2 being (Element of C1);
let C3 , C4 being (FinSequence of C1);
assume L2: (( len C3 ) = ( len C4 ) & (for B6 being (Element of ( NAT )) holds (B6 in ( dom C3 ) implies ( C4 /. B6 ) = ( C2 * ( C3 /. B6 ) ))));
consider C5 being (Function of ( NAT ) , (the carrier of C1)) such that L3: ( Sum C4 ) = ( C5 . ( len C4 ) ) and L4: ( C5 . ( 0 ) ) = ( 0. C1 ) and L5: (for B7 being (Element of ( NAT )) holds (for B8 being (Element of C1) holds ((B7 < ( len C4 ) & B8 = ( C4 . ( B7 + 1 ) )) implies ( C5 . ( B7 + 1 ) ) = ( ( C5 . B7 ) + B8 )))) by RLVECT_1:def 12;
consider C6 being (Function of ( NAT ) , (the carrier of C1)) such that L6: ( Sum C3 ) = ( C6 . ( len C3 ) ) and L7: ( C6 . ( 0 ) ) = ( 0. C1 ) and L8: (for B9 being (Element of ( NAT )) holds (for B10 being (Element of C1) holds ((B9 < ( len C3 ) & B10 = ( C3 . ( B9 + 1 ) )) implies ( C6 . ( B9 + 1 ) ) = ( ( C6 . B9 ) + B10 )))) by RLVECT_1:def 12;
defpred S1[ (Element of ( NAT )) ] means ( C5 . $1 ) = ( C2 * ( C6 . $1 ) );
L9: S1[ ( 0 ) ] by L7 , L4 , VECTSP_1:6;
L10: (for B11 being (Element of ( NAT )) holds ((( 0 ) <= B11 & B11 < ( len C3 )) implies (S1[ B11 ] implies S1[ ( B11 + 1 ) ])))
proof
let C7 being (Element of ( NAT ));
assume L11: (( 0 ) <= C7 & C7 < ( len C3 ));
assume L12: S1[ C7 ];
L13: 1 <= ( C7 + 1 ) by NAT_1:11;
L14: ( C7 + 1 ) <= ( len C3 ) by L11 , NAT_1:13;
L15: ( C7 + 1 ) in ( Seg ( len C4 ) ) by L14 , L13 , L2;
L16: ( C7 + 1 ) in ( dom C4 ) by L15 , FINSEQ_1:def 3;
L17: ( C7 + 1 ) in ( Seg ( len C3 ) ) by L13 , L14;
L18: ( C7 + 1 ) in ( dom C3 ) by L17 , FINSEQ_1:def 3;
set D1 = ( C4 /. ( C7 + 1 ) );
set D2 = ( C3 /. ( C7 + 1 ) );
L19: D1 = ( C4 . ( C7 + 1 ) ) by L16 , PARTFUN1:def 6;
L20: D2 = ( C3 . ( C7 + 1 ) ) by L18 , PARTFUN1:def 6;
L21: ( C5 . ( C7 + 1 ) ) = ( ( C2 * ( C6 . C7 ) ) + D1 ) by L12 , L2 , L19 , L11 , L5
.= ( ( C2 * ( C6 . C7 ) ) + ( C2 * D2 ) ) by L2 , L18
.= ( C2 * ( ( C6 . C7 ) + D2 ) ) by VECTSP_1:def 2
.= ( C2 * ( C6 . ( C7 + 1 ) ) ) by L20 , L11 , L8;
thus L22: S1[ ( C7 + 1 ) ] by L21;
end;
L23: (for B12 being (Element of ( NAT )) holds ((( 0 ) <= B12 & B12 <= ( len C3 )) implies S1[ B12 ])) from INT_1:sch 7(L9 , L10);
thus L24: thesis by L23 , L2 , L6 , L3;
