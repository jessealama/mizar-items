theorem
L2: (for R4 being non  empty TopSpace holds (for R5 being non  empty (SubSpace of R4) holds (R4 is  T_2 implies R5 is  T_2)))
proof
let R4 being non  empty TopSpace;
let R5 being non  empty (SubSpace of R4);
assume L3: R4 is  T_2;
L4: (for B3 , B4 being (Point of R5) holds ((not B3 = B4) implies (ex B5 , B6 being (Subset of R5) st (B5 is  open & B6 is  open & B3 in B5 & B4 in B6 & B5 misses B6))))
proof
let C1 , C2 being (Point of R5);
assume that
L5: (not C1 = C2);
reconsider D1 = C1 , D2 = C2 as (Point of R4) by PRE_TOPC:25;
consider C3 , C4 being (Subset of R4) such that L6: C3 is  open and L7: C4 is  open and L8: (D1 in C3 & D2 in C4) and L9: C3 misses C4 by L3 , L5 , PRE_TOPC:def 10;
reconsider D3 = ( C3 /\ ( [#] R5 ) ) , D4 = ( C4 /\ ( [#] R5 ) ) as (Subset of R5);
L10: C4 in (the topology of R4) by L7 , PRE_TOPC:def 2;
L11: D4 in (the topology of R5) by L10 , PRE_TOPC:def 4;
take D3;
take D4;
L12: C3 in (the topology of R4) by L6 , PRE_TOPC:def 2;
L13: D3 in (the topology of R5) by L12 , PRE_TOPC:def 4;
thus L14: (D3 is  open & D4 is  open) by L13 , L11 , PRE_TOPC:def 2;
thus L15: (C1 in D3 & C2 in D4) by L8 , XBOOLE_0:def 4;
L16: ( C3 /\ C4 ) = ( {} ) by L9 , XBOOLE_0:def 7;
L17: ( D3 /\ D4 ) = ( ( C3 /\ ( C4 /\ ( [#] R5 ) ) ) /\ ( [#] R5 ) ) by XBOOLE_1:16
.= ( ( {} ) /\ ( [#] R5 ) ) by L16 , XBOOLE_1:16
.= ( {} );
thus L18: thesis by L17 , XBOOLE_0:def 7;
end;
thus L19: thesis by L4 , PRE_TOPC:def 10;
end;
theorem
L20: (for B7 being TopSpace holds (for B8 , B9 being (SubSpace of B7) holds ((the carrier of B8) c= (the carrier of B9) implies B8 is (SubSpace of B9))))
proof
let C5 being TopSpace;
let C6 , C7 being (SubSpace of C5);
assume L21: (the carrier of C6) c= (the carrier of C7);
L22: (for B10 being (Subset of C6) holds (B10 in (the topology of C6) iff (ex B11 being (Subset of C7) st (B11 in (the topology of C7) & B10 = ( B11 /\ ( [#] C6 ) )))))
proof
let C8 being (Subset of C6);
thus L23: (C8 in (the topology of C6) implies (ex B12 being (Subset of C7) st (B12 in (the topology of C7) & C8 = ( B12 /\ ( [#] C6 ) ))))
proof
assume L24: C8 in (the topology of C6);
consider C9 being (Subset of C5) such that L25: C9 in (the topology of C5) and L26: C8 = ( C9 /\ ( [#] C6 ) ) by L24 , PRE_TOPC:def 4;
reconsider D5 = ( C9 /\ ( [#] C7 ) ) as (Subset of C7);
L27: D5 in (the topology of C7) by L25 , PRE_TOPC:def 4;
L28: C8 = ( C9 /\ ( ( [#] C7 ) /\ ( [#] C6 ) ) ) by L21 , L26 , XBOOLE_1:28
.= ( D5 /\ ( [#] C6 ) ) by XBOOLE_1:16;
thus L29: thesis by L28 , L27;
end;

given C10 being (Subset of C7) such that
L30: C10 in (the topology of C7)
and
L31: C8 = ( C10 /\ ( [#] C6 ) );

consider C11 being (Subset of C5) such that L32: C11 in (the topology of C5) and L33: C10 = ( C11 /\ ( [#] C7 ) ) by L30 , PRE_TOPC:def 4;
L34: C8 = ( C11 /\ ( ( [#] C7 ) /\ ( [#] C6 ) ) ) by L31 , L33 , XBOOLE_1:16
.= ( C11 /\ ( [#] C6 ) ) by L21 , XBOOLE_1:28;
thus L35: thesis by L34 , L32 , PRE_TOPC:def 4;
end;
L36: (the carrier of C6) c= ( [#] C7 ) by L21;
thus L37: thesis by L36 , L22 , PRE_TOPC:def 4;
end;
theorem
L38: (for R4 being non  empty TopSpace holds (for R6 being (Subset of R4) holds (for R7 being (Subset of R4) holds (( R4 | R6 ) is (SubSpace of ( R4 | ( R6 \/ R7 ) )) & ( R4 | R7 ) is (SubSpace of ( R4 | ( R6 \/ R7 ) ))))))
proof
let R4 being non  empty TopSpace;
let R6 being (Subset of R4);
let R7 being (Subset of R4);
L39: (( [#] ( R4 | R6 ) ) = R6 & ( [#] ( R4 | ( R6 \/ R7 ) ) ) = ( R6 \/ R7 )) by PRE_TOPC:def 5;
thus L40: ( R4 | R6 ) is (SubSpace of ( R4 | ( R6 \/ R7 ) )) by L39 , L20 , XBOOLE_1:7;
L41: (( [#] ( R4 | R7 ) ) = R7 & ( [#] ( R4 | ( R6 \/ R7 ) ) ) = ( R6 \/ R7 )) by PRE_TOPC:def 5;
thus L42: thesis by L41 , L20 , XBOOLE_1:7;
end;
theorem
L43: (for R4 being non  empty TopSpace holds (for R8 being (Point of R4) holds (for B13 being non  empty (Subset of R4) holds (R8 in B13 implies (for B14 being (a_neighborhood of R8) holds (for B15 being (Point of ( R4 | B13 )) holds (for B16 being (Subset of ( R4 | B13 )) holds ((B16 = ( B14 /\ B13 ) & B15 = R8) implies B16 is (a_neighborhood of B15)))))))))
proof
let R4 being non  empty TopSpace;
let R8 being (Point of R4);
let C12 being non  empty (Subset of R4);
assume L44: R8 in C12;
let C13 being (a_neighborhood of R8);
let C14 being (Point of ( R4 | C12 ));
let C15 being (Subset of ( R4 | C12 ));
assume that
L45: C15 = ( C13 /\ C12 )
and
L46: C14 = R8;
L47: R8 in ( Int C13 ) by CONNSP_2:def 1;
consider C16 being (Subset of R4) such that L48: C16 is  open and L49: C16 c= C13 and L50: R8 in C16 by L47 , TOPS_1:22;
reconsider D6 = ( C16 /\ C12 ) as (Subset of ( R4 | C12 )) by TOPS_2:29;
L51: D6 c= C15 by L49 , L45 , XBOOLE_1:26;
L52: ( C16 /\ ( [#] ( R4 | C12 ) ) ) = ( C16 /\ C12 ) by PRE_TOPC:def 5;
L53: D6 is  open by L52 , L48 , TOPS_2:24;
L54: C14 in D6 by L44 , L50 , L46 , XBOOLE_0:def 4;
L55: C14 in ( Int C15 ) by L54 , L53 , L51 , TOPS_1:22;
thus L56: thesis by L55 , CONNSP_2:def 1;
end;
theorem
L57: (for B17 , B18 being TopSpace holds (for B19 being (Function of B17 , B18) holds (for B20 being (Subset of B18) holds (B19 is  continuous implies (for B21 being (Function of B17 , ( B18 | B20 )) holds (B21 = B19 implies B21 is  continuous))))))
proof
let C17 , C18 being TopSpace;
let C19 being (Function of C17 , C18);
let C20 being (Subset of C18);
assume L58: C19 is  continuous;
L59: (the carrier of ( C18 | C20 )) = ( [#] ( C18 | C20 ) )
.= C20 by PRE_TOPC:def 5;
let C21 being (Function of C17 , ( C18 | C20 ));
assume that
L60: C21 = C19;
L61: ( rng C19 ) c= (the carrier of ( C18 | C20 )) by L60 , RELAT_1:def 19;
L62: (for B22 being (Subset of ( C18 | C20 )) holds (B22 is  closed implies ( C21 " B22 ) is  closed))
proof
let C22 being (Subset of ( C18 | C20 ));
assume L63: C22 is  closed;
consider C23 being (Subset of C18) such that L64: C23 is  closed and L65: ( C23 /\ ( [#] ( C18 | C20 ) ) ) = C22 by L63 , PRE_TOPC:13;
L66: ( C21 " C22 ) = ( C19 " ( C23 /\ C20 ) ) by L60 , L65 , PRE_TOPC:def 5
.= ( ( C19 " C23 ) /\ ( C19 " C20 ) ) by FUNCT_1:68
.= ( ( C19 " C23 ) /\ ( C19 " ( ( rng C19 ) /\ C20 ) ) ) by RELAT_1:133
.= ( ( C19 " C23 ) /\ ( C19 " ( rng C19 ) ) ) by L59 , L61 , XBOOLE_1:28
.= ( ( C19 " C23 ) /\ ( dom C19 ) ) by RELAT_1:134
.= ( C19 " C23 ) by RELAT_1:132 , XBOOLE_1:28;
thus L67: thesis by L66 , L58 , L64 , PRE_TOPC:def 6;
end;
thus L68: thesis by L62 , PRE_TOPC:def 6;
end;
theorem
L69: (for B23 being TopStruct holds (for B24 being non  empty TopStruct holds (for B25 being (Subset of B23) holds (for B26 being (Function of B23 , B24) holds (for B27 being (Function of ( B23 | B25 ) , B24) holds ((B26 is  continuous & B27 = ( B26 | B25 )) implies B27 is  continuous))))))
