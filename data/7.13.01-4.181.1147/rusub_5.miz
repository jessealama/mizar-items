begin
definition
let C1 being non  empty RLSStruct;
let C2 , C3 being  Affine (Subset of C1);
pred C2 is_parallel_to C3
means
:L1: (ex B1 being (VECTOR of C1) st C2 = ( C3 + { B1 } ))
;end;
theorem
L3: (for B2 being  right_zeroed non  empty RLSStruct holds (for B3 being  Affine (Subset of B2) holds B3 is_parallel_to B3))
proof
let C4 being  right_zeroed non  empty RLSStruct;
let C5 being  Affine (Subset of C4);
take ( 0. C4 );
L4: (for B4 being set holds (B4 in ( C5 + { ( 0. C4 ) } ) implies B4 in C5))
proof
let C6 being set;
assume L5: C6 in ( C5 + { ( 0. C4 ) } );
L6: C6 in { ( B5 + B6 ) where B5 , B6 is (Element of C4) : (B5 in C5 & B6 in { ( 0. C4 ) }) } by L5 , RUSUB_4:def 9;
consider C7 , C8 being (Element of C4) such that L7: (C6 = ( C7 + C8 ) & C7 in C5) and L8: C8 in { ( 0. C4 ) } by L6;
L9: C8 = ( 0. C4 ) by L8 , TARSKI:def 1;
thus L10: thesis by L9 , L7 , RLVECT_1:def 4;
end;
L11: ( C5 + { ( 0. C4 ) } ) c= C5 by L4 , TARSKI:def 3;
L12: (for B7 being set holds (B7 in C5 implies B7 in ( C5 + { ( 0. C4 ) } )))
proof
let C9 being set;
assume L13: C9 in C5;
reconsider D1 = C9 as (Element of C4) by L13;
L14: (D1 = ( D1 + ( 0. C4 ) ) & ( 0. C4 ) in { ( 0. C4 ) }) by RLVECT_1:def 4 , TARSKI:def 1;
L15: D1 in { ( B8 + B9 ) where B8 , B9 is (Element of C4) : (B8 in C5 & B9 in { ( 0. C4 ) }) } by L14 , L13;
thus L16: thesis by L15 , RUSUB_4:def 9;
end;
L17: C5 c= ( C5 + { ( 0. C4 ) } ) by L12 , TARSKI:def 3;
thus L18: thesis by L17 , L11 , XBOOLE_0:def 10;
end;
theorem
L19: (for B10 being  add-associative  right_zeroed  right_complementable non  empty RLSStruct holds (for B11 , B12 being  Affine (Subset of B10) holds (B11 is_parallel_to B12 implies B12 is_parallel_to B11)))
proof
let C10 being  add-associative  right_zeroed  right_complementable non  empty RLSStruct;
let C11 , C12 being  Affine (Subset of C10);
assume L20: C11 is_parallel_to C12;
consider C13 being (VECTOR of C10) such that L21: C11 = ( C12 + { C13 } ) by L20 , L1;
set D2 = ( - C13 );
L22: (for B13 being set holds (B13 in C12 implies B13 in ( C11 + { D2 } )))
proof
let C14 being set;
assume L23: C14 in C12;
reconsider D3 = C14 as (Element of C10) by L23;
set D4 = ( D3 + C13 );
L24: C13 in { C13 } by TARSKI:def 1;
L25: D4 in { ( B14 + B15 ) where B14 , B15 is (Element of C10) : (B14 in C12 & B15 in { C13 }) } by L24 , L23;
L26: D4 in C11 by L25 , L21 , RUSUB_4:def 9;
L27: ( D3 + ( C13 + D2 ) ) = ( D4 + D2 ) by RLVECT_1:def 3;
L28: ( D3 + ( 0. C10 ) ) = ( D4 + D2 ) by L27 , RLVECT_1:5;
L29: D3 = ( D4 + D2 ) by L28 , RLVECT_1:4;
L30: D2 in { D2 } by TARSKI:def 1;
L31: D3 in { ( B16 + B17 ) where B16 , B17 is (Element of C10) : (B16 in C11 & B17 in { D2 }) } by L30 , L26 , L29;
thus L32: thesis by L31 , RUSUB_4:def 9;
end;
L33: C12 c= ( C11 + { D2 } ) by L22 , TARSKI:def 3;
take D2;
L34: (for B18 being set holds (B18 in ( C11 + { D2 } ) implies B18 in C12))
proof
let C15 being set;
assume L35: C15 in ( C11 + { D2 } );
L36: C15 in { ( B19 + B20 ) where B19 , B20 is (Element of C10) : (B19 in C11 & B20 in { D2 }) } by L35 , RUSUB_4:def 9;
consider C16 , C17 being (Element of C10) such that L37: C15 = ( C16 + C17 ) and L38: C16 in C11 and L39: C17 in { D2 } by L36;
reconsider D5 = C15 as (Element of C10) by L35;
L40: D5 = ( C16 + D2 ) by L37 , L39 , TARSKI:def 1;
L41: ( D5 + C13 ) = ( C16 + ( D2 + C13 ) ) by L40 , RLVECT_1:def 3;
L42: ( D5 + C13 ) = ( C16 + ( 0. C10 ) ) by L41 , RLVECT_1:5;
L43: C16 in { ( B21 + B22 ) where B21 , B22 is (Element of C10) : (B21 in C12 & B22 in { C13 }) } by L21 , L38 , RUSUB_4:def 9;
consider C18 , C19 being (Element of C10) such that L44: (C16 = ( C18 + C19 ) & C18 in C12) and L45: C19 in { C13 } by L43;
L46: C13 = C19 by L45 , TARSKI:def 1;
thus L47: thesis by L46 , L42 , L44 , RLVECT_1:4 , RLVECT_1:8;
end;
L48: ( C11 + { D2 } ) c= C12 by L34 , TARSKI:def 3;
thus L49: thesis by L48 , L33 , XBOOLE_0:def 10;
end;
theorem
L50: (for B23 being  Abelian  add-associative  right_zeroed  right_complementable non  empty RLSStruct holds (for B24 , B25 , B26 being  Affine (Subset of B23) holds ((B24 is_parallel_to B25 & B25 is_parallel_to B26) implies B24 is_parallel_to B26)))
proof
let C20 being  Abelian  add-associative  right_zeroed  right_complementable non  empty RLSStruct;
let C21 , C22 , C23 being  Affine (Subset of C20);
assume that
L51: C21 is_parallel_to C22
and
L52: C22 is_parallel_to C23;
consider C24 being (Element of C20) such that L53: C21 = ( C22 + { C24 } ) by L51 , L1;
consider C25 being (Element of C20) such that L54: C22 = ( C23 + { C25 } ) by L52 , L1;
set D6 = ( C25 + C24 );
L55: (for B27 being set holds (B27 in ( C23 + { D6 } ) implies B27 in C21))
proof
let C26 being set;
L56: C25 in { C25 } by TARSKI:def 1;
assume L57: C26 in ( C23 + { D6 } );
reconsider D7 = C26 as (Element of C20) by L57;
L58: D7 in { ( B28 + B29 ) where B28 , B29 is (Element of C20) : (B28 in C23 & B29 in { D6 }) } by L57 , RUSUB_4:def 9;
consider C27 , C28 being (Element of C20) such that L59: D7 = ( C27 + C28 ) and L60: C27 in C23 and L61: C28 in { D6 } by L58;
L62: D7 = ( C27 + ( C25 + C24 ) ) by L59 , L61 , TARSKI:def 1
.= ( ( C27 + C25 ) + C24 ) by RLVECT_1:def 3;
L63: ( D7 - C24 ) = ( ( C27 + C25 ) + ( C24 - C24 ) ) by L62 , RLVECT_1:def 3
.= ( ( C27 + C25 ) + ( 0. C20 ) ) by RLVECT_1:15
.= ( C27 + C25 ) by RLVECT_1:4;
L64: ( D7 - C24 ) in { ( B30 + B31 ) where B30 , B31 is (Element of C20) : (B30 in C23 & B31 in { C25 }) } by L63 , L60 , L56;
L65: ( D7 - C24 ) in C22 by L64 , L54 , RUSUB_4:def 9;
set D8 = ( D7 - C24 );
L66: C24 in { C24 } by TARSKI:def 1;
L67: ( D8 + C24 ) = ( D7 - ( C24 - C24 ) ) by RLVECT_1:29
.= ( D7 - ( 0. C20 ) ) by RLVECT_1:15
.= D7 by RLVECT_1:13;
L68: D7 in { ( B32 + B33 ) where B32 , B33 is (Element of C20) : (B32 in C22 & B33 in { C24 }) } by L67 , L65 , L66;
thus L69: thesis by L68 , L53 , RUSUB_4:def 9;
end;
L70: ( C23 + { D6 } ) c= C21 by L55 , TARSKI:def 3;
L71: (for B34 being set holds (B34 in C21 implies B34 in ( C23 + { D6 } )))
proof
let C29 being set;
L72: D6 in { D6 } by TARSKI:def 1;
assume L73: C29 in C21;
reconsider D9 = C29 as (Element of C20) by L73;
L74: D9 in { ( B35 + B36 ) where B35 , B36 is (Element of C20) : (B35 in C22 & B36 in { C24 }) } by L53 , L73 , RUSUB_4:def 9;
consider C30 , C31 being (Element of C20) such that L75: D9 = ( C30 + C31 ) and L76: C30 in C22 and L77: C31 in { C24 } by L74;
L78: C31 = C24 by L77 , TARSKI:def 1;
L79: C30 in { ( B37 + B38 ) where B37 , B38 is (Element of C20) : (B37 in C23 & B38 in { C25 }) } by L54 , L76 , RUSUB_4:def 9;
consider C32 , C33 being (Element of C20) such that L80: C30 = ( C32 + C33 ) and L81: C32 in C23 and L82: C33 in { C25 } by L79;
L83: C33 = C25 by L82 , TARSKI:def 1;
L84: D9 = ( C32 + D6 ) by L83 , L75 , L80 , L78 , RLVECT_1:def 3;
L85: D9 in { ( B39 + B40 ) where B39 , B40 is (Element of C20) : (B39 in C23 & B40 in { D6 }) } by L84 , L81 , L72;
thus L86: thesis by L85 , RUSUB_4:def 9;
end;
L87: C21 c= ( C23 + { D6 } ) by L71 , TARSKI:def 3;
L88: C21 = ( C23 + { D6 } ) by L87 , L70 , XBOOLE_0:def 10;
thus L89: thesis by L88 , L1;
end;
definition
let C34 being non  empty addLoopStr;
let C35 , C36 being (Subset of C34);
func C35 - C36 -> (Subset of C34) equals 
{ ( B41 - B42 ) where B41 , B42 is (Element of C34) : (B41 in C35 & B42 in C36) };
coherence
proof
defpred S1[ set , set ] means ($1 in C35 & $2 in C36);
deffunc H1((Element of C34) , (Element of C34)) = ( $1 - $2 );
L90: { H1(B43 , B44) where B43 , B44 is (Element of C34) : S1[ B43 , B44 ] } is (Subset of C34) from DOMAIN_1:sch 9;
thus L91: thesis by L90;
end;
end;
theorem
L93: (for B45 being RealLinearSpace holds (for B46 , B47 being  Affine (Subset of B45) holds ( B46 - B47 ) is  Affine))
proof
let C37 being RealLinearSpace;
let C38 , C39 being  Affine (Subset of C37);
L94: (for B48 , B49 being (VECTOR of C37) holds (for B50 being Real holds ((B48 in ( C38 - C39 ) & B49 in ( C38 - C39 )) implies ( ( ( 1 - B50 ) * B48 ) + ( B50 * B49 ) ) in ( C38 - C39 ))))
proof
let C40 , C41 being (VECTOR of C37);
let C42 being Real;
assume that
L95: C40 in ( C38 - C39 )
and
L96: C41 in ( C38 - C39 );
consider C43 , C44 being (Element of C37) such that L97: C40 = ( C43 - C44 ) and L98: (C43 in C38 & C44 in C39) by L95;
consider C45 , C46 being (Element of C37) such that L99: C41 = ( C45 - C46 ) and L100: (C45 in C38 & C46 in C39) by L96;
L101: ( ( ( 1 - C42 ) * C40 ) + ( C42 * C41 ) ) = ( ( ( ( 1 - C42 ) * C43 ) - ( ( 1 - C42 ) * C44 ) ) + ( C42 * ( C45 - C46 ) ) ) by L97 , L99 , RLVECT_1:34
.= ( ( ( ( 1 - C42 ) * C43 ) - ( ( 1 - C42 ) * C44 ) ) + ( ( C42 * C45 ) - ( C42 * C46 ) ) ) by RLVECT_1:34
.= ( ( ( ( ( 1 - C42 ) * C43 ) + ( - ( ( 1 - C42 ) * C44 ) ) ) + ( C42 * C45 ) ) - ( C42 * C46 ) ) by RLVECT_1:def 3
.= ( ( ( ( ( 1 - C42 ) * C43 ) + ( C42 * C45 ) ) + ( - ( ( 1 - C42 ) * C44 ) ) ) + ( - ( C42 * C46 ) ) ) by RLVECT_1:def 3
.= ( ( ( ( 1 - C42 ) * C43 ) + ( C42 * C45 ) ) + ( ( - ( ( 1 - C42 ) * C44 ) ) + ( - ( C42 * C46 ) ) ) ) by RLVECT_1:def 3
.= ( ( ( ( 1 - C42 ) * C43 ) + ( C42 * C45 ) ) - ( ( ( 1 - C42 ) * C44 ) + ( C42 * C46 ) ) ) by RLVECT_1:31;
L102: (( ( ( 1 - C42 ) * C43 ) + ( C42 * C45 ) ) in C38 & ( ( ( 1 - C42 ) * C44 ) + ( C42 * C46 ) ) in C39) by L98 , L100 , RUSUB_4:def 4;
thus L103: thesis by L102 , L101;
end;
thus L104: thesis by L94 , RUSUB_4:def 4;
end;
theorem
L105: (for B51 being non  empty addLoopStr holds (for B52 , B53 being (Subset of B51) holds ((B52 is  empty or B53 is  empty) implies ( B52 + B53 ) is  empty)))
proof
let C47 being non  empty addLoopStr;
let C48 , C49 being (Subset of C47);
assume L106: (C48 is  empty or C49 is  empty);
assume L107: (not ( C48 + C49 ) is  empty);
consider C50 being set such that L108: C50 in ( C48 + C49 ) by L107 , XBOOLE_0:def 1;
L109: C50 in { ( B54 + B55 ) where B54 , B55 is (Element of C47) : (B54 in C48 & B55 in C49) } by L108 , RUSUB_4:def 9;
L110: (ex B56 , B57 being (Element of C47) st (C50 = ( B56 + B57 ) & B56 in C48 & B57 in C49)) by L109;
thus L111: contradiction by L110 , L106;
end;
theorem
L112: (for B58 being non  empty addLoopStr holds (for B59 , B60 being non  empty (Subset of B58) holds ( B59 + B60 ) is non  empty))
proof
let C51 being non  empty addLoopStr;
let C52 , C53 being non  empty (Subset of C51);
consider C54 being set such that L113: C54 in C52 by XBOOLE_0:def 1;
consider C55 being set such that L114: C55 in C53 by XBOOLE_0:def 1;
reconsider D10 = C54 , D11 = C55 as (Element of C51) by L113 , L114;
L115: ( D10 + D11 ) in { ( B61 + B62 ) where B61 , B62 is (Element of C51) : (B61 in C52 & B62 in C53) } by L113 , L114;
thus L116: thesis by L115 , RUSUB_4:def 9;
end;
theorem
L117: (for B63 being non  empty addLoopStr holds (for B64 , B65 being (Subset of B63) holds ((B64 is  empty or B65 is  empty) implies ( B64 - B65 ) is  empty)))
proof
let C56 being non  empty addLoopStr;
let C57 , C58 being (Subset of C56);
assume L118: (C57 is  empty or C58 is  empty);
assume L119: (not ( C57 - C58 ) is  empty);
consider C59 being set such that L120: C59 in ( C57 - C58 ) by L119 , XBOOLE_0:def 1;
L121: (ex B66 , B67 being (Element of C56) st (C59 = ( B66 - B67 ) & B66 in C57 & B67 in C58)) by L120;
thus L122: contradiction by L121 , L118;
end;
theorem
L123: (for B68 being non  empty addLoopStr holds (for B69 , B70 being non  empty (Subset of B68) holds ( B69 - B70 ) is non  empty))
proof
let C60 being non  empty addLoopStr;
let C61 , C62 being non  empty (Subset of C60);
consider C63 being set such that L124: C63 in C61 by XBOOLE_0:def 1;
consider C64 being set such that L125: C64 in C62 by XBOOLE_0:def 1;
reconsider D12 = C63 , D13 = C64 as (Element of C60) by L124 , L125;
L126: ( D12 - D13 ) in { ( B71 - B72 ) where B71 , B72 is (Element of C60) : (B71 in C61 & B72 in C62) } by L124 , L125;
thus L127: thesis by L126;
end;
theorem
L128: (for B73 being  Abelian  add-associative  right_zeroed  right_complementable non  empty addLoopStr holds (for B74 , B75 being (Subset of B73) holds (for B76 being (Element of B73) holds (B74 = ( B75 + { B76 } ) iff ( B74 - { B76 } ) = B75))))
proof
let C65 being  Abelian  add-associative  right_zeroed  right_complementable non  empty addLoopStr;
let C66 , C67 being (Subset of C65);
let C68 being (Element of C65);
L129: (( C66 - { C68 } ) = C67 implies C66 = ( C67 + { C68 } ))
proof
assume L130: ( C66 - { C68 } ) = C67;
L131: (for B77 being set holds (B77 in ( C67 + { C68 } ) implies B77 in C66))
proof
let C69 being set;
assume L132: C69 in ( C67 + { C68 } );
reconsider D14 = C69 as (Element of C65) by L132;
L133: D14 in { ( B78 + B79 ) where B78 , B79 is (Element of C65) : (B78 in C67 & B79 in { C68 }) } by L132 , RUSUB_4:def 9;
consider C70 , C71 being (Element of C65) such that L134: D14 = ( C70 + C71 ) and L135: C70 in C67 and L136: C71 in { C68 } by L133;
L137: ( D14 - C71 ) = ( C70 + ( C71 - C71 ) ) by L134 , RLVECT_1:def 3
.= ( C70 + ( 0. C65 ) ) by RLVECT_1:15
.= C70 by RLVECT_1:4;
L138: C71 = C68 by L136 , TARSKI:def 1;
consider C72 , C73 being (Element of C65) such that L139: ( D14 - C68 ) = ( C72 - C73 ) and L140: C72 in C66 and L141: C73 in { C68 } by L138 , L130 , L135 , L137;
L142: C73 = C68 by L141 , TARSKI:def 1;
L143: ( ( D14 - C68 ) + C68 ) = ( C72 - ( C68 - C68 ) ) by L142 , L139 , RLVECT_1:29
.= ( C72 - ( 0. C65 ) ) by RLVECT_1:15
.= C72 by RLVECT_1:13;
L144: C72 = ( D14 - ( C68 - C68 ) ) by L143 , RLVECT_1:29
.= ( D14 - ( 0. C65 ) ) by RLVECT_1:15;
thus L145: thesis by L144 , L140 , RLVECT_1:13;
end;
L146: ( C67 + { C68 } ) c= C66 by L131 , TARSKI:def 3;
L147: (for B80 being set holds (B80 in C66 implies B80 in ( C67 + { C68 } )))
proof
let C74 being set;
assume L148: C74 in C66;
reconsider D15 = C74 as (Element of C65) by L148;
L149: C68 in { C68 } by TARSKI:def 1;
L150: ( D15 - C68 ) in { ( B81 - B82 ) where B81 , B82 is (Element of C65) : (B81 in C66 & B82 in { C68 }) } by L149 , L148;
consider C75 being (Element of C65) such that L151: ( D15 - C68 ) = C75 and L152: C75 in C67 by L150 , L130;
L153: ( C75 + C68 ) = ( D15 - ( C68 - C68 ) ) by L151 , RLVECT_1:29
.= ( D15 - ( 0. C65 ) ) by RLVECT_1:15
.= D15 by RLVECT_1:13;
L154: D15 in { ( B83 + B84 ) where B83 , B84 is (Element of C65) : (B83 in C67 & B84 in { C68 }) } by L153 , L149 , L152;
thus L155: thesis by L154 , RUSUB_4:def 9;
end;
L156: C66 c= ( C67 + { C68 } ) by L147 , TARSKI:def 3;
thus L157: thesis by L156 , L146 , XBOOLE_0:def 10;
end;
L158: (C66 = ( C67 + { C68 } ) implies ( C66 - { C68 } ) = C67)
proof
assume L159: C66 = ( C67 + { C68 } );
L160: (for B85 being set holds (B85 in ( C66 - { C68 } ) implies B85 in C67))
proof
let C76 being set;
assume L161: C76 in ( C66 - { C68 } );
reconsider D16 = C76 as (Element of C65) by L161;
consider C77 , C78 being (Element of C65) such that L162: D16 = ( C77 - C78 ) and L163: C77 in C66 and L164: C78 in { C68 } by L161;
L165: ( D16 + C78 ) = ( C77 - ( C78 - C78 ) ) by L162 , RLVECT_1:29
.= ( C77 - ( 0. C65 ) ) by RLVECT_1:15
.= C77 by RLVECT_1:13;
L166: C78 = C68 by L164 , TARSKI:def 1;
L167: ( D16 + C68 ) in { ( B86 + B87 ) where B86 , B87 is (Element of C65) : (B86 in C67 & B87 in { C68 }) } by L166 , L159 , L163 , L165 , RUSUB_4:def 9;
consider C79 , C80 being (Element of C65) such that L168: (( D16 + C68 ) = ( C79 + C80 ) & C79 in C67) and L169: C80 in { C68 } by L167;
L170: C80 = C68 by L169 , TARSKI:def 1;
thus L171: thesis by L170 , L168 , RLVECT_1:8;
end;
L172: ( C66 - { C68 } ) c= C67 by L160 , TARSKI:def 3;
L173: (for B88 being set holds (B88 in C67 implies B88 in ( C66 - { C68 } )))
proof
let C81 being set;
assume L174: C81 in C67;
reconsider D17 = C81 as (Element of C65) by L174;
L175: C68 in { C68 } by TARSKI:def 1;
