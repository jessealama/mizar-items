begin
L1: (for B1 being Ring holds (for B2 being (Scalar of B1) holds (( - B2 ) = ( 0. B1 ) implies B2 = ( 0. B1 ))))
proof
let C1 being Ring;
let C2 being (Scalar of C1);
assume L2: ( - C2 ) = ( 0. C1 );
L3: ( - ( - C2 ) ) = ( 0. C1 ) by L2 , RLVECT_1:12;
thus L4: thesis by L3 , RLVECT_1:17;
end;
theorem
L5: (for B3 being non  degenerated  add-associative  right_zeroed  right_complementable non  empty doubleLoopStr holds ( 0. B3 ) <> ( - ( 1. B3 ) ))
proof
let C3 being non  degenerated  add-associative  right_zeroed  right_complementable non  empty doubleLoopStr;
assume L6: ( 0. C3 ) = ( - ( 1. C3 ) );
L7: ( 0. C3 ) = ( - ( - ( 1. C3 ) ) ) by L6 , RLVECT_1:12
.= ( 1. C3 ) by RLVECT_1:17;
thus L8: contradiction by L7;
end;
theorem
L9: (for R3 being Ring holds (for R4 being (LeftMod of R3) holds (for R5 being (Linear_Combination of R4) holds (for R11 being  finite (Subset of R4) holds (( Carrier R5 ) c= R11 implies (ex R9 being (FinSequence of (the carrier of R4)) st (R9 is  one-to-one & ( rng R9 ) = R11 & ( Sum R5 ) = ( Sum ( R5 (#) R9 ) ))))))))
proof
let R3 being Ring;
let R4 being (LeftMod of R3);
let R5 being (Linear_Combination of R4);
let R11 being  finite (Subset of R4);
assume L10: ( Carrier R5 ) c= R11;
set D1 = ( R11 \ ( Carrier R5 ) );
consider R10 being (FinSequence of (the carrier of R4)) such that L11: R10 is  one-to-one and L12: ( rng R10 ) = ( Carrier R5 ) and L13: ( Sum R5 ) = ( Sum ( R5 (#) R10 ) ) by VECTSP_6:def 6;
consider C4 being FinSequence such that L14: ( rng C4 ) = D1 and L15: C4 is  one-to-one by FINSEQ_4:58;
reconsider D2 = C4 as (FinSequence of (the carrier of R4)) by L14 , FINSEQ_1:def 4;
L16: ( rng R10 ) misses ( rng D2 )
proof
assume L17: ( rng R10 ) meets ( rng D2 );
L18: (ex B4 being set st (B4 in ( Carrier R5 ) & B4 in D1)) by L17 , L12 , L14 , XBOOLE_0:3;
thus L19: thesis by L18 , XBOOLE_0:def 5;
end;
L20: ( len D2 ) = ( len ( R5 (#) D2 ) ) by VECTSP_6:def 5;
L21:
now
let C5 being (Element of ( NAT ));
assume L22: C5 in ( dom D2 );
L23: ( D2 /. C5 ) = ( D2 . C5 ) by L22 , PARTFUN1:def 6;
L24: ( D2 /. C5 ) in D1 by L23 , L14 , L22 , FUNCT_1:def 3;
L25: (not ( D2 /. C5 ) in ( Carrier R5 )) by L24 , XBOOLE_0:def 5;
L26: C5 in ( dom ( R5 (#) D2 ) ) by L20 , L22 , FINSEQ_3:29;
L27: ( ( R5 (#) D2 ) . C5 ) = ( ( R5 . ( D2 /. C5 ) ) * ( D2 /. C5 ) ) by L26 , VECTSP_6:def 5;
thus L28: ( ( R5 (#) D2 ) . C5 ) = ( ( 0. R3 ) * ( D2 /. C5 ) ) by L27 , L25;
end;
L29: ( Sum ( R5 (#) D2 ) ) = ( ( 0. R3 ) * ( Sum D2 ) ) by L21 , L20 , RLVECT_2:67
.= ( 0. R4 ) by VECTSP_1:14;
set D3 = ( R10 ^ D2 );
take D3;
L30: ( Sum ( R5 (#) D3 ) ) = ( Sum ( ( R5 (#) R10 ) ^ ( R5 (#) D2 ) ) ) by VECTSP_6:13
.= ( ( Sum ( R5 (#) R10 ) ) + ( 0. R4 ) ) by L29 , RLVECT_1:41
.= ( Sum R5 ) by L13 , RLVECT_1:def 4;
L31: ( rng D3 ) = ( ( Carrier R5 ) \/ D1 ) by L12 , L14 , FINSEQ_1:31
.= R11 by L10 , XBOOLE_1:45;
thus L32: thesis by L31 , L11 , L15 , L30 , L16 , FINSEQ_3:91;
end;
theorem
L33: (for R3 being Ring holds (for R4 being (LeftMod of R3) holds (for R5 being (Linear_Combination of R4) holds (for R6 being (Scalar of R3) holds ( Sum ( R6 * R5 ) ) = ( R6 * ( Sum R5 ) )))))
proof
let R3 being Ring;
let R4 being (LeftMod of R3);
let R5 being (Linear_Combination of R4);
let R6 being (Scalar of R3);
set D4 = ( R6 * R5 );
L34: ( Carrier D4 ) c= ( Carrier R5 ) by VECTSP_6:28;
consider R9 being (FinSequence of (the carrier of R4)) such that L35: (R9 is  one-to-one & ( rng R9 ) = ( Carrier R5 )) and L36: ( Sum D4 ) = ( Sum ( D4 (#) R9 ) ) by L34 , L9;
set D5 = ( D4 (#) R9 );
set D6 = ( R5 (#) R9 );
L37: ( len D5 ) = ( len R9 ) by VECTSP_6:def 5
.= ( len D6 ) by VECTSP_6:def 5;
L38: ( len D5 ) = ( len R9 ) by VECTSP_6:def 5;
L39: ( dom R9 ) = ( Seg ( len D5 ) ) by L38 , FINSEQ_1:def 3;
L40:
now
let C6 being (Element of ( NAT ));
let R7 being (Vector of R4);
assume that
L41: C6 in ( dom D5 )
and
L42: R7 = ( D6 . C6 );
set D7 = ( R9 /. C6 );
L43: C6 in ( Seg ( len D5 ) ) by L41 , FINSEQ_1:def 3;
L44: D7 = ( R9 . C6 ) by L43 , L39 , PARTFUN1:def 6;
thus L45: ( D5 . C6 ) = ( ( D4 . D7 ) * D7 ) by L44 , L39 , L43 , VECTSP_6:8
.= ( ( R6 * ( R5 . D7 ) ) * D7 ) by VECTSP_6:def 9
.= ( R6 * ( ( R5 . D7 ) * D7 ) ) by VECTSP_1:def 16
.= ( R6 * R7 ) by L39 , L42 , L43 , L44 , VECTSP_6:8;
end;
L46: ( Sum R5 ) = ( Sum ( R5 (#) R9 ) ) by L35 , VECTSP_6:def 6;
thus L47: thesis by L46 , L36 , L37 , L40 , RLVECT_2:66;
end;
definition
let R3 being Ring;
let R4 being (LeftMod of R3);
let R15 being (Subset of R4);
func Lin R15 ->  strict (Subspace of R4) means 
:L48: (the carrier of it) = { ( Sum R18 ) where R18 is (Linear_Combination of R15) : (not contradiction) };
existence
proof
set D8 = { ( Sum R18 ) where R18 is (Linear_Combination of R15) : (not contradiction) };
L49: D8 c= (the carrier of R4)
proof
let R1 being set;
assume L50: R1 in D8;
L51: (ex R18 being (Linear_Combination of R15) st R1 = ( Sum R18 )) by L50;
thus L52: thesis by L51;
end;
reconsider D9 = D8 as (Subset of R4) by L49;
reconsider D10 = ( ZeroLC R4 ) as (Linear_Combination of R15) by VECTSP_6:5;
L53: D9 is  linearly-closed
proof
thus L54: (for R7 being (Vector of R4) holds (for R8 being (Vector of R4) holds ((R7 in D9 & R8 in D9) implies ( R7 + R8 ) in D9)))
proof
let R7 being (Vector of R4);
let R8 being (Vector of R4);
assume that
L55: R7 in D9
and
L56: R8 in D9;
consider C7 being (Linear_Combination of R15) such that L57: R7 = ( Sum C7 ) by L55;
consider C8 being (Linear_Combination of R15) such that L58: R8 = ( Sum C8 ) by L56;
reconsider D11 = ( C7 + C8 ) as (Linear_Combination of R15) by VECTSP_6:24;
L59: ( R7 + R8 ) = ( Sum D11 ) by L57 , L58 , VECTSP_6:44;
thus L60: thesis by L59;
end;

let R6 being (Scalar of R3);
let R7 being (Vector of R4);
assume L61: R7 in D9;
consider R18 being (Linear_Combination of R15) such that L62: R7 = ( Sum R18 ) by L61;
reconsider D12 = ( R6 * R18 ) as (Linear_Combination of R15) by VECTSP_6:31;
L63: ( R6 * R7 ) = ( Sum D12 ) by L62 , L33;
thus L64: thesis by L63;
end;
L65: ( Sum D10 ) = ( 0. R4 ) by VECTSP_6:15;
L66: ( 0. R4 ) in D9 by L65;
thus L67: thesis by L66 , L53 , VECTSP_4:34;
end;
uniqueness by VECTSP_4:29;
end;
