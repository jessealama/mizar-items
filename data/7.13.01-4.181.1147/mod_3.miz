theorem
L98: (for R3 being Ring holds (for R4 being (LeftMod of R3) holds ( Lin ( {} (the carrier of R4) ) ) = ( (0). R4 )))
proof
let R3 being Ring;
let R4 being (LeftMod of R3);
set D19 = ( Lin ( {} (the carrier of R4) ) );
L99:
now
