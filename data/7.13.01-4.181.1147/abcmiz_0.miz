registration
cluster  ->  Noetherian for 1 -element 1 -element 1 -element 1 -element RelStr;
coherence
proof
let C3 being 1 -element RelStr;
let C4 being set;
set D1 = (the InternalRel of C3);
assume L5: C4 c= ( field D1 );
assume L6: C4 <> ( {} );
reconsider D2 = C4 as non  empty set by L6;
set D3 = the (Element of D2);
take D3;
thus L7: D3 in C4;
L8: D3 in ( field D1 ) by L7 , L5;
let C5 being set;
L9: ( field D1 ) c= ( (the carrier of C3) \/ (the carrier of C3) ) by RELSET_1:8;
assume L10: C5 in C4;
L11: C5 in ( field D1 ) by L10 , L5;
thus L12: thesis by L11 , L8 , L9 , ZFMISC_1:def 10;
end;
end;
definition
let C6 being non  empty RelStr;
redefine attr C6 is  Noetherian
means
:L14: (for B1 being non  empty (Subset of C6) holds (ex B2 being (Element of C6) st (B2 in B1 & (for B3 being (Element of C6) holds (B3 in B1 implies (not B2 < B3))))));
compatibility
proof
set D4 = (the InternalRel of C6);
thus L15: (C6 is  Noetherian implies (for B4 being non  empty (Subset of C6) holds (ex B5 being (Element of C6) st (B5 in B4 & (for B6 being (Element of C6) holds (B6 in B4 implies (not B5 < B6)))))))
proof
assume L16: (for B7 being set holds ((B7 c= ( field D4 ) & B7 <> ( {} )) implies (ex B8 being set st (B8 in B7 & (for B9 being set holds ((B9 in B7 & B8 <> B9) implies (not [ B8 , B9 ] in D4)))))));
let C7 being non  empty (Subset of C6);
set D5 = the (Element of C7);
reconsider D6 = D5 as (Element of C6);
set D7 = ( C7 /\ ( field D4 ) );
per cases ;
suppose L17: C7 misses ( field D4 );

take D6;
thus L18: D6 in C7;
let C8 being (Element of C6);
assume that
L19: C8 in C7
and
L20: D6 < C8;
L21: D6 <= C8 by L20 , ORDERS_2:def 6;
L22: [ D6 , C8 ] in D4 by L21 , ORDERS_2:def 5;
L23: D6 in ( field D4 ) by L22 , RELAT_1:15;
thus L24: contradiction by L23 , L17 , XBOOLE_0:3;
end;
suppose L25: C7 meets ( field D4 );

L26: D7 <> ( {} ) by L25 , XBOOLE_0:def 7;
consider C9 being set such that L27: C9 in D7 and L28: (for B10 being set holds ((B10 in D7 & C9 <> B10) implies (not [ C9 , B10 ] in D4))) by L26 , L16 , XBOOLE_1:17;
reconsider D8 = C9 as (Element of C6) by L27;
take D8;
thus L29: D8 in C7 by L27 , XBOOLE_0:def 4;
let C10 being (Element of C6);
assume that
L30: C10 in C7
and
L31: D8 < C10;
L32: D8 <= C10 by L31 , ORDERS_2:def 6;
L33: [ D8 , C10 ] in D4 by L32 , ORDERS_2:def 5;
L34: C10 in ( field D4 ) by L33 , RELAT_1:15;
L35: C10 in D7 by L34 , L30 , XBOOLE_0:def 4;
thus L36: contradiction by L35 , L28 , L31 , L33;
end;
end;

assume L38: (for B11 being non  empty (Subset of C6) holds (ex B12 being (Element of C6) st (B12 in B11 & (for B13 being (Element of C6) holds (B13 in B11 implies (not B12 < B13))))));
let C11 being set;
assume that
L39: C11 c= ( field D4 )
and
L40: C11 <> ( {} );
L41: ( field D4 ) c= ( (the carrier of C6) \/ (the carrier of C6) ) by RELSET_1:8;
reconsider D9 = C11 as non  empty (Subset of C6) by L41 , L39 , L40 , XBOOLE_1:1;
consider C12 being (Element of C6) such that L42: C12 in D9 and L43: (for B14 being (Element of C6) holds (B14 in D9 implies (not C12 < B14))) by L38;
take C12;
thus L44: C12 in C11 by L42;
let C13 being set;
assume that
L45: C13 in C11
and
L46: C12 <> C13;
L47: C13 in D9 by L45;
reconsider D10 = C13 as (Element of C6) by L47;
L48: (not C12 < D10) by L43 , L45;
L49: (not C12 <= D10) by L48 , L46 , ORDERS_2:def 6;
thus L50: thesis by L49 , ORDERS_2:def 5;
end;
