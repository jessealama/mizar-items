let R8 being (Subset of R3);
let R9 being (Subset of R3);
thus L13: ((R8 in { R3 } & R9 in { R3 }) implies ( R8 /\ R9 ) in { R3 })
proof
assume L14: (R8 in { R3 } & R9 in { R3 });
L15: (R8 = R3 & R9 = R3) by L14 , TARSKI:def 1;
thus L16: thesis by L15 , TARSKI:def 1;
end;

thus L17: ((R8 in { R3 } & R8 c= R9) implies R9 in { R3 })
proof
