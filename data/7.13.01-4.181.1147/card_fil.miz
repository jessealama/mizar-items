proof
defpred S1[ set ] means (ex R9 being (Subset of R3) st (R9 in { ( R8 /\ R5 ) where R8 is (Subset of R3) : R8 in R14 } & R9 c= $1));
set D8 = { R4 where R4 is (Subset of R3) : S1[ R4 ] };
L120: R3 in R14 by L26;
L121: ( R3 /\ R5 ) in { ( R8 /\ R5 ) where R8 is (Subset of R3) : R8 in R14 } by L120;
L122: R5 in { ( R8 /\ R5 ) where R8 is (Subset of R3) : R8 in R14 } by L121 , XBOOLE_1:28;
L123: R5 in { R4 where R4 is (Subset of R3) : (ex R9 being (Subset of R3) st (R9 in { ( R8 /\ R5 ) where R8 is (Subset of R3) : R8 in R14 } & R9 c= R4)) } by L122;
L124: D8 is (Subset-Family of R3) from DOMAIN_1:sch 7;
thus L125: thesis by L124 , L123;
end;
end;
theorem
L127: (for R3 being non  empty set holds (for R5 being (Subset of R3) holds (for R14 being (Filter of R3) holds (for R6 being (Subset of R3) holds (R6 in ( Extend_Filter (R14 , R5) ) iff (ex R7 being (Subset of R3) st (R7 in R14 & ( R7 /\ R5 ) c= R6)))))))
proof
let R3 being non  empty set;
let R5 being (Subset of R3);
let R14 being (Filter of R3);
let R6 being (Subset of R3);
thus L128: (R6 in ( Extend_Filter (R14 , R5) ) implies (ex R7 being (Subset of R3) st (R7 in R14 & ( R7 /\ R5 ) c= R6)))
proof
defpred S2[ set ] means (ex R9 being (Subset of R3) st (R9 in { ( R8 /\ R5 ) where R8 is (Subset of R3) : R8 in R14 } & R9 c= $1));
assume L129: R6 in ( Extend_Filter (R14 , R5) );
L130: R6 in { R4 where R4 is (Subset of R3) : S2[ R4 ] } by L129;
L131: S2[ R6 ] from ElemProp(L130);
consider R9 being (Subset of R3) such that L132: R9 in { ( R8 /\ R5 ) where R8 is (Subset of R3) : R8 in R14 } and L133: R9 c= R6 by L131;
consider R10 being (Subset of R3) such that L134: R9 = ( R10 /\ R5 ) and L135: R10 in R14 by L132;
take R10;
thus L136: R10 in R14 by L135;
thus L137: thesis by L133 , L134;
end;

given R7 being (Subset of R3) such that
L138: R7 in R14
and
L139: ( R7 /\ R5 ) c= R6;

L140: (ex R9 being (Subset of R3) st (R9 in { ( R8 /\ R5 ) where R8 is (Subset of R3) : R8 in R14 } & R9 c= R6))
proof
take ( R7 /\ R5 );
thus L141: ( R7 /\ R5 ) in { ( R8 /\ R5 ) where R8 is (Subset of R3) : R8 in R14 } by L138;
thus L142: thesis by L139;
end;
thus L143: thesis by L140;
end;
theorem
L144: (for R3 being non  empty set holds (for R5 being (Subset of R3) holds (for R14 being (Filter of R3) holds ((for R8 being (Subset of R3) holds (R8 in R14 implies R8 meets R5)) implies (R5 in ( Extend_Filter (R14 , R5) ) & ( Extend_Filter (R14 , R5) ) is (Filter of R3) & R14 c= ( Extend_Filter (R14 , R5) ))))))
proof
let R3 being non  empty set;
let R5 being (Subset of R3);
let R14 being (Filter of R3);
assume L145: (for R8 being (Subset of R3) holds (R8 in R14 implies R8 meets R5));
L146: (R3 in R14 & ( R3 /\ R5 ) c= R5) by L26 , XBOOLE_1:17;
thus L147: R5 in ( Extend_Filter (R14 , R5) ) by L146 , L127;
thus L148: ( Extend_Filter (R14 , R5) ) is (Filter of R3)
proof
thus L149: (not ( {} ) in ( Extend_Filter (R14 , R5) ))
proof
assume L150: ( {} ) in ( Extend_Filter (R14 , R5) );
consider R7 being (Subset of R3) such that L151: R7 in R14 and L152: ( R7 /\ R5 ) c= ( {} ) by L150 , L127;
L153: R7 meets R5 by L145 , L151;
L154: ( R7 /\ R5 ) <> ( {} ) by L153 , XBOOLE_0:def 7;
thus L155: contradiction by L154 , L152;
end;

let R8 being (Subset of R3);
let R9 being (Subset of R3);
thus L156: ((R8 in ( Extend_Filter (R14 , R5) ) & R9 in ( Extend_Filter (R14 , R5) )) implies ( R8 /\ R9 ) in ( Extend_Filter (R14 , R5) ))
proof
assume that
L157: R8 in ( Extend_Filter (R14 , R5) )
and
L158: R9 in ( Extend_Filter (R14 , R5) );
consider R10 being (Subset of R3) such that L159: R10 in R14 and L160: ( R10 /\ R5 ) c= R8 by L157 , L127;
consider R11 being (Subset of R3) such that L161: R11 in R14 and L162: ( R11 /\ R5 ) c= R9 by L158 , L127;
L163: ( ( R10 /\ R5 ) /\ ( R11 /\ R5 ) ) = ( R10 /\ ( R5 /\ ( R11 /\ R5 ) ) ) by XBOOLE_1:16
.= ( R10 /\ ( R11 /\ ( R5 /\ R5 ) ) ) by XBOOLE_1:16
.= ( ( R10 /\ R11 ) /\ R5 ) by XBOOLE_1:16;
L164: ( ( R10 /\ R11 ) /\ R5 ) c= ( R8 /\ R9 ) by L163 , L160 , L162 , XBOOLE_1:27;
L165: ( R10 /\ R11 ) in R14 by L159 , L161 , L18;
thus L166: thesis by L165 , L164 , L127;
end;

thus L167: ((R8 in ( Extend_Filter (R14 , R5) ) & R8 c= R9) implies R9 in ( Extend_Filter (R14 , R5) ))
proof
L168: ( R3 \ R5 ) misses R5 by XBOOLE_1:79;
L169: ( ( R9 \/ ( R3 \ R5 ) ) /\ R5 ) = ( ( R9 /\ R5 ) \/ ( ( R3 \ R5 ) /\ R5 ) ) by XBOOLE_1:23
.= ( ( R9 /\ R5 ) \/ ( {} ) ) by L168 , XBOOLE_0:def 7
.= ( R9 /\ R5 );
L170: ( ( R9 \/ ( R3 \ R5 ) ) /\ R5 ) c= R9 by L169 , XBOOLE_1:17;
assume that
L171: R8 in ( Extend_Filter (R14 , R5) )
