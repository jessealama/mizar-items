definition
let R3 being non  empty set;
let R14 being (Filter of R3);
let R5 being (Subset of R3);
func Extend_Filter (R14 , R5) -> non  empty (Subset-Family of R3) equals 
{ R4 where R4 is (Subset of R3) : (ex R9 being (Subset of R3) st (R9 in { ( R8 /\ R5 ) where R8 is (Subset of R3) : R8 in R14 } & R9 c= R4)) };
