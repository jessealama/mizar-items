theorem
L1: (for R1 being AffinSpace holds (for R5 being (Subset of R1) holds ((R1 is AffinPlane & R5 = (the carrier of R1)) implies R5 is  being_plane)))
proof
let R1 being AffinSpace;
let R5 being (Subset of R1);
assume that
L2: R1 is AffinPlane
and
L3: R5 = (the carrier of R1);
consider C1 , C2 , C3 being (Element of R1) such that L4: (not  LIN C1 , C2 , C3) by AFF_1:12;
set D1 = ( Line (C1 , C2) );
set D2 = ( Line (C1 , C3) );
L5: C2 in D1 by AFF_1:15;
L6: C3 in D2 by AFF_1:15;
L7: C1 <> C2 by L4 , AFF_1:7;
L8: D1 is  being_line by L7 , AFF_1:def 3;
set D3 = ( Plane (D2 , D1) );
L9: C1 in D1 by AFF_1:15;
L10: C1 <> C3 by L4 , AFF_1:7;
L11: D2 is  being_line by L10 , AFF_1:def 3;
L12: C1 in D2 by AFF_1:15;
L13: (not D2 // D1)
proof
assume L14: D2 // D1;
L15: C3 in D1 by L14 , L9 , L12 , L6 , AFF_1:45;
thus L16: contradiction by L15 , L4 , L9 , L5 , L8 , AFF_1:21;
end;
L17:
now
let R11 being set;
assume L18: R11 in R5;
reconsider D4 = R11 as (Element of R1) by L18;
set D5 = ( D4 * D2 );
L19: D5 is  being_line by L11 , AFF_4:27;
L20: D2 // D5 by L11 , AFF_4:def 3;
L21: (not D5 // D1) by L20 , L13 , AFF_1:44;
consider C4 being (Element of R1) such that L22: C4 in D5 and L23: C4 in D1 by L21 , L2 , L8 , L19 , AFF_1:58;
L24: D4 in D5 by L11 , AFF_4:def 3;
L25: D4 , C4 // D2 by L24 , L20 , L22 , AFF_1:40;
L26: D4 in { R10 where R10 is (Element of R1) : (ex B1 being (Element of R1) st (R10 , B1 // D2 & B1 in D1)) } by L25 , L23;
thus L27: R11 in D3 by L26 , AFF_4:def 1;
end;
L28: R5 c= D3 by L17 , TARSKI:def 3;
L29: D3 is  being_plane by L8 , L11 , L13 , AFF_4:def 2;
thus L30: thesis by L29 , L3 , L28 , XBOOLE_0:def 10;
end;
