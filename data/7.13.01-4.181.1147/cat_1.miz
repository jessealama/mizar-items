definition
struct (MultiGraphStruct) CatStr(# carrier , carrier' -> set , Source , Target -> (Function of the carrier' , the carrier) , Comp -> (PartFunc of [: the carrier' , the carrier' :] , the carrier') #);
end;
definition
let R1 being CatStr;
mode Object of R1
 is (Element of R1);
mode Morphism of R1
 is (Element of (the carrier' of R1));
end;
registration
cluster non  void non  empty for CatStr;
existence
proof
take CatStr (# { ( 0 ) } , { 1 } , ( 1 :-> ( 0 ) ) , ( 1 :-> ( 0 ) ) , ( (1 , 1) :-> 1 ) #);
thus L3: thesis;
end;
end;
definition
let R1 being CatStr;
let R2 being (Morphism of R1);
let R3 being (Morphism of R1);
assume L5: [ R3 , R2 ] in ( dom (the Comp of R1) );
func R3 (*) R2 -> (Morphism of R1) equals 
:L6: ( (the Comp of R1) . (R3 , R2) );
coherence by L5 , PARTFUN1:4;
end;
definition
canceled 2;
let C1 being non  void non  empty CatStr;
let C2 , C3 being (Object of C1);
func Hom (C2 , C3) -> (Subset of (the carrier' of C1)) equals 
{ B1 where B1 is (Morphism of C1) : (( dom B1 ) = C2 & ( cod B1 ) = C3) };
correctness
proof
set D1 = { B2 where B2 is (Morphism of C1) : (( dom B2 ) = C2 & ( cod B2 ) = C3) };
L8: D1 c= (the carrier' of C1)
proof
let C4 being set;
assume L9: C4 in D1;
L10: (ex B3 being (Morphism of C1) st (C4 = B3 & ( dom B3 ) = C2 & ( cod B3 ) = C3)) by L9;
thus L11: thesis by L10;
end;
thus L12: thesis by L8;
end;
end;
theorem
L14: (for R4 being non  void non  empty CatStr holds (for R5 being (Morphism of R4) holds (for R7 being (Object of R4) holds (for R8 being (Object of R4) holds (R5 in ( Hom (R7 , R8) ) iff (( dom R5 ) = R7 & ( cod R5 ) = R8))))))
proof
