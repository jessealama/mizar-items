let R5 being (Morphism of R4);
let R7 being (Object of R4);
let R8 being (Object of R4);
thus L15: (R5 in ( Hom (R7 , R8) ) implies (( dom R5 ) = R7 & ( cod R5 ) = R8))
proof
assume L16: R5 in ( Hom (R7 , R8) );
L17: (ex R6 being (Morphism of R4) st (R5 = R6 & ( dom R6 ) = R7 & ( cod R6 ) = R8)) by L16;
thus L18: thesis by L17;
end;

thus L19: thesis;
end;
