theorem
L1: (for R1 being Lattice holds (for R4 being (Filter of R1) holds (for R5 being (Filter of R1) holds ( R4 /\ R5 ) is (Filter of R1))))
proof
let R1 being Lattice;
let R4 being (Filter of R1);
let R5 being (Filter of R1);
consider R6 being (Element of R1) such that L2: R6 in R4 by SUBSET_1:4;
consider R7 being (Element of R1) such that L3: R7 in R5 by SUBSET_1:4;
L4: ( R6 "\/" R7 ) in R5 by L3 , FILTER_0:10;
L5: ( R6 "\/" R7 ) in R4 by L2 , FILTER_0:10;
reconsider D1 = ( R4 /\ R5 ) as non  empty (Subset of R1) by L5 , L4 , XBOOLE_0:def 4;
L6:
now
let R6 being (Element of R1);
let R7 being (Element of R1);
L7: ((( R6 "/\" R7 ) in R4 & ( R6 "/\" R7 ) in R5) iff (R6 in R4 & R7 in R4 & R6 in R5 & R7 in R5)) by FILTER_0:8;
thus L8: ((R6 in ( R4 /\ R5 ) & R7 in ( R4 /\ R5 )) iff ( R6 "/\" R7 ) in ( R4 /\ R5 )) by L7 , XBOOLE_0:def 4;
end;
L9: D1 is (Filter of R1) by L6 , FILTER_0:8;
thus L10: thesis by L9;
end;
theorem
L11: (for R1 being Lattice holds (for R6 being (Element of R1) holds (for R7 being (Element of R1) holds (<. R6 .) = <. R7 .) implies R6 = R7))))
proof
let R1 being Lattice;
let R6 being (Element of R1);
let R7 being (Element of R1);
assume L12: <. R6 .) = <. R7 .);
L13: R7 in <. R6 .) by L12;
L14: R6 [= R7 by L13 , FILTER_0:15;
L15: R6 in <. R7 .) by L12;
L16: R7 [= R6 by L15 , FILTER_0:15;
thus L17: thesis by L16 , L14 , LATTICES:8;
end;
