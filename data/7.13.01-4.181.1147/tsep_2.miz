antonym C7 , C8 do_not_constitute_a_decomposition  for C7 , C8 constitute_a_decomposition ;
end;
theorem
L8: (for R1 being non  empty 1-sorted holds (for R3 being (Subset of R1) holds (for R4 being (Subset of R1) holds (R3 , R4 constitute_a_decomposition  iff (R3 misses R4 & ( R3 \/ R4 ) = ( [#] R1 )))))) by L5;
theorem
L9: (for R1 being non  empty 1-sorted holds (for R3 being (Subset of R1) holds (for R4 being (Subset of R1) holds (R3 , R4 constitute_a_decomposition  implies (R3 = ( R4 ` ) & R4 = ( R3 ` ))))))
proof
let R1 being non  empty 1-sorted;
let R3 being (Subset of R1);
let R4 being (Subset of R1);
assume L10: R3 , R4 constitute_a_decomposition ;
L11: R3 misses R4 by L10 , L5;
L12: R3 c= ( R4 ` ) by L11 , SUBSET_1:23;
L13: R4 c= ( R3 ` ) by L11 , SUBSET_1:23;
L14: ( R3 \/ R4 ) = ( [#] R1 ) by L10 , L5;
L15: ( R4 ` ) c= R3 by L14 , TDLAT_1:1;
thus L16: R3 = ( R4 ` ) by L15 , L12 , XBOOLE_0:def 10;
L17: ( R3 ` ) c= R4 by L14 , TDLAT_1:1;
thus L18: thesis by L17 , L13 , XBOOLE_0:def 10;
end;
theorem
L19: (for R1 being non  empty 1-sorted holds (for R3 being (Subset of R1) holds (for R4 being (Subset of R1) holds ((R3 = ( R4 ` ) or R4 = ( R3 ` )) implies R3 , R4 constitute_a_decomposition ))))
proof
