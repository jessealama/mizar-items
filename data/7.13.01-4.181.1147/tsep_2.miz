theorem
L61: (for R7 being non  empty TopSpace holds (for B3 , B4 being (Subset of R7) holds (B3 , B4 constitute_a_decomposition  implies (B3 is  open iff B4 is  closed))))
proof
let R7 being non  empty TopSpace;
let C12 , C13 being (Subset of R7);
assume L62: C12 , C13 constitute_a_decomposition ;
L63: C13 = ( C12 ` ) by L62 , L9;
thus L64: (C12 is  open implies C13 is  closed) by L63 , TOPS_1:4;
assume L65: C13 is  closed;
L66: C12 = ( C13 ` ) by L62 , L9;
thus L67: thesis by L66 , L65 , TOPS_1:3;
end;
theorem
L68: (for R7 being non  empty TopSpace holds (for B5 , B6 being (Subset of R7) holds (B5 , B6 constitute_a_decomposition  implies (B5 is  closed iff B6 is  open)))) by L61;
theorem
L69: (for R13 being non  empty 1-sorted holds (for R15 being (Subset of R13) holds (for R16 being (Subset of R13) holds (for R17 being (Subset of R13) holds (for R18 being (Subset of R13) holds ((R15 , R16 constitute_a_decomposition  & R17 , R18 constitute_a_decomposition ) implies ( R15 /\ R17 ) , ( R16 \/ R18 ) constitute_a_decomposition ))))))
proof
let R13 being non  empty 1-sorted;
let R15 being (Subset of R13);
let R16 being (Subset of R13);
let R17 being (Subset of R13);
let R18 being (Subset of R13);
assume L70: R15 , R16 constitute_a_decomposition ;
L71: R15 misses R16 by L70 , L5;
L72: ( R15 /\ R16 ) = ( {} ) by L71 , XBOOLE_0:def 7;
assume L73: R17 , R18 constitute_a_decomposition ;
L74: ( R17 \/ R18 ) = ( [#] R13 ) by L73 , L5;
L75: R17 misses R18 by L73 , L5;
L76: ( R17 /\ R18 ) = ( {} R13 ) by L75 , XBOOLE_0:def 7;
L77: ( ( R15 /\ R17 ) /\ ( R16 \/ R18 ) ) = ( ( ( R17 /\ R15 ) /\ R16 ) \/ ( ( R15 /\ R17 ) /\ R18 ) ) by XBOOLE_1:23
.= ( ( R17 /\ ( R15 /\ R16 ) ) \/ ( ( R15 /\ R17 ) /\ R18 ) ) by XBOOLE_1:16
.= ( ( R17 /\ ( R15 /\ R16 ) ) \/ ( R15 /\ ( R17 /\ R18 ) ) ) by XBOOLE_1:16
.= ( {} R13 ) by L76 , L72;
L78: ( R15 /\ R17 ) misses ( R16 \/ R18 ) by L77 , XBOOLE_0:def 7;
L79: ( ( R15 /\ R17 ) \/ ( R16 \/ R18 ) ) = ( ( R15 \/ ( R16 \/ R18 ) ) /\ ( R17 \/ ( R16 \/ R18 ) ) ) by XBOOLE_1:24
.= ( ( ( R15 \/ R16 ) \/ R18 ) /\ ( R17 \/ ( R18 \/ R16 ) ) ) by XBOOLE_1:4
.= ( ( ( R15 \/ R16 ) \/ R18 ) /\ ( ( R17 \/ R18 ) \/ R16 ) ) by XBOOLE_1:4
.= ( ( ( [#] R13 ) \/ R18 ) /\ ( ( [#] R13 ) \/ R16 ) ) by L70 , L74 , L5
.= ( ( ( [#] R13 ) \/ R18 ) /\ ( [#] R13 ) ) by XBOOLE_1:12
.= ( ( [#] R13 ) /\ ( [#] R13 ) ) by XBOOLE_1:12
.= ( [#] R13 );
thus L80: thesis by L79 , L78 , L5;
end;
theorem
L81: (for R13 being non  empty 1-sorted holds (for R15 being (Subset of R13) holds (for R16 being (Subset of R13) holds (for R17 being (Subset of R13) holds (for R18 being (Subset of R13) holds ((R15 , R16 constitute_a_decomposition  & R17 , R18 constitute_a_decomposition ) implies ( R15 \/ R17 ) , ( R16 /\ R18 ) constitute_a_decomposition )))))) by L69;
