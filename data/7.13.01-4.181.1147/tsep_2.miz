theorem
L29: (for R1 being non  empty 1-sorted holds (for R2 being (Subset of R1) holds R2 , R2 do_not_constitute_a_decomposition ))
proof
let R1 being non  empty 1-sorted;
let R2 being (Subset of R1);
assume L30: R2 , R2 constitute_a_decomposition ;
per cases ;
suppose L31: R2 is non  empty;

L32: R2 = ( R2 ` ) by L30 , L9;
L33: R2 misses R2 by L32 , SUBSET_1:23;
L34: ( R2 /\ R2 ) = ( {} ) by L33 , XBOOLE_0:def 7;
thus L35: contradiction by L34 , L31;
end;
suppose L36: R2 is  empty;

L37: ( {} ) = ( R2 \/ R2 ) by L36
.= (the carrier of R1) by L30 , L5;
thus L38: contradiction by L37;
end;
end;
definition
let C9 being non  empty 1-sorted;
let C10 , C11 being (Subset of C9);
redefine pred C10 , C11 constitute_a_decomposition 
;
irreflexivity
