theorem
L1: (for R1 being non  empty 1-sorted holds (for B1 , B2 being (Subset of R1) holds ( ( B1 ` ) \ ( B2 ` ) ) = ( B2 \ B1 )))
proof
let R1 being non  empty 1-sorted;
let C1 , C2 being (Subset of R1);
L2: ( ( C1 ` ) \ ( C2 ` ) ) = ( ( [#] R1 ) \ ( C1 \/ ( C2 ` ) ) ) by XBOOLE_1:41;
L3: ( ( C1 ` ) \ ( C2 ` ) ) = ( ( C1 ` ) /\ ( ( C2 ` ) ` ) ) by L2 , XBOOLE_1:53;
thus L4: thesis by L3 , SUBSET_1:13;
end;
definition
let C3 being 1-sorted;
let C4 , C5 being (Subset of C3);
pred C4 , C5 constitute_a_decomposition 
means
:L5: (C4 misses C5 & ( C4 \/ C5 ) = (the carrier of C3))
;symmetry
;
end;
notation
let C6 being 1-sorted;
