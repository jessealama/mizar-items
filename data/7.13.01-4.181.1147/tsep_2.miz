theorem
L154: (for R19 being non  empty TopSpace holds (for R20 being (Subset of R19) holds (for R21 being (Subset of R19) holds (for R22 being non  empty (SubSpace of R19) holds (for R23 being (Subset of R22) holds (for R24 being (Subset of R22) holds ((R23 = R20 & R24 = R21) implies (R20 , R21 are_separated  iff R23 , R24 are_separated ))))))))
proof
let R19 being non  empty TopSpace;
let R20 being (Subset of R19);
let R21 being (Subset of R19);
let R22 being non  empty (SubSpace of R19);
let R23 being (Subset of R22);
let R24 being (Subset of R22);
assume that
L155: R23 = R20
and
L156: R24 = R21;
L157: ( Cl R24 ) = ( ( Cl R21 ) /\ ( [#] R22 ) ) by L156 , PRE_TOPC:17;
L158: ( Cl R23 ) = ( ( Cl R20 ) /\ ( [#] R22 ) ) by L155 , PRE_TOPC:17;
thus L159: (R20 , R21 are_separated  implies R23 , R24 are_separated )
proof
assume L160: R20 , R21 are_separated ;
L161: R20 misses ( Cl R21 ) by L160 , CONNSP_1:def 1;
L162: ( R20 /\ ( Cl R21 ) ) = ( {} ) by L161 , XBOOLE_0:def 7;
L163: ( R23 /\ ( Cl R24 ) ) = ( ( {} ) /\ ( [#] R22 ) ) by L162 , L155 , L157 , XBOOLE_1:16;
L164: R23 misses ( Cl R24 ) by L163 , XBOOLE_0:def 7;
L165: ( Cl R20 ) misses R21 by L160 , CONNSP_1:def 1;
L166: ( ( Cl R20 ) /\ R21 ) = ( {} ) by L165 , XBOOLE_0:def 7;
L167: ( ( Cl R23 ) /\ R24 ) = ( ( {} ) /\ ( [#] R22 ) ) by L166 , L156 , L158 , XBOOLE_1:16;
L168: ( Cl R23 ) misses R24 by L167 , XBOOLE_0:def 7;
