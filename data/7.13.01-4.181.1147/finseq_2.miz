let R2 being Nat;
let R25 being FinSequence;
assume L106: ( len R25 ) = ( R1 + R2 );
reconsider D1 = R1 as (Element of ( NAT )) by ORDINAL1:def 12;
reconsider D2 = ( R25 | ( Seg D1 ) ) as FinSequence by FINSEQ_1:15;
consider C7 being FinSequence such that L107: R25 = ( D2 ^ C7 ) by FINSEQ_1:80;
take D2;
take C7;
L108: R1 <= ( len R25 ) by L106 , NAT_1:11;
thus L109: ( len D2 ) = R1 by L108 , FINSEQ_1:17;
L110: ( len ( D2 ^ C7 ) ) = ( R1 + ( len C7 ) ) by L109 , FINSEQ_1:22;
thus L111: ( len C7 ) = R2 by L110 , L106 , L107;
thus L112: thesis by L107;
end;
theorem
