scheme FraenkelInvolution { F1() -> non  empty set , F2 , F3() -> (Subset of F1()) , F4(set) -> (Element of F1()) } : F2() = { F4(B1) where B1 is (Element of F1()) : B1 in F3() }
provided
L1: F3() = { F4(B2) where B2 is (Element of F1()) : B2 in F2() }
and
L2: (for B3 being (Element of F1()) holds F4(F4(B3)) = B3)
proof
thus L3:now
let C1 being set;
assume L4: C1 in F2();
reconsider D1 = C1 as (Element of F1()) by L4;
L5: F4(D1) in F3() by L1 , L4;
L6: F4(F4(D1)) = D1 by L2;
thus L7: C1 in { F4(B4) where B4 is (Element of F1()) : B4 in F3() } by L6 , L5;
end;
let C2 being set;
assume L8: C2 in { F4(B5) where B5 is (Element of F1()) : B5 in F3() };
consider C3 being (Element of F1()) such that L9: C2 = F4(C3) and L10: C3 in F3() by L8;
L11: (ex B6 being (Element of F1()) st (C3 = F4(B6) & B6 in F2())) by L1 , L10;
thus L12: thesis by L11 , L2 , L9;
end;
scheme FraenkelComplement1 { F5() -> non  empty RelStr , F6() -> (Subset-Family of F5()) , F7() -> set , F8(set) -> (Subset of F5()) } : ( COMPLEMENT F6() ) = { ( F8(B7) ` ) where B7 is (Element of F5()) : B7 in F7() }
