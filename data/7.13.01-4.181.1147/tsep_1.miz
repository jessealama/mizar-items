begin
L1: (for B1 being set holds (for B2 , B3 , B4 being (Subset of B1) holds (( B2 \ B3 ) = ( {} ) implies B2 misses ( B4 \ B3 ))))
proof
let C1 being set;
let C2 , C3 , C4 being (Subset of C1);
assume L2: ( C2 \ C3 ) = ( {} );
L3: C2 c= C3 by L2 , XBOOLE_1:37;
L4: C3 misses ( C4 \ C3 ) by XBOOLE_1:79;
thus L5: thesis by L4 , L3 , XBOOLE_1:63;
end;
L6: (for B5 , B6 , B7 being set holds ( ( B5 /\ B6 ) \ B7 ) = ( ( B5 \ B7 ) /\ ( B6 \ B7 ) ))
proof
let C5 , C6 , C7 being set;
L7: ( C5 \ C7 ) misses C7 by XBOOLE_1:79;
thus L8: ( ( C5 /\ C6 ) \ C7 ) = ( ( C5 \ C7 ) /\ C6 ) by XBOOLE_1:49
.= ( ( C5 \ C7 ) \ ( ( C5 \ C7 ) \ C6 ) ) by XBOOLE_1:48
.= ( ( C5 \ C7 ) \ ( C5 \ ( C7 \/ C6 ) ) ) by XBOOLE_1:41
.= ( ( ( C5 \ C7 ) \ C5 ) \/ ( ( C5 \ C7 ) /\ ( C7 \/ C6 ) ) ) by XBOOLE_1:52
.= ( ( {} ) \/ ( ( C5 \ C7 ) /\ ( C7 \/ C6 ) ) ) by XBOOLE_1:37
.= ( ( C5 \ C7 ) /\ ( ( C6 \ C7 ) \/ C7 ) ) by XBOOLE_1:39
.= ( ( ( C5 \ C7 ) /\ ( C6 \ C7 ) ) \/ ( ( C5 \ C7 ) /\ C7 ) ) by XBOOLE_1:23
.= ( ( ( C5 \ C7 ) /\ ( C6 \ C7 ) ) \/ ( {} ) ) by L7 , XBOOLE_0:def 7
.= ( ( C5 \ C7 ) /\ ( C6 \ C7 ) );
end;
theorem
L9: (for B8 being TopStruct holds (for B9 being (SubSpace of B8) holds (the carrier of B9) is (Subset of B8)))
proof
let C8 being TopStruct;
let C9 being (SubSpace of C8);
reconsider D1 = ( [#] C9 ) as (Subset of ( [#] C8 )) by PRE_TOPC:def 4;
L10: D1 c= ( [#] C8 );
thus L11: thesis by L10;
end;
theorem
L12: (for B10 being TopStruct holds B10 is (SubSpace of B10))
proof
let C10 being TopStruct;
thus L13: ( [#] C10 ) c= ( [#] C10 );
thus L14: (for B11 being (Subset of C10) holds (B11 in (the topology of C10) iff (ex B12 being (Subset of C10) st (B12 in (the topology of C10) & B11 = ( B12 /\ ( [#] C10 ) )))))
proof
let C11 being (Subset of C10);
thus L15: (C11 in (the topology of C10) implies (ex B13 being (Subset of C10) st (B13 in (the topology of C10) & C11 = ( B13 /\ ( [#] C10 ) ))))
proof
assume L16: C11 in (the topology of C10);
take C11;
thus L17: thesis by L16 , XBOOLE_1:28;
end;

thus L18: thesis by XBOOLE_1:28;
end;

end;
theorem
L15: (for B14 being  strict TopStruct holds ( B14 | ( [#] B14 ) ) = B14)
proof
let C12 being  strict TopStruct;
reconsider D2 = C12 as (SubSpace of C12) by L12;
reconsider D3 = ( [#] D2 ) as (Subset of C12);
L16: ( C12 | D3 ) = D2 by PRE_TOPC:def 5;
thus L17: thesis by L16;
end;
theorem
L18: (for R1 being TopSpace holds (for B15 , B16 being (SubSpace of R1) holds ((the carrier of B15) c= (the carrier of B16) iff B15 is (SubSpace of B16))))
proof
let R1 being TopSpace;
let C13 , C14 being (SubSpace of R1);
set D4 = (the carrier of C13);
set D5 = (the carrier of C14);
L19: D4 = ( [#] C13 );
L20: D5 = ( [#] C14 );
thus L21: ((the carrier of C13) c= (the carrier of C14) implies C13 is (SubSpace of C14))
proof
assume L22: D4 c= D5;
L23: (for B17 being (Subset of C13) holds (B17 in (the topology of C13) iff (ex B18 being (Subset of C14) st (B18 in (the topology of C14) & B17 = ( B18 /\ D4 )))))
proof
let C15 being (Subset of C13);
thus L24: (C15 in (the topology of C13) implies (ex B19 being (Subset of C14) st (B19 in (the topology of C14) & C15 = ( B19 /\ D4 ))))
proof
assume L25: C15 in (the topology of C13);
consider C16 being (Subset of R1) such that L26: C16 in (the topology of R1) and L27: C15 = ( C16 /\ D4 ) by L25 , L19 , PRE_TOPC:def 4;
reconsider D6 = ( C16 /\ D5 ) as (Subset of C14) by XBOOLE_1:17;
take D6;
thus L28: D6 in (the topology of C14) by L20 , L26 , PRE_TOPC:def 4;
L29: ( D6 /\ D4 ) = ( C16 /\ ( D5 /\ D4 ) ) by XBOOLE_1:16
.= ( C16 /\ D4 ) by L22 , XBOOLE_1:28;
thus L30: thesis by L29 , L27;
end;

given C17 being (Subset of C14) such that
L31: C17 in (the topology of C14)
and
L32: C15 = ( C17 /\ D4 );

consider C18 being (Subset of R1) such that L33: C18 in (the topology of R1) and L34: C17 = ( C18 /\ ( [#] C14 ) ) by L31 , PRE_TOPC:def 4;
L35: C15 = ( C18 /\ ( D5 /\ D4 ) ) by L32 , L34 , XBOOLE_1:16
.= ( C18 /\ D4 ) by L22 , XBOOLE_1:28;
thus L36: thesis by L35 , L19 , L33 , PRE_TOPC:def 4;
end;
thus L37: thesis by L23 , L19 , L20 , L22 , PRE_TOPC:def 4;
end;

thus L38: thesis by L19 , L20 , PRE_TOPC:def 4;
end;
L39: (for B20 being TopStruct holds (for B21 being (SubSpace of B20) holds ( the TopStruct of B21 ) is  strict  strict  strict  strict (SubSpace of B20)))
proof
let C19 being TopStruct;
let C20 being (SubSpace of C19);
set D7 = ( the TopStruct of C20 );
L40: D7 is (SubSpace of C19)
proof
L41: ( [#] C20 ) = (the carrier of C20);
thus L42: ( [#] D7 ) c= ( [#] C19 ) by L41 , PRE_TOPC:def 4;
let C21 being (Subset of D7);
thus L43: (C21 in (the topology of D7) implies (ex B22 being (Subset of C19) st (B22 in (the topology of C19) & C21 = ( B22 /\ ( [#] D7 ) )))) by L41 , PRE_TOPC:def 4;
given C22 being (Subset of C19) such that
L44: (C22 in (the topology of C19) & C21 = ( C22 /\ ( [#] D7 ) ));

thus L45: thesis by L41 , L44 , PRE_TOPC:def 4;
end;
thus L46: thesis by L40;
end;
theorem
L47: (for B23 being TopStruct holds (for B24 , B25 being (SubSpace of B23) holds ((the carrier of B24) = (the carrier of B25) implies ( the TopStruct of B24 ) = ( the TopStruct of B25 ))))
proof
let C23 being TopStruct;
let C24 , C25 being (SubSpace of C23);
reconsider D8 = ( the TopStruct of C24 ) , D9 = ( the TopStruct of C25 ) as  strict (SubSpace of C23) by L39;
set D10 = (the carrier of C24);
set D11 = (the carrier of C25);
assume L48: D10 = D11;
L49: D10 = ( [#] D8 );
L50: D11 = ( [#] D9 );
reconsider D12 = D10 as (Subset of C23) by BORSUK_1:1;
L51: D8 = ( C23 | D12 ) by L49 , PRE_TOPC:def 5;
thus L52: thesis by L51 , L48 , L50 , PRE_TOPC:def 5;
end;
theorem
L53: (for R1 being TopSpace holds (for B26 , B27 being (SubSpace of R1) holds ((B26 is (SubSpace of B27) & B27 is (SubSpace of B26)) implies ( the TopStruct of B26 ) = ( the TopStruct of B27 ))))
proof
let R1 being TopSpace;
let C26 , C27 being (SubSpace of R1);
set D13 = (the carrier of C26);
set D14 = (the carrier of C27);
assume L54: (C26 is (SubSpace of C27) & C27 is (SubSpace of C26));
L55: (D13 c= D14 & D14 c= D13) by L54 , L18;
L56: D13 = D14 by L55 , XBOOLE_0:def 10;
thus L57: thesis by L56 , L47;
end;
theorem
L58: (for R1 being TopSpace holds (for B28 being (SubSpace of R1) holds (for B29 being (SubSpace of B28) holds B29 is (SubSpace of R1))))
proof
let R1 being TopSpace;
let C28 being (SubSpace of R1);
let C29 being (SubSpace of C28);
L59: ( [#] C29 ) c= ( [#] C28 ) by PRE_TOPC:def 4;
L60: ( [#] C28 ) c= ( [#] R1 ) by PRE_TOPC:def 4;
thus L61: ( [#] C29 ) c= ( [#] R1 ) by L60 , L59 , XBOOLE_1:1;
thus L62: (for B30 being (Subset of C29) holds (B30 in (the topology of C29) iff (ex B31 being (Subset of R1) st (B31 in (the topology of R1) & B30 = ( B31 /\ ( [#] C29 ) )))))
proof
let C30 being (Subset of C29);
reconsider D15 = C30 as (Subset of C29);
thus L63: (C30 in (the topology of C29) implies (ex B32 being (Subset of R1) st (B32 in (the topology of R1) & C30 = ( B32 /\ ( [#] C29 ) ))))
proof
assume L64: C30 in (the topology of C29);
consider C31 being (Subset of C28) such that L65: C31 in (the topology of C28) and L66: C30 = ( C31 /\ ( [#] C29 ) ) by L64 , PRE_TOPC:def 4;
consider C32 being (Subset of R1) such that L67: C32 in (the topology of R1) and L68: C31 = ( C32 /\ ( [#] C28 ) ) by L65 , PRE_TOPC:def 4;
L69: ( C32 /\ ( [#] C29 ) ) = ( C32 /\ ( ( [#] C28 ) /\ ( [#] C29 ) ) ) by L59 , XBOOLE_1:28
.= C30 by L66 , L68 , XBOOLE_1:16;
thus L70: thesis by L69 , L67;
end;

given C33 being (Subset of R1) such that
L71: C33 in (the topology of R1)
and
L72: C30 = ( C33 /\ ( [#] C29 ) );

reconsider D16 = ( C33 /\ ( [#] C28 ) ) as (Subset of C28);
reconsider D17 = C33 as (Subset of R1);
L73: D17 is  open by L71 , PRE_TOPC:def 2;
L74: D16 is  open by L73 , TOPS_2:24;
L75: ( D16 /\ ( [#] C29 ) ) = ( C33 /\ ( ( [#] C28 ) /\ ( [#] C29 ) ) ) by XBOOLE_1:16
.= C30 by L59 , L72 , XBOOLE_1:28;
L76: D15 is  open by L75 , L74 , TOPS_2:24;
thus L77: thesis by L76 , PRE_TOPC:def 2;
end;

end;
theorem
L63: (for R1 being TopSpace holds (for B33 being (SubSpace of R1) holds (for B34 , B35 being (Subset of R1) holds (for B36 being (Subset of B33) holds ((B34 is  closed & B34 c= (the carrier of B33) & B35 c= B34 & B35 = B36) implies (B36 is  closed iff B35 is  closed))))))
proof
let R1 being TopSpace;
let C34 being (SubSpace of R1);
let C35 , C36 being (Subset of R1);
let C37 being (Subset of C34);
assume that
L64: C35 is  closed
and
L65: C35 c= (the carrier of C34)
and
L66: C36 c= C35
and
L67: C36 = C37;
thus L68: (C37 is  closed implies C36 is  closed)
proof
assume L69: C37 is  closed;
consider C38 being (Subset of R1) such that L70: C38 is  closed and L71: ( C38 /\ ( [#] C34 ) ) = C37 by L69 , PRE_TOPC:13;
L72: C36 c= C38 by L67 , L71 , XBOOLE_1:17;
L73: C36 c= ( C38 /\ C35 ) by L72 , L66 , XBOOLE_1:19;
L74: ( C38 /\ C35 ) c= C36 by L65 , L67 , L71 , XBOOLE_1:26;
thus L75: thesis by L74 , L64 , L70 , L73 , XBOOLE_0:def 10;
end;

thus L76: thesis by L67 , TOPS_2:26;
end;
theorem
L77: (for R1 being TopSpace holds (for B37 being (SubSpace of R1) holds (for B38 , B39 being (Subset of R1) holds (for B40 being (Subset of B37) holds ((B38 is  open & B38 c= (the carrier of B37) & B39 c= B38 & B39 = B40) implies (B40 is  open iff B39 is  open))))))
proof
let R1 being TopSpace;
let C39 being (SubSpace of R1);
let C40 , C41 being (Subset of R1);
let C42 being (Subset of C39);
assume that
L78: C40 is  open
and
L79: C40 c= (the carrier of C39)
and
L80: C41 c= C40
and
L81: C41 = C42;
thus L82: (C42 is  open implies C41 is  open)
proof
assume L83: C42 is  open;
consider C43 being (Subset of R1) such that L84: C43 is  open and L85: ( C43 /\ ( [#] C39 ) ) = C42 by L83 , TOPS_2:24;
L86: C41 c= C43 by L81 , L85 , XBOOLE_1:17;
L87: C41 c= ( C43 /\ C40 ) by L86 , L80 , XBOOLE_1:19;
L88: ( C43 /\ C40 ) c= C41 by L79 , L81 , L85 , XBOOLE_1:26;
thus L89: thesis by L88 , L78 , L84 , L87 , XBOOLE_0:def 10;
end;

thus L90: thesis by L81 , TOPS_2:25;
end;
theorem
L91: (for B41 being non  empty TopStruct holds (for B42 being non  empty (Subset of B41) holds (ex B43 being  strict non  empty (SubSpace of B41) st B42 = (the carrier of B43))))
proof
