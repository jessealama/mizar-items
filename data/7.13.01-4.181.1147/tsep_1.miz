theorem
L153: (for R1 being TopSpace holds (for B67 being  open (SubSpace of R1) holds (for B68 being  open (SubSpace of B67) holds B68 is  open  open  open  open (SubSpace of R1))))
proof
let R1 being TopSpace;
let C75 being  open (SubSpace of R1);
let C76 being  open (SubSpace of C75);
L154:
now
reconsider D30 = ( [#] C75 ) as (Subset of R1) by BORSUK_1:1;
