theorem
L1: (for R7 being set holds R7 c= ( succ R7 )) by XBOOLE_1:7;
theorem
L2: (for R7 being set holds (for R8 being set holds (( succ R7 ) c= R8 implies R7 c= R8)))
proof
let R7 being set;
let R8 being set;
L3: R7 c= ( succ R7 ) by XBOOLE_1:7;
thus L4: thesis by L3 , XBOOLE_1:1;
end;
theorem
L5: (for R3 being Ordinal holds (for R4 being Ordinal holds (R3 in R4 iff ( succ R3 ) in ( succ R4 ))))
proof
let R3 being Ordinal;
let R4 being Ordinal;
L6: (R3 in R4 iff ( succ R3 ) c= R4) by ORDINAL1:21;
thus L7: thesis by L6 , ORDINAL1:22;
end;
theorem
L8: (for R3 being Ordinal holds (for R7 being set holds (R7 c= R3 implies ( union R7 ) is Ordinal)))
proof
let R3 being Ordinal;
let R7 being set;
assume L9: R7 c= R3;
L10: (for R9 being set holds (R9 in R7 implies R9 is Ordinal)) by L9;
thus L11: thesis by L10 , ORDINAL1:23;
end;
theorem
L12: (for R7 being set holds ( union ( On R7 ) ) is Ordinal)
proof
let R7 being set;
L13: (for R9 being set holds (R9 in ( On R7 ) implies R9 is Ordinal)) by ORDINAL1:def 9;
thus L14: thesis by L13 , ORDINAL1:23;
end;
theorem
L15: (for R3 being Ordinal holds (for R7 being set holds (R7 c= R3 implies ( On R7 ) = R7)))
proof
let R3 being Ordinal;
let R7 being set;
defpred S1[ set ] means ($1 in R7 & $1 is Ordinal);
assume L16: R7 c= R3;
L17: (for R9 being set holds (R9 in R7 iff S1[ R9 ])) by L16;
L18: (for R9 being set holds (R9 in ( On R7 ) iff S1[ R9 ])) by ORDINAL1:def 9;
thus L19: thesis from XBOOLE_0:sch 2(L18 , L17);
end;
theorem
L20: (for R3 being Ordinal holds ( On { R3 } ) = { R3 })
proof
let R3 being Ordinal;
L21: { R3 } c= ( succ R3 ) by XBOOLE_1:7;
thus L22: thesis by L21 , L15;
