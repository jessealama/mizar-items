theorem
L23: (for R3 being Ordinal holds (R3 <> ( {} ) implies ( {} ) in R3))
proof
let R3 being Ordinal;
L24: ( {} ) c= R3 by XBOOLE_1:2;
assume L25: R3 <> ( {} );
L26: ( {} ) c< R3 by L25 , L24 , XBOOLE_0:def 8;
thus L27: thesis by L26 , ORDINAL1:11;
end;
theorem
L28: (for R3 being Ordinal holds ( inf R3 ) = ( {} ))
proof
let R3 being Ordinal;
L29: ( inf R3 ) = ( meet R3 ) by ORDINAL2:8;
L30: (R3 <> ( {} ) implies thesis) by L29 , L23 , SETFAM_1:4;
thus L31: thesis by L30 , L29 , SETFAM_1:def 1;
end;
theorem
L32: (for R3 being Ordinal holds ( inf { R3 } ) = R3)
proof
let R3 being Ordinal;
thus L33: ( inf { R3 } ) = ( meet { R3 } ) by L20
.= R3 by SETFAM_1:10;
end;
theorem
L34: (for R3 being Ordinal holds (for R7 being set holds (R7 c= R3 implies ( meet R7 ) is Ordinal)))
proof
