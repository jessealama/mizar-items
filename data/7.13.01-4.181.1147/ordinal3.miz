theorem
L60: (for R3 being Ordinal holds (for R4 being Ordinal holds (for R5 being Ordinal holds (for R6 being Ordinal holds (((R3 c= R4 or R3 in R4) & R5 in R6) implies ( R3 +^ R5 ) in ( R4 +^ R6 ))))))
proof
let R3 being Ordinal;
let R4 being Ordinal;
let R5 being Ordinal;
let R6 being Ordinal;
assume that
L61: (R3 c= R4 or R3 in R4)
and
L62: R5 in R6;
L63: ( R4 +^ R5 ) in ( R4 +^ R6 ) by L62 , ORDINAL2:32;
L64: R3 c= R4 by L61 , ORDINAL1:def 2;
thus L65: thesis by L64 , L63 , ORDINAL1:12 , ORDINAL2:34;
end;
theorem
L66: (for R3 being Ordinal holds (for R4 being Ordinal holds (for R5 being Ordinal holds (for R6 being Ordinal holds ((R3 c= R4 & R5 c= R6) implies ( R3 +^ R5 ) c= ( R4 +^ R6 ))))))
proof
let R3 being Ordinal;
let R4 being Ordinal;
let R5 being Ordinal;
let R6 being Ordinal;
assume that
L67: R3 c= R4
and
L68: R5 c= R6;
L69: ( R4 +^ R5 ) c= ( R4 +^ R6 ) by L68 , ORDINAL2:33;
L70: ( R3 +^ R5 ) c= ( R4 +^ R5 ) by L67 , ORDINAL2:34;
thus L71: thesis by L70 , L69 , XBOOLE_1:1;
end;
theorem
L72: (for R3 being Ordinal holds (for R4 being Ordinal holds (for R5 being Ordinal holds (for R6 being Ordinal holds ((R3 in R4 & ((R5 c= R6 & R6 <> ( {} )) or R5 in R6)) implies ( R3 *^ R5 ) in ( R4 *^ R6 ))))))
proof
let R3 being Ordinal;
let R4 being Ordinal;
let R5 being Ordinal;
let R6 being Ordinal;
assume that
L73: R3 in R4
and
L74: ((R5 c= R6 & R6 <> ( {} )) or R5 in R6);
L75: R5 c= R6 by L74 , ORDINAL1:def 2;
L76: ( R3 *^ R6 ) in ( R4 *^ R6 ) by L73 , L74 , ORDINAL2:40;
thus L77: thesis by L76 , L75 , ORDINAL1:12 , ORDINAL2:42;
end;
theorem
L78: (for R3 being Ordinal holds (for R4 being Ordinal holds (for R5 being Ordinal holds (for R6 being Ordinal holds ((R3 c= R4 & R5 c= R6) implies ( R3 *^ R5 ) c= ( R4 *^ R6 ))))))
proof
let R3 being Ordinal;
let R4 being Ordinal;
let R5 being Ordinal;
let R6 being Ordinal;
assume that
L79: R3 c= R4
and
L80: R5 c= R6;
L81: ( R4 *^ R5 ) c= ( R4 *^ R6 ) by L80 , ORDINAL2:42;
L82: ( R3 *^ R5 ) c= ( R4 *^ R5 ) by L79 , ORDINAL2:41;
thus L83: thesis by L82 , L81 , XBOOLE_1:1;
end;
theorem
L84: (for R4 being Ordinal holds (for R5 being Ordinal holds (for R6 being Ordinal holds (( R4 +^ R5 ) = ( R4 +^ R6 ) implies R5 = R6))))
proof
let R4 being Ordinal;
let R5 being Ordinal;
let R6 being Ordinal;
assume that
L85: ( R4 +^ R5 ) = ( R4 +^ R6 )
and
L86: R5 <> R6;
L87: (R5 in R6 or R6 in R5) by L86 , ORDINAL1:14;
L88: ( R4 +^ R5 ) in ( R4 +^ R5 ) by L87 , L85 , ORDINAL2:32;
thus L89: contradiction by L88;
end;
theorem
L90: (for R4 being Ordinal holds (for R5 being Ordinal holds (for R6 being Ordinal holds (( R4 +^ R5 ) in ( R4 +^ R6 ) implies R5 in R6))))
proof
let R4 being Ordinal;
let R5 being Ordinal;
let R6 being Ordinal;
assume that
L91: ( R4 +^ R5 ) in ( R4 +^ R6 )
and
L92: (not R5 in R6);
L93: R6 c= R5 by L92 , ORDINAL1:16;
thus L94: contradiction by L93 , L91 , ORDINAL1:5 , ORDINAL2:33;
end;
theorem
L95: (for R4 being Ordinal holds (for R5 being Ordinal holds (for R6 being Ordinal holds (( R4 +^ R5 ) c= ( R4 +^ R6 ) implies R5 c= R6))))
proof
let R4 being Ordinal;
let R5 being Ordinal;
let R6 being Ordinal;
assume L96: ( R4 +^ R5 ) c= ( R4 +^ R6 );
L97: ((( R4 +^ R5 ) c= ( R4 +^ R6 ) & ( R4 +^ R5 ) <> ( R4 +^ R6 )) iff ( R4 +^ R5 ) c< ( R4 +^ R6 )) by XBOOLE_0:def 8;
L98: (R5 = R6 or R5 in R6) by L97 , L96 , L84 , L90 , ORDINAL1:11;
thus L99: thesis by L98 , ORDINAL1:def 2;
end;
theorem
L100: (for R3 being Ordinal holds (for R4 being Ordinal holds (R3 c= ( R3 +^ R4 ) & R4 c= ( R3 +^ R4 ))))
proof
let R3 being Ordinal;
let R4 being Ordinal;
L101: ( ( {} ) +^ R4 ) c= ( R3 +^ R4 ) by ORDINAL2:34 , XBOOLE_1:2;
L102: ( R3 +^ ( {} ) ) c= ( R3 +^ R4 ) by ORDINAL2:33 , XBOOLE_1:2;
thus L103: thesis by L102 , L101 , ORDINAL2:27 , ORDINAL2:30;
end;
theorem
L104: (for R3 being Ordinal holds (for R4 being Ordinal holds (for R5 being Ordinal holds (R3 in R4 implies (R3 in ( R4 +^ R5 ) & R3 in ( R5 +^ R4 ))))))
proof
let R3 being Ordinal;
let R4 being Ordinal;
let R5 being Ordinal;
assume L105: R3 in R4;
L106: R4 c= ( R5 +^ R4 ) by L100;
L107: R4 c= ( R4 +^ R5 ) by L100;
thus L108: thesis by L107 , L105 , L106;
end;
theorem
L109: (for R3 being Ordinal holds (for R4 being Ordinal holds (( R3 +^ R4 ) = ( {} ) implies (R3 = ( {} ) & R4 = ( {} )))))
proof
let R3 being Ordinal;
let R4 being Ordinal;
assume L110: ( R3 +^ R4 ) = ( {} );
L111: R4 c= ( {} ) by L110 , L100;
L112: R3 c= ( {} ) by L110 , L100;
thus L113: thesis by L112 , L111 , XBOOLE_1:3;
end;
theorem
L114: (for R3 being Ordinal holds (for R4 being Ordinal holds (R3 c= R4 implies (ex R5 being Ordinal st R4 = ( R3 +^ R5 )))))
proof
let R3 being Ordinal;
let R4 being Ordinal;
defpred S2[ Ordinal ] means (R3 c= $1 implies (ex R5 being Ordinal st $1 = ( R3 +^ R5 )));
L115: (for R4 being Ordinal holds (S2[ R4 ] implies S2[ ( succ R4 ) ]))
proof
let R4 being Ordinal;
assume that
L116: (R3 c= R4 implies (ex R5 being Ordinal st R4 = ( R3 +^ R5 )))
and
L117: R3 c= ( succ R4 );
L118:
now
assume L119: R3 <> ( succ R4 );
L120: R3 c< ( succ R4 ) by L119 , L117 , XBOOLE_0:def 8;
L121: R3 in ( succ R4 ) by L120 , ORDINAL1:11;
consider R5 being Ordinal such that L122: R4 = ( R3 +^ R5 ) by L121 , L116 , ORDINAL1:22;
L123: ( succ R4 ) = ( R3 +^ ( succ R5 ) ) by L122 , ORDINAL2:28;
thus L124: thesis by L123;
end;
L125: (R3 = ( succ R4 ) implies ( succ R4 ) = ( R3 +^ ( {} ) )) by ORDINAL2:27;
thus L126: thesis by L125 , L118;
end;
L127: (for R4 being Ordinal holds ((R4 <> ( {} ) & R4 is  limit_ordinal & (for R5 being Ordinal holds (R5 in R4 implies S2[ R5 ]))) implies S2[ R4 ]))
proof
deffunc H1(Ordinal) = ( R3 +^ $1 );
let R4 being Ordinal;
assume that
L128: R4 <> ( {} )
and
L129: R4 is  limit_ordinal
and
L130: (for R5 being Ordinal holds (R5 in R4 implies (R3 c= R5 implies (ex R6 being Ordinal st R5 = ( R3 +^ R6 )))))
and
L131: R3 c= R4;
defpred S3[ Ordinal ] means R4 c= ( R3 +^ $1 );
L132: R4 = ( ( {} ) +^ R4 ) by ORDINAL2:30;
L133: ( ( {} ) +^ R4 ) c= ( R3 +^ R4 ) by ORDINAL2:34 , XBOOLE_1:2;
L134: (ex R5 being Ordinal st S3[ R5 ]) by L133 , L132;
consider R5 being Ordinal such that L135: (S3[ R5 ] & (for R6 being Ordinal holds (S3[ R6 ] implies R5 c= R6))) from ORDINAL1:sch 1(L134);
consider C1 being Ordinal-Sequence such that L136: (( dom C1 ) = R5 & (for R6 being Ordinal holds (R6 in R5 implies ( C1 . R6 ) = H1(R6)))) from ORDINAL2:sch 3;
L137:
now
L138: (for R6 being Ordinal holds (R6 in ( rng C1 ) implies R6 in R4))
proof
let R6 being Ordinal;
assume L139: R6 in ( rng C1 );
consider R10 being set such that L140: R10 in ( dom C1 ) and L141: R6 = ( C1 . R10 ) by L139 , FUNCT_1:def 3;
reconsider D1 = R10 as Ordinal by L140;
L142: (not R5 c= D1) by L136 , L140 , ORDINAL1:5;
L143: ( C1 . D1 ) = ( R3 +^ D1 ) by L136 , L140;
thus L144: thesis by L143 , L135 , L141 , L142 , ORDINAL1:16;
end;
L145: ( sup ( rng C1 ) ) c= R4 by L138 , ORDINAL2:20;
L146: R5 is  limit_ordinal
proof
assume L147: (not thesis);
consider R6 being Ordinal such that L148: R5 = ( succ R6 ) by L147 , ORDINAL1:29;
L149: (not R5 c= R6) by L148 , ORDINAL1:5 , ORDINAL1:6;
L150: ( R3 +^ R6 ) in R4 by L149 , L135 , ORDINAL1:16;
L151: ( succ ( R3 +^ R6 ) ) c= R4 by L150 , ORDINAL1:21;
L152: ( succ ( R3 +^ R6 ) ) = ( R3 +^ ( succ R6 ) ) by ORDINAL2:28;
L153: R4 = ( succ ( R3 +^ R6 ) ) by L152 , L135 , L148 , L151 , XBOOLE_0:def 10;
thus L154: contradiction by L153 , L129 , ORDINAL1:29;
end;
assume L155: R5 <> ( {} );
L156: ( R3 +^ R5 ) = ( sup C1 ) by L155 , L136 , L146 , ORDINAL2:29
.= ( sup ( rng C1 ) );
L157: R4 = ( R3 +^ R5 ) by L156 , L135 , L145 , XBOOLE_0:def 10;
thus L158: thesis by L157;
end;
L159:
now
assume L160: R5 = ( {} );
L161: ( R3 +^ R5 ) = R3 by L160 , ORDINAL2:27;
thus L162: R4 = R3 by L161 , L131 , L135 , XBOOLE_0:def 10
.= ( R3 +^ ( {} ) ) by ORDINAL2:27;
end;
thus L163: thesis by L159 , L137;
end;
L164: S2[ ( {} ) ]
proof
assume L165: R3 c= ( {} );
L166: R3 = ( {} ) by L165 , XBOOLE_1:3;
L167: ( {} ) = ( R3 +^ ( {} ) ) by L166 , ORDINAL2:30;
thus L168: thesis by L167;
end;
L169: (for R4 being Ordinal holds S2[ R4 ]) from ORDINAL2:sch 1(L164 , L115 , L127);
thus L170: thesis by L169;
end;
theorem
L171: (for R3 being Ordinal holds (for R4 being Ordinal holds (R3 in R4 implies (ex R5 being Ordinal st (R4 = ( R3 +^ R5 ) & R5 <> ( {} ))))))
proof
let R3 being Ordinal;
let R4 being Ordinal;
assume L172: R3 in R4;
L173: R3 c= R4 by L172 , ORDINAL1:def 2;
consider R5 being Ordinal such that L174: R4 = ( R3 +^ R5 ) by L173 , L114;
take R5;
thus L175: R4 = ( R3 +^ R5 ) by L174;
assume L176: R5 = ( {} );
L177: R4 = R3 by L176 , L174 , ORDINAL2:27;
thus L178: contradiction by L177 , L172;
end;
theorem
L179: (for R3 being Ordinal holds (for R4 being Ordinal holds ((R3 <> ( {} ) & R3 is  limit_ordinal) implies ( R4 +^ R3 ) is  limit_ordinal)))
proof
let R3 being Ordinal;
let R4 being Ordinal;
assume that
L180: R3 <> ( {} )
and
L181: R3 is  limit_ordinal;
L182: ( {} ) c= R3 by XBOOLE_1:2;
L183: ( {} ) c< R3 by L182 , L180 , XBOOLE_0:def 8;
deffunc H2(Ordinal) = ( R4 +^ $1 );
consider C2 being Ordinal-Sequence such that L184: (( dom C2 ) = R3 & (for R5 being Ordinal holds (R5 in R3 implies ( C2 . R5 ) = H2(R5)))) from ORDINAL2:sch 3;
L185: ( R4 +^ R3 ) = ( sup C2 ) by L180 , L181 , L184 , ORDINAL2:29
.= ( sup ( rng C2 ) );
L186: (for R5 being Ordinal holds (R5 in ( R4 +^ R3 ) implies ( succ R5 ) in ( R4 +^ R3 )))
proof
let R5 being Ordinal;
assume that
L187: R5 in ( R4 +^ R3 );
L188:
now
assume L189: (not R5 in R4);
consider R6 being Ordinal such that L190: R5 = ( R4 +^ R6 ) by L189 , L114 , ORDINAL1:16;
L191:
now
assume L192: (not R6 in R3);
L193: ( R4 +^ R3 ) c= ( R4 +^ R6 ) by L192 , ORDINAL1:16 , ORDINAL2:33;
L194: R5 in R5 by L193 , L187 , L190;
thus L195: contradiction by L194;
end;
L196: ( succ R6 ) in R3 by L191 , L181 , ORDINAL1:28;
L197: ( C2 . ( succ R6 ) ) = ( R4 +^ ( succ R6 ) ) by L196 , L184
.= ( succ ( R4 +^ R6 ) ) by ORDINAL2:28;
L198: ( succ R5 ) in ( rng C2 ) by L197 , L184 , L190 , L196 , FUNCT_1:def 3;
thus L199: thesis by L198 , L185 , ORDINAL2:19;
end;
L200:
now
assume L201: R5 in R4;
L202: ( succ R5 ) c= R4 by L201 , ORDINAL1:21;
L203: ( ( succ R5 ) +^ ( {} ) ) = ( succ R5 ) by ORDINAL2:27;
L204: ( R4 +^ ( {} ) ) in ( R4 +^ R3 ) by L183 , ORDINAL1:11 , ORDINAL2:32;
thus L205: thesis by L204 , L202 , L203 , ORDINAL1:12 , ORDINAL2:34;
end;
thus L206: thesis by L200 , L188;
end;
thus L207: thesis by L186 , ORDINAL1:28;
end;
theorem
L208: (for R3 being Ordinal holds (for R4 being Ordinal holds (for R5 being Ordinal holds ( ( R3 +^ R4 ) +^ R5 ) = ( R3 +^ ( R4 +^ R5 ) ))))
proof
let R3 being Ordinal;
let R4 being Ordinal;
let R5 being Ordinal;
defpred S4[ Ordinal ] means ( ( R3 +^ R4 ) +^ $1 ) = ( R3 +^ ( R4 +^ $1 ) );
L209: (for R5 being Ordinal holds (S4[ R5 ] implies S4[ ( succ R5 ) ]))
proof
let R5 being Ordinal;
assume that
L210: ( ( R3 +^ R4 ) +^ R5 ) = ( R3 +^ ( R4 +^ R5 ) );
thus L211: ( ( R3 +^ R4 ) +^ ( succ R5 ) ) = ( succ ( ( R3 +^ R4 ) +^ R5 ) ) by ORDINAL2:28
.= ( R3 +^ ( succ ( R4 +^ R5 ) ) ) by L210 , ORDINAL2:28
.= ( R3 +^ ( R4 +^ ( succ R5 ) ) ) by ORDINAL2:28;
end;
L212: (for R5 being Ordinal holds ((R5 <> ( {} ) & R5 is  limit_ordinal & (for R6 being Ordinal holds (R6 in R5 implies S4[ R6 ]))) implies S4[ R5 ]))
proof
deffunc H3(Ordinal) = ( ( R3 +^ R4 ) +^ $1 );
let R5 being Ordinal;
assume that
L213: R5 <> ( {} )
and
L214: R5 is  limit_ordinal
and
L215: (for R6 being Ordinal holds (R6 in R5 implies S4[ R6 ]));
consider C3 being Ordinal-Sequence such that L216: (( dom C3 ) = R5 & (for R6 being Ordinal holds (R6 in R5 implies ( C3 . R6 ) = H3(R6)))) from ORDINAL2:sch 3;
deffunc H4(Ordinal) = ( R3 +^ $1 );
consider C4 being Ordinal-Sequence such that L217: (( dom C4 ) = ( R4 +^ R5 ) & (for R6 being Ordinal holds (R6 in ( R4 +^ R5 ) implies ( C4 . R6 ) = H4(R6)))) from ORDINAL2:sch 3;
L218: ( rng C3 ) c= ( rng C4 )
proof
let R9 being set;
assume L219: R9 in ( rng C3 );
consider R10 being set such that L220: R10 in ( dom C3 ) and L221: R9 = ( C3 . R10 ) by L219 , FUNCT_1:def 3;
