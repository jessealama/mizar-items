let C23 being set;
L33: ( dom C16 ) = ( ( dom C14 ) /\ [: R7 , R7 :] ) by L11 , RELAT_1:61;
assume L34: C23 in ( dom ( |: C14 , C15 :| || [: R7 , R8 :] ) );
L35: C23 in ( ( dom |: C14 , C15 :| ) /\ [: [: R7 , R8 :] , [: R7 , R8 :] :] ) by L34 , RELAT_1:61;
L36: C23 in [: [: R7 , R8 :] , [: R7 , R8 :] :] by L35 , XBOOLE_0:def 4;
L37: ( dom C17 ) = ( ( dom C15 ) /\ [: R8 , R8 :] ) by L13 , RELAT_1:61;
L38: C23 in ( dom |: C14 , C15 :| ) by L35 , XBOOLE_0:def 4;
consider C24 , C25 , C26 , C27 being set such that L39: C23 = [ [ C24 , C26 ] , [ C25 , C27 ] ] and L40: [ C24 , C25 ] in ( dom C14 ) and L41: [ C26 , C27 ] in ( dom C15 ) by L38 , FUNCT_4:def 3;
L42: [ C25 , C27 ] in [: R7 , R8 :] by L39 , L36 , ZFMISC_1:87;
L43: C27 in R8 by L42 , ZFMISC_1:87;
L44: [ C24 , C26 ] in [: R7 , R8 :] by L39 , L36 , ZFMISC_1:87;
L45: C26 in R8 by L44 , ZFMISC_1:87;
L46: [ C26 , C27 ] in [: R8 , R8 :] by L45 , L43 , ZFMISC_1:87;
L47: [ C26 , C27 ] in ( dom C17 ) by L46 , L41 , L37 , XBOOLE_0:def 4;
L48: C25 in R7 by L42 , ZFMISC_1:87;
L49: C24 in R7 by L44 , ZFMISC_1:87;
L50: [ C24 , C25 ] in [: R7 , R7 :] by L49 , L48 , ZFMISC_1:87;
L51: [ C24 , C25 ] in ( dom C16 ) by L50 , L40 , L33 , XBOOLE_0:def 4;
thus L52: thesis by L51 , L39 , L47 , FUNCT_4:def 3;
end;
L53:
now
let C28 being set;
assume L54: C28 in D1;
consider C29 , C30 , C31 , C32 being set such that L55: C28 = [ [ C29 , C31 ] , [ C30 , C32 ] ] and L56: [ C29 , C30 ] in ( dom C16 ) and L57: [ C31 , C32 ] in ( dom C17 ) by L54 , FUNCT_4:def 3;
thus L58: ( |: C16 , C17 :| . C28 ) = ( |: C16 , C17 :| . ([ C29 , C31 ] , [ C30 , C32 ]) ) by L55
.= [ ( C16 . (C29 , C30) ) , ( C17 . (C31 , C32) ) ] by L56 , L57 , FUNCT_4:def 3
.= [ ( C14 . [ C29 , C30 ] ) , ( C17 . [ C31 , C32 ] ) ] by L11 , L56 , FUNCT_1:47
.= [ ( C14 . (C29 , C30) ) , ( C15 . (C31 , C32) ) ] by L13 , L57 , FUNCT_1:47
.= ( |: C14 , C15 :| . ([ C29 , C31 ] , [ C30 , C32 ]) ) by L12 , L14 , L56 , L57 , FUNCT_4:def 3
.= ( ( |: C14 , C15 :| || [: R7 , R8 :] ) . C28 ) by L15 , L54 , L55 , FUNCT_1:47;
end;
L59: (for B5 being (Element of [: [: R7 , R8 :] , [: R7 , R8 :] :]) holds (B5 in D1 implies ( |: C16 , C17 :| . B5 ) = ( ( |: C14 , C15 :| || [: R7 , R8 :] ) . B5 ))) by L53;
L60: ( rng ( |: C14 , C15 :| || [: R7 , R8 :] ) ) c= ( rng |: C16 , C17 :| )
proof
let C33 being set;
assume L61: C33 in ( rng ( |: C14 , C15 :| || [: R7 , R8 :] ) );
consider C34 being set such that L62: C34 in ( dom ( |: C14 , C15 :| || [: R7 , R8 :] ) ) and L63: C33 = ( ( |: C14 , C15 :| || [: R7 , R8 :] ) . C34 ) by L61 , FUNCT_1:def 3;
L64: C33 = ( |: C16 , C17 :| . C34 ) by L15 , L53 , L62 , L63;
