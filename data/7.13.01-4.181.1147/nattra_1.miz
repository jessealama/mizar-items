theorem
L2: (for R1 being non  empty set holds (for R2 being non  empty set holds (for R3 being non  empty set holds (for R4 being non  empty set holds (for R5 being (Function of R1 , R3) holds (for R6 being (Function of R2 , R4) holds (for R7 being non  empty (Subset of R1) holds (for R8 being non  empty (Subset of R2) holds ( [: R5 , R6 :] | [: R7 , R8 :] ) = [: ( R5 | R7 ) , ( R6 | R8 ) :]))))))))
proof
let R1 being non  empty set;
let R2 being non  empty set;
let R3 being non  empty set;
let R4 being non  empty set;
let R5 being (Function of R1 , R3);
let R6 being (Function of R2 , R4);
let R7 being non  empty (Subset of R1);
let R8 being non  empty (Subset of R2);
L3:
now
let C5 being (Element of [: R7 , R8 :]);
consider C6 being (Element of R7), C7 being (Element of R8) such that L4: C5 = [ C6 , C7 ] by DOMAIN_1:1;
L5: ( ( R6 | R8 ) . C7 ) = ( R6 . C7 ) by FUNCT_1:49;
L6: ( ( R5 | R7 ) . C6 ) = ( R5 . C6 ) by FUNCT_1:49;
thus L7: ( [: ( R5 | R7 ) , ( R6 | R8 ) :] . C5 ) = ( [: ( R5 | R7 ) , ( R6 | R8 ) :] . (C6 , C7) ) by L4
.= [ ( R5 . C6 ) , ( R6 . C7 ) ] by L6 , L5 , FUNCT_3:75
.= ( [: R5 , R6 :] . (C6 , C7) ) by FUNCT_3:75
.= ( ( [: R5 , R6 :] | [: R7 , R8 :] ) . C5 ) by L4 , FUNCT_1:49;
end;
thus L8: thesis by L3 , FUNCT_2:63;
end;
