end;
L151: (the Comp of C54) c= (the Comp of R9) by CAT_2:def 4;
thus L152: (the Comp of C54) = ( (the Comp of R9) | (( ( dom (the Comp of R9) ) /\ [: (the carrier' of C54) , (the carrier' of C54) :] ) qua set) ) by L151 , L139 , GRFUNC_1:23
.= ( ( ((the Comp of R9) qua Relation) | (( dom (the Comp of R9) ) qua set) ) | ([: (the carrier' of C54) , (the carrier' of C54) :] qua set) ) by RELAT_1:71
.= ( (the Comp of R9) || (the carrier' of C54) );
end;
theorem
L153: (for R9 being Category holds (for B15 being non  empty (Subset of (the carrier of R9)) holds (for B16 being non  empty (Subset of (the carrier' of R9)) holds ((for B17 being (Element of R9) holds (B17 in B15 implies ( id B17 ) in B16)) implies (for B18 , B19 being (Function of B16 , B15) holds ((B18 = ( (the Source of R9) | B16 ) & B19 = ( (the Target of R9) | B16 )) implies (for B20 being (PartFunc of [: B16 , (B16 qua non  empty set) :] , B16) holds (B20 = ( (the Comp of R9) || B16 ) implies CatStr (# B15 , B16 , B18 , B19 , B20 #) is (Subcategory of R9)))))))))
proof
let R9 being Category;
let C58 being non  empty (Subset of (the carrier of R9));
let C59 being non  empty (Subset of (the carrier' of R9));
assume that
L154: (for B21 being (Element of R9) holds (B21 in C58 implies ( id B21 ) in C59));
let C60 , C61 being (Function of C59 , C58);
assume that
L155: C60 = ( (the Source of R9) | C59 )
and
L156: C61 = ( (the Target of R9) | C59 );
let C62 being (PartFunc of [: C59 , (C59 qua non  empty set) :] , C59);
