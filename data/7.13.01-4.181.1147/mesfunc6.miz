theorem
L1: (for R1 being non  empty set holds (for R5 being (PartFunc of R1 , ( REAL )) holds |. ( R_EAL R5 ) .| = ( R_EAL ( abs R5 ) )))
proof
let R1 being non  empty set;
let R5 being (PartFunc of R1 , ( REAL ));
L2:
now
let C1 being (Element of R1);
assume L3: C1 in ( dom |. ( R_EAL R5 ) .| );
L4: ( |. ( R_EAL R5 ) .| . C1 ) = |. ( ( R_EAL R5 ) . C1 ) .| by L3 , MESFUNC1:def 10;
L5: ( |. ( R_EAL R5 ) .| . C1 ) = ( abs ( R5 . C1 ) ) by L4 , EXTREAL2:1;
thus L6: ( |. ( R_EAL R5 ) .| . C1 ) = ( ( R_EAL ( abs R5 ) ) . C1 ) by L5 , VALUED_1:18;
end;
L7: ( dom |. ( R_EAL R5 ) .| ) = ( dom ( R_EAL R5 ) ) by MESFUNC1:def 10
.= ( dom ( abs R5 ) ) by VALUED_1:def 11;
thus L8: thesis by L7 , L2 , PARTFUN1:5;
end;
theorem
L9: (for B1 being non  empty set holds (for B2 being (SigmaField of B1) holds (for B3 being (sigma_Measure of B2) holds (for B4 being (PartFunc of B1 , ( ExtREAL )) holds (for B5 being Real holds ((( dom B4 ) in B2 & (for B6 being set holds (B6 in ( dom B4 ) implies ( B4 . B6 ) = B5))) implies B4 is_simple_func_in B2))))))
proof
let C2 being non  empty set;
let C3 being (SigmaField of C2);
let C4 being (sigma_Measure of C3);
let C5 being (PartFunc of C2 , ( ExtREAL ));
let C6 being Real;
assume that
L10: ( dom C5 ) in C3
and
L11: (for B7 being set holds (B7 in ( dom C5 ) implies ( C5 . B7 ) = C6));
reconsider D1 = ( dom C5 ) as (Element of C3) by L10;
L12: (ex B8 being (Finite_Sep_Sequence of C3) st (( dom C5 ) = ( union ( rng B8 ) ) & (for B9 being Nat holds (for B10 , B11 being (Element of C2) holds ((B9 in ( dom B8 ) & B10 in ( B8 . B9 ) & B11 in ( B8 . B9 )) implies ( C5 . B10 ) = ( C5 . B11 ))))))
proof
set D2 = <* D1 *>;
L13: ( dom D2 ) = ( Seg 1 ) by FINSEQ_1:38;
L14:
now
let C7 , C8 being Nat;
assume that
L15: C7 in ( dom D2 )
and
L16: (C8 in ( dom D2 ) & C7 <> C8);
L17: C7 = 1 by L13 , L15 , FINSEQ_1:2 , TARSKI:def 1;
thus L18: ( D2 . C7 ) misses ( D2 . C8 ) by L17 , L13 , L16 , FINSEQ_1:2 , TARSKI:def 1;
end;
L19: (for B12 being Nat holds (B12 in ( dom D2 ) implies ( D2 . B12 ) = D1))
proof
let C9 being Nat;
assume L20: C9 in ( dom D2 );
L21: C9 = 1 by L20 , L13 , FINSEQ_1:2 , TARSKI:def 1;
thus L22: thesis by L21 , FINSEQ_1:40;
end;
reconsider D3 = D2 as (Finite_Sep_Sequence of C3) by L14 , MESFUNC3:4;
take D3;
L23: D3 = <* ( D3 . 1 ) *> by FINSEQ_1:40;
L24: ( rng D3 ) = { ( D3 . 1 ) } by L23 , FINSEQ_1:38;
L25: 1 in ( Seg 1 );
L26: ( D3 . 1 ) = D1 by L25 , L13 , L19;
thus L27: ( dom C5 ) = ( union ( rng D3 ) ) by L26 , L24 , ZFMISC_1:25;
thus L28:now
let C10 being Nat;
let C11 , C12 being (Element of C2);
assume that
L29: C10 in ( dom D3 )
and
L30: C11 in ( D3 . C10 )
and
L31: C12 in ( D3 . C10 );
L32: ( D3 . C10 ) = D1 by L19 , L29;
L33: ( C5 . C11 ) = C6 by L32 , L11 , L30;
thus L34: ( C5 . C11 ) = ( C5 . C12 ) by L33 , L11 , L31 , L32;
end;
end;
L29:
now
let C13 being (Element of C2);
assume L30: C13 in ( dom C5 );
L31: ( C5 . C13 ) = C6 by L30 , L11;
L32: ( -infty ) < ( C5 . C13 ) by L31 , XXREAL_0:12;
L33: ( - ( +infty ) ) < ( C5 . C13 ) by L32 , XXREAL_3:def 3;
L34: ( C5 . C13 ) < ( +infty ) by L31 , XXREAL_0:9;
thus L35: |. ( C5 . C13 ) .| < ( +infty ) by L34 , L33 , EXTREAL2:11;
end;
L36: C5 is  real-valued by L29 , MESFUNC2:def 1;
thus L37: thesis by L36 , L12 , MESFUNC2:def 4;
end;
