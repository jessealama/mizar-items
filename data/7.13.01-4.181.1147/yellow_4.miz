theorem
L1: (for B1 being RelStr holds (for B2 being set holds (for B3 being (Element of B1) holds ((B3 in B2 &  ex_sup_of B2 , B1) implies B3 <= ( "\/" (B2 , B1) )))))
proof
let C1 being RelStr;
let C2 being set;
let C3 being (Element of C1);
assume that
L2: C3 in C2
and
L3:  ex_sup_of C2 , C1;
L4: C2 is_<=_than ( "\/" (C2 , C1) ) by L3 , YELLOW_0:def 9;
thus L5: thesis by L4 , L2 , LATTICE3:def 9;
end;
theorem
