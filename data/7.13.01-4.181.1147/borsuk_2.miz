L1: (for B1 being  real number holds ((( 0 ) <= B1 & B1 <= 1) iff B1 in (the carrier of ( I[01] ))))
proof
let C1 being  real number;
L2: [. ( 0 ) , 1 .] = { B2 where B2 is Real : (( 0 ) <= B2 & B2 <= 1) } by RCOMP_1:def 1;
L3: C1 is Real by XREAL_0:def 1;
thus L4: ((( 0 ) <= C1 & C1 <= 1) implies C1 in (the carrier of ( I[01] ))) by L3 , L2 , BORSUK_1:40;
assume L5: C1 in (the carrier of ( I[01] ));
L6: (ex B3 being Real st (B3 = C1 & ( 0 ) <= B3 & B3 <= 1)) by L5 , L2 , BORSUK_1:40;
thus L7: thesis by L6;
end;
scheme FrCard { F1() -> non  empty set , F2() -> set , F3(set) -> set , P1[set] } : ( card { F3(B4) where B4 is (Element of F1()) : (B4 in F2() & P1[ B4 ]) } ) c= ( card F2() )
proof
consider C2 being Function such that L8: (( dom C2 ) = F2() & (for B5 being set holds (B5 in F2() implies ( C2 . B5 ) = F3(B5)))) from FUNCT_1:sch 3;
L9: { F3(B6) where B6 is (Element of F1()) : (B6 in F2() & P1[ B6 ]) } c= ( rng C2 )
proof
let C3 being set;
assume L10: C3 in { F3(B7) where B7 is (Element of F1()) : (B7 in F2() & P1[ B7 ]) };
consider C4 being (Element of F1()) such that L11: C3 = F3(C4) and L12: C4 in F2() and L13: P1[ C4 ] by L10;
L14: ( C2 . C4 ) = C3 by L8 , L11 , L12;
thus L15: thesis by L14 , L8 , L12 , FUNCT_1:def 3;
end;
thus L16: thesis by L9 , L8 , CARD_1:12;
end;
theorem
L17: (for R1 being non  empty TopSpace holds (for R2 being non  empty TopSpace holds (for R3 being non  empty TopSpace holds (for R4 being non  empty TopSpace holds (for B8 being (Function of R2 , R4) holds (for B9 being (Function of R3 , R4) holds ((R2 is (SubSpace of R1) & R3 is (SubSpace of R1) & ( ( [#] R2 ) \/ ( [#] R3 ) ) = ( [#] R1 ) & R2 is  compact & R3 is  compact & R1 is  T_2 & B8 is  continuous & B9 is  continuous & (for B10 being set holds (B10 in ( ( [#] R2 ) /\ ( [#] R3 ) ) implies ( B8 . B10 ) = ( B9 . B10 )))) implies (ex B11 being (Function of R1 , R4) st (B11 = ( B8 +* B9 ) & B11 is  continuous)))))))))
