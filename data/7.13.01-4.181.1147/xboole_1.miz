theorem
L1: (for R4 being set holds (for R6 being set holds (for R8 being set holds ((R4 c= R6 & R6 c= R8) implies R4 c= R8))))
proof
let R4 being set;
let R6 being set;
let R8 being set;
assume that
L2: R4 c= R6
and
L3: R6 c= R8;
let R1 being set;
assume L4: R1 in R4;
L5: R1 in R6 by L4 , L2 , TARSKI:def 3;
thus L6: thesis by L5 , L3 , TARSKI:def 3;
end;
