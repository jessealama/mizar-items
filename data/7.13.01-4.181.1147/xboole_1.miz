let R6 being set;
let R8 being set;
thus L142: ( ( ( R4 /\ R6 ) \/ ( R6 /\ R8 ) ) \/ ( R8 /\ R4 ) ) = ( ( ( ( R4 /\ R6 ) \/ ( R6 /\ R8 ) ) \/ R8 ) /\ ( ( ( R4 /\ R6 ) \/ ( R6 /\ R8 ) ) \/ R4 ) ) by L127
.= ( ( ( R4 /\ R6 ) \/ ( ( R6 /\ R8 ) \/ R8 ) ) /\ ( ( ( R4 /\ R6 ) \/ ( R6 /\ R8 ) ) \/ R4 ) ) by L12
.= ( ( ( R4 /\ R6 ) \/ R8 ) /\ ( ( ( R4 /\ R6 ) \/ ( R6 /\ R8 ) ) \/ R4 ) ) by L108
