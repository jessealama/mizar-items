end;
theorem
L16: (for R4 being set holds (for R5 being set holds (for R10 being RealNormSpace holds (for R11 being RealNormSpace holds (for R13 being (PartFunc of R10 , R11) holds (for R14 being (PartFunc of R10 , R11) holds ((R13 is_uniformly_continuous_on R4 & R14 is_uniformly_continuous_on R5) implies ( R13 + R14 ) is_uniformly_continuous_on ( R4 /\ R5 ))))))))
proof
let R4 being set;
let R5 being set;
let R10 being RealNormSpace;
let R11 being RealNormSpace;
let R13 being (PartFunc of R10 , R11);
let R14 being (PartFunc of R10 , R11);
assume that
L17: R13 is_uniformly_continuous_on R4
and
L18: R14 is_uniformly_continuous_on R5;
L19: R14 is_uniformly_continuous_on ( R4 /\ R5 ) by L18 , L5 , XBOOLE_1:17;
L20: ( R4 /\ R5 ) c= ( dom R14 ) by L19 , L1;
L21: R13 is_uniformly_continuous_on ( R4 /\ R5 ) by L17 , L5 , XBOOLE_1:17;
L22: ( R4 /\ R5 ) c= ( dom R13 ) by L21 , L1;
L23: ( R4 /\ R5 ) c= ( ( dom R13 ) /\ ( dom R14 ) ) by L22 , L20 , XBOOLE_1:19;
thus L24: ( R4 /\ R5 ) c= ( dom ( R13 + R14 ) ) by L23 , VFUNCT_1:def 1;
