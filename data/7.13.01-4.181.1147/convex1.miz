begin
definition
let C1 being non  empty RLSStruct;
let C2 being (Subset of C1);
let C3 being Real;
func C3 * C2 -> (Subset of C1) equals 
{ ( C3 * B1 ) where B1 is (Element of C1) : B1 in C2 };
coherence
proof
deffunc H1((Element of C1)) = ( C3 * $1 );
defpred S1[ set ] means $1 in C2;
L1: { H1(B2) where B2 is (Element of C1) : S1[ B2 ] } is (Subset of C1) from DOMAIN_1:sch 8;
thus L2: thesis by L1;
end;
end;
definition
let C4 being non  empty RLSStruct;
let C5 being (Subset of C4);
attr C5 is  convex
means
:L4: (for B3 , B4 being (VECTOR of C4) holds (for B5 being Real holds ((( 0 ) < B5 & B5 < 1 & B3 in C5 & B4 in C5) implies ( ( B5 * B3 ) + ( ( 1 - B5 ) * B4 ) ) in C5)));
end;
theorem
L6: (for B6 being  vector-distributive  scalar-distributive  scalar-associative  scalar-unital non  empty RLSStruct holds (for B7 being (Subset of B6) holds (for B8 being Real holds (B7 is  convex implies ( B8 * B7 ) is  convex))))
proof
let C6 being  vector-distributive  scalar-distributive  scalar-associative  scalar-unital non  empty RLSStruct;
let C7 being (Subset of C6);
let C8 being Real;
assume L7: C7 is  convex;
L8: (for B9 , B10 being (VECTOR of C6) holds (for B11 being Real holds ((( 0 ) < B11 & B11 < 1 & B9 in ( C8 * C7 ) & B10 in ( C8 * C7 )) implies ( ( B11 * B9 ) + ( ( 1 - B11 ) * B10 ) ) in ( C8 * C7 ))))
proof
let C9 , C10 being (VECTOR of C6);
let C11 being Real;
assume that
L9: (( 0 ) < C11 & C11 < 1)
and
L10: C9 in ( C8 * C7 )
and
L11: C10 in ( C8 * C7 );
consider C12 being (Element of C6) such that L12: C10 = ( C8 * C12 ) and L13: C12 in C7 by L11;
consider C13 being (Element of C6) such that L14: C9 = ( C8 * C13 ) and L15: C13 in C7 by L10;
L16: ( ( C11 * C9 ) + ( ( 1 - C11 ) * C10 ) ) = ( ( ( C8 * C11 ) * C13 ) + ( ( 1 - C11 ) * ( C8 * C12 ) ) ) by L14 , L12 , RLVECT_1:def 7
.= ( ( ( C8 * C11 ) * C13 ) + ( ( C8 * ( 1 - C11 ) ) * C12 ) ) by RLVECT_1:def 7
.= ( ( C8 * ( C11 * C13 ) ) + ( ( C8 * ( 1 - C11 ) ) * C12 ) ) by RLVECT_1:def 7
.= ( ( C8 * ( C11 * C13 ) ) + ( C8 * ( ( 1 - C11 ) * C12 ) ) ) by RLVECT_1:def 7
.= ( C8 * ( ( C11 * C13 ) + ( ( 1 - C11 ) * C12 ) ) ) by RLVECT_1:def 5;
L17: ( ( C11 * C13 ) + ( ( 1 - C11 ) * C12 ) ) in C7 by L7 , L9 , L15 , L13 , L4;
thus L18: thesis by L17 , L16;
end;
thus L19: thesis by L8 , L4;
end;
theorem
L20: (for B12 being  Abelian  add-associative  vector-distributive  scalar-distributive  scalar-associative  scalar-unital non  empty RLSStruct holds (for B13 , B14 being (Subset of B12) holds ((B13 is  convex & B14 is  convex) implies ( B13 + B14 ) is  convex)))
proof
let C14 being  Abelian  add-associative  vector-distributive  scalar-distributive  scalar-associative  scalar-unital non  empty RLSStruct;
let C15 , C16 being (Subset of C14);
assume L21: (C15 is  convex & C16 is  convex);
L22: (for B15 , B16 being (VECTOR of C14) holds (for B17 being Real holds ((( 0 ) < B17 & B17 < 1 & B15 in ( C15 + C16 ) & B16 in ( C15 + C16 )) implies ( ( B17 * B15 ) + ( ( 1 - B17 ) * B16 ) ) in ( C15 + C16 ))))
proof
let C17 , C18 being (VECTOR of C14);
let C19 being Real;
assume that
L23: (( 0 ) < C19 & C19 < 1)
and
L24: C17 in ( C15 + C16 )
and
L25: C18 in ( C15 + C16 );
L26: C18 in { ( B18 + B19 ) where B18 , B19 is (Element of C14) : (B18 in C15 & B19 in C16) } by L25 , RUSUB_4:def 9;
consider C20 , C21 being (Element of C14) such that L27: C18 = ( C20 + C21 ) and L28: (C20 in C15 & C21 in C16) by L26;
L29: C17 in { ( B20 + B21 ) where B20 , B21 is (Element of C14) : (B20 in C15 & B21 in C16) } by L24 , RUSUB_4:def 9;
consider C22 , C23 being (Element of C14) such that L30: C17 = ( C22 + C23 ) and L31: (C22 in C15 & C23 in C16) by L29;
L32: ( ( C19 * C17 ) + ( ( 1 - C19 ) * C18 ) ) = ( ( ( C19 * C22 ) + ( C19 * C23 ) ) + ( ( 1 - C19 ) * ( C20 + C21 ) ) ) by L30 , L27 , RLVECT_1:def 5
.= ( ( ( C19 * C22 ) + ( C19 * C23 ) ) + ( ( ( 1 - C19 ) * C20 ) + ( ( 1 - C19 ) * C21 ) ) ) by RLVECT_1:def 5
.= ( ( ( ( C19 * C22 ) + ( C19 * C23 ) ) + ( ( 1 - C19 ) * C20 ) ) + ( ( 1 - C19 ) * C21 ) ) by RLVECT_1:def 3
.= ( ( ( ( C19 * C22 ) + ( ( 1 - C19 ) * C20 ) ) + ( C19 * C23 ) ) + ( ( 1 - C19 ) * C21 ) ) by RLVECT_1:def 3
.= ( ( ( C19 * C22 ) + ( ( 1 - C19 ) * C20 ) ) + ( ( C19 * C23 ) + ( ( 1 - C19 ) * C21 ) ) ) by RLVECT_1:def 3;
L33: (( ( C19 * C22 ) + ( ( 1 - C19 ) * C20 ) ) in C15 & ( ( C19 * C23 ) + ( ( 1 - C19 ) * C21 ) ) in C16) by L21 , L23 , L31 , L28 , L4;
L34: ( ( C19 * C17 ) + ( ( 1 - C19 ) * C18 ) ) in { ( B22 + B23 ) where B22 , B23 is (Element of C14) : (B22 in C15 & B23 in C16) } by L33 , L32;
thus L35: thesis by L34 , RUSUB_4:def 9;
end;
thus L36: thesis by L22 , L4;
end;
theorem
L37: (for B24 being RealLinearSpace holds (for B25 , B26 being (Subset of B24) holds ((B25 is  convex & B26 is  convex) implies ( B25 - B26 ) is  convex)))
proof
let C24 being RealLinearSpace;
let C25 , C26 being (Subset of C24);
assume L38: (C25 is  convex & C26 is  convex);
L39: (for B27 , B28 being (VECTOR of C24) holds (for B29 being Real holds ((( 0 ) < B29 & B29 < 1 & B27 in ( C25 - C26 ) & B28 in ( C25 - C26 )) implies ( ( B29 * B27 ) + ( ( 1 - B29 ) * B28 ) ) in ( C25 - C26 ))))
proof
let C27 , C28 being (VECTOR of C24);
let C29 being Real;
assume that
L40: (( 0 ) < C29 & C29 < 1)
and
L41: C27 in ( C25 - C26 )
and
L42: C28 in ( C25 - C26 );
L43: C28 in { ( B30 - B31 ) where B30 , B31 is (Element of C24) : (B30 in C25 & B31 in C26) } by L42 , RUSUB_5:def 2;
consider C30 , C31 being (Element of C24) such that L44: C28 = ( C30 - C31 ) and L45: (C30 in C25 & C31 in C26) by L43;
L46: C27 in { ( B32 - B33 ) where B32 , B33 is (Element of C24) : (B32 in C25 & B33 in C26) } by L41 , RUSUB_5:def 2;
consider C32 , C33 being (Element of C24) such that L47: C27 = ( C32 - C33 ) and L48: (C32 in C25 & C33 in C26) by L46;
L49: ( ( C29 * C27 ) + ( ( 1 - C29 ) * C28 ) ) = ( ( ( C29 * C32 ) - ( C29 * C33 ) ) + ( ( 1 - C29 ) * ( C30 - C31 ) ) ) by L47 , L44 , RLVECT_1:34
.= ( ( ( C29 * C32 ) - ( C29 * C33 ) ) + ( ( ( 1 - C29 ) * C30 ) - ( ( 1 - C29 ) * C31 ) ) ) by RLVECT_1:34
.= ( ( ( ( C29 * C32 ) - ( C29 * C33 ) ) + ( ( 1 - C29 ) * C30 ) ) - ( ( 1 - C29 ) * C31 ) ) by RLVECT_1:def 3
.= ( ( ( C29 * C32 ) - ( ( C29 * C33 ) - ( ( 1 - C29 ) * C30 ) ) ) - ( ( 1 - C29 ) * C31 ) ) by RLVECT_1:29
.= ( ( ( C29 * C32 ) + ( ( ( 1 - C29 ) * C30 ) + ( - ( C29 * C33 ) ) ) ) - ( ( 1 - C29 ) * C31 ) ) by RLVECT_1:33
.= ( ( ( ( C29 * C32 ) + ( ( 1 - C29 ) * C30 ) ) + ( - ( C29 * C33 ) ) ) - ( ( 1 - C29 ) * C31 ) ) by RLVECT_1:def 3
.= ( ( ( C29 * C32 ) + ( ( 1 - C29 ) * C30 ) ) + ( ( - ( C29 * C33 ) ) - ( ( 1 - C29 ) * C31 ) ) ) by RLVECT_1:def 3
.= ( ( ( C29 * C32 ) + ( ( 1 - C29 ) * C30 ) ) - ( ( C29 * C33 ) + ( ( 1 - C29 ) * C31 ) ) ) by RLVECT_1:30;
L50: (( ( C29 * C32 ) + ( ( 1 - C29 ) * C30 ) ) in C25 & ( ( C29 * C33 ) + ( ( 1 - C29 ) * C31 ) ) in C26) by L38 , L40 , L48 , L45 , L4;
L51: ( ( C29 * C27 ) + ( ( 1 - C29 ) * C28 ) ) in { ( B34 - B35 ) where B34 , B35 is (Element of C24) : (B34 in C25 & B35 in C26) } by L50 , L49;
thus L52: thesis by L51 , RUSUB_5:def 2;
end;
thus L53: thesis by L39 , L4;
end;
theorem
L54: (for B36 being non  empty RLSStruct holds (for B37 being (Subset of B36) holds (B37 is  convex iff (for B38 being Real holds ((( 0 ) < B38 & B38 < 1) implies ( ( B38 * B37 ) + ( ( 1 - B38 ) * B37 ) ) c= B37)))))
proof
let C34 being non  empty RLSStruct;
let C35 being (Subset of C34);
L55: (C35 is  convex implies (for B39 being Real holds ((( 0 ) < B39 & B39 < 1) implies ( ( B39 * C35 ) + ( ( 1 - B39 ) * C35 ) ) c= C35)))
proof
assume L56: C35 is  convex;
let C36 being Real;
assume L57: (( 0 ) < C36 & C36 < 1);
L58: (for B40 being (Element of C34) holds (B40 in ( ( C36 * C35 ) + ( ( 1 - C36 ) * C35 ) ) implies B40 in C35))
proof
let C37 being (Element of C34);
assume L59: C37 in ( ( C36 * C35 ) + ( ( 1 - C36 ) * C35 ) );
L60: C37 in { ( B41 + B42 ) where B41 , B42 is (Element of C34) : (B41 in ( C36 * C35 ) & B42 in ( ( 1 - C36 ) * C35 )) } by L59 , RUSUB_4:def 9;
consider C38 , C39 being (Element of C34) such that L61: C37 = ( C38 + C39 ) and L62: (C38 in ( C36 * C35 ) & C39 in ( ( 1 - C36 ) * C35 )) by L60;
L63: ((ex B43 being (Element of C34) st (C38 = ( C36 * B43 ) & B43 in C35)) & (ex B44 being (Element of C34) st (C39 = ( ( 1 - C36 ) * B44 ) & B44 in C35))) by L62;
thus L64: thesis by L63 , L56 , L57 , L61 , L4;
end;
thus L65: thesis by L58 , SUBSET_1:2;
end;
L66: ((for B45 being Real holds ((( 0 ) < B45 & B45 < 1) implies ( ( B45 * C35 ) + ( ( 1 - B45 ) * C35 ) ) c= C35)) implies C35 is  convex)
proof
assume L67: (for B46 being Real holds ((( 0 ) < B46 & B46 < 1) implies ( ( B46 * C35 ) + ( ( 1 - B46 ) * C35 ) ) c= C35));
let C40 , C41 being (VECTOR of C34);
let C42 being Real;
assume L68: (( 0 ) < C42 & C42 < 1);
L69: ( ( C42 * C35 ) + ( ( 1 - C42 ) * C35 ) ) c= C35 by L68 , L67;
assume L70: (C40 in C35 & C41 in C35);
L71: (( C42 * C40 ) in ( C42 * C35 ) & ( ( 1 - C42 ) * C41 ) in { ( ( 1 - C42 ) * B47 ) where B47 is (Element of C34) : B47 in C35 }) by L70;
L72: ( ( C42 * C40 ) + ( ( 1 - C42 ) * C41 ) ) in { ( B48 + B49 ) where B48 , B49 is (Element of C34) : (B48 in ( C42 * C35 ) & B49 in ( ( 1 - C42 ) * C35 )) } by L71;
L73: ( ( C42 * C40 ) + ( ( 1 - C42 ) * C41 ) ) in ( ( C42 * C35 ) + ( ( 1 - C42 ) * C35 ) ) by L72 , RUSUB_4:def 9;
thus L74: thesis by L73 , L69;
end;
thus L75: thesis by L66 , L55;
end;
theorem
L76: (for B50 being  Abelian non  empty RLSStruct holds (for B51 being (Subset of B50) holds (B51 is  convex implies (for B52 being Real holds ((( 0 ) < B52 & B52 < 1) implies ( ( ( 1 - B52 ) * B51 ) + ( B52 * B51 ) ) c= B51)))))
proof
let C43 being  Abelian non  empty RLSStruct;
let C44 being (Subset of C43);
assume L77: C44 is  convex;
let C45 being Real;
assume L78: (( 0 ) < C45 & C45 < 1);
L79: (for B53 being (Element of C43) holds (B53 in ( ( ( 1 - C45 ) * C44 ) + ( C45 * C44 ) ) implies B53 in C44))
proof
let C46 being (Element of C43);
assume L80: C46 in ( ( ( 1 - C45 ) * C44 ) + ( C45 * C44 ) );
L81: C46 in { ( B54 + B55 ) where B54 , B55 is (Element of C43) : (B54 in ( ( 1 - C45 ) * C44 ) & B55 in ( C45 * C44 )) } by L80 , RUSUB_4:def 9;
consider C47 , C48 being (Element of C43) such that L82: C46 = ( C47 + C48 ) and L83: (C47 in ( ( 1 - C45 ) * C44 ) & C48 in ( C45 * C44 )) by L81;
L84: ((ex B56 being (Element of C43) st (C47 = ( ( 1 - C45 ) * B56 ) & B56 in C44)) & (ex B57 being (Element of C43) st (C48 = ( C45 * B57 ) & B57 in C44))) by L83;
thus L85: thesis by L84 , L77 , L78 , L82 , L4;
end;
thus L86: thesis by L79 , SUBSET_1:2;
end;
theorem
L87: (for B58 being  Abelian  add-associative  vector-distributive  scalar-distributive  scalar-associative  scalar-unital non  empty RLSStruct holds (for B59 , B60 being (Subset of B58) holds ((B59 is  convex & B60 is  convex) implies (for B61 being Real holds ( ( B61 * B59 ) + ( ( 1 - B61 ) * B60 ) ) is  convex))))
proof
let C49 being  Abelian  add-associative  vector-distributive  scalar-distributive  scalar-associative  scalar-unital non  empty RLSStruct;
let C50 , C51 being (Subset of C49);
assume that
L88: C50 is  convex
and
L89: C51 is  convex;
let C52 being Real;
let C53 , C54 being (VECTOR of C49);
let C55 being Real;
assume that
L90: (( 0 ) < C55 & C55 < 1)
and
L91: C53 in ( ( C52 * C50 ) + ( ( 1 - C52 ) * C51 ) )
and
L92: C54 in ( ( C52 * C50 ) + ( ( 1 - C52 ) * C51 ) );
L93: C53 in { ( B62 + B63 ) where B62 , B63 is (VECTOR of C49) : (B62 in ( C52 * C50 ) & B63 in ( ( 1 - C52 ) * C51 )) } by L91 , RUSUB_4:def 9;
L94: C54 in { ( B64 + B65 ) where B64 , B65 is (VECTOR of C49) : (B64 in ( C52 * C50 ) & B65 in ( ( 1 - C52 ) * C51 )) } by L92 , RUSUB_4:def 9;
consider C56 , C57 being (VECTOR of C49) such that L95: C54 = ( C56 + C57 ) and L96: C56 in ( C52 * C50 ) and L97: C57 in ( ( 1 - C52 ) * C51 ) by L94;
consider C58 , C59 being (VECTOR of C49) such that L98: C53 = ( C58 + C59 ) and L99: C58 in ( C52 * C50 ) and L100: C59 in ( ( 1 - C52 ) * C51 ) by L93;
consider C60 being (VECTOR of C49) such that L101: C56 = ( C52 * C60 ) and L102: C60 in C50 by L96;
consider C61 being (VECTOR of C49) such that L103: C58 = ( C52 * C61 ) and L104: C61 in C50 by L99;
L105: ( ( C55 * C58 ) + ( ( 1 - C55 ) * C56 ) ) = ( ( ( C55 * C52 ) * C61 ) + ( ( 1 - C55 ) * ( C52 * C60 ) ) ) by L103 , L101 , RLVECT_1:def 7
.= ( ( ( C55 * C52 ) * C61 ) + ( ( ( 1 - C55 ) * C52 ) * C60 ) ) by RLVECT_1:def 7
.= ( ( C52 * ( C55 * C61 ) ) + ( ( ( 1 - C55 ) * C52 ) * C60 ) ) by RLVECT_1:def 7
.= ( ( C52 * ( C55 * C61 ) ) + ( C52 * ( ( 1 - C55 ) * C60 ) ) ) by RLVECT_1:def 7
.= ( C52 * ( ( C55 * C61 ) + ( ( 1 - C55 ) * C60 ) ) ) by RLVECT_1:def 5;
consider C62 being (VECTOR of C49) such that L106: C57 = ( ( 1 - C52 ) * C62 ) and L107: C62 in C51 by L97;
consider C63 being (VECTOR of C49) such that L108: C59 = ( ( 1 - C52 ) * C63 ) and L109: C63 in C51 by L100;
L110: ( ( C55 * C59 ) + ( ( 1 - C55 ) * C57 ) ) = ( ( ( C55 * ( 1 - C52 ) ) * C63 ) + ( ( 1 - C55 ) * ( ( 1 - C52 ) * C62 ) ) ) by L108 , L106 , RLVECT_1:def 7
.= ( ( ( C55 * ( 1 - C52 ) ) * C63 ) + ( ( ( 1 - C55 ) * ( 1 - C52 ) ) * C62 ) ) by RLVECT_1:def 7
.= ( ( ( 1 - C52 ) * ( C55 * C63 ) ) + ( ( ( 1 - C55 ) * ( 1 - C52 ) ) * C62 ) ) by RLVECT_1:def 7
.= ( ( ( 1 - C52 ) * ( C55 * C63 ) ) + ( ( 1 - C52 ) * ( ( 1 - C55 ) * C62 ) ) ) by RLVECT_1:def 7
.= ( ( 1 - C52 ) * ( ( C55 * C63 ) + ( ( 1 - C55 ) * C62 ) ) ) by RLVECT_1:def 5;
L111: ( ( C55 * C63 ) + ( ( 1 - C55 ) * C62 ) ) in C51 by L89 , L90 , L109 , L107 , L4;
L112: ( ( C55 * C59 ) + ( ( 1 - C55 ) * C57 ) ) in ( ( 1 - C52 ) * C51 ) by L111 , L110;
L113: ( ( C55 * C61 ) + ( ( 1 - C55 ) * C60 ) ) in C50 by L88 , L90 , L104 , L102 , L4;
L114: ( ( C55 * C58 ) + ( ( 1 - C55 ) * C56 ) ) in { ( C52 * B66 ) where B66 is (VECTOR of C49) : B66 in C50 } by L113 , L105;
L115: ( ( C55 * C53 ) + ( ( 1 - C55 ) * C54 ) ) = ( ( ( C55 * C58 ) + ( C55 * C59 ) ) + ( ( 1 - C55 ) * ( C56 + C57 ) ) ) by L98 , L95 , RLVECT_1:def 5
.= ( ( ( C55 * C58 ) + ( C55 * C59 ) ) + ( ( ( 1 - C55 ) * C56 ) + ( ( 1 - C55 ) * C57 ) ) ) by RLVECT_1:def 5
.= ( ( ( ( C55 * C58 ) + ( C55 * C59 ) ) + ( ( 1 - C55 ) * C56 ) ) + ( ( 1 - C55 ) * C57 ) ) by RLVECT_1:def 3
.= ( ( ( ( C55 * C58 ) + ( ( 1 - C55 ) * C56 ) ) + ( C55 * C59 ) ) + ( ( 1 - C55 ) * C57 ) ) by RLVECT_1:def 3
.= ( ( ( C55 * C58 ) + ( ( 1 - C55 ) * C56 ) ) + ( ( C55 * C59 ) + ( ( 1 - C55 ) * C57 ) ) ) by RLVECT_1:def 3;
L116: ( ( C55 * C53 ) + ( ( 1 - C55 ) * C54 ) ) in { ( B67 + B68 ) where B67 , B68 is (VECTOR of C49) : (B67 in ( C52 * C50 ) & B68 in ( ( 1 - C52 ) * C51 )) } by L115 , L114 , L112;
thus L117: thesis by L116 , RUSUB_4:def 9;
end;
L118: (for B69 being  vector-distributive  scalar-distributive  scalar-associative  scalar-unital non  empty RLSStruct holds (for B70 being (Subset of B69) holds ( 1 * B70 ) = B70))
proof
let C64 being  vector-distributive  scalar-distributive  scalar-associative  scalar-unital non  empty RLSStruct;
let C65 being (Subset of C64);
L119: (for B71 being (Element of C64) holds (B71 in C65 implies B71 in ( 1 * C65 )))
proof
let C66 being (Element of C64);
L120: C66 = ( 1 * C66 ) by RLVECT_1:def 8;
assume L121: C66 in C65;
thus L122: thesis by L121 , L120;
end;
L123: C65 c= ( 1 * C65 ) by L119 , SUBSET_1:2;
L124: (for B72 being (Element of C64) holds (B72 in ( 1 * C65 ) implies B72 in C65))
proof
let C67 being (Element of C64);
assume L125: C67 in ( 1 * C65 );
L126: (ex B73 being (Element of C64) st (C67 = ( 1 * B73 ) & B73 in C65)) by L125;
thus L127: thesis by L126 , RLVECT_1:def 8;
end;
L128: ( 1 * C65 ) c= C65 by L124 , SUBSET_1:2;
thus L129: thesis by L128 , L123 , XBOOLE_0:def 10;
end;
L130: (for B74 being RealLinearSpace holds (for B75 being non  empty (Subset of B74) holds ( ( 0 ) * B75 ) = { ( 0. B74 ) }))
proof
let C68 being RealLinearSpace;
let C69 being non  empty (Subset of C68);
L131: (for B76 being (Element of C68) holds (B76 in { ( 0. C68 ) } implies B76 in ( ( 0 ) * C69 )))
