L113: (for B59 , B60 being  /\-complete  up-complete Semilattice holds (( the RelStr of B59 ) = ( the RelStr of B60 ) implies (the topology of ( ConvergenceSpace ( lim_inf-Convergence B59 ) )) c= (the topology of ( ConvergenceSpace ( lim_inf-Convergence B60 ) ))))
proof
let C67 , C68 being  /\-complete  up-complete Semilattice;
assume that
L114: ( the RelStr of C67 ) = ( the RelStr of C68 );
let C69 being set;
L115: (the topology of ( ConvergenceSpace ( lim_inf-Convergence C68 ) )) = { B61 where B61 is (Subset of C68) : (for B62 being (Element of C68) holds (B62 in B61 implies (for B63 being (net of C68) holds ([ B63 , B62 ] in ( lim_inf-Convergence C68 ) implies B63 is_eventually_in B61)))) } by YELLOW_6:def 24;
L116: (the topology of ( ConvergenceSpace ( lim_inf-Convergence C67 ) )) = { B64 where B64 is (Subset of C67) : (for B65 being (Element of C67) holds (B65 in B64 implies (for B66 being (net of C67) holds ([ B66 , B65 ] in ( lim_inf-Convergence C67 ) implies B66 is_eventually_in B64)))) } by YELLOW_6:def 24;
assume L117: C69 in (the topology of ( ConvergenceSpace ( lim_inf-Convergence C67 ) ));
consider C70 being (Subset of C67) such that L118: C69 = C70 and L119: (for B67 being (Element of C67) holds (B67 in C70 implies (for B68 being (net of C67) holds ([ B68 , B67 ] in ( lim_inf-Convergence C67 ) implies B68 is_eventually_in C70)))) by L117 , L116;
reconsider D33 = C70 as (Subset of C68) by L114;
L120:
now
let C71 being (Element of C68);
assume that
L121: C71 in D33;
let C72 being (net of C68);
