definition
let C1 being non  empty Poset;
let C2 being non  empty (Subset of C1);
let C3 being (Filter of ( BoolePoset C2 ));
func lim_inf C3 -> (Element of C1) equals 
( "\/" ({ ( inf B1 ) where B1 is (Subset of C1) : B1 in C3 } , C1) );
correctness;
end;
theorem
L2: (for B2 , B3 being  complete LATTICE holds (( the RelStr of B2 ) = ( the RelStr of B3 ) implies (for B4 being non  empty (Subset of B2) holds (for B5 being non  empty (Subset of B3) holds (for B6 being (Filter of ( BoolePoset B4 )) holds (for B7 being (Filter of ( BoolePoset B5 )) holds (B6 = B7 implies ( lim_inf B6 ) = ( lim_inf B7 ))))))))
proof
let C4 , C5 being  complete LATTICE;
assume that
L3: ( the RelStr of C4 ) = ( the RelStr of C5 );
let C6 being non  empty (Subset of C4);
let C7 being non  empty (Subset of C5);
let C8 being (Filter of ( BoolePoset C6 ));
let C9 being (Filter of ( BoolePoset C7 ));
assume that
L4: C8 = C9;
set D1 = { ( inf B8 ) where B8 is (Subset of C5) : B8 in C9 };
set D2 = { ( inf B9 ) where B9 is (Subset of C4) : B9 in C8 };
L5: D1 c= D2
proof
let R1 being set;
assume L6: R1 in D1;
consider C10 being (Subset of C5) such that L7: R1 = ( inf C10 ) and L8: C10 in C9 by L6;
L9: C8 c= (the carrier of ( BoolePoset C6 ));
L10: C8 c= ( bool C6 ) by L9 , WAYBEL_7:2;
reconsider D3 = C10 as (Subset of C4) by L10 , L4 , L8 , XBOOLE_1:1;
L11: ( inf D3 ) = ( inf C10 ) by L3 , YELLOW_0:17 , YELLOW_0:27;
thus L12: thesis by L11 , L4 , L7 , L8;
end;
L13: D2 c= D1
proof
let R1 being set;
assume L14: R1 in D2;
consider C11 being (Subset of C4) such that L15: R1 = ( inf C11 ) and L16: C11 in C8 by L14;
L17: C9 c= (the carrier of ( BoolePoset C7 ));
L18: C9 c= ( bool C7 ) by L17 , WAYBEL_7:2;
reconsider D4 = C11 as (Subset of C5) by L18 , L4 , L16 , XBOOLE_1:1;
L19: ( inf C11 ) = ( inf D4 ) by L3 , YELLOW_0:17 , YELLOW_0:27;
thus L20: thesis by L19 , L4 , L15 , L16;
end;
L21: D2 = D1 by L13 , L5 , XBOOLE_0:def 10;
thus L22: thesis by L21 , L3 , YELLOW_0:17 , YELLOW_0:26;
end;
definition
let C12 being non  empty TopRelStr;
attr C12 is  lim-inf
means
:L23: (the topology of C12) = ( xi C12 );
end;
registration
cluster  lim-inf ->  TopSpace-like for non  empty non  empty non  empty non  empty TopRelStr;
coherence
proof
let C13 being non  empty TopRelStr;
set D5 = ( ConvergenceSpace ( lim_inf-Convergence C13 ) );
assume L25: C13 is  lim-inf;
L26: (the topology of C13) = ( xi C13 ) by L25 , L23
.= (the topology of D5) by WAYBEL28:def 4;
L27: (for B10 being (Subset-Family of C13) holds (B10 c= (the topology of C13) implies ( union B10 ) in (the topology of C13)))
proof
let C14 being (Subset-Family of C13);
reconsider D6 = C14 as (Subset-Family of D5) by YELLOW_6:def 24;
assume L28: C14 c= (the topology of C13);
L29: ( union D6 ) in (the topology of D5) by L28 , L26 , PRE_TOPC:def 1;
thus L30: thesis by L29 , L26;
end;
L31: (the carrier of C13) = (the carrier of D5) by YELLOW_6:def 24;
L32: (the carrier of C13) in (the topology of C13) by L31 , L26 , PRE_TOPC:def 1;
L33: (for B11 , B12 being (Subset of C13) holds ((B11 in (the topology of C13) & B12 in (the topology of C13)) implies ( B11 /\ B12 ) in (the topology of C13))) by L26 , PRE_TOPC:def 1;
thus L34: thesis by L33 , L32 , L27 , PRE_TOPC:def 1;
end;
end;
registration
cluster  trivial ->  lim-inf for TopLattice;
coherence
proof
let C15 being TopLattice;
assume L36: C15 is  trivial;
L37: (the carrier of C15) is 1 -element by L36;
reconsider D7 = C15 as 1 -element TopLattice by L37 , STRUCT_0:def 19;
L38: (the carrier of ( ConvergenceSpace ( lim_inf-Convergence C15 ) )) = (the carrier of D7) by YELLOW_6:def 24;
reconsider D8 = ( ConvergenceSpace ( lim_inf-Convergence C15 ) ) as 1 -element TopSpace by L38 , STRUCT_0:def 19;
set D9 = the (Point of D7);
reconsider D10 = D9 as (Point of D8) by YELLOW_6:def 24;
thus L39: (the topology of C15) = { ( {} ) , { D10 } } by YELLOW_9:9
.= (the topology of D8) by YELLOW_9:9
.= ( xi C15 ) by WAYBEL28:def 4;
end;
end;
