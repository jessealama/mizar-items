let R8 being (Subset of R5);
assume that
L4: ( ( Cl R6 ) /\ R8 ) = ( {} )
and
L5: ( R6 /\ ( Cl R8 ) ) = ( {} );
L6: R6 c= ( Cl R6 ) by PRE_TOPC:18;
thus L7: ( R6 /\ R8 ) = ( {} ) by L6 , L4 , XBOOLE_1:3 , XBOOLE_1:27;
end;
