L128: D16 is_a_fixpoint_of D15 by L71;
L129: D16 = ( D15 . D16 ) by L128 , ABIAN:def 3;
thus L130: F2(D16) = D16 by L129 , L114;
thus L131: thesis;
end;
theorem
L132: (for R14 being non  empty set holds (for R15 being non  empty set holds (for R16 being (Function of R14 , R15) holds (for R17 being (Function of R15 , R14) holds (ex B19 , B20 , B21 , B22 being set st (B19 misses B20 & B21 misses B22 & ( B19 \/ B20 ) = R14 & ( B21 \/ B22 ) = R15 & ( R16 .: B19 ) = B21 & ( R17 .: B22 ) = B20))))))
proof
let R14 being non  empty set;
let R15 being non  empty set;
let R16 being (Function of R14 , R15);
let R17 being (Function of R15 , R14);
deffunc H1(set) = ( R14 \ ( R17 .: ( R15 \ ( R16 .: $1 ) ) ) );
L133: (for B23 being set holds (B23 in ( bool R14 ) implies H1(B23) in ( bool R14 )));
consider C28 being (Function of ( bool R14 ) , ( bool R14 )) such that L134: (for B24 being set holds (B24 in ( bool R14 ) implies ( C28 . B24 ) = H1(B24))) from FUNCT_2:sch 2(L133);
