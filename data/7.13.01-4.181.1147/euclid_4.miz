theorem
L1: (for R7 being Nat holds (for R8 being (Element of ( REAL R7 )) holds (( ( ( 0 ) * R8 ) + R8 ) = R8 & ( R8 + ( 0* R7 ) ) = R8)))
proof
let R7 being Nat;
let R8 being (Element of ( REAL R7 ));
