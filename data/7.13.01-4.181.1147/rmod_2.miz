let R7 being (RightMod of R5);
let R10 being (Vector of R7);
let R13 being (Vector of R7);
let R19 being (Submodule of R7);
reconsider D11 = (the carrier of R19) as (Subset of R7) by L68;
assume L121: (R10 in R19 & R13 in R19);
L122: (R10 in (the carrier of R19) & R13 in (the carrier of R19)) by L121 , STRUCT_0:def 5;
L123: D11 is  linearly-closed by L104;
L124: ( R10 + R13 ) in (the carrier of R19) by L123 , L122 , L1;
thus L125: thesis by L124 , STRUCT_0:def 5;
end;
theorem
L126: (for R5 being Ring holds (for R6 being (Scalar of R5) holds (for R7 being (RightMod of R5) holds (for R13 being (Vector of R7) holds (for R19 being (Submodule of R7) holds (R13 in R19 implies ( R13 * R6 ) in R19))))))
proof
let R5 being Ring;
let R6 being (Scalar of R5);
let R7 being (RightMod of R5);
let R13 being (Vector of R7);
let R19 being (Submodule of R7);
reconsider D12 = (the carrier of R19) as (Subset of R7) by L68;
assume L127: R13 in R19;
L128: R13 in (the carrier of R19) by L127 , STRUCT_0:def 5;
L129: D12 is  linearly-closed by L104;
L130: ( R13 * R6 ) in (the carrier of R19) by L129 , L128 , L1;
thus L131: thesis by L130 , STRUCT_0:def 5;
end;
theorem
L132: (for R5 being Ring holds (for R7 being (RightMod of R5) holds (for R13 being (Vector of R7) holds (for R19 being (Submodule of R7) holds (R13 in R19 implies ( - R13 ) in R19)))))
proof
