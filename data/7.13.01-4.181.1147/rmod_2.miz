end;
theorem
L141: (for R5 being Ring holds (for R7 being (RightMod of R5) holds R7 is (Submodule of R7)))
proof
let R5 being Ring;
let R7 being (RightMod of R5);
L142: (the rmult of R7) = ( (the rmult of R7) | ([: (the carrier of R7) , (the carrier of R5) :] qua set) ) by RELSET_1:19;
L143: (( 0. R7 ) = ( 0. R7 ) & (the addF of R7) = ( (the addF of R7) | ([: (the carrier of R7) , (the carrier of R7) :] qua set) )) by RELSET_1:19;
thus L144: thesis by L143 , L142 , L68;
end;
theorem
L145: (for R5 being Ring holds (for B1 , B2 being  strict (RightMod of R5) holds ((B2 is (Submodule of B1) & B1 is (Submodule of B2)) implies B2 = B1)))
proof
let R5 being Ring;
let C1 , C2 being  strict (RightMod of R5);
assume that
L146: C2 is (Submodule of C1)
and
L147: C1 is (Submodule of C2);
set D13 = (the carrier of C1);
set D14 = (the carrier of C2);
L148: (D14 c= D13 & D13 c= D14) by L146 , L147 , L68;
L149: D14 = D13 by L148 , XBOOLE_0:def 10;
set D15 = (the addF of C1);
set D16 = (the addF of C2);
L150: (D16 = ( D15 || D14 ) & D15 = ( D16 || D13 )) by L146 , L147 , L68;
L151: D16 = D15 by L150 , L149 , RELAT_1:72;
set D17 = (the rmult of C1);
