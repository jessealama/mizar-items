begin
definition
let R5 being Ring;
let R7 being (RightMod of R5);
let R16 being (Subset of R7);
attr R16 is  linearly-closed
means
:L1: ((for R13 being (Vector of R7) holds (for R10 being (Vector of R7) holds ((R13 in R16 & R10 in R16) implies ( R13 + R10 ) in R16))) & (for R6 being (Scalar of R5) holds (for R13 being (Vector of R7) holds (R13 in R16 implies ( R13 * R6 ) in R16))));
end;
theorem
L3: (for R5 being Ring holds (for R7 being (RightMod of R5) holds (for R16 being (Subset of R7) holds ((R16 <> ( {} ) & R16 is  linearly-closed) implies ( 0. R7 ) in R16))))
proof
let R5 being Ring;
let R7 being (RightMod of R5);
let R16 being (Subset of R7);
assume that
L4: R16 <> ( {} )
and
L5: R16 is  linearly-closed;
set D1 = the (Element of R16);
reconsider D2 = D1 as (Element of R7) by L4 , TARSKI:def 3;
L6: ( D2 * ( 0. R5 ) ) in R16 by L4 , L5 , L1;
thus L7: thesis by L6 , VECTSP_2:32;
end;
theorem
L8: (for R5 being Ring holds (for R7 being (RightMod of R5) holds (for R16 being (Subset of R7) holds (R16 is  linearly-closed implies (for R13 being (Vector of R7) holds (R13 in R16 implies ( - R13 ) in R16))))))
proof
let R5 being Ring;
let R7 being (RightMod of R5);
let R16 being (Subset of R7);
assume L9: R16 is  linearly-closed;
let R13 being (Vector of R7);
assume L10: R13 in R16;
L11: ( R13 * ( - ( 1_ R5 ) ) ) in R16 by L10 , L9 , L1;
thus L12: thesis by L11 , VECTSP_2:32;
end;
theorem
L13: (for R5 being Ring holds (for R7 being (RightMod of R5) holds (for R16 being (Subset of R7) holds (R16 is  linearly-closed implies (for R13 being (Vector of R7) holds (for R10 being (Vector of R7) holds ((R13 in R16 & R10 in R16) implies ( R13 - R10 ) in R16)))))))
proof
let R5 being Ring;
let R7 being (RightMod of R5);
let R16 being (Subset of R7);
assume L14: R16 is  linearly-closed;
let R13 being (Vector of R7);
let R10 being (Vector of R7);
assume that
L15: R13 in R16
and
L16: R10 in R16;
L17: ( - R10 ) in R16 by L14 , L16 , L8;
thus L18: thesis by L17 , L14 , L15 , L1;
end;
theorem
L19: (for R5 being Ring holds (for R7 being (RightMod of R5) holds { ( 0. R7 ) } is  linearly-closed))
proof
let R5 being Ring;
let R7 being (RightMod of R5);
thus L20: (for R13 being (Vector of R7) holds (for R10 being (Vector of R7) holds ((R13 in { ( 0. R7 ) } & R10 in { ( 0. R7 ) }) implies ( R13 + R10 ) in { ( 0. R7 ) })))
proof
let R13 being (Vector of R7);
let R10 being (Vector of R7);
assume L21: (R13 in { ( 0. R7 ) } & R10 in { ( 0. R7 ) });
L22: (R13 = ( 0. R7 ) & R10 = ( 0. R7 )) by L21 , TARSKI:def 1;
L23: ( R13 + R10 ) = ( 0. R7 ) by L22 , RLVECT_1:def 4;
thus L24: thesis by L23 , TARSKI:def 1;
end;

let R6 being (Scalar of R5);
let R13 being (Vector of R7);
assume L25: R13 in { ( 0. R7 ) };
L26: R13 = ( 0. R7 ) by L25 , TARSKI:def 1;
thus L27: thesis by L26 , L25 , VECTSP_2:32;
end;
theorem
L28: (for R5 being Ring holds (for R7 being (RightMod of R5) holds (for R16 being (Subset of R7) holds ((the carrier of R7) = R16 implies R16 is  linearly-closed))))
proof
let R5 being Ring;
let R7 being (RightMod of R5);
let R16 being (Subset of R7);
assume L29: (the carrier of R7) = R16;
thus L30: (for R13 being (Vector of R7) holds (for R10 being (Vector of R7) holds ((R13 in R16 & R10 in R16) implies ( R13 + R10 ) in R16))) by L29;
let R6 being (Scalar of R5);
let R13 being (Vector of R7);
assume L31: R13 in R16;
thus L32: thesis by L29;
end;
theorem
L33: (for R5 being Ring holds (for R7 being (RightMod of R5) holds (for R16 being (Subset of R7) holds (for R17 being (Subset of R7) holds (for R18 being (Subset of R7) holds ((R16 is  linearly-closed & R17 is  linearly-closed & R18 = { ( R13 + R10 ) where R13 is (Vector of R7) , R10 is (Vector of R7) : (R13 in R16 & R10 in R17) }) implies R18 is  linearly-closed))))))
proof
let R5 being Ring;
let R7 being (RightMod of R5);
let R16 being (Subset of R7);
let R17 being (Subset of R7);
let R18 being (Subset of R7);
assume that
L34: (R16 is  linearly-closed & R17 is  linearly-closed)
and
L35: R18 = { ( R13 + R10 ) where R13 is (Vector of R7) , R10 is (Vector of R7) : (R13 in R16 & R10 in R17) };
thus L36: (for R13 being (Vector of R7) holds (for R10 being (Vector of R7) holds ((R13 in R18 & R10 in R18) implies ( R13 + R10 ) in R18)))
proof
let R13 being (Vector of R7);
let R10 being (Vector of R7);
assume that
L37: R13 in R18
and
L38: R10 in R18;
consider R14 being (Vector of R7), R15 being (Vector of R7) such that L39: R13 = ( R14 + R15 ) and L40: (R14 in R16 & R15 in R17) by L35 , L37;
consider R11 being (Vector of R7), R12 being (Vector of R7) such that L41: R10 = ( R11 + R12 ) and L42: (R11 in R16 & R12 in R17) by L35 , L38;
L43: ( R13 + R10 ) = ( ( ( R14 + R15 ) + R11 ) + R12 ) by L39 , L41 , RLVECT_1:def 3
.= ( ( ( R14 + R11 ) + R15 ) + R12 ) by RLVECT_1:def 3
.= ( ( R14 + R11 ) + ( R15 + R12 ) ) by RLVECT_1:def 3;
L44: (( R14 + R11 ) in R16 & ( R15 + R12 ) in R17) by L34 , L40 , L42 , L1;
thus L45: thesis by L44 , L35 , L43;
end;

let R6 being (Scalar of R5);
let R13 being (Vector of R7);
assume L46: R13 in R18;
consider R14 being (Vector of R7), R15 being (Vector of R7) such that L47: R13 = ( R14 + R15 ) and L48: (R14 in R16 & R15 in R17) by L46 , L35;
L49: ( R13 * R6 ) = ( ( R14 * R6 ) + ( R15 * R6 ) ) by L47 , VECTSP_2:def 9;
L50: (( R14 * R6 ) in R16 & ( R15 * R6 ) in R17) by L34 , L48 , L1;
thus L51: thesis by L50 , L35 , L49;
end;
theorem
L52: (for R5 being Ring holds (for R7 being (RightMod of R5) holds (for R16 being (Subset of R7) holds (for R17 being (Subset of R7) holds ((R16 is  linearly-closed & R17 is  linearly-closed) implies ( R16 /\ R17 ) is  linearly-closed)))))
proof
let R5 being Ring;
let R7 being (RightMod of R5);
let R16 being (Subset of R7);
let R17 being (Subset of R7);
assume that
L53: R16 is  linearly-closed
and
L54: R17 is  linearly-closed;
thus L55: (for R13 being (Vector of R7) holds (for R10 being (Vector of R7) holds ((R13 in ( R16 /\ R17 ) & R10 in ( R16 /\ R17 )) implies ( R13 + R10 ) in ( R16 /\ R17 ))))
proof
let R13 being (Vector of R7);
let R10 being (Vector of R7);
assume L56: (R13 in ( R16 /\ R17 ) & R10 in ( R16 /\ R17 ));
L57: (R13 in R17 & R10 in R17) by L56 , XBOOLE_0:def 4;
L58: ( R13 + R10 ) in R17 by L57 , L54 , L1;
L59: (R13 in R16 & R10 in R16) by L56 , XBOOLE_0:def 4;
L60: ( R13 + R10 ) in R16 by L59 , L53 , L1;
thus L61: thesis by L60 , L58 , XBOOLE_0:def 4;
end;

let R6 being (Scalar of R5);
let R13 being (Vector of R7);
assume L62: R13 in ( R16 /\ R17 );
L63: R13 in R17 by L62 , XBOOLE_0:def 4;
L64: ( R13 * R6 ) in R17 by L63 , L54 , L1;
L65: R13 in R16 by L62 , XBOOLE_0:def 4;
L66: ( R13 * R6 ) in R16 by L65 , L53 , L1;
thus L67: thesis by L66 , L64 , XBOOLE_0:def 4;
end;
definition
let R5 being Ring;
let R7 being (RightMod of R5);
mode Submodule of R7
 -> (RightMod of R5)
means :L68: ((the carrier of it) c= (the carrier of R7) & ( 0. it ) = ( 0. R7 ) & (the addF of it) = ( (the addF of R7) | ([: (the carrier of it) , (the carrier of it) :] qua set) ) & (the rmult of it) = ( (the rmult of R7) | ([: (the carrier of it) , (the carrier of R5) :] qua set) ));
existence
proof
take R7;
thus L69: ((the carrier of R7) c= (the carrier of R7) & ( 0. R7 ) = ( 0. R7 ));
thus L70: thesis by RELSET_1:19;
end;
end;
theorem
L72: (for R1 being set holds (for R5 being Ring holds (for R7 being (RightMod of R5) holds (for R20 being (Submodule of R7) holds (for R21 being (Submodule of R7) holds ((R1 in R20 & R20 is (Submodule of R21)) implies R1 in R21))))))
proof
let R1 being set;
let R5 being Ring;
let R7 being (RightMod of R5);
let R20 being (Submodule of R7);
let R21 being (Submodule of R7);
assume L73: (R1 in R20 & R20 is (Submodule of R21));
L74: (R1 in (the carrier of R20) & (the carrier of R20) c= (the carrier of R21)) by L73 , L68 , STRUCT_0:def 5;
thus L75: thesis by L74 , STRUCT_0:def 5;
end;
theorem
L76: (for R1 being set holds (for R5 being Ring holds (for R7 being (RightMod of R5) holds (for R19 being (Submodule of R7) holds (R1 in R19 implies R1 in R7)))))
proof
let R1 being set;
let R5 being Ring;
let R7 being (RightMod of R5);
let R19 being (Submodule of R7);
assume L77: R1 in R19;
L78: R1 in (the carrier of R19) by L77 , STRUCT_0:def 5;
L79: (the carrier of R19) c= (the carrier of R7) by L68;
thus L80: thesis by L79 , L78 , STRUCT_0:def 5;
end;
theorem
L81: (for R5 being Ring holds (for R7 being (RightMod of R5) holds (for R19 being (Submodule of R7) holds (for R22 being (Vector of R19) holds R22 is (Vector of R7)))))
proof
let R5 being Ring;
let R7 being (RightMod of R5);
let R19 being (Submodule of R7);
let R22 being (Vector of R19);
L82: R22 in R7 by L76 , RLVECT_1:1;
thus L83: thesis by L82 , STRUCT_0:def 5;
end;
theorem
L84: (for R5 being Ring holds (for R7 being (RightMod of R5) holds (for R19 being (Submodule of R7) holds ( 0. R19 ) = ( 0. R7 )))) by L68;
theorem
L85: (for R5 being Ring holds (for R7 being (RightMod of R5) holds (for R20 being (Submodule of R7) holds (for R21 being (Submodule of R7) holds ( 0. R20 ) = ( 0. R21 )))))
proof
let R5 being Ring;
let R7 being (RightMod of R5);
let R20 being (Submodule of R7);
let R21 being (Submodule of R7);
thus L86: ( 0. R20 ) = ( 0. R7 ) by L68
.= ( 0. R21 ) by L68;
end;
theorem
L87: (for R5 being Ring holds (for R7 being (RightMod of R5) holds (for R10 being (Vector of R7) holds (for R13 being (Vector of R7) holds (for R19 being (Submodule of R7) holds (for R23 being (Vector of R19) holds (for R24 being (Vector of R19) holds ((R23 = R13 & R24 = R10) implies ( R23 + R24 ) = ( R13 + R10 )))))))))
proof
let R5 being Ring;
let R7 being (RightMod of R5);
let R10 being (Vector of R7);
let R13 being (Vector of R7);
let R19 being (Submodule of R7);
let R23 being (Vector of R19);
let R24 being (Vector of R19);
assume L88: (R13 = R23 & R10 = R24);
set D3 = [: (the carrier of R19) , (the carrier of R19) :];
L89: ( R23 + R24 ) = ( ( (the addF of R7) | (D3 qua set) ) . [ R23 , R24 ] ) by L68;
thus L90: thesis by L89 , L88 , FUNCT_1:49;
end;
theorem
L91: (for R5 being Ring holds (for R6 being (Scalar of R5) holds (for R7 being (RightMod of R5) holds (for R13 being (Vector of R7) holds (for R19 being (Submodule of R7) holds (for R22 being (Vector of R19) holds (R22 = R13 implies ( R22 * R6 ) = ( R13 * R6 ))))))))
proof
let R5 being Ring;
let R6 being (Scalar of R5);
let R7 being (RightMod of R5);
let R13 being (Vector of R7);
let R19 being (Submodule of R7);
let R22 being (Vector of R19);
assume L92: R22 = R13;
L93: ( R22 * R6 ) = ( ( (the rmult of R7) | ([: (the carrier of R19) , (the carrier of R5) :] qua set) ) . [ R22 , R6 ] ) by L68;
thus L94: thesis by L93 , L92 , FUNCT_1:49;
end;
theorem
L95: (for R5 being Ring holds (for R7 being (RightMod of R5) holds (for R13 being (Vector of R7) holds (for R19 being (Submodule of R7) holds (for R22 being (Vector of R19) holds (R22 = R13 implies ( - R13 ) = ( - R22 )))))))
proof
let R5 being Ring;
let R7 being (RightMod of R5);
let R13 being (Vector of R7);
let R19 being (Submodule of R7);
let R22 being (Vector of R19);
L96: (( - R13 ) = ( R13 * ( - ( 1_ R5 ) ) ) & ( - R22 ) = ( R22 * ( - ( 1_ R5 ) ) )) by VECTSP_2:32;
assume L97: R22 = R13;
thus L98: thesis by L97 , L96 , L91;
end;
theorem
L99: (for R5 being Ring holds (for R7 being (RightMod of R5) holds (for R10 being (Vector of R7) holds (for R13 being (Vector of R7) holds (for R19 being (Submodule of R7) holds (for R23 being (Vector of R19) holds (for R24 being (Vector of R19) holds ((R23 = R13 & R24 = R10) implies ( R23 - R24 ) = ( R13 - R10 )))))))))
proof
let R5 being Ring;
let R7 being (RightMod of R5);
let R10 being (Vector of R7);
let R13 being (Vector of R7);
let R19 being (Submodule of R7);
let R23 being (Vector of R19);
let R24 being (Vector of R19);
assume that
L100: R23 = R13
and
L101: R24 = R10;
L102: ( - R24 ) = ( - R10 ) by L101 , L95;
thus L103: thesis by L102 , L100 , L87;
end;
L104: (for R5 being Ring holds (for R7 being (RightMod of R5) holds (for R16 being (Subset of R7) holds (for R19 being (Submodule of R7) holds ((the carrier of R19) = R16 implies R16 is  linearly-closed)))))
proof
let R5 being Ring;
let R7 being (RightMod of R5);
let R16 being (Subset of R7);
let R19 being (Submodule of R7);
set D4 = (the carrier of R19);
reconsider D5 = R19 as (RightMod of R5);
assume L105: (the carrier of R19) = R16;
thus L106: (for R13 being (Vector of R7) holds (for R10 being (Vector of R7) holds ((R13 in R16 & R10 in R16) implies ( R13 + R10 ) in R16)))
proof
let R13 being (Vector of R7);
let R10 being (Vector of R7);
assume L107: (R13 in R16 & R10 in R16);
reconsider D6 = R13 , D7 = R10 as (Vector of D5) by L107 , L105;
reconsider D8 = ( D6 + D7 ) as (Element of D4);
L108: D8 in R16 by L105;
thus L109: thesis by L108 , L87;
end;

let R6 being (Scalar of R5);
let R13 being (Vector of R7);
assume L110: R13 in R16;
reconsider D9 = R13 as (Vector of D5) by L110 , L105;
reconsider D10 = ( D9 * R6 ) as (Element of D4);
L111: D10 in R16 by L105;
thus L112: thesis by L111 , L91;
end;
theorem
L113: (for R5 being Ring holds (for R7 being (RightMod of R5) holds (for R19 being (Submodule of R7) holds ( 0. R7 ) in R19)))
proof
