scheme UparrowUnion { F1() -> RelStr , P1[set] } : (for B1 being (Subset-Family of F1()) holds (B1 = { B2 where B2 is (Subset of F1()) : P1[ B2 ] } implies ( uparrow ( union B1 ) ) = ( union { ( uparrow B3 ) where B3 is (Subset of F1()) : P1[ B3 ] } )))
proof
let C1 being (Subset-Family of F1());
assume L1: C1 = { B4 where B4 is (Subset of F1()) : P1[ B4 ] };
L2: ( union { ( uparrow B5 ) where B5 is (Subset of F1()) : P1[ B5 ] } ) c= ( uparrow ( union C1 ) )
proof
let C2 being set;
assume L3: C2 in ( union { ( uparrow B6 ) where B6 is (Subset of F1()) : P1[ B6 ] } );
consider C3 being set such that L4: C2 in C3 and L5: C3 in { ( uparrow B7 ) where B7 is (Subset of F1()) : P1[ B7 ] } by L3 , TARSKI:def 4;
consider C4 being (Subset of F1()) such that L6: C3 = ( uparrow C4 ) and L7: P1[ C4 ] by L5;
reconsider D1 = C2 as (Element of F1()) by L4 , L6;
consider C5 being (Element of F1()) such that L8: C5 <= D1 and L9: C5 in C4 by L4 , L6 , WAYBEL_0:def 16;
L10: C4 in C1 by L1 , L7;
L11: C5 in ( union C1 ) by L10 , L9 , TARSKI:def 4;
thus L12: thesis by L11 , L8 , WAYBEL_0:def 16;
end;
L13: ( uparrow ( union C1 ) ) c= ( union { ( uparrow B8 ) where B8 is (Subset of F1()) : P1[ B8 ] } )
proof
let C6 being set;
assume L14: C6 in ( uparrow ( union C1 ) );
reconsider D2 = C6 as (Element of F1()) by L14;
consider C7 being (Element of F1()) such that L15: C7 <= D2 and L16: C7 in ( union C1 ) by L14 , WAYBEL_0:def 16;
consider C8 being set such that L17: C7 in C8 and L18: C8 in C1 by L16 , TARSKI:def 4;
consider C9 being (Subset of F1()) such that L19: C8 = C9 and L20: P1[ C9 ] by L1 , L18;
L21: ( uparrow C9 ) in { ( uparrow B9 ) where B9 is (Subset of F1()) : P1[ B9 ] } by L20;
L22: C6 in ( uparrow C9 ) by L15 , L17 , L19 , WAYBEL_0:def 16;
thus L23: thesis by L22 , L21 , TARSKI:def 4;
end;
thus L24: thesis by L13 , L2 , XBOOLE_0:def 10;
end;
