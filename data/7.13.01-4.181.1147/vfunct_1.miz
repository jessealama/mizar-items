let R3 being set;
let R4 being non  empty set;
let R6 being RealNormSpace;
let R7 being (PartFunc of R4 , R6);
assume L716: ( R7 | R3 ) is  constant;
consider C53 being (VECTOR of R6) such that L717: (for R5 being (Element of R4) holds (R5 in ( R3 /\ ( dom R7 ) ) implies ( R7 /. R5 ) = C53)) by L716 , PARTFUN2:35;
L718:
now
take D14 = ||. C53 .||;
let R5 being (Element of R4);
assume L719: R5 in ( R3 /\ ( dom R7 ) );
thus L720: ||. ( R7 /. R5 ) .|| <= D14 by L719 , L717;
end;
thus L721: thesis by L718 , L509;
end;
theorem
L722: (for R3 being set holds (for R4 being non  empty set holds (for R6 being RealNormSpace holds (for R7 being (PartFunc of R4 , R6) holds (( R7 | R3 ) is  constant implies ((for R11 being Real holds ( R11 (#) R7 ) is_bounded_on R3) & ( - R7 ) is_bounded_on R3 & ( ||. R7 .|| | R3 ) is  bounded))))))
proof
let R3 being set;
let R4 being non  empty set;
let R6 being RealNormSpace;
let R7 being (PartFunc of R4 , R6);
assume L723: ( R7 | R3 ) is  constant;
thus L724:now
let R11 being Real;
L725: ( ( R11 (#) R7 ) | R3 ) is  constant by L723 , L686;
thus L726: ( R11 (#) R7 ) is_bounded_on R3 by L725 , L715;
end;
L727: ( ( - R7 ) | R3 ) is  constant by L723 , L697;
thus L728: ( - R7 ) is_bounded_on R3 by L727 , L715;
L729: ( ||. R7 .|| | R3 ) is  constant by L723 , L697;
thus L730: thesis by L729;
end;
theorem
L731: (for R2 being set holds (for R3 being set holds (for R4 being non  empty set holds (for R6 being RealNormSpace holds (for R8 being (PartFunc of R4 , R6) holds (for R9 being (PartFunc of R4 , R6) holds ((R8 is_bounded_on R2 & ( R9 | R3 ) is  constant) implies ( R8 + R9 ) is_bounded_on ( R2 /\ R3 ))))))))
proof
let R2 being set;
let R3 being set;
let R4 being non  empty set;
let R6 being RealNormSpace;
let R8 being (PartFunc of R4 , R6);
let R9 being (PartFunc of R4 , R6);
assume that
L732: R8 is_bounded_on R2
and
L733: ( R9 | R3 ) is  constant;
L734: R9 is_bounded_on R3 by L733 , L715;
thus L735: thesis by L734 , L732 , L561;
end;
theorem
L736: (for R2 being set holds (for R3 being set holds (for R4 being non  empty set holds (for R6 being RealNormSpace holds (for R8 being (PartFunc of R4 , R6) holds (for R9 being (PartFunc of R4 , R6) holds ((R8 is_bounded_on R2 & ( R9 | R3 ) is  constant) implies (( R8 - R9 ) is_bounded_on ( R2 /\ R3 ) & ( R9 - R8 ) is_bounded_on ( R2 /\ R3 )))))))))
proof
let R2 being set;
let R3 being set;
let R4 being non  empty set;
let R6 being RealNormSpace;
let R8 being (PartFunc of R4 , R6);
let R9 being (PartFunc of R4 , R6);
assume that
L737: R8 is_bounded_on R2
and
L738: ( R9 | R3 ) is  constant;
L739: R9 is_bounded_on R3 by L738 , L715;
L740: ( - R9 ) is_bounded_on R3 by L739 , L546;
L741: ( R8 + ( - R9 ) ) is_bounded_on ( R2 /\ R3 ) by L740 , L737 , L561;
L742: ( - R8 ) is_bounded_on R2 by L737 , L546;
L743: ( R9 + ( - R8 ) ) is_bounded_on ( R3 /\ R2 ) by L742 , L739 , L561;
thus L744: thesis by L743 , L741 , L310;
end;
