begin
definition
let R4 being non  empty set;
let C1 being non  empty addLoopStr;
let C2 , C3 being (PartFunc of R4 , C1);
deffunc H1(set) = ( ( C2 /. $1 ) + ( C3 /. $1 ) );
deffunc H2(set) = ( ( C2 /. $1 ) - ( C3 /. $1 ) );
defpred S1[ set ] means $1 in ( ( dom C2 ) /\ ( dom C3 ) );
set D1 = ( ( dom C2 ) /\ ( dom C3 ) );
func C2 + C3 -> (PartFunc of R4 , C1) means 
:L1: (( dom it ) = ( ( dom C2 ) /\ ( dom C3 ) ) & (for R5 being (Element of R4) holds (R5 in ( dom it ) implies ( it /. R5 ) = ( ( C2 /. R5 ) + ( C3 /. R5 ) ))));
existence
proof
consider C4 being (PartFunc of R4 , C1) such that L2: (for R5 being (Element of R4) holds (R5 in ( dom C4 ) iff S1[ R5 ])) and L3: (for R5 being (Element of R4) holds (R5 in ( dom C4 ) implies ( C4 /. R5 ) = H1(R5))) from PARTFUN2:sch 2;
take C4;
thus L4: thesis by L2 , L3 , SUBSET_1:3;
end;
uniqueness
proof
thus L5: (for B1 , B2 being (PartFunc of R4 , C1) holds ((( dom B1 ) = D1 & (for R5 being (Element of R4) holds (R5 in ( dom B1 ) implies ( B1 /. R5 ) = H1(R5))) & ( dom B2 ) = D1 & (for R5 being (Element of R4) holds (R5 in ( dom B2 ) implies ( B2 /. R5 ) = H1(R5)))) implies B1 = B2)) from PARTFUN2:sch 3;
end;
func C2 - C3 -> (PartFunc of R4 , C1) means 
:L6: (( dom it ) = ( ( dom C2 ) /\ ( dom C3 ) ) & (for R5 being (Element of R4) holds (R5 in ( dom it ) implies ( it /. R5 ) = ( ( C2 /. R5 ) - ( C3 /. R5 ) ))));
existence
proof
consider C5 being (PartFunc of R4 , C1) such that L7: (for R5 being (Element of R4) holds (R5 in ( dom C5 ) iff S1[ R5 ])) and L8: (for R5 being (Element of R4) holds (R5 in ( dom C5 ) implies ( C5 /. R5 ) = H2(R5))) from PARTFUN2:sch 2;
take C5;
thus L9: thesis by L7 , L8 , SUBSET_1:3;
end;
uniqueness
proof
thus L10: (for B3 , B4 being (PartFunc of R4 , C1) holds ((( dom B3 ) = D1 & (for B5 being (Element of R4) holds (B5 in ( dom B3 ) implies ( B3 /. B5 ) = H2(B5))) & ( dom B4 ) = D1 & (for B6 being (Element of R4) holds (B6 in ( dom B4 ) implies ( B4 /. B6 ) = H2(B6)))) implies B3 = B4)) from PARTFUN2:sch 3;
end;
end;
registration
let R4 being non  empty set;
let C6 being non  empty addLoopStr;
let C7 , C8 being (Function of R4 , C6);
cluster ( C7 + C8 ) ->  total;
coherence
proof
thus L12: ( dom ( C7 + C8 ) ) = ( ( dom C7 ) /\ ( dom C8 ) ) by L1
.= ( R4 /\ ( dom C8 ) ) by FUNCT_2:def 1
.= ( R4 /\ R4 ) by FUNCT_2:def 1
.= R4;
end;
cluster ( C7 - C8 ) ->  total;
coherence
proof
thus L13: ( dom ( C7 - C8 ) ) = ( ( dom C7 ) /\ ( dom C8 ) ) by L6
.= ( R4 /\ ( dom C8 ) ) by FUNCT_2:def 1
.= ( R4 /\ R4 ) by FUNCT_2:def 1
.= R4;
end;
end;
definition
let R4 being non  empty set;
let C9 being non  empty RLSStruct;
let C10 being (PartFunc of R4 , ( REAL ));
let C11 being (PartFunc of R4 , C9);
deffunc H3((Element of R4)) = ( ( C10 . $1 ) * ( C11 /. $1 ) );
defpred S2[ set ] means $1 in ( ( dom C10 ) /\ ( dom C11 ) );
set D2 = ( ( dom C10 ) /\ ( dom C11 ) );
func C10 (#) C11 -> (PartFunc of R4 , C9) means 
:L15: (( dom it ) = ( ( dom C10 ) /\ ( dom C11 ) ) & (for R5 being (Element of R4) holds (R5 in ( dom it ) implies ( it /. R5 ) = ( ( C10 . R5 ) * ( C11 /. R5 ) ))));
existence
proof
consider C12 being (PartFunc of R4 , C9) such that L16: (for R5 being (Element of R4) holds (R5 in ( dom C12 ) iff S2[ R5 ])) and L17: (for R5 being (Element of R4) holds (R5 in ( dom C12 ) implies ( C12 /. R5 ) = H3(R5))) from PARTFUN2:sch 2;
take C12;
thus L18: thesis by L16 , L17 , SUBSET_1:3;
end;
uniqueness
proof
thus L19: (for B7 , B8 being (PartFunc of R4 , C9) holds ((( dom B7 ) = D2 & (for B9 being (Element of R4) holds (B9 in ( dom B7 ) implies ( B7 /. B9 ) = H3(B9))) & ( dom B8 ) = D2 & (for B10 being (Element of R4) holds (B10 in ( dom B8 ) implies ( B8 /. B10 ) = H3(B10)))) implies B7 = B8)) from PARTFUN2:sch 3;
end;
end;
registration
let R4 being non  empty set;
let C13 being non  empty RLSStruct;
let C14 being (Function of R4 , ( REAL ));
let C15 being (Function of R4 , C13);
cluster ( C14 (#) C15 ) ->  total;
coherence
proof
thus L21: ( dom ( C14 (#) C15 ) ) = ( ( dom C14 ) /\ ( dom C15 ) ) by L15
.= ( R4 /\ ( dom C15 ) ) by FUNCT_2:def 1
.= ( R4 /\ R4 ) by FUNCT_2:def 1
.= R4;
end;
end;
definition
let R4 being non  empty set;
let C16 being non  empty RLSStruct;
let C17 being (PartFunc of R4 , C16);
let R11 being Real;
deffunc H4((Element of R4)) = ( R11 * ( C17 /. $1 ) );
defpred S3[ set ] means $1 in ( dom C17 );
func R11 (#) C17 -> (PartFunc of R4 , C16) means 
:L23: (( dom it ) = ( dom C17 ) & (for R5 being (Element of R4) holds (R5 in ( dom it ) implies ( it /. R5 ) = ( R11 * ( C17 /. R5 ) ))));
existence
proof
consider C18 being (PartFunc of R4 , C16) such that L24: (for R5 being (Element of R4) holds (R5 in ( dom C18 ) iff S3[ R5 ])) and L25: (for R5 being (Element of R4) holds (R5 in ( dom C18 ) implies ( C18 /. R5 ) = H4(R5))) from PARTFUN2:sch 2;
take C18;
thus L26: thesis by L24 , L25 , SUBSET_1:3;
end;
uniqueness
proof
set D3 = ( dom C17 );
thus L27: (for B11 , B12 being (PartFunc of R4 , C16) holds ((( dom B11 ) = D3 & (for B13 being (Element of R4) holds (B13 in ( dom B11 ) implies ( B11 /. B13 ) = H4(B13))) & ( dom B12 ) = D3 & (for B14 being (Element of R4) holds (B14 in ( dom B12 ) implies ( B12 /. B14 ) = H4(B14)))) implies B11 = B12)) from PARTFUN2:sch 3;
end;
end;
registration
let R4 being non  empty set;
let C19 being non  empty RLSStruct;
let C20 being (Function of R4 , C19);
let R11 being Real;
cluster ( R11 (#) C20 ) ->  total;
coherence
proof
thus L29: ( dom ( R11 (#) C20 ) ) = ( dom C20 ) by L23
.= R4 by FUNCT_2:def 1;
end;
end;
definition
let R4 being non  empty set;
let C21 being non  empty addLoopStr;
let C22 being (PartFunc of R4 , C21);
deffunc H5((Element of R4)) = ( - ( C22 /. $1 ) );
defpred S4[ set ] means $1 in ( dom C22 );
func - C22 -> (PartFunc of R4 , C21) means 
:L31: (( dom it ) = ( dom C22 ) & (for R5 being (Element of R4) holds (R5 in ( dom it ) implies ( it /. R5 ) = ( - ( C22 /. R5 ) ))));
existence
proof
consider C23 being (PartFunc of R4 , C21) such that L32: (for R5 being (Element of R4) holds (R5 in ( dom C23 ) iff S4[ R5 ])) and L33: (for R5 being (Element of R4) holds (R5 in ( dom C23 ) implies ( C23 /. R5 ) = H5(R5))) from PARTFUN2:sch 2;
take C23;
thus L34: thesis by L32 , L33 , SUBSET_1:3;
end;
uniqueness
proof
set D4 = ( dom C22 );
thus L35: (for B15 , B16 being (PartFunc of R4 , C21) holds ((( dom B15 ) = D4 & (for B17 being (Element of R4) holds (B17 in ( dom B15 ) implies ( B15 /. B17 ) = H5(B17))) & ( dom B16 ) = D4 & (for B18 being (Element of R4) holds (B18 in ( dom B16 ) implies ( B16 /. B18 ) = H5(B18)))) implies B15 = B16)) from PARTFUN2:sch 3;
end;
end;
registration
let R4 being non  empty set;
let C24 being non  empty addLoopStr;
let C25 being (Function of R4 , C24);
cluster ( - C25 ) ->  total;
coherence
proof
thus L37: ( dom ( - C25 ) ) = ( dom C25 ) by L31
.= R4 by FUNCT_2:def 1;
end;
end;
theorem
L39: (for R4 being non  empty set holds (for R6 being RealNormSpace holds (for R9 being (PartFunc of R4 , R6) holds (for B19 being (PartFunc of R4 , ( REAL )) holds ( ( dom ( B19 (#) R9 ) ) \ ( ( B19 (#) R9 ) " { ( 0. R6 ) } ) ) = ( ( ( dom B19 ) \ ( B19 " { ( 0 ) } ) ) /\ ( ( dom R9 ) \ ( R9 " { ( 0. R6 ) } ) ) )))))
proof
let R4 being non  empty set;
let R6 being RealNormSpace;
let R9 being (PartFunc of R4 , R6);
let C26 being (PartFunc of R4 , ( REAL ));
thus L40: ( ( dom ( C26 (#) R9 ) ) \ ( ( C26 (#) R9 ) " { ( 0. R6 ) } ) ) c= ( ( ( dom C26 ) \ ( C26 " { ( 0 ) } ) ) /\ ( ( dom R9 ) \ ( R9 " { ( 0. R6 ) } ) ) )
proof
let R1 being set;
assume L41: R1 in ( ( dom ( C26 (#) R9 ) ) \ ( ( C26 (#) R9 ) " { ( 0. R6 ) } ) );
L42: R1 in ( dom ( C26 (#) R9 ) ) by L41 , XBOOLE_0:def 5;
reconsider D5 = R1 as (Element of R4) by L41;
L43: (not R1 in ( ( C26 (#) R9 ) " { ( 0. R6 ) } )) by L41 , XBOOLE_0:def 5;
L44: (not ( ( C26 (#) R9 ) /. D5 ) in { ( 0. R6 ) }) by L43 , L42 , PARTFUN2:26;
L45: ( ( C26 (#) R9 ) /. D5 ) <> ( 0. R6 ) by L44 , TARSKI:def 1;
L46: ( ( C26 . D5 ) * ( R9 /. D5 ) ) <> ( 0. R6 ) by L45 , L42 , L15;
L47: ( R9 /. D5 ) <> ( 0. R6 ) by L46 , RLVECT_1:10;
L48: (not ( R9 /. D5 ) in { ( 0. R6 ) }) by L47 , TARSKI:def 1;
L49: (not D5 in ( R9 " { ( 0. R6 ) } )) by L48 , PARTFUN2:26;
L50: D5 in ( ( dom C26 ) /\ ( dom R9 ) ) by L42 , L15;
L51: D5 in ( dom R9 ) by L50 , XBOOLE_0:def 4;
L52: R1 in ( ( dom R9 ) \ ( R9 " { ( 0. R6 ) } ) ) by L51 , L49 , XBOOLE_0:def 5;
L53: ( C26 . D5 ) <> ( 0 ) by L46 , RLVECT_1:10;
L54: (not ( C26 . D5 ) in { ( 0 ) }) by L53 , TARSKI:def 1;
L55: (not D5 in ( C26 " { ( 0 ) } )) by L54 , FUNCT_1:def 7;
L56: D5 in ( dom C26 ) by L50 , XBOOLE_0:def 4;
L57: R1 in ( ( dom C26 ) \ ( C26 " { ( 0 ) } ) ) by L56 , L55 , XBOOLE_0:def 5;
thus L58: thesis by L57 , L52 , XBOOLE_0:def 4;
end;

thus L59: ( ( ( dom C26 ) \ ( C26 " { ( 0 ) } ) ) /\ ( ( dom R9 ) \ ( R9 " { ( 0. R6 ) } ) ) ) c= ( ( dom ( C26 (#) R9 ) ) \ ( ( C26 (#) R9 ) " { ( 0. R6 ) } ) )
proof
let R1 being set;
assume L60: R1 in ( ( ( dom C26 ) \ ( C26 " { ( 0 ) } ) ) /\ ( ( dom R9 ) \ ( R9 " { ( 0. R6 ) } ) ) );
reconsider D6 = R1 as (Element of R4) by L60;
L61: R1 in ( ( dom R9 ) \ ( R9 " { ( 0. R6 ) } ) ) by L60 , XBOOLE_0:def 4;
L62: R1 in ( dom R9 ) by L61 , XBOOLE_0:def 5;
L63: (not R1 in ( R9 " { ( 0. R6 ) } )) by L61 , XBOOLE_0:def 5;
L64: (not ( R9 /. D6 ) in { ( 0. R6 ) }) by L63 , L62 , PARTFUN2:26;
L65: ( R9 /. D6 ) <> ( 0. R6 ) by L64 , TARSKI:def 1;
L66: R1 in ( ( dom C26 ) \ ( C26 " { ( 0 ) } ) ) by L60 , XBOOLE_0:def 4;
L67: R1 in ( dom C26 ) by L66 , XBOOLE_0:def 5;
L68: D6 in ( ( dom C26 ) /\ ( dom R9 ) ) by L67 , L62 , XBOOLE_0:def 4;
L69: D6 in ( dom ( C26 (#) R9 ) ) by L68 , L15;
L70: (not R1 in ( C26 " { ( 0 ) } )) by L66 , XBOOLE_0:def 5;
L71: (not ( C26 . D6 ) in { ( 0 ) }) by L70 , L67 , FUNCT_1:def 7;
L72: ( C26 . D6 ) <> ( 0 ) by L71 , TARSKI:def 1;
L73: ( ( C26 . D6 ) * ( R9 /. D6 ) ) <> ( 0. R6 ) by L72 , L65 , RLVECT_1:11;
L74: ( ( C26 (#) R9 ) /. D6 ) <> ( 0. R6 ) by L73 , L69 , L15;
L75: (not ( ( C26 (#) R9 ) /. D6 ) in { ( 0. R6 ) }) by L74 , TARSKI:def 1;
L76: (not R1 in ( ( C26 (#) R9 ) " { ( 0. R6 ) } )) by L75 , PARTFUN2:26;
thus L77: thesis by L76 , L69 , XBOOLE_0:def 5;
end;

end;
theorem
L60: (for R4 being non  empty set holds (for R6 being RealNormSpace holds (for R7 being (PartFunc of R4 , R6) holds (( ||. R7 .|| " { ( 0 ) } ) = ( R7 " { ( 0. R6 ) } ) & ( ( - R7 ) " { ( 0. R6 ) } ) = ( R7 " { ( 0. R6 ) } )))))
proof
let R4 being non  empty set;
let R6 being RealNormSpace;
let R7 being (PartFunc of R4 , R6);
L61:
now
let R5 being (Element of R4);
thus L62: (R5 in ( ||. R7 .|| " { ( 0 ) } ) implies R5 in ( R7 " { ( 0. R6 ) } ))
proof
assume L63: R5 in ( ||. R7 .|| " { ( 0 ) } );
L64: R5 in ( dom ||. R7 .|| ) by L63 , FUNCT_1:def 7;
L65: ( ||. R7 .|| . R5 ) in { ( 0 ) } by L63 , FUNCT_1:def 7;
L66: ( ||. R7 .|| . R5 ) = ( 0 ) by L65 , TARSKI:def 1;
L67: ||. ( R7 /. R5 ) .|| = ( 0 ) by L66 , L64 , NORMSP_0:def 3;
L68: ( R7 /. R5 ) = ( 0. R6 ) by L67 , NORMSP_0:def 5;
L69: ( R7 /. R5 ) in { ( 0. R6 ) } by L68 , TARSKI:def 1;
L70: R5 in ( dom R7 ) by L64 , NORMSP_0:def 3;
thus L71: thesis by L70 , L69 , PARTFUN2:26;
end;

assume L72: R5 in ( R7 " { ( 0. R6 ) } );
L73: R5 in ( dom R7 ) by L72 , PARTFUN2:26;
L74: R5 in ( dom ||. R7 .|| ) by L73 , NORMSP_0:def 3;
L75: ( R7 /. R5 ) in { ( 0. R6 ) } by L72 , PARTFUN2:26;
L76: ( R7 /. R5 ) = ( 0. R6 ) by L75 , TARSKI:def 1;
L77: ||. ( R7 /. R5 ) .|| = ( 0 ) by L76;
L78: ( ||. R7 .|| . R5 ) = ( 0 ) by L77 , L74 , NORMSP_0:def 3;
L79: ( ||. R7 .|| . R5 ) in { ( 0 ) } by L78 , TARSKI:def 1;
thus L80: R5 in ( ||. R7 .|| " { ( 0 ) } ) by L79 , L74 , FUNCT_1:def 7;
end;
thus L81: ( ||. R7 .|| " { ( 0 ) } ) = ( R7 " { ( 0. R6 ) } ) by L61 , SUBSET_1:3;
L82:
now
let R5 being (Element of R4);
thus L83: (R5 in ( ( - R7 ) " { ( 0. R6 ) } ) implies R5 in ( R7 " { ( 0. R6 ) } ))
proof
assume L84: R5 in ( ( - R7 ) " { ( 0. R6 ) } );
L85: R5 in ( dom ( - R7 ) ) by L84 , PARTFUN2:26;
L86: ( ( - R7 ) /. R5 ) in { ( 0. R6 ) } by L84 , PARTFUN2:26;
L87: ( ( - R7 ) /. R5 ) = ( 0. R6 ) by L86 , TARSKI:def 1;
L88: ( - ( - ( R7 /. R5 ) ) ) = ( - ( 0. R6 ) ) by L87 , L85 , L31;
L89: ( - ( - ( R7 /. R5 ) ) ) = ( 0. R6 ) by L88 , RLVECT_1:12;
L90: ( R7 /. R5 ) = ( 0. R6 ) by L89 , RLVECT_1:17;
L91: ( R7 /. R5 ) in { ( 0. R6 ) } by L90 , TARSKI:def 1;
L92: R5 in ( dom R7 ) by L85 , L31;
thus L93: thesis by L92 , L91 , PARTFUN2:26;
end;

assume L94: R5 in ( R7 " { ( 0. R6 ) } );
L95: R5 in ( dom R7 ) by L94 , PARTFUN2:26;
L96: R5 in ( dom ( - R7 ) ) by L95 , L31;
L97: ( R7 /. R5 ) in { ( 0. R6 ) } by L94 , PARTFUN2:26;
L98: ( R7 /. R5 ) = ( 0. R6 ) by L97 , TARSKI:def 1;
L99: ( ( - R7 ) /. R5 ) = ( - ( 0. R6 ) ) by L98 , L96 , L31;
L100: ( ( - R7 ) /. R5 ) = ( 0. R6 ) by L99 , RLVECT_1:12;
L101: ( ( - R7 ) /. R5 ) in { ( 0. R6 ) } by L100 , TARSKI:def 1;
thus L102: R5 in ( ( - R7 ) " { ( 0. R6 ) } ) by L101 , L96 , PARTFUN2:26;
end;
thus L103: thesis by L82 , SUBSET_1:3;
end;
theorem
L104: (for R4 being non  empty set holds (for R6 being RealNormSpace holds (for R7 being (PartFunc of R4 , R6) holds (for R11 being Real holds (R11 <> ( 0 ) implies ( ( R11 (#) R7 ) " { ( 0. R6 ) } ) = ( R7 " { ( 0. R6 ) } ))))))
proof
let R4 being non  empty set;
let R6 being RealNormSpace;
let R7 being (PartFunc of R4 , R6);
let R11 being Real;
assume L105: R11 <> ( 0 );
L106:
now
let R5 being (Element of R4);
thus L107: (R5 in ( ( R11 (#) R7 ) " { ( 0. R6 ) } ) implies R5 in ( R7 " { ( 0. R6 ) } ))
proof
assume L108: R5 in ( ( R11 (#) R7 ) " { ( 0. R6 ) } );
