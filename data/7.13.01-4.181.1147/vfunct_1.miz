L114: ( R7 /. R5 ) = ( 0. R6 ) by L113 , L105 , RLVECT_1:36;
L115: ( R7 /. R5 ) in { ( 0. R6 ) } by L114 , TARSKI:def 1;
L116: R5 in ( dom R7 ) by L109 , L23;
thus L117: thesis by L116 , L115 , PARTFUN2:26;
end;

assume L118: R5 in ( R7 " { ( 0. R6 ) } );
L119: R5 in ( dom R7 ) by L118 , PARTFUN2:26;
L120: R5 in ( dom ( R11 (#) R7 ) ) by L119 , L23;
L121: ( R7 /. R5 ) in { ( 0. R6 ) } by L118 , PARTFUN2:26;
L122: ( R11 * ( R7 /. R5 ) ) = ( R11 * ( 0. R6 ) ) by L121 , TARSKI:def 1;
L123: ( R11 * ( R7 /. R5 ) ) = ( 0. R6 ) by L122 , RLVECT_1:10;
L124: ( ( R11 (#) R7 ) /. R5 ) = ( 0. R6 ) by L123 , L120 , L23;
L125: ( ( R11 (#) R7 ) /. R5 ) in { ( 0. R6 ) } by L124 , TARSKI:def 1;
thus L126: R5 in ( ( R11 (#) R7 ) " { ( 0. R6 ) } ) by L125 , L120 , PARTFUN2:26;
end;
thus L127: thesis by L106 , SUBSET_1:3;
end;
theorem
L128: (for R4 being non  empty set holds (for R6 being RealNormSpace holds (for R8 being (PartFunc of R4 , R6) holds (for R9 being (PartFunc of R4 , R6) holds ( R8 + R9 ) = ( R9 + R8 )))))
proof
let R4 being non  empty set;
let R6 being RealNormSpace;
let R8 being (PartFunc of R4 , R6);
let R9 being (PartFunc of R4 , R6);
L129: ( dom ( R8 + R9 ) ) = ( ( dom R9 ) /\ ( dom R8 ) ) by L1
.= ( dom ( R9 + R8 ) ) by L1;
L130:
now
let R5 being (Element of R4);
assume L131: R5 in ( dom ( R8 + R9 ) );
thus L132: ( ( R8 + R9 ) /. R5 ) = ( ( R9 /. R5 ) + ( R8 /. R5 ) ) by L131 , L1
.= ( ( R9 + R8 ) /. R5 ) by L129 , L131 , L1;
end;
thus L133: thesis by L130 , L129 , PARTFUN2:1;
end;
definition
