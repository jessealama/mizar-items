assume L16: (not C6 is  T_1);
consider C7 , C8 being (Point of C6) such that L17: C7 <> C8 and L18: C8 in ( Cl { C7 } ) by L16 , L7;
set D2 = ( ( NAT ) --> C7 );
take C7;
take C8;
take D2;
thus L19: D2 = ( ( NAT ) --> C7 );
thus L20: C7 <> C8 by L17;
thus L21: D2 is_convergent_to C8
proof
let C9 being (Subset of C6);
assume L22: (C9 is  open & C8 in C9);
take ( 0 );
L23: { C7 } meets C9 by L18 , L22 , PRE_TOPC:def 7;
L24: C7 in C9 by L23 , ZFMISC_1:50;
thus L25: thesis by L24 , FUNCOP_1:7;
end;

end;
