L1: (for B1 being non  empty TopSpace holds ((for B2 being (Point of B1) holds ( Cl { B2 } ) = { B2 }) implies B1 is  T_1))
proof
let C1 being non  empty TopSpace;
assume L2: (for B3 being (Point of C1) holds ( Cl { B3 } ) = { B3 });
L3: (for B4 being (Point of C1) holds { B4 } is  closed)
proof
let C2 being (Point of C1);
L4: ( Cl { C2 } ) = { C2 } by L2;
thus L5: thesis by L4 , PRE_TOPC:22;
end;
thus L6: thesis by L3 , URYSOHN1:19;
end;
L7: (for B5 being non  empty TopSpace holds ((not B5 is  T_1) implies (ex B6 , B7 being (Point of B5) st (B6 <> B7 & B7 in ( Cl { B6 } )))))
proof
let C3 being non  empty TopSpace;
assume L8: (not C3 is  T_1);
consider C4 being (Point of C3) such that L9: ( Cl { C4 } ) <> { C4 } by L8 , L1;
L10: ((not { C4 } c= ( Cl { C4 } )) or (not ( Cl { C4 } ) c= { C4 })) by L9 , XBOOLE_0:def 10;
consider C5 being set such that L11: C5 in ( Cl { C4 } ) and L12: (not C5 in { C4 }) by L10 , PRE_TOPC:18 , TARSKI:def 3;
reconsider D1 = C5 as (Point of C3) by L11;
take C4;
take D1;
thus L13: C4 <> D1 by L12 , TARSKI:def 1;
thus L14: thesis by L11;
end;
L15: (for B8 being non  empty TopSpace holds ((not B8 is  T_1) implies (ex B9 , B10 being (Point of B8) st (ex B11 being (sequence of B8) st (B11 = ( ( NAT ) --> B9 ) & B9 <> B10 & B11 is_convergent_to B10)))))
proof
