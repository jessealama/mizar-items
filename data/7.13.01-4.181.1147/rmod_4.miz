begin
L1: (for R1 being Ring holds (for R2 being (RightMod of R1) holds (for R13 being (Vector of R2) holds (for R18 being (FinSequence of R2) holds (for R19 being (FinSequence of R2) holds ((( len R18 ) = ( ( len R19 ) + 1 ) & R19 = ( R18 | ( Seg ( len R19 ) ) ) & R13 = ( R18 . ( len R18 ) )) implies ( Sum R18 ) = ( ( Sum R19 ) + R13 )))))))
proof
let R1 being Ring;
let R2 being (RightMod of R1);
let R13 being (Vector of R2);
let R18 being (FinSequence of R2);
let R19 being (FinSequence of R2);
L2: ((( len R18 ) = ( ( len R19 ) + 1 ) & R19 = ( R18 | ( dom R19 ) ) & R13 = ( R18 . ( len R18 ) )) implies ( Sum R18 ) = ( ( Sum R19 ) + R13 )) by RLVECT_1:38;
thus L3: thesis by L2 , FINSEQ_1:def 3;
end;
theorem
L4: (for R1 being Ring holds (for R2 being (RightMod of R1) holds (for R3 being (Scalar of R1) holds (for R18 being (FinSequence of R2) holds (for R19 being (FinSequence of R2) holds ((( len R18 ) = ( len R19 ) & (for R11 being Nat holds (for R13 being (Vector of R2) holds ((R11 in ( dom R18 ) & R13 = ( R19 . R11 )) implies ( R18 . R11 ) = ( R13 * R3 ))))) implies ( Sum R18 ) = ( ( Sum R19 ) * R3 )))))))
proof
let R1 being Ring;
let R2 being (RightMod of R1);
let R3 being (Scalar of R1);
let R18 being (FinSequence of R2);
let R19 being (FinSequence of R2);
defpred S1[ Nat ] means (for B1 , B2 being (FinSequence of R2) holds ((( len B1 ) = ( len B2 ) & ( len B1 ) = $1 & (for R11 being Nat holds (for R13 being (Vector of R2) holds ((R11 in ( dom B1 ) & R13 = ( B2 . R11 )) implies ( B1 . R11 ) = ( R13 * R3 ))))) implies ( Sum B1 ) = ( ( Sum B2 ) * R3 )));
L5:
now
let C1 being (Element of ( NAT ));
assume L6: (for B3 , B4 being (FinSequence of R2) holds ((( len B3 ) = ( len B4 ) & ( len B3 ) = C1 & (for R11 being Nat holds (for R13 being (Vector of R2) holds ((R11 in ( dom B3 ) & R13 = ( B4 . R11 )) implies ( B3 . R11 ) = ( R13 * R3 ))))) implies ( Sum B3 ) = ( ( Sum B4 ) * R3 )));
let C2 , C3 being (FinSequence of R2);
assume that
L7: ( len C2 ) = ( len C3 )
and
L8: ( len C2 ) = ( C1 + 1 )
and
L9: (for R11 being Nat holds (for R13 being (Vector of R2) holds ((R11 in ( dom C2 ) & R13 = ( C3 . R11 )) implies ( C2 . R11 ) = ( R13 * R3 ))));
reconsider D1 = ( C2 | ( Seg C1 ) ) , D2 = ( C3 | ( Seg C1 ) ) as (FinSequence of R2) by FINSEQ_1:18;
L10: C1 <= ( C1 + 1 ) by NAT_1:12;
L11: ( len D1 ) = C1 by L10 , L8 , FINSEQ_1:17;
L12: ( len D2 ) = C1 by L7 , L8 , L10 , FINSEQ_1:17;
L13:
now
L14: ( len D1 ) <= ( len C2 ) by L8 , L10 , FINSEQ_1:17;
L15: ( dom D1 ) c= ( dom C2 ) by L14 , FINSEQ_3:30;
let R11 being Nat;
let R13 being (Vector of R2);
assume that
L16: R11 in ( dom D1 )
and
L17: R13 = ( D2 . R11 );
L18: ( dom D1 ) = ( dom D2 ) by L11 , L12 , FINSEQ_3:29;
L19: ( C3 . R11 ) = ( D2 . R11 ) by L18 , L16 , FUNCT_1:47;
L20: ( C2 . R11 ) = ( R13 * R3 ) by L19 , L9 , L16 , L17 , L15;
thus L21: ( D1 . R11 ) = ( R13 * R3 ) by L20 , L16 , FUNCT_1:47;
end;
L22: ( C1 + 1 ) in ( Seg ( C1 + 1 ) ) by FINSEQ_1:4;
L23: (( C1 + 1 ) in ( dom C2 ) & ( C1 + 1 ) in ( dom C3 )) by L22 , L7 , L8 , FINSEQ_1:def 3;
reconsider D3 = ( C2 . ( C1 + 1 ) ) , D4 = ( C3 . ( C1 + 1 ) ) as (Vector of R2) by L23 , FINSEQ_2:11;
L24: ( C1 + 1 ) in ( dom C2 ) by L8 , L22 , FINSEQ_1:def 3;
L25: D3 = ( D4 * R3 ) by L24 , L9;
thus L26: ( Sum C2 ) = ( ( Sum D1 ) + D3 ) by L8 , L11 , L1
.= ( ( ( Sum D2 ) * R3 ) + ( D4 * R3 ) ) by L6 , L11 , L12 , L13 , L25
.= ( ( ( Sum D2 ) + D4 ) * R3 ) by VECTSP_2:def 9
.= ( ( Sum C3 ) * R3 ) by L7 , L8 , L12 , L1;
end;
L27: (for B5 being (Element of ( NAT )) holds (S1[ B5 ] implies S1[ ( B5 + 1 ) ])) by L5;
L28:
now
let C4 , C5 being (FinSequence of R2);
assume that
L29: ( len C4 ) = ( len C5 )
and
L30: ( len C4 ) = ( 0 )
and
L31: (for R11 being Nat holds (for R13 being (Vector of R2) holds ((R11 in ( dom C4 ) & R13 = ( C5 . R11 )) implies ( C4 . R11 ) = ( R13 * R3 ))));
L32: C4 = ( <*> (the carrier of R2) ) by L30;
L33: ( Sum C4 ) = ( 0. R2 ) by L32 , RLVECT_1:43;
L34: C5 = ( <*> (the carrier of R2) ) by L29 , L30;
L35: ( Sum C5 ) = ( 0. R2 ) by L34 , RLVECT_1:43;
thus L36: ( Sum C4 ) = ( ( Sum C5 ) * R3 ) by L35 , L33 , VECTSP_2:32;
end;
L37: S1[ ( 0 ) ] by L28;
L38: (for B6 being (Element of ( NAT )) holds S1[ B6 ]) from NAT_1:sch 1(L37 , L27);
thus L39: thesis by L38;
end;
L40: (for R1 being Ring holds (for R2 being (RightMod of R1) holds (for R3 being (Scalar of R1) holds (for R18 being (FinSequence of R2) holds (for R19 being (FinSequence of R2) holds ((( len R18 ) = ( len R19 ) & (for R11 being Nat holds (R11 in ( dom R18 ) implies ( R19 . R11 ) = ( ( R18 /. R11 ) * R3 )))) implies ( Sum R19 ) = ( ( Sum R18 ) * R3 )))))))
proof
let R1 being Ring;
let R2 being (RightMod of R1);
let R3 being (Scalar of R1);
let R18 being (FinSequence of R2);
let R19 being (FinSequence of R2);
assume that
L41: ( len R18 ) = ( len R19 )
and
L42: (for R11 being Nat holds (R11 in ( dom R18 ) implies ( R19 . R11 ) = ( ( R18 /. R11 ) * R3 )));
L43:
now
let R11 being Nat;
let R13 being (Vector of R2);
assume that
L44: R11 in ( dom R19 )
and
L45: R13 = ( R18 . R11 );
L46: R11 in ( dom R18 ) by L41 , L44 , FINSEQ_3:29;
L47: R13 = ( R18 /. R11 ) by L46 , L45 , PARTFUN1:def 6;
thus L48: ( R19 . R11 ) = ( R13 * R3 ) by L47 , L42 , L46;
end;
thus L49: thesis by L43 , L41 , L4;
end;
theorem
L50: (for R1 being Ring holds (for R2 being (RightMod of R1) holds (for R3 being (Scalar of R1) holds ( ( Sum ( <*> (the carrier of R2) ) ) * R3 ) = ( 0. R2 ))))
proof
let R1 being Ring;
let R2 being (RightMod of R1);
let R3 being (Scalar of R1);
thus L51: ( ( Sum ( <*> (the carrier of R2) ) ) * R3 ) = ( ( 0. R2 ) * R3 ) by RLVECT_1:43
