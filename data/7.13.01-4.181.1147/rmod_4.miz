let R2 being (RightMod of R1);
let R13 being (Vector of R2);
L65: ( Sum <* R13 *> ) = R13 by RLVECT_1:44;
L66: (( rng <* R13 *> ) = { R13 } & <* R13 *> is  one-to-one) by FINSEQ_1:39 , FINSEQ_3:93;
thus L67: thesis by L66 , L65 , L56;
end;
theorem
L68: (for R1 being Ring holds (for R2 being (RightMod of R1) holds (for R14 being (Vector of R2) holds (for R15 being (Vector of R2) holds (R14 <> R15 implies ( Sum { R14 , R15 } ) = ( R14 + R15 ))))))
proof
let R1 being Ring;
let R2 being (RightMod of R1);
let R14 being (Vector of R2);
let R15 being (Vector of R2);
assume L69: R14 <> R15;
L70: <* R14 , R15 *> is  one-to-one by L69 , FINSEQ_3:94;
L71: (( rng <* R14 , R15 *> ) = { R14 , R15 } & ( Sum <* R14 , R15 *> ) = ( R14 + R15 )) by FINSEQ_2:127 , RLVECT_1:45;
thus L72: thesis by L71 , L70 , L56;
end;
theorem
L73: (for R1 being Ring holds (for R2 being (RightMod of R1) holds (for R14 being (Vector of R2) holds (for R15 being (Vector of R2) holds (for R16 being (Vector of R2) holds ((R14 <> R15 & R15 <> R16 & R14 <> R16) implies ( Sum { R14 , R15 , R16 } ) = ( ( R14 + R15 ) + R16 )))))))
proof
let R1 being Ring;
let R2 being (RightMod of R1);
let R14 being (Vector of R2);
let R15 being (Vector of R2);
let R16 being (Vector of R2);
assume L74: (R14 <> R15 & R15 <> R16 & R14 <> R16);
L75: <* R14 , R15 , R16 *> is  one-to-one by L74 , FINSEQ_3:95;
L76: (( rng <* R14 , R15 , R16 *> ) = { R14 , R15 , R16 } & ( Sum <* R14 , R15 , R16 *> ) = ( ( R14 + R15 ) + R16 )) by FINSEQ_2:128 , RLVECT_1:46;
thus L77: thesis by L76 , L75 , L56;
end;
theorem
L78: (for R1 being Ring holds (for R2 being (RightMod of R1) holds (for R24 being  finite (Subset of R2) holds (for R25 being  finite (Subset of R2) holds (R25 misses R24 implies ( Sum ( R25 \/ R24 ) ) = ( ( Sum R25 ) + ( Sum R24 ) ))))))
proof
let R1 being Ring;
let R2 being (RightMod of R1);
let R24 being  finite (Subset of R2);
let R25 being  finite (Subset of R2);
consider R18 being (FinSequence of R2) such that L79: ( rng R18 ) = ( R25 \/ R24 ) and L80: (R18 is  one-to-one & ( Sum ( R25 \/ R24 ) ) = ( Sum R18 )) by L56;
consider R19 being (FinSequence of R2) such that L81: ( rng R19 ) = R25 and L82: R19 is  one-to-one and L83: ( Sum R25 ) = ( Sum R19 ) by L56;
consider R20 being (FinSequence of R2) such that L84: ( rng R20 ) = R24 and L85: R20 is  one-to-one and L86: ( Sum R24 ) = ( Sum R20 ) by L56;
set D6 = ( R19 ^ R20 );
assume L87: R25 misses R24;
L88: D6 is  one-to-one by L87 , L81 , L82 , L84 , L85 , FINSEQ_3:91;
L89: ( rng D6 ) = ( rng R18 ) by L79 , L81 , L84 , FINSEQ_1:31;
thus L90: ( Sum ( R25 \/ R24 ) ) = ( Sum D6 ) by L89 , L80 , L88 , RLVECT_1:42
.= ( ( Sum R25 ) + ( Sum R24 ) ) by L83 , L86 , RLVECT_1:41;
end;
theorem
L91: (for R1 being Ring holds (for R2 being (RightMod of R1) holds (for R24 being  finite (Subset of R2) holds (for R25 being  finite (Subset of R2) holds ( Sum ( R25 \/ R24 ) ) = ( ( ( Sum R25 ) + ( Sum R24 ) ) - ( Sum ( R25 /\ R24 ) ) )))))
proof
let R1 being Ring;
let R2 being (RightMod of R1);
let R24 being  finite (Subset of R2);
let R25 being  finite (Subset of R2);
set D7 = ( R24 \ R25 );
set D8 = ( R25 \ R24 );
set D9 = ( D7 \/ D8 );
set D10 = ( R25 /\ R24 );
L92: ( D7 \/ D10 ) = R24 by XBOOLE_1:51;
L93: ( D8 \/ D10 ) = R25 by XBOOLE_1:51;
L94: D9 = ( R25 \+\ R24 );
L95: ( D9 \/ D10 ) = ( R25 \/ R24 ) by L94 , XBOOLE_1:93;
L96: ( ( Sum ( R25 \/ R24 ) ) + ( Sum D10 ) ) = ( ( ( Sum D9 ) + ( Sum D10 ) ) + ( Sum D10 ) ) by L95 , L94 , L78 , XBOOLE_1:103
.= ( ( ( ( Sum D7 ) + ( Sum D8 ) ) + ( Sum D10 ) ) + ( Sum D10 ) ) by L78 , XBOOLE_1:82
.= ( ( ( Sum D7 ) + ( ( Sum D10 ) + ( Sum D8 ) ) ) + ( Sum D10 ) ) by RLVECT_1:def 3
.= ( ( ( Sum D7 ) + ( Sum D10 ) ) + ( ( Sum D8 ) + ( Sum D10 ) ) ) by RLVECT_1:def 3
.= ( ( Sum R24 ) + ( ( Sum D8 ) + ( Sum D10 ) ) ) by L92 , L78 , XBOOLE_1:89
.= ( ( Sum R25 ) + ( Sum R24 ) ) by L93 , L78 , XBOOLE_1:89;
thus L97: thesis by L96 , RLSUB_2:61;
end;
theorem
L98: (for R1 being Ring holds (for R2 being (RightMod of R1) holds (for R24 being  finite (Subset of R2) holds (for R25 being  finite (Subset of R2) holds ( Sum ( R25 /\ R24 ) ) = ( ( ( Sum R25 ) + ( Sum R24 ) ) - ( Sum ( R25 \/ R24 ) ) )))))
proof
let R1 being Ring;
let R2 being (RightMod of R1);
let R24 being  finite (Subset of R2);
let R25 being  finite (Subset of R2);
L99: ( Sum ( R25 \/ R24 ) ) = ( ( ( Sum R25 ) + ( Sum R24 ) ) - ( Sum ( R25 /\ R24 ) ) ) by L91;
L100: ( ( Sum R25 ) + ( Sum R24 ) ) = ( ( Sum ( R25 /\ R24 ) ) + ( Sum ( R25 \/ R24 ) ) ) by L99 , RLSUB_2:61;
thus L101: thesis by L100 , RLSUB_2:61;
end;
theorem
L102: (for R1 being Ring holds (for R2 being (RightMod of R1) holds (for R24 being  finite (Subset of R2) holds (for R25 being  finite (Subset of R2) holds ( Sum ( R25 \ R24 ) ) = ( ( Sum ( R25 \/ R24 ) ) - ( Sum R24 ) )))))
proof
let R1 being Ring;
let R2 being (RightMod of R1);
let R24 being  finite (Subset of R2);
let R25 being  finite (Subset of R2);
L103: ( R25 \ R24 ) misses R24 by XBOOLE_1:79;
L104: ( ( R25 \ R24 ) /\ R24 ) = ( {} R2 ) by L103 , XBOOLE_0:def 7;
L105: ( ( R25 \ R24 ) \/ R24 ) = ( R25 \/ R24 ) by XBOOLE_1:39;
L106: ( Sum ( R25 \/ R24 ) ) = ( ( ( Sum ( R25 \ R24 ) ) + ( Sum R24 ) ) - ( Sum ( ( R25 \ R24 ) /\ R24 ) ) ) by L105 , L91;
L107: ( Sum ( R25 \/ R24 ) ) = ( ( ( Sum ( R25 \ R24 ) ) + ( Sum R24 ) ) - ( 0. R2 ) ) by L106 , L104 , L61
.= ( ( Sum ( R25 \ R24 ) ) + ( Sum R24 ) ) by VECTSP_1:18;
thus L108: thesis by L107 , RLSUB_2:61;
end;
theorem
L109: (for R1 being Ring holds (for R2 being (RightMod of R1) holds (for R24 being  finite (Subset of R2) holds (for R25 being  finite (Subset of R2) holds ( Sum ( R25 \ R24 ) ) = ( ( Sum R25 ) - ( Sum ( R25 /\ R24 ) ) )))))
proof
let R1 being Ring;
let R2 being (RightMod of R1);
let R24 being  finite (Subset of R2);
let R25 being  finite (Subset of R2);
L110: ( R25 \ ( R25 /\ R24 ) ) = ( R25 \ R24 ) by XBOOLE_1:47;
L111: ( Sum ( R25 \ R24 ) ) = ( ( Sum ( R25 \/ ( R25 /\ R24 ) ) ) - ( Sum ( R25 /\ R24 ) ) ) by L110 , L102;
thus L112: thesis by L111 , XBOOLE_1:22;
end;
theorem
L113: (for R1 being Ring holds (for R2 being (RightMod of R1) holds (for R24 being  finite (Subset of R2) holds (for R25 being  finite (Subset of R2) holds ( Sum ( R25 \+\ R24 ) ) = ( ( Sum ( R25 \/ R24 ) ) - ( Sum ( R25 /\ R24 ) ) )))))
proof
let R1 being Ring;
let R2 being (RightMod of R1);
let R24 being  finite (Subset of R2);
let R25 being  finite (Subset of R2);
L114: ( R25 \+\ R24 ) = ( ( R25 \/ R24 ) \ ( R25 /\ R24 ) ) by XBOOLE_1:101;
thus L115: ( Sum ( R25 \+\ R24 ) ) = ( ( Sum ( R25 \/ R24 ) ) - ( Sum ( ( R25 \/ R24 ) /\ ( R25 /\ R24 ) ) ) ) by L114 , L109
.= ( ( Sum ( R25 \/ R24 ) ) - ( Sum ( ( ( R25 \/ R24 ) /\ R25 ) /\ R24 ) ) ) by XBOOLE_1:16
.= ( ( Sum ( R25 \/ R24 ) ) - ( Sum ( R25 /\ R24 ) ) ) by XBOOLE_1:21;
end;
theorem
L116: (for R1 being Ring holds (for R2 being (RightMod of R1) holds (for R24 being  finite (Subset of R2) holds (for R25 being  finite (Subset of R2) holds ( Sum ( R25 \+\ R24 ) ) = ( ( Sum ( R25 \ R24 ) ) + ( Sum ( R24 \ R25 ) ) ))))) by L78 , XBOOLE_1:82;
definition
let R1 being Ring;
let R2 being (RightMod of R1);
mode Linear_Combination of R2
 -> (Element of ( Funcs ((the carrier of R2) , (the carrier of R1)) ))
means :L117: (ex R25 being  finite (Subset of R2) st (for R13 being (Vector of R2) holds ((not R13 in R25) implies ( it . R13 ) = ( 0. R1 ))));
existence
proof
L118: ( {} R2 ) = ( {} );
reconsider D11 = ( {} ) as  finite (Subset of R2) by L118;
reconsider D12 = ( (the carrier of R2) --> ( 0. R1 ) ) as (Function of R2 , R1);
reconsider D13 = D12 as (Element of ( Funcs ((the carrier of R2) , (the carrier of R1)) )) by FUNCT_2:8;
take D13;
take D11;
thus L119: thesis by FUNCOP_1:7;
end;
end;
definition
let R1 being Ring;
let R2 being (RightMod of R1);
let R26 being (Linear_Combination of R2);
func Carrier R26 ->  finite (Subset of R2) equals 
{ R13 where R13 is (Vector of R2) : ( R26 . R13 ) <> ( 0. R1 ) };
coherence
proof
set D14 = { R13 where R13 is (Vector of R2) : ( R26 . R13 ) <> ( 0. R1 ) };
consider R25 being  finite (Subset of R2) such that L121: (for R13 being (Vector of R2) holds ((not R13 in R25) implies ( R26 . R13 ) = ( 0. R1 ))) by L117;
L122: D14 c= R25
proof
