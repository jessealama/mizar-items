let R2 being (RightMod of R1);
let R3 being (Scalar of R1);
let R12 being (Vector of R2);
let R13 being (Vector of R2);
thus L53: ( ( Sum <* R13 , R12 *> ) * R3 ) = ( ( R13 + R12 ) * R3 ) by RLVECT_1:45
.= ( ( R13 * R3 ) + ( R12 * R3 ) ) by VECTSP_2:def 9;
end;
theorem
L54: (for R1 being Ring holds (for R2 being (RightMod of R1) holds (for R3 being (Scalar of R1) holds (for R12 being (Vector of R2) holds (for R13 being (Vector of R2) holds (for R17 being (Vector of R2) holds ( ( Sum <* R13 , R12 , R17 *> ) * R3 ) = ( ( ( R13 * R3 ) + ( R12 * R3 ) ) + ( R17 * R3 ) )))))))
proof
