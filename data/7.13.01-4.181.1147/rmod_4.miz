let R2 being (RightMod of R1);
let R3 being (Scalar of R1);
let R12 being (Vector of R2);
let R13 being (Vector of R2);
let R17 being (Vector of R2);
thus L55: ( ( Sum <* R13 , R12 , R17 *> ) * R3 ) = ( ( ( R13 + R12 ) + R17 ) * R3 ) by RLVECT_1:46
.= ( ( ( R13 + R12 ) * R3 ) + ( R17 * R3 ) ) by VECTSP_2:def 9
.= ( ( ( R13 * R3 ) + ( R12 * R3 ) ) + ( R17 * R3 ) ) by VECTSP_2:def 9;
end;
definition
let R1 being Ring;
let R2 being (RightMod of R1);
let R25 being  finite (Subset of R2);
func Sum R25 -> (Vector of R2) means 
:L56: (ex R18 being (FinSequence of R2) st (( rng R18 ) = R25 & R18 is  one-to-one & it = ( Sum R18 )));
existence
proof
consider R7 being FinSequence such that L57: ( rng R7 ) = R25 and L58: R7 is  one-to-one by FINSEQ_4:58;
reconsider D5 = R7 as (FinSequence of R2) by L57 , FINSEQ_1:def 4;
take ( Sum D5 );
take D5;
thus L59: thesis by L57 , L58;
end;
uniqueness by RLVECT_1:42;
end;
theorem
L61: (for R1 being Ring holds (for R2 being (RightMod of R1) holds ( Sum ( {} R2 ) ) = ( 0. R2 )))
proof
let R1 being Ring;
let R2 being (RightMod of R1);
L62: ( Sum ( <*> (the carrier of R2) ) ) = ( 0. R2 ) by RLVECT_1:43;
thus L63: thesis by L62 , L56 , RELAT_1:38;
end;
theorem
L64: (for R1 being Ring holds (for R2 being (RightMod of R1) holds (for R13 being (Vector of R2) holds ( Sum { R13 } ) = R13)))
proof
