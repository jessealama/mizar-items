theorem
L3: (for B1 , B2 being FinSequence holds (( B1 ^ B2 ) is  constant implies (B1 is  constant & B2 is  constant)))
proof
let C1 , C2 being FinSequence;
assume L4: ( C1 ^ C2 ) is  constant;
reconsider D1 = ( C1 ^ C2 ) as  constant FinSequence by L4;
L5: ( rng C1 ) c= ( rng D1 ) by FINSEQ_1:29;
thus L6: C1 is  constant by L5;
L7: ( rng C2 ) c= ( rng D1 ) by FINSEQ_1:30;
thus L8: thesis by L7;
end;
theorem
L9: (for B3 , B4 being set holds (<* B3 , B4 *> is  constant implies B3 = B4))
proof
let C3 , C4 being set;
L10: ( rng <* C3 , C4 *> ) = ( rng ( <* C3 *> ^ <* C4 *> ) ) by FINSEQ_1:def 9
.= ( ( rng <* C3 *> ) \/ ( rng <* C4 *> ) ) by FINSEQ_1:31
.= ( ( rng <* C3 *> ) \/ { C4 } ) by FINSEQ_1:38
.= ( { C3 } \/ { C4 } ) by FINSEQ_1:38
.= { C3 , C4 } by ENUMSET1:1;
L11: C4 in { C3 , C4 } by TARSKI:def 2;
assume L12: <* C3 , C4 *> is  constant;
reconsider D2 = <* C3 , C4 *> as  constant Function by L12;
L13: C3 in { C3 , C4 } by TARSKI:def 2;
L14: ( rng D2 ) is  trivial;
thus L15: thesis by L14 , L10 , L13 , L11 , ZFMISC_1:def 10;
end;
theorem
L16: (for B5 , B6 , B7 being set holds (<* B5 , B6 , B7 *> is  constant implies (B5 = B6 & B6 = B7 & B7 = B5)))
proof
