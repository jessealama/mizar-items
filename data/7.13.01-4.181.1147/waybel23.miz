theorem
L1: (for B1 being non  empty Poset holds (for B2 being (Element of B1) holds ( compactbelow B2 ) = ( ( waybelow B2 ) /\ (the carrier of ( CompactSublatt B1 )) )))
proof
let C1 being non  empty Poset;
let C2 being (Element of C1);
L2: ( compactbelow C2 ) c= ( ( waybelow C2 ) /\ (the carrier of ( CompactSublatt C1 )) )
proof
let C3 being set;
assume L3: C3 in ( compactbelow C2 );
reconsider D1 = C3 as (Element of C1) by L3;
L4: C3 in ( ( downarrow C2 ) /\ (the carrier of ( CompactSublatt C1 )) ) by L3 , WAYBEL_8:5;
L5: C3 in ( downarrow C2 ) by L4 , XBOOLE_0:def 4;
L6: D1 <= C2 by L5 , WAYBEL_0:17;
L7: C3 in (the carrier of ( CompactSublatt C1 )) by L4 , XBOOLE_0:def 4;
L8: D1 is  compact by L7 , WAYBEL_8:def 1;
L9: D1 << D1 by L8 , WAYBEL_3:def 2;
L10: D1 << C2 by L9 , L6 , WAYBEL_3:2;
L11: D1 in ( waybelow C2 ) by L10 , WAYBEL_3:7;
thus L12: thesis by L11 , L7 , XBOOLE_0:def 4;
end;
L13: ( ( waybelow C2 ) /\ (the carrier of ( CompactSublatt C1 )) ) c= ( compactbelow C2 )
proof
let C4 being set;
assume L14: C4 in ( ( waybelow C2 ) /\ (the carrier of ( CompactSublatt C1 )) );
reconsider D2 = C4 as (Element of C1) by L14;
L15: C4 in ( waybelow C2 ) by L14 , XBOOLE_0:def 4;
L16: D2 << C2 by L15 , WAYBEL_3:7;
L17: D2 <= C2 by L16 , WAYBEL_3:1;
L18: C4 in ( downarrow C2 ) by L17 , WAYBEL_0:17;
L19: C4 in (the carrier of ( CompactSublatt C1 )) by L14 , XBOOLE_0:def 4;
L20: C4 in ( ( downarrow C2 ) /\ (the carrier of ( CompactSublatt C1 )) ) by L19 , L18 , XBOOLE_0:def 4;
thus L21: thesis by L20 , WAYBEL_8:5;
end;
thus L22: thesis by L13 , L2 , XBOOLE_0:def 10;
end;
