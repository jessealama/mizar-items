assume L131: ( C51 "/\" C52 ) <= ( C48 ~ );
L132: C48 <= ( ( ~ C51 ) "\/" ( ~ C52 ) ) by L131 , L130 , YELLOW_7:2;
L133: (C48 <= ( ~ C51 ) or C48 <= ( ~ C52 )) by L132 , L129;
thus L134: (C51 <= ( C48 ~ ) or C52 <= ( C48 ~ )) by L133 , YELLOW_7:2;
end;
theorem
L135: (for B49 being  complete non  empty Poset holds (for B50 being non  empty (Subset of B49) holds ( downarrow ( inf B50 ) ) = ( meet { ( downarrow B51 ) where B51 is (Element of B49) : B51 in B50 } )))
proof
let C53 being  complete non  empty Poset;
let C54 being non  empty (Subset of C53);
set D16 = { ( downarrow B52 ) where B52 is (Element of C53) : B52 in C54 };
consider C55 being set such that L136: C55 in C54 by XBOOLE_0:def 1;
L137: D16 c= ( bool (the carrier of C53) )
proof
let C56 being set;
assume L138: C56 in D16;
L139: (ex B53 being (Element of C53) st (C56 = ( downarrow B53 ) & B53 in C54)) by L138;
thus L140: thesis by L139;
end;
reconsider D17 = C55 as (Element of C53) by L136;
L141: ( downarrow D17 ) in D16 by L136;
reconsider D18 = D16 as (Subset-Family of C53) by L137;
reconsider D19 = D18 as (Subset-Family of C53);
L142:
now
let C57 being set;
thus L143:now
assume L144: C57 in ( downarrow ( inf C54 ) );
reconsider D20 = C57 as (Element of C53) by L144;
L145: D20 <= ( inf C54 ) by L144 , WAYBEL_0:17;
L146:
now
let C58 being set;
assume L147: C58 in D19;
consider C59 being (Element of C53) such that L148: C58 = ( downarrow C59 ) and L149: C59 in C54 by L147;
L150: ( inf C54 ) is_<=_than C54 by YELLOW_0:33;
L151: ( inf C54 ) <= C59 by L150 , L149 , LATTICE3:def 8;
L152: D20 <= C59 by L151 , L145 , ORDERS_2:3;
thus L153: C57 in C58 by L152 , L148 , WAYBEL_0:17;
end;
thus L154: C57 in ( meet D19 ) by L146 , L141 , SETFAM_1:def 1;
end;
assume L155: C57 in ( meet D19 );
reconsider D21 = C57 as (Element of C53) by L155;
L156:
now
let C60 being (Element of C53);
assume L157: C60 in C54;
L158: ( downarrow C60 ) in D19 by L157;
L159: D21 in ( downarrow C60 ) by L158 , L155 , SETFAM_1:def 1;
thus L160: D21 <= C60 by L159 , WAYBEL_0:17;
end;
L161: D21 is_<=_than C54 by L156 , LATTICE3:def 8;
