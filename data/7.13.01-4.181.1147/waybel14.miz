let C20 , C21 being (Element of C19);
L44:
now
let C22 being set;
thus L45:now
assume L46: C22 in ( uparrow ( C20 "\/" C21 ) );
reconsider D6 = C22 as (Element of C19) by L46;
L47: D6 >= ( C20 "\/" C21 ) by L46 , WAYBEL_0:18;
L48: ( C20 "\/" C21 ) >= C21 by YELLOW_0:22;
L49: D6 >= C21 by L48 , L47 , YELLOW_0:def 2;
L50: D6 in ( uparrow C21 ) by L49 , WAYBEL_0:18;
L51: ( C20 "\/" C21 ) >= C20 by YELLOW_0:22;
L52: D6 >= C20 by L51 , L47 , YELLOW_0:def 2;
L53: D6 in ( uparrow C20 ) by L52 , WAYBEL_0:18;
thus L54: C22 in ( ( uparrow C20 ) /\ ( uparrow C21 ) ) by L53 , L50 , XBOOLE_0:def 4;
end;
assume L55: C22 in ( ( uparrow C20 ) /\ ( uparrow C21 ) );
reconsider D7 = C22 as (Element of C19) by L55;
L56: C22 in ( uparrow C21 ) by L55 , XBOOLE_0:def 4;
L57: D7 >= C21 by L56 , WAYBEL_0:18;
L58: C22 in ( uparrow C20 ) by L55 , XBOOLE_0:def 4;
L59: D7 >= C20 by L58 , WAYBEL_0:18;
L60: ( C20 "\/" C21 ) <= D7 by L59 , L57 , YELLOW_0:22;
thus L61: C22 in ( uparrow ( C20 "\/" C21 ) ) by L60 , WAYBEL_0:18;
end;
thus L62: thesis by L44 , TARSKI:1;
end;
theorem
L63: (for B23 being  complete  antisymmetric non  empty RelStr holds (for B24 being  lower (Subset of B23) holds (( sup B24 ) in B24 implies B24 = ( downarrow ( sup B24 ) ))))
