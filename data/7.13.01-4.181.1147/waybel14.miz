theorem
L1: (for B1 being set holds (for B2 being  finite (Subset-Family of B1) holds (ex B3 being  finite (Subset-Family of B1) st (B3 c= B2 & ( union B3 ) = ( union B2 ) & (for B4 being (Subset of B1) holds (B4 in B3 implies (not B4 c= ( union ( B3 \ { B4 } ) ))))))))
proof
let C1 being set;
defpred S1[ Nat ] means (for B5 being  finite (Subset-Family of C1) holds (( card B5 ) = $1 implies (ex B6 being  finite (Subset-Family of C1) st (B6 c= B5 & ( union B6 ) = ( union B5 ) & (for B7 being (Subset of C1) holds (B7 in B6 implies (not B7 c= ( union ( B6 \ { B7 } ) ))))))));
L2:
now
let C2 being Nat;
assume L3: S1[ C2 ];
thus L4: S1[ ( C2 + 1 ) ]
proof
let C3 being  finite (Subset-Family of C1);
assume L5: ( card C3 ) = ( C2 + 1 );
per cases ;
suppose L6: (ex B8 being (Subset of C1) st (B8 in C3 & B8 c= ( union ( C3 \ { B8 } ) )));

consider C4 being (Subset of C1) such that L7: C4 in C3 and L8: C4 c= ( union ( C3 \ { C4 } ) ) by L6;
reconsider D1 = ( C3 \ { C4 } ) as  finite (Subset-Family of C1);
L9: { C4 } c= C3 by L7 , ZFMISC_1:31;
L10: C3 = ( D1 \/ { C4 } ) by L9 , XBOOLE_1:45;
L11: ( union C3 ) c= ( union D1 )
proof
let C5 being set;
assume L12: C5 in ( union C3 );
consider C6 being set such that L13: C5 in C6 and L14: C6 in C3 by L12 , TARSKI:def 4;
per cases  by L10 , L14 , XBOOLE_0:def 3;
suppose L15: C6 in D1;

L16: C6 c= ( union D1 ) by L15 , ZFMISC_1:74;
thus L17: thesis by L16 , L13;
end;
suppose L18: C6 in { C4 };

L19: C6 = C4 by L18 , TARSKI:def 1;
thus L20: thesis by L19 , L8 , L13;
end;
end;
L22: C4 in { C4 } by TARSKI:def 1;
L23: (not C4 in D1) by L22 , XBOOLE_0:def 5;
L24: ( card ( D1 \/ { C4 } ) ) = ( ( card D1 ) + 1 ) by L23 , CARD_2:41;
consider C7 being  finite (Subset-Family of C1) such that L25: C7 c= D1 and L26: ( union C7 ) = ( union D1 ) and L27: (for B9 being (Subset of C1) holds (B9 in C7 implies (not B9 c= ( union ( C7 \ { B9 } ) )))) by L24 , L3 , L5 , L10 , XCMPLX_1:2;
take C7;
L28: D1 c= C3 by L10 , XBOOLE_1:7;
thus L29: C7 c= C3 by L28 , L25 , XBOOLE_1:1;
L30: ( union D1 ) c= ( union C3 ) by L10 , XBOOLE_1:7 , ZFMISC_1:77;
thus L31: ( union C7 ) = ( union C3 ) by L30 , L26 , L11 , XBOOLE_0:def 10;
thus L32: thesis by L27;
end;
suppose L33: (not (ex B10 being (Subset of C1) st (B10 in C3 & B10 c= ( union ( C3 \ { B10 } ) ))));

take D2 = C3;
thus L34: D2 c= C3;
thus L35: ( union D2 ) = ( union C3 );
thus L36: thesis by L33;
end;
end;

end;
let C8 being  finite (Subset-Family of C1);
L5: ( card C8 ) = ( card C8 );
L6: S1[ ( 0 ) ]
proof
let C9 being  finite (Subset-Family of C1);
assume L7: ( card C9 ) = ( 0 );
take D3 = C9;
thus L8: D3 c= C9;
thus L9: ( union D3 ) = ( union C9 );
thus L10: thesis by L7;
end;
L11: (for B11 being Nat holds S1[ B11 ]) from NAT_1:sch 2(L6 , L2);
thus L12: thesis by L11 , L5;
end;
L13: (for B12 being 1-sorted holds (for B13 , B14 being (Subset of B12) holds (B13 c= ( B14 ` ) iff B14 c= ( B13 ` ))))
proof
let C10 being 1-sorted;
let C11 , C12 being (Subset of C10);
L14: C12 = ( ( C12 ` ) ` );
thus L15: thesis by L14 , SUBSET_1:12;
end;
theorem
L16: (for B15 being 1-sorted holds (for B16 being (Subset of B15) holds (( B16 ` ) = (the carrier of B15) iff B16 is  empty)))
proof
let C13 being 1-sorted;
let C14 being (Subset of C13);
thus L17:now
assume L18: ( C14 ` ) = (the carrier of C13);
L19: C14 = ( ( [#] (the carrier of C13) ) ` ) by L18;
thus L20: C14 is  empty by L19 , XBOOLE_1:37;
end;
assume L21: C14 is  empty;
thus L22: thesis by L21;
end;
theorem
L23: (for B17 being  antisymmetric  with_infima  transitive non  empty RelStr holds (for B18 , B19 being (Element of B17) holds ( downarrow ( B18 "/\" B19 ) ) = ( ( downarrow B18 ) /\ ( downarrow B19 ) )))
proof
let C15 being  antisymmetric  with_infima  transitive non  empty RelStr;
let C16 , C17 being (Element of C15);
L24:
now
let C18 being set;
thus L25:now
assume L26: C18 in ( downarrow ( C16 "/\" C17 ) );
reconsider D4 = C18 as (Element of C15) by L26;
L27: D4 <= ( C16 "/\" C17 ) by L26 , WAYBEL_0:17;
L28: ( C16 "/\" C17 ) <= C17 by YELLOW_0:23;
L29: D4 <= C17 by L28 , L27 , YELLOW_0:def 2;
L30: D4 in ( downarrow C17 ) by L29 , WAYBEL_0:17;
L31: ( C16 "/\" C17 ) <= C16 by YELLOW_0:23;
L32: D4 <= C16 by L31 , L27 , YELLOW_0:def 2;
L33: D4 in ( downarrow C16 ) by L32 , WAYBEL_0:17;
thus L34: C18 in ( ( downarrow C16 ) /\ ( downarrow C17 ) ) by L33 , L30 , XBOOLE_0:def 4;
end;
assume L35: C18 in ( ( downarrow C16 ) /\ ( downarrow C17 ) );
reconsider D5 = C18 as (Element of C15) by L35;
L36: C18 in ( downarrow C17 ) by L35 , XBOOLE_0:def 4;
L37: D5 <= C17 by L36 , WAYBEL_0:17;
L38: C18 in ( downarrow C16 ) by L35 , XBOOLE_0:def 4;
L39: D5 <= C16 by L38 , WAYBEL_0:17;
L40: ( C16 "/\" C17 ) >= D5 by L39 , L37 , YELLOW_0:23;
thus L41: C18 in ( downarrow ( C16 "/\" C17 ) ) by L40 , WAYBEL_0:17;
end;
thus L42: thesis by L24 , TARSKI:1;
end;
theorem
L43: (for B20 being  antisymmetric  with_suprema  transitive non  empty RelStr holds (for B21 , B22 being (Element of B20) holds ( uparrow ( B21 "\/" B22 ) ) = ( ( uparrow B21 ) /\ ( uparrow B22 ) )))
proof
