L1: (for B1 being non  empty RelStr holds (for B2 being (Function of ( NAT ) , (the carrier of B1)) holds (for B3 being (Element of ( NAT )) holds { ( B2 . B4 ) where B4 is (Element of ( NAT )) : B4 <= B3 } is non  empty  finite non  empty  finite non  empty  finite non  empty  finite (Subset of B1))))
proof
let C1 being non  empty RelStr;
let C2 being (Function of ( NAT ) , (the carrier of C1));
let C3 being (Element of ( NAT ));
set D1 = { ( C2 . B5 ) where B5 is (Element of ( NAT )) : B5 <= C3 };
