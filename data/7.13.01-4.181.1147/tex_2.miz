L46: (for R1 being TopStruct holds (for B19 being (SubSpace of R1) holds (for B20 being (Subset of R1) holds (B20 = (the carrier of B19) implies (B20 is  proper iff B19 is  proper)))))
proof
let R1 being TopStruct;
let C21 being (SubSpace of R1);
let C22 being (Subset of R1);
assume L47: C22 = (the carrier of C21);
thus L48:now
assume L49: C22 is  proper;
L50: (for B21 being (Subset of R1) holds (B21 = (the carrier of C21) implies B21 is  proper)) by L49 , L47;
thus L51: C21 is  proper by L50 , L44;
end;
thus L52: thesis by L47 , L44;
end;
