theorem
L1: (for B1 being non  empty set holds (for B2 being 1 -element set holds (B1 c= B2 implies B1 = B2)))
proof
let C1 being non  empty set;
let C2 being 1 -element set;
assume L2: C1 c= C2;
L3: (ex B3 being (Element of C2) st C2 = { B3 }) by SUBSET_1:46;
thus L4: thesis by L3 , L2 , ZFMISC_1:33;
end;
theorem
L5: (for B4 being 1 -element set holds (for B5 being set holds (( B4 /\ B5 ) is non  empty implies B4 c= B5)))
proof
let C3 being 1 -element set;
let C4 being set;
L6: ( C3 /\ C4 ) c= C4 by XBOOLE_1:17;
assume L7: ( C3 /\ C4 ) is non  empty;
consider C5 being set such that L8: C5 in ( C3 /\ C4 ) by L7 , XBOOLE_0:def 1;
L9: (ex B6 being (Element of C3) st C3 = { B6 }) by SUBSET_1:46;
L10: ( C3 /\ C4 ) c= C3 by XBOOLE_1:17;
L11: { C5 } c= C3 by L10 , L8 , ZFMISC_1:31;
L12: { C5 } = C3 by L11 , L9 , ZFMISC_1:18;
thus L13: thesis by L12 , L8 , L6 , ZFMISC_1:31;
end;
