definition
let C1 being set;
attr C1 is  complex-membered
means
:L1: (for R1 being set holds (R1 in C1 implies R1 is  complex));
attr C1 is  ext-real-membered
means
:L2: (for R1 being set holds (R1 in C1 implies R1 is  ext-real));
attr C1 is  real-membered
means
:L3: (for R1 being set holds (R1 in C1 implies R1 is  real));
attr C1 is  rational-membered
means
:L4: (for R1 being set holds (R1 in C1 implies R1 is  rational));
attr C1 is  integer-membered
means
:L5: (for R1 being set holds (R1 in C1 implies R1 is  integer));
attr C1 is  natural-membered
means
:L6: (for R1 being set holds (R1 in C1 implies R1 is  natural));
end;
registration
cluster  natural-membered ->  integer-membered for set;
coherence
proof
let R2 being set;
assume L8: R2 is  natural-membered;
let R1 being set;
assume L9: R1 in R2;
L10: R1 is  natural by L9 , L8 , L6;
L11: R1 in ( omega ) by L10 , ORDINAL1:def 12;
thus L12: R1 in ( INT ) by L11 , NUMBERS:17;
end;
cluster  integer-membered ->  rational-membered for set;
coherence
proof
let R2 being set;
assume L13: R2 is  integer-membered;
let R1 being set;
assume L14: R1 in R2;
L15: R1 is  integer by L14 , L13 , L5;
L16: R1 in ( INT ) by L15 , INT_1:def 2;
thus L17: R1 in ( RAT ) by L16 , NUMBERS:14;
end;
cluster  rational-membered ->  real-membered for set;
coherence
proof
let R2 being set;
