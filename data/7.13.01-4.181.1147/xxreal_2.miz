begin
scheme FinInter { F1 , F2() -> Integer , F3(set) -> set , P1[set] } : { F3(B1) where B1 is (Element of ( INT )) : (F1() <= B1 & B1 <= F2() & P1[ B1 ]) } is  finite
proof
set D1 = { F3(B2) where B2 is (Element of ( INT )) : (F1() <= B2 & B2 <= F2() & P1[ B2 ]) };
per cases ;
suppose L1: F2() < F1();

L2:
now
assume L3: D1 <> ( {} );
consider C1 being set such that L4: C1 in D1 by L3 , XBOOLE_0:def 1;
L5: (ex B3 being (Element of ( INT )) st (C1 = F3(B3) & F1() <= B3 & B3 <= F2() & P1[ B3 ])) by L4;
thus L6: contradiction by L5 , L1 , XXREAL_0:2;
end;
reconsider D2 = D1 as  empty set by L2;
L7: D2 is  finite;
thus L8: thesis by L7;
end;
suppose L9: F1() <= F2();

reconsider D3 = ( F2() - F1() ) as (Element of ( NAT )) by L9 , INT_1:5;
set D4 = { F3(B4) where B4 is (Element of ( INT )) : (F1() <= B4 & B4 <= F2() & P1[ B4 ]) };
defpred S1[ set , set ] means (ex B5 being Integer st ($1 = B5 & $2 = F3(( B5 + F1() ))));
L10: (for B6 being set holds (B6 in ( D3 + 1 ) implies (ex B7 being set st S1[ B6 , B7 ])))
proof
let C2 being set;
assume L11: C2 in ( D3 + 1 );
reconsider D5 = C2 as Nat by L11;
take F3(( D5 + F1() ));
take D5;
thus L12: thesis;
end;
consider C3 being Function such that L13: ( dom C3 ) = ( D3 + 1 ) and L14: (for B8 being set holds (B8 in ( D3 + 1 ) implies S1[ B8 , ( C3 . B8 ) ])) from CLASSES1:sch 1(L10);
L15: D4 c= ( rng C3 )
proof
let C4 being set;
assume L16: C4 in D4;
consider C5 being (Element of ( INT )) such that L17: C4 = F3(C5) and L18: F1() <= C5 and L19: C5 <= F2() and L20: P1[ C5 ] by L16;
reconsider D6 = ( C5 - F1() ) as (Element of ( NAT )) by L18 , INT_1:5;
L21: D6 <= D3 by L19 , XREAL_1:9;
L22: D6 < ( D3 + 1 ) by L21 , NAT_1:13;
L23: D6 in ( D3 + 1 ) by L22 , NAT_1:44;
L24: S1[ ( C5 - F1() ) , ( C3 . ( C5 - F1() ) ) ] by L23 , L14;
L25: ( C3 . ( C5 - F1() ) ) = F3(( ( C5 + F1() ) - F1() )) by L24
.= F3(C5);
thus L26: thesis by L25 , L13 , L17 , L23 , FUNCT_1:def 3;
end;
L27: ( rng C3 ) is  finite by L13 , FINSET_1:8;
thus L28: thesis by L27 , L15;
end;
end;
