begin
definition
let C1 being non  empty set;
let C2 being ComplexNormSpace;
let C3 being (PartFunc of C1 , ( COMPLEX ));
let C4 being (PartFunc of C1 , C2);
deffunc H1((Element of C1)) = ( ( C3 /. $1 ) * ( C4 /. $1 ) );
defpred S1[ set ] means $1 in ( ( dom C3 ) /\ ( dom C4 ) );
set D1 = ( ( dom C3 ) /\ ( dom C4 ) );
func C3 (#) C4 -> (PartFunc of C1 , C2) means 
:L1: (( dom it ) = ( ( dom C3 ) /\ ( dom C4 ) ) & (for B1 being (Element of C1) holds (B1 in ( dom it ) implies ( it /. B1 ) = ( ( C3 /. B1 ) * ( C4 /. B1 ) ))));
existence
proof
consider C5 being (PartFunc of C1 , C2) such that L2: (for B2 being (Element of C1) holds (B2 in ( dom C5 ) iff S1[ B2 ])) and L3: (for B3 being (Element of C1) holds (B3 in ( dom C5 ) implies ( C5 /. B3 ) = H1(B3))) from PARTFUN2:sch 2;
take C5;
thus L4: ( dom C5 ) = ( ( dom C3 ) /\ ( dom C4 ) ) by L2 , SUBSET_1:3;
let C6 being (Element of C1);
assume L5: C6 in ( dom C5 );
thus L6: thesis by L5 , L3;
end;
uniqueness
proof
thus L7: (for B4 , B5 being (PartFunc of C1 , C2) holds ((( dom B4 ) = D1 & (for B6 being (Element of C1) holds (B6 in ( dom B4 ) implies ( B4 /. B6 ) = H1(B6))) & ( dom B5 ) = D1 & (for B7 being (Element of C1) holds (B7 in ( dom B5 ) implies ( B5 /. B7 ) = H1(B7)))) implies B4 = B5)) from PARTFUN2:sch 3;
end;
end;
definition
let C7 being non  empty set;
let C8 being ComplexNormSpace;
let C9 being (PartFunc of C7 , C8);
let C10 being Complex;
deffunc H2((Element of C7)) = ( C10 * ( C9 /. $1 ) );
defpred S2[ set ] means $1 in ( dom C9 );
set D2 = ( dom C9 );
func C10 (#) C9 -> (PartFunc of C7 , C8) means 
:L9: (( dom it ) = ( dom C9 ) & (for B8 being (Element of C7) holds (B8 in ( dom it ) implies ( it /. B8 ) = ( C10 * ( C9 /. B8 ) ))));
existence
proof
consider C11 being (PartFunc of C7 , C8) such that L10: (for B9 being (Element of C7) holds (B9 in ( dom C11 ) iff S2[ B9 ])) and L11: (for B10 being (Element of C7) holds (B10 in ( dom C11 ) implies ( C11 /. B10 ) = H2(B10))) from PARTFUN2:sch 2;
take C11;
thus L12: ( dom C11 ) = ( dom C9 ) by L10 , SUBSET_1:3;
let C12 being (Element of C7);
assume L13: C12 in ( dom C11 );
thus L14: thesis by L13 , L11;
end;
uniqueness
proof
thus L15: (for B11 , B12 being (PartFunc of C7 , C8) holds ((( dom B11 ) = D2 & (for B13 being (Element of C7) holds (B13 in ( dom B11 ) implies ( B11 /. B13 ) = H2(B13))) & ( dom B12 ) = D2 & (for B14 being (Element of C7) holds (B14 in ( dom B12 ) implies ( B12 /. B14 ) = H2(B14)))) implies B11 = B12)) from PARTFUN2:sch 3;
end;
end;
theorem
L17: (for R1 being non  empty set holds (for R2 being ComplexNormSpace holds (for B15 being (PartFunc of R1 , ( COMPLEX )) holds (for B16 being (PartFunc of R1 , R2) holds ( ( dom ( B15 (#) B16 ) ) \ ( ( B15 (#) B16 ) " { ( 0. R2 ) } ) ) = ( ( ( dom B15 ) \ ( B15 " { ( 0 ) } ) ) /\ ( ( dom B16 ) \ ( B16 " { ( 0. R2 ) } ) ) )))))
proof
let R1 being non  empty set;
let R2 being ComplexNormSpace;
let C13 being (PartFunc of R1 , ( COMPLEX ));
let C14 being (PartFunc of R1 , R2);
thus L18: ( ( dom ( C13 (#) C14 ) ) \ ( ( C13 (#) C14 ) " { ( 0. R2 ) } ) ) c= ( ( ( dom C13 ) \ ( C13 " { ( 0 ) } ) ) /\ ( ( dom C14 ) \ ( C14 " { ( 0. R2 ) } ) ) )
proof
let C15 being set;
assume L19: C15 in ( ( dom ( C13 (#) C14 ) ) \ ( ( C13 (#) C14 ) " { ( 0. R2 ) } ) );
reconsider D3 = C15 as (Element of R1) by L19;
L20: C15 in ( dom ( C13 (#) C14 ) ) by L19 , XBOOLE_0:def 5;
L21: D3 in ( ( dom C13 ) /\ ( dom C14 ) ) by L20 , L1;
L22: (not C15 in ( ( C13 (#) C14 ) " { ( 0. R2 ) } )) by L19 , XBOOLE_0:def 5;
L23: (not ( ( C13 (#) C14 ) /. D3 ) in { ( 0. R2 ) }) by L22 , L20 , PARTFUN2:26;
L24: ( ( C13 (#) C14 ) /. D3 ) <> ( 0. R2 ) by L23 , TARSKI:def 1;
L25: ( ( C13 /. D3 ) * ( C14 /. D3 ) ) <> ( 0. R2 ) by L24 , L20 , L1;
L26: ( C13 /. D3 ) <> ( 0c ) by L25 , CLVECT_1:1;
L27: (not ( C13 /. D3 ) in { ( 0 ) }) by L26 , TARSKI:def 1;
L28: ( C14 /. D3 ) <> ( 0. R2 ) by L25 , CLVECT_1:1;
L29: (not ( C14 /. D3 ) in { ( 0. R2 ) }) by L28 , TARSKI:def 1;
L30: (not D3 in ( C14 " { ( 0. R2 ) } )) by L29 , PARTFUN2:26;
L31: D3 in ( dom C14 ) by L21 , XBOOLE_0:def 4;
L32: C15 in ( ( dom C14 ) \ ( C14 " { ( 0. R2 ) } ) ) by L31 , L30 , XBOOLE_0:def 5;
L33: D3 in ( dom C13 ) by L21 , XBOOLE_0:def 4;
L34: (not ( C13 . D3 ) in { ( 0 ) }) by L33 , L27 , PARTFUN1:def 6;
L35: (not D3 in ( C13 " { ( 0 ) } )) by L34 , FUNCT_1:def 7;
L36: D3 in ( dom C13 ) by L21 , XBOOLE_0:def 4;
L37: C15 in ( ( dom C13 ) \ ( C13 " { ( 0 ) } ) ) by L36 , L35 , XBOOLE_0:def 5;
thus L38: thesis by L37 , L32 , XBOOLE_0:def 4;
end;

thus L39: ( ( ( dom C13 ) \ ( C13 " { ( 0 ) } ) ) /\ ( ( dom C14 ) \ ( C14 " { ( 0. R2 ) } ) ) ) c= ( ( dom ( C13 (#) C14 ) ) \ ( ( C13 (#) C14 ) " { ( 0. R2 ) } ) )
proof
let C16 being set;
assume L40: C16 in ( ( ( dom C13 ) \ ( C13 " { ( 0 ) } ) ) /\ ( ( dom C14 ) \ ( C14 " { ( 0. R2 ) } ) ) );
reconsider D4 = C16 as (Element of R1) by L40;
L41: C16 in ( ( dom C13 ) \ ( C13 " { ( 0 ) } ) ) by L40 , XBOOLE_0:def 4;
L42: C16 in ( dom C13 ) by L41 , XBOOLE_0:def 5;
L43: (not C16 in ( C13 " { ( 0 ) } )) by L41 , XBOOLE_0:def 5;
L44: (not ( C13 . D4 ) in { ( 0 ) }) by L43 , L42 , FUNCT_1:def 7;
L45: ( C13 . D4 ) <> ( 0 ) by L44 , TARSKI:def 1;
L46: ( C13 /. D4 ) <> ( 0 ) by L45 , L42 , PARTFUN1:def 6;
L47: C16 in ( ( dom C14 ) \ ( C14 " { ( 0. R2 ) } ) ) by L40 , XBOOLE_0:def 4;
L48: C16 in ( dom C14 ) by L47 , XBOOLE_0:def 5;
L49: D4 in ( ( dom C13 ) /\ ( dom C14 ) ) by L48 , L42 , XBOOLE_0:def 4;
L50: D4 in ( dom ( C13 (#) C14 ) ) by L49 , L1;
L51: (not C16 in ( C14 " { ( 0. R2 ) } )) by L47 , XBOOLE_0:def 5;
L52: (not ( C14 /. D4 ) in { ( 0. R2 ) }) by L51 , L48 , PARTFUN2:26;
L53: ( C14 /. D4 ) <> ( 0. R2 ) by L52 , TARSKI:def 1;
L54: ( ( C13 /. D4 ) * ( C14 /. D4 ) ) <> ( 0. R2 ) by L53 , L46 , CLVECT_1:2;
L55: ( ( C13 (#) C14 ) /. D4 ) <> ( 0. R2 ) by L54 , L50 , L1;
L56: (not ( ( C13 (#) C14 ) /. D4 ) in { ( 0. R2 ) }) by L55 , TARSKI:def 1;
L57: (not C16 in ( ( C13 (#) C14 ) " { ( 0. R2 ) } )) by L56 , PARTFUN2:26;
thus L58: thesis by L57 , L50 , XBOOLE_0:def 5;
end;

end;
theorem
L40: (for R1 being non  empty set holds (for R2 being ComplexNormSpace holds (for R3 being (PartFunc of R1 , R2) holds (( ||. R3 .|| " { ( 0 ) } ) = ( R3 " { ( 0. R2 ) } ) & ( ( - R3 ) " { ( 0. R2 ) } ) = ( R3 " { ( 0. R2 ) } )))))
proof
let R1 being non  empty set;
let R2 being ComplexNormSpace;
let R3 being (PartFunc of R1 , R2);
L41:
now
let C17 being (Element of R1);
thus L42: (C17 in ( ||. R3 .|| " { ( 0 ) } ) implies C17 in ( R3 " { ( 0. R2 ) } ))
proof
assume L43: C17 in ( ||. R3 .|| " { ( 0 ) } );
L44: C17 in ( dom ||. R3 .|| ) by L43 , FUNCT_1:def 7;
L45: ( ||. R3 .|| . C17 ) in { ( 0 ) } by L43 , FUNCT_1:def 7;
L46: ( ||. R3 .|| . C17 ) = ( 0 ) by L45 , TARSKI:def 1;
L47: ||. ( R3 /. C17 ) .|| = ( 0 ) by L46 , L44 , NORMSP_0:def 3;
L48: ( R3 /. C17 ) = ( 0. R2 ) by L47 , NORMSP_0:def 5;
L49: ( R3 /. C17 ) in { ( 0. R2 ) } by L48 , TARSKI:def 1;
L50: C17 in ( dom R3 ) by L44 , NORMSP_0:def 3;
thus L51: thesis by L50 , L49 , PARTFUN2:26;
end;

assume L52: C17 in ( R3 " { ( 0. R2 ) } );
L53: C17 in ( dom R3 ) by L52 , PARTFUN2:26;
L54: C17 in ( dom ||. R3 .|| ) by L53 , NORMSP_0:def 3;
L55: ( R3 /. C17 ) in { ( 0. R2 ) } by L52 , PARTFUN2:26;
L56: ( R3 /. C17 ) = ( 0. R2 ) by L55 , TARSKI:def 1;
L57: ||. ( R3 /. C17 ) .|| = ( 0 ) by L56 , NORMSP_0:def 6;
L58: ( ||. R3 .|| . C17 ) = ( 0 ) by L57 , L54 , NORMSP_0:def 3;
L59: ( ||. R3 .|| . C17 ) in { ( 0 ) } by L58 , TARSKI:def 1;
thus L60: C17 in ( ||. R3 .|| " { ( 0 ) } ) by L59 , L54 , FUNCT_1:def 7;
end;
thus L61: ( ||. R3 .|| " { ( 0 ) } ) = ( R3 " { ( 0. R2 ) } ) by L41 , SUBSET_1:3;
L62:
now
let C18 being (Element of R1);
thus L63: (C18 in ( ( - R3 ) " { ( 0. R2 ) } ) implies C18 in ( R3 " { ( 0. R2 ) } ))
proof
assume L64: C18 in ( ( - R3 ) " { ( 0. R2 ) } );
L65: C18 in ( dom ( - R3 ) ) by L64 , PARTFUN2:26;
L66: ( ( - R3 ) /. C18 ) in { ( 0. R2 ) } by L64 , PARTFUN2:26;
L67: ( ( - R3 ) /. C18 ) = ( 0. R2 ) by L66 , TARSKI:def 1;
L68: ( - ( - ( R3 /. C18 ) ) ) = ( - ( 0. R2 ) ) by L67 , L65 , VFUNCT_1:def 5;
L69: ( - ( - ( R3 /. C18 ) ) ) = ( 0. R2 ) by L68 , RLVECT_1:12;
L70: ( R3 /. C18 ) = ( 0. R2 ) by L69 , RLVECT_1:17;
L71: ( R3 /. C18 ) in { ( 0. R2 ) } by L70 , TARSKI:def 1;
L72: C18 in ( dom R3 ) by L65 , VFUNCT_1:def 5;
thus L73: thesis by L72 , L71 , PARTFUN2:26;
end;

assume L74: C18 in ( R3 " { ( 0. R2 ) } );
L75: C18 in ( dom R3 ) by L74 , PARTFUN2:26;
L76: C18 in ( dom ( - R3 ) ) by L75 , VFUNCT_1:def 5;
L77: ( R3 /. C18 ) in { ( 0. R2 ) } by L74 , PARTFUN2:26;
L78: ( R3 /. C18 ) = ( 0. R2 ) by L77 , TARSKI:def 1;
L79: ( ( - R3 ) /. C18 ) = ( - ( 0. R2 ) ) by L78 , L76 , VFUNCT_1:def 5;
L80: ( ( - R3 ) /. C18 ) = ( 0. R2 ) by L79 , RLVECT_1:12;
L81: ( ( - R3 ) /. C18 ) in { ( 0. R2 ) } by L80 , TARSKI:def 1;
thus L82: C18 in ( ( - R3 ) " { ( 0. R2 ) } ) by L81 , L76 , PARTFUN2:26;
end;
thus L83: thesis by L62 , SUBSET_1:3;
end;
theorem
L84: (for R1 being non  empty set holds (for R2 being ComplexNormSpace holds (for R3 being (PartFunc of R1 , R2) holds (for R7 being (Element of ( COMPLEX )) holds (R7 <> ( 0c ) implies ( ( R7 (#) R3 ) " { ( 0. R2 ) } ) = ( R3 " { ( 0. R2 ) } ))))))
proof
let R1 being non  empty set;
let R2 being ComplexNormSpace;
let R3 being (PartFunc of R1 , R2);
let R7 being (Element of ( COMPLEX ));
assume L85: R7 <> ( 0c );
L86:
now
let C19 being (Element of R1);
thus L87: (C19 in ( ( R7 (#) R3 ) " { ( 0. R2 ) } ) implies C19 in ( R3 " { ( 0. R2 ) } ))
proof
assume L88: C19 in ( ( R7 (#) R3 ) " { ( 0. R2 ) } );
L89: C19 in ( dom ( R7 (#) R3 ) ) by L88 , PARTFUN2:26;
L90: ( ( R7 (#) R3 ) /. C19 ) in { ( 0. R2 ) } by L88 , PARTFUN2:26;
L91: ( ( R7 (#) R3 ) /. C19 ) = ( 0. R2 ) by L90 , TARSKI:def 1;
L92: ( R7 * ( R3 /. C19 ) ) = ( 0. R2 ) by L91 , L89 , L9;
L93: ( R7 * ( R3 /. C19 ) ) = ( R7 * ( 0. R2 ) ) by L92 , CLVECT_1:1;
L94: ( R3 /. C19 ) = ( 0. R2 ) by L93 , L85 , CLVECT_1:11;
L95: ( R3 /. C19 ) in { ( 0. R2 ) } by L94 , TARSKI:def 1;
L96: C19 in ( dom R3 ) by L89 , L9;
thus L97: thesis by L96 , L95 , PARTFUN2:26;
end;

assume L98: C19 in ( R3 " { ( 0. R2 ) } );
L99: C19 in ( dom R3 ) by L98 , PARTFUN2:26;
L100: C19 in ( dom ( R7 (#) R3 ) ) by L99 , L9;
L101: ( R3 /. C19 ) in { ( 0. R2 ) } by L98 , PARTFUN2:26;
L102: ( R7 * ( R3 /. C19 ) ) = ( R7 * ( 0. R2 ) ) by L101 , TARSKI:def 1;
L103: ( R7 * ( R3 /. C19 ) ) = ( 0. R2 ) by L102 , CLVECT_1:1;
L104: ( ( R7 (#) R3 ) /. C19 ) = ( 0. R2 ) by L103 , L100 , L9;
L105: ( ( R7 (#) R3 ) /. C19 ) in { ( 0. R2 ) } by L104 , TARSKI:def 1;
thus L106: C19 in ( ( R7 (#) R3 ) " { ( 0. R2 ) } ) by L105 , L100 , PARTFUN2:26;
end;
thus L107: thesis by L86 , SUBSET_1:3;
end;
theorem
L108: (for R1 being non  empty set holds (for R2 being ComplexNormSpace holds (for R4 being (PartFunc of R1 , R2) holds (for R5 being (PartFunc of R1 , R2) holds ( R4 + R5 ) = ( R5 + R4 )))))
proof
let R1 being non  empty set;
let R2 being ComplexNormSpace;
let R4 being (PartFunc of R1 , R2);
let R5 being (PartFunc of R1 , R2);
L109: ( dom ( R4 + R5 ) ) = ( ( dom R5 ) /\ ( dom R4 ) ) by VFUNCT_1:def 1
.= ( dom ( R5 + R4 ) ) by VFUNCT_1:def 1;
L110:
now
let C20 being (Element of R1);
assume L111: C20 in ( dom ( R4 + R5 ) );
thus L112: ( ( R4 + R5 ) /. C20 ) = ( ( R5 /. C20 ) + ( R4 /. C20 ) ) by L111 , VFUNCT_1:def 1
.= ( ( R5 + R4 ) /. C20 ) by L109 , L111 , VFUNCT_1:def 1;
end;
thus L113: thesis by L110 , L109 , PARTFUN2:1;
end;
definition
let R1 being non  empty set;
let R2 being ComplexNormSpace;
let R4 being (PartFunc of R1 , R2);
let R5 being (PartFunc of R1 , R2);
redefine func R4 + R5;

commutativity
 by L108;
end;
theorem
L115: (for R1 being non  empty set holds (for R2 being ComplexNormSpace holds (for R4 being (PartFunc of R1 , R2) holds (for R5 being (PartFunc of R1 , R2) holds (for R6 being (PartFunc of R1 , R2) holds ( ( R4 + R5 ) + R6 ) = ( R4 + ( R5 + R6 ) ))))))
proof
let R1 being non  empty set;
let R2 being ComplexNormSpace;
let R4 being (PartFunc of R1 , R2);
let R5 being (PartFunc of R1 , R2);
let R6 being (PartFunc of R1 , R2);
L116: ( dom ( ( R4 + R5 ) + R6 ) ) = ( ( dom ( R4 + R5 ) ) /\ ( dom R6 ) ) by VFUNCT_1:def 1
.= ( ( ( dom R4 ) /\ ( dom R5 ) ) /\ ( dom R6 ) ) by VFUNCT_1:def 1
.= ( ( dom R4 ) /\ ( ( dom R5 ) /\ ( dom R6 ) ) ) by XBOOLE_1:16
.= ( ( dom R4 ) /\ ( dom ( R5 + R6 ) ) ) by VFUNCT_1:def 1
.= ( dom ( R4 + ( R5 + R6 ) ) ) by VFUNCT_1:def 1;
L117:
now
let C21 being (Element of R1);
assume L118: C21 in ( dom ( ( R4 + R5 ) + R6 ) );
L119: C21 in ( ( dom ( R4 + R5 ) ) /\ ( dom R6 ) ) by L118 , VFUNCT_1:def 1;
L120: C21 in ( dom ( R4 + R5 ) ) by L119 , XBOOLE_0:def 4;
L121: C21 in ( ( dom R4 ) /\ ( dom ( R5 + R6 ) ) ) by L116 , L118 , VFUNCT_1:def 1;
L122: C21 in ( dom ( R5 + R6 ) ) by L121 , XBOOLE_0:def 4;
thus L123: ( ( ( R4 + R5 ) + R6 ) /. C21 ) = ( ( ( R4 + R5 ) /. C21 ) + ( R6 /. C21 ) ) by L118 , VFUNCT_1:def 1
.= ( ( ( R4 /. C21 ) + ( R5 /. C21 ) ) + ( R6 /. C21 ) ) by L120 , VFUNCT_1:def 1
.= ( ( R4 /. C21 ) + ( ( R5 /. C21 ) + ( R6 /. C21 ) ) ) by RLVECT_1:def 3
.= ( ( R4 /. C21 ) + ( ( R5 + R6 ) /. C21 ) ) by L122 , VFUNCT_1:def 1
.= ( ( R4 + ( R5 + R6 ) ) /. C21 ) by L116 , L118 , VFUNCT_1:def 1;
end;
thus L124: thesis by L117 , L116 , PARTFUN2:1;
end;
theorem
L125: (for R1 being non  empty set holds (for R2 being ComplexNormSpace holds (for B17 , B18 being (PartFunc of R1 , ( COMPLEX )) holds (for B19 being (PartFunc of R1 , R2) holds ( ( B17 (#) B18 ) (#) B19 ) = ( B17 (#) ( B18 (#) B19 ) )))))
proof
let R1 being non  empty set;
let R2 being ComplexNormSpace;
let C22 , C23 being (PartFunc of R1 , ( COMPLEX ));
let C24 being (PartFunc of R1 , R2);
L126: ( dom ( ( C22 (#) C23 ) (#) C24 ) ) = ( ( dom ( C22 (#) C23 ) ) /\ ( dom C24 ) ) by L1
.= ( ( ( dom C22 ) /\ ( dom C23 ) ) /\ ( dom C24 ) ) by VALUED_1:def 4
.= ( ( dom C22 ) /\ ( ( dom C23 ) /\ ( dom C24 ) ) ) by XBOOLE_1:16
.= ( ( dom C22 ) /\ ( dom ( C23 (#) C24 ) ) ) by L1
.= ( dom ( C22 (#) ( C23 (#) C24 ) ) ) by L1;
L127:
now
let C25 being (Element of R1);
assume L128: C25 in ( dom ( ( C22 (#) C23 ) (#) C24 ) );
L129: C25 in ( ( dom ( C22 (#) C23 ) ) /\ ( dom C24 ) ) by L128 , L1;
L130: C25 in ( dom ( C22 (#) C23 ) ) by L129 , XBOOLE_0:def 4;
L131: ( dom ( C22 (#) C23 ) ) = ( ( dom C22 ) /\ ( dom C23 ) ) by VALUED_1:def 4;
L132: C25 in ( dom C22 ) by L131 , L130 , XBOOLE_0:def 4;
L133: ( C22 . C25 ) = ( C22 /. C25 ) by L132 , PARTFUN1:def 6;
L134: C25 in ( dom C23 ) by L130 , L131 , XBOOLE_0:def 4;
L135: ( C23 . C25 ) = ( C23 /. C25 ) by L134 , PARTFUN1:def 6;
L136: ( ( C22 (#) C23 ) /. C25 ) = ( ( C22 (#) C23 ) . C25 ) by L130 , PARTFUN1:def 6
.= ( ( C22 /. C25 ) * ( C23 /. C25 ) ) by L130 , L133 , L135 , VALUED_1:def 4;
L137: C25 in ( ( dom C22 ) /\ ( dom ( C23 (#) C24 ) ) ) by L126 , L128 , L1;
L138: C25 in ( dom ( C23 (#) C24 ) ) by L137 , XBOOLE_0:def 4;
thus L139: ( ( ( C22 (#) C23 ) (#) C24 ) /. C25 ) = ( ( ( C22 (#) C23 ) /. C25 ) * ( C24 /. C25 ) ) by L128 , L1
.= ( ( C22 /. C25 ) * ( ( C23 /. C25 ) * ( C24 /. C25 ) ) ) by L136 , CLVECT_1:def 4
.= ( ( C22 /. C25 ) * ( ( C23 (#) C24 ) /. C25 ) ) by L138 , L1
.= ( ( C22 (#) ( C23 (#) C24 ) ) /. C25 ) by L126 , L128 , L1;
end;
thus L140: thesis by L127 , L126 , PARTFUN2:1;
