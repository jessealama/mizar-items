begin
L1: (for B1 , B2 being RelStr holds (for B3 , B4 being (Element of B1) holds (for B5 , B6 being (Element of B2) holds ((B3 = B5 & B4 = B6 & ( the RelStr of B1 ) = ( the RelStr of B2 )) implies (B3 <= B4 implies B5 <= B6)))))
proof
let C1 , C2 being RelStr;
let C3 , C4 being (Element of C1);
let C5 , C6 being (Element of C2);
assume that
L2: C3 = C5
and
L3: C4 = C6
and
L4: ( the RelStr of C1 ) = ( the RelStr of C2 );
assume L5: C3 <= C4;
L6: [ C5 , C6 ] in (the InternalRel of C2) by L5 , L2 , L3 , L4 , ORDERS_2:def 5;
thus L7: thesis by L6 , ORDERS_2:def 5;
end;
scheme Irrel { F1 , F2() -> non  empty set , P1[set] , F3(set) -> set , F4(set , set) -> set } : { F3(B7) where B7 is (Element of F1()) : P1[ B7 ] } = { F4(B8 , B9) where B8 is (Element of F2()) , B9 is (Element of F1()) : P1[ B9 ] }
provided
L8: (for B10 being (Element of F2()) holds (for B11 being (Element of F1()) holds F3(B11) = F4(B10 , B11)))
proof
set D1 = { F3(B12) where B12 is (Element of F1()) : P1[ B12 ] };
set D2 = { F4(B13 , B14) where B13 is (Element of F2()) , B14 is (Element of F1()) : P1[ B14 ] };
thus L9: D1 c= D2
proof
let C7 being set;
set D3 = the (Element of F2());
assume L10: C7 in D1;
consider C8 being (Element of F1()) such that L11: C7 = F3(C8) and L12: P1[ C8 ] by L10;
L13: C7 = F4(D3 , C8) by L8 , L11;
thus L14: thesis by L13 , L12;
end;

let C9 being set;
assume L15: C9 in D2;
consider C10 being (Element of F2()), C11 being (Element of F1()) such that L16: C9 = F4(C10 , C11) and L17: P1[ C11 ] by L15;
L18: C9 = F3(C11) by L8 , L16;
thus L19: thesis by L18 , L17;
end;
