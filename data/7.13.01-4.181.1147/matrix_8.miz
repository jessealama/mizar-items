theorem
L140: (for R2 being Nat holds (for R3 being Field holds (for R4 being (Matrix of R2 , R3) holds (R4 is  symmetric implies ( R4 * ( R4 @ ) ) is  symmetric))))
proof
let R2 being Nat;
let R3 being Field;
let R4 being (Matrix of R2 , R3);
assume L141: R4 is  symmetric;
per cases  by NAT_1:3;
suppose L142: R2 > ( 0 );

L143: (( width R4 ) = R2 & ( len R4 ) = R2) by MATRIX_1:24;
L144: ( ( R4 * ( R4 @ ) ) @ ) = ( ( R4 * R4 ) @ ) by L141 , MATRIX_6:def 5
.= ( ( R4 @ ) * ( R4 @ ) ) by L142 , L143 , MATRIX_3:22
.= ( R4 * ( R4 @ ) ) by L141 , MATRIX_6:def 5;
thus L145: thesis by L144 , MATRIX_6:def 5;
end;
suppose L146: R2 = ( 0 );

L147: ( ( R4 * ( R4 @ ) ) @ ) = ( R4 * ( R4 @ ) ) by L146 , MATRIX_1:35;
thus L148: thesis by L147 , MATRIX_6:def 5;
end;
end;
theorem
L150: (for R2 being Nat holds (for R3 being Field holds (for R4 being (Matrix of R2 , R3) holds (R4 is  symmetric implies ( ( R4 @ ) * R4 ) is  symmetric))))
proof
let R2 being Nat;
let R3 being Field;
let R4 being (Matrix of R2 , R3);
assume L151: R4 is  symmetric;
per cases  by NAT_1:3;
suppose L152: R2 > ( 0 );

