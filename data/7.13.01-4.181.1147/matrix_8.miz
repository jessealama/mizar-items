begin
definition
let C1 being Nat;
let C2 being Field;
let C3 being (Matrix of C1 , C2);
attr C3 is  Idempotent
means
:L1: ( C3 * C3 ) = C3;
attr C3 is  Nilpotent
means
:L2: ( C3 * C3 ) = ( 0. (C2 , C1) );
attr C3 is  Involutory
means
:L3: ( C3 * C3 ) = ( 1. (C2 , C1) );
attr C3 is  Self_Reversible
means
:L4: (C3 is  invertible & ( C3 ~ ) = C3);
end;
theorem
L6: (for R2 being Nat holds (for R3 being Field holds ( 1. (R3 , R2) ) is  Idempotent  Involutory))
proof
let R2 being Nat;
let R3 being Field;
L7: ( ( 1. (R3 , R2) ) * ( 1. (R3 , R2) ) ) = ( 1. (R3 , R2) ) by MATRIX_3:18;
thus L8: thesis by L7 , L1 , L3;
end;
theorem
L9: (for R2 being Nat holds (for R3 being Field holds ( 0. (R3 , R2) ) is  Idempotent  Nilpotent))
proof
let R2 being Nat;
let R3 being Field;
L10: (( width ( 0. (R3 , R2 , R2) ) ) = R2 & ( len ( 0. (R3 , R2 , R2) ) ) = R2) by MATRIX_1:24;
L11: ( ( 0. (R3 , R2 , R2) ) * ( 0. (R3 , R2) ) ) = ( 0. (R3 , R2 , R2) ) by L10 , MATRIX_6:1;
thus L12: thesis by L11 , L1 , L2;
end;
registration
let C4 being Nat;
let C5 being Field;
cluster ( 1. (C5 , C4) ) ->  Idempotent  Involutory;
coherence by L6;
cluster ( 0. (C5 , C4) ) ->  Idempotent  Nilpotent;
coherence by L9;
end;
theorem
L14: (for R2 being Nat holds (for R3 being Field holds (for R4 being (Matrix of R2 , R3) holds (R2 > ( 0 ) implies (R4 is  Idempotent iff ( R4 @ ) is  Idempotent)))))
proof
let R2 being Nat;
let R3 being Field;
let R4 being (Matrix of R2 , R3);
assume L15: R2 > ( 0 );
set D1 = ( R4 @ );
L16: (( width R4 ) = R2 & ( len R4 ) = R2) by MATRIX_1:24;
thus L17: (R4 is  Idempotent implies D1 is  Idempotent)
proof
assume L18: R4 is  Idempotent;
L19: ( D1 * D1 ) = ( ( R4 * R4 ) @ ) by L15 , L16 , MATRIX_3:22
.= D1 by L18 , L1;
thus L20: thesis by L19 , L1;
end;

L21: (( width D1 ) = R2 & ( len D1 ) = R2) by MATRIX_1:24;
assume L22: D1 is  Idempotent;
L23: R4 = ( D1 @ ) by L15 , L16 , MATRIX_2:13
.= ( ( D1 * D1 ) @ ) by L22 , L1
.= ( ( D1 @ ) * ( D1 @ ) ) by L15 , L21 , MATRIX_3:22
.= ( R4 * ( ( R4 @ ) @ ) ) by L15 , L16 , MATRIX_2:13
.= ( R4 * R4 ) by L15 , L16 , MATRIX_2:13;
thus L24: thesis by L23 , L1;
end;
theorem
L25: (for R2 being Nat holds (for R3 being Field holds (for R4 being (Matrix of R2 , R3) holds (R4 is  Involutory implies R4 is  invertible))))
proof
let R2 being Nat;
let R3 being Field;
let R4 being (Matrix of R2 , R3);
assume L26: R4 is  Involutory;
L27: ( R4 * R4 ) = ( 1. (R3 , R2) ) by L26 , L3;
L28: R4 is_reverse_of R4 by L27 , MATRIX_6:def 2;
thus L29: thesis by L28 , MATRIX_6:def 3;
end;
theorem
L30: (for R2 being Nat holds (for R3 being Field holds (for R4 being (Matrix of R2 , R3) holds (for R5 being (Matrix of R2 , R3) holds ((R4 is  Idempotent & R5 is  Idempotent & R4 commutes_with R5) implies ( R4 * R4 ) commutes_with ( R5 * R5 ))))))
proof
let R2 being Nat;
let R3 being Field;
let R4 being (Matrix of R2 , R3);
let R5 being (Matrix of R2 , R3);
assume that
L31: R4 is  Idempotent
and
L32: (R5 is  Idempotent & R4 commutes_with R5);
L33: ( R4 * R4 ) = R4 by L31 , L1;
thus L34: thesis by L33 , L32 , L1;
end;
theorem
L35: (for R2 being Nat holds (for R3 being Field holds (for R4 being (Matrix of R2 , R3) holds (for R5 being (Matrix of R2 , R3) holds ((R2 > ( 0 ) & R4 is  Idempotent & R5 is  Idempotent & R4 commutes_with R5 & ( R4 * R5 ) = ( 0. (R3 , R2) )) implies ( R4 + R5 ) is  Idempotent)))))
proof
let R2 being Nat;
let R3 being Field;
let R4 being (Matrix of R2 , R3);
let R5 being (Matrix of R2 , R3);
assume that
L36: R2 > ( 0 )
and
L37: (R4 is  Idempotent & R5 is  Idempotent)
and
L38: R4 commutes_with R5
and
L39: ( R4 * R5 ) = ( 0. (R3 , R2) );
L40: ( R4 * R5 ) = ( 0. (R3 , R2 , R2) ) by L39;
L41: (( R4 * R4 ) = R4 & ( R5 * R5 ) = R5) by L37 , L1;
L42: ( ( R4 + R5 ) * ( R4 + R5 ) ) = ( ( ( ( R4 * R4 ) + ( 0. (R3 , R2) ) ) + ( 0. (R3 , R2) ) ) + ( R5 * R5 ) ) by L36 , L38 , L39 , MATRIX_6:35
.= ( ( ( R4 * R4 ) + ( 0. (R3 , R2) ) ) + ( R5 * R5 ) ) by L40 , MATRIX_3:4
.= ( R4 + R5 ) by L41 , L40 , MATRIX_3:4;
thus L43: thesis by L42 , L1;
end;
theorem
L44: (for R2 being Nat holds (for R3 being Field holds (for R4 being (Matrix of R2 , R3) holds (for R5 being (Matrix of R2 , R3) holds ((R4 is  Idempotent & R5 is  Idempotent & ( R4 * R5 ) = ( - ( R5 * R4 ) )) implies ( R4 + R5 ) is  Idempotent)))))
proof
let R2 being Nat;
let R3 being Field;
let R4 being (Matrix of R2 , R3);
let R5 being (Matrix of R2 , R3);
assume that
L45: (R4 is  Idempotent & R5 is  Idempotent)
and
L46: ( R4 * R5 ) = ( - ( R5 * R4 ) );
L47: (( R4 * R4 ) = R4 & ( R5 * R5 ) = R5) by L45 , L1;
per cases  by NAT_1:3;
suppose L48: R2 > ( 0 );

L49: (( len ( R4 * R5 ) ) = R2 & ( width ( R4 * R5 ) ) = R2) by MATRIX_1:24;
L50: (( len R5 ) = R2 & ( width R5 ) = R2) by MATRIX_1:24;
L51: (( len ( ( R4 * R4 ) + ( R5 * R4 ) ) ) = R2 & ( width ( ( R4 * R4 ) + ( R5 * R4 ) ) ) = R2) by MATRIX_1:24;
L52: (( len ( R5 * R4 ) ) = R2 & ( width ( R5 * R4 ) ) = R2) by MATRIX_1:24;
L53: (( len ( R4 * R4 ) ) = R2 & ( width ( R4 * R4 ) ) = R2) by MATRIX_1:24;
L54: (( len R4 ) = R2 & ( width R4 ) = R2) by MATRIX_1:24;
L55: (( len ( R4 + R5 ) ) = R2 & ( width ( R4 + R5 ) ) = R2) by MATRIX_1:24;
L56: ( ( R4 + R5 ) * ( R4 + R5 ) ) = ( ( ( R4 + R5 ) * R4 ) + ( ( R4 + R5 ) * R5 ) ) by L55 , L48 , L54 , L50 , MATRIX_4:62
.= ( ( ( R4 * R4 ) + ( R5 * R4 ) ) + ( ( R4 + R5 ) * R5 ) ) by L48 , L54 , L50 , MATRIX_4:63
.= ( ( ( R4 * R4 ) + ( R5 * R4 ) ) + ( ( R4 * R5 ) + ( R5 * R5 ) ) ) by L48 , L54 , L50 , MATRIX_4:63
.= ( ( ( ( R4 * R4 ) + ( R5 * R4 ) ) + ( R4 * R5 ) ) + ( R5 * R5 ) ) by L49 , L51 , MATRIX_3:3
.= ( ( ( R4 * R4 ) + ( ( R5 * R4 ) + ( - ( R5 * R4 ) ) ) ) + ( R5 * R5 ) ) by L46 , L53 , L52 , MATRIX_3:3
.= ( ( ( R4 * R4 ) + ( 0. (R3 , R2 , R2) ) ) + ( R5 * R5 ) ) by L52 , MATRIX_4:2
.= ( R4 + R5 ) by L47 , MATRIX_3:4;
thus L57: thesis by L56 , L1;
end;
suppose L58: R2 = ( 0 );

L59: ( ( R4 + R5 ) * ( R4 + R5 ) ) = ( R4 + R5 ) by L58 , MATRIX_1:35;
thus L60: thesis by L59 , L1;
end;
end;
theorem
L62: (for R2 being Nat holds (for R3 being Field holds (for R4 being (Matrix of R2 , R3) holds (for R5 being (Matrix of R2 , R3) holds ((R4 is  Idempotent & R5 is  invertible) implies ( ( ( R5 ~ ) * R4 ) * R5 ) is  Idempotent)))))
proof
let R2 being Nat;
let R3 being Field;
let R4 being (Matrix of R2 , R3);
let R5 being (Matrix of R2 , R3);
assume that
L63: R4 is  Idempotent
and
L64: R5 is  invertible;
L65: ( R5 ~ ) is_reverse_of R5 by L64 , MATRIX_6:def 4;
L66: ( width R5 ) = R2 by MATRIX_1:24;
L67: ( len R5 ) = R2 by MATRIX_1:24;
L68: (( len ( R4 * R5 ) ) = R2 & ( width ( ( ( R5 ~ ) * R4 ) * R5 ) ) = R2) by MATRIX_1:24;
L69: ( len ( R5 ~ ) ) = R2 by MATRIX_1:24;
L70: ( width ( ( R5 ~ ) * R4 ) ) = R2 by MATRIX_1:24;
L71: ( width ( R5 ~ ) ) = R2 by MATRIX_1:24;
L72: (( len R4 ) = R2 & ( width R4 ) = R2) by MATRIX_1:24;
L73: ( ( ( ( R5 ~ ) * R4 ) * R5 ) * ( ( ( R5 ~ ) * R4 ) * R5 ) ) = ( ( ( ( R5 ~ ) * R4 ) * R5 ) * ( ( R5 ~ ) * ( R4 * R5 ) ) ) by L72 , L67 , L71 , MATRIX_3:33
.= ( ( ( ( ( R5 ~ ) * R4 ) * R5 ) * ( R5 ~ ) ) * ( R4 * R5 ) ) by L69 , L71 , L68 , MATRIX_3:33
.= ( ( ( ( R5 ~ ) * R4 ) * ( R5 * ( R5 ~ ) ) ) * ( R4 * R5 ) ) by L67 , L66 , L69 , L70 , MATRIX_3:33
.= ( ( ( ( R5 ~ ) * R4 ) * ( 1. (R3 , R2) ) ) * ( R4 * R5 ) ) by L65 , MATRIX_6:def 2
.= ( ( ( R5 ~ ) * R4 ) * ( R4 * R5 ) ) by MATRIX_3:19
.= ( ( ( ( R5 ~ ) * R4 ) * R4 ) * R5 ) by L72 , L67 , L70 , MATRIX_3:33
.= ( ( ( R5 ~ ) * ( R4 * R4 ) ) * R5 ) by L72 , L71 , MATRIX_3:33
.= ( ( ( R5 ~ ) * R4 ) * R5 ) by L63 , L1;
thus L74: thesis by L73 , L1;
end;
theorem
L75: (for R2 being Nat holds (for R3 being Field holds (for R4 being (Matrix of R2 , R3) holds (R4 is  invertible  Idempotent implies ( R4 ~ ) is  Idempotent))))
proof
let R2 being Nat;
let R3 being Field;
let R4 being (Matrix of R2 , R3);
assume L76: R4 is  invertible  Idempotent;
L77: ( R4 ~ ) = ( ( R4 * R4 ) ~ ) by L76 , L1
.= ( ( R4 ~ ) * ( R4 ~ ) ) by L76 , MATRIX_6:36;
thus L78: thesis by L77 , L1;
end;
theorem
L79: (for R2 being Nat holds (for R3 being Field holds (for R4 being (Matrix of R2 , R3) holds (R4 is  invertible  Idempotent implies R4 = ( 1. (R3 , R2) )))))
proof
let R2 being Nat;
let R3 being Field;
let R4 being (Matrix of R2 , R3);
L80: (( len R4 ) = R2 & ( width R4 ) = R2) by MATRIX_1:24;
L81: ( width ( R4 ~ ) ) = R2 by MATRIX_1:24;
assume L82: R4 is  invertible  Idempotent;
L83: ( R4 ~ ) is_reverse_of R4 by L82 , MATRIX_6:def 4;
L84: ( R4 * R4 ) = R4 by L82 , L1;
L85: ( 1. (R3 , R2) ) = ( ( R4 ~ ) * ( R4 * R4 ) ) by L84 , L83 , MATRIX_6:def 2
.= ( ( ( R4 ~ ) * R4 ) * R4 ) by L80 , L81 , MATRIX_3:33
.= ( ( 1. (R3 , R2) ) * R4 ) by L83 , MATRIX_6:def 2
.= R4 by MATRIX_3:18;
thus L86: thesis by L85;
end;
theorem
L87: (for R2 being Nat holds (for R3 being Field holds (for R4 being (Matrix of R2 , R3) holds (for R5 being (Matrix of R2 , R3) holds ((R4 is  Idempotent & R5 is  Idempotent & R4 commutes_with R5) implies ( R4 * R5 ) is  Idempotent)))))
proof
let R2 being Nat;
let R3 being Field;
let R4 being (Matrix of R2 , R3);
let R5 being (Matrix of R2 , R3);
assume that
L88: R4 is  Idempotent
and
L89: R5 is  Idempotent
and
L90: R4 commutes_with R5;
L91: ( len R4 ) = R2 by MATRIX_1:24;
L92: ( width R5 ) = R2 by MATRIX_1:24;
L93: (( width R4 ) = R2 & ( len R5 ) = R2) by MATRIX_1:24;
L94: ( width ( R4 * R5 ) ) = R2 by MATRIX_1:24;
L95: ( ( R4 * R5 ) * ( R4 * R5 ) ) = ( ( ( R4 * R5 ) * R4 ) * R5 ) by L94 , L91 , L93 , MATRIX_3:33
.= ( ( R4 * ( R5 * R4 ) ) * R5 ) by L91 , L93 , L92 , MATRIX_3:33
.= ( ( R4 * ( R4 * R5 ) ) * R5 ) by L90 , MATRIX_6:def 1
.= ( ( ( R4 * R4 ) * R5 ) * R5 ) by L91 , L93 , MATRIX_3:33
.= ( ( R4 * R5 ) * R5 ) by L88 , L1
.= ( R4 * ( R5 * R5 ) ) by L93 , L92 , MATRIX_3:33
.= ( R4 * R5 ) by L89 , L1;
thus L96: thesis by L95 , L1;
end;
theorem
L97: (for R2 being Nat holds (for R3 being Field holds (for R4 being (Matrix of R2 , R3) holds (for R5 being (Matrix of R2 , R3) holds ((R4 is  Idempotent & R5 is  Idempotent & R4 commutes_with R5) implies ( ( R4 @ ) * ( R5 @ ) ) is  Idempotent)))))
proof
let R2 being Nat;
let R3 being Field;
let R4 being (Matrix of R2 , R3);
let R5 being (Matrix of R2 , R3);
assume that
L98: R4 is  Idempotent
and
L99: R5 is  Idempotent
and
L100: R4 commutes_with R5;
set D2 = ( ( R4 @ ) * ( R5 @ ) );
per cases  by NAT_1:3;
suppose L101: R2 > ( 0 );

L102: ( R4 * R4 ) = R4 by L98 , L1;
L103: ( len R4 ) = R2 by MATRIX_1:24;
L104: ( len ( R4 @ ) ) = R2 by MATRIX_1:24;
L105: ( width ( R5 @ ) ) = R2 by MATRIX_1:24;
L106: ( width R5 ) = R2 by MATRIX_1:24;
L107: ( len R5 ) = R2 by MATRIX_1:24;
L108: ( width R4 ) = R2 by MATRIX_1:24;
L109: (( width ( R4 @ ) ) = R2 & ( len ( R5 @ ) ) = R2) by MATRIX_1:24;
L110: ( width D2 ) = R2 by MATRIX_1:24;
L111: ( D2 * D2 ) = ( ( ( ( R4 @ ) * ( R5 @ ) ) * ( R4 @ ) ) * ( R5 @ ) ) by L110 , L104 , L109 , MATRIX_3:33
.= ( ( ( R4 @ ) * ( ( R5 @ ) * ( R4 @ ) ) ) * ( R5 @ ) ) by L104 , L109 , L105 , MATRIX_3:33
.= ( ( ( R4 @ ) * ( ( R4 * R5 ) @ ) ) * ( R5 @ ) ) by L101 , L108 , L107 , L106 , MATRIX_3:22
.= ( ( ( R4 @ ) * ( ( R5 * R4 ) @ ) ) * ( R5 @ ) ) by L100 , MATRIX_6:def 1
.= ( ( ( R4 @ ) * ( ( R4 @ ) * ( R5 @ ) ) ) * ( R5 @ ) ) by L101 , L103 , L108 , L106 , MATRIX_3:22
.= ( ( ( ( R4 @ ) * ( R4 @ ) ) * ( R5 @ ) ) * ( R5 @ ) ) by L104 , L109 , MATRIX_3:33
.= ( ( ( ( R4 * R4 ) @ ) * ( R5 @ ) ) * ( R5 @ ) ) by L101 , L103 , L108 , MATRIX_3:22
.= ( ( R4 @ ) * ( ( R5 @ ) * ( R5 @ ) ) ) by L102 , L109 , L105 , MATRIX_3:33
.= ( ( R4 @ ) * ( ( R5 * R5 ) @ ) ) by L101 , L107 , L106 , MATRIX_3:22
.= ( ( R4 @ ) * ( R5 @ ) ) by L99 , L1;
thus L112: thesis by L111 , L1;
end;
suppose L113: R2 = ( 0 );

L114: ( ( ( R4 @ ) * ( R5 @ ) ) * ( ( R4 @ ) * ( R5 @ ) ) ) = ( ( R4 @ ) * ( R5 @ ) ) by L113 , MATRIX_1:35;
thus L115: thesis by L114 , L1;
end;
end;
