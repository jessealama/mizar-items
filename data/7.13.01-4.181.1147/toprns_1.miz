begin
definition
let C1 being Nat;
mode Real_Sequence of C1
 is (sequence of ( TOP-REAL C1 ));
end;
theorem
L2: (for R1 being (Element of ( NAT )) holds (for B1 being Function holds (B1 is (Real_Sequence of R1) iff (( dom B1 ) = ( NAT ) & (for R2 being (Element of ( NAT )) holds ( B1 . R2 ) is (Point of ( TOP-REAL R1 )))))))
proof
let R1 being (Element of ( NAT ));
let C2 being Function;
thus L3: (C2 is (Real_Sequence of R1) implies (( dom C2 ) = ( NAT ) & (for R2 being (Element of ( NAT )) holds ( C2 . R2 ) is (Point of ( TOP-REAL R1 ))))) by NORMSP_1:12;
assume that
L4: ( dom C2 ) = ( NAT )
and
L5: (for R2 being (Element of ( NAT )) holds ( C2 . R2 ) is (Point of ( TOP-REAL R1 )));
L6: (for R10 being set holds (R10 in ( NAT ) implies ( C2 . R10 ) is (Point of ( TOP-REAL R1 )))) by L5;
thus L7: thesis by L6 , L4 , NORMSP_1:12;
end;
definition
let R1 being (Element of ( NAT ));
let C3 being (Real_Sequence of R1);
attr C3 is  non-zero
means
:L8: ( rng C3 ) c= ( NonZero ( TOP-REAL R1 ) );
end;
theorem
L10: (for R1 being (Element of ( NAT )) holds (for R18 being (Real_Sequence of R1) holds (R18 is  non-zero iff (for R10 being set holds (R10 in ( NAT ) implies ( R18 . R10 ) <> ( 0. ( TOP-REAL R1 ) ))))))
proof
let R1 being (Element of ( NAT ));
let R18 being (Real_Sequence of R1);
thus L11: (R18 is  non-zero implies (for R10 being set holds (R10 in ( NAT ) implies ( R18 . R10 ) <> ( 0. ( TOP-REAL R1 ) ))))
proof
assume L12: R18 is  non-zero;
L13: ( rng R18 ) c= ( NonZero ( TOP-REAL R1 ) ) by L12 , L8;
let R10 being set;
assume L14: R10 in ( NAT );
L15: R10 in ( dom R18 ) by L14 , L2;
L16: ( R18 . R10 ) in ( rng R18 ) by L15 , FUNCT_1:def 3;
L17: (not ( R18 . R10 ) in { ( 0. ( TOP-REAL R1 ) ) }) by L16 , L13 , XBOOLE_0:def 5;
thus L18: thesis by L17 , TARSKI:def 1;
end;

assume L19: (for R10 being set holds (R10 in ( NAT ) implies ( R18 . R10 ) <> ( 0. ( TOP-REAL R1 ) )));
L20:
now
let R11 being set;
assume L21: R11 in ( rng R18 );
consider R10 being set such that L22: R10 in ( dom R18 ) and L23: ( R18 . R10 ) = R11 by L21 , FUNCT_1:def 3;
L24: R10 in ( NAT ) by L22 , NORMSP_1:12;
L25: R11 <> ( 0. ( TOP-REAL R1 ) ) by L24 , L19 , L23;
L26: (not R11 in { ( 0. ( TOP-REAL R1 ) ) }) by L25 , TARSKI:def 1;
L27: R11 is (Point of ( TOP-REAL R1 )) by L23 , L24 , NORMSP_1:12;
thus L28: R11 in ( NonZero ( TOP-REAL R1 ) ) by L27 , L26 , XBOOLE_0:def 5;
end;
L29: ( rng R18 ) c= ( NonZero ( TOP-REAL R1 ) ) by L20 , TARSKI:def 3;
thus L30: thesis by L29 , L8;
end;
theorem
L31: (for R1 being (Element of ( NAT )) holds (for R18 being (Real_Sequence of R1) holds (R18 is  non-zero iff (for R2 being (Element of ( NAT )) holds ( R18 . R2 ) <> ( 0. ( TOP-REAL R1 ) )))))
proof
let R1 being (Element of ( NAT ));
let R18 being (Real_Sequence of R1);
thus L32: (R18 is  non-zero implies (for R2 being (Element of ( NAT )) holds ( R18 . R2 ) <> ( 0. ( TOP-REAL R1 ) ))) by L10;
assume L33: (for R2 being (Element of ( NAT )) holds ( R18 . R2 ) <> ( 0. ( TOP-REAL R1 ) ));
L34: (for R10 being set holds (R10 in ( NAT ) implies ( R18 . R10 ) <> ( 0. ( TOP-REAL R1 ) ))) by L33;
thus L35: thesis by L34 , L10;
end;
definition
let C4 being Nat;
let C5 , C6 being (Real_Sequence of C4);
func C5 + C6 -> (Real_Sequence of C4) equals 
( C5 + C6 );
coherence
proof
L36: (( dom C5 ) = ( NAT ) & ( dom C6 ) = ( NAT )) by FUNCT_2:def 1;
L37: ( dom ( C5 + C6 ) ) = ( ( dom C5 ) /\ ( dom C6 ) ) by VFUNCT_1:def 1;
thus L38: thesis by L37 , L36 , FUNCT_2:67;
end;
end;
definition
let R7 being Real;
let C7 being Nat;
let C8 being (Real_Sequence of C7);
func R7 * C8 -> (Real_Sequence of C7) equals 
( R7 (#) C8 );
coherence
proof
L40: ( dom C8 ) = ( NAT ) by FUNCT_2:def 1;
L41: ( dom ( R7 (#) C8 ) ) = ( dom C8 ) by VFUNCT_1:def 4;
thus L42: thesis by L41 , L40 , FUNCT_2:67;
end;
end;
definition
let C9 being Nat;
let C10 being (Real_Sequence of C9);
func - C10 -> (Real_Sequence of C9) equals 
( - C10 );
coherence
proof
L44: ( dom C10 ) = ( NAT ) by FUNCT_2:def 1;
L45: ( dom ( - C10 ) ) = ( dom C10 ) by VFUNCT_1:def 5;
thus L46: thesis by L45 , L44 , FUNCT_2:67;
end;
end;
definition
let C11 being Nat;
let C12 , C13 being (Real_Sequence of C11);
func C12 - C13 -> (Real_Sequence of C11) equals 
( C12 + ( - C13 ) );
correctness;
end;
definition
let C14 being Nat;
let C15 being (Real_Sequence of C14);
func |.C15 .| -> Real_Sequence means 
:L49: (for R2 being (Element of ( NAT )) holds ( it . R2 ) = |. ( C15 . R2 ) .|);
existence
proof
deffunc H1((Element of ( NAT ))) = |. ( C15 . $1 ) .|;
thus L50: (ex B2 being Real_Sequence st (for R2 being (Element of ( NAT )) holds ( B2 . R2 ) = H1(R2))) from SEQ_1:sch 1;
end;
uniqueness
proof
let C16 , C17 being Real_Sequence;
assume that
L51: (for R2 being (Element of ( NAT )) holds ( C16 . R2 ) = |. ( C15 . R2 ) .|)
and
L52: (for R2 being (Element of ( NAT )) holds ( C17 . R2 ) = |. ( C15 . R2 ) .|);
let R2 being (Element of ( NAT ));
L53: ( C17 . R2 ) = |. ( C15 . R2 ) .| by L52;
thus L54: ( C16 . R2 ) = ( C17 . R2 ) by L53 , L51;
end;
end;
theorem
L56: (for B3 , B4 being Nat holds (for B5 , B6 being (Real_Sequence of B3) holds ( ( B5 + B6 ) . B4 ) = ( ( B5 . B4 ) + ( B6 . B4 ) )))
proof
let C18 , C19 being Nat;
let C20 , C21 being (Real_Sequence of C18);
reconsider D1 = C19 as (Element of ( NAT )) by ORDINAL1:def 12;
L57: ( dom ( C20 + C21 ) ) = ( NAT ) by FUNCT_2:def 1;
thus L58: ( ( C20 + C21 ) . C19 ) = ( ( C20 + C21 ) /. D1 )
.= ( ( C20 /. D1 ) + ( C21 /. D1 ) ) by L57 , VFUNCT_1:def 1
.= ( ( C20 . C19 ) + ( C21 . C19 ) );
end;
theorem
L59: (for R7 being Real holds (for B7 , B8 being Nat holds (for B9 being (Real_Sequence of B7) holds ( ( R7 * B9 ) . B8 ) = ( R7 * ( B9 . B8 ) ))))
proof
let R7 being Real;
let C22 , C23 being Nat;
let C24 being (Real_Sequence of C22);
reconsider D2 = C23 as (Element of ( NAT )) by ORDINAL1:def 12;
L60: ( dom ( R7 * C24 ) ) = ( NAT ) by FUNCT_2:def 1;
thus L61: ( ( R7 * C24 ) . C23 ) = ( ( R7 * C24 ) /. D2 )
.= ( R7 * ( C24 /. D2 ) ) by L60 , VFUNCT_1:def 4
.= ( R7 * ( C24 . C23 ) );
end;
theorem
L62: (for B10 , B11 being Nat holds (for B12 being (Real_Sequence of B10) holds ( ( - B12 ) . B11 ) = ( - ( B12 . B11 ) )))
proof
let C25 , C26 being Nat;
let C27 being (Real_Sequence of C25);
reconsider D3 = C26 as (Element of ( NAT )) by ORDINAL1:def 12;
L63: ( dom ( - C27 ) ) = ( NAT ) by FUNCT_2:def 1;
thus L64: ( ( - C27 ) . C26 ) = ( ( - C27 ) /. D3 )
.= ( - ( C27 /. D3 ) ) by L63 , VFUNCT_1:def 5
.= ( - ( C27 . C26 ) );
end;
theorem
L65: (for R1 being (Element of ( NAT )) holds (for R7 being Real holds (for R12 being (Point of ( TOP-REAL R1 )) holds ( ( abs R7 ) * |. R12 .| ) = |. ( R7 * R12 ) .|))) by EUCLID:11;
theorem
L66: (for R1 being (Element of ( NAT )) holds (for R7 being Real holds (for R18 being (Real_Sequence of R1) holds |. ( R7 * R18 ) .| = ( ( abs R7 ) (#) |. R18 .| ))))
proof
let R1 being (Element of ( NAT ));
let R7 being Real;
let R18 being (Real_Sequence of R1);
let R2 being (Element of ( NAT ));
thus L67: ( |. ( R7 * R18 ) .| . R2 ) = |. ( ( R7 * R18 ) . R2 ) .| by L49
.= |. ( R7 * ( R18 . R2 ) ) .| by L59
.= ( ( abs R7 ) * |. ( R18 . R2 ) .| ) by L65
.= ( ( abs R7 ) * ( |. R18 .| . R2 ) ) by L49
.= ( ( ( abs R7 ) (#) |. R18 .| ) . R2 ) by SEQ_1:9;
end;
theorem
L68: (for R1 being (Element of ( NAT )) holds (for R19 being (Real_Sequence of R1) holds (for R20 being (Real_Sequence of R1) holds ( R19 + R20 ) = ( R20 + R19 ))))
proof
let R1 being (Element of ( NAT ));
let R19 being (Real_Sequence of R1);
let R20 being (Real_Sequence of R1);
let R2 being (Element of ( NAT ));
thus L69: ( ( R19 + R20 ) . R2 ) = ( ( R19 . R2 ) + ( R20 . R2 ) ) by L56
.= ( ( R20 + R19 ) . R2 ) by L56;
end;
theorem
L70: (for R1 being (Element of ( NAT )) holds (for R19 being (Real_Sequence of R1) holds (for R20 being (Real_Sequence of R1) holds (for R21 being (Real_Sequence of R1) holds ( ( R19 + R20 ) + R21 ) = ( R19 + ( R20 + R21 ) )))))
proof
let R1 being (Element of ( NAT ));
let R19 being (Real_Sequence of R1);
let R20 being (Real_Sequence of R1);
let R21 being (Real_Sequence of R1);
let R2 being (Element of ( NAT ));
thus L71: ( ( ( R19 + R20 ) + R21 ) . R2 ) = ( ( ( R19 + R20 ) . R2 ) + ( R21 . R2 ) ) by L56
.= ( ( ( R19 . R2 ) + ( R20 . R2 ) ) + ( R21 . R2 ) ) by L56
.= ( ( R19 . R2 ) + ( ( R20 . R2 ) + ( R21 . R2 ) ) ) by EUCLID:26
.= ( ( R19 . R2 ) + ( ( R20 + R21 ) . R2 ) ) by L56
.= ( ( R19 + ( R20 + R21 ) ) . R2 ) by L56;
end;
theorem
L72: (for R1 being (Element of ( NAT )) holds (for R18 being (Real_Sequence of R1) holds ( - R18 ) = ( ( - 1 ) * R18 )))
proof
let R1 being (Element of ( NAT ));
let R18 being (Real_Sequence of R1);
let R2 being (Element of ( NAT ));
thus L73: ( ( ( - 1 ) * R18 ) . R2 ) = ( ( - 1 ) * ( R18 . R2 ) ) by L59
.= ( - ( R18 . R2 ) ) by EUCLID:39
.= ( ( - R18 ) . R2 ) by L62;
end;
theorem
L74: (for R1 being (Element of ( NAT )) holds (for R7 being Real holds (for R19 being (Real_Sequence of R1) holds (for R20 being (Real_Sequence of R1) holds ( R7 * ( R19 + R20 ) ) = ( ( R7 * R19 ) + ( R7 * R20 ) )))))
proof
let R1 being (Element of ( NAT ));
let R7 being Real;
let R19 being (Real_Sequence of R1);
let R20 being (Real_Sequence of R1);
let R2 being (Element of ( NAT ));
thus L75: ( ( R7 * ( R19 + R20 ) ) . R2 ) = ( R7 * ( ( R19 + R20 ) . R2 ) ) by L59
.= ( R7 * ( ( R19 . R2 ) + ( R20 . R2 ) ) ) by L56
.= ( ( R7 * ( R19 . R2 ) ) + ( R7 * ( R20 . R2 ) ) ) by EUCLID:32
.= ( ( ( R7 * R19 ) . R2 ) + ( R7 * ( R20 . R2 ) ) ) by L59
.= ( ( ( R7 * R19 ) . R2 ) + ( ( R7 * R20 ) . R2 ) ) by L59
.= ( ( ( R7 * R19 ) + ( R7 * R20 ) ) . R2 ) by L56;
end;
theorem
L76: (for R1 being (Element of ( NAT )) holds (for R6 being Real holds (for R7 being Real holds (for R18 being (Real_Sequence of R1) holds ( ( R7 * R6 ) * R18 ) = ( R7 * ( R6 * R18 ) )))))
proof
let R1 being (Element of ( NAT ));
let R6 being Real;
let R7 being Real;
let R18 being (Real_Sequence of R1);
let R2 being (Element of ( NAT ));
thus L77: ( ( ( R7 * R6 ) * R18 ) . R2 ) = ( ( R7 * R6 ) * ( R18 . R2 ) ) by L59
.= ( R7 * ( R6 * ( R18 . R2 ) ) ) by EUCLID:30
.= ( R7 * ( ( R6 * R18 ) . R2 ) ) by L59
.= ( ( R7 * ( R6 * R18 ) ) . R2 ) by L59;
end;
theorem
L78: (for R1 being (Element of ( NAT )) holds (for R7 being Real holds (for R19 being (Real_Sequence of R1) holds (for R20 being (Real_Sequence of R1) holds ( R7 * ( R19 - R20 ) ) = ( ( R7 * R19 ) - ( R7 * R20 ) )))))
proof
let R1 being (Element of ( NAT ));
let R7 being Real;
let R19 being (Real_Sequence of R1);
let R20 being (Real_Sequence of R1);
thus L79: ( R7 * ( R19 - R20 ) ) = ( ( R7 * R19 ) + ( R7 * ( - R20 ) ) ) by L74
.= ( ( R7 * R19 ) + ( R7 * ( ( - 1 ) * R20 ) ) ) by L72
.= ( ( R7 * R19 ) + ( ( ( - 1 ) * R7 ) * R20 ) ) by L76
.= ( ( R7 * R19 ) + ( ( - 1 ) * ( R7 * R20 ) ) ) by L76
.= ( ( R7 * R19 ) - ( R7 * R20 ) ) by L72;
end;
theorem
L80: (for R1 being (Element of ( NAT )) holds (for R19 being (Real_Sequence of R1) holds (for R20 being (Real_Sequence of R1) holds (for R21 being (Real_Sequence of R1) holds ( R19 - ( R20 + R21 ) ) = ( ( R19 - R20 ) - R21 )))))
proof
let R1 being (Element of ( NAT ));
let R19 being (Real_Sequence of R1);
let R20 being (Real_Sequence of R1);
let R21 being (Real_Sequence of R1);
thus L81: ( R19 - ( R20 + R21 ) ) = ( R19 + ( ( - 1 ) * ( R20 + R21 ) ) ) by L72
.= ( R19 + ( ( ( - 1 ) * R20 ) + ( ( - 1 ) * R21 ) ) ) by L74
.= ( R19 + ( ( - R20 ) + ( ( - 1 ) * R21 ) ) ) by L72
.= ( R19 + ( ( - R20 ) + ( - R21 ) ) ) by L72
.= ( ( R19 - R20 ) - R21 ) by L70;
end;
theorem
L82: (for R1 being (Element of ( NAT )) holds (for R18 being (Real_Sequence of R1) holds ( 1 * R18 ) = R18))
proof
let R1 being (Element of ( NAT ));
let R18 being (Real_Sequence of R1);
let R2 being (Element of ( NAT ));
thus L83: ( ( 1 * R18 ) . R2 ) = ( 1 * ( R18 . R2 ) ) by L59
.= ( R18 . R2 ) by EUCLID:29;
end;
theorem
L84: (for R1 being (Element of ( NAT )) holds (for R18 being (Real_Sequence of R1) holds ( - ( - R18 ) ) = R18))
proof
let R1 being (Element of ( NAT ));
let R18 being (Real_Sequence of R1);
thus L85: ( - ( - R18 ) ) = ( ( - 1 ) * ( - R18 ) ) by L72
.= ( ( - 1 ) * ( ( - 1 ) * R18 ) ) by L72
.= ( ( ( - 1 ) * ( - 1 ) ) * R18 ) by L76
.= R18 by L82;
end;
theorem
L86: (for R1 being (Element of ( NAT )) holds (for R19 being (Real_Sequence of R1) holds (for R20 being (Real_Sequence of R1) holds ( R19 - ( - R20 ) ) = ( R19 + R20 )))) by L84;
theorem
L87: (for R1 being (Element of ( NAT )) holds (for R19 being (Real_Sequence of R1) holds (for R20 being (Real_Sequence of R1) holds (for R21 being (Real_Sequence of R1) holds ( R19 - ( R20 - R21 ) ) = ( ( R19 - R20 ) + R21 )))))
proof
let R1 being (Element of ( NAT ));
let R19 being (Real_Sequence of R1);
let R20 being (Real_Sequence of R1);
let R21 being (Real_Sequence of R1);
thus L88: ( R19 - ( R20 - R21 ) ) = ( R19 + ( ( - 1 ) * ( R20 - R21 ) ) ) by L72
.= ( R19 + ( ( ( - 1 ) * R20 ) - ( ( - 1 ) * R21 ) ) ) by L78
.= ( R19 + ( ( - R20 ) - ( ( - 1 ) * R21 ) ) ) by L72
.= ( R19 + ( ( - R20 ) - ( - R21 ) ) ) by L72
.= ( R19 + ( ( - R20 ) + R21 ) ) by L84
.= ( ( R19 - R20 ) + R21 ) by L70;
end;
theorem
L89: (for R1 being (Element of ( NAT )) holds (for R19 being (Real_Sequence of R1) holds (for R20 being (Real_Sequence of R1) holds (for R21 being (Real_Sequence of R1) holds ( R19 + ( R20 - R21 ) ) = ( ( R19 + R20 ) - R21 ))))) by L70;
theorem
L90: (for R1 being (Element of ( NAT )) holds (for R7 being Real holds (for R18 being (Real_Sequence of R1) holds ((R7 <> ( 0 ) & R18 is  non-zero) implies ( R7 * R18 ) is  non-zero))))
proof
let R1 being (Element of ( NAT ));
let R7 being Real;
let R18 being (Real_Sequence of R1);
assume that
L91: R7 <> ( 0 )
and
L92: R18 is  non-zero
and
L93: (not ( R7 * R18 ) is  non-zero);
consider R4 being (Element of ( NAT )) such that L94: ( ( R7 * R18 ) . R4 ) = ( 0. ( TOP-REAL R1 ) ) by L93 , L31;
L95: ( R18 . R4 ) <> ( 0. ( TOP-REAL R1 ) ) by L92 , L31;
L96: ( R7 * ( R18 . R4 ) ) = ( 0. ( TOP-REAL R1 ) ) by L94 , L59;
thus L97: contradiction by L96 , L91 , L95 , EUCLID:31;
end;
theorem
L98: (for R1 being (Element of ( NAT )) holds (for R18 being (Real_Sequence of R1) holds (R18 is  non-zero implies ( - R18 ) is  non-zero)))
proof
let R1 being (Element of ( NAT ));
let R18 being (Real_Sequence of R1);
assume L99: R18 is  non-zero;
L100: ( ( - 1 ) * R18 ) is  non-zero by L99 , L90;
thus L101: thesis by L100 , L72;
end;
theorem
L102: (for R1 being (Element of ( NAT )) holds |. ( 0. ( TOP-REAL R1 ) ) .| = ( 0 ))
proof
let R1 being (Element of ( NAT ));
thus L103: |. ( 0. ( TOP-REAL R1 ) ) .| = |. ( 0* R1 ) .| by EUCLID:70
.= ( 0 ) by EUCLID:7;
end;
theorem
L104: (for R1 being (Element of ( NAT )) holds (for R12 being (Point of ( TOP-REAL R1 )) holds (|. R12 .| = ( 0 ) implies R12 = ( 0. ( TOP-REAL R1 ) ))))
proof
let R1 being (Element of ( NAT ));
let R12 being (Point of ( TOP-REAL R1 ));
reconsider D4 = R12 as (Element of ( REAL R1 )) by EUCLID:22;
assume L105: |. R12 .| = ( 0 );
L106: D4 = ( 0* R1 ) by L105 , EUCLID:8
.= ( 0. ( TOP-REAL R1 ) ) by EUCLID:70;
thus L107: thesis by L106;
end;
theorem
L108: (for R1 being (Element of ( NAT )) holds (for R12 being (Point of ( TOP-REAL R1 )) holds |. R12 .| >= ( 0 )));
theorem
L109: (for R1 being (Element of ( NAT )) holds (for R12 being (Point of ( TOP-REAL R1 )) holds |. ( - R12 ) .| = |. R12 .|)) by EUCLID:71;
theorem
L110: (for R1 being (Element of ( NAT )) holds (for R13 being (Point of ( TOP-REAL R1 )) holds (for R14 being (Point of ( TOP-REAL R1 )) holds |. ( R13 - R14 ) .| = |. ( R14 - R13 ) .|)))
proof
let R1 being (Element of ( NAT ));
let R13 being (Point of ( TOP-REAL R1 ));
let R14 being (Point of ( TOP-REAL R1 ));
thus L111: |. ( R13 - R14 ) .| = |. ( - ( R13 - R14 ) ) .| by EUCLID:71
.= |. ( R14 - R13 ) .| by EUCLID:44;
end;
L112: (for R1 being (Element of ( NAT )) holds (for R13 being (Point of ( TOP-REAL R1 )) holds (for R14 being (Point of ( TOP-REAL R1 )) holds (|. ( R13 - R14 ) .| = ( 0 ) implies R13 = R14))))
proof
let R1 being (Element of ( NAT ));
let R13 being (Point of ( TOP-REAL R1 ));
let R14 being (Point of ( TOP-REAL R1 ));
assume L113: |. ( R13 - R14 ) .| = ( 0 );
L114: ( R13 - R14 ) = ( 0. ( TOP-REAL R1 ) ) by L113 , L104;
thus L115: thesis by L114 , EUCLID:43;
end;
L116: (for R1 being (Element of ( NAT )) holds (for R13 being (Point of ( TOP-REAL R1 )) holds (for R14 being (Point of ( TOP-REAL R1 )) holds (R13 = R14 implies |. ( R13 - R14 ) .| = ( 0 )))))
proof
let R1 being (Element of ( NAT ));
let R13 being (Point of ( TOP-REAL R1 ));
let R14 being (Point of ( TOP-REAL R1 ));
assume L117: R13 = R14;
L118: |. ( R13 - R14 ) .| = |. ( 0. ( TOP-REAL R1 ) ) .| by L117 , EUCLID:42
.= ( 0 ) by L102;
thus L119: thesis by L118;
