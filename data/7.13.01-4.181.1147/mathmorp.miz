definition
let C1 being non  empty addMagma;
let C2 being (Element of C1);
let C3 being (Subset of C1);
func C3 + C2 -> (Subset of C1) equals 
{ ( B1 + C2 ) where B1 is (Element of C1) : B1 in C3 };
coherence
proof
L1:
now
let C4 being set;
assume L2: C4 in { ( B2 + C2 ) where B2 is (Point of C1) : B2 in C3 };
L3: (ex B3 being (Point of C1) st (C4 = ( B3 + C2 ) & B3 in C3)) by L2;
thus L4: C4 in (the carrier of C1) by L3;
end;
thus L5: thesis by L1 , TARSKI:def 3;
end;
end;
definition
let C5 being non  empty addLoopStr;
let C6 being (Subset of C5);
func C6 ! -> (Subset of C5) equals 
{ ( - B4 ) where B4 is (Point of C5) : B4 in C6 };
coherence
proof
L7:
now
let C7 being set;
assume L8: C7 in { ( - B5 ) where B5 is (Point of C5) : B5 in C6 };
L9: (ex B6 being (Point of C5) st (C7 = ( - B6 ) & B6 in C6)) by L8;
thus L10: C7 in (the carrier of C5) by L9;
end;
thus L11: thesis by L7 , TARSKI:def 3;
end;
end;
definition
let C8 being non  empty addMagma;
let C9 , C10 being (Subset of C8);
func C9 (-) C10 -> (Subset of C8) equals 
{ B7 where B7 is (Point of C8) : ( C10 + B7 ) c= C9 };
coherence
proof
L13:
now
let C11 being set;
assume L14: C11 in { B8 where B8 is (Point of C8) : ( C10 + B8 ) c= C9 };
L15: (ex B9 being (Point of C8) st (C11 = B9 & ( C10 + B9 ) c= C9)) by L14;
thus L16: C11 in (the carrier of C8) by L15;
end;
thus L17: thesis by L13 , TARSKI:def 3;
end;
end;
