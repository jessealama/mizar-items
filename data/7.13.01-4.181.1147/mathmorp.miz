theorem
L20: (for B10 being  add-associative  right_zeroed  right_complementable non  empty RLSStruct holds (for B11 being (Subset of B10) holds ( ( B11 ! ) ! ) = B11))
proof
let C15 being  add-associative  right_zeroed  right_complementable non  empty RLSStruct;
let C16 being (Subset of C15);
thus L21: ( ( C16 ! ) ! ) c= C16
proof
let C17 being set;
assume L22: C17 in ( ( C16 ! ) ! );
consider C18 being (Element of C15) such that L23: C17 = ( - C18 ) and L24: C18 in ( C16 ! ) by L22;
L25: (ex B12 being (Element of C15) st (C18 = ( - B12 ) & B12 in C16)) by L24;
thus L26: thesis by L25 , L23 , RLVECT_1:17;
end;

let C19 being set;
assume L27: C19 in C16;
reconsider D1 = C19 as (Point of C15) by L27;
L28: ( - D1 ) in { ( - B13 ) where B13 is (Point of C15) : B13 in C16 } by L27;
L29: ( - ( - D1 ) ) in { ( - B14 ) where B14 is (Point of C15) : B14 in ( C16 ! ) } by L28;
thus L30: thesis by L29 , RLVECT_1:17;
end;
theorem
L31: (for R1 being non  empty  Abelian  add-associative  right_zeroed  right_complementable RLSStruct holds (for R9 being (Point of R1) holds ( { ( 0. R1 ) } + R9 ) = { R9 }))
proof
let R1 being non  empty  Abelian  add-associative  right_zeroed  right_complementable RLSStruct;
let R9 being (Point of R1);
thus L32: ( { ( 0. R1 ) } + R9 ) c= { R9 }
proof
let C20 being set;
assume L33: C20 in ( { ( 0. R1 ) } + R9 );
consider C21 being (Point of R1) such that L34: C20 = ( C21 + R9 ) and L35: C21 in { ( 0. R1 ) } by L33;
L36: { C21 } c= { ( 0. R1 ) } by L35 , ZFMISC_1:31;
L37: C21 = ( 0. R1 ) by L36 , ZFMISC_1:18;
L38: { C20 } c= { R9 } by L37 , L34 , RLVECT_1:4;
thus L39: thesis by L38 , ZFMISC_1:31;
end;

let C22 being set;
assume L40: C22 in { R9 };
L41: { C22 } c= { R9 } by L40 , ZFMISC_1:31;
L42: C22 = R9 by L41 , ZFMISC_1:18;
L43: C22 = ( ( 0. R1 ) + R9 ) by L42 , RLVECT_1:4;
L44: ( 0. R1 ) in { ( 0. R1 ) } by ZFMISC_1:31;
thus L45: thesis by L44 , L43;
end;
theorem
L46: (for R1 being non  empty  Abelian  add-associative  right_zeroed  right_complementable RLSStruct holds (for R7 being (Subset of R1) holds (for R8 being (Subset of R1) holds (for R11 being (Point of R1) holds (R7 c= R8 implies ( R7 + R11 ) c= ( R8 + R11 ))))))
proof
let R1 being non  empty  Abelian  add-associative  right_zeroed  right_complementable RLSStruct;
let R7 being (Subset of R1);
let R8 being (Subset of R1);
let R11 being (Point of R1);
assume L47: R7 c= R8;
let C23 being set;
assume L48: C23 in ( R7 + R11 );
L49: (ex B15 being (Point of R1) st (C23 = ( B15 + R11 ) & B15 in R7)) by L48;
thus L50: thesis by L49 , L47;
end;
theorem
L51: (for R1 being non  empty  Abelian  add-associative  right_zeroed  right_complementable RLSStruct holds (for R9 being (Point of R1) holds (for R2 being (Subset of R1) holds (R2 = ( {} ) implies ( R2 + R9 ) = ( {} )))))
proof
let R1 being non  empty  Abelian  add-associative  right_zeroed  right_complementable RLSStruct;
let R9 being (Point of R1);
let R2 being (Subset of R1);
assume L52: R2 = ( {} );
L53:
now
given C24 being set such that
L54: C24 in ( R2 + R9 );

L55: (ex B16 being (Point of R1) st (C24 = ( B16 + R9 ) & B16 in R2)) by L54;
thus L56: contradiction by L55 , L52;
end;
thus L57: thesis by L53 , XBOOLE_0:def 1;
end;
theorem
L58: (for R1 being non  empty  Abelian  add-associative  right_zeroed  right_complementable RLSStruct holds (for R2 being (Subset of R1) holds ( R2 (-) { ( 0. R1 ) } ) = R2))
proof
let R1 being non  empty  Abelian  add-associative  right_zeroed  right_complementable RLSStruct;
let R2 being (Subset of R1);
thus L59: ( R2 (-) { ( 0. R1 ) } ) c= R2
proof
let C25 being set;
assume L60: C25 in ( R2 (-) { ( 0. R1 ) } );
L61: (ex B17 being (Point of R1) st (C25 = B17 & ( { ( 0. R1 ) } + B17 ) c= R2)) by L60;
L62: { C25 } c= R2 by L61 , L31;
thus L63: thesis by L62 , ZFMISC_1:31;
end;

let C26 being set;
assume L64: C26 in R2;
reconsider D2 = C26 as (Point of R1) by L64;
L65: { C26 } c= R2 by L64 , ZFMISC_1:31;
L66: ( { ( 0. R1 ) } + D2 ) c= R2 by L65 , L31;
thus L67: thesis by L66;
end;
L68: (for R1 being non  empty  Abelian  add-associative  right_zeroed  right_complementable RLSStruct holds (for R9 being (Point of R1) holds (for R10 being (Point of R1) holds ( { R9 } + R10 ) = ( { R10 } + R9 ))))
proof
let R1 being non  empty  Abelian  add-associative  right_zeroed  right_complementable RLSStruct;
let R9 being (Point of R1);
let R10 being (Point of R1);
thus L69: ( { R9 } + R10 ) c= ( { R10 } + R9 )
proof
let C27 being set;
assume L70: C27 in ( { R9 } + R10 );
consider C28 being (Point of R1) such that L71: C27 = ( C28 + R10 ) and L72: C28 in { R9 } by L70;
L73: { C28 } c= { R9 } by L72 , ZFMISC_1:31;
L74: C27 = ( R9 + R10 ) by L73 , L71 , ZFMISC_1:18;
L75: R10 in { R10 } by TARSKI:def 1;
thus L76: thesis by L75 , L74;
end;

let C29 being set;
assume L77: C29 in ( { R10 } + R9 );
consider C30 being (Point of R1) such that L78: C29 = ( C30 + R9 ) and L79: C30 in { R10 } by L77;
L80: { C30 } c= { R10 } by L79 , ZFMISC_1:31;
L81: C29 = ( R9 + R10 ) by L80 , L78 , ZFMISC_1:18;
L82: R9 in { R9 } by TARSKI:def 1;
thus L83: thesis by L82 , L81;
end;
theorem
L84: (for R1 being non  empty  Abelian  add-associative  right_zeroed  right_complementable RLSStruct holds (for R2 being (Subset of R1) holds ( R2 (+) { ( 0. R1 ) } ) = R2))
proof
let R1 being non  empty  Abelian  add-associative  right_zeroed  right_complementable RLSStruct;
let R2 being (Subset of R1);
thus L85: ( R2 (+) { ( 0. R1 ) } ) c= R2
proof
let C31 being set;
assume L86: C31 in ( R2 (+) { ( 0. R1 ) } );
consider C32 , C33 being (Point of R1) such that L87: (C31 = ( C32 + C33 ) & C32 in R2) and L88: C33 in { ( 0. R1 ) } by L86;
L89: { C33 } c= { ( 0. R1 ) } by L88 , ZFMISC_1:31;
L90: C33 = ( 0. R1 ) by L89 , ZFMISC_1:18;
thus L91: thesis by L90 , L87 , RLVECT_1:4;
end;

let C34 being set;
assume L92: C34 in R2;
reconsider D3 = C34 as (Point of R1) by L92;
L93: ( 0. R1 ) in { ( 0. R1 ) } by TARSKI:def 1;
L94: ( D3 + ( 0. R1 ) ) in { ( B18 + B19 ) where B18 , B19 is (Point of R1) : (B18 in R2 & B19 in { ( 0. R1 ) }) } by L93 , L92;
thus L95: thesis by L94 , RLVECT_1:4;
end;
theorem
L96: (for R1 being non  empty  Abelian  add-associative  right_zeroed  right_complementable RLSStruct holds (for R2 being (Subset of R1) holds (for R9 being (Point of R1) holds ( R2 (+) { R9 } ) = ( R2 + R9 ))))
proof
let R1 being non  empty  Abelian  add-associative  right_zeroed  right_complementable RLSStruct;
let R2 being (Subset of R1);
let R9 being (Point of R1);
thus L97: ( R2 (+) { R9 } ) c= ( R2 + R9 )
proof
let C35 being set;
assume L98: C35 in ( R2 (+) { R9 } );
consider C36 , C37 being (Point of R1) such that L99: C35 = ( C36 + C37 ) and L100: C36 in R2 and L101: C37 in { R9 } by L98;
L102: { C37 } c= { R9 } by L101 , ZFMISC_1:31;
L103: C35 = ( C36 + R9 ) by L102 , L99 , ZFMISC_1:18;
thus L104: thesis by L103 , L100;
end;

let C38 being set;
assume L105: C38 in ( R2 + R9 );
L106: (ex B20 being (Point of R1) st (C38 = ( B20 + R9 ) & B20 in R2)) by L105;
L107: R9 in { R9 } by TARSKI:def 1;
thus L108: thesis by L107 , L106;
end;
theorem
L109: (for R1 being non  empty  Abelian  add-associative  right_zeroed  right_complementable RLSStruct holds (for R2 being (Subset of R1) holds (for R3 being (Subset of R1) holds (R3 = ( {} ) implies ( R2 (-) R3 ) = (the carrier of R1)))))
proof
let R1 being non  empty  Abelian  add-associative  right_zeroed  right_complementable RLSStruct;
let R2 being (Subset of R1);
let R3 being (Subset of R1);
assume L110: R3 = ( {} );
L111: { B21 where B21 is (Point of R1) : ( R3 + B21 ) c= R2 } = (the carrier of R1)
proof
thus L112: { B22 where B22 is (Point of R1) : ( R3 + B22 ) c= R2 } c= (the carrier of R1)
proof
let C39 being set;
assume L113: C39 in { B23 where B23 is (Point of R1) : ( R3 + B23 ) c= R2 };
L114: (ex B24 being (Point of R1) st (C39 = B24 & ( R3 + B24 ) c= R2)) by L113;
thus L115: thesis by L114;
end;

let C40 being set;
assume L116: C40 in (the carrier of R1);
reconsider D4 = C40 as (Point of R1) by L116;
L117: ( R3 + D4 ) = ( {} ) by L110 , L51;
L118: ( R3 + D4 ) c= R2 by L117 , XBOOLE_1:2;
thus L119: thesis by L118;
end;
thus L120: thesis by L111;
end;
theorem
L121: (for R1 being non  empty  Abelian  add-associative  right_zeroed  right_complementable RLSStruct holds (for R2 being (Subset of R1) holds (for R3 being (Subset of R1) holds (for R5 being (Subset of R1) holds (R2 c= R3 implies (( R2 (-) R5 ) c= ( R3 (-) R5 ) & ( R2 (+) R5 ) c= ( R3 (+) R5 )))))))
proof
let R1 being non  empty  Abelian  add-associative  right_zeroed  right_complementable RLSStruct;
let R2 being (Subset of R1);
let R3 being (Subset of R1);
let R5 being (Subset of R1);
assume L122: R2 c= R3;
thus L123: ( R2 (-) R5 ) c= ( R3 (-) R5 )
proof
let C41 being set;
assume L124: C41 in ( R2 (-) R5 );
consider C42 being (Point of R1) such that L125: C41 = C42 and L126: ( R5 + C42 ) c= R2 by L124;
L127: ( R5 + C42 ) c= R3 by L122 , L126 , XBOOLE_1:1;
thus L128: thesis by L127 , L125;
end;

let C43 being set;
assume L129: C43 in ( R2 (+) R5 );
L130: (ex B25 , B26 being (Point of R1) st (C43 = ( B25 + B26 ) & B25 in R2 & B26 in R5)) by L129;
thus L131: thesis by L130 , L122;
end;
theorem
L132: (for R1 being non  empty  Abelian  add-associative  right_zeroed  right_complementable RLSStruct holds (for R2 being (Subset of R1) holds (for R7 being (Subset of R1) holds (for R8 being (Subset of R1) holds (R7 c= R8 implies (( R2 (-) R8 ) c= ( R2 (-) R7 ) & ( R2 (+) R7 ) c= ( R2 (+) R8 )))))))
proof
let R1 being non  empty  Abelian  add-associative  right_zeroed  right_complementable RLSStruct;
let R2 being (Subset of R1);
let R7 being (Subset of R1);
let R8 being (Subset of R1);
assume L133: R7 c= R8;
thus L134: ( R2 (-) R8 ) c= ( R2 (-) R7 )
proof
let C44 being set;
assume L135: C44 in ( R2 (-) R8 );
consider C45 being (Point of R1) such that L136: C44 = C45 and L137: ( R8 + C45 ) c= R2 by L135;
L138: ( R7 + C45 ) c= ( R8 + C45 ) by L133 , L46;
L139: ( R7 + C45 ) c= R2 by L138 , L137 , XBOOLE_1:1;
thus L140: thesis by L139 , L136;
end;

let C46 being set;
assume L141: C46 in ( R2 (+) R7 );
L142: (ex B27 , B28 being (Point of R1) st (C46 = ( B27 + B28 ) & B27 in R2 & B28 in R7)) by L141;
thus L143: thesis by L142 , L133;
end;
theorem
L144: (for R1 being non  empty  Abelian  add-associative  right_zeroed  right_complementable RLSStruct holds (for R2 being (Subset of R1) holds (for R5 being (Subset of R1) holds (( 0. R1 ) in R5 implies (( R2 (-) R5 ) c= R2 & R2 c= ( R2 (+) R5 ))))))
proof
let R1 being non  empty  Abelian  add-associative  right_zeroed  right_complementable RLSStruct;
let R2 being (Subset of R1);
let R5 being (Subset of R1);
assume L145: ( 0. R1 ) in R5;
thus L146: ( R2 (-) R5 ) c= R2
proof
let C47 being set;
assume L147: C47 in ( R2 (-) R5 );
consider C48 being (Point of R1) such that L148: C47 = C48 and L149: ( R5 + C48 ) c= R2 by L147;
L150: ( ( 0. R1 ) + C48 ) in { ( B29 + C48 ) where B29 is (Point of R1) : B29 in R5 } by L145;
L151: ( ( 0. R1 ) + C48 ) in R2 by L150 , L149;
thus L152: thesis by L151 , L148 , RLVECT_1:4;
end;

let C49 being set;
assume L153: C49 in R2;
reconsider D5 = C49 as (Point of R1) by L153;
L154: ( D5 + ( 0. R1 ) ) in { ( B30 + B31 ) where B30 , B31 is (Point of R1) : (B30 in R2 & B31 in R5) } by L145 , L153;
thus L155: thesis by L154 , RLVECT_1:4;
end;
theorem
L156: (for R1 being non  empty  Abelian  add-associative  right_zeroed  right_complementable RLSStruct holds (for R2 being (Subset of R1) holds (for R3 being (Subset of R1) holds ( R2 (+) R3 ) = ( R3 (+) R2 ))))
proof
let R1 being non  empty  Abelian  add-associative  right_zeroed  right_complementable RLSStruct;
let R2 being (Subset of R1);
let R3 being (Subset of R1);
thus L157: ( R2 (+) R3 ) c= ( R3 (+) R2 )
proof
let C50 being set;
assume L158: C50 in ( R2 (+) R3 );
L159: (ex B32 , B33 being (Point of R1) st (C50 = ( B32 + B33 ) & B32 in R2 & B33 in R3)) by L158;
thus L160: thesis by L159;
end;

let C51 being set;
assume L161: C51 in ( R3 (+) R2 );
L162: (ex B34 , B35 being (Point of R1) st (C51 = ( B34 + B35 ) & B34 in R3 & B35 in R2)) by L161;
thus L163: thesis by L162;
end;
L164: (for R1 being non  empty  Abelian  add-associative  right_zeroed  right_complementable RLSStruct holds (for R9 being (Point of R1) holds (for R11 being (Point of R1) holds (( ( R11 - R9 ) + R9 ) = R11 & ( ( R11 + R9 ) - R9 ) = R11))))
proof
let R1 being non  empty  Abelian  add-associative  right_zeroed  right_complementable RLSStruct;
let R9 being (Point of R1);
let R11 being (Point of R1);
L165: ( ( R11 + R9 ) - R9 ) = ( R11 + ( R9 - R9 ) ) by RLVECT_1:def 3
.= ( R11 + ( 0. R1 ) ) by RLVECT_1:15
.= R11 by RLVECT_1:4;
thus L166: thesis by L165 , RLVECT_1:28;
end;
theorem
L167: (for R1 being non  empty  Abelian  add-associative  right_zeroed  right_complementable RLSStruct holds (for R2 being (Subset of R1) holds (for R3 being (Subset of R1) holds (for R9 being (Point of R1) holds (for R10 being (Point of R1) holds (( R3 + R10 ) c= ( R2 + R9 ) iff ( R3 + ( R10 - R9 ) ) c= R2))))))
proof
let R1 being non  empty  Abelian  add-associative  right_zeroed  right_complementable RLSStruct;
let R2 being (Subset of R1);
let R3 being (Subset of R1);
let R9 being (Point of R1);
let R10 being (Point of R1);
thus L168: (( R3 + R10 ) c= ( R2 + R9 ) implies ( R3 + ( R10 - R9 ) ) c= R2)
proof
assume L169: ( R3 + R10 ) c= ( R2 + R9 );
let C52 being set;
assume L170: C52 in ( R3 + ( R10 - R9 ) );
consider C53 being (Point of R1) such that L171: C52 = ( C53 + ( R10 - R9 ) ) and L172: C53 in R3 by L170;
reconsider D6 = C52 as (Point of R1) by L171;
L173: D6 = ( ( C53 + R10 ) - R9 ) by L171 , RLVECT_1:28;
L174: ( D6 + R9 ) = ( C53 + R10 ) by L173 , L164;
L175: ( C53 + R10 ) in { ( B36 + R10 ) where B36 is (Point of R1) : B36 in R3 } by L172;
L176: ( D6 + R9 ) in ( R2 + R9 ) by L175 , L169 , L174;
consider C54 being (Point of R1) such that L177: ( D6 + R9 ) = ( C54 + R9 ) and L178: C54 in R2 by L176;
L179: ( ( D6 + R9 ) - R9 ) = C54 by L177 , L164;
thus L180: thesis by L179 , L178 , L164;
end;

assume L181: ( R3 + ( R10 - R9 ) ) c= R2;
let C55 being set;
assume L182: C55 in ( R3 + R10 );
consider C56 being (Point of R1) such that L183: C55 = ( C56 + R10 ) and L184: C56 in R3 by L182;
reconsider D7 = C55 as (Point of R1) by L183;
L185: (( D7 - R9 ) = ( C56 + ( R10 - R9 ) ) & ( C56 + ( R10 - R9 ) ) in { ( B37 + ( R10 - R9 ) ) where B37 is (Point of R1) : B37 in R3 }) by L183 , L184 , RLVECT_1:28;
L186: ( ( D7 - R9 ) + R9 ) in { ( B38 + R9 ) where B38 is (Point of R1) : B38 in R2 } by L185 , L181;
thus L187: thesis by L186 , L164;
end;
theorem
L188: (for R1 being non  empty  Abelian  add-associative  right_zeroed  right_complementable RLSStruct holds (for R2 being (Subset of R1) holds (for R3 being (Subset of R1) holds (for R11 being (Point of R1) holds ( ( R2 + R11 ) (-) R3 ) = ( ( R2 (-) R3 ) + R11 )))))
proof
let R1 being non  empty  Abelian  add-associative  right_zeroed  right_complementable RLSStruct;
let R2 being (Subset of R1);
let R3 being (Subset of R1);
let R11 being (Point of R1);
thus L189: ( ( R2 + R11 ) (-) R3 ) c= ( ( R2 (-) R3 ) + R11 )
proof
