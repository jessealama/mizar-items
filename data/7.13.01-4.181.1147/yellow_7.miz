L169: (C94 = ( "/\" (C95 , ( C87 opp )) ) &  ex_inf_of C95 , ( C87 opp )) by L168 , L69 , L98;
thus L170: thesis by L169 , L155;
end;

let C96 being set;
assume L171: C96 in ( fininfs C89 );
