theorem
L69: (for B22 being RelStr holds (for B23 being set holds ( ex_sup_of B23 , B22 iff  ex_inf_of B23 , ( B22 opp ))))
proof
let C46 being RelStr;
let C47 being set;
thus L70:now
assume L71:  ex_sup_of C47 , C46;
consider C48 being (Element of C46) such that L72: C47 is_<=_than C48 and L73: (for B24 being (Element of C46) holds (C47 is_<=_than B24 implies B24 >= C48)) and L74: (for B25 being (Element of C46) holds ((C47 is_<=_than B25 & (for B26 being (Element of C46) holds (C47 is_<=_than B26 implies B26 >= B25))) implies B25 = C48)) by L71 , YELLOW_0:def 7;
thus L75:  ex_inf_of C47 , ( C46 opp )
proof
take ( C48 ~ );
thus L76: C47 is_>=_than ( C48 ~ ) by L72 , L55;
thus L77:now
let C49 being (Element of ( C46 opp ));
assume L78: C47 is_>=_than C49;
L79: C47 is_<=_than ( ~ C49 ) by L78 , L66;
L80: ( ~ C49 ) >= C48 by L79 , L73;
thus L81: C49 <= ( C48 ~ ) by L80 , L5;
end;
let C50 being (Element of ( C46 opp ));
assume L82: C47 is_>=_than C50;
L83: C47 is_<=_than ( ~ C50 ) by L82 , L66;
assume L84: (for B27 being (Element of ( C46 opp )) holds (C47 is_>=_than B27 implies B27 <= C50));
L85:
now
let C51 being (Element of C46);
assume L86: C47 is_<=_than C51;
L87: C47 is_>=_than ( C51 ~ ) by L86 , L55;
L88: ( C51 ~ ) <= C50 by L87 , L84;
thus L89: C51 >= ( ~ C50 ) by L88 , L5;
end;
thus L90: thesis by L85 , L74 , L83;
end;

end;
assume L76:  ex_inf_of C47 , ( C46 opp );
consider C52 being (Element of ( C46 opp )) such that L77: C47 is_>=_than C52 and L78: (for B28 being (Element of ( C46 opp )) holds (C47 is_>=_than B28 implies B28 <= C52)) and L79: (for B29 being (Element of ( C46 opp )) holds ((C47 is_>=_than B29 & (for B30 being (Element of ( C46 opp )) holds (C47 is_>=_than B30 implies B30 <= B29))) implies B29 = C52)) by L76 , YELLOW_0:def 8;
take ( ~ C52 );
thus L80: C47 is_<=_than ( ~ C52 ) by L77 , L66;
thus L81:now
let C53 being (Element of C46);
assume L82: C47 is_<=_than C53;
L83: C47 is_>=_than ( C53 ~ ) by L82 , L55;
L84: ( C53 ~ ) <= C52 by L83 , L78;
thus L85: C53 >= ( ~ C52 ) by L84 , L5;
end;
let C54 being (Element of C46);
assume L86: C47 is_<=_than C54;
L87: C47 is_>=_than ( C54 ~ ) by L86 , L55;
assume L88: (for B31 being (Element of C46) holds (C47 is_<=_than B31 implies B31 >= C54));
L89:
now
let C55 being (Element of ( C46 opp ));
assume L90: C47 is_>=_than C55;
L91: C47 is_<=_than ( ~ C55 ) by L90 , L66;
L92: ( ~ C55 ) >= C54 by L91 , L88;
thus L93: C55 <= ( C54 ~ ) by L92 , L5;
end;
thus L94: thesis by L89 , L79 , L87;
end;
