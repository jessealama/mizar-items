theorem
L122: (for B38 , B39 being RelStr holds ((( the RelStr of B38 ) = ( the RelStr of B39 ) & B38 is  with_infima) implies B39 is  with_infima))
proof
let C64 , C65 being RelStr;
assume that
L123: ( the RelStr of C64 ) = ( the RelStr of C65 )
and
L124: (for B40 , B41 being (Element of C64) holds (ex B42 being (Element of C64) st (B42 <= B40 & B42 <= B41 & (for B43 being (Element of C64) holds ((B43 <= B40 & B43 <= B41) implies B43 <= B42)))));
let C66 , C67 being (Element of C65);
reconsider D1 = C66 , D2 = C67 as (Element of C64) by L123;
consider C68 being (Element of C64) such that L125: (C68 <= D1 & C68 <= D2) and L126: (for B44 being (Element of C64) holds ((B44 <= D1 & B44 <= D2) implies B44 <= C68)) by L124;
reconsider D3 = C68 as (Element of C65) by L123;
take D3;
thus L127: (D3 <= C66 & D3 <= C67) by L123 , L125 , YELLOW_0:1;
let C69 being (Element of C65);
reconsider D4 = C69 as (Element of C64) by L123;
assume L128: (C69 <= C66 & C69 <= C67);
L129: (D4 <= D1 & D4 <= D2) by L128 , L123 , YELLOW_0:1;
L130: D4 <= C68 by L129 , L126;
thus L131: thesis by L130 , L123 , YELLOW_0:1;
end;
theorem
L132: (for B45 , B46 being RelStr holds ((( the RelStr of B45 ) = ( the RelStr of B46 ) & B45 is  with_suprema) implies B46 is  with_suprema))
proof
let C70 , C71 being RelStr;
assume that
L133: ( the RelStr of C70 ) = ( the RelStr of C71 )
and
L134: (for B47 , B48 being (Element of C70) holds (ex B49 being (Element of C70) st (B49 >= B47 & B49 >= B48 & (for B50 being (Element of C70) holds ((B50 >= B47 & B50 >= B48) implies B50 >= B49)))));
let C72 , C73 being (Element of C71);
reconsider D5 = C72 , D6 = C73 as (Element of C70) by L133;
consider C74 being (Element of C70) such that L135: (C74 >= D5 & C74 >= D6) and L136: (for B51 being (Element of C70) holds ((B51 >= D5 & B51 >= D6) implies B51 >= C74)) by L134;
reconsider D7 = C74 as (Element of C71) by L133;
take D7;
thus L137: (D7 >= C72 & D7 >= C73) by L133 , L135 , YELLOW_0:1;
let C75 being (Element of C71);
reconsider D8 = C75 as (Element of C70) by L133;
assume L138: (C75 >= C72 & C75 >= C73);
L139: (D8 >= D5 & D8 >= D6) by L138 , L133 , YELLOW_0:1;
L140: D8 >= C74 by L139 , L136;
thus L141: thesis by L140 , L133 , YELLOW_0:1;
end;
