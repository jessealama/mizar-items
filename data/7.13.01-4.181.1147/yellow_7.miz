theorem
L2: (for B1 being RelStr holds (for B2 , B3 being (Element of ( B1 opp )) holds (B2 <= B3 iff ( ~ B2 ) >= ( ~ B3 ))))
proof
let C2 being RelStr;
let C3 , C4 being (Element of ( C2 opp ));
L3: (( ( ~ C3 ) ~ ) = ( ~ C3 ) & ( ( ~ C4 ) ~ ) = ( ~ C4 ));
thus L4: thesis by L3 , LATTICE3:9;
end;
theorem
L5: (for B4 being RelStr holds (for B5 being (Element of B4) holds (for B6 being (Element of ( B4 opp )) holds ((B5 <= ( ~ B6 ) iff ( B5 ~ ) >= B6) & (B5 >= ( ~ B6 ) iff ( B5 ~ ) <= B6)))))
proof
let C5 being RelStr;
let C6 being (Element of C5);
let C7 being (Element of ( C5 opp ));
L6: ( ~ ( C6 ~ ) ) = ( C6 ~ );
thus L7: thesis by L6 , L2;
end;
