L24: ( C19 + C20 ) = ( ( (the addF of C15) || (the carrier of C16) ) . [ C19 , C20 ] ) by L1;
thus L25: thesis by L24 , L23 , FUNCT_1:49;
end;
theorem
L26: (for B22 being RealUnitarySpace holds (for B23 being (Subspace of B22) holds (for B24 being (VECTOR of B22) holds (for B25 being (VECTOR of B23) holds (for B26 being Real holds (B25 = B24 implies ( B26 * B25 ) = ( B26 * B24 )))))))
proof
let C21 being RealUnitarySpace;
let C22 being (Subspace of C21);
let C23 being (VECTOR of C21);
let C24 being (VECTOR of C22);
let C25 being Real;
assume L27: C24 = C23;
L28: ( C25 * C24 ) = ( ( (the Mult of C21) | [: ( REAL ) , (the carrier of C22) :] ) . [ C25 , C24 ] ) by L1;
thus L29: thesis by L28 , L27 , FUNCT_1:49;
end;
theorem
L30: (for B27 being RealUnitarySpace holds (for B28 being (Subspace of B27) holds (for B29 , B30 being (VECTOR of B27) holds (for B31 , B32 being (VECTOR of B28) holds ((B31 = B29 & B32 = B30) implies ( B31 .|. B32 ) = ( B29 .|. B30 ))))))
proof
let C26 being RealUnitarySpace;
let C27 being (Subspace of C26);
let C28 , C29 being (VECTOR of C26);
let C30 , C31 being (VECTOR of C27);
reconsider D1 = C30 , D2 = C31 as (VECTOR of C26) by L15;
assume L31: (C30 = C28 & C31 = C29);
L32: ( C28 .|. C29 ) = ( (the scalar of C26) . [ D1 , D2 ] ) by L31 , BHSP_1:def 1;
L33: ( C30 .|. C31 ) = ( (the scalar of C27) . [ C30 , C31 ] ) by BHSP_1:def 1
.= ( ( (the scalar of C26) || (the carrier of C27) ) . [ C30 , C31 ] ) by L1;
thus L34: thesis by L33 , L32 , FUNCT_1:49;
end;
theorem
L35: (for B33 being RealUnitarySpace holds (for B34 being (Subspace of B33) holds (for B35 being (VECTOR of B33) holds (for B36 being (VECTOR of B34) holds (B36 = B35 implies ( - B35 ) = ( - B36 ))))))
proof
