begin
definition
let C1 being RealUnitarySpace;
mode Subspace of C1
 -> RealUnitarySpace
means :L1: ((the carrier of it) c= (the carrier of C1) & ( 0. it ) = ( 0. C1 ) & (the addF of it) = ( (the addF of C1) || (the carrier of it) ) & (the Mult of it) = ( (the Mult of C1) | [: ( REAL ) , (the carrier of it) :] ) & (the scalar of it) = ( (the scalar of C1) || (the carrier of it) ));
existence
proof
take C1;
L2: ( dom (the scalar of C1) ) = [: (the carrier of C1) , (the carrier of C1) :] by FUNCT_2:def 1;
L3: (( dom (the addF of C1) ) = [: (the carrier of C1) , (the carrier of C1) :] & ( dom (the Mult of C1) ) = [: ( REAL ) , (the carrier of C1) :]) by FUNCT_2:def 1;
thus L4: thesis by L3 , L2 , RELAT_1:69;
end;
end;
theorem
L6: (for B1 being RealUnitarySpace holds (for B2 , B3 being (Subspace of B1) holds (for B4 being set holds ((B4 in B2 & B2 is (Subspace of B3)) implies B4 in B3))))
proof
let C2 being RealUnitarySpace;
let C3 , C4 being (Subspace of C2);
let C5 being set;
assume L7: (C5 in C3 & C3 is (Subspace of C4));
L8: (C5 in (the carrier of C3) & (the carrier of C3) c= (the carrier of C4)) by L7 , L1 , STRUCT_0:def 5;
thus L9: thesis by L8 , STRUCT_0:def 5;
end;
theorem
L10: (for B5 being RealUnitarySpace holds (for B6 being (Subspace of B5) holds (for B7 being set holds (B7 in B6 implies B7 in B5))))
proof
