let C7 being (Subspace of C6);
let C8 being set;
assume L11: C8 in C7;
L12: C8 in (the carrier of C7) by L11 , STRUCT_0:def 5;
L13: (the carrier of C7) c= (the carrier of C6) by L1;
thus L14: thesis by L13 , L12 , STRUCT_0:def 5;
end;
theorem
L15: (for B8 being RealUnitarySpace holds (for B9 being (Subspace of B8) holds (for B10 being (VECTOR of B9) holds B10 is (VECTOR of B8))))
