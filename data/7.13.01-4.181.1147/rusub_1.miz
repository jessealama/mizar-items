let C33 being (Subspace of C32);
let C34 being (VECTOR of C32);
let C35 being (VECTOR of C33);
L36: (( - C34 ) = ( ( - 1 ) * C34 ) & ( - C35 ) = ( ( - 1 ) * C35 )) by RLVECT_1:16;
assume L37: C35 = C34;
thus L38: thesis by L37 , L36 , L26;
end;
theorem
L39: (for B37 being RealUnitarySpace holds (for B38 being (Subspace of B37) holds (for B39 , B40 being (VECTOR of B37) holds (for B41 , B42 being (VECTOR of B38) holds ((B41 = B40 & B42 = B39) implies ( B41 - B42 ) = ( B40 - B39 ))))))
proof
