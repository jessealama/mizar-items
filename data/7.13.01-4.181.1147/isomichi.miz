definition
let C5 being TopSpace;
let C6 being (Subset of C5);
redefine attr C6 is  condensed
means
:L24: (( Cl ( Int C6 ) ) = ( Cl C6 ) & ( Int ( Cl C6 ) ) = ( Int C6 ));
compatibility
proof
thus L25: (C6 is  condensed implies (( Cl ( Int C6 ) ) = ( Cl C6 ) & ( Int ( Cl C6 ) ) = ( Int C6 ))) by TDLAT_3:9;
assume that
L26: ( Cl ( Int C6 ) ) = ( Cl C6 )
and
L27: ( Int ( Cl C6 ) ) = ( Int C6 );
L28: ( Int C6 ) c= C6 by TOPS_1:16;
L29: C6 c= ( Cl ( Int C6 ) ) by L26 , PRE_TOPC:18;
thus L30: thesis by L29 , L27 , L28 , TOPS_1:def 6;
end;
end;
theorem
L32: (for R1 being TopSpace holds (for R2 being (Subset of R1) holds (R2 is  condensed iff (R2 is  subcondensed & R2 is  supercondensed))))
