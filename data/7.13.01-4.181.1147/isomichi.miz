end;
begin
definition
let C2 being TopStruct;
let C3 being (Subset of C2);
attr C3 is  supercondensed
means
:L12: ( Int ( Cl C3 ) ) = ( Int C3 );
attr C3 is  subcondensed
means
:L13: ( Cl ( Int C3 ) ) = ( Cl C3 );
