begin
registration
let C1 being non  trivial set;
cluster ( ADTS C1 ) -> non  trivial;
coherence
proof
L1: ( ADTS C1 ) = TopStruct (# C1 , ( cobool C1 ) #) by TEX_1:def 3;
thus L2: thesis by L1;
end;
end;
registration
cluster  anti-discrete non  trivial  strict for TopSpace;
existence
proof
set D1 = the non  trivial set;
take ( ADTS D1 );
thus L4: thesis;
end;
end;
theorem
L6: (for R1 being TopSpace holds (for R2 being (Subset of R1) holds (for R3 being (Subset of R1) holds ( ( Int ( Cl ( Int R2 ) ) ) /\ ( Int ( Cl ( Int R3 ) ) ) ) = ( Int ( Cl ( Int ( R2 /\ R3 ) ) ) ))))
proof
let R1 being TopSpace;
let R2 being (Subset of R1);
let R3 being (Subset of R1);
set D2 = ( Int R2 );
set D3 = ( Int R3 );
L7: ( ( Int ( Cl D2 ) ) /\ ( Int ( Cl D3 ) ) ) = ( Int ( Cl ( D2 /\ D3 ) ) ) by TDLAT_1:7;
thus L8: thesis by L7 , TOPS_1:17;
end;
theorem
L9: (for R1 being TopSpace holds (for R2 being (Subset of R1) holds (for R3 being (Subset of R1) holds ( Cl ( Int ( Cl ( R2 \/ R3 ) ) ) ) = ( ( Cl ( Int ( Cl R2 ) ) ) \/ ( Cl ( Int ( Cl R3 ) ) ) ))))
proof
let R1 being TopSpace;
let R2 being (Subset of R1);
let R3 being (Subset of R1);
set D4 = ( Cl R2 );
set D5 = ( Cl R3 );
L10: ( ( Cl ( Int D4 ) ) \/ ( Cl ( Int D5 ) ) ) = ( Cl ( Int ( D4 \/ D5 ) ) ) by TDLAT_1:6;
