begin
definition
let C1 being RealUnitarySpace;
let C2 being (Subset of C1);
func Lin C2 ->  strict (Subspace of C1) means 
:L1: (the carrier of it) = { ( Sum B1 ) where B1 is (Linear_Combination of C2) : (not contradiction) };
existence
proof
set D1 = { ( Sum B2 ) where B2 is (Linear_Combination of C2) : (not contradiction) };
L2: D1 c= (the carrier of C1)
proof
let C3 being set;
assume L3: C3 in D1;
L4: (ex B3 being (Linear_Combination of C2) st C3 = ( Sum B3 )) by L3;
thus L5: thesis by L4;
end;
reconsider D2 = D1 as (Subset of C1) by L2;
reconsider D3 = ( ZeroLC C1 ) as (Linear_Combination of C2) by RLVECT_2:22;
L6: D2 is  linearly-closed
proof
thus L7: (for B4 , B5 being (VECTOR of C1) holds ((B4 in D2 & B5 in D2) implies ( B4 + B5 ) in D2))
proof
let C4 , C5 being (VECTOR of C1);
assume that
L8: C4 in D2
and
L9: C5 in D2;
consider C6 being (Linear_Combination of C2) such that L10: C4 = ( Sum C6 ) by L8;
consider C7 being (Linear_Combination of C2) such that L11: C5 = ( Sum C7 ) by L9;
reconsider D4 = ( C6 + C7 ) as (Linear_Combination of C2) by RLVECT_2:38;
L12: ( C4 + C5 ) = ( Sum D4 ) by L10 , L11 , RLVECT_3:1;
thus L13: thesis by L12;
end;

let C8 being Real;
let C9 being (VECTOR of C1);
assume L14: C9 in D2;
consider C10 being (Linear_Combination of C2) such that L15: C9 = ( Sum C10 ) by L14;
reconsider D5 = ( C8 * C10 ) as (Linear_Combination of C2) by RLVECT_2:44;
L16: ( C8 * C9 ) = ( Sum D5 ) by L15 , RLVECT_3:2;
thus L17: thesis by L16;
end;
L18: ( Sum D3 ) = ( 0. C1 ) by RLVECT_2:30;
L19: ( 0. C1 ) in D2 by L18;
thus L20: thesis by L19 , L6 , RUSUB_1:29;
end;
uniqueness by RUSUB_1:24;
end;
theorem
L22: (for B6 being RealUnitarySpace holds (for B7 being (Subset of B6) holds (for B8 being set holds (B8 in ( Lin B7 ) iff (ex B9 being (Linear_Combination of B7) st B8 = ( Sum B9 ))))))
proof
let C11 being RealUnitarySpace;
let C12 being (Subset of C11);
let C13 being set;
thus L23: (C13 in ( Lin C12 ) implies (ex B10 being (Linear_Combination of C12) st C13 = ( Sum B10 )))
proof
assume L24: C13 in ( Lin C12 );
L25: C13 in (the carrier of ( Lin C12 )) by L24 , STRUCT_0:def 5;
L26: C13 in { ( Sum B11 ) where B11 is (Linear_Combination of C12) : (not contradiction) } by L25 , L1;
thus L27: thesis by L26;
end;

given C14 being (Linear_Combination of C12) such that
L28: C13 = ( Sum C14 );

L29: C13 in { ( Sum B12 ) where B12 is (Linear_Combination of C12) : (not contradiction) } by L28;
L30: C13 in (the carrier of ( Lin C12 )) by L29 , L1;
thus L31: thesis by L30 , STRUCT_0:def 5;
end;
theorem
L32: (for B13 being RealUnitarySpace holds (for B14 being (Subset of B13) holds (for B15 being set holds (B15 in B14 implies B15 in ( Lin B14 )))))
proof
deffunc H1(set) = ( 0 );
let C15 being RealUnitarySpace;
let C16 being (Subset of C15);
let C17 being set;
assume L33: C17 in C16;
reconsider D6 = C17 as (VECTOR of C15) by L33;
consider C18 being (Function of (the carrier of C15) , ( REAL )) such that L34: ( C18 . D6 ) = 1 and L35: (for B16 being (VECTOR of C15) holds (B16 <> D6 implies ( C18 . B16 ) = H1(B16))) from FUNCT_2:sch 6;
reconsider D7 = C18 as (Element of ( Funcs ((the carrier of C15) , ( REAL )) )) by FUNCT_2:8;
L36: (ex B17 being  finite (Subset of C15) st (for B18 being (VECTOR of C15) holds ((not B18 in B17) implies ( D7 . B18 ) = ( 0 ))))
proof
take D8 = { D6 };
let C19 being (VECTOR of C15);
assume L37: (not C19 in D8);
L38: C19 <> D6 by L37 , TARSKI:def 1;
thus L39: thesis by L38 , L35;
end;
reconsider D9 = D7 as (Linear_Combination of C15) by L36 , RLVECT_2:def 3;
L40: ( Carrier D9 ) c= { D6 }
proof
let C20 being set;
assume L41: C20 in ( Carrier D9 );
consider C21 being (VECTOR of C15) such that L42: C20 = C21 and L43: ( D9 . C21 ) <> ( 0 ) by L41;
L44: C21 = D6 by L35 , L43;
thus L45: thesis by L44 , L42 , TARSKI:def 1;
end;
reconsider D10 = D9 as (Linear_Combination of { D6 }) by L40 , RLVECT_2:def 6;
L46: ( Sum D10 ) = ( 1 * D6 ) by L34 , RLVECT_2:32
.= D6 by RLVECT_1:def 8;
L47: { D6 } c= C16 by L33 , ZFMISC_1:31;
L48: ( Carrier D10 ) c= C16 by L47 , L40 , XBOOLE_1:1;
reconsider D11 = D10 as (Linear_Combination of C16) by L48 , RLVECT_2:def 6;
L49: ( Sum D11 ) = D6 by L46;
thus L50: thesis by L49 , L22;
end;
L51: (for B19 being RealUnitarySpace holds (for B20 being set holds (B20 in ( (0). B19 ) iff B20 = ( 0. B19 ))))
proof
let C22 being RealUnitarySpace;
let C23 being set;
thus L52: (C23 in ( (0). C22 ) implies C23 = ( 0. C22 ))
proof
assume L53: C23 in ( (0). C22 );
L54: C23 in (the carrier of ( (0). C22 )) by L53 , STRUCT_0:def 5;
L55: C23 in { ( 0. C22 ) } by L54 , RUSUB_1:def 2;
thus L56: thesis by L55 , TARSKI:def 1;
end;

thus L57: thesis by RUSUB_1:11;
end;
theorem
L58: (for B21 being RealUnitarySpace holds ( Lin ( {} (the carrier of B21) ) ) = ( (0). B21 ))
proof
let C24 being RealUnitarySpace;
set D12 = ( Lin ( {} (the carrier of C24) ) );
L59:
now
let C25 being (VECTOR of C24);
thus L60: (C25 in D12 implies C25 in ( (0). C24 ))
proof
assume L61: C25 in D12;
L62: C25 in (the carrier of D12) by L61 , STRUCT_0:def 5;
L63: (the carrier of D12) = { ( Sum B22 ) where B22 is (Linear_Combination of ( {} (the carrier of C24) )) : (not contradiction) } by L1;
L64: (ex B23 being (Linear_Combination of ( {} (the carrier of C24) )) st C25 = ( Sum B23 )) by L63 , L62;
L65: C25 = ( 0. C24 ) by L64 , RLVECT_2:31;
thus L66: thesis by L65 , L51;
end;

assume L67: C25 in ( (0). C24 );
L68: C25 = ( 0. C24 ) by L67 , L51;
thus L69: C25 in D12 by L68 , RUSUB_1:11;
end;
thus L70: thesis by L59 , RUSUB_1:25;
end;
theorem
L71: (for B24 being RealUnitarySpace holds (for B25 being (Subset of B24) holds (( Lin B25 ) = ( (0). B24 ) implies (B25 = ( {} ) or B25 = { ( 0. B24 ) }))))
proof
let C26 being RealUnitarySpace;
let C27 being (Subset of C26);
assume that
L72: ( Lin C27 ) = ( (0). C26 )
and
L73: C27 <> ( {} );
thus L74: C27 c= { ( 0. C26 ) }
proof
let C28 being set;
assume L75: C28 in C27;
L76: C28 in ( Lin C27 ) by L75 , L32;
L77: C28 = ( 0. C26 ) by L76 , L72 , L51;
thus L78: thesis by L77 , TARSKI:def 1;
end;

set D13 = the (Element of C27);
let C29 being set;
assume L79: C29 in { ( 0. C26 ) };
L80: C29 = ( 0. C26 ) by L79 , TARSKI:def 1;
L81: (D13 in C27 & D13 in ( Lin C27 )) by L73 , L32;
thus L82: thesis by L81 , L72 , L80 , L51;
end;
theorem
L83: (for B26 being RealUnitarySpace holds (for B27 being  strict (Subspace of B26) holds (for B28 being (Subset of B26) holds (B28 = (the carrier of B27) implies ( Lin B28 ) = B27))))
proof
let C30 being RealUnitarySpace;
let C31 being  strict (Subspace of C30);
let C32 being (Subset of C30);
assume L84: C32 = (the carrier of C31);
L85:
now
let C33 being (VECTOR of C30);
thus L86: (C33 in ( Lin C32 ) implies C33 in C31)
proof
assume L87: C33 in ( Lin C32 );
L88: (ex B29 being (Linear_Combination of C32) st C33 = ( Sum B29 )) by L87 , L22;
L89: C32 is  linearly-closed by L84 , RUSUB_1:28;
L90: C33 in (the carrier of C31) by L89 , L84 , L88 , RLVECT_2:29;
thus L91: thesis by L90 , STRUCT_0:def 5;
end;

L92: (C33 in C31 iff C33 in (the carrier of C31)) by STRUCT_0:def 5;
thus L93: (C33 in C31 implies C33 in ( Lin C32 )) by L92 , L84 , L32;
end;
thus L94: thesis by L85 , RUSUB_1:25;
end;
theorem
L95: (for B30 being  strict RealUnitarySpace holds (for B31 being (Subset of B30) holds (B31 = (the carrier of B30) implies ( Lin B31 ) = B30)))
proof
let C34 being  strict RealUnitarySpace;
let C35 being (Subset of C34);
assume L96: C35 = (the carrier of C34);
L97: C35 = (the carrier of ( (Omega). C34 )) by L96 , RUSUB_1:def 3;
thus L98: ( Lin C35 ) = ( (Omega). C34 ) by L97 , L83
.= C34 by RUSUB_1:def 3;
end;
L99: (for B32 being RealUnitarySpace holds (for B33 , B34 , B35 being (Subspace of B32) holds (B33 is (Subspace of B35) implies ( B33 /\ B34 ) is (Subspace of B35))))
proof
let C36 being RealUnitarySpace;
let C37 , C38 , C39 being (Subspace of C36);
L100: ( C37 /\ C38 ) is (Subspace of C37) by RUSUB_2:16;
assume L101: C37 is (Subspace of C39);
thus L102: thesis by L101 , L100 , RUSUB_1:21;
end;
L103: (for B36 being RealUnitarySpace holds (for B37 , B38 , B39 being (Subspace of B36) holds ((B37 is (Subspace of B38) & B37 is (Subspace of B39)) implies B37 is (Subspace of ( B38 /\ B39 )))))
proof
let C40 being RealUnitarySpace;
let C41 , C42 , C43 being (Subspace of C40);
assume L104: (C41 is (Subspace of C42) & C41 is (Subspace of C43));
L105:
now
let C44 being (VECTOR of C40);
assume L106: C44 in C41;
L107: (C44 in C42 & C44 in C43) by L106 , L104 , RUSUB_1:1;
thus L108: C44 in ( C42 /\ C43 ) by L107 , RUSUB_2:3;
end;
thus L109: thesis by L105 , RUSUB_1:23;
end;
L110: (for B40 being RealUnitarySpace holds (for B41 , B42 , B43 being (Subspace of B40) holds (B41 is (Subspace of B42) implies B41 is (Subspace of ( B42 + B43 )))))
proof
let C45 being RealUnitarySpace;
let C46 , C47 , C48 being (Subspace of C45);
L111: C47 is (Subspace of ( C47 + C48 )) by RUSUB_2:7;
assume L112: C46 is (Subspace of C47);
thus L113: thesis by L112 , L111 , RUSUB_1:21;
end;
L114: (for B44 being RealUnitarySpace holds (for B45 , B46 , B47 being (Subspace of B44) holds ((B45 is (Subspace of B47) & B46 is (Subspace of B47)) implies ( B45 + B46 ) is (Subspace of B47))))
proof
let C49 being RealUnitarySpace;
let C50 , C51 , C52 being (Subspace of C49);
assume L115: (C50 is (Subspace of C52) & C51 is (Subspace of C52));
L116:
now
let C53 being (VECTOR of C49);
assume L117: C53 in ( C50 + C51 );
consider C54 , C55 being (VECTOR of C49) such that L118: (C54 in C50 & C55 in C51) and L119: C53 = ( C54 + C55 ) by L117 , RUSUB_2:1;
L120: (C54 in C52 & C55 in C52) by L115 , L118 , RUSUB_1:1;
thus L121: C53 in C52 by L120 , L119 , RUSUB_1:14;
end;
thus L122: thesis by L116 , RUSUB_1:23;
end;
theorem
L123: (for B48 being RealUnitarySpace holds (for B49 , B50 being (Subset of B48) holds (B49 c= B50 implies ( Lin B49 ) is (Subspace of ( Lin B50 )))))
proof
let C56 being RealUnitarySpace;
let C57 , C58 being (Subset of C56);
assume L124: C57 c= C58;
L125:
now
let C59 being (VECTOR of C56);
assume L126: C59 in ( Lin C57 );
consider C60 being (Linear_Combination of C57) such that L127: C59 = ( Sum C60 ) by L126 , L22;
reconsider D14 = C60 as (Linear_Combination of C58) by L124 , RLVECT_2:21;
L128: ( Sum D14 ) = C59 by L127;
thus L129: C59 in ( Lin C58 ) by L128 , L22;
end;
thus L130: thesis by L125 , RUSUB_1:23;
end;
theorem
L131: (for B51 being  strict RealUnitarySpace holds (for B52 , B53 being (Subset of B51) holds ((( Lin B52 ) = B51 & B52 c= B53) implies ( Lin B53 ) = B51)))
proof
let C61 being  strict RealUnitarySpace;
let C62 , C63 being (Subset of C61);
assume L132: (( Lin C62 ) = C61 & C62 c= C63);
L133: C61 is (Subspace of ( Lin C63 )) by L132 , L123;
thus L134: thesis by L133 , RUSUB_1:20;
end;
theorem
L135: (for B54 being RealUnitarySpace holds (for B55 , B56 being (Subset of B54) holds ( Lin ( B55 \/ B56 ) ) = ( ( Lin B55 ) + ( Lin B56 ) )))
proof
let C64 being RealUnitarySpace;
let C65 , C66 being (Subset of C64);
L136:
now
deffunc H2(set) = ( 0 );
let C67 being (VECTOR of C64);
assume L137: C67 in ( Lin ( C65 \/ C66 ) );
consider C68 being (Linear_Combination of ( C65 \/ C66 )) such that L138: C67 = ( Sum C68 ) by L137 , L22;
deffunc H3(set) = ( C68 . $1 );
set D15 = ( ( Carrier C68 ) \ C65 );
set D16 = ( ( Carrier C68 ) /\ C65 );
defpred S1[ set ] means $1 in D16;
defpred S2[ set ] means $1 in D15;
L139:
now
let C69 being set;
assume L140: C69 in (the carrier of C64);
reconsider D17 = C69 as (VECTOR of C64) by L140;
L141: (for B57 being (Function of (the carrier of C64) , ( REAL )) holds ( B57 . D17 ) in ( REAL ));
thus L142: (C69 in D16 implies ( C68 . C69 ) in ( REAL )) by L141;
assume L143: (not C69 in D16);
thus L144: ( 0 ) in ( REAL );
end;
L145: (for B58 being set holds (B58 in (the carrier of C64) implies ((S1[ B58 ] implies H3(B58) in ( REAL )) & ((not S1[ B58 ]) implies H2(B58) in ( REAL ))))) by L139;
consider C70 being (Function of (the carrier of C64) , ( REAL )) such that L146: (for B59 being set holds (B59 in (the carrier of C64) implies ((S1[ B59 ] implies ( C70 . B59 ) = H3(B59)) & ((not S1[ B59 ]) implies ( C70 . B59 ) = H2(B59))))) from FUNCT_2:sch 5(L145);
reconsider D18 = D16 as  finite (Subset of C64);
reconsider D19 = C70 as (Element of ( Funcs ((the carrier of C64) , ( REAL )) )) by FUNCT_2:8;
L147: (for B60 being (VECTOR of C64) holds ((not B60 in D18) implies ( D19 . B60 ) = ( 0 ))) by L146;
reconsider D20 = D19 as (Linear_Combination of C64) by L147 , RLVECT_2:def 3;
L148: ( Carrier D20 ) c= D18
proof
let C71 being set;
assume L149: C71 in ( Carrier D20 );
L150: (ex B61 being (VECTOR of C64) st (C71 = B61 & ( D20 . B61 ) <> ( 0 ))) by L149;
assume L151: (not C71 in D18);
thus L152: thesis by L151 , L146 , L150;
end;
L153: D18 c= C65 by XBOOLE_1:17;
L154: ( Carrier D20 ) c= C65 by L153 , L148 , XBOOLE_1:1;
reconsider D21 = D20 as (Linear_Combination of C65) by L154 , RLVECT_2:def 6;
L155:
now
let C72 being set;
assume L156: C72 in (the carrier of C64);
reconsider D22 = C72 as (VECTOR of C64) by L156;
L157: (for B62 being (Function of (the carrier of C64) , ( REAL )) holds ( B62 . D22 ) in ( REAL ));
thus L158: (C72 in D15 implies ( C68 . C72 ) in ( REAL )) by L157;
assume L159: (not C72 in D15);
thus L160: ( 0 ) in ( REAL );
end;
L161: (for B63 being set holds (B63 in (the carrier of C64) implies ((S2[ B63 ] implies H3(B63) in ( REAL )) & ((not S2[ B63 ]) implies H2(B63) in ( REAL ))))) by L155;
consider C73 being (Function of (the carrier of C64) , ( REAL )) such that L162: (for B64 being set holds (B64 in (the carrier of C64) implies ((S2[ B64 ] implies ( C73 . B64 ) = H3(B64)) & ((not S2[ B64 ]) implies ( C73 . B64 ) = H2(B64))))) from FUNCT_2:sch 5(L161);
reconsider D23 = D15 as  finite (Subset of C64);
reconsider D24 = C73 as (Element of ( Funcs ((the carrier of C64) , ( REAL )) )) by FUNCT_2:8;
L163: (for B65 being (VECTOR of C64) holds ((not B65 in D23) implies ( D24 . B65 ) = ( 0 ))) by L162;
reconsider D25 = D24 as (Linear_Combination of C64) by L163 , RLVECT_2:def 3;
L164: D23 c= C66
proof
let C74 being set;
assume L165: C74 in D23;
L166: (C74 in ( Carrier C68 ) & (not C74 in C65)) by L165 , XBOOLE_0:def 5;
L167: ( Carrier C68 ) c= ( C65 \/ C66 ) by RLVECT_2:def 6;
thus L168: thesis by L167 , L166 , XBOOLE_0:def 3;
end;
L169: ( Carrier D25 ) c= D23
proof
let C75 being set;
assume L170: C75 in ( Carrier D25 );
L171: (ex B66 being (VECTOR of C64) st (C75 = B66 & ( D25 . B66 ) <> ( 0 ))) by L170;
assume L172: (not C75 in D23);
thus L173: thesis by L172 , L162 , L171;
end;
L174: ( Carrier D25 ) c= C66 by L169 , L164 , XBOOLE_1:1;
reconsider D26 = D25 as (Linear_Combination of C66) by L174 , RLVECT_2:def 6;
L175: C68 = ( D21 + D26 )
proof
let C76 being (VECTOR of C64);
L176:
now
per cases ;
suppose L177: C76 in D18;

L178:
now
assume L179: C76 in D23;
L180: (not C76 in C65) by L179 , XBOOLE_0:def 5;
thus L181: contradiction by L180 , L177 , XBOOLE_0:def 4;
end;
thus L182: ( ( D21 + D26 ) . C76 ) = ( ( D21 . C76 ) + ( D26 . C76 ) ) by RLVECT_2:def 10
.= ( ( C68 . C76 ) + ( D26 . C76 ) ) by L146 , L177
.= ( ( C68 . C76 ) + ( 0 ) ) by L162 , L178
.= ( C68 . C76 );
end;
suppose L183: (not C76 in D18);

L184:
now
per cases ;
suppose L185: C76 in ( Carrier C68 );

L186:
now
assume L187: (not C76 in D23);
L188: ((not C76 in ( Carrier C68 )) or C76 in C65) by L187 , XBOOLE_0:def 5;
thus L189: contradiction by L188 , L183 , L185 , XBOOLE_0:def 4;
end;
thus L190: ( ( D21 + D26 ) . C76 ) = ( ( D21 . C76 ) + ( D26 . C76 ) ) by RLVECT_2:def 10
.= ( ( 0 ) + ( D26 . C76 ) ) by L146 , L183
.= ( C68 . C76 ) by L162 , L186;
end;
suppose L191: (not C76 in ( Carrier C68 ));

L192: (not C76 in D23) by L191 , XBOOLE_0:def 5;
L193: (not C76 in D18) by L191 , XBOOLE_0:def 4;
thus L194: ( ( D21 + D26 ) . C76 ) = ( ( D21 . C76 ) + ( D26 . C76 ) ) by RLVECT_2:def 10
.= ( ( 0 ) + ( D26 . C76 ) ) by L146 , L193
.= ( ( 0 ) + ( 0 ) ) by L162 , L192
.= ( C68 . C76 ) by L191;
end;
end;
thus L196: thesis by L184;
end;
end;
thus L198: thesis by L176;
end;
L199: C67 = ( ( Sum D21 ) + ( Sum D26 ) ) by L175 , L138 , RLVECT_3:1;
L200: (( Sum D21 ) in ( Lin C65 ) & ( Sum D26 ) in ( Lin C66 )) by L22;
