L49: ( dom ( Carrier ( C7 --> D6 ) ) ) = C7 by PARTFUN1:def 2;
L50: (for B6 being set holds (B6 in ( dom ( Carrier ( C7 --> D6 ) ) ) implies ( ( Carrier ( C7 --> D7 ) ) . B6 ) = ( ( Carrier ( C7 --> D6 ) ) . B6 )))
proof
let C9 being set;
assume L51: C9 in ( dom ( Carrier ( C7 --> D6 ) ) );
L52: (ex B7 being 1-sorted st (B7 = ( ( C7 --> D6 ) . C9 ) & ( ( Carrier ( C7 --> D6 ) ) . C9 ) = (the carrier of B7))) by L51 , PRALG_1:def 13;
L53: (ex B8 being 1-sorted st (B8 = ( ( C7 --> D7 ) . C9 ) & ( ( Carrier ( C7 --> D7 ) ) . C9 ) = (the carrier of B8))) by L51 , PRALG_1:def 13;
thus L54: ( ( Carrier ( C7 --> D7 ) ) . C9 ) = (the carrier of D7) by L53 , L51 , FUNCOP_1:7
.= ( bool 1 ) by WAYBEL_7:2
.= (the carrier of D6) by WAYBEL18:def 9 , YELLOW14:1
.= ( ( Carrier ( C7 --> D6 ) ) . C9 ) by L51 , L52 , FUNCOP_1:7;
end;
L55: ( the RelStr of C8 ) = ( the RelStr of ( product ( C7 --> D7 ) ) ) by YELLOW_9:def 4;
thus L56: (the carrier of C8) = ( product ( Carrier ( C7 --> D7 ) ) ) by L55 , YELLOW_1:def 4
.= ( product ( Carrier ( C7 --> D6 ) ) ) by L48 , L49 , L50 , FUNCT_1:2
.= (the carrier of ( product ( C7 --> D6 ) )) by WAYBEL18:def 3;
end;
theorem
L57: (for B9 , B10 being  complete LATTICE holds (for B11 being  Scott (TopAugmentation of B9) holds (for B12 being  Scott (TopAugmentation of B10) holds (for B13 being (Function of B9 , B10) holds (for B14 being (Function of B11 , B12) holds ((B13 = B14 & B13 is  isomorphic) implies B14 is  being_homeomorphism))))))
