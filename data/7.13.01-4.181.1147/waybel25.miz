theorem
L1: (for B1 being (Point of ( Sierpinski_Space )) holds (B1 = ( 0 ) implies { B1 } is  closed))
proof
set D1 = ( Sierpinski_Space );
let C1 being (Point of D1);
L2: (the carrier of D1) = { ( 0 ) , 1 } by WAYBEL18:def 9;
assume L3: C1 = ( 0 );
L4: ( ( [#] D1 ) \ { C1 } ) = { 1 }
proof
thus L5: ( ( [#] D1 ) \ { C1 } ) c= { 1 }
proof
let C2 being set;
assume L6: C2 in ( ( [#] D1 ) \ { C1 } );
L7: (not C2 in { C1 }) by L6 , XBOOLE_0:def 5;
L8: C2 <> ( 0 ) by L7 , L3 , TARSKI:def 1;
L9: C2 = 1 by L8 , L2 , L6 , TARSKI:def 2;
thus L10: thesis by L9 , TARSKI:def 1;
end;

let C3 being set;
assume L11: C3 in { 1 };
L12: C3 = 1 by L11 , TARSKI:def 1;
L13: (not C3 in { C1 }) by L12 , L3 , TARSKI:def 1;
L14: C3 in ( [#] D1 ) by L2 , L12 , TARSKI:def 2;
thus L15: thesis by L14 , L13 , XBOOLE_0:def 5;
end;
L16: (the topology of D1) = { ( {} ) , { 1 } , { ( 0 ) , 1 } } by WAYBEL18:def 9;
thus L17: ( ( [#] D1 ) \ { C1 } ) in (the topology of D1) by L16 , L4 , ENUMSET1:def 1;
end;
theorem
L18: (for B2 being (Point of ( Sierpinski_Space )) holds (B2 = 1 implies { B2 } is non  closed))
proof
set D2 = ( Sierpinski_Space );
let C4 being (Point of D2);
L19: (the carrier of D2) = { ( 0 ) , 1 } by WAYBEL18:def 9;
assume L20: C4 = 1;
L21: ( ( [#] D2 ) \ { C4 } ) = { ( 0 ) }
proof
thus L22: ( ( [#] D2 ) \ { C4 } ) c= { ( 0 ) }
proof
let C5 being set;
assume L23: C5 in ( ( [#] D2 ) \ { C4 } );
L24: (not C5 in { C4 }) by L23 , XBOOLE_0:def 5;
L25: C5 <> 1 by L24 , L20 , TARSKI:def 1;
L26: C5 = ( 0 ) by L25 , L19 , L23 , TARSKI:def 2;
thus L27: thesis by L26 , TARSKI:def 1;
end;

let C6 being set;
assume L28: C6 in { ( 0 ) };
L29: C6 = ( 0 ) by L28 , TARSKI:def 1;
L30: (not C6 in { C4 }) by L29 , L20 , TARSKI:def 1;
L31: C6 in ( [#] D2 ) by L19 , L29 , TARSKI:def 2;
thus L32: thesis by L31 , L30 , XBOOLE_0:def 5;
end;
L33: { ( 0 ) } <> { 1 } by ZFMISC_1:3;
L34: { ( 0 ) } <> { ( 0 ) , 1 } by ZFMISC_1:5;
L35: (the topology of D2) = { ( {} ) , { 1 } , { ( 0 ) , 1 } } by WAYBEL18:def 9;
thus L36: (not ( ( [#] D2 ) \ { C4 } ) in (the topology of D2)) by L35 , L33 , L34 , L21 , ENUMSET1:def 1;
end;
