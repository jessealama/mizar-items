definition
let C1 being non  empty TopSpace;
let C2 being (Subset of C1);
redefine attr C2 is  T_0
means
:L1: (for B1 , B2 being (Point of C1) holds ((B1 in C2 & B2 in C2) implies (B1 <> B2 implies ( MaxADSet B1 ) misses ( MaxADSet B2 ))));
compatibility
proof
thus L2: (C2 is  T_0 implies (for B3 , B4 being (Point of C1) holds ((B3 in C2 & B4 in C2) implies (B3 <> B4 implies ( MaxADSet B3 ) misses ( MaxADSet B4 )))))
proof
assume L3: C2 is  T_0;
let C3 , C4 being (Point of C1);
assume that
L4: C3 in C2
and
L5: C4 in C2;
assume L6: C3 <> C4;
L7:
now
per cases  by L3 , L4 , L5 , L6 , TSP_1:def 8;
suppose L8: (ex B5 being (Subset of C1) st (B5 is  open & C3 in B5 & (not C4 in B5)));

consider C5 being (Subset of C1) such that L9: C5 is  open and L10: C3 in C5 and L11: (not C4 in C5) by L8;
L12:
now
take D1 = C5;
thus L13: D1 is  open by L9;
thus L14: ( MaxADSet C3 ) c= D1 by L9 , L10 , TEX_4:24;
L15: C4 in ( ( [#] C1 ) \ D1 ) by L11 , XBOOLE_0:def 5;
L16: ( MaxADSet C4 ) c= ( D1 ` ) by L15 , L9 , TEX_4:23;
thus L17: D1 misses ( MaxADSet C4 ) by L16 , SUBSET_1:23;
end;
thus L18: ((ex B6 being (Subset of C1) st (B6 is  open & ( MaxADSet C3 ) c= B6 & B6 misses ( MaxADSet C4 ))) or (ex B7 being (Subset of C1) st (B7 is  open & B7 misses ( MaxADSet C3 ) & ( MaxADSet C4 ) c= B7))) by L12;
end;
suppose L19: (ex B8 being (Subset of C1) st (B8 is  open & (not C3 in B8) & C4 in B8));

consider C6 being (Subset of C1) such that L20: C6 is  open and L21: (not C3 in C6) and L22: C4 in C6 by L19;
L23:
now
take D2 = C6;
thus L24: D2 is  open by L20;
L25: C3 in ( ( [#] C1 ) \ D2 ) by L21 , XBOOLE_0:def 5;
L26: ( MaxADSet C3 ) c= ( D2 ` ) by L25 , L20 , TEX_4:23;
thus L27: D2 misses ( MaxADSet C3 ) by L26 , SUBSET_1:23;
thus L28: ( MaxADSet C4 ) c= D2 by L20 , L22 , TEX_4:24;
end;
thus L29: ((ex B9 being (Subset of C1) st (B9 is  open & ( MaxADSet C3 ) c= B9 & B9 misses ( MaxADSet C4 ))) or (ex B10 being (Subset of C1) st (B10 is  open & B10 misses ( MaxADSet C3 ) & ( MaxADSet C4 ) c= B10))) by L23;
end;
end;
thus L31: thesis by L7 , TEX_4:53;
end;

assume L32: (for B11 , B12 being (Point of C1) holds ((B11 in C2 & B12 in C2) implies (B11 <> B12 implies ( MaxADSet B11 ) misses ( MaxADSet B12 ))));
L33:
now
let C7 , C8 being (Point of C1);
assume that
L34: C7 in C2
and
L35: C8 in C2;
assume L36: C7 <> C8;
L37: ( MaxADSet C7 ) misses ( MaxADSet C8 ) by L36 , L32 , L34 , L35;
L38:
now
per cases  by L37 , TEX_4:53;
suppose L39: (ex B13 being (Subset of C1) st (B13 is  open & ( MaxADSet C7 ) c= B13 & B13 misses ( MaxADSet C8 )));

consider C9 being (Subset of C1) such that L40: C9 is  open and L41: ( MaxADSet C7 ) c= C9 and L42: C9 misses ( MaxADSet C8 ) by L39;
L43:
now
take D3 = C9;
thus L44: D3 is  open by L40;
L45: { C7 } c= ( MaxADSet C7 ) by TEX_4:18;
L46: C7 in ( MaxADSet C7 ) by L45 , ZFMISC_1:31;
thus L47: C7 in D3 by L46 , L41;
L48:
now
L49: { C8 } c= ( MaxADSet C8 ) by TEX_4:18;
L50: C8 in ( MaxADSet C8 ) by L49 , ZFMISC_1:31;
assume L51: C8 in D3;
thus L52: contradiction by L51 , L42 , L50 , XBOOLE_0:3;
end;
thus L53: (not C8 in D3) by L48;
end;
thus L54: ((ex B14 being (Subset of C1) st (B14 is  open & C7 in B14 & (not C8 in B14))) or (ex B15 being (Subset of C1) st (B15 is  open & (not C7 in B15) & C8 in B15))) by L43;
end;
suppose L55: (ex B16 being (Subset of C1) st (B16 is  open & B16 misses ( MaxADSet C7 ) & ( MaxADSet C8 ) c= B16));

consider C10 being (Subset of C1) such that L56: C10 is  open and L57: C10 misses ( MaxADSet C7 ) and L58: ( MaxADSet C8 ) c= C10 by L55;
L59:
now
take D4 = C10;
thus L60: D4 is  open by L56;
L61:
now
L62: { C7 } c= ( MaxADSet C7 ) by TEX_4:18;
L63: C7 in ( MaxADSet C7 ) by L62 , ZFMISC_1:31;
assume L64: C7 in D4;
thus L65: contradiction by L64 , L57 , L63 , XBOOLE_0:3;
end;
thus L66: (not C7 in D4) by L61;
L67: { C8 } c= ( MaxADSet C8 ) by TEX_4:18;
L68: C8 in ( MaxADSet C8 ) by L67 , ZFMISC_1:31;
thus L69: C8 in D4 by L68 , L58;
end;
thus L70: ((ex B17 being (Subset of C1) st (B17 is  open & C7 in B17 & (not C8 in B17))) or (ex B18 being (Subset of C1) st (B18 is  open & (not C7 in B18) & C8 in B18))) by L59;
end;
end;
thus L72: ((ex B19 being (Subset of C1) st (B19 is  open & C7 in B19 & (not C8 in B19))) or (ex B20 being (Subset of C1) st (B20 is  open & (not C7 in B20) & C8 in B20))) by L38;
end;
thus L73: thesis by L33 , TSP_1:def 8;
end;
end;
definition
let C11 being non  empty TopSpace;
let C12 being (Subset of C11);
redefine attr C12 is  T_0
means
:L75: (for B21 being (Point of C11) holds (B21 in C12 implies ( C12 /\ ( MaxADSet B21 ) ) = { B21 }));
compatibility
proof
thus L76: (C12 is  T_0 implies (for B22 being (Point of C11) holds (B22 in C12 implies ( C12 /\ ( MaxADSet B22 ) ) = { B22 })))
proof
assume L77: C12 is  T_0;
let C13 being (Point of C11);
assume L78: C13 in C12;
assume L79: ( C12 /\ ( MaxADSet C13 ) ) <> { C13 };
L80: { C13 } c= ( MaxADSet C13 ) by TEX_4:18;
L81: C13 in ( MaxADSet C13 ) by L80 , ZFMISC_1:31;
L82: C13 in ( C12 /\ ( MaxADSet C13 ) ) by L81 , L78 , XBOOLE_0:def 4;
consider C14 being set such that L83: C14 in ( C12 /\ ( MaxADSet C13 ) ) and L84: C14 <> C13 by L82 , L79 , ZFMISC_1:35;
reconsider D5 = C14 as (Point of C11) by L83;
L85: D5 in C12 by L83 , XBOOLE_0:def 4;
L86: ( MaxADSet C13 ) misses ( MaxADSet D5 ) by L85 , L77 , L78 , L84 , L1;
L87: D5 in ( MaxADSet C13 ) by L83 , XBOOLE_0:def 4;
thus L88: contradiction by L87 , L86 , TEX_4:21;
end;

assume L89: (for B23 being (Point of C11) holds (B23 in C12 implies ( C12 /\ ( MaxADSet B23 ) ) = { B23 }));
L90:
now
let C15 , C16 being (Point of C11);
assume that
L91: C15 in C12
and
L92: C16 in C12;
L93: ( C12 /\ ( MaxADSet C15 ) ) = { C15 } by L89 , L91;
L94: ( C12 /\ ( MaxADSet C16 ) ) = { C16 } by L89 , L92;
assume L95: C15 <> C16;
assume L96: ( MaxADSet C15 ) meets ( MaxADSet C16 );
L97: { C15 } = { C16 } by L96 , L93 , L94 , TEX_4:22;
thus L98: contradiction by L97 , L95 , ZFMISC_1:3;
end;
thus L99: thesis by L90 , L1;
end;
end;
definition
let C17 being non  empty TopSpace;
let C18 being (Subset of C17);
redefine attr C18 is  T_0
means
(for B24 being (Point of C17) holds (B24 in C18 implies (ex B25 being (Subset of C17) st (B25 is  maximal_anti-discrete & ( C18 /\ B25 ) = { B24 }))));
compatibility
proof
thus L101: (C18 is  T_0 implies (for B26 being (Point of C17) holds (B26 in C18 implies (ex B27 being (Subset of C17) st (B27 is  maximal_anti-discrete & ( C18 /\ B27 ) = { B26 })))))
proof
assume L102: C18 is  T_0;
let C19 being (Point of C17);
assume L103: C19 in C18;
take D6 = ( MaxADSet C19 );
thus L104: D6 is  maximal_anti-discrete by TEX_4:20;
thus L105: thesis by L102 , L103 , L75;
end;

assume L106: (for B28 being (Point of C17) holds (B28 in C18 implies (ex B29 being (Subset of C17) st (B29 is  maximal_anti-discrete & ( C18 /\ B29 ) = { B28 }))));
L107:
now
let C20 being (Point of C17);
assume L108: C20 in C18;
consider C21 being (Subset of C17) such that L109: C21 is  maximal_anti-discrete and L110: ( C18 /\ C21 ) = { C20 } by L108 , L106;
L111: C20 in ( C18 /\ C21 ) by L110 , ZFMISC_1:31;
L112: C20 in C21 by L111 , XBOOLE_0:def 4;
thus L113: ( C18 /\ ( MaxADSet C20 ) ) = { C20 } by L112 , L109 , L110 , TEX_4:27;
end;
thus L114: thesis by L107 , L75;
end;
end;
definition
let C22 being TopStruct;
let C23 being (Subset of C22);
attr C23 is  maximal_T_0
means
:L116: (C23 is  T_0 & (for B30 being (Subset of C22) holds ((B30 is  T_0 & C23 c= B30) implies C23 = B30)));
end;
theorem
L118: (for B31 , B32 being TopStruct holds (for B33 being (Subset of B31) holds (for B34 being (Subset of B32) holds ((( the TopStruct of B31 ) = ( the TopStruct of B32 ) & B33 = B34) implies (B33 is  maximal_T_0 implies B34 is  maximal_T_0)))))
proof
let C24 , C25 being TopStruct;
let C26 being (Subset of C24);
let C27 being (Subset of C25);
assume L119: ( the TopStruct of C24 ) = ( the TopStruct of C25 );
assume L120: C26 = C27;
assume L121: C26 is  maximal_T_0;
L122:
now
let C28 being (Subset of C25);
reconsider D7 = C28 as (Subset of C24) by L119;
assume L123: C28 is  T_0;
L124: D7 is  T_0 by L123 , L119 , TSP_1:3;
assume L125: C27 c= C28;
thus L126: C27 = C28 by L125 , L120 , L121 , L124 , L116;
end;
L127: C26 is  T_0 by L121 , L116;
L128: C27 is  T_0 by L127 , L119 , L120 , TSP_1:3;
thus L129: thesis by L128 , L122 , L116;
end;
definition
let C29 being non  empty TopSpace;
let C30 being (Subset of C29);
redefine attr C30 is  maximal_T_0
means
:L130: (C30 is  T_0 & ( MaxADSet C30 ) = (the carrier of C29));
compatibility
proof
thus L131: (C30 is  maximal_T_0 implies (C30 is  T_0 & ( MaxADSet C30 ) = (the carrier of C29)))
proof
assume L132: C30 is  maximal_T_0;
thus L133: C30 is  T_0 by L132 , L116;
L134: (the carrier of C29) c= ( MaxADSet C30 )
proof
assume L135: (not (the carrier of C29) c= ( MaxADSet C30 ));
consider C31 being set such that L136: C31 in (the carrier of C29) and L137: (not C31 in ( MaxADSet C30 )) by L135 , TARSKI:def 3;
reconsider D8 = C31 as (Point of C29) by L136;
set D9 = ( C30 \/ { D8 } );
L138: (for B35 being (Point of C29) holds (B35 in D9 implies ( D9 /\ ( MaxADSet B35 ) ) = { B35 }))
proof
let C32 being (Point of C29);
assume L139: C32 in D9;
L140: (C32 in C30 or C32 in { D8 }) by L139 , XBOOLE_0:def 3;
L141:
now
per cases  by L140 , TARSKI:def 1;
suppose L142: C32 in C30;

L143: ( { D8 } /\ ( MaxADSet C32 ) ) = ( {} )
proof
assume L144: ( { D8 } /\ ( MaxADSet C32 ) ) <> ( {} );
L145: { D8 } meets ( MaxADSet C32 ) by L144 , XBOOLE_0:def 7;
L146: D8 in ( MaxADSet C32 ) by L145 , ZFMISC_1:50;
L147: { C32 } c= C30 by L142 , ZFMISC_1:31;
L148: ( MaxADSet { C32 } ) c= ( MaxADSet C30 ) by L147 , TEX_4:31;
L149: ( MaxADSet C32 ) c= ( MaxADSet C30 ) by L148 , TEX_4:28;
thus L150: contradiction by L149 , L137 , L146;
end;
L151: ( D9 /\ ( MaxADSet C32 ) ) = ( ( C30 /\ ( MaxADSet C32 ) ) \/ ( {} ) ) by L143 , XBOOLE_1:23
.= ( C30 /\ ( MaxADSet C32 ) );
thus L152: thesis by L151 , L133 , L142 , L75;
end;
suppose L153: C32 = D8;

L154: { D8 } c= ( MaxADSet C32 ) by L153 , TEX_4:18;
L155: ( C30 /\ ( MaxADSet C32 ) ) = ( {} )
proof
L156: C30 c= ( MaxADSet C30 ) by TEX_4:32;
assume L157: ( C30 /\ ( MaxADSet C32 ) ) <> ( {} );
L158: ( ( MaxADSet C32 ) /\ ( MaxADSet C30 ) ) <> ( {} ) by L157 , L156 , XBOOLE_1:3 , XBOOLE_1:26;
L159: ( MaxADSet C32 ) meets ( MaxADSet C30 ) by L158 , XBOOLE_0:def 7;
L160: ( MaxADSet C32 ) c= ( MaxADSet C30 ) by L159 , TEX_4:30;
L161: D8 in ( MaxADSet C32 ) by L154 , ZFMISC_1:31;
thus L162: contradiction by L161 , L137 , L160;
end;
L163: ( D9 /\ ( MaxADSet C32 ) ) = ( ( {} ) \/ ( { D8 } /\ ( MaxADSet C32 ) ) ) by L155 , XBOOLE_1:23
.= ( { D8 } /\ ( MaxADSet C32 ) );
thus L164: thesis by L163 , L153 , TEX_4:18 , XBOOLE_1:28;
end;
end;
thus L166: thesis by L141;
end;
L167: D9 is  T_0 by L138 , L75;
L168: C30 c= ( MaxADSet C30 ) by TEX_4:32;
L169: { D8 } c= D9 by XBOOLE_1:7;
L170: C30 c= D9 by XBOOLE_1:7;
L171: C30 = D9 by L170 , L132 , L167 , L116;
L172: D8 in C30 by L171 , L169 , ZFMISC_1:31;
thus L173: contradiction by L172 , L137 , L168;
end;
thus L174: thesis by L134 , XBOOLE_0:def 10;
end;

assume L175: C30 is  T_0;
assume L176: ( MaxADSet C30 ) = (the carrier of C29);
L177: (for B36 being (Subset of C29) holds ((B36 is  T_0 & C30 c= B36) implies C30 = B36))
proof
let C33 being (Subset of C29);
assume L178: C33 is  T_0;
assume L179: C30 c= C33;
L180: C33 c= C30
proof
assume L181: (not C33 c= C30);
consider C34 being set such that L182: C34 in C33 and L183: (not C34 in C30) by L181 , TARSKI:def 3;
L184: C34 in (the carrier of C29) by L182;
reconsider D10 = C34 as (Point of C29) by L182;
L185: D10 in ( union { ( MaxADSet B37 ) where B37 is (Point of C29) : B37 in C30 } ) by L176 , L184 , TEX_4:def 11;
consider C35 being set such that L186: D10 in C35 and L187: C35 in { ( MaxADSet B38 ) where B38 is (Point of C29) : B38 in C30 } by L185 , TARSKI:def 4;
consider C36 being (Point of C29) such that L188: C35 = ( MaxADSet C36 ) and L189: C36 in C30 by L187;
L190: ( C30 /\ ( MaxADSet D10 ) ) c= ( C33 /\ ( MaxADSet D10 ) ) by L179 , XBOOLE_1:26;
L191: ( C30 /\ ( MaxADSet D10 ) ) c= { D10 } by L190 , L178 , L182 , L75;
L192: ( MaxADSet C36 ) = ( MaxADSet D10 ) by L186 , L188 , TEX_4:21;
L193: ( C30 /\ ( MaxADSet D10 ) ) = { C36 } by L192 , L175 , L189 , L75;
thus L194: contradiction by L193 , L183 , L189 , L191 , ZFMISC_1:18;
end;
thus L195: thesis by L180 , L179 , XBOOLE_0:def 10;
end;
thus L196: thesis by L177 , L175 , L116;
end;
