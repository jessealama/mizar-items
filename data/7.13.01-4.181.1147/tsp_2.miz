theorem
L198: (for R1 being non  empty TopSpace holds (for B39 being (Subset of R1) holds (B39 is  maximal_T_0 implies B39 is  dense)))
proof
let R1 being non  empty TopSpace;
let C37 being (Subset of R1);
assume L199: C37 is  maximal_T_0;
L200: ( MaxADSet C37 ) = ( [#] R1 ) by L199 , L130;
L201: ( Cl ( MaxADSet C37 ) ) = ( MaxADSet C37 ) by L200 , PRE_TOPC:22;
L202: ( MaxADSet C37 ) = (the carrier of R1) by L199 , L130;
L203: ( Cl C37 ) = (the carrier of R1) by L202 , L201 , TEX_4:62;
thus L204: thesis by L203 , TOPS_3:def 2;
end;
theorem
L205: (for R1 being non  empty TopSpace holds (for B40 being (Subset of R1) holds ((B40 is  open or B40 is  closed) implies (B40 is  maximal_T_0 implies (not B40 is  proper)))))
proof
let R1 being non  empty TopSpace;
let C38 being (Subset of R1);
assume L206: (C38 is  open or C38 is  closed);
L207: C38 = ( MaxADSet C38 ) by L206 , TEX_4:56 , TEX_4:60;
assume L208: C38 is  maximal_T_0;
L209: C38 = (the carrier of R1) by L208 , L207 , L130;
thus L210: thesis by L209 , SUBSET_1:def 6;
end;
theorem
L211: (for R1 being non  empty TopSpace holds (for B41 being  empty (Subset of R1) holds (not B41 is  maximal_T_0)))
proof
let R1 being non  empty TopSpace;
consider C39 being set such that L212: C39 in (the carrier of R1) by XBOOLE_0:def 1;
reconsider D11 = C39 as (Point of R1) by L212;
let C40 being  empty (Subset of R1);
L213:
now
take D12 = { D11 };
thus L214: (D12 is  T_0 & C40 c= D12 & C40 <> D12) by TSP_1:12 , XBOOLE_1:2;
end;
thus L215: thesis by L213 , L116;
end;
theorem
L216: (for R1 being non  empty TopSpace holds (for B42 being (Subset of R1) holds (B42 is  maximal_T_0 implies (for B43 being (Subset of R1) holds (B43 is  closed implies B43 = ( MaxADSet ( B42 /\ B43 ) ))))))
proof
let R1 being non  empty TopSpace;
let C41 being (Subset of R1);
assume L217: C41 is  maximal_T_0;
let C42 being (Subset of R1);
assume L218: C42 is  closed;
L219: ( MaxADSet ( C41 /\ C42 ) ) = ( ( MaxADSet C41 ) /\ ( MaxADSet C42 ) ) by L218 , TEX_4:64;
L220: ( MaxADSet ( C41 /\ C42 ) ) = ( ( MaxADSet C41 ) /\ C42 ) by L219 , L218 , TEX_4:60;
L221: C42 = ( (the carrier of R1) /\ C42 ) by XBOOLE_1:28;
thus L222: thesis by L221 , L217 , L220 , L130;
end;
theorem
L223: (for R1 being non  empty TopSpace holds (for B44 being (Subset of R1) holds (B44 is  maximal_T_0 implies (for B45 being (Subset of R1) holds (B45 is  open implies B45 = ( MaxADSet ( B44 /\ B45 ) ))))))
proof
let R1 being non  empty TopSpace;
let C43 being (Subset of R1);
assume L224: C43 is  maximal_T_0;
let C44 being (Subset of R1);
assume L225: C44 is  open;
L226: ( MaxADSet ( C43 /\ C44 ) ) = ( ( MaxADSet C43 ) /\ ( MaxADSet C44 ) ) by L225 , TEX_4:65;
L227: ( MaxADSet ( C43 /\ C44 ) ) = ( ( MaxADSet C43 ) /\ C44 ) by L226 , L225 , TEX_4:56;
L228: C44 = ( (the carrier of R1) /\ C44 ) by XBOOLE_1:28;
thus L229: thesis by L228 , L224 , L227 , L130;
end;
theorem
L230: (for R1 being non  empty TopSpace holds (for B46 being (Subset of R1) holds (B46 is  maximal_T_0 implies (for B47 being (Subset of R1) holds ( Cl B47 ) = ( MaxADSet ( B46 /\ ( Cl B47 ) ) ))))) by L216;
theorem
L231: (for R1 being non  empty TopSpace holds (for B48 being (Subset of R1) holds (B48 is  maximal_T_0 implies (for B49 being (Subset of R1) holds ( Int B49 ) = ( MaxADSet ( B48 /\ ( Int B49 ) ) ))))) by L223;
definition
let C45 being non  empty TopSpace;
let C46 being (Subset of C45);
redefine attr C46 is  maximal_T_0
means
:L232: (for B50 being (Point of C45) holds (ex B51 being (Point of C45) st (B51 in C46 & ( C46 /\ ( MaxADSet B50 ) ) = { B51 })));
compatibility
proof
thus L233: (C46 is  maximal_T_0 implies (for B52 being (Point of C45) holds (ex B53 being (Point of C45) st (B53 in C46 & ( C46 /\ ( MaxADSet B52 ) ) = { B53 }))))
proof
assume L234: C46 is  maximal_T_0;
