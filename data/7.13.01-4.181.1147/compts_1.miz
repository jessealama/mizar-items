begin
definition
let C1 being TopStruct;
attr C1 is  compact
means
:L1: (for B1 being (Subset-Family of C1) holds ((B1 is (Cover of C1) & B1 is  open) implies (ex B2 being (Subset-Family of C1) st (B2 c= B1 & B2 is (Cover of C1) & B2 is  finite))));
end;
definition
let C2 being non  empty TopSpace;
redefine attr C2 is  regular
means
(for B3 being (Point of C2) holds (for B4 being (Subset of C2) holds ((B4 <> ( {} ) & B4 is  closed & B3 in ( B4 ` )) implies (ex B5 , B6 being (Subset of C2) st (B5 is  open & B6 is  open & B3 in B5 & B4 c= B6 & B5 misses B6)))));
compatibility
proof
thus L3: (C2 is  regular implies (for B7 being (Point of C2) holds (for B8 being (Subset of C2) holds ((B8 <> ( {} ) & B8 is  closed & B7 in ( B8 ` )) implies (ex B9 , B10 being (Subset of C2) st (B9 is  open & B10 is  open & B7 in B9 & B8 c= B10 & B9 misses B10)))))) by PRE_TOPC:def 11;
assume L4: (for B11 being (Point of C2) holds (for B12 being (Subset of C2) holds ((B12 <> ( {} ) & B12 is  closed & B11 in ( B12 ` )) implies (ex B13 , B14 being (Subset of C2) st (B13 is  open & B14 is  open & B11 in B13 & B12 c= B14 & B13 misses B14)))));
let C3 being (Point of C2);
let C4 being (Subset of C2);
assume that
L5: C4 is  closed
and
L6: C3 in ( C4 ` );
per cases ;
suppose L7: C4 = ( {} );

take D1 = ( [#] C2 );
take D2 = ( {} C2 );
thus L8: (D1 is  open & D2 is  open);
thus L9: C3 in D1;
thus L10: thesis by L7 , XBOOLE_1:65;
end;
suppose L11: C4 <> ( {} );

thus L12: thesis by L11 , L4 , L5 , L6;
end;
end;
redefine attr C2 is  normal
means
(for B15 , B16 being (Subset of C2) holds ((B15 <> ( {} ) & B16 <> ( {} ) & B15 is  closed & B16 is  closed & B15 misses B16) implies (ex B17 , B18 being (Subset of C2) st (B17 is  open & B18 is  open & B15 c= B17 & B16 c= B18 & B17 misses B18))));
compatibility
proof
thus L14: (C2 is  normal implies (for B19 , B20 being (Subset of C2) holds ((B19 <> ( {} ) & B20 <> ( {} ) & B19 is  closed & B20 is  closed & B19 misses B20) implies (ex B21 , B22 being (Subset of C2) st (B21 is  open & B22 is  open & B19 c= B21 & B20 c= B22 & B21 misses B22))))) by PRE_TOPC:def 12;
assume L15: (for B23 , B24 being (Subset of C2) holds ((B23 <> ( {} ) & B24 <> ( {} ) & B23 is  closed & B24 is  closed & B23 misses B24) implies (ex B25 , B26 being (Subset of C2) st (B25 is  open & B26 is  open & B23 c= B25 & B24 c= B26 & B25 misses B26))));
let C5 , C6 being (Subset of C2);
assume that
L16: C5 is  closed
and
L17: C6 is  closed
and
L18: C5 misses C6;
per cases ;
suppose L19: C5 = ( {} );

take D3 = ( {} C2 );
take D4 = ( [#] C2 );
thus L20: (D3 is  open & D4 is  open);
thus L21: C5 c= D3 by L19;
thus L22: C6 c= D4;
thus L23: thesis by XBOOLE_1:65;
end;
suppose L24: C6 = ( {} );

take D5 = ( [#] C2 );
take D6 = ( {} C2 );
thus L25: (D5 is  open & D6 is  open);
thus L26: C5 c= D5;
thus L27: C6 c= D6 by L24;
thus L28: thesis by XBOOLE_1:65;
end;
suppose L29: (C5 <> ( {} ) & C6 <> ( {} ));

thus L30: thesis by L29 , L15 , L16 , L17 , L18;
end;
end;
end;
notation
let C7 being TopStruct;
synonym C7 is  Hausdorff for C7 is  T_2;
end;
definition
let C8 being TopStruct;
let C9 being (Subset of C8);
attr C9 is  compact
means
:L34: (for B27 being (Subset-Family of C8) holds ((B27 is (Cover of C9) & B27 is  open) implies (ex B28 being (Subset-Family of C8) st (B28 c= B27 & B28 is (Cover of C9) & B28 is  finite))));
end;
registration
let R4 being TopStruct;
cluster  empty ->  compact for (Subset of R4);
coherence
proof
reconsider D7 = ( {} ) as (Subset-Family of R4) by XBOOLE_1:2;
let C10 being (Subset of R4);
assume L36: C10 is  empty;
L37: C10 c= ( union D7 ) by L36 , XBOOLE_1:2;
let C11 being (Subset-Family of R4);
assume that
L38: C11 is (Cover of C10)
and
L39: C11 is  open;
take D7;
thus L40: thesis by L37 , SETFAM_1:def 11 , XBOOLE_1:2;
end;
end;
theorem
L42: (for R4 being TopStruct holds (R4 is  compact iff ( [#] R4 ) is  compact))
proof
let R4 being TopStruct;
thus L43: (R4 is  compact implies ( [#] R4 ) is  compact)
proof
assume L44: R4 is  compact;
let C12 being (Subset-Family of R4);
assume that
L45: C12 is (Cover of ( [#] R4 ))
and
L46: C12 is  open;
consider C13 being (Subset-Family of R4) such that L47: C13 c= C12 and L48: C13 is (Cover of R4) and L49: C13 is  finite by L44 , L45 , L46 , L1;
take C13;
thus L50: thesis by L47 , L48 , L49;
end;

assume L51: ( [#] R4 ) is  compact;
let C14 being (Subset-Family of R4);
assume that
L52: C14 is (Cover of R4)
and
L53: C14 is  open;
consider C15 being (Subset-Family of R4) such that L54: C15 c= C14 and L55: C15 is (Cover of ( [#] R4 )) and L56: C15 is  finite by L51 , L52 , L53 , L34;
take C15;
thus L57: thesis by L54 , L55 , L56;
end;
theorem
L58: (for R4 being TopStruct holds (for R5 being (SubSpace of R4) holds (for R7 being (Subset of R4) holds (R7 c= ( [#] R5 ) implies (R7 is  compact iff (for B29 being (Subset of R5) holds (B29 = R7 implies B29 is  compact)))))))
proof
let R4 being TopStruct;
let R5 being (SubSpace of R4);
let R7 being (Subset of R4);
L59: ( [#] R5 ) c= ( [#] R4 ) by PRE_TOPC:def 4;
reconsider D8 = ( [#] R5 ) as (Subset of R4) by L59;
assume L60: R7 c= ( [#] R5 );
L61: ( R7 /\ D8 ) = R7 by L60 , XBOOLE_1:28;
thus L62: (R7 is  compact implies (for B30 being (Subset of R5) holds (B30 = R7 implies B30 is  compact)))
proof
assume L63: R7 is  compact;
let C16 being (Subset of R5);
assume that
L64: C16 = R7;
thus L65: C16 is  compact
proof
let C17 being (Subset-Family of R5);
set D9 = C17;
assume that
L66: C17 is (Cover of C16)
and
L67: C17 is  open;
consider C18 being (Subset-Family of R4) such that L68: C18 is  open and L69: (for B31 being (Subset of R4) holds (B31 = ( [#] R5 ) implies D9 = ( C18 | B31 ))) by L67 , TOPS_2:39;
L70: R7 c= ( union C17 ) by L64 , L66 , SETFAM_1:def 11;
L71: ( union ( C18 | D8 ) ) c= ( union C18 ) by TOPS_2:34;
L72: ( union C17 ) c= ( union C18 ) by L71 , L69;
L73: R7 c= ( union C18 ) by L72 , L70 , XBOOLE_1:1;
L74: C18 is (Cover of R7) by L73 , SETFAM_1:def 11;
consider C19 being (Subset-Family of R4) such that L75: C19 c= C18 and L76: C19 is (Cover of R7) and L77: C19 is  finite by L74 , L63 , L68 , L34;
L78: ( C19 | D8 ) c= ( bool ( [#] ( R4 | D8 ) ) );
reconsider D10 = ( C19 | D8 ) as (Subset-Family of R5) by L78 , PRE_TOPC:def 5;
take D10;
L79: ( C19 | D8 ) c= ( C18 | D8 ) by L75 , TOPS_2:30;
thus L80: D10 c= C17 by L79 , L69;
L81: R7 c= ( union C19 ) by L76 , SETFAM_1:def 11;
L82: C16 c= ( union D10 ) by L81 , L61 , L64 , TOPS_2:32;
thus L83: D10 is (Cover of C16) by L82 , SETFAM_1:def 11;
thus L84: thesis by L77 , TOPS_2:36;
end;

end;

thus L66: ((for B32 being (Subset of R5) holds (B32 = R7 implies B32 is  compact)) implies R7 is  compact)
proof
reconsider D11 = R7 as (Subset of R5) by L60;
assume L67: (for B33 being (Subset of R5) holds (B33 = R7 implies B33 is  compact));
L68: D11 is  compact by L67;
thus L69: R7 is  compact
proof
let C20 being (Subset-Family of R4);
set D12 = C20;
assume that
L70: C20 is (Cover of R7)
and
L71: C20 is  open;
consider C21 being Function such that L72: ( dom C21 ) = D12 and L73: ( rng C21 ) = ( D12 | D8 ) and L74: (for R1 being set holds (R1 in C20 implies (for B34 being (Subset of R4) holds (B34 = R1 implies ( C21 . R1 ) = ( B34 /\ D8 ))))) by TOPS_2:40;
L75: ( D12 | D8 ) c= ( bool ( [#] ( R4 | D8 ) ) );
reconsider D13 = ( rng C21 ) as (Subset-Family of R5) by L75 , L73 , PRE_TOPC:def 5;
L76: D13 is  open
proof
let C22 being (Subset of R5);
assume L77: C22 in D13;
reconsider D14 = C22 as (Subset of ( R4 | D8 )) by L77 , L73;
consider C23 being (Subset of R4) such that L78: C23 in C20 and L79: ( C23 /\ D8 ) = D14 by L73 , L77 , TOPS_2:def 3;
L80: C23 is  open by L71 , L78 , TOPS_2:def 1;
L81: C23 in (the topology of R4) by L80 , PRE_TOPC:def 2;
L82: C22 in (the topology of R5) by L81 , L79 , PRE_TOPC:def 4;
thus L83: thesis by L82 , PRE_TOPC:def 2;
end;
L84: R7 c= ( union C20 ) by L70 , SETFAM_1:def 11;
L85: D11 c= ( union D13 ) by L84 , L61 , L73 , TOPS_2:32;
L86: D13 is (Cover of D11) by L85 , SETFAM_1:def 11;
consider C24 being (Subset-Family of R5) such that L87: C24 c= D13 and L88: C24 is (Cover of D11) and L89: C24 is  finite by L86 , L68 , L76 , L34;
consider C25 being set such that L90: C25 c= ( dom C21 ) and L91: C25 is  finite and L92: ( C21 .: C25 ) = C24 by L87 , L89 , ORDERS_1:85;
reconsider D15 = C25 as (Subset-Family of R4) by L72 , L90 , TOPS_2:2;
take D15;
L93: R7 c= ( union D15 )
proof
let R1 being set;
assume L94: R1 in R7;
L95: D11 c= ( union C24 ) by L88 , SETFAM_1:def 11;
consider C26 being set such that L96: R1 in C26 and L97: C26 in C24 by L95 , L94 , TARSKI:def 4;
consider R3 being set such that L98: R3 in ( dom C21 ) and L99: R3 in C25 and L100: ( C21 . R3 ) = C26 by L92 , L97 , FUNCT_1:def 6;
reconsider D16 = R3 as (Subset of R4) by L72 , L98;
L101: C26 = ( D16 /\ D8 ) by L72 , L74 , L98 , L100;
L102: R1 in D16 by L101 , L96 , XBOOLE_0:def 4;
thus L103: thesis by L102 , L99 , TARSKI:def 4;
end;
thus L104: thesis by L93 , L72 , L90 , L91 , SETFAM_1:def 11;
end;

end;

end;
theorem
L67: (for R4 being TopStruct holds (for R6 being (Subset of R4) holds ((R6 = ( {} ) implies (R6 is  compact iff ( R4 | R6 ) is  compact)) & ((R4 is  TopSpace-like & R6 <> ( {} )) implies (R6 is  compact iff ( R4 | R6 ) is  compact)))))
proof
let R4 being TopStruct;
let R6 being (Subset of R4);
L68: ( [#] ( R4 | R6 ) ) = R6 by PRE_TOPC:def 5;
thus L69: (R6 = ( {} ) implies (R6 is  compact iff ( R4 | R6 ) is  compact)) by L68 , L42;
assume that
L70: R4 is  TopSpace-like
and
L71: R6 <> ( {} );
reconsider D17 = R4 as non  empty TopSpace by L70 , L71;
reconsider D18 = R6 as non  empty (Subset of D17) by L71;
L72: (( [#] ( D17 | D18 ) ) is  compact iff ( D17 | D18 ) is  compact) by L42;
thus L73: (R6 is  compact implies ( R4 | R6 ) is  compact) by L72 , L68 , L58;
assume L74: ( R4 | R6 ) is  compact;
L75: (for B35 being (Subset of ( R4 | R6 )) holds (B35 = R6 implies B35 is  compact)) by L74 , L72 , PRE_TOPC:def 5;
thus L76: thesis by L75 , L68 , L58;
end;
theorem
L77: (for B36 being non  empty TopSpace holds (B36 is  compact iff (for B37 being (Subset-Family of B36) holds ((B37 is  centered & B37 is  closed) implies ( meet B37 ) <> ( {} )))))
proof
let C27 being non  empty TopSpace;
thus L78: (C27 is  compact implies (for B38 being (Subset-Family of C27) holds ((B38 is  centered & B38 is  closed) implies ( meet B38 ) <> ( {} ))))
proof
assume L79: C27 is  compact;
let C28 being (Subset-Family of C27);
assume that
L80: C28 is  centered
and
L81: C28 is  closed;
reconsider D19 = ( COMPLEMENT C28 ) as (Subset-Family of C27);
assume L82: ( meet C28 ) = ( {} );
L83: C28 <> ( {} ) by L80 , FINSET_1:def 3;
L84: ( union ( COMPLEMENT C28 ) ) = ( ( meet C28 ) ` ) by L83 , TOPS_2:7
.= ( [#] C27 ) by L82;
L85: ( COMPLEMENT C28 ) is (Cover of C27) by L84 , SETFAM_1:def 11;
L86: ( COMPLEMENT C28 ) is  open by L81 , TOPS_2:9;
consider C29 being (Subset-Family of C27) such that L87: C29 c= D19 and L88: C29 is (Cover of C27) and L89: C29 is  finite by L86 , L79 , L85 , L1;
set D20 = ( COMPLEMENT C29 );
L90: D20 is  finite by L89 , TOPS_2:8;
L91: D20 c= C28
proof
let C30 being set;
assume L92: C30 in D20;
reconsider D21 = C30 as (Subset of C27) by L92;
L93: ( D21 ` ) in C29 by L92 , SETFAM_1:def 7;
L94: ( ( D21 ` ) ` ) in C28 by L93 , L87 , SETFAM_1:def 7;
thus L95: thesis by L94;
end;
L96: C29 <> ( {} ) by L88 , TOPS_2:3;
L97: D20 <> ( {} ) by L96 , TOPS_2:5;
L98: ( meet D20 ) = ( ( union C29 ) ` ) by L88 , TOPS_2:3 , TOPS_2:6
.= ( ( [#] C27 ) \ ( [#] C27 ) ) by L88 , SETFAM_1:45
.= ( {} ) by XBOOLE_1:37;
thus L99: contradiction by L98 , L80 , L91 , L90 , L97 , FINSET_1:def 3;
end;

assume L100: (for B39 being (Subset-Family of C27) holds ((B39 is  centered & B39 is  closed) implies ( meet B39 ) <> ( {} )));
thus L101: C27 is  compact
proof
let C31 being (Subset-Family of C27);
assume that
L102: C31 is (Cover of C27)
and
L103: C31 is  open;
reconsider D22 = ( COMPLEMENT C31 ) as (Subset-Family of C27);
L104: D22 is  closed by L103 , TOPS_2:10;
L105: C31 <> ( {} ) by L102 , TOPS_2:3;
L106: D22 <> ( {} ) by L105 , TOPS_2:5;
L107: ( meet D22 ) = ( ( union C31 ) ` ) by L102 , TOPS_2:3 , TOPS_2:6
.= ( ( [#] C27 ) \ ( [#] C27 ) ) by L102 , SETFAM_1:45
.= ( {} ) by XBOOLE_1:37;
L108: (not D22 is  centered) by L107 , L100 , L104;
consider C32 being set such that L109: C32 <> ( {} ) and L110: C32 c= D22 and L111: C32 is  finite and L112: ( meet C32 ) = ( {} ) by L108 , L106 , FINSET_1:def 3;
reconsider D23 = C32 as (Subset-Family of C27) by L110 , XBOOLE_1:1;
take D24 = ( COMPLEMENT D23 );
thus L113: D24 c= C31
proof
let C33 being set;
assume L114: C33 in D24;
reconsider D25 = C33 as (Subset of C27) by L114;
L115: ( D25 ` ) in D23 by L114 , SETFAM_1:def 7;
L116: ( ( D25 ` ) ` ) in C31 by L115 , L110 , SETFAM_1:def 7;
thus L117: thesis by L116;
end;

L118: ( union D24 ) = ( ( meet D23 ) ` ) by L109 , TOPS_2:7
.= ( [#] C27 ) by L112;
thus L119: D24 is (Cover of C27) by L118 , SETFAM_1:def 11;
thus L120: thesis by L111 , TOPS_2:8;
end;

end;
theorem
L102: (for B40 being non  empty TopSpace holds (B40 is  compact iff (for B41 being (Subset-Family of B40) holds ((B41 <> ( {} ) & B41 is  closed & ( meet B41 ) = ( {} )) implies (ex B42 being (Subset-Family of B40) st (B42 <> ( {} ) & B42 c= B41 & B42 is  finite & ( meet B42 ) = ( {} )))))))
proof
let C34 being non  empty TopSpace;
thus L103: (C34 is  compact implies (for B43 being (Subset-Family of C34) holds ((B43 <> ( {} ) & B43 is  closed & ( meet B43 ) = ( {} )) implies (ex B44 being (Subset-Family of C34) st (B44 <> ( {} ) & B44 c= B43 & B44 is  finite & ( meet B44 ) = ( {} ))))))
proof
assume L104: C34 is  compact;
let C35 being (Subset-Family of C34);
assume that
L105: C35 <> ( {} )
and
L106: C35 is  closed
and
L107: ( meet C35 ) = ( {} );
L108: (not C35 is  centered) by L104 , L106 , L107 , L77;
consider C36 being set such that L109: C36 <> ( {} ) and L110: C36 c= C35 and L111: C36 is  finite and L112: ( meet C36 ) = ( {} ) by L108 , L105 , FINSET_1:def 3;
reconsider D26 = C36 as (Subset-Family of C34) by L110 , XBOOLE_1:1;
take D26;
thus L113: thesis by L109 , L110 , L111 , L112;
end;

assume L114: (for B45 being (Subset-Family of C34) holds ((B45 <> ( {} ) & B45 is  closed & ( meet B45 ) = ( {} )) implies (ex B46 being (Subset-Family of C34) st (B46 <> ( {} ) & B46 c= B45 & B46 is  finite & ( meet B46 ) = ( {} )))));
L115: (for B47 being (Subset-Family of C34) holds ((B47 is  centered & B47 is  closed) implies ( meet B47 ) <> ( {} )))
proof
let C37 being (Subset-Family of C34);
assume that
L116: C37 is  centered
and
L117: C37 is  closed;
assume L118: ( meet C37 ) = ( {} );
L119: C37 <> ( {} ) by L116 , FINSET_1:def 3;
L120: (ex B48 being (Subset-Family of C34) st (B48 <> ( {} ) & B48 c= C37 & B48 is  finite & ( meet B48 ) = ( {} ))) by L119 , L114 , L117 , L118;
thus L121: contradiction by L120 , L116 , FINSET_1:def 3;
end;
thus L122: thesis by L115 , L77;
end;
theorem
L123: (for R8 being TopSpace holds (R8 is  T_2 implies (for B49 being (Subset of R8) holds ((B49 <> ( {} ) & B49 is  compact) implies (for B50 being (Point of R8) holds (B50 in ( B49 ` ) implies (ex R9 being (Subset of R8) st (ex R10 being (Subset of R8) st (R9 is  open & R10 is  open & B50 in R9 & B49 c= R10 & R9 misses R10)))))))))
proof
let R8 being TopSpace;
assume L124: R8 is  T_2;
let C38 being (Subset of R8);
assume that
L125: C38 <> ( {} )
and
L126: C38 is  compact;
set D27 = the (Element of C38);
let C39 being (Point of R8);
assume L127: C39 in ( C38 ` );
L128: (not C39 in C38) by L127 , XBOOLE_0:def 5;
defpred S1[ set , set , set ] means (ex R9 being (Subset of R8) st (ex R10 being (Subset of R8) st ($2 = R9 & $3 = R10 & R9 is  open & R10 is  open & C39 in R9 & $1 in R10 & ( R9 /\ R10 ) = ( {} ))));
L129: (for R1 being set holds (R1 in C38 implies (ex R2 being set st (ex R3 being set st (R2 in (the topology of R8) & R3 in (the topology of R8) & S1[ R1 , R2 , R3 ])))))
proof
let R1 being set;
assume L130: R1 in C38;
reconsider D28 = R1 as (Point of R8) by L130;
consider C40 , C41 being (Subset of R8) such that L131: C40 is  open and L132: C41 is  open and L133: C39 in C40 and L134: D28 in C41 and L135: C40 misses C41 by L124 , L128 , L130 , PRE_TOPC:def 10;
reconsider D29 = C40 , D30 = C41 as set;
take D29;
take D30;
thus L136: (D29 in (the topology of R8) & D30 in (the topology of R8)) by L131 , L132 , PRE_TOPC:def 2;
take C40;
take C41;
thus L137: thesis by L131 , L132 , L133 , L134 , L135 , XBOOLE_0:def 7;
end;
consider C42 , C43 being Function such that L138: (( dom C42 ) = C38 & ( dom C43 ) = C38) and L139: (for R1 being set holds (R1 in C38 implies S1[ R1 , ( C42 . R1 ) , ( C43 . R1 ) ])) from MCART_1:sch 1(L129);
L140: ( C43 .: C38 ) c= ( bool (the carrier of R8) )
proof
