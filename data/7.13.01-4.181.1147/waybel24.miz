scheme FuncFraenkelSL { F1 , F2() -> non  empty RelStr , F3(set) -> (Element of F2()) , F4() -> Function , P1[set] } : ( F4() .: { F3(B16) where B16 is (Element of F1()) : P1[ B16 ] } ) = { ( F4() . F3(B17) ) where B17 is (Element of F1()) : P1[ B17 ] }
