theorem
L41: (for R1 being set holds (for R2 being set holds (for R5 being (Subset of ( R1 ^omega )) holds (for R6 being (Subset of ( R1 ^omega )) holds (<% R2 %> in ( R5 ^^ R6 ) iff ((( <%> R1 ) in R5 & <% R2 %> in R6) or (<% R2 %> in R5 & ( <%> R1 ) in R6)))))))
proof
let R1 being set;
let R2 being set;
let R5 being (Subset of ( R1 ^omega ));
let R6 being (Subset of ( R1 ^omega ));
thus L42: (<% R2 %> in ( R5 ^^ R6 ) implies ((( <%> R1 ) in R5 & <% R2 %> in R6) or (<% R2 %> in R5 & ( <%> R1 ) in R6)))
proof
assume L43: <% R2 %> in ( R5 ^^ R6 );
L44: (ex R8 being (Element of ( R1 ^omega )) st (ex R9 being (Element of ( R1 ^omega )) st (R8 in R5 & R9 in R6 & <% R2 %> = ( R8 ^ R9 )))) by L43 , FLANG_1:def 1;
thus L45: thesis by L44 , FLANG_1:4;
end;

L46: (( ( {} ) ^ <% R2 %> ) = <% R2 %> & ( <% R2 %> ^ ( {} ) ) = <% R2 %>);
assume L47: ((( <%> R1 ) in R5 & <% R2 %> in R6) or (<% R2 %> in R5 & ( <%> R1 ) in R6));
thus L48: thesis by L47 , L46 , FLANG_1:def 1;
end;
theorem
L49: (for R1 being set holds (for R2 being set holds (for R5 being (Subset of ( R1 ^omega )) holds (for R15 being Nat holds ((R2 in R5 & R2 <> ( <%> R1 ) & R15 > ( 0 )) implies ( R5 |^ R15 ) <> { ( <%> R1 ) })))))
