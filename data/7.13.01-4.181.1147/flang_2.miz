begin
theorem
L1: (for R10 being Nat holds (for R11 being Nat holds (for R14 being Nat holds (for R15 being Nat holds ((( R14 + R11 ) <= R10 & R10 <= ( R15 + R11 )) implies (ex R16 being Nat st (( R16 + R11 ) = R10 & R14 <= R16 & R16 <= R15)))))))
proof
let R10 being Nat;
let R11 being Nat;
let R14 being Nat;
let R15 being Nat;
assume that
L2: ( R14 + R11 ) <= R10
and
L3: R10 <= ( R15 + R11 );
L4: ( R14 + R11 ) <= ( R14 + R10 ) by L2 , XREAL_1:38;
L5: R11 <= R10 by L4 , XREAL_1:6;
reconsider D1 = ( R10 - R11 ) as Nat by L5 , NAT_1:21;
take D1;
thus L6: ( D1 + R11 ) = R10;
L7: ( R14 + R11 ) <= ( D1 + R11 ) by L2;
thus L8: thesis by L7 , L3 , XREAL_1:6;
end;
theorem
L9: (for R10 being Nat holds (for R11 being Nat holds (for R12 being Nat holds (for R14 being Nat holds (for R15 being Nat holds ((R14 <= R15 & R11 <= R12 & ( R14 + R11 ) <= R10 & R10 <= ( R15 + R12 )) implies (ex R16 being Nat st (ex R13 being Nat st (( R16 + R13 ) = R10 & R14 <= R16 & R16 <= R15 & R11 <= R13 & R13 <= R12)))))))))
proof
let R10 being Nat;
let R11 being Nat;
let R12 being Nat;
let R14 being Nat;
let R15 being Nat;
assume that
L10: R14 <= R15
and
L11: R11 <= R12
and
L12: ( R14 + R11 ) <= R10
and
L13: R10 <= ( R15 + R12 );
per cases ;
suppose L14: R10 <= ( R15 + R11 );

consider R16 being Nat such that L15: (( R16 + R11 ) = R10 & R14 <= R16 & R16 <= R15) by L14 , L12 , L1;
take R16;
take R11;
thus L16: (( R16 + R11 ) = R10 & R14 <= R16 & R16 <= R15) by L15;
thus L17: thesis by L11;
end;
suppose L18: R10 > ( R15 + R11 );

consider R13 being Nat such that L19: ( R13 + R15 ) = R10 and L20: (R11 <= R13 & R13 <= R12) by L18 , L13 , L1;
take R15;
take R13;
thus L21: (( R15 + R13 ) = R10 & R14 <= R15 & R15 <= R15) by L10 , L19;
thus L22: thesis by L20;
end;
end;
theorem
L24: (for R14 being Nat holds (for R15 being Nat holds (R14 < R15 implies (ex R11 being Nat st (( R14 + R11 ) = R15 & R11 > ( 0 ))))))
proof
let R14 being Nat;
let R15 being Nat;
assume L25: R14 < R15;
L26: ((ex R11 being Nat st ( R14 + R11 ) = R15) & ( R14 - R14 ) < ( R15 - R14 )) by L25 , NAT_1:10 , XREAL_1:14;
thus L27: thesis by L26;
end;
theorem
L28: (for R1 being set holds (for R8 being (Element of ( R1 ^omega )) holds (for R9 being (Element of ( R1 ^omega )) holds ((( R8 ^ R9 ) = R8 or ( R9 ^ R8 ) = R8) implies R9 = ( {} )))))
proof
let R1 being set;
let R8 being (Element of ( R1 ^omega ));
let R9 being (Element of ( R1 ^omega ));
assume L29: (( R8 ^ R9 ) = R8 or ( R9 ^ R8 ) = R8);
per cases  by L29;
suppose L30: ( R8 ^ R9 ) = R8;

L31: ( len ( R8 ^ R9 ) ) = ( ( len R8 ) + ( len R9 ) ) by AFINSQ_1:17;
thus L32: thesis by L31 , L30;
end;
suppose L33: ( R9 ^ R8 ) = R8;

L34: ( len ( R9 ^ R8 ) ) = ( ( len R9 ) + ( len R8 ) ) by AFINSQ_1:17;
thus L35: thesis by L34 , L33;
end;
end;
