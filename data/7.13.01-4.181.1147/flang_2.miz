let R1 being set;
let R5 being (Subset of ( R1 ^omega ));
let R15 being Nat;
thus L55: (( <%> R1 ) in ( R5 |^ R15 ) implies (R15 = ( 0 ) or ( <%> R1 ) in R5)) by FLANG_1:31;
assume L56: (R15 = ( 0 ) or ( <%> R1 ) in R5);
per cases  by L56;
suppose L57: R15 = ( 0 );

L58: ( R5 |^ R15 ) = { ( <%> R1 ) } by L57 , FLANG_1:29;
thus L59: thesis by L58 , ZFMISC_1:31;
