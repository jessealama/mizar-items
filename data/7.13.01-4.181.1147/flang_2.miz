let R11 being Nat;
let R14 being Nat;
let R15 being Nat;
assume L181: (R14 <= R11 & R11 <= R15);
L182: (for R2 being set holds (R2 in ( R5 |^ R11 ) implies R2 in ( R5 |^ (R14 , R15) ))) by L181 , L167;
