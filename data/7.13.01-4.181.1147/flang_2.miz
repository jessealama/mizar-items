assume L169: R2 in ( R5 |^ (R14 , R15) );
consider R4 being set such that L170: R2 in R4 and L171: R4 in { R6 where R6 is (Subset of ( R1 ^omega )) : (ex R11 being Nat st (R14 <= R11 & R11 <= R15 & R6 = ( R5 |^ R11 ))) } by L169 , TARSKI:def 4;
L172: R4 in { R6 where R6 is (Subset of ( R1 ^omega )) : S2[ R6 ] } by L171;
L173: S2[ R4 ] from CARD_FIL:sch 1(L172);
thus L174: thesis by L173 , L170;
end;

given R11 being Nat such that
L175: (R14 <= R11 & R11 <= R15 & R2 in ( R5 |^ R11 ));

defpred S3[ set ] means (ex R11 being Nat st (R14 <= R11 & R11 <= R15 & $1 = ( R5 |^ R11 )));
consider R6 being (Subset of ( R1 ^omega )) such that L176: R2 in R6 and L177: S3[ R6 ] by L175;
reconsider D3 = { R7 where R7 is (Subset of ( R1 ^omega )) : S3[ R7 ] } as (Subset-Family of ( R1 ^omega )) from DOMAIN_1:sch 7;
L178: R6 in D3 by L177;
thus L179: thesis by L178 , L176 , TARSKI:def 4;
end;
theorem
L180: (for R1 being set holds (for R5 being (Subset of ( R1 ^omega )) holds (for R11 being Nat holds (for R14 being Nat holds (for R15 being Nat holds ((R14 <= R11 & R11 <= R15) implies ( R5 |^ R11 ) c= ( R5 |^ (R14 , R15) )))))))
