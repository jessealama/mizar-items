theorem
L1: (for B1 being 1-sorted holds (for B2 , B3 being (Subset of B1) holds (B2 meets ( B3 ` ) iff ( B2 \ B3 ) <> ( {} ))))
proof
let C1 being 1-sorted;
let C2 , C3 being (Subset of C1);
thus L2:now
assume L3: C2 meets ( C3 ` );
L4: ( C2 /\ ( C3 ` ) ) <> ( {} ) by L3 , XBOOLE_0:def 7;
thus L5: ( C2 \ C3 ) <> ( {} ) by L4 , SUBSET_1:13;
end;
assume L6: ( C2 \ C3 ) <> ( {} );
L7: ( C2 /\ ( C3 ` ) ) <> ( {} ) by L6 , SUBSET_1:13;
thus L8: thesis by L7 , XBOOLE_0:def 7;
end;
theorem
L9: (for B4 being 1-sorted holds (B4 is  countable iff ( card ( [#] B4 ) ) c= ( omega )))
proof
