theorem
L148: (for B21 being  interval non  empty RelStr holds (for B22 being set holds  ex_sup_of B22 , B21))
proof
let C51 being  interval non  empty RelStr;
let C52 being set;
consider C53 , C54 being  real number such that L149: C53 <= C54 and L150: (the carrier of C51) = [. C53 , C54 .] by L3;
reconsider D16 = C53 , D17 = C54 as Real by XREAL_0:def 1;
reconsider D18 = ( C52 /\ [. D16 , D17 .] ) as (Subset of ( REAL ));
L151: [. D16 , D17 .] is non  empty  closed_interval by L149 , MEASURE5:14;
L152: [. D16 , D17 .] is  bounded_above by L151 , INTEGRA1:3;
L153: D18 is  bounded_above by L152 , XBOOLE_1:17 , XXREAL_2:43;
L154: ( C52 /\ [. D16 , D17 .] ) c= [. D16 , D17 .] by XBOOLE_1:17;
L155: (ex B23 being (Element of C51) st (D18 is_<=_than B23 & (for B24 being (Element of C51) holds (D18 is_<=_than B24 implies B23 <<= B24))))
proof
per cases ;
suppose L156: D18 is  empty;

reconsider D19 = D16 as (Element of C51) by L149 , L150 , XXREAL_1:1;
take D19;
thus L157: D18 is_<=_than D19 by L156 , YELLOW_0:6;
let C55 being (Element of C51);
L158: D19 <= C55 by L150 , XXREAL_1:1;
thus L159: thesis by L158 , L53;
end;
suppose L160: D18 is non  empty;

set D20 = the (Element of D18);
L161: D20 in D18 by L160;
reconsider D21 = D20 as Real by L161;
L162: D16 <= D21 by L154 , L161 , XXREAL_1:1;
L163: D21 <= ( upper_bound D18 ) by L153 , L160 , SEQ_4:def 1;
L164: D16 <= ( upper_bound D18 ) by L163 , L162 , XXREAL_0:2;
L165: ( upper_bound [. D16 , D17 .] ) = D17 by L149 , JORDAN5A:19;
L166: ( upper_bound D18 ) <= D17 by L165 , L152 , L160 , SEQ_4:48 , XBOOLE_1:17;
reconsider D22 = ( upper_bound D18 ) as (Element of C51) by L166 , L150 , L164 , XXREAL_1:1;
L167: (for B25 being (Element of C51) holds (D18 is_<=_than B25 implies D22 <<= B25))
proof
let C56 being (Element of C51);
assume L168: D18 is_<=_than C56;
L169: (for B26 being  real number holds (B26 in D18 implies B26 <= C56))
proof
let C57 being  real number;
assume L170: C57 in D18;
reconsider D23 = C57 as (Element of C51) by L170 , L150 , XBOOLE_0:def 4;
L171: D23 <<= C56 by L168 , L170 , LATTICE3:def 9;
thus L172: thesis by L171 , L53;
end;
L173: ( upper_bound D18 ) <= C56 by L169 , L160 , SEQ_4:144;
thus L174: thesis by L173 , L53;
end;
take D22;
L175: (for B27 being (Element of C51) holds (B27 in D18 implies B27 <<= D22))
proof
let C58 being (Element of C51);
assume L176: C58 in D18;
L177: C58 <= ( upper_bound D18 ) by L176 , L153 , SEQ_4:def 1;
thus L178: thesis by L177 , L53;
end;
thus L179: thesis by L175 , L167 , LATTICE3:def 9;
end;
end;
L181:  ex_sup_of D18 , C51 by L155 , YELLOW_0:15;
thus L182: thesis by L181 , L150 , YELLOW_0:50;
end;
