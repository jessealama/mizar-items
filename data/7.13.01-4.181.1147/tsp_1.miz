L113:
now
let C45 being (Subset of C41);
assume L114: C45 is  open;
L115: (C45 = ( {} ) or C45 = (the carrier of C41)) by L114 , TDLAT_3:18;
assume L116: D22 in C45;
thus L117: D23 in C45 by L116 , L115;
end;
thus L118: ((not (ex B73 being (Subset of C41) st (B73 is  open & D22 in B73 & (not D23 in B73)))) & (not (ex B74 being (Subset of C41) st (B74 is  open & (not D22 in B74) & D23 in B74)))) by L113 , L108;
end;
thus L119: thesis by L105 , L70;
