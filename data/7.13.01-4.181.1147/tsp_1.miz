registration
cluster  discrete ->  T_0 for non  empty non  empty non  empty non  empty TopSpace;
coherence
proof
let C46 being non  empty TopSpace;
assume L123: C46 is  discrete;
L124:
now
let C47 , C48 being (Point of C46);
assume L125: C47 <> C48;
L126:
now
