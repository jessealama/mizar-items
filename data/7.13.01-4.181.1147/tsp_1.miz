environ

 vocabularies PRE_TOPC, STRUCT_0, TARSKI, SUBSET_1, RCOMP_1, XBOOLE_0,
      ZFMISC_1, TDLAT_3, NATTRA_1, SETFAM_1, TSP_1;
 notations TARSKI, XBOOLE_0, SUBSET_1, DOMAIN_1, STRUCT_0, PRE_TOPC, BORSUK_1,
      TSEP_1, TDLAT_3, TEX_2, TEX_4;
 constructors REALSET2, BORSUK_1, TSEP_1, TDLAT_3, TEX_2, TEX_4, T_0TOPSP;
 registrations XBOOLE_0, STRUCT_0, TOPS_1, TDLAT_3, TEX_1, TEX_2, SUBSET_1,
      ZFMISC_1;
 requirements SUBSET, BOOLE;
 definitions T_0TOPSP, SUBSET_1, STRUCT_0;
 theorems ZFMISC_1, SUBSET_1, PRE_TOPC, TOPS_1, TSEP_1, TDLAT_3, TEX_1, TEX_2,
      TEX_4, TARSKI, T_0TOPSP, STRUCT_0, XBOOLE_0, XBOOLE_1;

begin
definition
let C1 being TopStruct;
redefine mode SubSpace of C1
means :L1: ((the carrier of it) c= (the carrier of C1) & (for B1 being (Subset of it) holds (B1 is  open iff (ex B2 being (Subset of C1) st (B2 is  open & B1 = ( B2 /\ (the carrier of it) ))))));
compatibility
proof
let C2 being TopStruct;
L2: (( [#] C2 ) = (the carrier of C2) & ( [#] C1 ) = (the carrier of C1));
thus L3: (C2 is (SubSpace of C1) implies ((the carrier of C2) c= (the carrier of C1) & (for B3 being (Subset of C2) holds (B3 is  open iff (ex B4 being (Subset of C1) st (B4 is  open & B3 = ( B4 /\ (the carrier of C2) )))))))
proof
assume L4: C2 is (SubSpace of C1);
thus L5: (the carrier of C2) c= (the carrier of C1) by L4 , L2 , PRE_TOPC:def 4;
thus L6: (for B5 being (Subset of C2) holds (B5 is  open iff (ex B6 being (Subset of C1) st (B6 is  open & B5 = ( B6 /\ (the carrier of C2) )))))
proof
let C3 being (Subset of C2);
thus L7: (C3 is  open implies (ex B7 being (Subset of C1) st (B7 is  open & C3 = ( B7 /\ (the carrier of C2) ))))
proof
assume L8: C3 is  open;
L9: C3 in (the topology of C2) by L8 , PRE_TOPC:def 2;
consider C4 being (Subset of C1) such that L10: C4 in (the topology of C1) and L11: C3 = ( C4 /\ ( [#] C2 ) ) by L9 , L4 , PRE_TOPC:def 4;
reconsider D1 = C4 as (Subset of C1);
take D1;
thus L12: D1 is  open by L10 , PRE_TOPC:def 2;
thus L13: thesis by L11;
end;

given C5 being (Subset of C1) such that
L14: C5 is  open
and
L15: C3 = ( C5 /\ (the carrier of C2) );

L16:
now
take D2 = C5;
thus L17: D2 in (the topology of C1) by L14 , PRE_TOPC:def 2;
thus L18: C3 = ( D2 /\ ( [#] C2 ) ) by L15;
end;
L19: C3 in (the topology of C2) by L16 , L4 , PRE_TOPC:def 4;
thus L20: thesis by L19 , PRE_TOPC:def 2;
end;

end;

assume that
L7: (the carrier of C2) c= (the carrier of C1)
and
L8: (for B8 being (Subset of C2) holds (B8 is  open iff (ex B9 being (Subset of C1) st (B9 is  open & B8 = ( B9 /\ (the carrier of C2) )))));
L9: (for B10 being (Subset of C2) holds (B10 in (the topology of C2) iff (ex B11 being (Subset of C1) st (B11 in (the topology of C1) & B10 = ( B11 /\ ( [#] C2 ) )))))
proof
let C6 being (Subset of C2);
reconsider D3 = C6 as (Subset of C2);
thus L10: (C6 in (the topology of C2) implies (ex B12 being (Subset of C1) st (B12 in (the topology of C1) & C6 = ( B12 /\ ( [#] C2 ) ))))
proof
reconsider D4 = C6 as (Subset of C2);
assume L11: C6 in (the topology of C2);
L12: D4 is  open by L11 , PRE_TOPC:def 2;
consider C7 being (Subset of C1) such that L13: C7 is  open and L14: C6 = ( C7 /\ (the carrier of C2) ) by L12 , L8;
take C7;
thus L15: C7 in (the topology of C1) by L13 , PRE_TOPC:def 2;
thus L16: thesis by L14;
end;

given C8 being (Subset of C1) such that
L17: C8 in (the topology of C1)
and
L18: C6 = ( C8 /\ ( [#] C2 ) );

reconsider D5 = C8 as (Subset of C1);
L19:
now
take D6 = D5;
thus L20: D6 is  open by L17 , PRE_TOPC:def 2;
thus L21: C6 = ( D6 /\ (the carrier of C2) ) by L18;
end;
L22: D3 is  open by L19 , L8;
thus L23: thesis by L22 , PRE_TOPC:def 2;
end;
L24: ( [#] C2 ) c= ( [#] C1 ) by L7;
thus L25: thesis by L24 , L9 , PRE_TOPC:def 4;
end;
end;
theorem
L27: (for B13 being TopStruct holds (for B14 being (SubSpace of B13) holds (for B15 being (Subset of B13) holds (B15 is  open implies (ex B16 being (Subset of B14) st (B16 is  open & B16 = ( B15 /\ (the carrier of B14) )))))))
proof
let C9 being TopStruct;
let C10 being (SubSpace of C9);
let C11 being (Subset of C9);
assume L28: C11 is  open;
L29: (( [#] C10 ) = (the carrier of C10) & ( [#] C9 ) = (the carrier of C9));
reconsider D7 = (the carrier of C10) as (Subset of C9) by L29 , PRE_TOPC:def 4;
reconsider D8 = ( C11 /\ D7 ) as (Subset of C10) by XBOOLE_1:17;
take D8;
thus L30: D8 is  open by L28 , L1;
thus L31: thesis;
end;
definition
let C12 being TopStruct;
redefine mode SubSpace of C12
means :L32: ((the carrier of it) c= (the carrier of C12) & (for B17 being (Subset of it) holds (B17 is  closed iff (ex B18 being (Subset of C12) st (B18 is  closed & B17 = ( B18 /\ (the carrier of it) ))))));
compatibility
proof
let C13 being TopStruct;
L33: (( [#] C13 ) = (the carrier of C13) & ( [#] C12 ) = (the carrier of C12));
thus L34: (C13 is (SubSpace of C12) implies ((the carrier of C13) c= (the carrier of C12) & (for B19 being (Subset of C13) holds (B19 is  closed iff (ex B20 being (Subset of C12) st (B20 is  closed & B19 = ( B20 /\ (the carrier of C13) )))))))
proof
assume L35: C13 is (SubSpace of C12);
thus L36: (the carrier of C13) c= (the carrier of C12) by L35 , L33 , PRE_TOPC:def 4;
L37: ( [#] C13 ) c= ( [#] C12 ) by L35 , PRE_TOPC:def 4;
L38: ( ( [#] C13 ) \ ( [#] C12 ) ) = ( {} ) by L37 , XBOOLE_1:37;
thus L39: (for B21 being (Subset of C13) holds (B21 is  closed iff (ex B22 being (Subset of C12) st (B22 is  closed & B21 = ( B22 /\ (the carrier of C13) )))))
proof
let C14 being (Subset of C13);
set D9 = ( ( [#] C13 ) \ C14 );
thus L40: (C14 is  closed implies (ex B23 being (Subset of C12) st (B23 is  closed & C14 = ( B23 /\ (the carrier of C13) ))))
proof
assume L41: C14 is  closed;
L42: ( ( [#] C13 ) \ C14 ) is  open by L41 , PRE_TOPC:def 3;
consider C15 being (Subset of C12) such that L43: C15 is  open and L44: ( ( [#] C13 ) \ C14 ) = ( C15 /\ (the carrier of C13) ) by L42 , L35 , L1;
take D10 = ( ( [#] C12 ) \ C15 );
L45: C15 = ( ( [#] C12 ) \ D10 ) by PRE_TOPC:3;
thus L46: D10 is  closed by L45 , L43 , PRE_TOPC:def 3;
L47: C14 = ( ( [#] C13 ) \ ( C15 /\ (the carrier of C13) ) ) by L44 , PRE_TOPC:3
.= ( ( ( [#] C13 ) \ C15 ) \/ ( ( [#] C13 ) \ (the carrier of C13) ) ) by XBOOLE_1:54
.= ( ( ( [#] C13 ) \ C15 ) \/ ( {} ) ) by XBOOLE_1:37
.= ( ( ( [#] C13 ) \ ( [#] C12 ) ) \/ ( ( [#] C13 ) /\ D10 ) ) by L45 , XBOOLE_1:52
.= ( ( [#] C13 ) /\ D10 ) by L38;
thus L48: thesis by L47;
end;

given C16 being (Subset of C12) such that
L49: C16 is  closed
and
L50: C14 = ( C16 /\ (the carrier of C13) );

L51:
now
take D11 = ( ( [#] C12 ) \ C16 );
L52: C16 = ( ( [#] C12 ) \ D11 ) by PRE_TOPC:3;
thus L53: D11 is  open by L49 , PRE_TOPC:def 3;
L54: D9 = ( ( ( [#] C13 ) \ C16 ) \/ ( ( [#] C13 ) \ (the carrier of C13) ) ) by L50 , XBOOLE_1:54
.= ( ( ( [#] C13 ) \ C16 ) \/ ( {} ) ) by XBOOLE_1:37
.= ( ( ( [#] C13 ) \ ( [#] C12 ) ) \/ ( ( [#] C13 ) /\ D11 ) ) by L52 , XBOOLE_1:52
.= ( ( [#] C13 ) /\ D11 ) by L38;
thus L55: D9 = ( D11 /\ (the carrier of C13) ) by L54;
end;
L56: D9 is  open by L51 , L35 , L1;
thus L57: thesis by L56 , PRE_TOPC:def 3;
end;

end;

assume that
L40: (the carrier of C13) c= (the carrier of C12)
and
L41: (for B24 being (Subset of C13) holds (B24 is  closed iff (ex B25 being (Subset of C12) st (B25 is  closed & B24 = ( B25 /\ (the carrier of C13) )))));
L42: ( ( [#] C13 ) \ ( [#] C12 ) ) = ( {} ) by L40 , XBOOLE_1:37;
L43: (for B26 being (Subset of C13) holds (B26 is  open iff (ex B27 being (Subset of C12) st (B27 is  open & B26 = ( B27 /\ (the carrier of C13) )))))
proof
let C17 being (Subset of C13);
set D12 = ( ( [#] C13 ) \ C17 );
thus L44: (C17 is  open implies (ex B28 being (Subset of C12) st (B28 is  open & C17 = ( B28 /\ (the carrier of C13) ))))
proof
set D13 = ( ( [#] C13 ) \ C17 );
L45: C17 = ( ( [#] C13 ) \ D13 ) by PRE_TOPC:3;
assume L46: C17 is  open;
L47: D13 is  closed by L46 , L45 , PRE_TOPC:def 3;
consider C18 being (Subset of C12) such that L48: C18 is  closed and L49: D13 = ( C18 /\ (the carrier of C13) ) by L47 , L41;
take D14 = ( ( [#] C12 ) \ C18 );
thus L50: D14 is  open by L48 , PRE_TOPC:def 3;
L51: C18 = ( ( [#] C12 ) \ D14 ) by PRE_TOPC:3;
L52: C17 = ( ( ( [#] C13 ) \ C18 ) \/ ( ( [#] C13 ) \ (the carrier of C13) ) ) by L45 , L49 , XBOOLE_1:54
.= ( ( ( [#] C13 ) \ C18 ) \/ ( {} ) ) by XBOOLE_1:37
.= ( ( ( [#] C13 ) \ ( [#] C12 ) ) \/ ( ( [#] C13 ) /\ D14 ) ) by L51 , XBOOLE_1:52
.= ( ( [#] C13 ) /\ D14 ) by L42;
thus L53: thesis by L52;
end;

given C19 being (Subset of C12) such that
L54: C19 is  open
and
L55: C17 = ( C19 /\ (the carrier of C13) );

L56:
now
take D15 = ( ( [#] C12 ) \ C19 );
L57: C19 = ( ( [#] C12 ) \ D15 ) by PRE_TOPC:3;
thus L58: D15 is  closed by L57 , L54 , PRE_TOPC:def 3;
L59: D12 = ( ( ( [#] C13 ) \ C19 ) \/ ( ( [#] C13 ) \ (the carrier of C13) ) ) by L55 , XBOOLE_1:54
.= ( ( ( [#] C13 ) \ C19 ) \/ ( {} ) ) by XBOOLE_1:37
.= ( ( ( [#] C13 ) \ ( [#] C12 ) ) \/ ( ( [#] C13 ) /\ D15 ) ) by L57 , XBOOLE_1:52
.= ( ( [#] C13 ) /\ D15 ) by L42;
thus L60: D12 = ( D15 /\ (the carrier of C13) ) by L59;
end;
L61: (C17 = ( ( [#] C13 ) \ D12 ) & D12 is  closed) by L56 , L41 , PRE_TOPC:3;
thus L62: thesis by L61 , PRE_TOPC:def 3;
end;
thus L63: C13 is (SubSpace of C12) by L43 , L40 , L1;
end;
end;
theorem
L65: (for B29 being TopStruct holds (for B30 being (SubSpace of B29) holds (for B31 being (Subset of B29) holds (B31 is  closed implies (ex B32 being (Subset of B30) st (B32 is  closed & B32 = ( B31 /\ (the carrier of B30) )))))))
proof
let C20 being TopStruct;
let C21 being (SubSpace of C20);
let C22 being (Subset of C20);
assume L66: C22 is  closed;
L67: (( [#] C21 ) = (the carrier of C21) & ( [#] C20 ) = (the carrier of C20));
reconsider D16 = (the carrier of C21) as (Subset of C20) by L67 , PRE_TOPC:def 4;
reconsider D17 = ( C22 /\ D16 ) as (Subset of C21) by XBOOLE_1:17;
take D17;
thus L68: D17 is  closed by L66 , L32;
thus L69: thesis;
end;
begin
definition
let C23 being TopStruct;
redefine attr C23 is  T_0
means
:L70: (C23 is  empty or (for B33 , B34 being (Point of C23) holds (B33 <> B34 implies ((ex B35 being (Subset of C23) st (B35 is  open & B33 in B35 & (not B34 in B35))) or (ex B36 being (Subset of C23) st (B36 is  open & (not B33 in B36) & B34 in B36))))));
compatibility
proof
thus L71:now
assume L72: C23 is  T_0;
assume L73: (not C23 is  empty);
let C24 , C25 being (Point of C23);
assume L74: C24 <> C25;
L75: (ex B37 being (Subset of C23) st (B37 is  open & ((C24 in B37 & (not C25 in B37)) or (C25 in B37 & (not C24 in B37))))) by L74 , L72 , L73 , T_0TOPSP:def 7;
thus L76: ((ex B38 being (Subset of C23) st (B38 is  open & C24 in B38 & (not C25 in B38))) or (ex B39 being (Subset of C23) st (B39 is  open & (not C24 in B39) & C25 in B39))) by L75;
end;
assume L77: (C23 is  empty or (for B40 , B41 being (Point of C23) holds (B40 <> B41 implies ((ex B42 being (Subset of C23) st (B42 is  open & B40 in B42 & (not B41 in B42))) or (ex B43 being (Subset of C23) st (B43 is  open & (not B40 in B43) & B41 in B43))))));
assume L78: (not C23 is  empty);
let C26 , C27 being (Point of C23);
assume L79: C26 <> C27;
L80: ((ex B44 being (Subset of C23) st (B44 is  open & C26 in B44 & (not C27 in B44))) or (ex B45 being (Subset of C23) st (B45 is  open & (not C26 in B45) & C27 in B45))) by L79 , L77 , L78;
thus L81: thesis by L80;
end;
end;
definition
let C28 being TopStruct;
redefine attr C28 is  T_0
means
:L83: (C28 is  empty or (for B46 , B47 being (Point of C28) holds (B46 <> B47 implies ((ex B48 being (Subset of C28) st (B48 is  closed & B46 in B48 & (not B47 in B48))) or (ex B49 being (Subset of C28) st (B49 is  closed & (not B46 in B49) & B47 in B49))))));
compatibility
proof
thus L84: (C28 is  T_0 implies (C28 is  empty or (for B50 , B51 being (Point of C28) holds (B50 <> B51 implies ((ex B52 being (Subset of C28) st (B52 is  closed & B50 in B52 & (not B51 in B52))) or (ex B53 being (Subset of C28) st (B53 is  closed & (not B50 in B53) & B51 in B53)))))))
proof
assume L85: (C28 is  empty or (for B54 , B55 being (Point of C28) holds (B54 <> B55 implies ((ex B56 being (Subset of C28) st (B56 is  open & B54 in B56 & (not B55 in B56))) or (ex B57 being (Subset of C28) st (B57 is  open & (not B54 in B57) & B55 in B57))))));
assume L86: (not C28 is  empty);
let C29 , C30 being (Point of C28);
assume L87: C29 <> C30;
thus L88:now
per cases  by L85 , L86 , L87;
suppose L89: (ex B58 being (Subset of C28) st (B58 is  open & C29 in B58 & (not C30 in B58)));

consider C31 being (Subset of C28) such that L90: C31 is  open and L91: C29 in C31 and L92: (not C30 in C31) by L89;
L93:
now
take D18 = ( C31 ` );
L94: C31 = ( ( [#] C28 ) \ D18 ) by PRE_TOPC:3;
thus L95: D18 is  closed by L94 , L90 , PRE_TOPC:def 3;
thus L96: (not C29 in D18) by L91 , XBOOLE_0:def 5;
thus L97: C30 in D18 by L86 , L92 , SUBSET_1:29;
end;
thus L98: thesis by L93;
end;
suppose L99: (ex B59 being (Subset of C28) st (B59 is  open & (not C29 in B59) & C30 in B59));

consider C32 being (Subset of C28) such that L100: C32 is  open and L101: (not C29 in C32) and L102: C30 in C32 by L99;
L103:
now
take D19 = ( C32 ` );
L104: C32 = ( ( [#] C28 ) \ D19 ) by PRE_TOPC:3;
thus L105: D19 is  closed by L104 , L100 , PRE_TOPC:def 3;
thus L106: C29 in D19 by L86 , L101 , SUBSET_1:29;
thus L107: (not C30 in D19) by L102 , XBOOLE_0:def 5;
end;
thus L108: thesis by L103;
end;
end;
end;

assume L89: (C28 is  empty or (for B60 , B61 being (Point of C28) holds (B60 <> B61 implies ((ex B62 being (Subset of C28) st (B62 is  closed & B60 in B62 & (not B61 in B62))) or (ex B63 being (Subset of C28) st (B63 is  closed & (not B60 in B63) & B61 in B63))))));
L90: ((not C28 is  empty) implies (for B64 , B65 being (Point of C28) holds (B64 <> B65 implies ((ex B66 being (Subset of C28) st (B66 is  open & B64 in B66 & (not B65 in B66))) or (ex B67 being (Subset of C28) st (B67 is  open & (not B64 in B67) & B65 in B67))))))
proof
assume L91: (not C28 is  empty);
let C33 , C34 being (Point of C28);
assume L92: C33 <> C34;
thus L93:now
per cases  by L89 , L91 , L92;
suppose L94: (ex B68 being (Subset of C28) st (B68 is  closed & C33 in B68 & (not C34 in B68)));

consider C35 being (Subset of C28) such that L95: C35 is  closed and L96: C33 in C35 and L97: (not C34 in C35) by L94;
L98:
now
take D20 = ( C35 ` );
L99: D20 = ( ( [#] C28 ) \ C35 );
thus L100: D20 is  open by L99 , L95 , PRE_TOPC:def 3;
thus L101: (not C33 in D20) by L96 , XBOOLE_0:def 5;
thus L102: C34 in D20 by L91 , L97 , SUBSET_1:29;
end;
thus L103: thesis by L98;
end;
suppose L104: (ex B69 being (Subset of C28) st (B69 is  closed & (not C33 in B69) & C34 in B69));

consider C36 being (Subset of C28) such that L105: C36 is  closed and L106: (not C33 in C36) and L107: C34 in C36 by L104;
L108:
now
take D21 = ( C36 ` );
L109: D21 = ( ( [#] C28 ) \ C36 );
thus L110: D21 is  open by L109 , L105 , PRE_TOPC:def 3;
thus L111: C33 in D21 by L91 , L106 , SUBSET_1:29;
thus L112: (not C34 in D21) by L107 , XBOOLE_0:def 5;
end;
thus L113: thesis by L108;
end;
end;
end;
thus L94: thesis by L90 , L70;
end;
end;
registration
cluster  trivial ->  T_0 for non  empty non  empty non  empty non  empty TopStruct;
coherence
proof
let C37 being non  empty TopStruct;
assume L96: C37 is  trivial;
consider C38 being (Point of C37) such that L97: (the carrier of C37) = { C38 } by L96 , TEX_1:def 1;
L98:
now
let C39 , C40 being (Point of C37);
assume L99: C39 <> C40;
L100: C39 = C38 by L97 , TARSKI:def 1;
thus L101: ((ex B70 being (Subset of C37) st (B70 is  open & C39 in B70 & (not C40 in B70))) or (ex B71 being (Subset of C37) st (B71 is  open & (not C39 in B71) & C40 in B71))) by L100 , L97 , L99 , TARSKI:def 1;
end;
thus L102: thesis by L98 , L70;
end;
end;
L104: (for B72 being  anti-discrete non  trivial TopStruct holds B72 is non  T_0)
proof
let C41 being  anti-discrete non  trivial TopStruct;
L105:
now
consider C42 , C43 being (Point of C41) such that L106: C42 <> C43 by STRUCT_0:def 10;
take D22 = C42;
take D23 = C43;
thus L107: D22 <> D23 by L106;
L108:
now
