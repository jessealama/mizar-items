theorem
L78: (for R5 being  real number holds (for R9 being (Point of ( TOP-REAL 2 )) holds (for R10 being (Point of ( TOP-REAL 2 )) holds ((( R9 `1 ) <= R5 & R5 <= ( R10 `1 )) implies ( LSeg (R9 , R10) ) meets ( Vertical_Line R5 )))))
proof
let R5 being  real number;
let R9 being (Point of ( TOP-REAL 2 ));
let R10 being (Point of ( TOP-REAL 2 ));
assume that
L79: ( R9 `1 ) <= R5
and
L80: R5 <= ( R10 `1 );
set D3 = ( R9 `1 );
set D4 = ( R10 `1 );
set D5 = ( ( R5 - D3 ) / ( D4 - D3 ) );
set D6 = ( ( ( 1 - D5 ) * R9 ) + ( D5 * R10 ) );
L81: ( D3 - D3 ) <= ( R5 - D3 ) by L79 , XREAL_1:9;
L82: ( R5 - D3 ) <= ( D4 - D3 ) by L80 , XREAL_1:9;
L83: D5 <= 1 by L82 , L81 , XREAL_1:183;
L84: D6 in ( LSeg (R9 , R10) ) by L83 , L81 , L82;
per cases ;
suppose L85: D3 <> D4;

L86: ( D4 - D3 ) <> ( 0 ) by L85;
L87: ( D6 `1 ) = ( ( ( 1 - D5 ) * D3 ) + ( D5 * D4 ) ) by TOPREAL9:41
.= ( D3 + ( D5 * ( D4 - D3 ) ) )
.= ( D3 + ( R5 - D3 ) ) by L86 , XCMPLX_1:87;
L88: D6 in ( Vertical_Line R5 ) by L87 , JORDAN6:31;
thus L89: thesis by L88 , L84 , XBOOLE_0:3;
end;
suppose L90: D3 = D4;

L91: R9 in ( LSeg (R9 , R10) ) by RLTOPSP1:68;
L92: D3 = R5 by L79 , L80 , L90 , XXREAL_0:1;
L93: R9 in ( Vertical_Line R5 ) by L92 , JORDAN6:31;
thus L94: thesis by L93 , L91 , XBOOLE_0:3;
end;
end;
