begin
L1: (for B1 being non  empty RLSStruct holds (for B2 , B3 being (Subset of B1) holds (for B4 , B5 being (VECTOR of B1) holds ((B4 in B2 & B5 in B3) implies ( B4 - B5 ) in ( B2 - B3 )))))
proof
let C1 being non  empty RLSStruct;
let C2 , C3 being (Subset of C1);
let C4 , C5 being (VECTOR of C1);
L2: ( C2 - C3 ) = { ( B6 - B7 ) where B6 , B7 is (VECTOR of C1) : (B6 in C2 & B7 in C3) } by RUSUB_5:def 2;
thus L3: thesis by L2;
end;
theorem
L4: (for B8 being RealLinearSpace holds (for B9 , B10 being  circled (Subset of B8) holds ( B9 - B10 ) is  circled))
proof
let C6 being RealLinearSpace;
let C7 , C8 being  circled (Subset of C6);
L5: ( C7 - C8 ) = { ( B11 - B12 ) where B11 , B12 is (VECTOR of C6) : (B11 in C7 & B12 in C8) } by RUSUB_5:def 2;
let C9 being Real;
assume L6: ( abs C9 ) <= 1;
L7: (( C9 * C7 ) c= C7 & ( C9 * C8 ) c= C8) by L6 , RLTOPSP1:def 6;
let C10 being set;
assume L8: C10 in ( C9 * ( C7 - C8 ) );
L9: ( C9 * ( C7 - C8 ) ) = { ( C9 * B13 ) where B13 is (VECTOR of C6) : B13 in ( C7 - C8 ) } by CONVEX1:def 1;
consider C11 being (VECTOR of C6) such that L10: C10 = ( C9 * C11 ) and L11: C11 in ( C7 - C8 ) by L9 , L8;
consider C12 , C13 being (VECTOR of C6) such that L12: C11 = ( C12 - C13 ) and L13: (C12 in C7 & C13 in C8) by L5 , L11;
L14: (( C9 * C12 ) in ( C9 * C7 ) & ( C9 * C13 ) in ( C9 * C8 )) by L13 , RLTOPSP1:1;
L15: C10 = ( ( C9 * C12 ) - ( C9 * C13 ) ) by L10 , L12 , RLVECT_1:34;
thus L16: thesis by L15 , L7 , L14 , L1;
end;
registration
let C14 being RealLinearSpace;
let C15 , C16 being  circled (Subset of C14);
cluster ( C15 - C16 ) ->  circled;
coherence by L4;
end;
definition
let C17 being non  empty RLSStruct;
let C18 being (Subset of C17);
redefine attr C18 is  circled
means
:L18: (for B14 being (VECTOR of C17) holds (for B15 being Real holds ((( abs B15 ) <= 1 & B14 in C18) implies ( B15 * B14 ) in C18)));
compatibility
proof
thus L19: (C18 is  circled implies (for B16 being (VECTOR of C17) holds (for B17 being Real holds ((( abs B17 ) <= 1 & B16 in C18) implies ( B17 * B16 ) in C18))))
proof
assume L20: C18 is  circled;
let C19 being (VECTOR of C17);
let C20 being Real;
assume L21: ( abs C20 ) <= 1;
L22: ( C20 * C18 ) c= C18 by L21 , L20 , RLTOPSP1:def 6;
assume L23: C19 in C18;
L24: ( C20 * C19 ) in ( C20 * C18 ) by L23 , RLTOPSP1:1;
thus L25: thesis by L24 , L22;
end;

assume L26: (for B18 being (VECTOR of C17) holds (for B19 being Real holds ((( abs B19 ) <= 1 & B18 in C18) implies ( B19 * B18 ) in C18)));
let C21 being Real;
assume L27: ( abs C21 ) <= 1;
L28: (for B20 being (Element of C17) holds (B20 in ( C21 * C18 ) implies B20 in C18))
proof
let C22 being (Element of C17);
assume L29: C22 in ( C21 * C18 );
consider C23 being (Element of C17) such that L30: C22 = C23 and L31: C23 in ( C21 * C18 ) by L29;
L32: C23 in { ( C21 * B21 ) where B21 is (Element of C17) : B21 in C18 } by L31 , CONVEX1:def 1;
L33: (ex B22 being (Element of C17) st (C23 = ( C21 * B22 ) & B22 in C18)) by L32;
thus L34: thesis by L33 , L26 , L27 , L30;
end;
thus L35: thesis by L28 , SUBSET_1:2;
end;
end;
theorem
L37: (for B23 being RealLinearSpace holds (for B24 being (Subset of B23) holds (for B25 being Real holds (B24 is  circled implies ( B25 * B24 ) is  circled))))
proof
let C24 being RealLinearSpace;
let C25 being (Subset of C24);
let C26 being Real;
assume L38: C25 is  circled;
L39: (for B26 being (VECTOR of C24) holds (for B27 being Real holds ((( abs B27 ) <= 1 & B26 in ( C26 * C25 )) implies ( B27 * B26 ) in ( C26 * C25 ))))
proof
let C27 being (VECTOR of C24);
let C28 being Real;
assume that
L40: ( abs C28 ) <= 1
and
L41: C27 in ( C26 * C25 );
L42: C27 in { ( C26 * B28 ) where B28 is (Element of C24) : B28 in C25 } by L41 , CONVEX1:def 1;
consider C29 being (Element of C24) such that L43: C27 = ( C26 * C29 ) and L44: C29 in C25 by L42;
L45: ( C28 * C27 ) = ( ( C26 * C28 ) * C29 ) by L43 , RLVECT_1:def 7
.= ( C26 * ( C28 * C29 ) ) by RLVECT_1:def 7;
L46: ( C28 * C29 ) in C25 by L38 , L40 , L44 , L18;
thus L47: thesis by L46 , L45 , RLTOPSP1:1;
end;
thus L48: thesis by L39 , L18;
end;
theorem
L49: (for B29 being RealLinearSpace holds (for B30 , B31 being (Subset of B29) holds (for B32 , B33 being Real holds ((B30 is  circled & B31 is  circled) implies ( ( B32 * B30 ) + ( B33 * B31 ) ) is  circled))))
proof
let C30 being RealLinearSpace;
let C31 , C32 being (Subset of C30);
let C33 , C34 being Real;
assume that
L50: C31 is  circled
and
L51: C32 is  circled;
let C35 being (VECTOR of C30);
let C36 being Real;
assume that
L52: ( abs C36 ) <= 1
and
L53: C35 in ( ( C33 * C31 ) + ( C34 * C32 ) );
L54: C35 in { ( B34 + B35 ) where B34 , B35 is (VECTOR of C30) : (B34 in ( C33 * C31 ) & B35 in ( C34 * C32 )) } by L53 , RUSUB_4:def 9;
consider C37 , C38 being (VECTOR of C30) such that L55: C35 = ( C37 + C38 ) and L56: C37 in ( C33 * C31 ) and L57: C38 in ( C34 * C32 ) by L54;
L58: C37 in { ( C33 * B36 ) where B36 is (VECTOR of C30) : B36 in C31 } by L56 , CONVEX1:def 1;
consider C39 being (VECTOR of C30) such that L59: C37 = ( C33 * C39 ) and L60: C39 in C31 by L58;
L61: ( C36 * C37 ) = ( ( C33 * C36 ) * C39 ) by L59 , RLVECT_1:def 7
.= ( C33 * ( C36 * C39 ) ) by RLVECT_1:def 7;
L62: C38 in { ( C34 * B37 ) where B37 is (VECTOR of C30) : B37 in C32 } by L57 , CONVEX1:def 1;
consider C40 being (VECTOR of C30) such that L63: C38 = ( C34 * C40 ) and L64: C40 in C32 by L62;
L65: ( C36 * C38 ) = ( ( C34 * C36 ) * C40 ) by L63 , RLVECT_1:def 7
.= ( C34 * ( C36 * C40 ) ) by RLVECT_1:def 7;
L66: ( C36 * C40 ) in C32 by L51 , L52 , L64 , L18;
L67: ( C36 * C38 ) in ( C34 * C32 ) by L66 , L65 , RLTOPSP1:1;
L68: ( C36 * C39 ) in C31 by L50 , L52 , L60 , L18;
L69: ( C36 * C37 ) in ( C33 * C31 ) by L68 , L61 , RLTOPSP1:1;
L70: ( C36 * ( C37 + C38 ) ) = ( ( C36 * C37 ) + ( C36 * C38 ) ) by RLVECT_1:def 5;
L71: ( C36 * ( C37 + C38 ) ) in { ( B38 + B39 ) where B38 , B39 is (VECTOR of C30) : (B38 in ( C33 * C31 ) & B39 in ( C34 * C32 )) } by L70 , L69 , L67;
thus L72: thesis by L71 , L55 , RUSUB_4:def 9;
end;
theorem
L73: (for B40 being RealLinearSpace holds (for B41 , B42 , B43 being (Subset of B40) holds (for B44 , B45 , B46 being Real holds ((B41 is  circled & B42 is  circled & B43 is  circled) implies ( ( ( B44 * B41 ) + ( B45 * B42 ) ) + ( B46 * B43 ) ) is  circled))))
proof
let C41 being RealLinearSpace;
let C42 , C43 , C44 being (Subset of C41);
let C45 , C46 , C47 being Real;
assume that
L74: (C42 is  circled & C43 is  circled)
and
L75: C44 is  circled;
L76: ( ( C45 * C42 ) + ( C46 * C43 ) ) is  circled by L74 , L49;
L77: ( ( 1 * ( ( C45 * C42 ) + ( C46 * C43 ) ) ) + ( C47 * C44 ) ) is  circled by L76 , L75 , L49;
thus L78: thesis by L77 , CONVEX1:32;
end;
theorem
L79: (for B47 being RealLinearSpace holds ( Up ( (0). B47 ) ) is  circled)
proof
let C48 being RealLinearSpace;
let C49 being (VECTOR of C48);
let C50 being Real;
assume that
L80: ( abs C50 ) <= 1
and
L81: C49 in ( Up ( (0). C48 ) );
L82: C49 in (the carrier of ( (0). C48 )) by L81 , RUSUB_4:def 5;
L83: C49 in { ( 0. C48 ) } by L82 , RLSUB_1:def 3;
L84: C49 = ( 0. C48 ) by L83 , TARSKI:def 1;
L85: ( C50 * C49 ) = ( 0. C48 ) by L84 , RLVECT_1:10;
L86: ( C50 * C49 ) in { ( 0. C48 ) } by L85 , TARSKI:def 1;
L87: ( C50 * C49 ) in (the carrier of ( (0). C48 )) by L86 , RLSUB_1:def 3;
thus L88: thesis by L87 , RUSUB_4:def 5;
end;
