theorem
L93: (for B49 being RealLinearSpace holds (for B50 , B51 being  circled (Subset of B49) holds ( B50 /\ B51 ) is  circled))
proof
let C54 being RealLinearSpace;
let C55 , C56 being  circled (Subset of C54);
let C57 being (VECTOR of C54);
