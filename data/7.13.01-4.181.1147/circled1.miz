begin
definition
let C64 being non  empty RLSStruct;
let C65 being (Subset of C64);
func Circled-Family C65 -> (Subset-Family of C64) means 
:L107: (for B55 being (Subset of C64) holds (B55 in it iff (B55 is  circled & C65 c= B55)));
existence
proof
defpred S1[ (Subset of C64) ] means ($1 is  circled & C65 c= $1);
thus L108: (ex B56 being (Subset-Family of C64) st (for B57 being (Subset of C64) holds (B57 in B56 iff S1[ B57 ]))) from SUBSET_1:sch 3;
end;
uniqueness
proof
let C66 , C67 being (Subset-Family of C64);
assume that
L109: (for B58 being (Subset of C64) holds (B58 in C66 iff (B58 is  circled & C65 c= B58)))
and
L110: (for B59 being (Subset of C64) holds (B59 in C67 iff (B59 is  circled & C65 c= B59)));
L111: (for B60 being (Subset of C64) holds (B60 in C66 iff B60 in C67))
proof
let C68 being (Subset of C64);
thus L112:now
assume L113: C68 in C66;
L114: (C68 is  circled & C65 c= C68) by L113 , L109;
thus L115: C68 in C67 by L114 , L110;
end;
assume L116: C68 in C67;
L117: (C68 is  circled & C65 c= C68) by L116 , L110;
thus L118: thesis by L117 , L109;
end;
thus L119: thesis by L111 , SETFAM_1:31;
end;
end;
