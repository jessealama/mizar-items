theorem
L163: (for B77 being RealLinearSpace holds (for B78 being (Subset of B77) holds (for B79 being Real holds ( B79 * ( Cir B78 ) ) = ( Cir ( B79 * B78 ) ))))
proof
let C92 being RealLinearSpace;
let C93 being (Subset of C92);
let C94 being Real;
thus L164: ( C94 * ( Cir C93 ) ) c= ( Cir ( C94 * C93 ) )
proof
let C95 being set;
per cases ;
suppose L165: C94 = ( 0 );

per cases ;
suppose L166: C93 = ( {} );

L167: C93 = ( {} C92 ) by L166;
L168: ( Cir C93 ) = ( {} ) by L167 , L160;
thus L169: thesis by L168 , CONVEX1:33;
end;
suppose L170: C93 <> ( {} );

L171: ( C94 * C93 ) = { ( 0. C92 ) } by L170 , L165 , CONVEX1:34;
L172: { ( 0. C92 ) } c= ( Cir ( C94 * C93 ) ) by L171 , L146;
L173: ( Cir C93 ) <> ( {} ) by L170 , L146 , XBOOLE_1:3;
L174: ( C94 * ( Cir C93 ) ) = { ( 0. C92 ) } by L173 , L165 , CONVEX1:34;
thus L175: thesis by L174 , L172;
end;
end;
suppose L177: C94 <> ( 0 );

L178: ( C94 * ( Cir C93 ) ) = { ( C94 * B80 ) where B80 is (Point of C92) : B80 in ( Cir C93 ) } by CONVEX1:def 1;
assume L179: C95 in ( C94 * ( Cir C93 ) );
consider C96 being (Point of C92) such that L180: C95 = ( C94 * C96 ) and L181: C96 in ( Cir C93 ) by L179 , L178;
L182: (for B81 being set holds (B81 in ( Circled-Family ( C94 * C93 ) ) implies ( C94 * C96 ) in B81))
proof
let C97 being set;
assume L183: C97 in ( Circled-Family ( C94 * C93 ) );
reconsider D4 = C97 as (Subset of C92) by L183;
L184: ( C94 * C93 ) c= D4 by L183 , L107;
L185: ( ( C94 " ) * ( C94 * C93 ) ) c= ( ( C94 " ) * D4 ) by L184 , CONVEX1:39;
L186: ( ( ( C94 " ) * C94 ) * C93 ) c= ( ( C94 " ) * D4 ) by L185 , CONVEX1:37;
L187: ( 1 * C93 ) c= ( ( C94 " ) * D4 ) by L186 , L177 , XCMPLX_0:def 7;
L188: C93 c= ( ( C94 " ) * D4 ) by L187 , CONVEX1:32;
L189: D4 is  circled by L183 , L107;
L190: ( ( C94 " ) * D4 ) is  circled by L189 , L37;
L191: ( ( C94 " ) * D4 ) in ( Circled-Family C93 ) by L190 , L188 , L107;
L192: (( ( C94 " ) * D4 ) = { ( ( C94 " ) * B82 ) where B82 is (Point of C92) : B82 in D4 } & C96 in ( ( C94 " ) * D4 )) by L191 , L181 , CONVEX1:def 1 , SETFAM_1:def 1;
consider C98 being (Point of C92) such that L193: C96 = ( ( C94 " ) * C98 ) and L194: C98 in D4 by L192;
L195: ( C94 * C96 ) = ( ( C94 * ( C94 " ) ) * C98 ) by L193 , RLVECT_1:def 7
.= ( 1 * C98 ) by L177 , XCMPLX_0:def 7
.= C98 by RLVECT_1:def 8;
thus L196: thesis by L195 , L194;
end;
thus L197: thesis by L182 , L180 , SETFAM_1:def 1;
end;
end;

L199: (( C94 * C93 ) c= ( C94 * ( Cir C93 ) ) & ( C94 * ( Cir C93 ) ) is  circled) by L37 , L146 , CONVEX1:39;
thus L200: thesis by L199 , L153;
end;
begin
definition
let C99 being RealLinearSpace;
let C100 being (Linear_Combination of C99);
