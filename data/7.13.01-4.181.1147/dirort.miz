theorem
L2: (for R1 being RealLinearSpace holds (for R2 being (VECTOR of R1) holds (for R3 being (VECTOR of R1) holds ( Gen R2 , R3 implies ((for B1 , B2 , B3 , B4 , B5 being (Element of ( CESpace (R1 , R2 , R3) )) holds (B1 , B1 '//' B3 , B5 & B1 , B3 '//' B5 , B5 & ((B1 , B3 '//' B2 , B4 & B1 , B3 '//' B4 , B2) implies (B1 = B3 or B2 = B4)) & ((B1 , B3 '//' B2 , B4 & B1 , B3 '//' B2 , B5) implies (B1 , B3 '//' B4 , B5 or B1 , B3 '//' B5 , B4)) & (B1 , B3 '//' B2 , B4 implies B3 , B1 '//' B4 , B2) & ((B1 , B3 '//' B2 , B4 & B1 , B3 '//' B4 , B5) implies B1 , B3 '//' B2 , B5) & (B1 , B2 '//' B3 , B4 implies (B3 , B4 '//' B1 , B2 or B3 , B4 '//' B2 , B1)))) & (for B6 , B7 , B8 being (Element of ( CESpace (R1 , R2 , R3) )) holds (ex B9 being (Element of ( CESpace (R1 , R2 , R3) )) st (B8 <> B9 & B8 , B9 '//' B6 , B7))) & (for B10 , B11 , B12 being (Element of ( CESpace (R1 , R2 , R3) )) holds (ex B13 being (Element of ( CESpace (R1 , R2 , R3) )) st (B12 <> B13 & B10 , B11 '//' B12 , B13))))))))
proof
let R1 being RealLinearSpace;
let R2 being (VECTOR of R1);
let R3 being (VECTOR of R1);
assume L3:  Gen R2 , R3;
set D1 = ( CESpace (R1 , R2 , R3) );
L4: D1 = AffinStruct (# (the carrier of R1) , ( CORTE (R1 , R2 , R3) ) #) by ANALORT:def 7;
thus L5: (for B14 , B15 , B16 , B17 , B18 being (Element of D1) holds (B14 , B14 '//' B16 , B18 & B14 , B16 '//' B18 , B18 & ((B14 , B16 '//' B15 , B17 & B14 , B16 '//' B17 , B15) implies (B14 = B16 or B15 = B17)) & ((B14 , B16 '//' B15 , B17 & B14 , B16 '//' B15 , B18) implies (B14 , B16 '//' B17 , B18 or B14 , B16 '//' B18 , B17)) & (B14 , B16 '//' B15 , B17 implies B16 , B14 '//' B17 , B15) & ((B14 , B16 '//' B15 , B17 & B14 , B16 '//' B17 , B18) implies B14 , B16 '//' B15 , B18) & (B14 , B15 '//' B16 , B17 implies (B16 , B17 '//' B14 , B15 or B16 , B17 '//' B15 , B14))))
proof
let C6 , C7 , C8 , C9 , C10 being (Element of D1);
reconsider D2 = C6 , D3 = C8 , D4 = C10 , D5 = C7 , D6 = C9 as (Element of R1) by L4;
L6: D2 , D2 , D3 , D4 are_COrte_wrt R2 , R3 by ANALORT:20;
thus L7: C6 , C6 '//' C8 , C10 by L6 , ANALORT:54;
L8: D2 , D3 , D4 , D4 are_COrte_wrt R2 , R3 by ANALORT:22;
thus L9: C6 , C8 '//' C10 , C10 by L8 , ANALORT:54;
L10: ((D2 , D3 , D5 , D6 are_COrte_wrt R2 , R3 & D2 , D3 , D6 , D5 are_COrte_wrt R2 , R3) implies (D2 = D3 or D5 = D6)) by L3 , ANALORT:30;
thus L11: ((C6 , C8 '//' C7 , C9 & C6 , C8 '//' C9 , C7) implies (C6 = C8 or C7 = C9)) by L10 , ANALORT:54;
L12: ((D2 , D3 , D5 , D6 are_COrte_wrt R2 , R3 & D2 , D3 , D5 , D4 are_COrte_wrt R2 , R3) implies (D2 , D3 , D6 , D4 are_COrte_wrt R2 , R3 or D2 , D3 , D4 , D6 are_COrte_wrt R2 , R3)) by L3 , ANALORT:32;
thus L13: ((C6 , C8 '//' C7 , C9 & C6 , C8 '//' C7 , C10) implies (C6 , C8 '//' C9 , C10 or C6 , C8 '//' C10 , C9)) by L12 , ANALORT:54;
L14: (D2 , D3 , D5 , D6 are_COrte_wrt R2 , R3 implies D3 , D2 , D6 , D5 are_COrte_wrt R2 , R3) by ANALORT:34;
thus L15: (C6 , C8 '//' C7 , C9 implies C8 , C6 '//' C9 , C7) by L14 , ANALORT:54;
L16: ((D2 , D3 , D5 , D6 are_COrte_wrt R2 , R3 & D2 , D3 , D6 , D4 are_COrte_wrt R2 , R3) implies D2 , D3 , D5 , D4 are_COrte_wrt R2 , R3) by L3 , ANALORT:36;
thus L17: ((C6 , C8 '//' C7 , C9 & C6 , C8 '//' C9 , C10) implies C6 , C8 '//' C7 , C10) by L16 , ANALORT:54;
L18: (D2 , D5 , D3 , D6 are_COrte_wrt R2 , R3 implies (D3 , D6 , D2 , D5 are_COrte_wrt R2 , R3 or D3 , D6 , D5 , D2 are_COrte_wrt R2 , R3)) by L3 , ANALORT:18;
thus L19: (C6 , C7 '//' C8 , C9 implies (C8 , C9 '//' C6 , C7 or C8 , C9 '//' C7 , C6)) by L18 , ANALORT:54;
end;

