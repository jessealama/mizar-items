definition
let R1 being (Element of ( NAT ));
let R14 being (PartFunc of ( REAL ) , ( REAL R1 ));
let R11 being  real number;
pred R14 is_continuous_in R11
means
:L1: (ex B1 being (PartFunc of ( REAL ) , ( REAL-NS R1 )) st (R14 = B1 & B1 is_continuous_in R11))
;end;
theorem
L3: (for R1 being (Element of ( NAT )) holds (for R11 being  real number holds (for R14 being (PartFunc of ( REAL ) , ( REAL R1 )) holds (for R17 being (PartFunc of ( REAL ) , ( REAL-NS R1 )) holds (R17 = R14 implies (R14 is_continuous_in R11 iff R17 is_continuous_in R11))))))
proof
let R1 being (Element of ( NAT ));
let R11 being  real number;
let R14 being (PartFunc of ( REAL ) , ( REAL R1 ));
let R17 being (PartFunc of ( REAL ) , ( REAL-NS R1 ));
assume L4: R17 = R14;
thus L5:now
assume L6: R14 is_continuous_in R11;
L7: (ex B2 being (PartFunc of ( REAL ) , ( REAL-NS R1 )) st (R14 = B2 & B2 is_continuous_in R11)) by L6 , L1;
thus L8: R17 is_continuous_in R11 by L7 , L4;
end;
thus L9: thesis by L4 , L1;
end;
theorem
L10: (for R1 being (Element of ( NAT )) holds (for R6 being set holds (for R11 being  real number holds (for R14 being (PartFunc of ( REAL ) , ( REAL R1 )) holds ((R11 in R6 & R14 is_continuous_in R11) implies ( R14 | R6 ) is_continuous_in R11)))))
proof
