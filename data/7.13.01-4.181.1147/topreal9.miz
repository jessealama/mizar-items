theorem
L1: (for R1 being (Element of ( NAT )) holds (for R6 being (Point of ( TOP-REAL R1 )) holds (for R7 being (Point of ( TOP-REAL R1 )) holds (for R8 being (Point of ( TOP-REAL R1 )) holds (( R6 + R7 ) = ( R6 + R8 ) implies R7 = R8)))))
proof
let R1 being (Element of ( NAT ));
let R6 being (Point of ( TOP-REAL R1 ));
let R7 being (Point of ( TOP-REAL R1 ));
let R8 being (Point of ( TOP-REAL R1 ));
assume L2: ( R6 + R7 ) = ( R6 + R8 );
L3: ( ( R6 - R6 ) + R7 ) = ( ( R6 + R8 ) - R6 ) by L2 , EUCLID:26;
L4: (( R6 - R6 ) = ( 0. ( TOP-REAL R1 ) ) & ( ( R6 - R6 ) + R7 ) = ( ( R6 - R6 ) + R8 )) by L3 , EUCLID:42 , EUCLID:26;
thus L5: R7 = ( ( 0. ( TOP-REAL R1 ) ) + R8 ) by L4 , EUCLID:27
.= R8 by EUCLID:27;
end;
theorem
L6: (for R1 being (Element of ( NAT )) holds (for R6 being (Point of ( TOP-REAL R1 )) holds (R1 is non  empty implies R6 <> ( R6 + ( 1.REAL R1 ) ))))
proof
let R1 being (Element of ( NAT ));
