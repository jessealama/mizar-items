assume L8: (R1 is non  empty & R6 = ( R6 + ( 1.REAL R1 ) ));
thus L9: thesis by L8 , L7 , L1 , REVROT_1:19;
end;
theorem
L10: (for R1 being (Element of ( NAT )) holds (for R4 being  real number holds (for R7 being (Point of ( TOP-REAL R1 )) holds (for R8 being (Point of ( TOP-REAL R1 )) holds (for B1 being set holds (B1 = ( ( ( 1 - R4 ) * R7 ) + ( R4 * R8 ) ) implies ((B1 = R7 iff (R4 = ( 0 ) or R7 = R8)) & (B1 = R8 iff (R4 = 1 or R7 = R8)))))))))
proof
let R1 being (Element of ( NAT ));
let R4 being  real number;
let R7 being (Point of ( TOP-REAL R1 ));
let R8 being (Point of ( TOP-REAL R1 ));
let C1 being set;
assume L11: C1 = ( ( ( 1 - R4 ) * R7 ) + ( R4 * R8 ) );
thus L12:now
assume L13: C1 = R7;
L14: ( 0. ( TOP-REAL R1 ) ) = ( ( ( ( 1 - R4 ) * R7 ) + ( R4 * R8 ) ) - R7 ) by L13 , L11 , EUCLID:42
.= ( ( ( ( 1 - R4 ) * R7 ) - R7 ) + ( R4 * R8 ) ) by EUCLID:26
.= ( ( ( ( 1 - R4 ) * R7 ) - ( 1 * R7 ) ) + ( R4 * R8 ) ) by EUCLID:29
.= ( ( ( ( 1 - R4 ) - 1 ) * R7 ) + ( R4 * R8 ) ) by EUCLID:50
.= ( ( R4 * R8 ) - ( R4 * R7 ) ) by EUCLID:40
.= ( R4 * ( R8 - R7 ) ) by EUCLID:49;
L15: (R4 = ( 0 ) or ( R8 - R7 ) = ( 0. ( TOP-REAL R1 ) )) by L14 , EUCLID:31;
thus L16: (R4 = ( 0 ) or R7 = R8) by L15 , EUCLID:43;
end;
thus L17:now
assume L18: (R4 = ( 0 ) or R7 = R8);
per cases  by L18;
suppose L19: R4 = ( 0 );

thus L20: C1 = ( R7 + ( ( 0 ) * R8 ) ) by L19 , L11 , EUCLID:29
.= ( R7 + ( 0. ( TOP-REAL R1 ) ) ) by EUCLID:29
.= R7 by EUCLID:27;
end;
suppose L21: R8 = R7;

thus L22: C1 = ( ( ( 1 - R4 ) + R4 ) * R7 ) by L21 , L11 , EUCLID:33
.= R7 by EUCLID:29;
end;
end;
thus L24:now
assume L25: C1 = R8;
L26: ( 0. ( TOP-REAL R1 ) ) = ( ( ( ( 1 - R4 ) * R7 ) + ( R4 * R8 ) ) - R8 ) by L25 , L11 , EUCLID:42
.= ( ( ( 1 - R4 ) * R7 ) + ( ( R4 * R8 ) - R8 ) ) by EUCLID:45
.= ( ( ( 1 - R4 ) * R7 ) + ( ( R4 * R8 ) + ( ( - 1 ) * R8 ) ) )
.= ( ( ( 1 - R4 ) * R7 ) + ( ( ( - 1 ) + R4 ) * R8 ) ) by EUCLID:33
.= ( ( ( 1 - R4 ) * R7 ) + ( ( - ( 1 - R4 ) ) * R8 ) )
.= ( ( ( 1 - R4 ) * R7 ) - ( ( 1 - R4 ) * R8 ) ) by EUCLID:40
.= ( ( 1 - R4 ) * ( R7 - R8 ) ) by EUCLID:49;
L27: (( ( 1 - R4 ) + R4 ) = ( ( 0 ) + R4 ) or ( R7 - R8 ) = ( 0. ( TOP-REAL R1 ) )) by L26 , EUCLID:31;
thus L28: (R4 = 1 or R7 = R8) by L27 , EUCLID:43;
end;
assume L29: (R4 = 1 or R7 = R8);
per cases  by L29;
suppose L30: R4 = 1;

thus L31: C1 = ( ( 0. ( TOP-REAL R1 ) ) + ( 1 * R8 ) ) by L30 , L11 , EUCLID:29
.= ( 1 * R8 ) by EUCLID:27
.= R8 by EUCLID:29;
end;
suppose L32: R7 = R8;

thus L33: C1 = ( ( ( 1 - R4 ) + R4 ) * R8 ) by L32 , L11 , EUCLID:33
.= R8 by EUCLID:29;
end;
end;
