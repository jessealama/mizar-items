theorem
L1: (for B1 being set holds (for B2 being non  empty set holds ( B1 /\ ( union B2 ) ) = ( union { ( B1 /\ B3 ) where B3 is (Element of B2) : (not contradiction) } )))
proof
let C1 being set;
let C2 being non  empty set;
thus L2:now
let C3 being set;
assume L3: C3 in ( C1 /\ ( union C2 ) );
L4: C3 in ( union C2 ) by L3 , XBOOLE_0:def 4;
consider C4 being set such that L5: C3 in C4 and L6: C4 in C2 by L4 , TARSKI:def 4;
L7: ( C1 /\ C4 ) in { ( C1 /\ B4 ) where B4 is (Element of C2) : (not contradiction) } by L6;
L8: C3 in C1 by L3 , XBOOLE_0:def 4;
L9: C3 in ( C1 /\ C4 ) by L8 , L5 , XBOOLE_0:def 4;
thus L10: C3 in ( union { ( C1 /\ B5 ) where B5 is (Element of C2) : (not contradiction) } ) by L9 , L7 , TARSKI:def 4;
end;
let C5 being set;
assume L11: C5 in ( union { ( C1 /\ B6 ) where B6 is (Element of C2) : (not contradiction) } );
consider C6 being set such that L12: C5 in C6 and L13: C6 in { ( C1 /\ B7 ) where B7 is (Element of C2) : (not contradiction) } by L11 , TARSKI:def 4;
consider C7 being (Element of C2) such that L14: C6 = ( C1 /\ C7 ) and L15: (not contradiction) by L13;
L16: C5 in C7 by L12 , L14 , XBOOLE_0:def 4;
L17: C5 in ( union C2 ) by L16 , TARSKI:def 4;
L18: C5 in C1 by L12 , L14 , XBOOLE_0:def 4;
thus L19: thesis by L18 , L17 , XBOOLE_0:def 4;
end;
theorem
L20: (for B8 being non  empty  reflexive  transitive RelStr holds (for B9 being non  empty  directed (Subset of ( InclPoset ( Ids B8 ) )) holds ( union B9 ) is (Ideal of B8)))
proof
let C8 being non  empty  reflexive  transitive RelStr;
let C9 being non  empty  directed (Subset of ( InclPoset ( Ids C8 ) ));
set D1 = the (Element of C9);
L21: C9 c= (the carrier of ( InclPoset ( Ids C8 ) ));
L22: C9 c= ( Ids C8 ) by L21 , YELLOW_1:1;
L23: C9 c= ( bool (the carrier of C8) )
proof
let C10 being set;
assume L24: C10 in C9;
L25: C10 in ( Ids C8 ) by L24 , L22;
L26: (ex B10 being (Ideal of C8) st (C10 = B10 & (not contradiction))) by L25;
thus L27: thesis by L26;
end;
L28: D1 in C9;
L29: D1 in ( Ids C8 ) by L28 , L22;
consider C11 being (Ideal of C8) such that L30: D1 = C11 and L31: (not contradiction) by L29;
L32: (for B11 being (Subset of C8) holds (B11 in C9 implies B11 is  directed))
proof
let C12 being (Subset of C8);
assume L33: C12 in C9;
L34: C12 in ( Ids C8 ) by L33 , L22;
L35: (ex B12 being (Ideal of C8) st (C12 = B12 & (not contradiction))) by L34;
thus L36: thesis by L35;
end;
L37: (for B13 , B14 being (Subset of C8) holds ((B13 in C9 & B14 in C9) implies (ex B15 being (Subset of C8) st (B15 in C9 & ( B13 \/ B14 ) c= B15))))
proof
let C13 , C14 being (Subset of C8);
assume that
L38: C13 in C9
and
L39: C14 in C9;
reconsider D2 = C13 , D3 = C14 as (Element of ( InclPoset ( Ids C8 ) )) by L38 , L39;
consider C15 being (Element of ( InclPoset ( Ids C8 ) )) such that L40: C15 in C9 and L41: D2 <= C15 and L42: D3 <= C15 by L38 , L39 , WAYBEL_0:def 1;
L43: C15 in ( Ids C8 ) by L22 , L40;
L44: (ex B16 being (Ideal of C8) st (C15 = B16 & (not contradiction))) by L43;
reconsider D4 = C15 as (Subset of C8) by L44;
take D4;
thus L45: D4 in C9 by L40;
L46: D3 c= C15 by L42 , YELLOW_1:3;
L47: D2 c= C15 by L41 , YELLOW_1:3;
thus L48: thesis by L47 , L46 , XBOOLE_1:8;
end;
L49: (for B17 being (Subset of C8) holds (B17 in C9 implies B17 is  lower))
proof
let C16 being (Subset of C8);
assume L50: C16 in C9;
L51: C16 in ( Ids C8 ) by L50 , L22;
L52: (ex B18 being (Ideal of C8) st (C16 = B18 & (not contradiction))) by L51;
thus L53: thesis by L52;
end;
set D5 = the (Element of C11);
L54: D5 in D1 by L30;
thus L55: thesis by L54 , L49 , L23 , L32 , L37 , TARSKI:def 4 , WAYBEL_0:26 , WAYBEL_0:46;
end;
L56:
now
let C17 being non  empty  reflexive  transitive RelStr;
let C18 being non  empty  directed (Subset of ( InclPoset ( Ids C17 ) ));
let C19 being (Element of ( InclPoset ( Ids C17 ) ));
assume that
L57: C19 = ( union C18 );
thus L58: C18 is_<=_than C19
proof
let C20 being (Element of ( InclPoset ( Ids C17 ) ));
assume L59: C20 in C18;
L60: C20 c= C19
proof
let C21 being set;
assume L61: C21 in C20;
thus L62: thesis by L61 , L57 , L59 , TARSKI:def 4;
end;
thus L63: C20 <= C19 by L60 , YELLOW_1:3;
end;

end;
L59:
now
let C22 being non  empty  reflexive  transitive RelStr;
let C23 being non  empty  directed (Subset of ( InclPoset ( Ids C22 ) ));
let C24 being (Element of ( InclPoset ( Ids C22 ) ));
assume that
L60: C24 = ( union C23 );
thus L61: (for B19 being (Element of ( InclPoset ( Ids C22 ) )) holds (B19 is_>=_than C23 implies C24 <= B19))
proof
let C25 being (Element of ( InclPoset ( Ids C22 ) ));
assume that
L62: (for B20 being (Element of ( InclPoset ( Ids C22 ) )) holds (B20 in C23 implies C25 >= B20));
L63: C24 c= C25
proof
let C26 being set;
assume L64: C26 in C24;
consider C27 being set such that L65: C26 in C27 and L66: C27 in C23 by L64 , L60 , TARSKI:def 4;
reconsider D6 = C27 as (Element of ( InclPoset ( Ids C22 ) )) by L66;
L67: C25 >= D6 by L62 , L66;
L68: D6 c= C25 by L67 , YELLOW_1:3;
thus L69: thesis by L68 , L65;
end;
thus L70: thesis by L63 , YELLOW_1:3;
end;

end;
registration
let C28 being non  empty  reflexive  transitive RelStr;
cluster ( InclPoset ( Ids C28 ) ) ->  up-complete;
coherence
proof
set D7 = ( InclPoset ( Ids C28 ) );
let C29 being non  empty  directed (Subset of D7);
reconsider D8 = ( union C29 ) as (Ideal of C28) by L20;
L62: D8 in ( Ids C28 );
reconsider D9 = D8 as (Element of D7) by L62 , YELLOW_1:1;
take D9;
thus L63: thesis by L56 , L59;
end;
end;
theorem
L65: (for B21 being non  empty  reflexive  transitive RelStr holds (for B22 being non  empty  directed (Subset of ( InclPoset ( Ids B21 ) )) holds ( sup B22 ) = ( union B22 )))
proof
let C30 being non  empty  reflexive  transitive RelStr;
let C31 being non  empty  directed (Subset of ( InclPoset ( Ids C30 ) ));
reconsider D10 = ( union C31 ) as (Ideal of C30) by L20;
L66:  ex_sup_of C31 , ( InclPoset ( Ids C30 ) ) by WAYBEL_0:75;
L67: D10 in ( Ids C30 );
reconsider D11 = D10 as (Element of ( InclPoset ( Ids C30 ) )) by L67 , YELLOW_1:1;
L68: (for B23 being (Element of ( InclPoset ( Ids C30 ) )) holds (B23 is_>=_than C31 implies D11 <= B23)) by L59;
L69: C31 is_<=_than D11 by L56;
thus L70: thesis by L69 , L68 , L66 , YELLOW_0:def 9;
end;
theorem
L71: (for B24 being Semilattice holds (for B25 being non  empty  directed (Subset of ( InclPoset ( Ids B24 ) )) holds (for B26 being (Element of ( InclPoset ( Ids B24 ) )) holds ( sup ( { B26 } "/\" B25 ) ) = ( union { ( B26 /\ B27 ) where B27 is (Element of B25) : (not contradiction) } ))))
proof
let C32 being Semilattice;
let C33 being non  empty  directed (Subset of ( InclPoset ( Ids C32 ) ));
let C34 being (Element of ( InclPoset ( Ids C32 ) ));
set D12 = ( InclPoset ( Ids C32 ) );
set D13 = { ( C34 /\ B28 ) where B28 is (Element of C33) : (not contradiction) };
set D14 = { ( C34 "/\" B29 ) where B29 is (Element of D12) : B29 in C33 };
L72: D14 c= (the carrier of D12)
proof
let C35 being set;
assume L73: C35 in D14;
L74: (ex B30 being (Element of D12) st (C35 = ( C34 "/\" B30 ) & B30 in C33)) by L73;
thus L75: thesis by L74;
end;
reconsider D15 = D14 as (Subset of D12) by L72;
L76:  ex_sup_of ( { C34 } "/\" C33 ) , D12 by WAYBEL_2:1;
L77:  ex_sup_of D15 , D12 by L76 , YELLOW_4:42;
L78: ( sup D15 ) is_>=_than D15 by L77 , YELLOW_0:def 9;
thus L79:now
set D16 = { ( C34 /\ B31 ) where B31 is (Element of C33) : (not contradiction) };
let C36 being set;
L80:  ex_sup_of C33 , D12 by WAYBEL_0:75;
L81: ( sup ( { C34 } "/\" C33 ) ) <= ( C34 "/\" ( sup C33 ) ) by L80 , L76 , YELLOW_4:53;
L82: ( sup ( { C34 } "/\" C33 ) ) c= ( C34 "/\" ( sup C33 ) ) by L81 , YELLOW_1:3;
assume L83: C36 in ( sup ( { C34 } "/\" C33 ) );
L84: C36 in ( C34 "/\" ( sup C33 ) ) by L83 , L82;
L85: C36 in ( C34 /\ ( sup C33 ) ) by L84 , YELLOW_2:43;
L86: C36 in ( sup C33 ) by L85 , XBOOLE_0:def 4;
L87: C36 in ( union C33 ) by L86 , L65;
consider C37 being set such that L88: C36 in C37 and L89: C37 in C33 by L87 , TARSKI:def 4;
reconsider D17 = C37 as (Element of D12) by L89;
L90: ( C34 /\ D17 ) in D16 by L89;
L91: C36 in C34 by L85 , XBOOLE_0:def 4;
L92: C36 in ( C34 /\ D17 ) by L91 , L88 , XBOOLE_0:def 4;
thus L93: C36 in ( union D16 ) by L92 , L90 , TARSKI:def 4;
end;
let C38 being set;
assume L94: C38 in ( union D13 );
consider C39 being set such that L95: C38 in C39 and L96: C39 in D13 by L94 , TARSKI:def 4;
consider C40 being (Element of C33) such that L97: C39 = ( C34 /\ C40 ) and L98: (not contradiction) by L96;
reconsider D18 = C40 as (Element of D12);
L99: D15 = ( { C34 } "/\" C33 ) by YELLOW_4:42;
L100: ( C34 "/\" D18 ) in ( { C34 } "/\" C33 ) by L99;
L101: ( sup D15 ) >= ( C34 "/\" D18 ) by L100 , L78 , L99 , LATTICE3:def 9;
L102: ( C34 "/\" D18 ) c= ( sup D15 ) by L101 , YELLOW_1:3;
L103: ( C34 /\ D18 ) = ( C34 "/\" D18 ) by YELLOW_2:43;
L104: C38 in ( sup D15 ) by L103 , L102 , L95 , L97;
thus L105: thesis by L104 , YELLOW_4:42;
end;
