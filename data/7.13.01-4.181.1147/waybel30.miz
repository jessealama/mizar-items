.= ( C42 /\ ( union C43 ) ) by L65
.= ( union { ( C42 /\ B32 ) where B32 is (Element of C43) : (not contradiction) } ) by L1
.= ( sup ( { C42 } "/\" C43 ) ) by L71;
end;
