L1: (for B1 , B2 being Nat holds (B2 in ( Seg B1 ) implies ( ( B1 + 1 ) - B2 ) in ( Seg B1 )))
proof
let C1 , C2 being Nat;
assume L2: C2 in ( Seg C1 );
L3: C2 <= C1 by L2 , FINSEQ_1:1;
L4: ( C2 + 1 ) <= ( C1 + 1 ) by L3 , XREAL_1:6;
L5: ( ( C2 + 1 ) - C2 ) <= ( ( C1 + 1 ) - C2 ) by L4 , XREAL_1:9;
L6: 1 <= C2 by L2 , FINSEQ_1:1;
L7: ( C1 + 1 ) <= ( C2 + C1 ) by L6 , XREAL_1:6;
L8: ( ( C1 + 1 ) - C2 ) <= ( ( C2 + C1 ) - C2 ) by L7 , XREAL_1:9;
L9: ( ( C1 + 1 ) - C2 ) in ( NAT ) by L5 , INT_1:3;
thus L10: thesis by L9 , L5 , L8;
end;
L11: (for B3 , B4 , B5 being Nat holds ([ B4 , B5 ] in [: ( Seg B3 ) , ( Seg B3 ) :] implies (( ( B3 + 1 ) - B5 ) in ( Seg B3 ) & ( ( B3 + 1 ) - B4 ) in ( Seg B3 ))))
