  TITLE = {On Same Equivalents of Well-foundedness},
  NOTE = {This work was partially supported by NSERC Grant OGP9207
		  and NATO CRG 951368.},
  SECTION1 = {Preliminaries},
  SECTION2 = {Well Founded Relational Structures},
  DAY = {25},
