vocabularies NUMBERS,XREAL_0,ORDINAL1,SIN_COS,CARD_1,SIN_COS4,ARYTM_3,RELAT_1,ARYTM_1,SQUARE_1,NEWTON,COMPLEX1,XXREAL_0,REAL_1,SIN_COS2,FUNCT_1,SIN_COS5;
notations ORDINAL1,NUMBERS,XCMPLX_0,XREAL_0,COMPLEX1,REAL_1,SQUARE_1,SIN_COS4,SIN_COS,NEWTON,NAT_1,SIN_COS2,SEQ_1,XXREAL_0;
