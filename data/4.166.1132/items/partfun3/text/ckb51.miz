begin
registration
let X being set;
let r being  negative  real number;
let f being  positive-yielding (PartFunc of X,( REAL ));
cluster ( r (#) f ) ->  negative-yielding;
coherence
proof
let z being  real number;
set R = ( r (#) f );
assume z in ( rng R );
then consider x being set such that A1: x in ( dom R ) and A2: ( R . x ) = z by FUNCT_1:def 3;
( dom R ) = ( dom f ) by VALUED_1:def 5;
then ( f . x ) in ( rng f ) by A1,FUNCT_1:def 3;
then reconsider a = ( f . x ) as  positive  real number by CKB8:def 1;
( r * a ) is  negative;
hence thesis by A1,A2,VALUED_1:def 5;
end;
end;
