begin
registration
let f being  non-empty Function;
cluster ( f " { ( 0 ) } ) ->  empty;
coherence
proof
assume (not thesis);
then consider x being set such that A1: x in ( f " { ( 0 ) } ) by XBOOLE_0:def 1;
x in ( dom f ) by A1,FUNCT_1:def 7;
then A2: ( f . x ) in ( rng f ) by FUNCT_1:def 3;
( f . x ) in { ( 0 ) } by A1,FUNCT_1:def 7;
then ( f . x ) = ( 0 ) by TARSKI:def 1;
hence thesis by A2,RELAT_1:def 9;
end;
