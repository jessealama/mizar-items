reserve y for bound_QC-variable;
reserve A for non  empty set;
reserve J for (interpretation of A);
reserve v for (Element of ( Valuations_in A ));
reserve w for (Element of ( Valuations_in A ));
reserve f for Function;
reserve a for (Element of A);
theorem
Th51: (J,v |= ( All (x,p) ) iff (for a holds J,( v . ( x | a ) ) |= p))
proof
thus (J,v |= ( All (x,p) ) implies (for a holds J,( v . ( x | a ) ) |= p))
proof
assume A1: J,v |= ( All (x,p) );
let a;
(for y holds (x <> y implies ( ( v . ( x | a ) ) . y ) = ( v . y ))) by CKB67:1;
hence thesis by A1,VALUAT_1:29;
end;

thus ((for a holds J,( v . ( x | a ) ) |= p) implies J,v |= ( All (x,p) ))
proof
assume A2: (for a holds J,( v . ( x | a ) ) |= p);
(for w holds ((for y holds (x <> y implies ( w . y ) = ( v . y ))) implies J,w |= p))
proof
let w;
assume that
A3: (for y holds (x <> y implies ( w . y ) = ( v . y )));
set c = ( w . x );
A4: (for b holds (b in ( dom w ) implies ( w . b ) = ( ( v . ( x | c ) ) . b )))
proof
let b;
assume b in ( dom w );
then reconsider y = b as bound_QC-variable;
now
assume A5: x <> y;
then ( w . y ) = ( v . y ) by A3;
hence thesis by A5,CKB67:1;
end;
hence thesis by CKB68:1;
end;
( v . ( x | c ) ) is (Element of ( Funcs (( bound_QC-variables ),A) )) by VALUAT_1:def 1;
then A6: (ex f st ((( v . ( x | c ) ) = f & ( dom f ) = ( bound_QC-variables )) & ( rng f ) c= A)) by FUNCT_2:def 2;
w is (Element of ( Funcs (( bound_QC-variables ),A) )) by VALUAT_1:def 1;
then (ex f st ((w = f & ( dom f ) = ( bound_QC-variables )) & ( rng f ) c= A)) by FUNCT_2:def 2;
then ( v . ( x | c ) ) = w by A4,A6,FUNCT_1:2;
hence thesis by A2;
end;
hence thesis by VALUAT_1:29;
end;

