theorem
Th54: ([ S,x ] is  quantifiable implies (for a holds (( Val_S (( v . ( ( S_Bound ( @ ( CQCSub_All ([ S,x ],xSQ) ) ) ) | a ) ),S) ) = ( ( NEx_Val (( v . ( ( S_Bound ( @ ( CQCSub_All ([ S,x ],xSQ) ) ) ) | a ) ),S,x,xSQ) ) +* ( x | a ) ) & ( dom ( RestrictSub (x,( All (x,( S `1 )) ),xSQ) ) ) misses { x })))
proof
assume A1: [ S,x ] is  quantifiable;
set finSub = ( RestrictSub (x,( All (x,( S `1 )) ),xSQ) );
let a;
set S1 = ( CQCSub_All ([ S,x ],xSQ) );
set z = ( S_Bound ( @ S1 ) );
A2: ( S `2 ) = ( ExpandSub (x,( S `1 ),( RestrictSub (x,( All (x,( S `1 )) ),xSQ) )) ) by A1,CKB58:1;
A3:now
reconsider F = { [ x,x ] } as Function;
A4: ( dom ( x | a ) ) = { x } by FUNCOP_1:13;
assume A5: (not x in ( rng finSub ));
then ( S `2 ) = ( finSub \/ F ) by A2,SUBSTUT1:def 13;
then A6: ( @ ( S `2 ) ) = ( finSub \/ F ) by SUBSTUT1:def 2;
A7:now
set q = ( All (x,( S `1 )) );
set X = { y1: (((y1 in ( still_not-bound_in q ) & y1 is (Element of ( dom xSQ ))) & y1 <> x) & y1 <> ( xSQ . y1 )) };
assume (not ( dom finSub ) misses { x });
then ( ( dom finSub ) /\ { x } ) <> ( {} ) by XBOOLE_0:def 7;
then consider b such that A8: b in ( ( dom finSub ) /\ { x } ) by XBOOLE_0:def 1;
finSub = ( xSQ | X ) by SUBSTUT1:def 6;
then finSub = ( ( @ xSQ ) | X ) by SUBSTUT1:def 2;
then ( @ finSub ) = ( ( @ xSQ ) | X ) by SUBSTUT1:def 2;
then ( dom ( @ finSub ) ) = ( ( dom ( @ xSQ ) ) /\ X ) by RELAT_1:61;
then A9: ( dom ( @ finSub ) ) c= X by XBOOLE_1:17;
b in ( dom finSub ) by A8,XBOOLE_0:def 4;
then b in ( dom ( @ finSub ) ) by SUBSTUT1:def 2;
then b in X by A9;
then A10: (ex y st ((((y = b & y in ( still_not-bound_in q )) & y is (Element of ( dom xSQ ))) & y <> x) & y <> ( xSQ . y )));
b in { x } by A8,XBOOLE_0:def 4;
hence contradiction by A10,TARSKI:def 1;
end;
hence ( dom finSub ) misses { x };
( dom { [ x,x ] } ) = { x } by RELAT_1:9;
then ( dom ( @ finSub ) ) misses ( dom F ) by A7,SUBSTUT1:def 2;
then A11: ( ( @ finSub ) \/ F ) = ( ( @ finSub ) +* F ) by FUNCT_4:31;
( v . ( z | a ) ) is (Element of ( Funcs (( bound_QC-variables ),A) )) by VALUAT_1:def 1;
then A12: (ex f st ((( v . ( z | a ) ) = f & ( dom f ) = ( bound_QC-variables )) & ( rng f ) c= A)) by FUNCT_2:def 2;
A13: ( rng F ) = { x } by RELAT_1:9;
then ( dom ( F * ( v . ( z | a ) ) ) ) = ( dom F ) by A12,RELAT_1:27;
then A14: ( dom ( F * ( v . ( z | a ) ) ) ) = { x } by RELAT_1:9;
A15: { [ x,x ] } = ( x .--> x ) by FUNCT_4:82;
(for b holds (b in ( dom ( x | a ) ) implies ( ( x | a ) . b ) = ( ( F * ( v . ( z | a ) ) ) . b )))
proof
let b;
assume that
A16: b in ( dom ( x | a ) );
b in ( dom ( F * ( v . ( z | a ) ) ) ) by A14,A16,FUNCOP_1:13;
then A17: ( ( F * ( v . ( z | a ) ) ) . b ) = ( ( v . ( z | a ) ) . ( F . b ) ) by FUNCT_1:12;
b = x by A4,A16,TARSKI:def 1;
then (( ( x | a ) . b ) = a & ( F . b ) = x) by A15,FUNCOP_1:72;
hence thesis by A1,A5,A17,CKB68:1,CKB73:1;
end;
then A18: ( x | a ) = ( F * ( v . ( z | a ) ) ) by A14,FUNCOP_1:13,FUNCT_1:2;
( ( ( @ finSub ) +* F ) * ( v . ( z | a ) ) ) = ( ( ( @ finSub ) * ( v . ( z | a ) ) ) +* ( F * ( v . ( z | a ) ) ) ) by A12,A13,FUNCT_7:9;
hence ( Val_S (( v . ( z | a ) ),S) ) = ( ( NEx_Val (( v . ( z | a ) ),S,x,xSQ) ) +* ( x | a ) ) by A11,A6,A18,SUBSTUT1:def 2;
end;
now
reconsider F = { [ x,( x. ( upVar (finSub,( S `1 )) ) ) ] } as Function;
assume A19: x in ( rng finSub );
A20:now
set q = ( All (x,( S `1 )) );
set X = { y1: (((y1 in ( still_not-bound_in q ) & y1 is (Element of ( dom xSQ ))) & y1 <> x) & y1 <> ( xSQ . y1 )) };
assume ( dom finSub ) meets { x };
then consider b such that A21: b in ( dom finSub ) and A22: b in { x } by XBOOLE_0:3;
finSub = ( xSQ | X ) by SUBSTUT1:def 6;
then finSub = ( ( @ xSQ ) | X ) by SUBSTUT1:def 2;
then ( @ finSub ) = ( ( @ xSQ ) | X ) by SUBSTUT1:def 2;
then ( dom ( @ finSub ) ) = ( ( dom ( @ xSQ ) ) /\ X ) by RELAT_1:61;
then A23: ( dom ( @ finSub ) ) c= X by XBOOLE_1:17;
b in ( dom ( @ finSub ) ) by A21,SUBSTUT1:def 2;
then b in X by A23;
then (ex y st ((((y = b & y in ( still_not-bound_in q )) & y is (Element of ( dom xSQ ))) & y <> x) & y <> ( xSQ . y )));
hence contradiction by A22,TARSKI:def 1;
end;
hence ( dom finSub ) misses { x };
( dom { [ x,( x. ( upVar (finSub,( S `1 )) ) ) ] } ) = { x } by RELAT_1:9;
then ( dom ( @ finSub ) ) misses ( dom F ) by A20,SUBSTUT1:def 2;
then A24: ( ( @ finSub ) \/ F ) = ( ( @ finSub ) +* F ) by FUNCT_4:31;
A25: ( dom ( x | a ) ) = { x } by FUNCOP_1:13;
( v . ( z | a ) ) is (Element of ( Funcs (( bound_QC-variables ),A) )) by VALUAT_1:def 1;
then A26: (ex f st ((( v . ( z | a ) ) = f & ( dom f ) = ( bound_QC-variables )) & ( rng f ) c= A)) by FUNCT_2:def 2;
( rng F ) = { ( x. ( upVar (finSub,( S `1 )) ) ) } by RELAT_1:9;
then ( dom ( F * ( v . ( z | a ) ) ) ) = ( dom F ) by A26,RELAT_1:27;
then A27: ( dom ( F * ( v . ( z | a ) ) ) ) = { x } by RELAT_1:9;
A28: { [ x,( x. ( upVar (finSub,( S `1 )) ) ) ] } = ( x .--> ( x. ( upVar (finSub,( S `1 )) ) ) ) by FUNCT_4:82;
(for b holds (b in ( dom ( x | a ) ) implies ( ( x | a ) . b ) = ( ( F * ( v . ( z | a ) ) ) . b )))
proof
let b;
assume that
A29: b in ( dom ( x | a ) );
b in ( dom ( F * ( v . ( z | a ) ) ) ) by A27,A29,FUNCOP_1:13;
then A30: ( ( F * ( v . ( z | a ) ) ) . b ) = ( ( v . ( z | a ) ) . ( F . b ) ) by FUNCT_1:12;
b = x by A25,A29,TARSKI:def 1;
then (( ( x | a ) . b ) = a & ( F . b ) = ( x. ( upVar (finSub,( S `1 )) ) )) by A28,FUNCOP_1:72;
hence thesis by A1,A19,A30,CKB68:1,CKB72:1;
end;
then A31: ( x | a ) = ( F * ( v . ( z | a ) ) ) by A27,FUNCOP_1:13,FUNCT_1:2;
( rng F ) = { ( x. ( upVar (finSub,( S `1 )) ) ) } by RELAT_1:9;
then A32: ( ( ( @ finSub ) +* F ) * ( v . ( z | a ) ) ) = ( ( ( @ finSub ) * ( v . ( z | a ) ) ) +* ( F * ( v . ( z | a ) ) ) ) by A26,FUNCT_7:9;
( S `2 ) = ( finSub \/ F ) by A2,A19,SUBSTUT1:def 13;
then ( @ ( S `2 ) ) = ( finSub \/ F ) by SUBSTUT1:def 2;
hence ( Val_S (( v . ( z | a ) ),S) ) = ( ( NEx_Val (( v . ( z | a ) ),S,x,xSQ) ) +* ( x | a ) ) by A24,A32,A31,SUBSTUT1:def 2;
end;
hence thesis by A3;
end;
