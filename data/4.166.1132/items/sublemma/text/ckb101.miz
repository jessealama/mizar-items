schemes CQC_LANG,XBOOLE_0,SUBSTUT1;
registrations XBOOLE_0,SUBSET_1,RELAT_1,FUNCT_1,ORDINAL1,FUNCOP_1,MEMBERED,QC_LANG1,CQC_LANG,SUBSTUT1,XXREAL_0,XXREAL_2,CARD_1,RELSET_1,CKB18,CKB38;
constructors DOMAIN_1,FUNCT_4,XXREAL_0,SEQ_4,QC_LANG3,VALUAT_1,SUBSTUT1,RELSET_1,CKB5,CKB6,CKB7,CKB9,CKB14,CKB25,CKB29,CKB37,CKB39,CKB40,CKB41,CKB43,CKB44,CKB66,CKB70,CKB71;
requirements NUMERALS,SUBSET,BOOLE;
begin
reserve p for (Element of ( CQC-WFF ));
reserve x for bound_QC-variable;
reserve y for bound_QC-variable;
reserve A for non  empty set;
reserve J for (interpretation of A);
reserve v for (Element of ( Valuations_in A ));
reserve vS for (Val_Sub of A);
reserve vS1 for (Val_Sub of A);
reserve vS2 for (Val_Sub of A);
reserve a for (Element of A);
theorem
Th84: ((for v holds (for vS holds (for vS1 holds (for vS2 holds ((((for y holds (y in ( dom vS1 ) implies (not y in ( still_not-bound_in p )))) & (for y holds (y in ( dom vS2 ) implies ( vS2 . y ) = ( v . y )))) & ( dom vS ) misses ( dom vS2 )) implies (J,( v . vS ) |= p iff J,( v . ( ( vS +* vS1 ) +* vS2 ) ) |= p)))))) implies (for v holds (for vS holds (for vS1 holds (for vS2 holds ((((for y holds (y in ( dom vS1 ) implies (not y in ( still_not-bound_in ( All (x,p) ) )))) & (for y holds (y in ( dom vS2 ) implies ( vS2 . y ) = ( v . y )))) & ( dom vS ) misses ( dom vS2 )) implies (J,( v . vS ) |= ( All (x,p) ) iff J,( v . ( ( vS +* vS1 ) +* vS2 ) ) |= ( All (x,p) ))))))))
proof
assume A1: (for v holds (for vS holds (for vS1 holds (for vS2 holds ((((for y holds (y in ( dom vS1 ) implies (not y in ( still_not-bound_in p )))) & (for y holds (y in ( dom vS2 ) implies ( vS2 . y ) = ( v . y )))) & ( dom vS ) misses ( dom vS2 )) implies (J,( v . vS ) |= p iff J,( v . ( ( vS +* vS1 ) +* vS2 ) ) |= p))))));
let v;
let vS;
let vS1;
let vS2;
assume that
A2: (for y holds (y in ( dom vS1 ) implies (not y in ( still_not-bound_in ( All (x,p) ) ))))
and
A3: ((for y holds (y in ( dom vS2 ) implies ( vS2 . y ) = ( v . y ))) & ( dom vS ) misses ( dom vS2 ));
set vS19 = ( vS1 | ( ( dom vS1 ) \ { x } ) );
set vS29 = ( vS2 | ( ( dom vS2 ) \ { x } ) );
A4: ( dom vS29 ) c= ( ( dom vS2 ) \ { x } ) by RELAT_1:58;
then A5: (for y holds (y in ( dom vS29 ) implies ( vS29 . y ) = ( ( v . vS ) . y ))) by A3,CKB100:1;
A6: ( dom vS29 ) misses { x } by A4,XBOOLE_1:63,XBOOLE_1:79;
A7: ( dom vS19 ) c= ( ( dom vS1 ) \ { x } ) by RELAT_1:58;
then A8: (for y holds (y in ( dom vS19 ) implies (not y in ( still_not-bound_in p )))) by A2,CKB99:1;
A9: ((for a holds J,( ( v . vS ) . ( x | a ) ) |= p) implies (for a holds J,( ( v . vS ) . ( ( ( x | a ) +* vS19 ) +* vS29 ) ) |= p))
proof
assume A10: (for a holds J,( ( v . vS ) . ( x | a ) ) |= p);
let a;
( dom vS29 ) misses ( dom ( x | a ) ) by A6,CKB79:1;
then (J,( ( v . vS ) . ( x | a ) ) |= p iff J,( ( v . vS ) . ( ( ( x | a ) +* vS19 ) +* vS29 ) ) |= p) by A1,A8,A5;
hence thesis by A10;
end;
A11: ( dom vS19 ) misses { x } by A7,XBOOLE_1:63,XBOOLE_1:79;
A12: (for a holds ( ( v . vS ) . ( ( ( x | a ) +* vS19 ) +* vS29 ) ) = ( ( v . ( ( vS +* vS1 ) +* vS2 ) ) . ( x | a ) ))
proof
let a;
( dom vS19 ) misses ( dom ( x | a ) ) by A11,CKB79:1;
then ( ( v . vS ) . ( ( ( x | a ) +* vS19 ) +* vS29 ) ) = ( ( v +* vS ) +* ( ( vS19 +* ( x | a ) ) +* vS29 ) ) by FUNCT_4:35;
then A13: ( ( v . vS ) . ( ( ( x | a ) +* vS19 ) +* vS29 ) ) = ( ( v +* vS ) +* ( vS19 +* ( ( x | a ) +* vS29 ) ) ) by FUNCT_4:14;
( dom vS29 ) misses ( dom ( x | a ) ) by A6,CKB79:1;
then ( ( v . vS ) . ( ( ( x | a ) +* vS19 ) +* vS29 ) ) = ( ( v +* vS ) +* ( vS19 +* ( vS29 +* ( x | a ) ) ) ) by A13,FUNCT_4:35;
then A14: ( ( v . vS ) . ( ( ( x | a ) +* vS19 ) +* vS29 ) ) = ( ( v +* vS ) +* ( ( vS19 +* vS29 ) +* ( x | a ) ) ) by FUNCT_4:14;
A15:now
assume x in ( dom vS1 );
then A16: ( vS19 +* ( x .--> ( vS1 . x ) ) ) = vS1 by CKB2:1;
A17:now
assume (not x in ( dom vS2 ));
then vS29 = ( vS2 | ( dom vS2 ) ) by ZFMISC_1:57;
then vS29 = vS2 by RELAT_1:68;
then A18: ( vS29 +* ( {} ) ) = vS2 by FUNCT_4:21;
( dom ( x .--> ( vS1 . x ) ) ) = { x } by FUNCOP_1:13;
then (( dom ( {} ) ) c= ( dom ( x | a ) ) & ( dom ( x .--> ( vS1 . x ) ) ) = ( dom ( x | a ) )) by CKB79:1,XBOOLE_1:2;
hence ( ( v . vS ) . ( ( ( x | a ) +* vS19 ) +* vS29 ) ) = ( ( v +* vS ) +* ( ( vS1 +* vS2 ) +* ( x | a ) ) ) by A14,A16,A18,CKB1:1;
end;
now
( dom ( x .--> ( vS2 . x ) ) ) = { x } by FUNCOP_1:13;
then A19: ( dom ( x .--> ( vS2 . x ) ) ) = ( dom ( x | a ) ) by CKB79:1;
( dom ( x .--> ( vS1 . x ) ) ) = { x } by FUNCOP_1:13;
then A20: ( dom ( x .--> ( vS1 . x ) ) ) = ( dom ( x | a ) ) by CKB79:1;
assume x in ( dom vS2 );
then ( vS29 +* ( x .--> ( vS2 . x ) ) ) = vS2 by CKB2:1;
hence ( ( v . vS ) . ( ( ( x | a ) +* vS19 ) +* vS29 ) ) = ( ( v +* vS ) +* ( ( vS1 +* vS2 ) +* ( x | a ) ) ) by A14,A16,A20,A19,CKB1:1;
end;
hence ( ( v . vS ) . ( ( ( x | a ) +* vS19 ) +* vS29 ) ) = ( ( v +* vS ) +* ( ( vS1 +* vS2 ) +* ( x | a ) ) ) by A17;
end;
now
A21: ( dom ( {} ) ) c= ( dom ( x | a ) ) by XBOOLE_1:2;
assume (not x in ( dom vS1 ));
then vS19 = ( vS1 | ( dom vS1 ) ) by ZFMISC_1:57;
then A22: vS19 = vS1 by RELAT_1:68;
then A23: ( vS19 +* ( {} ) ) = vS1 by FUNCT_4:21;
A24:now
( dom ( x .--> ( vS2 . x ) ) ) = { x } by FUNCOP_1:13;
then A25: ( dom ( x .--> ( vS2 . x ) ) ) = ( dom ( x | a ) ) by CKB79:1;
assume x in ( dom vS2 );
then ( vS29 +* ( x .--> ( vS2 . x ) ) ) = vS2 by CKB2:1;
hence ( ( v . vS ) . ( ( ( x | a ) +* vS19 ) +* vS29 ) ) = ( ( v +* vS ) +* ( ( vS1 +* vS2 ) +* ( x | a ) ) ) by A14,A23,A21,A25,CKB1:1;
end;
now
assume (not x in ( dom vS2 ));
then vS29 = ( vS2 | ( dom vS2 ) ) by ZFMISC_1:57;
hence ( ( v . vS ) . ( ( ( x | a ) +* vS19 ) +* vS29 ) ) = ( ( v +* vS ) +* ( ( vS1 +* vS2 ) +* ( x | a ) ) ) by A14,A22,RELAT_1:68;
end;
hence ( ( v . vS ) . ( ( ( x | a ) +* vS19 ) +* vS29 ) ) = ( ( v +* vS ) +* ( ( vS1 +* vS2 ) +* ( x | a ) ) ) by A24;
end;
then ( ( v . vS ) . ( ( ( x | a ) +* vS19 ) +* vS29 ) ) = ( ( ( v +* vS ) +* ( vS1 +* vS2 ) ) +* ( x | a ) ) by A15,FUNCT_4:14;
then ( ( v . vS ) . ( ( ( x | a ) +* vS19 ) +* vS29 ) ) = ( ( ( ( v +* vS ) +* vS1 ) +* vS2 ) +* ( x | a ) ) by FUNCT_4:14;
then ( ( v . vS ) . ( ( ( x | a ) +* vS19 ) +* vS29 ) ) = ( ( ( v +* ( vS +* vS1 ) ) +* vS2 ) +* ( x | a ) ) by FUNCT_4:14;
hence thesis by FUNCT_4:14;
end;
A26: ((for a holds J,( ( v . ( ( vS +* vS1 ) +* vS2 ) ) . ( x | a ) ) |= p) implies (for a holds J,( ( v . vS ) . ( ( ( x | a ) +* vS19 ) +* vS29 ) ) |= p))
proof
assume A27: (for a holds J,( ( v . ( ( vS +* vS1 ) +* vS2 ) ) . ( x | a ) ) |= p);
let a;
( ( v . vS ) . ( ( ( x | a ) +* vS19 ) +* vS29 ) ) = ( ( v . ( ( vS +* vS1 ) +* vS2 ) ) . ( x | a ) ) by A12;
hence thesis by A27;
end;
A28: ((for a holds J,( ( v . vS ) . ( ( ( x | a ) +* vS19 ) +* vS29 ) ) |= p) implies (for a holds J,( ( v . vS ) . ( x | a ) ) |= p))
proof
assume A29: (for a holds J,( ( v . vS ) . ( ( ( x | a ) +* vS19 ) +* vS29 ) ) |= p);
let a;
( dom vS29 ) misses ( dom ( x | a ) ) by A6,CKB79:1;
then (J,( ( v . vS ) . ( x | a ) ) |= p iff J,( ( v . vS ) . ( ( ( x | a ) +* vS19 ) +* vS29 ) ) |= p) by A1,A8,A5;
hence thesis by A29;
end;
((for a holds J,( ( v . vS ) . ( ( ( x | a ) +* vS19 ) +* vS29 ) ) |= p) implies (for a holds J,( ( v . ( ( vS +* vS1 ) +* vS2 ) ) . ( x | a ) ) |= p))
proof
assume A30: (for a holds J,( ( v . vS ) . ( ( ( x | a ) +* vS19 ) +* vS29 ) ) |= p);
let a;
( ( v . vS ) . ( ( ( x | a ) +* vS19 ) +* vS29 ) ) = ( ( v . ( ( vS +* vS1 ) +* vS2 ) ) . ( x | a ) ) by A12;
hence thesis by A30;
end;
hence thesis by A9,A28,A26,CKB69:1;
end;
