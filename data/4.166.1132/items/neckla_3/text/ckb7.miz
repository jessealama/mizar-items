begin
reserve a for set;
reserve b for set;
theorem
Th5: (for X being 1 -element set holds (for R being (Relation of X) holds R is_symmetric_in X))
proof
let X being 1 -element set;
let R being (Relation of X);
consider x being set such that A1: X = { x } by ZFMISC_1:131;
let a being set;
let b being set;
assume that
A2: a in X
and
A3: (b in X & [ a,b ] in R);
a = x by A1,A2,TARSKI:def 1;
hence thesis by A1,A3,TARSKI:def 1;
end;
