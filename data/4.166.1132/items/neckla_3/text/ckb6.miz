environ
vocabularies NUMBERS,RELAT_1,XBOOLE_0,ZFMISC_1,TARSKI,RELAT_2,STRUCT_0,FINSET_1,ORDERS_2,CARD_1,CAT_1,YELLOW_0,WELLORD1,NECKLA_2,NECKLACE,SUBSET_1,FUNCT_1,XXREAL_0,SEQM_3,REWRITE1,FINSEQ_1,NAT_1,ARYTM_3,FINSEQ_5,EQREL_1,MSUALG_5,CLASSES2,FUNCOP_1,FUNCT_4,ARYTM_1,NECKLA_3;
notations TARSKI,XBOOLE_0,ENUMSET1,SUBSET_1,ZFMISC_1,RELAT_1,RELAT_2,RELSET_1,FUNCT_1,PARTFUN1,FUNCT_2,FINSET_1,CARD_1,NUMBERS,XCMPLX_0,XXREAL_0,NAT_1,DOMAIN_1,STRUCT_0,ORDERS_2,WAYBEL_1,FUNCOP_1,CLASSES2,NECKLACE,WAYBEL_0,FUNCT_4,REWRITE1,FINSEQ_1,FINSEQ_5,EQREL_1,MSUALG_5,YELLOW_0,NECKLA_2,WELLORD1;
definitions XBOOLE_0,TARSKI,NECKLACE,RELAT_2,FUNCOP_1,WELLORD1,STRUCT_0;
theorems RELSET_1,NECKLACE,XBOOLE_0,WAYBEL_0,YELLOW_0,FUNCT_1,FUNCT_2,ORDERS_2,RELAT_2,SUBSET_1,ZFMISC_1,XBOOLE_1,TARSKI,MCART_1,CLASSES2,CLASSES1,NECKLA_2,FUNCOP_1,ENUMSET1,RELAT_1,SYSREL,PARTFUN1,GROUP_6,CARD_2,REWRITE1,FINSEQ_1,FINSEQ_3,FINSEQ_5,NAT_1,EQREL_1,MSUALG_6,MSUALG_5,CARD_1,NAT_2,FUNCT_4,FUNCT_3,WAYBEL_1,YELLOW_6,REALSET1,XREAL_1,XXREAL_0,ORDINAL1,NAT_D,STRUCT_0,CKB1,CKB2,CKB3,CKB5;
schemes NAT_1;
registrations XBOOLE_0,SUBSET_1,RELAT_1,FUNCT_1,PARTFUN1,FUNCT_2,FUNCOP_1,FINSET_1,XXREAL_0,XREAL_0,NAT_1,CARD_1,FINSEQ_1,REALSET1,STRUCT_0,ORDERS_2,YELLOW_0,NECKLACE,NECKLA_2,ORDINAL1,FUNCT_4,RELSET_1,ZFMISC_1,CKB4;
constructors XXREAL_0,NAT_1,EQREL_1,CLASSES1,TOLER_1,CLASSES2,FINSEQ_5,REWRITE1,REALSET2,MSUALG_5,ORDERS_3,WAYBEL_1,NECKLACE,NECKLA_2,RELSET_1;
requirements BOOLE,SUBSET,REAL,NUMERALS,ARITHM;
begin
reserve a for set;
registration
let X being  trivial set;
cluster  ->  trivial  reflexive  symmetric  transitive  strongly_connected for (Relation of X);
correctness
proof
let R being (Relation of X,X);
A1: R is_reflexive_in ( field R )
proof
per cases ;
suppose A2: R is  empty;

let x being set;
assume x in ( field R );
hence thesis by A2,RELAT_1:40;
end;
suppose R is non  empty;

then consider z being set such that A3: R = { [ z,z ] } by CKB5:1;
let x being set;
assume x in ( field R );
then A4: x in ( ( dom R ) \/ ( rng R ) ) by RELAT_1:def 6;
(( dom R ) = { z } & ( rng R ) = { z }) by A3,RELAT_1:9;
then x = z by A4,TARSKI:def 1;
hence thesis by A3,TARSKI:def 1;
end;
end;
A5: R is_transitive_in ( field R )
proof
per cases ;
suppose A6: R is  empty;

let x being set;
let y being set;
let z being set;
assume that
x in ( field R )
and
y in ( field R )
and
z in ( field R )
and
A7: [ x,y ] in R
and
[ y,z ] in R;
thus thesis by A6,A7;
end;
suppose A8: R is non  empty;

let x being set;
let y being set;
let z being set;
assume that
x in ( field R )
and
y in ( field R )
and
z in ( field R )
and
A9: [ x,y ] in R
and
A10: [ y,z ] in R;
consider a being set such that A11: R = { [ a,a ] } by A8,CKB5:1;
[ y,z ] = [ a,a ] by A11,A10,TARSKI:def 1;
then A12: z = a by ZFMISC_1:27;
[ x,y ] = [ a,a ] by A11,A9,TARSKI:def 1;
then x = a by ZFMISC_1:27;
hence thesis by A11,A12,TARSKI:def 1;
end;
end;
A13: R is_strongly_connected_in ( field R )
proof
per cases ;
suppose A14: R is  empty;

let x being set;
let y being set;
assume that
A15: x in ( field R )
and
y in ( field R );
thus thesis by A14,A15,RELAT_1:40;
end;
suppose A16: R is non  empty;

let x being set;
let y being set;
assume that
A17: x in ( field R )
and
A18: y in ( field R );
consider a being set such that A19: R = { [ a,a ] } by A16,CKB5:1;
A20: (( dom R ) = { a } & ( rng R ) = { a }) by A19,RELAT_1:9;
y in ( ( dom R ) \/ ( rng R ) ) by A18,RELAT_1:def 6;
then A21: y = a by A20,TARSKI:def 1;
x in ( ( dom R ) \/ ( rng R ) ) by A17,RELAT_1:def 6;
then x = a by A20,TARSKI:def 1;
hence thesis by A19,A21,TARSKI:def 1;
end;
end;
R is_symmetric_in ( field R )
proof
per cases ;
suppose A22: R is  empty;

let x being set;
let y being set;
assume that
x in ( field R )
and
y in ( field R )
and
A23: [ x,y ] in R;
thus thesis by A22,A23;
end;
suppose A24: R is non  empty;

let x being set;
let y being set;
assume that
x in ( field R )
and
y in ( field R )
and
A25: [ x,y ] in R;
consider a being set such that A26: R = { [ a,a ] } by A24,CKB5:1;
[ x,y ] = [ a,a ] by A26,A25,TARSKI:def 1;
then (x = a & y = a) by ZFMISC_1:27;
hence thesis by A26,TARSKI:def 1;
end;
end;
hence thesis by A1,A5,A13,RELAT_2:def 9,RELAT_2:def 11,RELAT_2:def 15,RELAT_2:def 16;
end;
end;
