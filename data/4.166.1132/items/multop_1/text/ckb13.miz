environ
vocabularies FUNCT_1,XBOOLE_0,SUBSET_1,ZFMISC_1,MULTOP_1;
notations XBOOLE_0,ZFMISC_1,SUBSET_1,FUNCT_1,FUNCT_2,MCART_1,DOMAIN_1,CKB1,CKB2,CKB6,CKB11,CKB12;
definitions CKB1,CKB11;
theorems FUNCT_2,MCART_1,CKB1,CKB3,CKB4,CKB5,CKB11;
schemes FUNCT_2,CKB7,CKB8,CKB9,CKB10;
registrations XBOOLE_0,SUBSET_1,RELSET_1;
constructors FUNCT_2,DOMAIN_1,RELSET_1,CKB1,CKB2,CKB11,CKB12;
requirements SUBSET,BOOLE;
begin
reserve D for non empty set;
reserve X for set;
reserve Y for set;
reserve Z for set;
reserve S for set;
reserve x for set;
reserve y for set;
reserve z for set;
reserve s for set;
reserve t for set;
theorem
Th6: (for f1,f2 being (Function of [: X,Y,Z,S :],D) holds ((for x holds (for y holds (for z holds (for s holds ((((x in X & y in Y) & z in Z) & s in S) implies ( f1 . [ x,y,z,s ] ) = ( f2 . [ x,y,z,s ] )))))) implies f1 = f2))
proof
let f1 being (Function of [: X,Y,Z,S :],D),f2 being (Function of [: X,Y,Z,S :],D);
assume that
A1: (for x holds (for y holds (for z holds (for s holds ((((x in X & y in Y) & z in Z) & s in S) implies ( f1 . [ x,y,z,s ] ) = ( f2 . [ x,y,z,s ] ))))));
(for t holds (t in [: X,Y,Z,S :] implies ( f1 . t ) = ( f2 . t )))
proof
let t;
assume t in [: X,Y,Z,S :];
then (ex x st (ex y st (ex z st (ex s st ((((x in X & y in Y) & z in Z) & s in S) & t = [ x,y,z,s ]))))) by MCART_1:79;
hence thesis by A1;
end;
hence thesis by FUNCT_2:12;
end;
