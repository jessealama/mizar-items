constructors REAL_1,SQUARE_1,BORSUK_1,MONOID_0,CONVEX1,TOPREAL9,BINARITH,FVSUM_1,LAPLACE,MATRIX_6,MATRIX11,TOPS_2,MATRIX13,MATRLIN2,MATRTOP1,REALSET1,RLVECT_3,SEQ_1,MATRIXJ1,RCOMP_1,ABIAN,SIN_COS,MATRIX_7,SETWISEO,GROUP_4,TMAP_1,FINSOP_1,MATRIXR2,SEQ_4,CKB4,CKB15,CKB27;
requirements NUMERALS,BOOLE,SUBSET,ARITHM,REAL;
begin
reserve r1 for  real number;
reserve r2 for  real number;
reserve i for Nat;
reserve j for Nat;
reserve k for Nat;
reserve m for Nat;
reserve n for Nat;
theorem
Th9: (((1 <= i & i < j) & j <= n) implies ( ( Rotation (i,j,n,r1) ) * ( Rotation (i,j,n,r2) ) ) = ( Rotation (i,j,n,( r1 + r2 )) ))
proof
assume that
A1: 1 <= i
and
A2: i < j
and
A3: j <= n;
set S = ( Seg n );
1 <= j by A1,A2,XXREAL_0:2;
then A4: j in S by A3,FINSEQ_1:1;
set O1 = ( Rotation (i,j,n,r1) );
set O2 = ( Rotation (i,j,n,r2) );
set O = ( Rotation (i,j,n,( r1 + r2 )) );
set O12 = ( O1 * O2 );
A5: ( width O1 ) = n by MATRIX_1:24;
i <= n by A2,A3,XXREAL_0:2;
then A6: i in S by A1,FINSEQ_1:1;
A7: ( Indices O1 ) = [: S,S :] by MATRIX_1:24;
A8: ( Indices O12 ) = [: S,S :] by MATRIX_1:24;
A9: ( Indices O ) = [: S,S :] by MATRIX_1:24;
A10: ( len O2 ) = n by MATRIX_1:25;
(for k holds (for m holds ([ k,m ] in ( Indices O12 ) implies ( O12 * (k,m) ) = ( O * (k,m) ))))
proof
let k;
let m;
assume A11: [ k,m ] in ( Indices O12 );
then A12: k in S by A8,ZFMISC_1:87;
A13: ( O12 * (k,m) ) = ( ( Line (O1,k) ) "*" ( Col (O2,m) ) ) by A5,A10,A11,MATRIX_3:def 4;
( len ( @ ( Line (O1,k) ) ) ) = n by A5,CARD_1:def 7;
then reconsider L = ( @ ( Line (O1,k) ) ) as (Element of ( TOP-REAL n )) by TOPREAL7:17;
A14: m in S by A8,A11,ZFMISC_1:87;
then A15: ( L . m ) = ( O1 * (k,m) ) by A5,MATRIX_1:def 7;
A16: ( @ L ) = ( Line (O1,k) );
A17: ( L . i ) = ( O1 * (k,i) ) by A5,A6,MATRIX_1:def 7;
A18: ( L . j ) = ( O1 * (k,j) ) by A4,A5,MATRIX_1:def 7;
per cases ;
suppose A19: m = i;

then A20: ( ( Line (O1,k) ) "*" ( Col (O2,m) ) ) = ( ( ( L . i ) * ( cos r2 ) ) + ( ( L . j ) * ( - ( sin r2 ) ) ) ) by A1,A2,A3,A16,CKB30:1;
per cases ;
suppose A21: k = i;

hence ( O12 * (k,m) ) = ( ( ( cos r1 ) * ( cos r2 ) ) + ( ( L . j ) * ( - ( sin r2 ) ) ) ) by A1,A2,A3,A13,A17,A20,CKB27:def 1
.= ( ( ( cos r1 ) * ( cos r2 ) ) + ( ( sin r1 ) * ( - ( sin r2 ) ) ) ) by A1,A2,A3,A18,A21,CKB27:def 1
.= ( ( ( cos r1 ) * ( cos r2 ) ) - ( ( sin r1 ) * ( sin r2 ) ) )
.= ( cos ( r1 + r2 ) ) by SIN_COS:75
.= ( O * (k,m) ) by A1,A2,A3,A19,A21,CKB27:def 1;
end;
suppose A22: k = j;

hence ( O12 * (k,m) ) = ( ( ( - ( sin r1 ) ) * ( cos r2 ) ) + ( ( L . j ) * ( - ( sin r2 ) ) ) ) by A1,A2,A3,A13,A17,A20,CKB27:def 1
.= ( ( ( - ( sin r1 ) ) * ( cos r2 ) ) + ( ( cos r1 ) * ( - ( sin r2 ) ) ) ) by A1,A2,A3,A18,A22,CKB27:def 1
.= ( - ( ( ( sin r1 ) * ( cos r2 ) ) + ( ( cos r1 ) * ( sin r2 ) ) ) )
.= ( - ( sin ( r1 + r2 ) ) ) by SIN_COS:75
.= ( O * (k,m) ) by A1,A2,A3,A19,A22,CKB27:def 1;
end;
suppose (k <> j & k <> i);

then (not k in { i,j }) by TARSKI:def 2;
then A23: ({ k,i } <> { i,j } & { k,j } <> { i,j }) by TARSKI:def 2;
A24: [ k,j ] in [: S,S :] by A4,A12,ZFMISC_1:87;
A25: [ k,i ] in [: S,S :] by A6,A12,ZFMISC_1:87;
then ( O1 * (k,i) ) = ( 0. ( F_Real ) ) by A1,A2,A3,A7,A23,CKB27:def 1;
hence ( O12 * (k,m) ) = ( ( ( 0 ) * ( cos r2 ) ) + ( ( 0 ) * ( - ( sin r2 ) ) ) ) by A1,A2,A3,A7,A13,A17,A18,A20,A23,A24,CKB27:def 1
.= ( O * (k,m) ) by A1,A2,A3,A9,A19,A23,A25,CKB27:def 1;
end;
end;
suppose A26: m = j;

then A27: ( ( Line (O1,k) ) "*" ( Col (O2,m) ) ) = ( ( ( L . i ) * ( sin r2 ) ) + ( ( L . j ) * ( cos r2 ) ) ) by A1,A2,A3,A16,CKB31:1;
per cases ;
suppose A28: k = i;

hence ( O12 * (k,m) ) = ( ( ( cos r1 ) * ( sin r2 ) ) + ( ( L . j ) * ( cos r2 ) ) ) by A1,A2,A3,A13,A17,A27,CKB27:def 1
.= ( ( ( cos r1 ) * ( sin r2 ) ) + ( ( sin r1 ) * ( cos r2 ) ) ) by A1,A2,A3,A18,A28,CKB27:def 1
.= ( sin ( r1 + r2 ) ) by SIN_COS:75
.= ( O * (k,m) ) by A1,A2,A3,A26,A28,CKB27:def 1;
end;
suppose A29: k = j;

hence ( O12 * (k,m) ) = ( ( ( - ( sin r1 ) ) * ( sin r2 ) ) + ( ( L . j ) * ( cos r2 ) ) ) by A1,A2,A3,A13,A17,A27,CKB27:def 1
.= ( ( ( cos r1 ) * ( cos r2 ) ) - ( ( sin r1 ) * ( sin r2 ) ) ) by A1,A2,A3,A18,A29,CKB27:def 1
.= ( cos ( r1 + r2 ) ) by SIN_COS:75
.= ( O * (k,m) ) by A1,A2,A3,A26,A29,CKB27:def 1;
end;
suppose (k <> j & k <> i);

then (not k in { i,j }) by TARSKI:def 2;
then A30: ({ k,i } <> { i,j } & { k,j } <> { i,j }) by TARSKI:def 2;
A31: [ k,j ] in [: S,S :] by A4,A12,ZFMISC_1:87;
[ k,i ] in [: S,S :] by A6,A12,ZFMISC_1:87;
then ( O1 * (k,i) ) = ( 0. ( F_Real ) ) by A1,A2,A3,A7,A30,CKB27:def 1;
hence ( O12 * (k,m) ) = ( ( ( 0 ) * ( sin r2 ) ) + ( ( 0 ) * ( cos r2 ) ) ) by A1,A2,A3,A7,A13,A17,A18,A27,A30,A31,CKB27:def 1
.= ( O * (k,m) ) by A1,A2,A3,A9,A26,A30,A31,CKB27:def 1;
end;
end;
suppose A32: (m <> i & m <> j);

then A33: ( O12 * (k,m) ) = ( L . m ) by A1,A2,A3,A13,A14,A16,CKB29:1;
A34: [ k,m ] in [: S,S :] by A11,MATRIX_1:24;
per cases ;
suppose A35: k = m;

then ( O1 * (k,m) ) = ( 1. ( F_Real ) ) by A1,A2,A3,A7,A8,A11,A32,CKB27:def 1;
hence thesis by A1,A2,A3,A9,A15,A32,A33,A34,A35,CKB27:def 1;
end;
suppose A36: k <> m;

(not m in { i,j }) by A32,TARSKI:def 2;
then A37: { k,m } <> { i,j } by TARSKI:def 2;
then ( O1 * (k,m) ) = ( 0. ( F_Real ) ) by A1,A2,A3,A7,A8,A11,A36,CKB27:def 1;
hence thesis by A1,A2,A3,A9,A15,A33,A34,A36,A37,CKB27:def 1;
end;
end;
end;
hence thesis by MATRIX_1:27;
end;
