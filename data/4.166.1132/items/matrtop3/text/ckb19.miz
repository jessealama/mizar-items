begin
reserve i for Nat;
reserve k for Nat;
reserve m for Nat;
reserve n for Nat;
theorem
Th15: (i in ( Seg n ) implies (( AxialSymmetry (i,n) ) is  diagonal & ( ( AxialSymmetry (i,n) ) ~ ) = ( AxialSymmetry (i,n) )))
proof
set S = ( Seg n );
set A = ( AxialSymmetry (i,n) );
set ONE = ( 1. (( F_Real ),n) );
set AA = ( A * A );
assume A1: i in S;
(for k holds (for m holds (([ k,m ] in ( Indices A ) & ( A * (k,m) ) <> ( 0. ( F_Real ) )) implies k = m))) by A1,CKB15:def 1;
hence A is  diagonal by MATRIX_1:def 14;
(for k holds (for m holds ([ k,m ] in ( Indices AA ) implies ( AA * (k,m) ) = ( ONE * (k,m) ))))
proof
let k;
let m;
assume that
A2: [ k,m ] in ( Indices AA );
A3: ( width A ) = n by MATRIX_1:24;
then ( len ( @ ( Line (A,k) ) ) ) = n by CARD_1:def 7;
then reconsider L = ( @ ( Line (A,k) ) ) as (Element of ( TOP-REAL n )) by TOPREAL7:17;
( len A ) = n by MATRIX_1:25;
then A4: ( AA * (k,m) ) = ( ( @ L ) "*" ( Col (A,m) ) ) by A2,A3,MATRIX_3:def 4;
A5: ( Indices AA ) = [: S,S :] by MATRIX_1:24;
then A6: m in S by A2,ZFMISC_1:87;
then A7: ( ( Line (A,k) ) . m ) = ( A * (k,m) ) by A3,MATRIX_1:def 7;
A8: ( Indices A ) = [: S,S :] by MATRIX_1:24;
A9: ( Indices ONE ) = [: S,S :] by MATRIX_1:24;
per cases ;
suppose A10: m <> i;

then A11: ( AA * (k,m) ) = ( A * (k,m) ) by A1,A4,A6,A7,CKB17:1;
per cases ;
suppose A12: k <> m;

then ( ONE * (k,m) ) = ( 0. ( F_Real ) ) by A2,A5,A9,MATRIX_1:def 11;
hence thesis by A1,A2,A5,A8,A11,A12,CKB15:def 1;
end;
suppose A13: k = m;

then ( ONE * (k,m) ) = ( 1. ( F_Real ) ) by A2,A5,A9,MATRIX_1:def 11;
hence thesis by A1,A2,A5,A8,A10,A11,A13,CKB15:def 1;
end;
end;
suppose A14: m = i;

then A15: ( AA * (k,m) ) = ( - ( A * (k,m) ) ) by A4,A6,A7,CKB18:1;
per cases ;
suppose A16: k <> m;

then ( A * (k,m) ) = ( 0. ( F_Real ) ) by A1,A2,A5,A8,CKB15:def 1;
hence thesis by A2,A5,A9,A15,A16,MATRIX_1:def 11;
end;
suppose A17: k = m;

then ( AA * (k,m) ) = ( - ( - ( 1. ( F_Real ) ) ) ) by A1,A14,A15,CKB15:def 1;
hence thesis by A2,A5,A9,A17,MATRIX_1:def 11;
end;
end;
end;
then AA = ONE by MATRIX_1:27;
then A is_reverse_of A by MATRIX_6:def 2;
hence thesis by MATRIX_6:def 4;
end;
