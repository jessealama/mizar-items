begin
reserve i for Nat;
reserve j for Nat;
reserve k for Nat;
reserve m for Nat;
reserve n for Nat;
theorem
Th10: (((1 <= i & i < j) & j <= n) implies ( Rotation (i,j,n,( 0 )) ) = ( 1. (( F_Real ),n) ))
proof
set O = ( Rotation (i,j,n,( 0 )) );
assume A1: ((1 <= i & i < j) & j <= n);
A2: (for k holds (for m holds (([ k,m ] in ( Indices O ) & k <> m) implies ( O * (k,m) ) = ( 0. ( F_Real ) ))))
proof
let k;
let m;
assume that
A4: [ k,m ] in ( Indices O )
and
A5: k <> m;
per cases ;
suppose ((k = i & m = j) or (k = j & m = i));

then (( O * (k,m) ) = ( sin ( 0 ) ) or ( O * (k,m) ) = ( - ( sin ( 0 ) ) )) by A1,CKB27:def 1;
hence thesis by SIN_COS:31;
end;
suppose (k = i & m <> j);

then (not m in { i,j }) by A5,TARSKI:def 2;
then { k,m } <> { i,j } by TARSKI:def 2;
hence thesis by A1,A4,A5,CKB27:def 1;
end;
suppose (k = j & m <> i);

then (not m in { i,j }) by A5,TARSKI:def 2;
then { k,m } <> { i,j } by TARSKI:def 2;
hence thesis by A1,A4,A5,CKB27:def 1;
end;
suppose (m = i & k <> j);

then (not k in { i,j }) by A5,TARSKI:def 2;
then { k,m } <> { i,j } by TARSKI:def 2;
hence thesis by A1,A4,A5,CKB27:def 1;
end;
suppose (m = j & k <> i);

then (not k in { i,j }) by A5,TARSKI:def 2;
then { k,m } <> { i,j } by TARSKI:def 2;
hence thesis by A1,A4,A5,CKB27:def 1;
end;
suppose (((k <> i & k <> j) & m <> i) & m <> j);

then (not m in { i,j }) by TARSKI:def 2;
then { k,m } <> { i,j } by TARSKI:def 2;
hence thesis by A1,A4,A5,CKB27:def 1;
end;
end;
(for k holds ([ k,k ] in ( Indices O ) implies ( O * (k,k) ) = ( 1. ( F_Real ) )))
proof
let k;
assume A6: [ k,k ] in ( Indices O );
((k = i or k = j) or (k <> i & k <> j));
hence thesis by A1,A6,CKB27:def 1,SIN_COS:31;
end;
hence thesis by A2,MATRIX_1:def 11;
end;
