begin
theorem
Th21: (for n being Ordinal holds (for T being  connected (TermOrder of n) holds (for L being  add-associative  right_zeroed  right_complementable  distributive  domRing-like non  trivial doubleLoopStr holds (for p being (Polynomial of n,L) holds (for a being non  zero (Element of L) holds ( HT (( a * p ),T) ) = ( HT (p,T) ))))))
proof
let n being Ordinal;
let T being  connected (TermOrder of n);
let L being  add-associative  right_zeroed  right_complementable  distributive  domRing-like non  trivial doubleLoopStr;
let p being (Polynomial of n,L);
let a being non  zero (Element of L);
set ht = ( HT (( a * p ),T) );
set htp = ( HT (p,T) );
per cases ;
suppose A1: ( Support ( a * p ) ) = ( {} );

now
assume ( Support p ) <> ( {} );
then reconsider sp = ( Support p ) as non  empty set;
set u = the (Element of sp);
u in ( Support p );
then reconsider u9 = u as (Element of ( Bags n ));
A2: ( ( a * p ) . u9 ) = ( a * ( p . u9 ) ) by POLYNOM7:def 9;
(( p . u9 ) <> ( 0. L ) & a <> ( 0. L )) by POLYNOM1:def 3;
then ( ( a * p ) . u9 ) <> ( 0. L ) by A2,VECTSP_2:def 1;
hence contradiction by A1,POLYNOM1:def 3;
end;
hence htp = ( EmptyBag n ) by TERMORD:def 6
.= ht by A1,TERMORD:def 6;
end;
suppose A3: ( Support ( a * p ) ) <> ( {} );

now
reconsider sp = ( Support ( a * p ) ) as non  empty set by A3;
set u = the (Element of sp);
u in ( Support ( a * p ) );
then reconsider u9 = u as (Element of ( Bags n ));
(( ( a * p ) . u9 ) <> ( 0. L ) & ( ( a * p ) . u9 ) = ( a * ( p . u9 ) )) by POLYNOM1:def 3,POLYNOM7:def 9;
then A4: ( p . u9 ) <> ( 0. L ) by VECTSP_1:6;
assume ( Support p ) = ( {} );
hence contradiction by A4,POLYNOM1:def 3;
end;
then htp in ( Support p ) by TERMORD:def 6;
then A5: ( p . htp ) <> ( 0. L ) by POLYNOM1:def 3;
A6:now
let b being (bag of n);
assume A7: b in ( Support ( a * p ) );
( Support ( a * p ) ) c= ( Support p ) by CKB34:1;
hence b <= htp,T by A7,TERMORD:def 6;
end;
( ( a * p ) . htp ) = ( a * ( p . htp ) ) by POLYNOM7:def 9;
then ( ( a * p ) . htp ) <> ( 0. L ) by A5,VECTSP_2:def 1;
then htp in ( Support ( a * p ) ) by POLYNOM1:def 3;
hence thesis by A6,TERMORD:def 6;
end;
end;
