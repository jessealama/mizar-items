theorem
Th19: (for n being Ordinal holds (for L being  add-associative  right_zeroed  right_complementable  distributive non  trivial doubleLoopStr holds (for p being (Polynomial of n,L) holds (for a being (Element of L) holds ( Support ( a * p ) ) c= ( Support p )))))
proof
let n being Ordinal;
let L being  add-associative  right_zeroed  right_complementable  distributive non  trivial doubleLoopStr;
let p being (Polynomial of n,L);
let a9 being (Element of L);
A1: ( dom ( 0_ (n,L) ) ) = ( Bags n ) by FUNCT_2:def 1
.= ( dom ( a9 * p ) ) by FUNCT_2:def 1;
per cases ;
suppose A2: a9 = ( 0. L );

now
let u being set;
assume u in ( dom ( a9 * p ) );
then reconsider u9 = u as (Element of ( Bags n ));
( ( a9 * p ) . u9 ) = ( a9 * ( p . u9 ) ) by POLYNOM7:def 9
.= ( 0. L ) by A2,VECTSP_1:7
.= ( ( 0_ (n,L) ) . u9 ) by POLYNOM1:22;
hence ( ( a9 * p ) . u ) = ( ( 0_ (n,L) ) . u );
end;
then ( a9 * p ) = ( 0_ (n,L) ) by A1,FUNCT_1:2;
then (for u being set holds (u in ( Support ( a9 * p ) ) implies u in ( Support p ))) by POLYNOM7:1;
hence thesis by TARSKI:def 3;
end;
suppose a9 <> ( 0. L );

then reconsider a = a9 as non  zero (Element of L) by STRUCT_0:def 12;
now
let u being set;
assume A3: u in ( Support ( a * p ) );
then reconsider u9 = u as (Element of ( Bags n ));
A4: ( ( a * p ) . u9 ) = ( a * ( p . u9 ) ) by POLYNOM7:def 9;
( ( a * p ) . u9 ) <> ( 0. L ) by A3,POLYNOM1:def 3;
then ( p . u9 ) <> ( 0. L ) by A4,VECTSP_1:6;
hence u in ( Support p ) by POLYNOM1:def 3;
end;
hence thesis by TARSKI:def 3;
end;
end;
