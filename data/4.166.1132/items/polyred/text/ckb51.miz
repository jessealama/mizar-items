begin
theorem
Th29: (for n being Ordinal holds (for T being  connected (TermOrder of n) holds (for L being non empty addLoopStr holds (for p,q being (Polynomial of n,L) holds (p <= q,T iff (not q < p,T))))))
proof
let n being Ordinal;
let T being  connected (TermOrder of n);
let L being non empty addLoopStr;
let p being (Polynomial of n,L),q being (Polynomial of n,L);
A1: ((not q < p,T) implies p <= q,T)
proof
assume A2: (not q < p,T);
now
per cases  by A2,CKB43:def 1;
case (not ( Support q ) <> ( Support p ));
hence thesis by CKB48:1;
end;
case (not q <= p,T);
hence thesis by CKB50:1;
end;
end;
hence thesis;
end;
(p <= q,T implies (not q < p,T))
proof
assume A3: p <= q,T;
assume A4: q < p,T;
then q <= p,T by CKB43:def 1;
then ( Support q ) = ( Support p ) by A3,CKB48:1;
hence thesis by A4,CKB43:def 1;
end;
hence thesis by A1;
end;
