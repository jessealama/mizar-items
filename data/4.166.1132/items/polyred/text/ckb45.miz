begin
theorem
Th24: (for n being Ordinal holds (for T being  connected (TermOrder of n) holds (for L being non  trivial ZeroStr holds (for p being  non-zero (Polynomial of n,L) holds ( PosetMax ( Support (p,T) ) ) = ( HT (p,T) )))))
proof
let n being Ordinal;
let T being  connected (TermOrder of n);
let L being non  trivial ZeroStr;
let p being  non-zero (Polynomial of n,L);
set htp = ( HT (p,T) );
set R = RelStr (# ( Bags n ),T #);
set sp = ( Support (p,T) );
p <> ( 0_ (n,L) ) by POLYNOM7:def 1;
then ( Support p ) <> ( {} ) by POLYNOM7:1;
then A1: htp in ( Support p ) by TERMORD:def 6;
now
assume (ex y being set st ((y in sp & y <> htp) & [ htp,y ] in (the InternalRel of R)));
then consider y being set such that A2: y in sp and A3: y <> htp and A4: [ htp,y ] in (the InternalRel of R);
y is (Element of ( Bags n )) by A2;
then reconsider y9 = y as (bag of n);
(y9 <= htp,T & htp <= y9,T) by A2,A4,TERMORD:def 2,TERMORD:def 6;
hence contradiction by A3,TERMORD:7;
end;
then htp is_maximal_wrt ( Support (p,T) ),(the InternalRel of R) by A1,WAYBEL_4:def 23;
hence thesis by A1,BAGORDER:def 13;
end;
