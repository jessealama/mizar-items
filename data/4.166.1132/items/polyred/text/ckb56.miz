requirements NUMERALS,REAL,SUBSET,BOOLE;
begin
theorem
Lm16: (for n being Ordinal holds (for T being  connected  admissible (TermOrder of n) holds (for L being  add-associative  right_complementable  right_zeroed non trivial addLoopStr holds (for p,q being (Polynomial of n,L) holds (q <> ( 0_ (n,L) ) implies ((( HT (p,T) ) = ( HT (q,T) ) & ( Red (p,T) ) <= ( Red (q,T) ),T) implies p <= q,T))))))
proof
let n being Ordinal;
let T being  connected  admissible (TermOrder of n);
let L being  add-associative  right_complementable  right_zeroed non trivial addLoopStr;
let p being (Polynomial of n,L),q being (Polynomial of n,L);
assume A1: q <> ( 0_ (n,L) );
set x = ( Support (p,T) );
set y = ( Support (q,T) );
set rp = ( Red (p,T) );
set rq = ( Red (q,T) );
set R = RelStr (# ( Bags n ),T #);
assume that
A2: ( HT (p,T) ) = ( HT (q,T) )
and
A3: ( Red (p,T) ) <= ( Red (q,T) ),T;
[ ( Support ( Red (p,T) ) ),( Support ( Red (q,T) ) ) ] in ( FinOrd R ) by A3,CKB42:def 1;
then A4: [ ( Support ( Red (p,T) ) ),( Support ( Red (q,T) ) ) ] in ( union ( rng ( FinOrd-Approx R ) ) ) by BAGORDER:def 15;
now
per cases ;
case p = ( 0_ (n,L) );
hence thesis by CKB47:1;
end;
case A5: p <> ( 0_ (n,L) );
then A6: x <> ( {} ) by POLYNOM7:1;
A7: q is  non-zero by A1,POLYNOM7:def 1;
A8: p is  non-zero by A5,POLYNOM7:def 1;
A9: ( Support rp ) = ( ( Support p ) \ { ( HT (p,T) ) } ) by TERMORD:36
.= ( x \ { ( PosetMax x ) } ) by A8,CKB45:1;
A10: y <> ( {} ) by A1,POLYNOM7:1;
A11: ( Support rq ) = ( ( Support q ) \ { ( HT (q,T) ) } ) by TERMORD:36
.= ( y \ { ( PosetMax y ) } ) by A7,CKB45:1;
( PosetMax x ) = ( HT (q,T) ) by A2,A8,CKB45:1
.= ( PosetMax y ) by A7,CKB45:1;
then [ x,y ] in ( union ( rng ( FinOrd-Approx R ) ) ) by A4,A6,A10,A9,A11,BAGORDER:35;
then [ x,y ] in ( FinOrd R ) by BAGORDER:def 15;
hence thesis by CKB42:def 1;
end;
end;
hence thesis;
end;
