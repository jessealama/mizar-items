constructors REWRITE1,VECTSP_2,TRIANG_1,WAYBEL_4,WELLFND1,IDEAL_1,BAGORDER,TERMORD,DOMAIN_1,RELSET_1,BINOP_2,FVSUM_1,VFUNCT_1,CKB24;
requirements NUMERALS,REAL,SUBSET,BOOLE;
begin
registration
let n being Nat;
let T being  admissible (TermOrder of n);
cluster RelStr (# ( Bags n ),T #) ->  well_founded;
coherence
proof
set R = RelStr (# ( Bags n ),T #);
set X = (the carrier of R);
now
let Y being set;
assume that
A1: Y c= X
and
A2: Y <> ( {} );
now
let u being set;
assume u in Y;
then reconsider u9 = u as (bag of n) by A1;
u9 <= u9,T by TERMORD:6;
then [ u9,u9 ] in T by TERMORD:def 2;
hence u in ( field T ) by RELAT_1:15;
end;
then Y c= ( field T ) by TARSKI:def 3;
hence (ex a being set st (a in Y & ( T -Seg a ) misses Y)) by A2,WELLORD1:def 2;
end;
then T is_well_founded_in (the carrier of R) by WELLORD1:def 3;
hence thesis by WELLFND1:def 2;
end;
end;
