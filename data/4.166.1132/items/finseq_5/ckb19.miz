begin
theorem
Th16: (for D being non empty set holds (for p being (Element of D) holds (for f being (FinSequence of D) holds ( ( <* p *> ^ f ) /. 1 ) = p)))
proof
let D being non empty set;
let p being (Element of D);
let f being (FinSequence of D);
( len ( <* p *> ^ f ) ) = ( 1 + ( len f ) ) by CKB11:1;
then 1 <= ( len ( <* p *> ^ f ) ) by NAT_1:11;
then 1 in ( dom ( <* p *> ^ f ) ) by FINSEQ_3:25;
then ( ( <* p *> ^ f ) /. 1 ) = ( ( <* p *> ^ f ) . 1 ) by PARTFUN1:def 6;
hence thesis by FINSEQ_1:41;
end;
