begin
reserve D for non empty set;
reserve p for (Element of D);
theorem
(for D being set holds (for f being (FinSequence of D) holds (p in ( rng f ) implies ( ( f -| p ) ^ <* p *> ) = ( f | ( p .. f ) ))))
proof
let D being set;
let f being (FinSequence of D);
assume A1: p in ( rng f );
then consider n being Nat such that A2: n = ( ( p .. f ) - 1 ) and A3: ( f -| p ) = ( f | ( Seg n ) ) by FINSEQ_4:def 5;
(( n + 1 ) in ( dom f ) & ( f . ( n + 1 ) ) = p) by A1,A2,FINSEQ_4:19,FINSEQ_4:20;
hence thesis by A2,A3,CKB13:1;
end;
