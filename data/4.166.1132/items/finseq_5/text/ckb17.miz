begin
reserve k for Nat;
reserve D for non empty set;
reserve f for (FinSequence of D);
reserve g for (FinSequence of D);
theorem
Th14: ((( len f ) = ( len g ) & (for k holds ((1 <= k & k <= ( len f )) implies ( f /. k ) = ( g /. k )))) implies f = g)
proof
assume that
A1: ( len f ) = ( len g )
and
A2: (for k holds ((1 <= k & k <= ( len f )) implies ( f /. k ) = ( g /. k )));
now
let k being Nat;
assume A3: (1 <= k & k <= ( len f ));
hence ( f . k ) = ( f /. k ) by FINSEQ_4:15
.= ( g /. k ) by A2,A3
.= ( g . k ) by A1,A3,FINSEQ_4:15;
end;
hence thesis by A1,FINSEQ_1:14;
end;
