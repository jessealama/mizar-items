begin
reserve i for (Element of ( NAT ));
reserve h for (Function of ( COMPLEX ),( COMPLEX ));
reserve g for (Function of ( REAL ),( REAL ));
theorem
Th55: (for y1 being (FinSequence of ( COMPLEX )) holds (for y2 being (FinSequence of ( REAL )) holds ((( len y1 ) = ( len y2 ) & (for i holds (i in ( dom y1 ) implies ( h . ( y1 . i ) ) = ( g . ( y2 . i ) )))) implies ( h * y1 ) = ( g * y2 ))))
proof
let y1 being (FinSequence of ( COMPLEX ));
let y2 being (FinSequence of ( REAL ));
assume that
A1: ( len y1 ) = ( len y2 )
and
A2: (for i holds (i in ( dom y1 ) implies ( h . ( y1 . i ) ) = ( g . ( y2 . i ) )));
A3: ( len ( g * y2 ) ) = ( len y1 ) by A1,FINSEQ_2:33;
A4:now
let i being Nat;
assume that
A5: 1 <= i
and
A6: i <= ( len ( h * y1 ) );
A7: i <= ( len y1 ) by A6,FINSEQ_2:33;
then A8: i in ( dom y1 ) by A5,FINSEQ_3:25;
A9: i in ( dom ( g * y2 ) ) by A3,A5,A7,FINSEQ_3:25;
i in ( dom ( h * y1 ) ) by A5,A6,FINSEQ_3:25;
hence ( ( h * y1 ) . i ) = ( h . ( y1 . i ) ) by FUNCT_1:12
.= ( g . ( y2 . i ) ) by A2,A8
.= ( ( g * y2 ) . i ) by A9,FUNCT_1:12;
end;
( len ( h * y1 ) ) = ( len y1 ) by FINSEQ_2:33;
hence thesis by A3,A4,FINSEQ_1:14;
end;
