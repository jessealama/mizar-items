begin
definition
let z being (FinSequence of ( COMPLEX ));
func z *' -> (FinSequence of ( COMPLEX ))
means
:Def1: (( len it ) = ( len z ) & (for i being Nat holds ((1 <= i & i <= ( len z )) implies ( it . i ) = ( ( z . i ) *' ))));
existence
proof
deffunc F(Nat) = ( ( z . $1 ) *' );
consider p being FinSequence such that A1: (( len p ) = ( len z ) & (for k being Nat holds (k in ( dom p ) implies ( p . k ) = F(k)))) from FINSEQ_1:sch 2;
( rng p ) c= ( COMPLEX )
proof
let s2 being set;
assume s2 in ( rng p );
then consider s1 being set such that A2: s1 in ( dom p ) and A3: s2 = ( p . s1 ) by FUNCT_1:def 3;
reconsider nx = s1 as (Element of ( NAT )) by A2;
( p . nx ) = ( ( z . nx ) *' ) by A1,A2;
hence thesis by A3;
end;
then A4: p is (FinSequence of ( COMPLEX )) by FINSEQ_1:def 4;
(for i being Nat holds ((1 <= i & i <= ( len z )) implies ( ( z . i ) *' ) = ( p . i )))
proof
let i being Nat;
assume (1 <= i & i <= ( len z ));
then i in ( dom p ) by A1,FINSEQ_3:25;
hence thesis by A1;
end;
hence thesis by A1,A4;
end;
uniqueness
proof
let p1 being (FinSequence of ( COMPLEX )),p2 being (FinSequence of ( COMPLEX ));
assume that
A5: ( len p1 ) = ( len z )
and
A6: (for i being Nat holds ((1 <= i & i <= ( len z )) implies ( ( z . i ) *' ) = ( p1 . i )))
and
A7: ( len p2 ) = ( len z )
and
A8: (for i being Nat holds ((1 <= i & i <= ( len z )) implies ( ( z . i ) *' ) = ( p2 . i )));
(for i being Nat holds ((1 <= i & i <= ( len p1 )) implies ( p1 . i ) = ( p2 . i )))
proof
let i being Nat;
assume A9: (1 <= i & i <= ( len p1 ));
then ( ( z . i ) *' ) = ( p1 . i ) by A5,A6;
hence thesis by A5,A8,A9;
end;
hence thesis by A5,A7,FINSEQ_1:14;
end;
