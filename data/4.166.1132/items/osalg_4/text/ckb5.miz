theorem
Th1: (for R being non  empty Poset holds (for A,B being (ManySortedSet of (the carrier of R)) holds (for OR being (ManySortedRelation of A,B) holds (OR is  os-compatible implies OR is (OrderSortedSet of R)))))
proof
let R being non  empty Poset;
let A being (ManySortedSet of (the carrier of R));
let B being (ManySortedSet of (the carrier of R));
let OR being (ManySortedRelation of A,B);
set OR1 = OR;
assume A1: OR is  os-compatible;
OR1 is  order-sorted
proof
let s1 being (Element of R);
let s2 being (Element of R);
assume that
A2: s1 <= s2;
(for x,y being set holds ([ x,y ] in ( OR . s1 ) implies [ x,y ] in ( OR . s2 )))
proof
let x being set;
let y being set;
assume that
A3: [ x,y ] in ( OR . s1 );
(x in ( A . s1 ) & y in ( B . s1 )) by A3,ZFMISC_1:87;
hence thesis by A1,A2,A3,CKB2:def 1;
end;
hence thesis by RELAT_1:def 3;
end;
hence thesis;
end;
