begin
theorem
Th9: (for R being  discrete non empty Poset holds R is  locally_directed)
proof
let R being  discrete non empty Poset;
let C being (Component of R);
consider x being (Element of R) such that A1: C = { x } by CKB25:1;
(for x,y being (Element of R) holds ((x in C & y in C) implies (ex z being (Element of R) st ((z in C & x <= z) & y <= z))))
proof
let x1 being (Element of R),y1 being (Element of R);
assume that
A2: x1 in C
and
A3: y1 in C;
take x1;
x1 = x by A1,A2,TARSKI:def 1;
hence thesis by A1,A3,TARSKI:def 1;
end;
hence thesis by WAYBEL_0:def 1;
end;
