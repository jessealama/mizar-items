begin
theorem
Th8: (for R being  discrete non  empty Poset holds (for C being (Component of R) holds (ex x being (Element of R) st C = { x })))
proof
let R being  discrete non  empty Poset;
let C being (Component of R);
consider x being set such that A1: x in (the carrier of R) and A2: C = ( Class (( Path_Rel R ),x) ) by EQREL_1:def 3;
reconsider x1 = x as (Element of R) by A1;
take x1;
(for y being set holds (y in C iff y = x1))
proof
let y being set;
hereby
assume A3: y in C;
then reconsider y1 = y as (Element of R);
[ y,x ] in ( Path_Rel R ) by A2,A3,EQREL_1:19;
then y1 = x1 by CKB24:1;
hence y = x1;
end;
assume y = x1;
hence thesis by A2,EQREL_1:20;
end;
hence thesis by TARSKI:def 1;
end;
