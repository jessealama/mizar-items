begin
reserve S for  locally_directed OrderSortedSign;
definition
let S;
let U1 being  non-empty (OSAlgebra of S);
let U2 being  non-empty (OSAlgebra of S);
let F being (ManySortedFunction of U1,U2);
let R being (OSCongruence of U1);
let s being (Element of S);
assume that
A1: F is_homomorphism U1,U2
and
A2: F is  order-sorted
and
A3: R c= ( OSCng F );
func OSHomQuot (F,R,s) -> (Function of ( (the Sorts of ( QuotOSAlg (U1,R) )) . s ),( (the Sorts of U2) . s ))
means
:Def29: (for x being (Element of ( (the Sorts of U1) . s )) holds ( it . ( OSClass (R,x) ) ) = ( ( F . s ) . x ));
existence
proof
set qa = ( QuotOSAlg (U1,R) );
set cqa = (the Sorts of qa);
set S1 = (the Sorts of U1);
set S2 = (the Sorts of U2);
defpred P[ set,set ]
 means
(for a being (Element of ( S1 . s )) holds ($1 = ( OSClass (R,a) ) implies $2 = ( ( F . s ) . a )));
A4: ( cqa . s ) = ( OSClass (R,s) ) by CKB36:def 1;
A5: (for x being set holds (x in ( cqa . s ) implies (ex y being set st (y in ( S2 . s ) & P[ x,y ]))))
proof
let x being set;
A6: ( R . s ) c= ( ( OSCng F ) . s ) by A3,PBOOLE:def 2;
assume x in ( cqa . s );
then consider a being set such that A7: a in ( (the Sorts of U1) . s ) and A8: x = ( Class (( CompClass (R,( CComp s )) ),a) ) by A4,CKB33:def 1;
reconsider a as (Element of ( S1 . s )) by A7;
take y = ( ( F . s ) . a );
thus y in ( S2 . s );
let b being (Element of ( S1 . s ));
assume A9: x = ( OSClass (R,b) );
x = ( OSClass (R,a) ) by A8;
then [ b,a ] in ( R . s ) by A9,CKB40:1;
then [ b,a ] in ( ( OSCng F ) . s ) by A6;
then [ b,a ] in ( ( MSCng F ) . s ) by A1,A2,CKB56:def 1;
then [ b,a ] in ( MSCng (F,s) ) by A1,MSUALG_4:def 18;
hence thesis by MSUALG_4:def 17;
end;
consider G being Function such that A10: ((( dom G ) = ( cqa . s ) & ( rng G ) c= ( S2 . s )) & (for x being set holds (x in ( cqa . s ) implies P[ x,( G . x ) ]))) from FUNCT_1:sch 5(A5);
reconsider G as (Function of ( cqa . s ),( S2 . s )) by A10,FUNCT_2:def 1,RELSET_1:4;
take G;
let a being (Element of ( S1 . s ));
thus thesis by A4,A10;
end;
uniqueness
proof
set qa = ( QuotOSAlg (U1,R) );
set cqa = (the Sorts of qa);
set u1 = (the Sorts of U1);
set u2 = (the Sorts of U2);
let H being (Function of ( cqa . s ),( u2 . s )),G being (Function of ( cqa . s ),( u2 . s ));
assume that
A11: (for a being (Element of ( u1 . s )) holds ( H . ( OSClass (R,a) ) ) = ( ( F . s ) . a ))
and
A12: (for a being (Element of ( u1 . s )) holds ( G . ( OSClass (R,a) ) ) = ( ( F . s ) . a ));
A13: ( cqa . s ) = ( OSClass (R,s) ) by CKB36:def 1;
(for x being set holds (x in ( cqa . s ) implies ( H . x ) = ( G . x )))
proof
let x being set;
assume x in ( cqa . s );
then consider y being set such that A14: y in ( (the Sorts of U1) . s ) and A15: x = ( Class (( CompClass (R,( CComp s )) ),y) ) by A13,CKB33:def 1;
reconsider y1 = y as (Element of ( u1 . s )) by A14;
A16: ( OSClass (R,y1) ) = x by A15;
hence ( H . x ) = ( ( F . s ) . y1 ) by A11
.= ( G . x ) by A12,A16;
end;
hence thesis by FUNCT_2:12;
end;
end;
