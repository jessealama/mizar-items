theorem
(for U1,U2 being  non-empty (OSAlgebra of S) holds (for F being (ManySortedFunction of U1,U2) holds ((F is_epimorphism U1,U2 & F is  order-sorted) implies ( QuotOSAlg (U1,( OSCng F )) ),U2 are_isomorphic )))
proof
let U1 being  non-empty (OSAlgebra of S);
let U2 being  non-empty (OSAlgebra of S);
let F being (ManySortedFunction of U1,U2);
assume (F is_epimorphism U1,U2 & F is  order-sorted);
then ( OSHomQuot F ) is_isomorphism ( QuotOSAlg (U1,( OSCng F )) ),U2 by CKB60:1;
hence thesis by MSUALG_3:def 11;
end;
