constructors PARTFUN1,XXREAL_0,REAL_1,NAT_1,SEQ_4,REALSET1,RFINSEQ,NAT_D,FINSEQ_5,TOPREAL1,BINOP_2,RELSET_1,FINSEQ_6,CKB7;
requirements REAL,NUMERALS,BOOLE,SUBSET,ARITHM;
begin
theorem
Th46: (for f being (FinSequence of ( TOP-REAL 2 )) holds (for p being (Point of ( TOP-REAL 2 )) holds (for i1 being (Element of ( NAT )) holds ((f is  s.n.c. & p in ( LSeg (f,i1) )) implies (i1 = ( Index (p,f) ) or i1 = ( ( Index (p,f) ) + 1 ))))))
proof
let f being (FinSequence of ( TOP-REAL 2 ));
let p being (Point of ( TOP-REAL 2 ));
let i1 being (Element of ( NAT ));
assume that
A1: f is  s.n.c.
and
A2: p in ( LSeg (f,i1) );
p in ( L~ f ) by A2,SPPOL_2:17;
then p in ( LSeg (f,( Index (p,f) )) ) by CKB10:1;
then p in ( ( LSeg (f,( Index (p,f) )) ) /\ ( LSeg (f,i1) ) ) by A2,XBOOLE_0:def 4;
then A3: ( LSeg (f,( Index (p,f) )) ) meets ( LSeg (f,i1) ) by XBOOLE_0:4;
assume A4: (not thesis);
( Index (p,f) ) <= i1 by A2,CKB8:1;
then ( Index (p,f) ) < i1 by A4,XXREAL_0:1;
then ( ( Index (p,f) ) + 1 ) <= i1 by NAT_1:13;
then ( ( Index (p,f) ) + 1 ) < i1 by A4,XXREAL_0:1;
hence contradiction by A1,A3,TOPREAL1:def 7;
end;
