begin
theorem
Th59: (for f being (FinSequence of ( TOP-REAL 2 )) holds (for p being (Point of ( TOP-REAL 2 )) holds (p in ( L~ f ) implies (( ( R_Cut (f,p) ) . ( len ( R_Cut (f,p) ) ) ) = p & (for i being (Element of ( NAT )) holds ((1 <= i & i <= ( Index (p,f) )) implies ( ( R_Cut (f,p) ) . i ) = ( f . i )))))))
proof
let f being (FinSequence of ( TOP-REAL 2 ));
let p being (Point of ( TOP-REAL 2 ));
assume A1: p in ( L~ f );
then A2: ( Index (p,f) ) < ( len f ) by CKB9:1;
now
per cases ;
suppose A3: p <> ( f . 1 );

A4: ( len ( ( mid (f,1,( Index (p,f) )) ) ^ <* p *> ) ) = ( ( len ( mid (f,1,( Index (p,f) )) ) ) + ( len <* p *> ) ) by FINSEQ_1:22
.= ( ( len ( mid (f,1,( Index (p,f) )) ) ) + 1 ) by FINSEQ_1:39;
( R_Cut (f,p) ) = ( ( mid (f,1,( Index (p,f) )) ) ^ <* p *> ) by A3,CKB23:def 1;
hence ( ( R_Cut (f,p) ) . ( len ( R_Cut (f,p) ) ) ) = p by A4,FINSEQ_1:42;
end;
suppose p = ( f . 1 );

then A5: ( R_Cut (f,p) ) = <* p *> by CKB23:def 1;
then ( len ( R_Cut (f,p) ) ) = 1 by FINSEQ_1:40;
hence ( ( R_Cut (f,p) ) . ( len ( R_Cut (f,p) ) ) ) = p by A5,FINSEQ_1:40;
end;
end;
hence ( ( R_Cut (f,p) ) . ( len ( R_Cut (f,p) ) ) ) = p;
A6: 1 <= ( Index (p,f) ) by A1,CKB9:1;
then ( len f ) > 1 by A2,XXREAL_0:2;
then A7: ( len ( mid (f,1,( Index (p,f) )) ) ) = ( ( ( Index (p,f) ) -' 1 ) + 1 ) by A6,A2,FINSEQ_6:118
.= ( Index (p,f) ) by A1,CKB9:1,XREAL_1:235;
thus (for i being (Element of ( NAT )) holds ((1 <= i & i <= ( Index (p,f) )) implies ( ( R_Cut (f,p) ) . i ) = ( f . i )))
proof
let i being (Element of ( NAT ));
assume that
A8: 1 <= i
and
A9: i <= ( Index (p,f) );
now
per cases ;
case p <> ( f . 1 );
then ( ( R_Cut (f,p) ) . i ) = ( ( ( mid (f,1,( Index (p,f) )) ) ^ <* p *> ) . i ) by CKB23:def 1
.= ( ( mid (f,1,( Index (p,f) )) ) . i ) by A7,A8,A9,FINSEQ_1:64
.= ( f . i ) by A2,A8,A9,FINSEQ_6:123;
hence thesis;
end;
case A10: p = ( f . 1 );
A11: ( len f ) > 1 by A6,A2,XXREAL_0:2;
then 1 in ( dom f ) by FINSEQ_3:25;
then A12: p = ( f /. 1 ) by A10,PARTFUN1:def 6;
( len f ) >= ( 1 + 1 ) by A11,NAT_1:13;
then ( Index (p,f) ) = 1 by A12,CKB12:1;
then A13: i = 1 by A8,A9,XXREAL_0:1;
( R_Cut (f,p) ) = <* p *> by A10,CKB23:def 1;
hence thesis by A10,A13,FINSEQ_1:40;
end;
end;
hence thesis;
end;

