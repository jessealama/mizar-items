begin
reserve i1 for (Element of ( NAT ));
reserve i2 for (Element of ( NAT ));
reserve D for non empty set;
theorem
Lm6: (for f1 being (FinSequence of D) holds (for i1 holds (for i2 holds ( mid (f1,i1,i2) ) = ( Rev ( mid (f1,i2,i1) ) ))))
proof
let f1 being (FinSequence of D);
let k1 being (Element of ( NAT )),k2 being (Element of ( NAT ));
now
per cases ;
case A1: k1 <= k2;
then A2: ( mid (f1,k1,k2) ) = ( ( f1 /^ ( k1 -' 1 ) ) | ( ( k2 -' k1 ) + 1 ) ) by FINSEQ_6:def 3;
now
per cases  by A1,XXREAL_0:1;
case k1 < k2;
then ( mid (f1,k2,k1) ) = ( Rev ( mid (f1,k1,k2) ) ) by A2,FINSEQ_6:def 3;
hence thesis by FINSEQ_6:25;
end;
case A3: k1 = k2;
A4: (k1 = ( 0 ) or ( ( 0 ) + 1 ) <= k1) by NAT_1:13;
now
per cases  by A4;
case k1 = ( 0 );
then A5: ( mid (f1,k1,k2) ) = ( f1 | 1 ) by A3,CKB52:1;
now
per cases ;
case ( len f1 ) = ( 0 );
then f1 = ( <*> D );
then ( f1 | 1 ) = ( <*> D ) by CKB50:1;
hence thesis by A3,A5;
end;
case ( len f1 ) <> ( 0 );
then f1 <> ( <*> D );
then ( f1 | 1 ) = <* ( f1 /. 1 ) *> by FINSEQ_5:20;
hence thesis by A3,A5,FINSEQ_5:60;
end;
end;
hence thesis;
end;
case (1 <= k1 & k1 <= ( len f1 ));
then ( mid (f1,k1,k1) ) = <* ( f1 /. k1 ) *> by CKB51:1;
hence thesis by A3,FINSEQ_5:60;
end;
case ( len f1 ) < k1;
then ( mid (f1,k1,k1) ) = ( <*> D ) by CKB53:1;
hence thesis by A3;
end;
end;
hence thesis;
end;
end;
hence thesis;
end;
case A6: k1 > k2;
then ( mid (f1,k1,k2) ) = ( Rev ( ( f1 /^ ( k2 -' 1 ) ) | ( ( k1 -' k2 ) + 1 ) ) ) by FINSEQ_6:def 3;
hence thesis by A6,FINSEQ_6:def 3;
end;
end;
hence thesis;
end;
