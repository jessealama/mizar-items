begin
reserve x for set;
reserve y for set;
definition
let R being Relation;
func R [*] -> Relation means 
(for x,y being set holds ([ x,y ] in it iff ((x in ( field R ) & y in ( field R )) & (ex p being FinSequence st (((( len p ) >= 1 & ( p . 1 ) = x) & ( p . ( len p ) ) = y) & (for i being Nat holds ((i >= 1 & i < ( len p )) implies [ ( p . i ),( p . ( i + 1 ) ) ] in R)))))));
existence
proof
defpred X[ set,set ]
 means
(ex p being FinSequence st (((( len p ) >= 1 & ( p . 1 ) = $1) & ( p . ( len p ) ) = $2) & (for i being Nat holds ((i >= 1 & i < ( len p )) implies [ ( p . i ),( p . ( i + 1 ) ) ] in R))));
thus (ex S being Relation st (for x,y being set holds ([ x,y ] in S iff ((x in ( field R ) & y in ( field R )) & X[ x,y ])))) from RELAT_1:sch 1;
end;
uniqueness
proof
let R1 being Relation;
let R2 being Relation;
assume that
A1: (for x,y being set holds ([ x,y ] in R1 iff ((x in ( field R ) & y in ( field R )) & (ex p being FinSequence st (((( len p ) >= 1 & ( p . 1 ) = x) & ( p . ( len p ) ) = y) & (for i being Nat holds ((i >= 1 & i < ( len p )) implies [ ( p . i ),( p . ( i + 1 ) ) ] in R)))))))
and
A2: (for x,y being set holds ([ x,y ] in R2 iff ((x in ( field R ) & y in ( field R )) & (ex p being FinSequence st (((( len p ) >= 1 & ( p . 1 ) = x) & ( p . ( len p ) ) = y) & (for i being Nat holds ((i >= 1 & i < ( len p )) implies [ ( p . i ),( p . ( i + 1 ) ) ] in R)))))));
let x being set;
let y being set;
thus ([ x,y ] in R1 implies [ x,y ] in R2)
proof
assume A3: [ x,y ] in R1;
then A4: (x in ( field R ) & y in ( field R )) by A1;
(ex p being FinSequence st (((( len p ) >= 1 & ( p . 1 ) = x) & ( p . ( len p ) ) = y) & (for i being Nat holds ((i >= 1 & i < ( len p )) implies [ ( p . i ),( p . ( i + 1 ) ) ] in R)))) by A1,A3;
hence thesis by A2,A4;
end;

assume A5: [ x,y ] in R2;
then A6: (x in ( field R ) & y in ( field R )) by A2;
(ex p being FinSequence st (((( len p ) >= 1 & ( p . 1 ) = x) & ( p . ( len p ) ) = y) & (for i being Nat holds ((i >= 1 & i < ( len p )) implies [ ( p . i ),( p . ( i + 1 ) ) ] in R)))) by A2,A5;
hence thesis by A1,A6;
end;
end;
