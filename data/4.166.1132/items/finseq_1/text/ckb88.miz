begin
reserve k for Nat;
reserve n for Nat;
reserve x for set;
reserve X for set;
reserve p for FinSequence;
reserve q for FinSequence;
reserve r for FinSequence;
defpred P[ set,set ]
 means
(ex k st ($1 = k & $2 = ( k + 1 )));
scheme IndSeq { P[FinSequence] } : (for p holds P[ p ])
provided
A1: P[ ( {} ) ]
and
A2: (for p holds (for x holds (P[ p ] implies P[ ( p ^ <* x *> ) ])))
proof
let p;
defpred R[ set ]
 means
(for p holds (( len p ) = $1 implies P[ p ]));
consider X being (Subset of ( REAL )) such that A3: (for x being Real holds (x in X iff R[ x ])) from SUBSET_1:sch 3;
(for k holds k in X)
proof
defpred S[ Nat ]
 means
$1 in X;
(for q holds (( len q ) = ( 0 ) implies P[ q ])) by A1,CKB43:1;
then A4: S[ ( 0 ) ] by A3;
now
let n;
assume that
A5: n in X;
now
let q;
assume A6: ( len q ) = ( n + 1 );
then q <> ( {} );
then consider r,x such that A7: q = ( r ^ <* x *> ) by CKB86:1;
( len q ) = ( ( len r ) + ( len <* x *> ) ) by A7,CKB51:1
.= ( ( len r ) + 1 ) by CKB79:1;
then P[ r ] by A3,A5,A6;
hence P[ q ] by A2,A7;
end;
hence ( n + 1 ) in X by A3;
end;
then A8: (for n holds (S[ n ] implies S[ ( n + 1 ) ]));
thus (for n holds S[ n ]) from NAT_1:sch 2(A4,A8);
end;
then ( len p ) in X;
hence thesis by A3;
end;
