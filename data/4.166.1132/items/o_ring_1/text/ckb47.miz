A25: (( f /. n ) = ( ( f /. k ) * ( f /. l ) ) or ( f /. n ) = ( ( f /. k ) + ( f /. l ) ))
and
A26: (((k <> ( 0 ) & k < n) & l <> ( 0 )) & l < n);

(( ( f ^ g ) /. k ) = ( f /. k ) & ( ( f ^ g ) /. l ) = ( f /. l )) by A22,A26,CKB4:1,XXREAL_0:2;
