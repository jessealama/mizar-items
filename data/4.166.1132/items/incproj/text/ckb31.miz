begin
registration
let CPS being CollProjectiveSpace;
cluster ( IncProjSp_of CPS ) ->  partial  linear  up-2-dimensional  up-3-rank  Vebleian;
coherence
proof
set PS = ( IncProjSp_of CPS );
A1: (for a,b,c,d,p,q being (POINT of PS) holds (for M,N,P,Q being (LINE of PS) holds (((((((((((((a on M & b on M) & c on N) & d on N) & p on M) & p on N) & a on P) & c on P) & b on Q) & d on Q) & (not p on P)) & (not p on Q)) & M <> N) implies (ex q being (POINT of PS) st (q on P & q on Q))))) by CKB19:1;
A2: ((ex p being (POINT of PS) st (ex P being (LINE of PS) st (not p on P))) & (for P being (LINE of PS) holds (ex a,b,c being (POINT of PS) st (((((a <> b & b <> c) & c <> a) & a on P) & b on P) & c on P)))) by CKB17:1,CKB18:1;
((for p,q being (POINT of PS) holds (for P,Q being (LINE of PS) holds ((((p on P & q on P) & p on Q) & q on Q) implies (p = q or P = Q)))) & (for p,q being (POINT of PS) holds (ex P being (LINE of PS) st (p on P & q on P)))) by CKB14:1,CKB15:1;
hence thesis by A2,A1,CKB26:def 1,CKB27:def 1,CKB28:def 1,CKB29:def 1,CKB30:def 1;
end;
end;
