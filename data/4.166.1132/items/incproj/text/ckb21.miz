environ
vocabularies SUBSET_1,COLLSP,PRE_TOPC,INCSP_1,TARSKI,STRUCT_0,PARSP_2,XBOOLE_0,RELAT_1,ANPROJ_2,ZFMISC_1,FDIFF_1,ANALOAF,VECTSP_1,AFF_2,INCPROJ;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,DOMAIN_1,RELSET_1,STRUCT_0,PRE_TOPC,INCSP_1,COLLSP,ANPROJ_2,CKB1,CKB2,CKB5,CKB6;
definitions INCSP_1,CKB2,CKB5,CKB6;
theorems COLLSP,ANPROJ_2,TARSKI,RELAT_1,INCSP_1,ZFMISC_1,CKB2,CKB4,CKB5,CKB6,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20;
schemes RELSET_1;
registrations RELSET_1,STRUCT_0,ANPROJ_2,CKB3,CKB7;
constructors DOMAIN_1,INCSP_1,ANPROJ_2,CKB1,CKB2,CKB5,CKB6;
requirements SUBSET,BOOLE;
begin
reserve CPS for CollProjectiveSpace;
reserve p for (POINT of ( IncProjSp_of CPS ));
reserve q for (POINT of ( IncProjSp_of CPS ));
reserve M for (LINE of ( IncProjSp_of CPS ));
reserve N for (LINE of ( IncProjSp_of CPS ));
theorem
Th19: ((ex p,p1,r,r1 being (Point of CPS) st (not (ex s being (Point of CPS) st (p,p1,s is_collinear  & r,r1,s is_collinear )))) implies (ex M st (ex N st (not (ex q st (q on M & q on N))))))
proof
given p being (Point of CPS),p1 being (Point of CPS),r being (Point of CPS),r1 being (Point of CPS) such that
A1: (not (ex s being (Point of CPS) st (p,p1,s is_collinear  & r,r1,s is_collinear )));

set M99 = ( Line (p,p1) );
set N99 = ( Line (r,r1) );
(p <> p1 & r <> r1)
proof
A2:now
assume p = p1;
then A3: p,p1,r1 is_collinear  by COLLSP:2;
r,r1,r1 is_collinear  by COLLSP:2;
hence contradiction by A1,A3;
end;
A4:now
assume r = r1;
then (p,p1,p1 is_collinear  & r,r1,p1 is_collinear ) by COLLSP:2;
hence contradiction by A1;
end;
assume (not thesis);
hence contradiction by A2,A4;
end;
then reconsider M9 = M99,N9 = N99 as (LINE of CPS) by COLLSP:def 7;
reconsider M = M9,N = N9 as (LINE of ( IncProjSp_of CPS )) by CKB4:1;
take M;
take N;
thus (not (ex q st (q on M & q on N)))
proof
assume (not thesis);
then consider q such that A5: q on M and A6: q on N;
reconsider q9 = q as (Point of CPS);
q9 in N99 by A6,CKB11:1;
then A7: r,r1,q9 is_collinear  by COLLSP:11;
q9 in M99 by A5,CKB11:1;
then p,p1,q9 is_collinear  by COLLSP:11;
hence contradiction by A1,A7;
end;

end;
