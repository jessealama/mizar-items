environ
vocabularies XBOOLE_0,ANALOAF,SUBSET_1,DIRAF,PARSP_2,FDIFF_1,SEMI_AF1;
notations STRUCT_0,ANALOAF,DIRAF,CKB1,CKB3,CKB25;
definitions CKB1,CKB25;
theorems PARDEPAP,DIRAF,CKB1,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30;
registrations ANALOAF,STRUCT_0,CKB2;
constructors DIRAF,CKB1,CKB25;
begin
reserve SAS for Semi_Affine_Space;
reserve a for (Element of SAS);
reserve b for (Element of SAS);
reserve c for (Element of SAS);
reserve d for (Element of SAS);
theorem
Th43: (((not a,b,c is_collinear ) & a,b // c,d) implies (not a,b,d is_collinear ))
proof
assume that
A1: (not a,b,c is_collinear )
and
A2: a,b // c,d;
now
assume that
A3: c <> d
and
A4: a,b,d is_collinear ;
a,b // a,d by A4,CKB25:def 1;
then A5: a,b // d,a by CKB9:1;
A6: a,b // d,c by A2,CKB9:1;
A7: a,c // c,a by CKB1:def 1;
A8: (not a,b // a,c) by A1,CKB25:def 1;
then a <> b by CKB6:1;
then A9: d,c // d,a by A6,A5,CKB1:def 1;
c <> a by A8,CKB1:def 1;
then (not c,d // c,a) by A2,A3,A8,A7,CKB18:1;
hence contradiction by A9,CKB10:1;
end;
hence thesis by A1;
end;
