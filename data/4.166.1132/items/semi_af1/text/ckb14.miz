environ
vocabularies XBOOLE_0,ANALOAF,SUBSET_1,DIRAF,PARSP_2,FDIFF_1,SEMI_AF1;
notations STRUCT_0,ANALOAF,DIRAF,CKB1,CKB3;
definitions CKB1;
theorems PARDEPAP,DIRAF,CKB1,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13;
registrations ANALOAF,STRUCT_0,CKB2;
constructors DIRAF,CKB1;
begin
reserve SAS for Semi_Affine_Space;
reserve a for (Element of SAS);
reserve b for (Element of SAS);
reserve c for (Element of SAS);
reserve x for (Element of SAS);
theorem
(((a,b // a,x & b,c // b,x) & c,a // c,x) implies a,b // a,c)
proof
assume that
A1: a,b // a,x
and
A2: b,c // b,x
and
A3: c,a // c,x;
now
assume A4: a <> x;
(a,x // a,b & a,x // a,c) by A1,A3,CKB10:1;
hence thesis by A4,CKB1:def 1;
end;
hence thesis by A2,CKB10:1;
end;
