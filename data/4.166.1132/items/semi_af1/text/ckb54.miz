environ
vocabularies XBOOLE_0,ANALOAF,SUBSET_1,DIRAF,PARSP_2,FDIFF_1,SEMI_AF1;
notations STRUCT_0,ANALOAF,DIRAF,CKB1,CKB3,CKB25,CKB40;
definitions CKB1,CKB25,CKB40;
theorems PARDEPAP,DIRAF,CKB1,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB37,CKB38,CKB39,CKB40,CKB41,CKB42,CKB43,CKB44,CKB45,CKB46,CKB47,CKB48,CKB49,CKB50,CKB51,CKB52,CKB53;
registrations ANALOAF,STRUCT_0,CKB2;
constructors DIRAF,CKB1,CKB25,CKB40;
begin
reserve SAS for Semi_Affine_Space;
reserve a for (Element of SAS);
reserve a9 for (Element of SAS);
reserve b for (Element of SAS);
reserve b9 for (Element of SAS);
reserve c for (Element of SAS);
reserve c9 for (Element of SAS);
theorem
Th67: (( parallelogram a,a9,b,b9 &  parallelogram a,a9,c,c9) implies b,c // b9,c9)
proof
assume that
A1:  parallelogram a,a9,b,b9
and
A2:  parallelogram a,a9,c,c9;
A3: (a,a9 // c,c9 & a,c // a9,c9) by A2,CKB40:def 1;
(not a,a9,c is_collinear ) by A2,CKB40:def 1;
then A4: (not a,a9 // a,c) by CKB25:def 1;
(not a,a9,b is_collinear ) by A1,CKB40:def 1;
then A5: (not a,a9 // a,b) by CKB25:def 1;
(a,a9 // b,b9 & a,b // a9,b9) by A1,CKB40:def 1;
hence thesis by A5,A4,A3,CKB1:def 1;
end;
