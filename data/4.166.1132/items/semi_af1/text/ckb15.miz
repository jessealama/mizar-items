environ
vocabularies XBOOLE_0,ANALOAF,SUBSET_1,DIRAF,PARSP_2,FDIFF_1,SEMI_AF1;
notations STRUCT_0,ANALOAF,DIRAF,CKB1,CKB3;
definitions CKB1;
theorems PARDEPAP,DIRAF,CKB1,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14;
registrations ANALOAF,STRUCT_0,CKB2;
constructors DIRAF,CKB1;
begin
reserve SAS for Semi_Affine_Space;
reserve a for (Element of SAS);
reserve b for (Element of SAS);
reserve c for (Element of SAS);
reserve p for (Element of SAS);
reserve q for (Element of SAS);
theorem
Th25: (((not a,b // a,c) & p <> q) implies (((not p,q // p,a) or (not p,q // p,b)) or (not p,q // p,c)))
proof
assume that
A1: (not a,b // a,c)
and
A2: p <> q;
now
assume that
A3: a <> p
and
A4: p,q // p,a
and
A5: p,q // p,b
and
A6: p,q // p,c;
p,a // p,c by A2,A4,A6,CKB1:def 1;
then A7: a,p // a,c by CKB1:def 1;
p,a // p,b by A2,A4,A5,CKB1:def 1;
then a,p // a,b by CKB1:def 1;
hence contradiction by A1,A3,A7,CKB1:def 1;
end;
hence thesis by A1,A2,CKB1:def 1;
end;
