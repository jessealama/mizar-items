environ
vocabularies XBOOLE_0,ANALOAF,SUBSET_1,DIRAF,PARSP_2,FDIFF_1,SEMI_AF1;
notations STRUCT_0,ANALOAF,DIRAF,CKB1,CKB3;
definitions CKB1;
theorems PARDEPAP,DIRAF,CKB1,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20;
registrations ANALOAF,STRUCT_0,CKB2;
constructors DIRAF,CKB1;
begin
reserve SAS for Semi_Affine_Space;
reserve p for (Element of SAS);
reserve q for (Element of SAS);
reserve r1 for (Element of SAS);
reserve r2 for (Element of SAS);
theorem
Th32: ((((not p,q // p,r1) & p,r1 // p,r2) & q,r1 // q,r2) implies r1 = r2)
proof
assume that
A1: ((not p,q // p,r1) & p,r1 // p,r2)
and
A2: q,r1 // q,r2;
A3: r1,r2 // r1,q by A2,CKB17:1;
((not r1,p // r1,q) & r1,r2 // r1,p) by A1,CKB17:1;
hence thesis by A3,CKB1:def 1;
end;
