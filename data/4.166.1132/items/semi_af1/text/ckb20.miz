environ
vocabularies XBOOLE_0,ANALOAF,SUBSET_1,DIRAF,PARSP_2,FDIFF_1,SEMI_AF1;
notations STRUCT_0,ANALOAF,DIRAF,CKB1,CKB3;
definitions CKB1;
theorems PARDEPAP,DIRAF,CKB1,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19;
registrations ANALOAF,STRUCT_0,CKB2;
constructors DIRAF,CKB1;
begin
reserve SAS for Semi_Affine_Space;
reserve a for (Element of SAS);
reserve b for (Element of SAS);
reserve c for (Element of SAS);
reserve p for (Element of SAS);
reserve r for (Element of SAS);
theorem
Th31: ((((not a,b // a,c) & a,c // p,r) & b,c // p,r) implies p = r)
proof
assume that
A1: ((not a,b // a,c) & a,c // p,r)
and
A2: b,c // p,r;
A3: p,r // b,c by A2,CKB9:1;
((not a,c // b,c) & p,r // a,c) by A1,CKB9:1,CKB17:1;
hence thesis by A3,CKB1:def 1;
end;
