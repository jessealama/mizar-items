environ
vocabularies XBOOLE_0,ANALOAF,SUBSET_1,DIRAF,PARSP_2,FDIFF_1,SEMI_AF1;
notations STRUCT_0,ANALOAF,DIRAF,CKB1,CKB3,CKB25,CKB40;
definitions CKB1,CKB25,CKB40;
theorems PARDEPAP,DIRAF,CKB1,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB37,CKB38,CKB39,CKB40;
registrations ANALOAF,STRUCT_0,CKB2;
constructors DIRAF,CKB1,CKB25,CKB40;
begin
reserve SAS for Semi_Affine_Space;
reserve a for (Element of SAS);
reserve b for (Element of SAS);
reserve c for (Element of SAS);
reserve d for (Element of SAS);
theorem
Th54: ( parallelogram a,b,c,d implies (((((a <> b & a <> c) & c <> b) & a <> d) & b <> d) & c <> d))
proof
assume A1:  parallelogram a,b,c,d;
then (not a,b,c is_collinear ) by CKB40:def 1;
hence ((a <> b & a <> c) & c <> b) by CKB28:1;
A2:now
assume a = d;
then a,b // c,a by A1,CKB40:def 1;
then A3: a,b // a,c by CKB9:1;
(not a,b,c is_collinear ) by A1,CKB40:def 1;
hence contradiction by A3,CKB25:def 1;
end;
A4:now
assume c = d;
then a,c // b,c by A1,CKB40:def 1;
then c,a // c,b by CKB9:1;
then A5: c,a,b is_collinear  by CKB25:def 1;
(not a,b,c is_collinear ) by A1,CKB40:def 1;
hence contradiction by A5,CKB26:1;
end;
A6:now
assume b = d;
then a,b // c,b by A1,CKB40:def 1;
then b,a // b,c by CKB9:1;
then A7: b,a,c is_collinear  by CKB25:def 1;
(not a,b,c is_collinear ) by A1,CKB40:def 1;
hence contradiction by A7,CKB26:1;
end;
assume (not thesis);
hence contradiction by A2,A6,A4;
end;
