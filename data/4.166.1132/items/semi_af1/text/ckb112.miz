environ
vocabularies XBOOLE_0,ANALOAF,SUBSET_1,DIRAF,PARSP_2,FDIFF_1,SEMI_AF1;
notations STRUCT_0,ANALOAF,DIRAF,CKB1,CKB3,CKB25,CKB40,CKB60,CKB82,CKB83,CKB84,CKB100,CKB101;
definitions CKB1,CKB25,CKB40,CKB60,CKB82,CKB83,CKB84,CKB100,CKB101;
theorems PARDEPAP,DIRAF,CKB1,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB37,CKB38,CKB39,CKB40,CKB41,CKB42,CKB43,CKB44,CKB45,CKB46,CKB47,CKB48,CKB49,CKB50,CKB51,CKB52,CKB53,CKB54,CKB55,CKB56,CKB57,CKB58,CKB59,CKB60,CKB61,CKB62,CKB63,CKB64,CKB65,CKB66,CKB67,CKB68,CKB69,CKB70,CKB71,CKB72,CKB73,CKB74,CKB75,CKB76,CKB77,CKB78,CKB79,CKB80,CKB81,CKB82,CKB83,CKB84,CKB85,CKB86,CKB87,CKB88,CKB89,CKB90,CKB91,CKB92,CKB93,CKB94,CKB95,CKB96,CKB97,CKB98,CKB99,CKB100,CKB101,CKB102,CKB103,CKB104,CKB105,CKB106,CKB107,CKB108,CKB109,CKB110,CKB111;
registrations ANALOAF,STRUCT_0,CKB2;
constructors DIRAF,CKB1,CKB25,CKB40,CKB60,CKB82,CKB83,CKB84,CKB100,CKB101;
begin
reserve SAS for Semi_Affine_Space;
reserve a for (Element of SAS);
reserve b for (Element of SAS);
reserve c for (Element of SAS);
reserve d for (Element of SAS);
reserve o for (Element of SAS);
reserve p for (Element of SAS);
reserve q for (Element of SAS);
reserve r for (Element of SAS);
reserve s for (Element of SAS);
reserve x for (Element of SAS);
reserve y for (Element of SAS);
theorem
((( trap a,p,b,q,o &  trap a,p,c,r,o) &  trap b,q,d,s,o) implies c,d // r,s)
proof
assume that
A1:  trap a,p,b,q,o
and
A2:  trap a,p,c,r,o
and
A3:  trap b,q,d,s,o;
A4:now
assume A5: (not o,a,d is_collinear );
 trap b,q,a,p,o by A1,CKB105:1;
then  trap a,p,d,s,o by A3,A5,CKB111:1;
hence thesis by A2,CKB110:1;
end;
A6:now
(not o,a,b is_collinear ) by A1,CKB100:def 1;
then (not b,a,o is_collinear ) by CKB26:1;
then consider x such that A7:  parallelogram b,a,o,x by CKB49:1;
o,b,q is_collinear  by A1,CKB100:def 1;
then o,b // o,q by CKB25:def 1;
then A8: b,o // q,o by CKB9:1;
A9: o,x // o,x by CKB4:1;
(b,o // a,x & b <> o) by A7,CKB40:def 1,CKB41:1;
then A10: q,o // a,x by A8,CKB1:def 1;
A11: (not o,x,b is_collinear ) by A7,CKB43:1;
A12: o <> d by A3,CKB102:1;
assume that
A13: o <> p
and
A14: o,b,c is_collinear 
and
A15: o,a,d is_collinear ;
(not o,p,q is_collinear ) by A1,A13,CKB107:1;
then (not q,p,o is_collinear ) by CKB26:1;
then consider y such that A16:  parallelogram q,p,o,y by CKB49:1;
A17: (not o,x,a is_collinear ) by A7,CKB43:1;
A18: o <> c by A2,CKB102:1;
a,b // p,q by A1,CKB100:def 1;
then A19: b,a // q,p by CKB9:1;
A20: o,a,p is_collinear  by A1,CKB100:def 1;
(b,a // o,x & b <> a) by A7,CKB40:def 1,CKB41:1;
then A21: q,p // o,x by A19,CKB1:def 1;
A22: o <> x by A7,CKB41:1;
(q <> p & q,p // o,y) by A16,CKB40:def 1,CKB41:1;
then o,x // o,y by A21,CKB1:def 1;
then A23: o,x,y is_collinear  by CKB25:def 1;
(q,o // p,y & q <> o) by A16,CKB40:def 1,CKB41:1;
then A24: a,x // p,y by A10,CKB1:def 1;
(not o,a,x is_collinear ) by A7,CKB43:1;
then A25:  trap a,p,x,y,o by A23,A24,A20,CKB100:def 1;
(not o,b,x is_collinear ) by A7,CKB43:1;
then A26:  trap b,q,x,y,o by A1,A25,CKB111:1;
o,a // o,d by A15,CKB25:def 1;
then A27:  trap x,y,d,s,o by A3,A26,A22,A12,A17,A9,CKB27:1,CKB111:1;
o,b // o,c by A14,CKB25:def 1;
then  trap x,y,c,r,o by A2,A25,A11,A22,A18,A9,CKB27:1,CKB111:1;
hence thesis by A27,CKB110:1;
end;
A28:now
assume A29: o = p;
then o = q by A1,CKB105:1,CKB106:1;
then A30: o = s by A3,CKB105:1,CKB106:1;
o = r by A2,A29,CKB105:1,CKB106:1;
hence thesis by A30,CKB1:def 1;
end;
now
assume (not o,b,c is_collinear );
then  trap b,q,c,r,o by A1,A2,CKB111:1;
hence thesis by A3,CKB110:1;
end;
hence thesis by A4,A28,A6;
end;
