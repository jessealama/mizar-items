environ
vocabularies XBOOLE_0,ANALOAF,SUBSET_1,DIRAF,PARSP_2,FDIFF_1,SEMI_AF1;
notations STRUCT_0,ANALOAF,DIRAF,CKB1,CKB3,CKB25,CKB40;
definitions CKB1,CKB25,CKB40;
theorems PARDEPAP,DIRAF,CKB1,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB37,CKB38,CKB39,CKB40,CKB41,CKB42,CKB43,CKB44,CKB45,CKB46,CKB47,CKB48,CKB49,CKB50,CKB51,CKB52;
registrations ANALOAF,STRUCT_0,CKB2;
constructors DIRAF,CKB1,CKB25,CKB40;
begin
reserve SAS for Semi_Affine_Space;
reserve a for (Element of SAS);
reserve b for (Element of SAS);
reserve c for (Element of SAS);
reserve p for (Element of SAS);
reserve q for (Element of SAS);
theorem
Th66: (a <> b implies (ex c st ((a,b,c is_collinear  & c <> a) & c <> b)))
proof
assume a <> b;
then consider p such that A1: (not a,b,p is_collinear ) by CKB29:1;
consider q such that A2:  parallelogram a,b,p,q by A1,CKB49:1;
(not p,q,b is_collinear ) by A2,CKB43:1;
then consider c such that A3:  parallelogram p,q,b,c by CKB49:1;
take c;
A4: p,q // b,c by A3,CKB40:def 1;
(p <> q & a,b // p,q) by A2,CKB40:def 1,CKB41:1;
then a,b // b,c by A4,CKB11:1;
then b,a // b,c by CKB9:1;
then A5: b,a,c is_collinear  by CKB25:def 1;
A6: (not a,q // b,p) by A2,CKB51:1;
(p,b // q,c & (not b,c,p is_collinear )) by A3,CKB40:def 1,CKB42:1;
hence thesis by A6,A5,CKB9:1,CKB26:1,CKB28:1;
end;
