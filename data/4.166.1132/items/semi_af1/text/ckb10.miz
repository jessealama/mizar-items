environ
vocabularies XBOOLE_0,ANALOAF,SUBSET_1,DIRAF,PARSP_2,FDIFF_1,SEMI_AF1;
notations STRUCT_0,ANALOAF,DIRAF,CKB1,CKB3;
definitions CKB1;
theorems PARDEPAP,DIRAF,CKB1,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9;
registrations ANALOAF,STRUCT_0,CKB2;
constructors DIRAF,CKB1;
begin
reserve SAS for Semi_Affine_Space;
reserve a for (Element of SAS);
reserve b for (Element of SAS);
reserve c for (Element of SAS);
theorem
Th18: (a,b // a,c implies ((((((((((((((((((((((a,c // a,b & b,a // a,c) & a,b // c,a) & a,c // b,a) & b,a // c,a) & c,a // a,b) & c,a // b,a) & b,a // b,c) & a,b // b,c) & b,a // c,b) & b,c // b,a) & a,b // c,b) & c,b // b,a) & b,c // a,b) & c,b // a,b) & c,a // c,b) & a,c // c,b) & c,a // b,c) & a,c // b,c) & c,b // c,a) & b,c // c,a) & c,b // a,c) & b,c // a,c))
proof
assume A1: a,b // a,c;
then a,c // a,b by CKB5:1;
then A2: c,a // c,b by CKB1:def 1;
b,a // b,c by A1,CKB1:def 1;
hence thesis by A1,A2,CKB9:1;
end;
