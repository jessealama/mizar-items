environ
vocabularies XBOOLE_0,ANALOAF,SUBSET_1,DIRAF,PARSP_2,FDIFF_1,SEMI_AF1;
notations STRUCT_0,ANALOAF,DIRAF,CKB1,CKB3,CKB25,CKB40;
definitions CKB1,CKB25,CKB40;
theorems PARDEPAP,DIRAF,CKB1,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB37,CKB38,CKB39,CKB40,CKB41,CKB42,CKB43,CKB44,CKB45,CKB46,CKB47;
registrations ANALOAF,STRUCT_0,CKB2;
constructors DIRAF,CKB1,CKB25,CKB40;
begin
reserve SAS for Semi_Affine_Space;
reserve a for (Element of SAS);
reserve b for (Element of SAS);
reserve c for (Element of SAS);
reserve d for (Element of SAS);
theorem
Th61: ( parallelogram a,b,c,d implies ((((( parallelogram a,c,b,d &  parallelogram c,d,a,b) &  parallelogram b,a,d,c) &  parallelogram c,a,d,b) &  parallelogram d,b,c,a) &  parallelogram b,d,a,c))
proof
assume A1:  parallelogram a,b,c,d;
then A2: ((not c,d,a is_collinear ) & (not b,a,d is_collinear )) by CKB43:1;
A3: ((not a,c,b is_collinear ) & a,c // b,d) by A1,CKB40:def 1,CKB43:1;
A4: ((not c,a,d is_collinear ) & (not d,b,c is_collinear )) by A1,CKB43:1;
A5: a,b // c,d by A1,CKB40:def 1;
then A6: d,c // b,a by CKB9:1;
A7: a,c // b,d by A1,CKB40:def 1;
then A8: (c,a // d,b & b,d // a,c) by CKB9:1;
A9: (not b,d,a is_collinear ) by A1,CKB43:1;
A10: d,b // c,a by A7,CKB9:1;
(c,d // a,b & b,a // d,c) by A5,CKB9:1;
hence thesis by A5,A3,A2,A8,A4,A10,A6,A9,CKB40:def 1;
end;
