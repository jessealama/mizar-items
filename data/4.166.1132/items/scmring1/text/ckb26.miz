constructors XXREAL_0,FINSEQ_4,REALSET2,CAT_2,RELSET_1,AMI_2,FUNCOP_1,SETFAM_1,FINSEQ_2,RECDEF_2,CKB2,CKB4,CKB6,CKB14,CKB15,CKB19,CKB23,CKB24,CKB25;
requirements NUMERALS,REAL,SUBSET,BOOLE;
begin
reserve I for (Element of ( Segm 8 ));
reserve S for non  empty 1-sorted;
theorem
(for x being (Element of ( SCM-Instr S )) holds (for mk,ml being (Element of ( SCM-Data-Loc )) holds (x = [ I,( {} ),<* mk,ml *> ] implies (( x address_1 ) = mk & ( x address_2 ) = ml))))
proof
let x being (Element of ( SCM-Instr S ));
let mk being (Element of ( SCM-Data-Loc ));
let ml being (Element of ( SCM-Data-Loc ));
assume A1: x = [ I,( {} ),<* mk,ml *> ];
then consider f being (FinSequence of ( SCM-Data-Loc )) such that A2: f = ( x `3_3 ) and A3: ( x address_1 ) = ( f /. 1 ) by CKB24:def 1;
f = <* mk,ml *> by A1,A2,RECDEF_2:def 3;
hence ( x address_1 ) = mk by A3,FINSEQ_4:17;
consider f being (FinSequence of ( SCM-Data-Loc )) such that A4: f = ( x `3_3 ) and A5: ( x address_2 ) = ( f /. 2 ) by A1,CKB25:def 1;
f = <* mk,ml *> by A1,A4,RECDEF_2:def 3;
hence thesis by A5,FINSEQ_4:17;
end;
