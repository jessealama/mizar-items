begin
reserve I for (Element of ( Segm 8 ));
reserve S for non  empty 1-sorted;
theorem
(for x being (Element of ( SCM-Instr S )) holds (for mk being (Element of ( NAT )) holds (for ml being (Element of ( SCM-Data-Loc )) holds (x = [ I,<* mk *>,<* ml *> ] implies (( x cjump_address ) = mk & ( x cond_address ) = ml)))))
proof
let x being (Element of ( SCM-Instr S ));
let mk being (Element of ( NAT ));
let ml being (Element of ( SCM-Data-Loc ));
assume A1: x = [ I,<* mk *>,<* ml *> ];
then consider mk9 being (Element of ( NAT )) such that A2: <* mk9 *> = ( x `2_3 ) and A3: ( x cjump_address ) = ( <* mk9 *> /. 1 ) by CKB29:def 1;
<* mk9 *> = <* mk *> by A1,A2,RECDEF_2:def 2;
hence ( x cjump_address ) = mk by A3,FINSEQ_4:16;
consider ml9 being (Element of ( SCM-Data-Loc )) such that A4: <* ml9 *> = ( x `3_3 ) and A5: ( x cond_address ) = ( <* ml9 *> /. 1 ) by A1,CKB30:def 1;
<* ml9 *> = <* ml *> by A1,A4,RECDEF_2:def 3;
hence thesis by A5,FINSEQ_4:16;
end;
