constructors XXREAL_0,FINSEQ_4,REALSET2,CAT_2,RELSET_1,AMI_2,FUNCOP_1,SETFAM_1,FINSEQ_2,RECDEF_2,CKB2,CKB4,CKB6,CKB14,CKB15,CKB19,CKB23,CKB24,CKB25,CKB27,CKB29,CKB30,CKB32,CKB33,CKB34;
requirements NUMERALS,REAL,SUBSET,BOOLE;
begin
reserve I for (Element of ( Segm 8 ));
reserve S for non  empty 1-sorted;
theorem
(for x being (Element of ( SCM-Instr S )) holds (for mk being (Element of ( SCM-Data-Loc )) holds (for r being (Element of S) holds (x = [ I,( {} ),<* mk,r *> ] implies (( x const_address ) = mk & ( x const_value ) = r)))))
proof
let x being (Element of ( SCM-Instr S ));
let mk being (Element of ( SCM-Data-Loc ));
let r being (Element of S);
A1: (mk is (Element of ( ( SCM-Data-Loc ) \/ (the carrier of S) )) & r is (Element of ( ( SCM-Data-Loc ) \/ (the carrier of S) ))) by XBOOLE_0:def 3;
assume A2: x = [ I,( {} ),<* mk,r *> ];
then consider f being (FinSequence of ( ( SCM-Data-Loc ) \/ (the carrier of S) )) such that A3: f = ( x `3_3 ) and A4: ( x const_address ) = ( f /. 1 ) by CKB33:def 1;
f = <* mk,r *> by A2,A3,RECDEF_2:def 3;
hence ( x const_address ) = mk by A4,A1,FINSEQ_4:17;
consider f being (FinSequence of ( ( SCM-Data-Loc ) \/ (the carrier of S) )) such that A5: f = ( x `3_3 ) and A6: ( x const_value ) = ( f /. 2 ) by A2,CKB34:def 1;
f = <* mk,r *> by A2,A5,RECDEF_2:def 3;
hence thesis by A1,A6,FINSEQ_4:17;
end;
