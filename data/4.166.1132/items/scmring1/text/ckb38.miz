begin
definition
let R being  good Ring;
func SCM-Exec R -> (Action of ( SCM-Instr R ),( product ( SCM-OK R ) )) means 
(for x being (Element of ( SCM-Instr R )) holds (for y being (SCM-State of R) holds ( ( it . x ) . y ) = ( SCM-Exec-Res (x,y) )));
existence
proof
deffunc U((Element of ( SCM-Instr R )),(SCM-State of R)) = ( SCM-Exec-Res ($1,$2) );
consider f being (Function of [: ( SCM-Instr R ),( product ( SCM-OK R ) ) :],( product ( SCM-OK R ) )) such that A1: (for x being (Element of ( SCM-Instr R )) holds (for y being (SCM-State of R) holds ( f . (x,y) ) = U(x,y))) from BINOP_1:sch 4;
take ( curry f );
let x being (Element of ( SCM-Instr R ));
let y being (SCM-State of R);
thus ( ( ( curry f ) . x ) . y ) = ( f . (x,y) ) by CAT_2:3
.= ( SCM-Exec-Res (x,y) ) by A1;
end;
uniqueness
proof
let f being (Action of ( SCM-Instr R ),( product ( SCM-OK R ) ));
let g being (Action of ( SCM-Instr R ),( product ( SCM-OK R ) ));
assume that
A2: (for x being (Element of ( SCM-Instr R )) holds (for y being (SCM-State of R) holds ( ( f . x ) . y ) = ( SCM-Exec-Res (x,y) )))
and
A3: (for x being (Element of ( SCM-Instr R )) holds (for y being (SCM-State of R) holds ( ( g . x ) . y ) = ( SCM-Exec-Res (x,y) )));
now
let x being (Element of ( SCM-Instr R ));
reconsider gx = ( g . x ),fx = ( f . x ) as (Function of ( product ( SCM-OK R ) ),( product ( SCM-OK R ) )) by FUNCT_2:66;
now
let y being (SCM-State of R);
thus ( fx . y ) = ( SCM-Exec-Res (x,y) ) by A2
.= ( gx . y ) by A3;
end;
hence ( f . x ) = ( g . x ) by FUNCT_2:63;
end;
hence f = g by FUNCT_2:63;
end;
end;
