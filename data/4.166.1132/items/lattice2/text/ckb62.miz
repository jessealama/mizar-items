reserve L for Lattice;
reserve c for (Element of L);
theorem
Th64: (for L being Lattice holds (L is  upper-bounded iff ( L .: ) is  lower-bounded))
proof
let L being Lattice;
thus (L is  upper-bounded implies ( L .: ) is  lower-bounded)
proof
given c being (Element of L) such that
A1: (for a being (Element of L) holds (( c "\/" a ) = c & ( a "\/" c ) = c));

reconsider c9 = c as (Element of ( L .: ));
take c9;
let a9 being (Element of ( L .: ));
reconsider a = a9 as (Element of L);
thus ( c9 "/\" a9 ) = ( c "\/" a )
.= c9 by A1;
hence ( a9 "/\" c9 ) = c9;
end;

given c being (Element of ( L .: )) such that
A2: (for a being (Element of ( L .: )) holds (( c "/\" a ) = c & ( a "/\" c ) = c));

reconsider c9 = c as (Element of L);
take c9;
let a9 being (Element of L);
reconsider a = a9 as (Element of ( L .: ));
thus ( c9 "\/" a9 ) = ( c "/\" a )
.= c9 by A2;
hence ( a9 "\/" c9 ) = c9;
end;
