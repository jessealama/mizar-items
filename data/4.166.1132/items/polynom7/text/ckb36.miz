theorem
Lm2: (for X being set holds (for L being non empty ZeroStr holds (for c being (ConstPoly of X,L) holds (( term c ) = ( EmptyBag X ) & ( coefficient c ) = ( c . ( EmptyBag X ) )))))
proof
let n being set;
let L being non empty ZeroStr;
let c being (ConstPoly of n,L);
set eb = ( EmptyBag n );
A1:now
per cases ;
case ( c . eb ) <> ( 0. L );
hence ( term c ) = ( EmptyBag n ) by CKB20:def 1;
end;
case A2: ( c . eb ) = ( 0. L );
( Support c ) = ( {} )
proof
set u = the (Element of ( Support c ));
assume A3: ( Support c ) <> ( {} );
