begin
theorem
(for L being  right_zeroed  add-associative  right_complementable  well-unital  distributive non trivial doubleLoopStr holds (for p being (Polynomial of ( {} ),L) holds (for x being (Function of ( {} ),L) holds ( eval (p,x) ) = ( p . ( EmptyBag ( {} ) ) ))))
proof
set n = ( {} );
let L being  right_zeroed  add-associative  right_complementable  well-unital  distributive non trivial doubleLoopStr;
let p being (Polynomial of ( {} ),L);
let x being (Function of ( {} ),L);
A1: (for b being (bag of n) holds b = ( {} ))
proof
let b being (bag of n);
b in ( Bags n ) by PRE_POLY:def 12;
hence thesis by PRE_POLY:51,TARSKI:def 1;
end;
then A2: ( EmptyBag n ) = ( {} );
consider a being (Element of L) such that A3: p = ( { ( EmptyBag n ) } --> a ) by CKB11:1;
A4: ( EmptyBag n ) in { ( EmptyBag n ) } by TARSKI:def 1;
then A5: ( p . ( EmptyBag n ) ) = a by A3,FUNCOP_1:7;
A6: ( dom p ) = { ( EmptyBag n ) } by A3,FUNCOP_1:13;
now
per cases ;
case A7: a = ( 0. L );
( Support p ) = ( {} )
proof
set u = the (Element of ( Support p ));
assume A8: ( Support p ) <> ( {} );
then u in ( Support p );
then reconsider u as (Element of ( Bags n ));
( p . u ) <> ( 0. L ) by A8,POLYNOM1:def 3;
hence thesis by A1,A2,A5,A7;
end;
then reconsider Sp = ( Support p ) as  empty (Subset of ( Bags n ));
consider y being (FinSequence of (the carrier of L)) such that A9: ( len y ) = ( len ( SgmX (( BagOrder n ),( Support p )) ) ) and A10: ( eval (p,x) ) = ( Sum y ) and (for i being (Element of ( NAT )) holds ((1 <= i & i <= ( len y )) implies ( y /. i ) = ( ( ( p * ( SgmX (( BagOrder n ),( Support p )) ) ) /. i ) * ( eval (( ( ( SgmX (( BagOrder n ),( Support p )) ) /. i ) @ ),x) ) ))) by POLYNOM2:def 4;
( SgmX (( BagOrder n ),Sp) ) = ( {} ) by POLYNOM2:7,POLYNOM2:18;
then y = ( <*> (the carrier of L) ) by A9;
hence ( eval (p,x) ) = a by A7,A10,RLVECT_1:43;
end;
case A11: a <> ( 0. L );
reconsider sp = ( Support p ) as  finite (Subset of ( Bags n ));
set sg = ( SgmX (( BagOrder n ),sp) );
A12: ( BagOrder n ) linearly_orders sp by POLYNOM2:18;
A13: (for u being set holds (u in ( Support p ) implies u in { ( {} ) }))
proof
let u being set;
assume u in ( Support p );
then reconsider u as (Element of ( Bags n ));
u = ( {} ) by A1;
hence thesis by TARSKI:def 1;
end;
(for u being set holds (u in { ( {} ) } implies u in ( Support p )))
proof
let u being set;
assume u in { ( {} ) };
then u = ( EmptyBag n ) by A2,TARSKI:def 1;
hence thesis by A5,A11,POLYNOM1:def 3;
end;
then ( Support p ) = { ( {} ) } by A13,TARSKI:1;
then A14: ( rng sg ) = { ( {} ) } by A12,PRE_POLY:def 2;
then A15: ( {} ) in ( rng sg ) by TARSKI:def 1;
then A16: 1 in ( dom sg ) by FINSEQ_3:31;
then ( sg . 1 ) in ( dom p ) by A2,A6,A14,FUNCT_1:3;
then 1 in ( dom ( p * sg ) ) by A16,FUNCT_1:11;
then A17: ( ( p * sg ) /. 1 ) = ( ( p * sg ) . 1 ) by PARTFUN1:def 6
.= ( p . ( sg . 1 ) ) by A16,FUNCT_1:13
.= a by A2,A3,A14,A16,FUNCOP_1:7,FUNCT_1:3;
A18: (for u being set holds (u in ( dom sg ) implies u in { 1 }))
proof
let u being set;
assume A19: u in ( dom sg );
assume A20: (not u in { 1 });
reconsider u as (Element of ( NAT )) by A19;
( sg /. u ) = ( sg . u ) by A19,PARTFUN1:def 6;
then A21: ( sg /. u ) in ( rng sg ) by A19,FUNCT_1:3;
A22: u <> 1 by A20,TARSKI:def 1;
A23: 1 < u
proof
consider k being Nat such that A24: ( dom sg ) = ( Seg k ) by FINSEQ_1:def 2;
( Seg k ) = { m where m is (Element of ( NAT )): (1 <= m & m <= k) } by FINSEQ_1:def 1;
then (ex m9 being (Element of ( NAT )) st ((m9 = u & 1 <= m9) & m9 <= k)) by A19,A24;
hence thesis by A22,XXREAL_0:1;
end;
( sg /. 1 ) = ( sg . 1 ) by A15,A19,FINSEQ_3:31,PARTFUN1:def 6;
then ( sg /. 1 ) in ( rng sg ) by A16,FUNCT_1:3;
then ( sg /. 1 ) = ( {} ) by A14,TARSKI:def 1
.= ( sg /. u ) by A14,A21,TARSKI:def 1;
hence thesis by A12,A16,A19,A23,PRE_POLY:def 2;
end;
(for u being set holds (u in { 1 } implies u in ( dom sg ))) by A16,TARSKI:def 1;
then ( dom sg ) = ( Seg 1 ) by A18,FINSEQ_1:2,TARSKI:1;
then A25: ( len sg ) = 1 by FINSEQ_1:def 3;
consider y being (FinSequence of (the carrier of L)) such that A26: ( len y ) = ( len sg ) and A27: ( Sum y ) = ( eval (p,x) ) and A28: (for i being (Element of ( NAT )) holds ((1 <= i & i <= ( len y )) implies ( y /. i ) = ( ( ( p * sg ) /. i ) * ( eval (( ( sg /. i ) @ ),x) ) ))) by POLYNOM2:def 4;
( dom y ) = ( Seg ( len y ) ) by FINSEQ_1:def 3
.= ( dom sg ) by A26,FINSEQ_1:def 3;
then ( y . 1 ) = ( y /. 1 ) by A15,FINSEQ_3:31,PARTFUN1:def 6
.= ( ( ( p * sg ) /. 1 ) * ( eval (( ( sg /. 1 ) @ ),x) ) ) by A25,A26,A28
.= ( ( ( p * sg ) /. 1 ) * ( eval (( EmptyBag n ),x) ) ) by A1,A2
.= ( ( ( p * sg ) /. 1 ) * ( 1. L ) ) by POLYNOM2:14
.= a by A17,VECTSP_1:def 4;
then y = <* a *> by A25,A26,FINSEQ_1:40;
hence ( eval (p,x) ) = a by A27,RLVECT_1:44;
end;
end;
hence thesis by A3,A4,FUNCOP_1:7;
end;
