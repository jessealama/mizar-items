schemes FUNCT_2;
registrations XBOOLE_0,ORDINAL1,RELSET_1,FUNCOP_1,FINSET_1,XREAL_0,STRUCT_0,VECTSP_1,ALGSTR_1,POLYNOM1,POLYNOM2,ALGSTR_0,CARD_1,SUBSET_1,PRE_POLY,ZFMISC_1,FUNCT_1,CKB1,CKB3,CKB4,CKB8,CKB9,CKB15,CKB17;
constructors RFUNCT_3,REALSET2,GRCAT_1,GROUP_6,TRIANG_1,QUOFIELD,POLYNOM2,ALGSTR_1,RELSET_1,FUNCT_7,FVSUM_1,VFUNCT_1,CKB2,CKB7,CKB14,CKB19,CKB20,CKB21;
requirements NUMERALS,BOOLE,SUBSET,ARITHM;
begin
theorem
(for X being set holds (for L being non  empty ZeroStr holds (for m being (Monomial of X,L) holds ( Monom (( coefficient m ),( term m )) ) = m)))
proof
let X being set;
let L being non  empty ZeroStr;
let m being (Monomial of X,L);
A1: (( dom m ) = ( Bags X ) & ( dom ( Monom (( coefficient m ),( term m )) ) ) = ( Bags X )) by FUNCT_2:def 1;
per cases  by CKB18:1;
suppose A2: ( Support m ) = ( {} );

A3:now
A4: ( term m ) is (Element of ( Bags X )) by PRE_POLY:def 12;
assume ( coefficient m ) <> ( 0. L );
hence contradiction by A2,A4,POLYNOM1:def 3;
end;
A5: m = ( 0_ (X,L) ) by A2,CKB5:1;
set m9 = ( Monom (( coefficient m ),( term m )) );
A6: ( dom ( 0_ (X,L) ) ) = ( dom ( ( Bags X ) --> ( 0. L ) ) ) by POLYNOM1:def 7
.= ( Bags X ) by FUNCOP_1:13;
A7: ( dom ( ( EmptyBag X ) .--> ( 0. L ) ) ) = { ( EmptyBag X ) } by FUNCOP_1:13;
then A8: ( EmptyBag X ) in ( dom ( ( EmptyBag X ) .--> ( 0. L ) ) ) by TARSKI:def 1;
A9: ( term m ) = ( EmptyBag X ) by A2,CKB20:def 1;
then A10: ( m9 . ( EmptyBag X ) ) = ( ( ( 0_ (X,L) ) +* ( ( EmptyBag X ) .--> ( 0. L ) ) ) . ( EmptyBag X ) ) by A3,A6,FUNCT_7:def 3
.= ( ( ( EmptyBag X ) .--> ( 0. L ) ) . ( EmptyBag X ) ) by A8,FUNCT_4:13
.= ( 0. L ) by FUNCOP_1:72;
now
let x being set;
assume x in ( Bags X );
then reconsider x9 = x as (Element of ( Bags X ));
now
per cases ;
case x9 = ( EmptyBag X );
hence ( m9 . x9 ) = ( 0. L ) by A10;
end;
case x <> ( EmptyBag X );
then A11: (not x9 in ( dom ( ( EmptyBag X ) .--> ( 0. L ) ) )) by A7,TARSKI:def 1;
( m9 . x9 ) = ( ( ( 0_ (X,L) ) +* ( ( EmptyBag X ) .--> ( 0. L ) ) ) . x9 ) by A9,A3,A6,FUNCT_7:def 3
.= ( ( 0_ (X,L) ) . x9 ) by A11,FUNCT_4:11;
hence ( m9 . x9 ) = ( 0. L ) by POLYNOM1:22;
end;
end;
hence ( m . x ) = ( m9 . x ) by A5,POLYNOM1:22;
end;
hence thesis by A1,FUNCT_1:2;
end;
suppose (ex b being (bag of X) st ( Support m ) = { b });

then consider b being (bag of X) such that A12: ( Support m ) = { b };
set a = ( m . b );
( dom ( b .--> a ) ) = { b } by FUNCOP_1:13;
then A13: b in ( dom ( b .--> a ) ) by TARSKI:def 1;
set m9 = ( Monom (( coefficient m ),( term m )) );
A14: ( dom ( 0_ (X,L) ) ) = ( dom ( ( Bags X ) --> ( 0. L ) ) ) by POLYNOM1:def 7
.= ( Bags X ) by FUNCOP_1:13;
A15: b in ( Support m ) by A12,TARSKI:def 1;
then a <> ( 0. L ) by POLYNOM1:def 3;
then A16: ( term m ) = b by CKB20:def 1;
A17:now
let u being set;
assume A18: u in ( Support ( Monom (( coefficient m ),( term m )) ) );
then reconsider u9 = u as (Element of ( Bags X ));
now
A19: ( dom ( b .--> a ) ) = { b } by FUNCOP_1:13;
assume u <> b;
then A20: (not u9 in ( dom ( b .--> a ) )) by A19,TARSKI:def 1;
b in ( dom ( 0_ (X,L) ) ) by A14,PRE_POLY:def 12;
then ( m9 . u9 ) = ( ( ( 0_ (X,L) ) +* ( b .--> a ) ) . u9 ) by A16,FUNCT_7:def 3
.= ( ( 0_ (X,L) ) . u9 ) by A20,FUNCT_4:11;
hence ( m9 . u9 ) = ( 0. L ) by POLYNOM1:22;
end;
hence u in { b } by A18,POLYNOM1:def 3,TARSKI:def 1;
end;
b in ( Bags X ) by PRE_POLY:def 12;
then A21: ( m9 . b ) = ( ( ( 0_ (X,L) ) +* ( b .--> a ) ) . b ) by A16,A14,FUNCT_7:def 3
.= ( ( b .--> a ) . b ) by A13,FUNCT_4:13
.= a by FUNCOP_1:72;
now
let u being set;
assume u in { b };
then A22: u = b by TARSKI:def 1;
( m9 . b ) <> ( 0. L ) by A15,A21,POLYNOM1:def 3;
hence u in ( Support ( Monom (( coefficient m ),( term m )) ) ) by A15,A22,POLYNOM1:def 3;
end;
then A23: ( Support ( Monom (( coefficient m ),( term m )) ) ) = { b } by A17,TARSKI:1;
now
let x being set;
assume x in ( Bags X );
then reconsider x9 = x as (Element of ( Bags X ));
now
per cases ;
case x = b;
hence ( ( Monom (( coefficient m ),( term m )) ) . x9 ) = ( m . x9 ) by A21;
end;
case A24: x <> b;
then A25: (not x in ( Support ( Monom (( coefficient m ),( term m )) ) )) by A23,TARSKI:def 1;
(not x in ( Support m )) by A12,A24,TARSKI:def 1;
hence ( m . x9 ) = ( 0. L ) by POLYNOM1:def 3
.= ( ( Monom (( coefficient m ),( term m )) ) . x9 ) by A25,POLYNOM1:def 3;
end;
end;
hence ( m . x ) = ( ( Monom (( coefficient m ),( term m )) ) . x );
end;
hence thesis by A1,FUNCT_1:2;
end;
end;
