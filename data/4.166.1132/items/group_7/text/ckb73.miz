begin
reserve G1 for non  empty multMagma;
reserve x1 for (Element of G1);
reserve x2 for (Element of G1);
theorem
Th31: ( <* x1 *> * <* x2 *> ) = <* ( x1 * x2 ) *>
proof
set G = <* G1 *>;
A1: ( G . 1 ) = G1 by FINSEQ_1:def 8;
reconsider l = <* x1 *>,p = <* x2 *>,lpl = ( <* x1 *> * <* x2 *> ),lpp = <* ( x1 * x2 ) *> as (Element of ( product ( Carrier G ) )) by CKB8:def 1;
A2: ( l . 1 ) = x1 by FINSEQ_1:def 8;
A3: ( p . 1 ) = x2 by FINSEQ_1:def 8;
A4: ( lpp . 1 ) = ( x1 * x2 ) by FINSEQ_1:def 8;
A5: 1 in { 1 } by TARSKI:def 1;
A6: (for k being Nat holds ((1 <= k & k <= 1) implies ( lpl . k ) = ( lpp . k )))
proof
let k being Nat;
assume that
A7: 1 <= k
and
A8: k <= 1;
k in ( NAT ) by ORDINAL1:def 12;
then k in ( Seg 1 ) by A7,A8;
then k = 1 by FINSEQ_1:2,TARSKI:def 1;
hence thesis by A5,A2,A3,A1,A4,CKB10:1;
end;
( dom lpl ) = ( dom ( Carrier G ) ) by CARD_3:9
.= ( Seg 1 ) by FINSEQ_1:2,PARTFUN1:def 2;
then A9: ( len lpl ) = 1 by FINSEQ_1:def 3;
( len lpp ) = 1 by FINSEQ_1:39;
hence thesis by A9,A6,FINSEQ_1:14;
end;
