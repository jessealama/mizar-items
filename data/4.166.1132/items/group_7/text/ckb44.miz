begin
definition
let G1 being non  empty multMagma;
let x being (Element of G1);
redefine func <*x *> -> (Element of ( product <* G1 *> ));

coherence
proof
set xy = <* x *>;
set G = <* G1 *>;
A1: ( dom ( Carrier G ) ) = { 1 } by PARTFUN1:def 2;
A2: ( G . 1 ) = G1 by FINSEQ_1:def 8;
A3: ( xy . 1 ) = x by FINSEQ_1:def 8;
A4: (for a being set holds (a in { 1 } implies ( xy . a ) in ( ( Carrier G ) . a )))
proof
let a being set;
assume A5: a in { 1 };
then A6: a = 1 by TARSKI:def 1;
then (ex R being 1-sorted st (R = ( G . 1 ) & ( ( Carrier G ) . 1 ) = (the carrier of R))) by A5,PRALG_1:def 13;
hence thesis by A3,A2,A6;
end;
( dom xy ) = { 1 } by FINSEQ_1:2,FINSEQ_1:def 8;
then xy in ( product ( Carrier G ) ) by A1,A4,CARD_3:9;
hence thesis by CKB8:def 1;
end;
end;
