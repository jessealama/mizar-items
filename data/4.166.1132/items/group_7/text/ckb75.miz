requirements NUMERALS,BOOLE,SUBSET;
begin
reserve G1 for non  empty multMagma;
reserve G2 for non  empty multMagma;
reserve G3 for non  empty multMagma;
reserve x1 for (Element of G1);
reserve x2 for (Element of G1);
reserve y1 for (Element of G2);
reserve y2 for (Element of G2);
reserve z1 for (Element of G3);
reserve z2 for (Element of G3);
theorem
( <* x1,y1,z1 *> * <* x2,y2,z2 *> ) = <* ( x1 * x2 ),( y1 * y2 ),( z1 * z2 ) *>
proof
set G = <* G1,G2,G3 *>;
A1: 3 in { 1,2,3 } by ENUMSET1:def 1;
A2: ( G . 1 ) = G1 by FINSEQ_1:45;
A3: ( G . 3 ) = G3 by FINSEQ_1:45;
A4: ( G . 2 ) = G2 by FINSEQ_1:45;
reconsider l = <* x1,y1,z1 *>,p = <* x2,y2,z2 *>,lpl = ( <* x1,y1,z1 *> * <* x2,y2,z2 *> ),lpp = <* ( x1 * x2 ),( y1 * y2 ),( z1 * z2 ) *> as (Element of ( product ( Carrier G ) )) by CKB8:def 1;
A5: 2 in { 1,2,3 } by ENUMSET1:def 1;
A6: ( l . 1 ) = x1 by FINSEQ_1:45;
A7: ( l . 3 ) = z1 by FINSEQ_1:45;
A8: ( l . 2 ) = y1 by FINSEQ_1:45;
A9: ( lpp . 3 ) = ( z1 * z2 ) by FINSEQ_1:45;
A10: ( lpp . 2 ) = ( y1 * y2 ) by FINSEQ_1:45;
A11: ( lpp . 1 ) = ( x1 * x2 ) by FINSEQ_1:45;
A12: ( p . 3 ) = z2 by FINSEQ_1:45;
A13: ( p . 2 ) = y2 by FINSEQ_1:45;
A14: ( p . 1 ) = x2 by FINSEQ_1:45;
A15: 1 in { 1,2,3 } by ENUMSET1:def 1;
A16: (for k being Nat holds ((1 <= k & k <= 3) implies ( lpl . k ) = ( lpp . k )))
proof
let k being Nat;
assume that
A17: 1 <= k
and
A18: k <= 3;
k in ( NAT ) by ORDINAL1:def 12;
then A19: k in ( Seg 3 ) by A17,A18;
per cases  by A19,ENUMSET1:def 1,FINSEQ_3:1;
suppose k = 1;

hence thesis by A15,A6,A14,A2,A11,CKB10:1;
end;
suppose k = 2;

hence thesis by A5,A8,A13,A4,A10,CKB10:1;
end;
suppose k = 3;

hence thesis by A1,A7,A12,A3,A9,CKB10:1;
end;
end;
( dom lpl ) = ( dom ( Carrier G ) ) by CARD_3:9
.= ( Seg 3 ) by FINSEQ_3:1,PARTFUN1:def 2;
then A20: ( len lpl ) = 3 by FINSEQ_1:def 3;
( len lpp ) = 3 by FINSEQ_1:45;
hence thesis by A20,A16,FINSEQ_1:14;
end;
