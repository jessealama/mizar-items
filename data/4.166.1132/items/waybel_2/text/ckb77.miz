theorem
Th53: (for L being  up-complete Semilattice holds (L is  meet-continuous iff ( inf_op L ) is  directed-sups-preserving))
proof
let L being  up-complete Semilattice;
hereby
assume L is  meet-continuous;
then (for x being (Element of L) holds (for D being non empty  directed (Subset of L) holds (x <= ( sup D ) implies x <= ( sup ( { x } "/\" D ) )))) by CKB68:1;
hence ( inf_op L ) is  directed-sups-preserving by CKB69:1;
end;
assume ( inf_op L ) is  directed-sups-preserving;
then (for D1,D2 being non empty  directed (Subset of L) holds ( ( sup D1 ) "/\" ( sup D2 ) ) = ( sup ( D1 "/\" D2 ) )) by CKB66:1;
hence (L is  up-complete & L is  satisfying_MC) by CKB67:1;
end;
