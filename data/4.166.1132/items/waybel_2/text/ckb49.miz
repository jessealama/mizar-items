begin
definition
let L being non  empty RelStr;
func sup_op L -> (Function of [: L,L :],L) means 
:Def5: (for x,y being (Element of L) holds ( it . (x,y) ) = ( x "\/" y ));
existence
proof
deffunc F((Element of L),(Element of L)) = ( $1 "\/" $2 );
A1: (for x,y being (Element of L) holds F(x,y) is (Element of L));
consider f being (Function of [: L,L :],L) such that A2: (for x,y being (Element of L) holds ( f . (x,y) ) = F(x,y)) from YELLOW_3:sch 6(A1);
take f;
thus thesis by A2;
end;
uniqueness
proof
let f being (Function of [: L,L :],L);
let g being (Function of [: L,L :],L);
assume that
A3: (for x,y being (Element of L) holds ( f . (x,y) ) = ( x "\/" y ))
and
A4: (for x,y being (Element of L) holds ( g . (x,y) ) = ( x "\/" y ));
now
let x being (Element of L);
let y being (Element of L);
thus ( f . (x,y) ) = ( x "\/" y ) by A3
.= ( g . (x,y) ) by A4;
end;
hence f = g by YELLOW_3:13;
end;
end;
