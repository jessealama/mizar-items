theorem
Th20: (for L being non empty  reflexive RelStr holds (for D being non empty  directed (Subset of L) holds (for n being (Function of D,(the carrier of L)) holds (for N being (prenet of L) holds ((n = ( id D ) & N = NetStr (# D,( (the InternalRel of L) |_2 D ),n #)) implies N is  eventually-directed)))))
proof
let L being non empty  reflexive RelStr;
let D being non empty  directed (Subset of L);
let n being (Function of D,(the carrier of L));
let N being (prenet of L);
assume that
A1: n = ( id D )
and
A2: N = NetStr (# D,( (the InternalRel of L) |_2 D ),n #);
(for i being (Element of N) holds (ex j being (Element of N) st (for k being (Element of N) holds (j <= k implies ( N . i ) <= ( N . k )))))
proof
let i being (Element of N);
take j = i;
let k being (Element of N);
assume that
A3: j <= k;
(the InternalRel of N) c= (the InternalRel of L) by A2,XBOOLE_1:17;
then A4: N is (SubRelStr of L) by A2,YELLOW_0:def 13;
reconsider nj = ( n . j ),nk = ( n . k ) as (Element of L) by A2,FUNCT_2:5;
(nj = j & nk = k) by A1,A2,FUNCT_1:18;
hence thesis by A2,A3,A4,YELLOW_0:59;
end;
hence thesis by WAYBEL_0:11;
