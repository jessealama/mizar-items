proof
let L being non  empty RelStr;
assume that
A1: L is  reflexive  connected;
thus L is  with_infima
proof
let x being (Element of L);
let y being (Element of L);
per cases  by A1,WAYBEL_0:def 29;
suppose A2: x <= y;

take z = x;
thus (z <= x & z <= y) by A1,A2,YELLOW_0:def 1;
thus thesis;
end;
suppose A3: y <= x;

take z = y;
thus (z <= x & z <= y) by A1,A3,YELLOW_0:def 1;
thus thesis;
end;
end;

let x being (Element of L);
let y being (Element of L);
per cases  by A1,WAYBEL_0:def 29;
suppose A4: x <= y;

take z = y;
thus (z >= x & z >= y) by A1,A4,YELLOW_0:def 1;
thus thesis;
end;
suppose A5: y <= x;

take z = x;
thus (z >= x & z >= y) by A1,A5,YELLOW_0:def 1;
thus thesis;
end;
end;
end;
