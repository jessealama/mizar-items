registrations XBOOLE_0,SUBSET_1,RELAT_1,FUNCT_1,FUNCT_2,FINSET_1,FINSUB_1,STRUCT_0,LATTICE3,YELLOW_0,WAYBEL_0,YELLOW_1,YELLOW_2,WAYBEL_1,YELLOW_3,YELLOW_4,CARD_1,ORDINAL1,ORDERS_2,RELSET_1,CKB1,CKB2,CKB3,CKB14;
constructors DOMAIN_1,FINSUB_1,TOLER_1,ORDERS_3,WAYBEL_1,YELLOW_3,YELLOW_4,RELSET_1;
