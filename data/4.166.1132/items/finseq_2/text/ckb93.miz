reserve D9 for non empty set;
reserve E for non empty set;
reserve d for (Element of D);
reserve d19 for (Element of D9);
theorem
(for F being (Function of [: D,D9 :],E) holds (for p being (FinSequence of D9) holds (p = <* d19 *> implies ( F [;] (d,p) ) = <* ( F . (d,d19) ) *>)))
proof
let F being (Function of [: D,D9 :],E);
let p being (FinSequence of D9);
assume that
A1: p = <* d19 *>;
A2: ( p . 1 ) = d19 by A1,FINSEQ_1:40;
reconsider r = ( F [;] (d,p) ) as (FinSequence of E) by CKB90:1;
( len p ) = 1 by A1,FINSEQ_1:39;
then A3: ( len r ) = 1 by CKB91:1;
then 1 in ( Seg ( len r ) );
then 1 in ( dom r ) by FINSEQ_1:def 3;
then ( r . 1 ) = ( F . (d,d19) ) by A2,FUNCOP_1:32;
hence thesis by A3,FINSEQ_1:40;
end;
