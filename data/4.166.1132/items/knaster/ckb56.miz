begin
reserve O for Ordinal;
reserve L for  complete Lattice;
reserve f for  monotone (UnOp of L);
theorem
( FixPoints f ) is  complete
proof
set F = ( FixPoints f );
set cF = (the carrier of F);
set cL = (the carrier of L);
let X being set;
set Y = ( X /\ cF );
set s = ( "\/" (Y,L) );
A1: Y c= cF by XBOOLE_1:17;
Y is_less_than ( f . s )
proof
let q being (Element of cL);
reconsider q9 = q as (Element of L);
assume A2: q in Y;
then q [= s by LATTICE3:38;
then A3: ( f . q ) [= ( f . s ) by QUANTAL1:def 12;
q9 is_a_fixpoint_of f by A1,A2,CKB54:1;
hence q [= ( f . s ) by A3,ABIAN:def 3;
end;
then A4: s [= ( f . s ) by LATTICE3:def 21;
then consider O such that ( card O ) c= ( card cL ) and A5: ( (f,O) +. s ) is_a_fixpoint_of f by CKB45:1;
reconsider p9 = ( (f,O) +. s ) as (Element of L);
reconsider p = p9 as (Element of cF) by A5,CKB54:1;
take p;
thus X is_less_than p
proof
let q being (Element of cF);
(q in cF & cF c= cL) by CKB53:1;
then reconsider qL9 = q as (Element of L);
assume q in X;
then q in Y by XBOOLE_0:def 4;
then A6: qL9 [= s by LATTICE3:38;
s [= p9 by A4,CKB36:1;
then qL9 [= p9 by A6,LATTICES:7;
hence q [= p by CKB55:1;
end;

let r being (Element of cF);
assume that
A7: X is_less_than r;
reconsider r99 = r as (Element of F);
(cF = { x where x is (Element of L): x is_a_fixpoint_of f } & r in (the carrier of F)) by CKB52:1;
then consider r9 being (Element of L) such that A8: r9 = r and A9: r9 is_a_fixpoint_of f;
A10: Y c= X by XBOOLE_1:17;
Y is_less_than r9
proof
let q being (Element of cL);
assume A11: q in Y;
then reconsider q99 = q as (Element of F) by A1;
q99 [= r99 by A10,A7,A11,LATTICE3:def 17;
hence q [= r9 by A8,CKB55:1;
end;
then s [= r9 by LATTICE3:def 21;
then p9 [= r9 by A9,CKB49:1;
hence p [= r by A8,CKB55:1;
end;
