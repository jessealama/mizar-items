schemes NAT_1,FUNCT_1,FUNCT_2,DOMAIN_1,ORDINAL2,CKB13;
registrations XBOOLE_0,SUBSET_1,FUNCT_1,ORDINAL1,RELSET_1,CARD_1,STRUCT_0,LATTICES,LATTICE3,QUANTAL1,CKB5,CKB32,CKB34;
constructors SETFAM_1,WELLORD1,WELLORD2,BINOP_1,DOMAIN_1,ORDINAL2,NAT_1,ABIAN,BOOLEALG,QUANTAL1,COHSP_1,RELSET_1,CKB4,CKB6,CKB7,CKB18,CKB19,CKB28,CKB29,CKB30,CKB31,CKB33;
requirements NUMERALS,BOOLE,SUBSET;
begin
reserve O for Ordinal;
reserve O1 for Ordinal;
reserve O2 for Ordinal;
reserve L for  complete Lattice;
reserve f for  monotone (UnOp of L);
reserve a for (Element of L);
theorem
Th25: (for a holds (a [= ( f . a ) implies (for O holds a [= ( (f,O) +. a ))))
proof
let a;
defpred S[ Ordinal ]
 means
a [= ( (f,$1) +. a );
deffunc F(Ordinal) = ( (f,$1) +. a );
A1:now
let O1;
assume that
A2: O1 <> ( {} )
and
A3: O1 is  limit_ordinal
and
A4: (for O2 holds (O2 in O1 implies S[ O2 ]));
consider O2 being set such that A5: O2 in O1 by A2,XBOOLE_0:def 1;
reconsider O2 as Ordinal by A5;
A6: S[ O2 ] by A4,A5;
consider Ls being T-Sequence such that A7: (( dom Ls ) = O1 & (for O2 being Ordinal holds (O2 in O1 implies ( Ls . O2 ) = F(O2)))) from ORDINAL2:sch 2;
(( Ls . O2 ) = ( (f,O2) +. a ) & ( Ls . O2 ) in ( rng Ls )) by A7,A5,FUNCT_1:def 3;
then ( (f,O2) +. a ) [= ( "\/" (( rng Ls ),L) ) by LATTICE3:38;
then a [= ( "\/" (( rng Ls ),L) ) by A6,LATTICES:7;
hence S[ O1 ] by A2,A3,A7,CKB24:1;
end;
assume A8: a [= ( f . a );
A9:now
let O1;
assume S[ O1 ];
then ( f . a ) [= ( f . ( (f,O1) +. a ) ) by QUANTAL1:def 12;
then a [= ( f . ( (f,O1) +. a ) ) by A8,LATTICES:7;
hence S[ ( succ O1 ) ] by CKB22:1;
end;
A10: S[ ( {} ) ] by CKB20:1;
thus (for O holds S[ O ]) from ORDINAL2:sch 1(A10,A9,A1);
end;
