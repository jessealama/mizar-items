registrations XBOOLE_0,SUBSET_1,FUNCT_1,ORDINAL1,RELSET_1,CARD_1,STRUCT_0,LATTICES,LATTICE3,QUANTAL1,CKB5;
constructors SETFAM_1,WELLORD1,WELLORD2,BINOP_1,DOMAIN_1,ORDINAL2,NAT_1,ABIAN,BOOLEALG,QUANTAL1,COHSP_1,RELSET_1,CKB4,CKB6,CKB7;
requirements NUMERALS,BOOLE,SUBSET;
begin
reserve x for set;
reserve y for set;
reserve z for set;
reserve u for set;
reserve X for non empty set;
reserve Y for non empty set;
reserve f for (Function of X,Y);
theorem
Th13: (f is  bijective implies X,Y are_equipotent )
proof
assume A1: f is  bijective;
take h = f;
A2: h is  one-to-one  onto by A1,FUNCT_2:def 4;
then A3: ( rng h ) = Y by FUNCT_2:def 3;
hereby
let x;
assume A4: x in X;
take y = ( h . x );
thus y in Y by A3,A4,FUNCT_2:4;
x in ( dom h ) by A4,FUNCT_2:def 1;
hence [ x,y ] in h by FUNCT_1:1;
end;
hereby
let y;
assume y in Y;
then consider x such that A5: (x in ( dom h ) & y = ( h . x )) by A3,FUNCT_1:def 3;
take x;
thus (x in X & [ x,y ] in h) by A5,FUNCT_1:1;
end;
let x;
let y;
let z;
let u;
assume that
A6: [ x,y ] in h
and
A7: [ z,u ] in h;
A8: (z in ( dom h ) & u = ( h . z )) by A7,FUNCT_1:1;
(x in ( dom h ) & y = ( h . x )) by A6,FUNCT_1:1;
hence thesis by A2,A8,FUNCT_1:def 4;
end;
