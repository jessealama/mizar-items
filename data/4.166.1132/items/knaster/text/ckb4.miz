begin
definition
let A being non empty set;
let B being non empty set;
let f being (Function of A,B);
redefine attr f is  c=-monotone
means
:Def3: (for x,y being (Element of A) holds (x c= y implies ( f . x ) c= ( f . y )));
compatibility
proof
( dom f ) = A by FUNCT_2:def 1;
hence (f is  c=-monotone implies (for x,y being (Element of A) holds (x c= y implies ( f . x ) c= ( f . y )))) by COHSP_1:def 11;
assume A1: (for x,y being (Element of A) holds (x c= y implies ( f . x ) c= ( f . y )));
let x being set,y being set;
assume ((x in ( dom f ) & y in ( dom f )) & x c= y);
hence thesis by A1;
end;
end;
