schemes NAT_1,FUNCT_1,FUNCT_2,DOMAIN_1,ORDINAL2;
registrations XBOOLE_0,SUBSET_1,FUNCT_1,ORDINAL1,RELSET_1,CARD_1,STRUCT_0,LATTICES,LATTICE3,QUANTAL1,CKB5;
constructors SETFAM_1,WELLORD1,WELLORD2,BINOP_1,DOMAIN_1,ORDINAL2,NAT_1,ABIAN,BOOLEALG,QUANTAL1,COHSP_1,RELSET_1,CKB4,CKB6,CKB7;
requirements NUMERALS,BOOLE,SUBSET;
begin
reserve X for set;
reserve f for  c=-monotone (Function of ( bool X ),( bool X ));
theorem
Th7: ( gfp (X,f) ) is_a_fixpoint_of f
proof
defpred P[ set ]
 means
$1 c= ( f . $1 );
reconsider H = { h where h is (Subset of X): P[ h ] } as (Subset-Family of X) from DOMAIN_1:sch 7;
set A = ( union H );
now
let x being set;
assume A1: x in H;
then consider h being (Subset of X) such that A2: x = h and A3: h c= ( f . h );
h c= A by A1,A2,ZFMISC_1:74;
then ( f . h ) c= ( f . A ) by CKB4:def 1;
hence x c= ( f . A ) by A2,A3,XBOOLE_1:1;
end;
then A4: A c= ( f . A ) by ZFMISC_1:76;
then ( f . A ) c= ( f . ( f . A ) ) by CKB4:def 1;
then ( f . A ) in H;
then ( f . A ) c= A by ZFMISC_1:74;
hence ( f . ( gfp (X,f) ) ) = ( gfp (X,f) ) by A4,XBOOLE_0:def 10;
end;
