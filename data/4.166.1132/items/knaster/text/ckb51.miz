constructors SETFAM_1,WELLORD1,WELLORD2,BINOP_1,DOMAIN_1,ORDINAL2,NAT_1,ABIAN,BOOLEALG,QUANTAL1,COHSP_1,RELSET_1,CKB4,CKB6,CKB7,CKB18,CKB19,CKB28,CKB29,CKB30,CKB31,CKB33;
requirements NUMERALS,BOOLE,SUBSET;
begin
reserve O for Ordinal;
definition
let L being  complete Lattice;
let f being (UnOp of L);
assume that
A1: f is  monotone;
func FixPoints f ->  strict Lattice
means
:Def11: (ex P being non empty  with_suprema  with_infima (Subset of L) st (P = { x where x is (Element of L): x is_a_fixpoint_of f } & it = ( latt P )));
existence
proof
defpred P[ set ]
 means
$1 is_a_fixpoint_of f;
set P = { x where x is (Element of L): P[ x ] };
A2: P is (Subset of L) from DOMAIN_1:sch 7;
consider a being (Element of L) such that A3: a is_a_fixpoint_of f by A1,CKB35:1;
A4: a in P by A3;
reconsider P as (Subset of L) by A2;
A5: P is  with_suprema
proof
let x being (Element of L),y being (Element of L);
assume that
A6: x in P
and
A7: y in P;
consider a being (Element of L) such that A8: x = a and A9: a is_a_fixpoint_of f by A6;
consider b being (Element of L) such that A10: y = b and A11: b is_a_fixpoint_of f by A7;
reconsider a,b as (Element of L);
reconsider ab = ( a "\/" b ) as (Element of L);
consider O such that ( card O ) c= ( card (the carrier of L) ) and A12: ( (f,O) +. ab ) is_a_fixpoint_of f and A13: (a [= ( (f,O) +. ab ) & b [= ( (f,O) +. ab )) by A1,A9,A11,CKB47:1;
set z = ( (f,O) +. ab );
take z;
thus z in P by A12;
thus (x [= z & y [= z) by A8,A10,A13;
let z9 being (Element of L);
assume that
A14: z9 in P
and
A15: (x [= z9 & y [= z9);
A16: (ex zz being (Element of L) st (zz = z9 & zz is_a_fixpoint_of f)) by A14;
ab [= z9 by A8,A10,A15,FILTER_0:6;
hence z [= z9 by A1,A16,CKB49:1;
end;
P is  with_infima
proof
let x being (Element of L),y being (Element of L);
assume that
A17: x in P
and
A18: y in P;
consider a being (Element of L) such that A19: x = a and A20: a is_a_fixpoint_of f by A17;
consider b being (Element of L) such that A21: y = b and A22: b is_a_fixpoint_of f by A18;
reconsider a,b as (Element of L);
reconsider ab = ( a "/\" b ) as (Element of L);
consider O such that ( card O ) c= ( card (the carrier of L) ) and A23: ( (f,O) -. ab ) is_a_fixpoint_of f and A24: (( (f,O) -. ab ) [= a & ( (f,O) -. ab ) [= b) by A1,A20,A22,CKB48:1;
set z = ( (f,O) -. ab );
take z;
thus z in P by A23;
thus (z [= x & z [= y) by A19,A21,A24;
let z9 being (Element of L);
assume that
A25: z9 in P
and
A26: (z9 [= x & z9 [= y);
A27: (ex zz being (Element of L) st (zz = z9 & zz is_a_fixpoint_of f)) by A25;
z9 [= ab by A19,A21,A26,FILTER_0:7;
hence z9 [= z by A1,A27,CKB50:1;
end;
then reconsider P as non empty  with_suprema  with_infima (Subset of L) by A4,A5;
take ( latt P );
take P;
thus P = { x where x is (Element of L): x is_a_fixpoint_of f };
thus thesis;
end;
uniqueness;
end;
