requirements REAL,NUMERALS,BOOLE,SUBSET,ARITHM;
begin
reserve K for Field;
theorem
Th12: (for x1,x2,y1,y2 being (FinSequence of K) holds ((( len x1 ) = ( len x2 ) & ( len y1 ) = ( len y2 )) implies |( ( x1 ^ y1 ),( x2 ^ y2 ) )| = ( |( x1,x2 )| + |( y1,y2 )| )))
proof
let x1 being (FinSequence of K);
let x2 being (FinSequence of K);
let y1 being (FinSequence of K);
let y2 being (FinSequence of K);
A1: ( Sum ( ( mlt (x1,x2) ) ^ ( mlt (y1,y2) ) ) ) = ( ( Sum ( mlt (x1,x2) ) ) + ( Sum ( mlt (y1,y2) ) ) ) by RLVECT_1:41;
assume (( len x1 ) = ( len x2 ) & ( len y1 ) = ( len y2 ));
then ( Sum ( mlt (( x1 ^ y1 ),( x2 ^ y2 )) ) ) = ( ( Sum ( mlt (x1,x2) ) ) + ( Sum ( mlt (y1,y2) ) ) ) by A1,CKB9:1;
then |( ( x1 ^ y1 ),( x2 ^ y2 ) )| = ( ( Sum ( mlt (x1,x2) ) ) + ( Sum ( mlt (y1,y2) ) ) ) by FVSUM_1:def 9;
then |( ( x1 ^ y1 ),( x2 ^ y2 ) )| = ( ( Sum ( mlt (x1,x2) ) ) + |( y1,y2 )| ) by FVSUM_1:def 9;
hence thesis by FVSUM_1:def 9;
end;
