begin
reserve s for  real number;
reserve r3 for Real;
theorem
Lm6: (for X being (Subset of ( REAL )) holds (for s being Real holds (X is  bounded_above implies ( s ++ X ) is  bounded_above)))
proof
let X being (Subset of ( REAL ));
let p being Real;
given s such that
A1: s is (UpperBound of X);

take ( p + s );
let t being  ext-real number;
assume t in ( p ++ X );
then t in { ( p + r3 ): r3 in X } by CKB73:1;
then consider r3 such that A2: t = ( p + r3 ) and A3: r3 in X;
r3 <= s by A1,A3,XXREAL_2:def 1;
hence thesis by A2,XREAL_1:6;
end;
