begin
reserve n for Nat;
theorem
Th2: (for Lv being (Linear_Combination of ( n -VectSp_over ( F_Real ) )) holds (for Lr being (Linear_Combination of ( TOP-REAL n )) holds (Lr = Lv implies ( Carrier Lr ) = ( Carrier Lv ))))
proof
set V = ( n -VectSp_over ( F_Real ) );
set T = ( TOP-REAL n );
let Lv being (Linear_Combination of V);
let Lr being (Linear_Combination of T);
assume that
A1: Lr = Lv;
thus ( Carrier Lr ) c= ( Carrier Lv )
proof
let x being set;
assume A2: x in ( Carrier Lr );
then reconsider v = x as (Element of T);
reconsider u = v as (Element of V) by CKB1:1;
( Lv . u ) <> ( 0. ( F_Real ) ) by A1,A2,RLVECT_2:19;
hence thesis by VECTSP_6:1;
end;

let x being set;
assume x in ( Carrier Lv );
then consider u being (Element of V) such that A3: x = u and A4: ( Lv . u ) <> ( 0. ( F_Real ) ) by VECTSP_6:1;
reconsider v = u as (Element of T) by CKB1:1;
v in ( Carrier Lr ) by A1,A4,RLVECT_2:19;
hence thesis by A3;
end;
