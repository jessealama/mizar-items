constructors BINARITH,ENTROPY1,FVSUM_1,LAPLACE,MATRIX_6,MATRIX11,MATRIX13,MATRLIN2,MATRTOP1,MONOID_0,RANKNULL,REALSET1,RELSET_1,RLAFFIN1,RLVECT_3,RUSUB_5,SEQ_1,SQUARE_1,VECTSP10,MATRIX15,MEMBERED;
requirements ARITHM,NUMERALS,REAL,BOOLE,SUBSET;
begin
reserve n for Nat;
theorem
Th7: (for Af being (Subset of ( n -VectSp_over ( F_Real ) )) holds (for Ar being (Subset of ( TOP-REAL n )) holds (Af = Ar implies (Af is  linearly-independent iff Ar is  linearly-independent))))
proof
set V = ( n -VectSp_over ( F_Real ) );
let AV being (Subset of V);
set T = ( TOP-REAL n );
let AR being (Subset of T);
assume that
A1: AV = AR;
hereby
assume A2: AV is  linearly-independent;
now
let L being (Linear_Combination of AR);
reconsider L1 = L as (Linear_Combination of V) by CKB4:1;
A3: ( Carrier L1 ) = ( Carrier L ) by CKB5:1;
assume ( Sum L ) = ( 0. T );
then A4: ( 0. V ) = ( Sum L ) by CKB2:1
.= ( Sum L1 ) by CKB8:1;
( Carrier L ) c= AR by RLVECT_2:def 6;
then L1 is (Linear_Combination of AV) by A1,A3,VECTSP_6:def 4;
hence ( Carrier L ) = ( {} ) by A2,A3,A4,VECTSP_7:def 1;
end;
hence AR is  linearly-independent by RLVECT_3:def 1;
end;
assume A5: AR is  linearly-independent;
now
let L being (Linear_Combination of AV);
reconsider L1 = L as (Linear_Combination of T) by CKB4:1;
A6: ( Carrier L1 ) = ( Carrier L ) by CKB5:1;
( Carrier L ) c= AV by VECTSP_6:def 4;
then reconsider L1 as (Linear_Combination of AR) by A1,A6,RLVECT_2:def 6;
assume ( Sum L ) = ( 0. V );
then ( 0. T ) = ( Sum L ) by CKB2:1
.= ( Sum L1 ) by CKB8:1;
hence ( Carrier L ) = ( {} ) by A5,A6,RLVECT_3:def 1;
end;
hence thesis by VECTSP_7:def 1;
end;
