registrations CARD_1,EUCLID,FINSEQ_1,FINSEQ_2,FINSET_1,FUNCT_1,FUNCT_2,MATRIX13,MATRLIN2,MATRTOP1,MEMBERED,MONOID_0,NAT_1,NUMBERS,PRVECT_1,REALSET1,RELAT_1,RELSET_1,RLAFFIN1,RLVECT_3,RVSUM_1,STRUCT_0,RLVECT_2,VALUED_0,VECTSP_1,VECTSP_9,XBOOLE_0,XREAL_0,XXREAL_0,ZFMISC_1;
constructors BINARITH,ENTROPY1,FVSUM_1,LAPLACE,MATRIX_6,MATRIX11,MATRIX13,MATRLIN2,MATRTOP1,MONOID_0,RANKNULL,REALSET1,RELSET_1,RLAFFIN1,RLVECT_3,RUSUB_5,SEQ_1,SQUARE_1,VECTSP10,MATRIX15,MEMBERED;
requirements ARITHM,NUMERALS,REAL,BOOLE,SUBSET;
begin
reserve n for Nat;
theorem
Th6: (for Af being (Subset of ( n -VectSp_over ( F_Real ) )) holds (for Ar being (Subset of ( TOP-REAL n )) holds (Af = Ar implies ( [#] ( Lin Ar ) ) = ( [#] ( Lin Af ) ))))
proof
set V = ( n -VectSp_over ( F_Real ) );
set T = ( TOP-REAL n );
let Af being (Subset of V);
let Ar being (Subset of T);
assume that
A1: Af = Ar;
hereby
let x being set;
assume x in ( [#] ( Lin Ar ) );
then x in ( Lin Ar ) by STRUCT_0:def 5;
then consider L being (Linear_Combination of Ar) such that A2: x = ( Sum L ) by RLVECT_3:14;
reconsider L1 = L as (Linear_Combination of V) by CKB4:1;
(( Carrier L1 ) = ( Carrier L ) & ( Carrier L ) c= Ar) by CKB5:1,RLVECT_2:def 6;
then A3: L1 is (Linear_Combination of Af) by A1,VECTSP_6:def 4;
( Sum L1 ) = ( Sum L ) by CKB8:1;
then x in ( Lin Af ) by A2,A3,VECTSP_7:7;
hence x in ( [#] ( Lin Af ) ) by STRUCT_0:def 5;
end;
let x being set;
assume x in ( [#] ( Lin Af ) );
then x in ( Lin Af ) by STRUCT_0:def 5;
then consider L being (Linear_Combination of Af) such that A4: x = ( Sum L ) by VECTSP_7:7;
reconsider L1 = L as (Linear_Combination of T) by CKB4:1;
(( Carrier L1 ) = ( Carrier L ) & ( Carrier L ) c= Af) by CKB5:1,VECTSP_6:def 4;
then A5: L1 is (Linear_Combination of Ar) by A1,RLVECT_2:def 6;
( Sum L1 ) = ( Sum L ) by CKB8:1;
then x in ( Lin Ar ) by A4,A5,RLVECT_3:14;
hence thesis by STRUCT_0:def 5;
end;
