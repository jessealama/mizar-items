begin
scheme ExLeftOperation { E() -> set,S() ->  Group-like non empty multMagma,f((Element of S())) -> (Function of E(),E()) } : (ex T being (LeftOperation of S(),E()) st (for s being (Element of S()) holds ( T . s ) = f(s)))
provided
A1: f(( 1_ S() )) = ( id E() )
and
A2: (for s1,s2 being (Element of S()) holds f(( s1 * s2 )) = ( f(s1) * f(s2) ))
proof
set T = { [ s,f(s) ] where s is (Element of S()): (not contradiction) };
A3:now
let x being set;
assume x in T;
then consider s being (Element of S()) such that A4: x = [ s,f(s) ];
f(s) in ( Funcs (E(),E()) ) by FUNCT_2:9;
hence x in [: (the carrier of S()),( Funcs (E(),E()) ) :] by A4,ZFMISC_1:def 2;
end;
now
let x being set,y1 being set,y2 being set;
assume [ x,y1 ] in T;
then consider s1 being (Element of S()) such that A5: [ x,y1 ] = [ s1,f(s1) ];
A6: x = s1 by A5,ZFMISC_1:27;
assume [ x,y2 ] in T;
then consider s2 being (Element of S()) such that A7: [ x,y2 ] = [ s2,f(s2) ];
x = s2 by A7,ZFMISC_1:27;
hence y1 = y2 by A5,A7,A6,ZFMISC_1:27;
end;
then reconsider T as  Function-like (Relation of (the carrier of S()),( Funcs (E(),E()) )) by A3,FUNCT_1:def 1,TARSKI:def 3;
now
let x being set;
hereby
assume x in (the carrier of S());
then reconsider s = x as (Element of S());
reconsider y = f(s) as set;
take y;
thus [ x,y ] in T;
end;
given y being set such that
A8: [ x,y ] in T;

consider s being (Element of S()) such that A9: [ x,y ] = [ s,f(s) ] by A8;
x = s by A9,ZFMISC_1:27;
hence x in (the carrier of S());
end;
then (the carrier of S()) = ( dom T ) by RELAT_1:def 4;
then reconsider T as (Action of (the carrier of S()),E()) by FUNCT_2:def 1;
A10:now
let s1 being (Element of S()),s2 being (Element of S());
s1 in (the carrier of S());
then s1 in ( dom T ) by FUNCT_2:def 1;
then [ s1,( T . s1 ) ] in T by FUNCT_1:1;
then consider s19 being (Element of S()) such that A11: [ s1,( T . s1 ) ] = [ s19,f(s19) ];
A12: (s1 = s19 & f(s19) = ( T . s1 )) by A11,ZFMISC_1:27;
s2 in (the carrier of S());
then s2 in ( dom T ) by FUNCT_2:def 1;
then [ s2,( T . s2 ) ] in T by FUNCT_1:1;
then consider s29 being (Element of S()) such that A13: [ s2,( T . s2 ) ] = [ s29,f(s29) ];
( s1 * s2 ) in (the carrier of S());
then ( s1 * s2 ) in ( dom T ) by FUNCT_2:def 1;
then [ ( s1 * s2 ),( T . ( s1 * s2 ) ) ] in T by FUNCT_1:1;
then consider s129 being (Element of S()) such that A14: [ ( s1 * s2 ),( T . ( s1 * s2 ) ) ] = [ s129,f(s129) ];
A15: (s2 = s29 & f(s29) = ( T . s2 )) by A13,ZFMISC_1:27;
thus ( T ^ ( s1 * s2 ) ) = f(s129) by A14,ZFMISC_1:27
.= f(( s1 * s2 )) by A14,ZFMISC_1:27
.= ( ( T ^ s1 ) * ( T ^ s2 ) ) by A2,A12,A15;
end;
A16: ( dom T ) = (the carrier of S()) by FUNCT_2:def 1;
then [ ( 1_ S() ),( T . ( 1_ S() ) ) ] in T by FUNCT_1:1;
then consider s9 being (Element of S()) such that A17: [ ( 1_ S() ),( T . ( 1_ S() ) ) ] = [ s9,f(s9) ];
(( 1_ S() ) = s9 & ( T . ( 1_ S() ) ) = f(s9)) by A17,ZFMISC_1:27;
then reconsider T as (LeftOperation of S(),E()) by A1,A10,CKB3:def 1;
take T;
thus (for s being (Element of S()) holds ( T . s ) = f(s))
proof
let s being (Element of S());
[ s,( T . s ) ] in T by A16,FUNCT_1:1;
then consider s9 being (Element of S()) such that A18: [ s,( T . s ) ] = [ s9,f(s9) ];
s = s9 by A18,ZFMISC_1:27;
hence thesis by A18,ZFMISC_1:27;
end;

