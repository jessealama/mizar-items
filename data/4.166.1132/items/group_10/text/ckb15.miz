constructors SETFAM_1,WELLORD2,NAT_D,EQREL_1,BINARITH,WSIERP_1,REALSET2,GR_CY_1,GROUP_4,CARD_FIN,NAT_3,TOPGRP_1,SEQ_1,DYNKIN,RELSET_1,CKB2,CKB3,CKB9,CKB10,CKB14;
requirements NUMERALS,REAL,SUBSET,BOOLE,ARITHM;
begin
definition
let E being non empty set;
let n being  natural number;
let S being  Group-like non empty multMagma;
let LO being (LeftOperation of S,E);
assume A1: ( card n ) c= ( card E );
func the_extension_of_left_operation_of (n,LO) -> (LeftOperation of S,( the_subsets_of_card (n,E) ))
means
:Def5: (for s being (Element of S) holds ( it . s ) = ( the_extension_of_left_translation_of (n,s,LO) ));
existence
proof
deffunc f((Element of S)) = ( the_extension_of_left_translation_of (n,$1,LO) );
set EE = ( the_subsets_of_card (n,E) );
A2: (for s1,s2 being (Element of S) holds f(( s1 * s2 )) = ( f(s1) * f(s2) ))
proof
let s1 being (Element of S),s2 being (Element of S);
set f12 = ( the_extension_of_left_translation_of (n,( s1 * s2 ),LO) );
set f1 = ( the_extension_of_left_translation_of (n,s1,LO) );
set f2 = ( the_extension_of_left_translation_of (n,s2,LO) );
f1 in ( Funcs (EE,EE) ) by FUNCT_2:9;
then A3: (ex f being Function st ((f1 = f & ( dom f ) = EE) & ( rng f ) c= EE)) by FUNCT_2:def 2;
f2 in ( Funcs (EE,EE) ) by FUNCT_2:9;
then A4: (ex f being Function st ((f2 = f & ( dom f ) = EE) & ( rng f ) c= EE)) by FUNCT_2:def 2;
f12 in ( Funcs (EE,EE) ) by FUNCT_2:9;
then A5: (ex f being Function st ((f12 = f & ( dom f ) = EE) & ( rng f ) c= EE)) by FUNCT_2:def 2;
A6:now
let x being set;
hereby
assume A7: x in ( dom f12 );
hence x in ( dom f2 ) by A4;
( f2 . x ) in ( rng f2 ) by A4,A7,FUNCT_1:3;
hence ( f2 . x ) in ( dom f1 ) by A3;
end;
assume that
A8: x in ( dom f2 )
and
( f2 . x ) in ( dom f1 );
thus x in ( dom f12 ) by A5,A8;
end;
now
let x being set;
assume A9: x in ( dom f12 );
then reconsider X1 = x as (Element of EE);
( f2 . x ) in ( rng f2 ) by A4,A9,FUNCT_1:3;
then reconsider X2 = ( f2 . x ) as (Element of EE);
thus ( f12 . x ) = ( ( LO ^ ( s1 * s2 ) ) .: X1 ) by A1,CKB14:def 1
.= ( ( ( LO ^ s1 ) * ( LO ^ s2 ) ) .: X1 ) by CKB3:def 1
.= ( ( LO ^ s1 ) .: ( ( LO ^ s2 ) .: X1 ) ) by RELAT_1:126
.= ( ( LO ^ s1 ) .: X2 ) by A1,CKB14:def 1
.= ( f1 . ( f2 . x ) ) by A1,CKB14:def 1;
end;
hence thesis by A6,FUNCT_1:10;
end;
A10: f(( 1_ S )) = ( id EE )
proof
set f = ( the_extension_of_left_translation_of (n,( 1_ S ),LO) );
A11:now
let x being set;
assume x in EE;
then reconsider X = x as (Element of EE);
EE is non empty by A1,CKB12:1;
then X in EE;
then consider X9 being (Subset of E) such that A12: X9 = X and ( card X9 ) = n;
( f . X ) = ( ( LO ^ ( 1_ S ) ) .: X ) by A1,CKB14:def 1;
then ( f . x ) = ( ( id E ) .: X9 ) by A12,CKB3:def 1
.= X9 by FUNCT_1:92;
hence ( f . x ) = x by A12;
end;
f in ( Funcs (EE,EE) ) by FUNCT_2:9;
then (ex f9 being Function st ((f = f9 & ( dom f9 ) = EE) & ( rng f9 ) c= EE)) by FUNCT_2:def 2;
hence thesis by A11,FUNCT_1:17;
end;
(ex T being (LeftOperation of S,EE) st (for s being (Element of S) holds ( T . s ) = f(s))) from CKB6:sch 1(A10,A2);
hence thesis;
end;
uniqueness
proof
let T1 being (LeftOperation of S,( the_subsets_of_card (n,E) )),T2 being (LeftOperation of S,( the_subsets_of_card (n,E) ));
assume that
A13: (for s being (Element of S) holds ( T1 . s ) = ( the_extension_of_left_translation_of (n,s,LO) ))
and
A14: (for s being (Element of S) holds ( T2 . s ) = ( the_extension_of_left_translation_of (n,s,LO) ));
let x being (Element of S);
thus ( T1 . x ) = ( the_extension_of_left_translation_of (n,x,LO) ) by A13
.= ( T2 . x ) by A14;
end;
end;
