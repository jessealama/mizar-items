begin
theorem
Th9: (for S being  unital non  empty multMagma holds (for E being non  empty  finite set holds (for x being (Element of E) holds (for T being (LeftOperation of S,E) holds (x is_fixed_under T implies ( the_orbit_of (x,T) ) = { x })))))
proof
let S being  unital non  empty multMagma;
let E being non  empty  finite set;
let x being (Element of E);
let T being (LeftOperation of S,E);
set X = ( the_orbit_of (x,T) );
assume A1: x is_fixed_under T;
now
assume X <> { x };
then A2: (not (for x9 being set holds (x9 in X iff x9 = x))) by TARSKI:def 1;
(ex x9 being set st x9 in X) by XBOOLE_0:def 1;
then consider x99 being set such that A3: x99 <> x and A4: x99 in X by A2;
consider y9 being (Element of E) such that A5: x99 = y9 and A6: x,y9 are_conjugated_under T by A4;
(ex s being (Element of S) st y9 = ( ( T ^ s ) . x )) by A6,CKB24:def 1;
hence contradiction by A1,A3,A5,CKB22:def 1;
end;
hence thesis;
end;
