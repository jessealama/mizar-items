let m2 being  natural number;
let p being  prime  natural number;
assume that
( ( p |^ ( 0 ) ) * n ) = ( m1 * m2 )
and
(not p divides n)
and
(not p divides m2);
( p |^ ( 0 ) ) = 1 by NEWTON:4;
hence thesis by NAT_D:6;
end;
(for k being Nat holds P[ k ]) from NAT_1:sch 2(A9,A1);
hence thesis;
end;
