environ
vocabularies CAT_1,STRUCT_0,FUNCT_1,XBOOLE_0,SUBSET_1,ZFMISC_1,TARSKI,MCART_1,RELAT_1,GRAPH_1,PARTFUN1,CAT_2,FUNCOP_1,FUNCT_3,COMMACAT;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,RELAT_1,FUNCT_1,PARTFUN1,FUNCT_2,BINOP_1,FUNCOP_1,STRUCT_0,GRAPH_1,CAT_1,CAT_2,MCART_1,DOMAIN_1,CKB1,CKB3,CKB4,CKB5,CKB7,CKB8,CKB9;
definitions TARSKI,XBOOLE_0,CAT_1,BINOP_1,FUNCOP_1,GRAPH_1,CKB1,CKB3,CKB7,CKB8,CKB9;
theorems TARSKI,ZFMISC_1,MCART_1,FUNCT_1,FUNCT_2,CAT_1,CAT_2,GRFUNC_1,RELSET_1,FUNCOP_1,CKB1,CKB2,CKB3,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11;
schemes FUNCT_2,CLASSES1;
registrations XBOOLE_0,SUBSET_1,RELSET_1,CAT_1,CAT_2,STRUCT_0;
constructors DOMAIN_1,CAT_2,FUNCOP_1,RELSET_1,CKB1,CKB3,CKB4,CKB5,CKB7,CKB8,CKB9;
requirements SUBSET,BOOLE;
begin
reserve C for Category;
reserve c for (Object of C);
reserve c1 for (Object of C);
reserve c2 for (Object of C);
reserve x for set;
definition
let C;
let c;
func 1Cat c ->  strict (Subcategory of C)
equals
( 1Cat (c,( id c )) );
coherence
proof
A1:now
let a being (Object of ( 1Cat (c,( id c )) ));
( id a ) = ( id c ) by TARSKI:def 1;
hence (for c1 holds (a = c1 implies ( id a ) = ( id c1 ))) by TARSKI:def 1;
end;
A2:now
let a being (Object of ( 1Cat (c,( id c )) )),b being (Object of ( 1Cat (c,( id c )) ));
A3: (a = c & b = c) by TARSKI:def 1;
(( id c ) in ( Hom (c,c) ) & ( Hom (a,a) ) = { ( id c ) }) by CKB11:1,CAT_1:26;
hence (for c1 holds (for c2 holds ((a = c1 & b = c2) implies ( Hom (a,b) ) c= ( Hom (c1,c2) )))) by A3,ZFMISC_1:31;
end;
set m = ( id c );
set s = ( (m,m) .--> m );
A4: ( dom s ) = { [ m,m ] } by FUNCOP_1:13;
A5: ( dom m ) = c by CAT_1:19;
A6: ( cod m ) = c by CAT_1:19;
A7: ( s . (m,m) ) = m by FUNCOP_1:71;
A8:now
let x;
assume A9: x in ( dom (the Comp of ( 1Cat (c,m) )) );
hence ( (the Comp of ( 1Cat (c,m) )) . x ) = m by A7,A4,TARSKI:def 1
.= ( m * (m qua (Morphism of C)) ) by A6,CAT_1:21
.= ( (the Comp of C) . (m,m) ) by A5,A6,CAT_1:16
.= ( (the Comp of C) . x ) by A4,A9,TARSKI:def 1;
end;
[ m,m ] in ( dom (the Comp of C) ) by A5,A6,CAT_1:15;
then ( dom (the Comp of ( 1Cat (c,m) )) ) c= ( dom (the Comp of C) ) by A4,ZFMISC_1:31;
then ((the carrier of ( 1Cat (c,( id c )) )) = { c } & (the Comp of ( 1Cat (c,( id c )) )) c= (the Comp of C)) by A8,GRFUNC_1:2;
hence thesis by A2,A1,CAT_2:def 4;
end;
end;
