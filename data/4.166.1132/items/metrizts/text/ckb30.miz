begin
reserve T for TopSpace;
reserve A for (Subset of T);
reserve F for (Subset-Family of T);
reserve G for (Subset-Family of T);
reserve C for Cardinal;
theorem
Th13: (for T holds ((for F holds ((F is  open & F is (Cover of T)) implies (ex G st ((G c= F & G is (Cover of T)) & ( card G ) c= C)))) implies (for A holds ((A is  closed & A is  discrete) implies ( card A ) c= C))))
proof
let T;
assume A1: (for F holds ((F is  open & F is (Cover of T)) implies (ex G st ((G c= F & G is (Cover of T)) & ( card G ) c= C))));
set TOP = (the topology of T);
let A;
assume that
A2: A is  closed
and
A3: A is  discrete;
( A ` ) in TOP by A2,PRE_TOPC:def 2;
then A4: { ( A ` ) } c= TOP by ZFMISC_1:31;
defpred Q[ set,set ]
 means
{ $2 } = ( $1 /\ A );
defpred P[ set,set ]
 means
( A /\ $2 ) = { $1 };
A5: (for x being set holds (x in A implies (ex y being set st (y in TOP & P[ x,y ]))))
proof
let x being set;
assume x in A;
then consider G being (Subset of T) such that A6: (G is  open & ( A /\ G ) = { x }) by A3,TEX_2:26;
take G;
thus thesis by A6,PRE_TOPC:def 2;
end;
consider p being (Function of A,TOP) such that A7: (for x being set holds (x in A implies P[ x,( p . x ) ])) from FUNCT_2:sch 1(A5);
reconsider RNG = ( rng p ),AA = { ( A ` ) } as  open (Subset-Family of T) by A4,TOPS_2:11,XBOOLE_1:1;
reconsider RngA = ( RNG \/ AA ) as  open (Subset-Family of T) by TOPS_2:13;
( [#] T ) c= ( union RngA )
proof
let x being set;
assume A8: x in ( [#] T );
per cases ;
suppose A9: x in A;

( dom p ) = A by FUNCT_2:def 1;
then ( p . x ) in ( rng p ) by A9,FUNCT_1:def 3;
then A10: ( p . x ) in RngA by XBOOLE_0:def 3;
(x in { x } & ( A /\ ( p . x ) ) = { x }) by A7,A9,TARSKI:def 1;
then x in ( p . x ) by XBOOLE_0:def 4;
hence thesis by A10,TARSKI:def 4;
end;
suppose A11: (not x in A);

( A ` ) in AA by TARSKI:def 1;
then A12: ( A ` ) in RngA by XBOOLE_0:def 3;
x in ( A ` ) by A8,A11,XBOOLE_0:def 5;
hence thesis by A12,TARSKI:def 4;
end;
end;
then RngA is (Cover of T) by SETFAM_1:def 11;
then consider G being (Subset-Family of T) such that A13: G c= RngA and A14: G is (Cover of T) and A15: ( card G ) c= C by A1;
A16: (for x being set holds (x in ( G \ AA ) implies (ex y being set st (y in A & Q[ x,y ]))))
proof
let x being set;
assume x in ( G \ AA );
then (x in G & (not x in AA)) by XBOOLE_0:def 5;
then x in RNG by A13,XBOOLE_0:def 3;
then consider y being set such that A17: (y in ( dom p ) & ( p . y ) = x) by FUNCT_1:def 3;
take y;
thus thesis by A7,A17;
end;
consider q being (Function of ( G \ AA ),A) such that A18: (for x being set holds (x in ( G \ AA ) implies Q[ x,( q . x ) ])) from FUNCT_2:sch 1(A16);
per cases ;
suppose A is  empty;

hence thesis;
end;
suppose A is non empty;

then A19: ( dom q ) = ( G \ AA ) by FUNCT_2:def 1;
A c= ( rng q )
proof
let x being set;
assume that
A20: x in A;
T is non empty by A20;
then consider U being (Subset of T) such that A21: x in U and A22: U in G by A14,A20,PCOMPS_1:3;
(not x in ( A ` )) by A20,XBOOLE_0:def 5;
then (not U in AA) by A21,TARSKI:def 1;
then U in ( G \ AA ) by A22,XBOOLE_0:def 5;
then A23: (( q . U ) in ( rng q ) & { ( q . U ) } = ( U /\ A )) by A18,A19,FUNCT_1:def 3;
x in ( A /\ U ) by A20,A21,XBOOLE_0:def 4;
hence thesis by A23,TARSKI:def 1;
end;
then A24: ( card A ) c= ( card ( G \ AA ) ) by A19,CARD_1:12;
( card ( G \ AA ) ) c= ( card G ) by CARD_1:11,XBOOLE_1:36;
then ( card A ) c= ( card G ) by A24,XBOOLE_1:1;
hence ( card A ) c= C by A15,XBOOLE_1:1;
end;
end;
