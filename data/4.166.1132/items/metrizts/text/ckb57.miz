requirements ARITHM,BOOLE,NUMERALS,REAL,SUBSET;
begin
registration
let T being  Lindelof TopSpace;
let A being  closed (Subset of T);
cluster ( T | A ) ->  Lindelof;
coherence
proof
reconsider E = { ( {} ) } as (Subset-Family of ( T | A )) by SETFAM_1:46;
defpred P[ set,set ]
 means
( $2 /\ A ) = $1;
set TOP = (the topology of T);
let FA being (Subset-Family of ( T | A ));
assume that
A1: FA is  open
and
A2: FA is (Cover of ( T | A ));
A3: (for x being set holds (x in FA implies (ex y being set st (y in TOP & P[ x,y ]))))
proof
let x being set;
assume that
A4: x in FA;
reconsider X = x as (Subset of ( T | A )) by A4;
X is  open by A1,A4,TOPS_2:def 1;
then X in (the topology of ( T | A )) by PRE_TOPC:def 2;
then consider Q being (Subset of T) such that A5: (Q in TOP & X = ( Q /\ ( [#] ( T | A ) ) )) by PRE_TOPC:def 4;
take Q;
thus thesis by A5,PRE_TOPC:def 5;
end;
consider f being (Function of FA,TOP) such that A6: (for x being set holds (x in FA implies P[ x,( f . x ) ])) from FUNCT_2:sch 1(A3);
( A ` ) in TOP by PRE_TOPC:def 2;
then (TOP is  open & { ( A ` ) } c= TOP) by ZFMISC_1:31;
then reconsider RNG = ( rng f ),AA = { ( A ` ) } as  open (Subset-Family of T) by TOPS_2:11,XBOOLE_1:1;
reconsider RA = ( RNG \/ AA ) as  open (Subset-Family of T) by TOPS_2:13;
A7: A = ( [#] ( T | A ) ) by PRE_TOPC:def 5;
A8: ( dom f ) = FA by FUNCT_2:def 1;
( [#] T ) c= ( union RA )
proof
A9: ( A \/ ( A ` ) ) = ( [#] (the carrier of T) ) by SUBSET_1:10;
let x being set;
assume that
A10: x in ( [#] T );
per cases  by A9,A10,XBOOLE_0:def 3;
suppose A11: x in A;

A = ( union FA ) by A2,A7,SETFAM_1:45;
then consider y being set such that A12: x in y and A13: y in FA by A11,TARSKI:def 4;
( f . y ) in RNG by A8,A13,FUNCT_1:def 3;
then A14: ( f . y ) in RA by XBOOLE_0:def 3;
( ( f . y ) /\ A ) = y by A6,A13;
then x in ( f . y ) by A12,XBOOLE_0:def 4;
hence thesis by A14,TARSKI:def 4;
end;
suppose A15: x in ( A ` );

( A ` ) in AA by TARSKI:def 1;
then ( A ` ) in RA by XBOOLE_0:def 3;
hence thesis by A15,TARSKI:def 4;
end;
end;
then RA is (Cover of T) by SETFAM_1:def 11;
then consider G being (Subset-Family of T) such that A16: G c= RA and A17: G is (Cover of T) and A18: G is  countable by CKB43:def 1;
set GA = ( G | A );
take GE = ( GA \ E );
thus GE c= FA
proof
let x being set;
assume A19: x in GE;
then A20: x <> ( {} ) by ZFMISC_1:56;
x in GA by A19,ZFMISC_1:56;
then consider R being (Subset of T) such that A21: R in G and A22: ( R /\ A ) = x by TOPS_2:def 3;
per cases  by A16,A21,XBOOLE_0:def 3;
suppose R in RNG;

then consider y being set such that A23: y in ( dom f ) and A24: ( f . y ) = R by FUNCT_1:def 3;
y = ( R /\ A ) by A6,A23,A24;
hence thesis by A22,A23;
end;
suppose R in AA;

then R = ( A ` ) by TARSKI:def 1;
then x = ( A \ A ) by A22,SUBSET_1:13;
hence thesis by A20,XBOOLE_1:37;
end;
end;

( union G ) = ( [#] T ) by A17,SETFAM_1:45;
then ( [#] ( T | A ) ) = ( union GA ) by A7,TOPS_2:33
.= ( union GE ) by PARTIT1:2;
hence GE is (Cover of ( T | A )) by SETFAM_1:def 11;
A25: ( card GA ) c= ( card G ) by CKB18:1;
( card G ) c= ( omega ) by A18,CARD_3:def 14;
then ( card GA ) c= ( omega ) by A25,XBOOLE_1:1;
then GA is  countable by CARD_3:def 14;
hence thesis by CARD_3:95;
end;
end;
