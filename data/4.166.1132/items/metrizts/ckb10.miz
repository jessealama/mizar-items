proof
let T being TopSpace;
assume that
A6: T is  metrizable;
per cases ;
suppose A7: T is  empty;

then (for F1,F2 being (Subset of T) holds (((F1 is  closed & F2 is  closed) & F1 misses F2) implies (ex G1,G2 being (Subset of T) st ((((G1 is  open & G2 is  open) & F1 c= G1) & F2 c= G2) & G1 misses G2))));
then T is  normal by PRE_TOPC:def 12;
hence thesis by A7;
end;
suppose T is non empty;

then reconsider t = T as  metrizable non empty TopSpace by A6;
(t is  regular & (ex Bn being (FamilySequence of t) st Bn is  Basis_sigma_locally_finite)) by NAGATA_2:19;
then (t is  T_1 & T is  normal) by NAGATA_1:20,NAGATA_2:19;
hence thesis;
end;
