begin
reserve n for Nat;
reserve K for Field;
theorem
(for A,B being (Matrix of n,K) holds (( 0 ) < n implies ( Det ( A * B ) ) = ( ( Det A ) * ( Det B ) )))
proof
let A being (Matrix of n,K),B being (Matrix of n,K);
assume that
A1: ( 0 ) < n;
set P = ( Permutations n );
set KK = (the carrier of K);
set mm = (the multF of K);
set aa = (the addF of K);
set AB = ( A * B );
consider SUM1 being (Function of P,KK) such that A2: ( Det AB ) = ( aa $$ (( FinOmega P ),SUM1) ) and A3: (for perm being (Element of ( Permutations n )) holds ( SUM1 . perm ) = ( ( mm $$ ( Path_matrix (perm,A) ) ) * ( - (( Det B ),perm) ) )) by A1,CKB85:1;
set Path = ( Path_product A );
set F = ( FinOmega P );
A4: F = P by MATRIX_2:26,MATRIX_2:def 14;
then consider Ga being (Function of ( Fin P ),KK) such that A5: ( Det A ) = ( Ga . F ) and A6: (for e being (Element of KK) holds (e is_a_unity_wrt aa implies ( Ga . ( {} ) ) = e)) and A7: (for x being (Element of P) holds ( Ga . { x } ) = ( Path . x )) and A8: (for B9 being (Element of ( Fin P )) holds ((B9 c= F & B9 <> ( {} )) implies (for x being (Element of P) holds (x in ( F \ B9 ) implies ( Ga . ( B9 \/ { x } ) ) = ( aa . (( Ga . B9 ),( Path . x )) ))))) by SETWISEO:def 3;
A9: ( Ga . ( {} ) ) = ( 0. K ) by A6,FVSUM_1:6;
consider Gs being (Function of ( Fin P ),KK) such that A10: ( Det AB ) = ( Gs . F ) and A11: (for e being (Element of KK) holds (e is_a_unity_wrt aa implies ( Gs . ( {} ) ) = e)) and A12: (for x being (Element of P) holds ( Gs . { x } ) = ( SUM1 . x )) and A13: (for B9 being (Element of ( Fin P )) holds ((B9 c= F & B9 <> ( {} )) implies (for x being (Element of P) holds (x in ( F \ B9 ) implies ( Gs . ( B9 \/ { x } ) ) = ( aa . (( Gs . B9 ),( SUM1 . x )) ))))) by A2,A4,SETWISEO:def 3;
defpred S[ set ]
 means
(for B9 being (Element of ( Fin P )) holds (B9 = $1 implies ( Gs . B9 ) = ( ( Ga . B9 ) * ( Det B ) )));
A14: (for B9 being (Element of ( Fin P )) holds (for b being (Element of P) holds ((S[ B9 ] & (not b in B9)) implies S[ ( B9 \/ { b } ) ])))
proof
let B9 being (Element of ( Fin P ));
let b being (Element of P);
assume that
A15: S[ B9 ]
and
A16: (not b in B9);
set mA = ( mm $$ ( Path_matrix (b,A) ) );
let Bb being (Element of ( Fin P ));
assume that
A17: Bb = ( B9 \/ { b } );
A18:now
per cases ;
suppose A19: b is  even;

then A20: ( - (mA,b) ) = mA by MATRIX_2:def 13;
( - (( Det B ),b) ) = ( Det B ) by A19,MATRIX_2:def 13;
hence ( SUM1 . b ) = ( ( - (mA,b) ) * ( Det B ) ) by A3,A20
.= ( ( Path . b ) * ( Det B ) ) by MATRIX_3:def 8;
end;
suppose A21: b is  odd;

then A22: ( - (mA,b) ) = ( - mA ) by MATRIX_2:def 13;
( - (( Det B ),b) ) = ( - ( Det B ) ) by A21,MATRIX_2:def 13;
then ( - ( ( - (mA,b) ) * ( Det B ) ) ) = ( ( - mA ) * ( - (( Det B ),b) ) ) by A22,VECTSP_1:9
.= ( - ( mA * ( - (( Det B ),b) ) ) ) by VECTSP_1:9;
then ( ( mA * ( - (( Det B ),b) ) ) - ( ( - (mA,b) ) * ( Det B ) ) ) = ( 0. K ) by VECTSP_1:16;
then A23: ( ( - (mA,b) ) * ( Det B ) ) = ( mA * ( - (( Det B ),b) ) ) by VECTSP_1:19;
( - (mA,b) ) = ( Path . b ) by MATRIX_3:def 8;
hence ( SUM1 . b ) = ( ( Path . b ) * ( Det B ) ) by A3,A23;
end;
end;
per cases ;
suppose A24: B9 = ( {} );

then ( Ga . Bb ) = ( Path . b ) by A7,A17;
hence thesis by A12,A17,A18,A24;
end;
suppose A25: B9 <> ( {} );

A26: B9 c= P by FINSUB_1:def 5;
A27: b in ( F \ B9 ) by A4,A16,XBOOLE_0:def 5;
then ( Gs . Bb ) = ( aa . (( Gs . B9 ),( SUM1 . b )) ) by A4,A13,A17,A25,A26;
then A28: ( Gs . Bb ) = ( ( ( Ga . B9 ) * ( Det B ) ) + ( ( Path . b ) * ( Det B ) ) ) by A15,A18;
( Ga . Bb ) = ( ( Ga . B9 ) + ( Path . b ) ) by A4,A8,A17,A25,A27,A26;
hence thesis by A28,VECTSP_1:def 7;
end;
end;
( Gs . ( {} ) ) = ( 0. K ) by A11,FVSUM_1:6;
then A29: S[ ( {}. P ) ] by A9,VECTSP_1:7;
(for B being (Element of ( Fin P )) holds S[ B ]) from SETWISEO:sch 2(A29,A14);
hence thesis by A10,A5;
end;
