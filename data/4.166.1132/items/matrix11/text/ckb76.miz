begin
reserve k for Nat;
reserve n for Nat;
reserve K for Field;
reserve p2 for (Element of ( Permutations ( n + 2 ) ));
registration
let K being  Abelian non  empty addLoopStr;
cluster ( addFinS K ) ->  commutative;
coherence
proof
set KK = (the carrier of K);
let p1 being (Element of ( KK * ));
let p2 being (Element of ( KK * ));
A1: ( rng p2 ) c= KK by FINSEQ_1:def 4;
A2: ( dom ( p1 + p2 ) ) = ( ( dom p1 ) /\ ( dom p2 ) ) by CKB75:1;
A3: ( dom ( p2 + p1 ) ) = ( ( dom p2 ) /\ ( dom p1 ) ) by CKB75:1;
A4: ( rng p1 ) c= KK by FINSEQ_1:def 4;
now
let k being Nat;
assume that
A5: k in ( dom ( p1 + p2 ) );
k in ( dom p2 ) by A2,A5,XBOOLE_0:def 4;
then A6: ( p2 . k ) in ( rng p2 ) by FUNCT_1:def 3;
k in ( dom p1 ) by A2,A5,XBOOLE_0:def 4;
then ( p1 . k ) in ( rng p1 ) by FUNCT_1:def 3;
then reconsider p1k = ( p1 . k ),p2k = ( p2 . k ) as (Element of K) by A4,A1,A6;
( ( p1 + p2 ) . k ) = ( p1k + p2k ) by A5,FVSUM_1:17;
hence ( ( p1 + p2 ) . k ) = ( ( p2 + p1 ) . k ) by A2,A3,A5,FVSUM_1:17;
end;
then A7: ( p1 + p2 ) = ( p2 + p1 ) by A3,CKB75:1,FINSEQ_1:13;
( ( addFinS K ) . (p1,p2) ) = ( p1 + p2 ) by CKB74:def 1;
hence thesis by A7,CKB74:def 1;
end;
end;
