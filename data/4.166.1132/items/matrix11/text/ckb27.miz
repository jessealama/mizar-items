constructors RELAT_2,WELLORD2,SETWISEO,XXREAL_0,NAT_1,NAT_D,FINSOP_1,SETWOP_2,FINSEQ_7,CAT_2,ALGSTR_1,GROUP_4,FVSUM_1,SGRAPH1,MATRIX_6,MATRIX_7,RELSET_1,CKB9,CKB18;
requirements ARITHM,NUMERALS,REAL,BOOLE,SUBSET;
begin
reserve x for set;
reserve X for set;
theorem
Th18: (for X holds (for x holds ((not x in X) implies (for p1 being (Permutation of ( X \/ { x } )) holds (( p1 . x ) = x implies (ex p being (Permutation of X) st ( p1 | X ) = p))))))
proof
let X;
let x;
assume that
A1: (not x in X);
let p1 being (Permutation of ( X \/ { x } ));
assume that
A2: ( p1 . x ) = x;
A3: X c= ( X \/ { x } ) by XBOOLE_1:7;
set pX = ( p1 | X );
A4: ( dom p1 ) = ( X \/ { x } ) by FUNCT_2:52;
then A5: ( dom pX ) = X by RELAT_1:62,XBOOLE_1:7;
A6: ( rng p1 ) = ( X \/ { x } ) by FUNCT_2:def 3;
then A7: ( rng pX ) c= ( X \/ { x } ) by RELAT_1:70;
A8: ( rng pX ) c= X
proof
let y being set;
assume that
A9: y in ( rng pX );
consider x9 being set such that A10: x9 in ( dom pX ) and A11: ( pX . x9 ) = y by A9,FUNCT_1:def 3;
assume A12: (not y in X);
y in ( rng pX ) by A10,A11,FUNCT_1:def 3;
then y in { x } by A7,A12,XBOOLE_0:def 3;
then A13: y = x by TARSKI:def 1;
( pX . x9 ) = ( p1 . x9 ) by A10,FUNCT_1:47;
hence thesis by A1,A2,A3,A4,A5,A7,A9,A10,A11,A13,FUNCT_1:def 4;
end;
X c= ( rng pX )
proof
let y being set;
assume that
A14: y in X;
consider x9 being set such that A15: x9 in ( dom p1 ) and A16: ( p1 . x9 ) = y by A3,A6,A14,FUNCT_1:def 3;
A17: x9 in X
proof
assume (not x9 in X);
then x9 in { x } by A4,A15,XBOOLE_0:def 3;
hence thesis by A1,A2,A14,A16,TARSKI:def 1;
end;
then ( pX . x9 ) = ( p1 . x9 ) by A5,FUNCT_1:47;
hence thesis by A5,A16,A17,FUNCT_1:def 3;
end;
then A18: ( rng pX ) = X by A8,XBOOLE_0:def 10;
A19: pX is  one-to-one by FUNCT_1:52;
reconsider pX as (Function of X,X) by A5,A18,FUNCT_2:1;
pX is  onto by A18,FUNCT_2:def 3;
hence thesis by A19;
end;
