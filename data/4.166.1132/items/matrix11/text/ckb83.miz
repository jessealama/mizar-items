begin
reserve x for set;
reserve y for set;
reserve i for Nat;
reserve j for Nat;
reserve k for Nat;
reserve n for Nat;
reserve m for Nat;
reserve D for non  empty set;
reserve K for Field;
reserve P for (Permutation of ( Seg n ));
reserve F for (Function of ( Seg n ),( Seg n ));
reserve M for (Matrix of n,m,D);
reserve A for (Matrix of n,K);
theorem
Lm10: (for A,B being (Matrix of n,n,K) holds (for i holds ((i <= n & ( 0 ) < n) implies (ex P being (Function of ( Funcs (( Seg i ),( Seg n )) ),(the carrier of K)) st (for F being (Function of ( Seg i ),( Seg n )) holds (for M being (Matrix of n,n,K) holds ((M = ( ( A * B ) +* ( ( B * ( ( idseq n ) +* F ) ) | ( Seg i ) ) ) & (for j holds ((j in ( Seg i ) implies ( M . j ) = ( B . ( F . j ) )) & ((not j in ( Seg i )) implies ( M . j ) = ( ( A * B ) . j ))))) implies (ex Path being (FinSequence of K) st ((( len Path ) = i & (for Fj,j being Nat holds ((j in ( Seg i ) & Fj = ( F . j )) implies ( Path . j ) = ( A * (j,Fj) )))) & ( P . F ) = ( ( (the multF of K) $$ Path ) * ( Det M ) ))))))))))
proof
let A being (Matrix of n,n,K);
let B being (Matrix of n,n,K);
let i;
assume that
A1: i <= n
and
A2: ( 0 ) < n;
set KK = (the carrier of K);
set I = ( idseq n );
set Sn = ( Seg n );
set Si = ( Seg i );
set mm = (the multF of K);
set FF = ( Funcs (Si,Sn) );
reconsider Sn as non  empty set by A2;
set AB = ( A * B );
reconsider AB as (Matrix of n,K);
defpred PP[ set,set ]
 means
(for F being (Function of ( Seg i ),( Seg n )) holds (F = $1 implies (for M being (Matrix of n,n,K) holds ((M = ( ( A * B ) +* ( ( B * ( I +* F ) ) | Si ) ) & (for j holds ((j in Si implies ( M . j ) = ( B . ( F . j ) )) & ((not j in Si) implies ( M . j ) = ( ( A * B ) . j ))))) implies (ex Path being (FinSequence of K) st ((( len Path ) = i & (for Fj,j being Nat holds ((j in Si & Fj = ( F . j )) implies ( Path . j ) = ( A * (j,Fj) )))) & $2 = ( ( mm $$ Path ) * ( Det M ) )))))));
(ex f being Function st (( dom f ) = Si & ( rng f ) c= Sn)) by FUNCT_1:8;
then reconsider FF as non  empty set;
A3: (for x being (Element of FF) holds (ex y being (Element of KK) st PP[ x,y ]))
proof
let x being (Element of FF);
reconsider F = x as (Function of Si,Sn) by FUNCT_2:66;
defpred Path[ set,set ]
 means
(for Fj,j being Nat holds ((j = $1 & Fj = ( F . j )) implies $2 = ( A * (j,Fj) )));
A4: i is (Element of ( NAT )) by ORDINAL1:def 12;
A5: (for x9 being set holds (x9 in Si implies (ex y st (y in KK & Path[ x9,y ]))))
proof
let x9 being set;
assume that
A6: x9 in Si;
reconsider i = x9 as Nat by A6;
A7: ( rng F ) c= ( Seg n ) by RELAT_1:def 19;
Si = ( dom F ) by FUNCT_2:def 1;
then ( F . i ) in ( rng F ) by A6,FUNCT_1:def 3;
then ( F . i ) in Sn by A7;
then reconsider Fi = ( F . i ) as Nat;
take ( A * (i,Fi) );
thus thesis;
end;
consider path being (Function of Si,KK) such that A8: (for x holds (x in Si implies Path[ x,( path . x ) ])) from FUNCT_2:sch 1(A5);
( dom path ) = Si by FUNCT_2:def 1;
then reconsider p = path as FinSequence by FINSEQ_1:def 2;
( rng path ) c= KK by RELAT_1:def 19;
then reconsider p as (FinSequence of K) by FINSEQ_1:def 4;
consider M being (Matrix of n,K) such that M = ( AB +* ( ( B * ( I +* F ) ) | Si ) ) and A9: (for j holds ((j in Si implies ( M . j ) = ( B . ( F . j ) )) & ((not j in Si) implies ( M . j ) = ( AB . j )))) by A1,A2,CKB82:1;
take ( ( mm $$ p ) * ( Det M ) );
let F9 being (Function of Si,( Seg n ));
assume that
A10: F9 = x;
let M9 being (Matrix of n,n,K);
assume that
M9 = ( ( A * B ) +* ( ( B * ( I +* F9 ) ) | Si ) )
and
A11: (for j holds ((j in ( Seg i ) implies ( M9 . j ) = ( B . ( F9 . j ) )) & ((not j in ( Seg i )) implies ( M9 . j ) = ( ( A * B ) . j ))));
take p;
( dom path ) = Si by FUNCT_2:def 1;
hence (( len p ) = i & (for Fj,j being Nat holds ((j in Si & Fj = ( F9 . j )) implies ( p . j ) = ( A * (j,Fj) )))) by A8,A10,A4,FINSEQ_1:def 3;
A12: ( len M9 ) = n by MATRIX_1:24;
A13:now
let k being Nat;
assume that
1 <= k
and
k <= ( len M );
per cases ;
suppose A14: k in Si;

then ( M . k ) = ( B . ( F . k ) ) by A9;
hence ( M . k ) = ( M9 . k ) by A10,A11,A14;
end;
suppose A15: (not k in Si);

then ( M . k ) = ( AB . k ) by A9;
hence ( M . k ) = ( M9 . k ) by A11,A15;
end;
end;
( len M ) = n by MATRIX_1:24;
hence thesis by A12,A13,FINSEQ_1:14;
end;
consider P being (Function of FF,KK) such that A16: (for x being (Element of FF) holds PP[ x,( P . x ) ]) from FUNCT_2:sch 3(A3);
reconsider P as (Function of ( Funcs (Si,( Seg n )) ),KK);
take P;
let F being (Function of Si,( Seg n ));
reconsider F9 = F as (Function of Si,Sn);
let M being (Matrix of n,K);
assume that
A17: M = ( ( A * B ) +* ( ( B * ( I +* F ) ) | Si ) )
and
A18: (for j holds ((j in Si implies ( M . j ) = ( B . ( F . j ) )) & ((not j in Si) implies ( M . j ) = ( ( A * B ) . j ))));
F9 in FF by FUNCT_2:8;
hence thesis by A16,A17,A18;
end;
