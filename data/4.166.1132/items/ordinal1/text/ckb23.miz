environ
vocabularies TARSKI,XBOOLE_0,SUBSET_1,FUNCT_1,RELAT_1,ZFMISC_1,ORDINAL1;
notations TARSKI,XBOOLE_0,ZFMISC_1,ENUMSET1,SUBSET_1,RELAT_1,FUNCT_1,CKB6,CKB12,CKB13,CKB15,CKB18,CKB20;
definitions TARSKI,XBOOLE_0,RELAT_1,FUNCT_1,CKB6,CKB12,CKB13,CKB15;
theorems TARSKI,XBOOLE_0,FUNCT_1,ZFMISC_1,ENUMSET1,RELAT_1,XBOOLE_1,SUBSET_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB21,CKB22;
schemes XBOOLE_0,FUNCT_1;
registrations XBOOLE_0,SUBSET_1,RELAT_1,FUNCT_1,CKB7,CKB16,CKB17,CKB19;
constructors TARSKI,ENUMSET1,SUBSET_1,FUNCT_1,CKB6,CKB12,CKB13,CKB15;
requirements SUBSET,BOOLE;
begin
reserve A for Ordinal;
reserve B for Ordinal;
reserve C for Ordinal;
theorem
(for A being  epsilon-transitive set holds (for B,C being Ordinal holds ((A c= B & B in C) implies A in C)))
proof
let A being  epsilon-transitive set;
let B being Ordinal;
let C being Ordinal;
assume that
A1: A c= B
and
A2: B in C;
B c= C by A2,CKB12:def 1;
then A3: A c= C by A1,XBOOLE_1:1;
A <> C by A1,A2,CKB5:1;
then A c< C by A3,XBOOLE_0:def 8;
hence thesis by CKB22:1;
end;
