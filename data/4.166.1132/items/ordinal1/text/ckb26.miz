definition
let A;
let B;
redefine pred A c= B
means
(for C holds (C in A implies C in B))
;compatibility
proof
thus (A c= B implies (for C holds (C in A implies C in B)));
assume A1: (for C holds (C in A implies C in B));
let x;
assume A2: x in A;
then reconsider C = x as Ordinal by CKB24:1;
C in B by A1,A2;
hence thesis;
end;
connectedness
proof
let A;
let B;
given C such that
A3: (C in A & (not C in B));

let D;
((A in B or A = B) or B in A) by CKB25:1;
hence thesis by A3,CKB21:1;
