begin
reserve a for set;
reserve b for set;
reserve X for set;
reserve A for Ordinal;
theorem
Th36: (for X holds ((for a holds (a in X implies a is Ordinal)) implies (ex A st X c= A)))
proof
let X;
assume A1: (for a holds (a in X implies a is Ordinal));
then A2: ( union X ) is Ordinal by CKB41:1;
then reconsider A = ( succ ( union X ) ) as Ordinal;
take A;
let a;
assume A3: a in X;
then reconsider B = a as Ordinal by A1;
(b in B implies b in ( union X )) by A3,TARSKI:def 4;
then B c= ( union X ) by TARSKI:def 3;
hence thesis by A2,CKB38:1;
end;
