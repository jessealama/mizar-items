environ
vocabularies TARSKI,XBOOLE_0,SUBSET_1,FUNCT_1,RELAT_1,ZFMISC_1,ORDINAL1;
notations TARSKI,XBOOLE_0,ZFMISC_1,ENUMSET1,SUBSET_1,RELAT_1,FUNCT_1,CKB6;
definitions TARSKI,XBOOLE_0,RELAT_1,FUNCT_1,CKB6;
theorems TARSKI,XBOOLE_0,FUNCT_1,ZFMISC_1,ENUMSET1,RELAT_1,XBOOLE_1,SUBSET_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB8;
schemes XBOOLE_0,FUNCT_1;
registrations XBOOLE_0,SUBSET_1,RELAT_1,FUNCT_1,CKB7;
constructors TARSKI,ENUMSET1,SUBSET_1,FUNCT_1,CKB6;
requirements SUBSET,BOOLE;
begin
reserve X for set;
reserve Y for set;
theorem
(( succ X ) = ( succ Y ) implies X = Y)
proof
assume that
A1: ( succ X ) = ( succ Y )
and
A2: X <> Y;
Y in ( X \/ { X } ) by A1,CKB8:1;
then A3: (Y in X or Y in { X }) by XBOOLE_0:def 3;
X in ( Y \/ { Y } ) by A1,CKB8:1;
then (X in Y or X in { Y }) by XBOOLE_0:def 3;
hence contradiction by A2,A3,TARSKI:def 1;
end;
