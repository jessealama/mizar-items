environ
vocabularies TARSKI,XBOOLE_0,SUBSET_1,FUNCT_1,RELAT_1,ZFMISC_1,ORDINAL1;
notations TARSKI,XBOOLE_0,ZFMISC_1,ENUMSET1,SUBSET_1,RELAT_1,FUNCT_1,CKB6,CKB12,CKB13,CKB15,CKB18,CKB20,CKB26;
definitions TARSKI,XBOOLE_0,RELAT_1,FUNCT_1,CKB6,CKB12,CKB13,CKB15,CKB26;
theorems TARSKI,XBOOLE_0,FUNCT_1,ZFMISC_1,ENUMSET1,RELAT_1,XBOOLE_1,SUBSET_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB30,CKB31,CKB35,CKB36,CKB37,CKB38;
schemes XBOOLE_0,FUNCT_1;
registrations XBOOLE_0,SUBSET_1,RELAT_1,FUNCT_1,CKB7,CKB16,CKB17,CKB19,CKB29,CKB32,CKB33,CKB34;
constructors TARSKI,ENUMSET1,SUBSET_1,FUNCT_1,CKB6,CKB12,CKB13,CKB15,CKB26;
requirements SUBSET,BOOLE;
begin
reserve a for set;
reserve X for set;
reserve x for set;
reserve A for Ordinal;
reserve B for Ordinal;
reserve C for Ordinal;
scheme OrdinalMin { P[Ordinal] } : (ex A st (P[ A ] & (for B holds (P[ B ] implies A c= B))))
provided
A1: (ex A st P[ A ])
proof
consider A such that A2: P[ A ] by A1;
defpred R[ set ]
 means
(ex B st ($1 = B & P[ B ]));
consider X such that A3: (x in X iff (x in ( succ A ) & R[ x ])) from XBOOLE_0:sch 1;
(for a holds (a in X implies a in ( succ A ))) by A3;
then A4: X c= ( succ A ) by TARSKI:def 3;
A in ( succ A ) by CKB8:1;
then X <> ( {} ) by A2,A3;
then consider C such that A5: C in X and A6: (for B holds (B in X implies C c= B)) by A4,CKB36:1;
C in ( succ A ) by A3,A5;
then A7: C c= ( succ A ) by CKB12:def 1;
take C;
(ex B st (C = B & P[ B ])) by A3,A5;
hence P[ C ];
let B;
assume A8: P[ B ];
assume A9: (not C c= B);
then B c< C by XBOOLE_0:def 8;
then B in C by CKB22:1;
then B in X by A3,A8,A7;
hence contradiction by A6,A9;
end;
