environ
vocabularies TARSKI,XBOOLE_0,SUBSET_1,FUNCT_1,RELAT_1,ZFMISC_1,ORDINAL1;
notations TARSKI,XBOOLE_0,ZFMISC_1,ENUMSET1,SUBSET_1,RELAT_1,FUNCT_1,CKB6,CKB12,CKB13,CKB15,CKB18,CKB20;
definitions TARSKI,XBOOLE_0,RELAT_1,FUNCT_1,CKB6,CKB12,CKB13,CKB15;
theorems TARSKI,XBOOLE_0,FUNCT_1,ZFMISC_1,ENUMSET1,RELAT_1,XBOOLE_1,SUBSET_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB21,CKB22,CKB23;
schemes XBOOLE_0,FUNCT_1;
registrations XBOOLE_0,SUBSET_1,RELAT_1,FUNCT_1,CKB7,CKB16,CKB17,CKB19;
constructors TARSKI,ENUMSET1,SUBSET_1,FUNCT_1,CKB6,CKB12,CKB13,CKB15;
requirements SUBSET,BOOLE;
begin
reserve a for set;
reserve b for set;
reserve c for set;
reserve y for set;
reserve z for set;
reserve A for Ordinal;
theorem
Th23: (a in A implies a is Ordinal)
proof
assume A1: a in A;
then A2: a c= A by CKB12:def 1;
now
let y;
assume A3: y in a;
then A4: y c= A by A2,CKB12:def 1;
assume (not y c= a);
then consider b such that A5: (b in y & (not b in a)) by TARSKI:def 3;
b in ( y \ a ) by A5,XBOOLE_0:def 5;
then consider z such that A6: z in ( y \ a ) and (not (ex c st (c in ( y \ a ) & c in z))) by TARSKI:2;
z in y by A6;
then ((z in a or z = a) or a in z) by A1,A4,CKB13:def 1;
hence contradiction by A3,A6,CKB1:1,XBOOLE_0:def 5;
end;
then A7: a is  epsilon-transitive by CKB12:def 1;
(for y holds (for z holds ((y in a & z in a) implies ((y in z or y = z) or z in y)))) by A2,CKB13:def 1;
then a is  epsilon-connected by CKB13:def 1;
hence thesis by A7;
end;
