begin
reserve a for set;
reserve X for set;
reserve x for set;
reserve y for set;
theorem
Th37: (not (ex X st (for x holds (x in X iff x is Ordinal))))
proof
given X such that
A1: (for x holds (x in X iff x is Ordinal));

A2: X is  epsilon-transitive
proof
let x;
assume x in X;
then A3: x is Ordinal by A1;
thus thesis
proof
let a;
assume a in x;
then a is Ordinal by A3,CKB24:1;
hence thesis by A1;
end;

end;
X is  epsilon-connected
proof
let x;
let y;
assume (x in X & y in X);
then (x is Ordinal & y is Ordinal) by A1;
hence thesis by CKB25:1;
end;
then X in X by A1,A2;
hence contradiction;
end;
