constructors TARSKI,ENUMSET1,SUBSET_1,FUNCT_1,CKB6,CKB12,CKB13,CKB15,CKB26,CKB46,CKB49,CKB60,CKB66;
requirements SUBSET,BOOLE;
begin
reserve X for set;
reserve Y for set;
reserve Z for set;
reserve x for set;
reserve A for Ordinal;
definition
let X;
func Lim X -> set means 
(x in it iff (x in X & (ex A st (x = A & A is  limit_ordinal))));
existence
proof
defpred P[ set ]
 means
(ex A st ($1 = A & A is  limit_ordinal));
thus (ex Y being set st (for x being set holds (x in Y iff (x in X & P[ x ])))) from XBOOLE_0:sch 1;
end;
uniqueness
proof
defpred P[ set ]
 means
($1 in X & (ex A st ($1 = A & A is  limit_ordinal)));
let Y;
let Z;
assume that
A3: (x in Y iff P[ x ])
and
A4: (x in Z iff P[ x ]);
thus Y = Z from XBOOLE_0:sch 2(A3,A4);
end;
end;
