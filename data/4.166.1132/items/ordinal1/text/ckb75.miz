environ
vocabularies TARSKI,XBOOLE_0,SUBSET_1,FUNCT_1,RELAT_1,ZFMISC_1,ORDINAL1;
notations TARSKI,XBOOLE_0,ZFMISC_1,ENUMSET1,SUBSET_1,RELAT_1,FUNCT_1,CKB6,CKB12,CKB13,CKB15,CKB18,CKB20,CKB26,CKB46,CKB49,CKB51,CKB53,CKB60,CKB66,CKB67,CKB69,CKB71;
definitions TARSKI,XBOOLE_0,RELAT_1,FUNCT_1,CKB6,CKB12,CKB13,CKB15,CKB26,CKB46,CKB49,CKB60,CKB66,CKB67,CKB69,CKB71;
theorems TARSKI,XBOOLE_0,FUNCT_1,ZFMISC_1,ENUMSET1,RELAT_1,XBOOLE_1,SUBSET_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB30,CKB31,CKB35,CKB36,CKB37,CKB38,CKB41,CKB42,CKB43,CKB44,CKB45,CKB46,CKB47,CKB48,CKB49,CKB54,CKB55,CKB57,CKB59,CKB60,CKB61,CKB65,CKB66,CKB67,CKB68,CKB69,CKB71;
schemes XBOOLE_0,FUNCT_1,CKB39,CKB40,CKB62,CKB63,CKB64;
registrations XBOOLE_0,SUBSET_1,RELAT_1,FUNCT_1,CKB7,CKB16,CKB17,CKB19,CKB29,CKB32,CKB33,CKB34,CKB50,CKB52,CKB56,CKB58,CKB70,CKB72,CKB73,CKB74;
constructors TARSKI,ENUMSET1,SUBSET_1,FUNCT_1,CKB6,CKB12,CKB13,CKB15,CKB26,CKB46,CKB49,CKB60,CKB66,CKB67,CKB69,CKB71;
requirements SUBSET,BOOLE;
begin
reserve x for set;
reserve y for set;
reserve z for set;
reserve A for Ordinal;
reserve B for Ordinal;
reserve D for Ordinal;
reserve F for Function;
scheme ALFA { D() -> non  empty set,P[set,set] } : (ex F st (( dom F ) = D() & (for d being (Element of D()) holds (ex A st ((A = ( F . d ) & P[ d,A ]) & (for B holds (P[ d,B ] implies A c= B)))))))
provided
A1: (for d being (Element of D()) holds (ex A st P[ d,A ]))
proof
defpred Q[ set,set ]
 means
(ex A st ((A = $2 & P[ $1,A ]) & (for B holds (P[ $1,B ] implies A c= B))));
A2: (for x holds (x in D() implies (ex y st Q[ x,y ])))
proof
let x;
assume x in D();
then reconsider d = x as (Element of D());
defpred Q[ Ordinal ]
 means
P[ d,$1 ];
A3: (ex A st Q[ A ]) by A1;
consider A such that A4: (Q[ A ] & (for B holds (Q[ B ] implies A c= B))) from CKB39:sch 1(A3);
reconsider y = A as set;
take y;
take A;
thus thesis by A4;
end;
A5: (for x holds (for y holds (for z holds (((x in D() & Q[ x,y ]) & Q[ x,z ]) implies y = z))))
proof
let x;
let y;
let z;
assume that
x in D();
given A1 being Ordinal such that
A6: A1 = y
and
A7: (P[ x,A1 ] & (for B holds (P[ x,B ] implies A1 c= B)));

given A2 being Ordinal such that
A8: A2 = z
and
A9: (P[ x,A2 ] & (for B holds (P[ x,B ] implies A2 c= B)));

(A1 c= A2 & A2 c= A1) by A7,A9;
hence thesis by A6,A8,XBOOLE_0:def 10;
end;
consider F such that A10: (( dom F ) = D() & (for x holds (x in D() implies Q[ x,( F . x ) ]))) from FUNCT_1:sch 2(A5,A2);
take F;
thus ( dom F ) = D() by A10;
let d being (Element of D());
thus thesis by A10;
end;
