begin
reserve X for set;
reserve x for set;
reserve y for set;
theorem
Th31: ((for x holds (x in X implies (x is Ordinal & x c= X))) implies X is  ordinal)
proof
assume A1: (for x holds (x in X implies (x is Ordinal & x c= X)));
thus X is  epsilon-transitive
proof
let x;
assume x in X;
hence thesis by A1;
end;

let x;
let y;
assume (x in X & y in X);
then (x is Ordinal & y is Ordinal) by A1;
hence thesis by CKB25:1;
end;
