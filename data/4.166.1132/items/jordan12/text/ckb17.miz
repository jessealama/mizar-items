theorem
Th13: (for f holds (for g holds (f,g are_in_general_position  implies (for k holds ( ( L~ f ) /\ ( LSeg (g,k) ) ) is  finite))))
proof
let f;
let g;
assume f,g are_in_general_position ;
then A1: ( ( L~ f ) /\ ( L~ g ) ) is  finite by CKB16:1;
let k;
( ( ( L~ f ) /\ ( L~ g ) ) /\ ( LSeg (g,k) ) ) = ( ( L~ f ) /\ ( ( L~ g ) /\ ( LSeg (g,k) ) ) ) by XBOOLE_1:16
.= ( ( L~ f ) /\ ( LSeg (g,k) ) ) by TOPREAL3:19,XBOOLE_1:28;
hence thesis by A1,FINSET_1:3;
end;
