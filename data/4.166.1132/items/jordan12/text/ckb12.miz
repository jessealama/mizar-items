begin
reserve i for (Element of ( NAT ));
theorem
Th8: (for f1,f2,g1,g2 being (FinSequence of ( TOP-REAL 2 )) holds (( f1 ^' f2 ),( g1 ^' g2 ) are_in_general_position  implies ( f1 ^' f2 ),g1 are_in_general_position ))
proof
let f1 being (FinSequence of ( TOP-REAL 2 ));
let f2 being (FinSequence of ( TOP-REAL 2 ));
let g1 being (FinSequence of ( TOP-REAL 2 ));
let g2 being (FinSequence of ( TOP-REAL 2 ));
assume that
A1: ( f1 ^' f2 ),( g1 ^' g2 ) are_in_general_position ;
A2: ( g1 ^' g2 ) is_in_general_position_wrt ( f1 ^' f2 ) by A1,CKB10:def 1;
A3:now
let i;
assume (1 <= i & i < ( len ( f1 ^' f2 ) ));
then A4: ( ( L~ ( g1 ^' g2 ) ) /\ ( LSeg (( f1 ^' f2 ),i) ) ) is  trivial by A2,CKB9:def 1;
( ( L~ g1 ) /\ ( LSeg (( f1 ^' f2 ),i) ) ) c= ( ( L~ ( g1 ^' g2 ) ) /\ ( LSeg (( f1 ^' f2 ),i) ) ) by CKB8:1,XBOOLE_1:26;
hence ( ( L~ g1 ) /\ ( LSeg (( f1 ^' f2 ),i) ) ) is  trivial by A4;
end;
A5: ( f1 ^' f2 ) is_in_general_position_wrt ( g1 ^' g2 ) by A1,CKB10:def 1;
A6:now
let i;
assume that
A7: 1 <= i
and
A8: i < ( len g1 );
( len g1 ) <= ( len ( g1 ^' g2 ) ) by TOPREAL8:7;
then i < ( len ( g1 ^' g2 ) ) by A8,XXREAL_0:2;
then ( ( L~ ( f1 ^' f2 ) ) /\ ( LSeg (( g1 ^' g2 ),i) ) ) is  trivial by A5,A7,CKB9:def 1;
hence ( ( L~ ( f1 ^' f2 ) ) /\ ( LSeg (g1,i) ) ) is  trivial by A8,TOPREAL8:28;
end;
( L~ ( g1 ^' g2 ) ) misses ( rng ( f1 ^' f2 ) ) by A2,CKB9:def 1;
then ( L~ g1 ) misses ( rng ( f1 ^' f2 ) ) by CKB8:1,XBOOLE_1:63;
then A9: g1 is_in_general_position_wrt ( f1 ^' f2 ) by A3,CKB9:def 1;
( L~ ( f1 ^' f2 ) ) misses ( rng ( g1 ^' g2 ) ) by A5,CKB9:def 1;
then ( L~ ( f1 ^' f2 ) ) misses ( rng g1 ) by TOPREAL8:10,XBOOLE_1:63;
then ( f1 ^' f2 ) is_in_general_position_wrt g1 by A6,CKB9:def 1;
hence thesis by A9,CKB10:def 1;
end;
