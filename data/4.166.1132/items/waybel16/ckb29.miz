theorem
Th23: (for L being Semilattice holds (for x being (Element of L) holds (x is  completely-irreducible implies x is  irreducible)))
proof
let L being Semilattice;
let x being (Element of L);
assume x is  completely-irreducible;
then A1: x in ( Irr L ) by CKB25:def 1;
( Irr L ) c= ( IRR L ) by CKB28:1;
hence thesis by A1,WAYBEL_6:def 4;
end;
