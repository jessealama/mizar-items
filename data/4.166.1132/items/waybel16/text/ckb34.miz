begin
theorem
Th28: (for L being  distributive LATTICE holds (for p,q,u being (Element of L) holds (((p < q & (for s being (Element of L) holds (p < s implies q <= s))) & (not u <= p)) implies (not ( u "/\" q ) <= p))))
proof
let L being  distributive LATTICE;
let p being (Element of L);
let q being (Element of L);
let u being (Element of L);
assume that
A1: p < q
and
A2: ((for s being (Element of L) holds (p < s implies q <= s)) & (not u <= p))
and
A3: ( u "/\" q ) <= p;
A4: p <= q by A1,ORDERS_2:def 6;
p = ( p "\/" ( u "/\" q ) ) by A3,YELLOW_0:24
.= ( ( p "\/" u ) "/\" ( q "\/" p ) ) by WAYBEL_1:5
.= ( ( p "\/" u ) "/\" q ) by A4,YELLOW_0:24
.= ( q "/\" ( q "\/" u ) ) by A1,A2,CKB33:1
.= q by LATTICE3:18;
hence contradiction by A1;
end;
