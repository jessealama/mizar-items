begin
reserve X for non empty TopSpace;
theorem
(for Y,X1,X2 being non empty (SubSpace of X) holds ((X1 meets Y & X2 meets Y) implies (X1,X2 are_separated  implies (( X1 meet Y ),( X2 meet Y ) are_separated  & ( Y meet X1 ),( Y meet X2 ) are_separated ))))
proof
let Y being non empty (SubSpace of X),X1 being non empty (SubSpace of X),X2 being non empty (SubSpace of X);
assume that
A1: X1 meets Y
and
A2: X2 meets Y;
reconsider A2 = (the carrier of X2) as (Subset of X) by CKB3:1;
reconsider A1 = (the carrier of X1) as (Subset of X) by CKB3:1;
reconsider C = (the carrier of Y) as (Subset of X) by CKB3:1;
assume X1,X2 are_separated ;
then A3: A1,A2 are_separated  by CKB77:def 1;
now
let D1 being (Subset of X),D2 being (Subset of X);
assume (D1 = (the carrier of ( X1 meet Y )) & D2 = (the carrier of ( X2 meet Y )));
then (( A1 /\ C ) = D1 & ( A2 /\ C ) = D2) by A1,A2,CKB36:def 1;
hence D1,D2 are_separated  by A3,CKB51:1;
end;
hence ( X1 meet Y ),( X2 meet Y ) are_separated  by CKB77:def 1;
then ( X1 meet Y ),( Y meet X2 ) are_separated  by A2,CKB37:1;
hence ( Y meet X1 ),( Y meet X2 ) are_separated  by A1,CKB37:1;
end;
