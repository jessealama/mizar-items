environ
vocabularies RELAT_1,EQREL_1,XBOOLE_0,SUBSET_1,TARSKI,SETFAM_1,ZFMISC_1,FUNCT_1,RELSET_2;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,SETFAM_1,RELAT_1,FUNCT_1,RELSET_1,FUNCT_2,EQREL_1,CKB1;
definitions XBOOLE_0,TARSKI,EQREL_1,SUBSET_1,RELAT_1;
theorems RELAT_1,TARSKI,SETFAM_1,ZFMISC_1,XBOOLE_1,SUBSET_1,RELSET_1,XBOOLE_0,FUNCT_1,FUNCT_2,MSSUBFAM,EQREL_1,SYSREL,ORDERS_1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12;
schemes CLASSES1,DOMAIN_1;
registrations XBOOLE_0,SUBSET_1,RELAT_1,PARTFUN1,RELSET_1;
constructors SETFAM_1,FUNCT_2,EQREL_1,RELSET_1,DOMAIN_1;
requirements SUBSET,BOOLE;
begin
reserve x for set;
reserve y for set;
reserve R1 for Relation;
reserve R2 for Relation;
theorem
( Im (( R1 \ R2 ),x) ) = ( ( Im (R1,x) ) \ ( Im (R2,x) ) )
proof
thus ( Im (( R1 \ R2 ),x) ) c= ( ( Im (R1,x) ) \ ( Im (R2,x) ) )
proof
let y;
assume y in ( Im (( R1 \ R2 ),x) );
then A1: [ x,y ] in ( R1 \ R2 ) by CKB10:1;
then A2: (not [ x,y ] in R2) by XBOOLE_0:def 5;
A3: y in ( Im (R1,x) ) by A1,CKB10:1;
(not y in ( Im (R2,x) )) by A2,CKB10:1;
hence thesis by A3,XBOOLE_0:def 5;
end;

let y;
assume A4: y in ( ( Im (R1,x) ) \ ( Im (R2,x) ) );
then A5: (not y in ( Im (R2,x) )) by XBOOLE_0:def 5;
A6: [ x,y ] in R1 by A4,CKB10:1;
(not [ x,y ] in R2) by A5,CKB10:1;
then [ x,y ] in ( R1 \ R2 ) by A6,XBOOLE_0:def 5;
hence thesis by CKB10:1;
end;
