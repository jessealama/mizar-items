environ
vocabularies RELAT_1,EQREL_1,XBOOLE_0,SUBSET_1,TARSKI,SETFAM_1,ZFMISC_1,FUNCT_1,RELSET_2;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,SETFAM_1,RELAT_1,FUNCT_1,RELSET_1,FUNCT_2,EQREL_1,CKB1;
definitions XBOOLE_0,TARSKI,EQREL_1,SUBSET_1,RELAT_1;
theorems RELAT_1,TARSKI,SETFAM_1,ZFMISC_1,XBOOLE_1,SUBSET_1,RELSET_1,XBOOLE_0,FUNCT_1,FUNCT_2,MSSUBFAM,EQREL_1,SYSREL,ORDERS_1,CKB2,CKB3,CKB4,CKB5;
schemes CLASSES1,DOMAIN_1;
registrations XBOOLE_0,SUBSET_1,RELAT_1,PARTFUN1,RELSET_1;
constructors SETFAM_1,FUNCT_2,EQREL_1,RELSET_1,DOMAIN_1;
requirements SUBSET,BOOLE;
begin
reserve X for set;
reserve Y for set;
theorem
{_{ ( X \ Y ) }_} = ( {_{ X }_} \ {_{ Y }_} )
proof
thus {_{ ( X \ Y ) }_} c= ( {_{ X }_} \ {_{ Y }_} )
proof
let y being set;
assume y in {_{ ( X \ Y ) }_};
then consider x being set such that A1: y = { x } and A2: x in ( X \ Y ) by CKB2:1;
A3: (not x in Y) by A2,XBOOLE_0:def 5;
A4: y in {_{ X }_} by A1,A2,CKB2:1;
(not y in {_{ Y }_})
proof
assume (not thesis);
then (ex x1 being set st (y = { x1 } & x1 in Y)) by CKB2:1;
hence contradiction by A1,A3,ZFMISC_1:3;
end;
hence thesis by A4,XBOOLE_0:def 5;
end;

let y being set;
assume A5: y in ( {_{ X }_} \ {_{ Y }_} );
then A6: y in {_{ X }_} by XBOOLE_0:def 5;
A7: (not y in {_{ Y }_}) by A5,XBOOLE_0:def 5;
consider x being set such that A8: y = { x } and A9: x in X by A6,CKB2:1;
(not x in Y) by A7,A8,CKB2:1;
then x in ( X \ Y ) by A9,XBOOLE_0:def 5;
hence thesis by A8,CKB2:1;
end;
