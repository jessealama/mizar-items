begin
reserve A for set;
reserve B for set;
theorem
(for FR being (Subset-Family of [: A,B :]) holds (for A,B being set holds (for X being (Subset of A) holds { ( R .:^ X ) where R is (Subset of [: A,B :]): R in FR } is (Subset-Family of B))))
proof
let FR being (Subset-Family of [: A,B :]);
let A being set,B being set;
let X being (Subset of A);
deffunc F((Subset of [: A,B :])) = ( $1 .:^ X );
defpred P[ set ]
 means
$1 in FR;
set G = { F(R) where R is (Subset of [: A,B :]): P[ R ] };
thus G is (Subset-Family of B) from DOMAIN_1:sch 8;
end;
