environ
vocabularies RELAT_1,EQREL_1,XBOOLE_0,SUBSET_1,TARSKI,SETFAM_1,ZFMISC_1,FUNCT_1,RELSET_2;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,SETFAM_1,RELAT_1,FUNCT_1,RELSET_1,FUNCT_2,EQREL_1,CKB1;
definitions XBOOLE_0,TARSKI,EQREL_1,SUBSET_1,RELAT_1;
theorems RELAT_1,TARSKI,SETFAM_1,ZFMISC_1,XBOOLE_1,SUBSET_1,RELSET_1,XBOOLE_0,FUNCT_1,FUNCT_2,MSSUBFAM,EQREL_1,SYSREL,ORDERS_1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9;
schemes CLASSES1,DOMAIN_1;
registrations XBOOLE_0,SUBSET_1,RELAT_1,PARTFUN1,RELSET_1;
constructors SETFAM_1,FUNCT_2,EQREL_1,RELSET_1,DOMAIN_1;
requirements SUBSET,BOOLE;
begin
reserve x for set;
reserve y for set;
reserve R for Relation;
theorem
Th9: (y in ( Im (R,x) ) iff [ x,y ] in R)
proof
thus (y in ( Im (R,x) ) implies [ x,y ] in R)
proof
assume y in ( Im (R,x) );
then (ex a being set st ([ a,y ] in R & a in { x })) by RELAT_1:def 13;
hence thesis by TARSKI:def 1;
end;

assume A1: [ x,y ] in R;
x in { x } by TARSKI:def 1;
hence thesis by A1,RELAT_1:def 13;
end;
