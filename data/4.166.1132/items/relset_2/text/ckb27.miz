environ
vocabularies RELAT_1,EQREL_1,XBOOLE_0,SUBSET_1,TARSKI,SETFAM_1,ZFMISC_1,FUNCT_1,RELSET_2;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,SETFAM_1,RELAT_1,FUNCT_1,RELSET_1,FUNCT_2,EQREL_1,CKB1,CKB15,CKB16,CKB22,CKB23;
definitions XBOOLE_0,TARSKI,EQREL_1,SUBSET_1,RELAT_1,CKB22;
theorems RELAT_1,TARSKI,SETFAM_1,ZFMISC_1,XBOOLE_1,SUBSET_1,RELSET_1,XBOOLE_0,FUNCT_1,FUNCT_2,MSSUBFAM,EQREL_1,SYSREL,ORDERS_1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB24,CKB25,CKB26;
schemes CLASSES1,DOMAIN_1;
registrations XBOOLE_0,SUBSET_1,RELAT_1,PARTFUN1,RELSET_1;
constructors SETFAM_1,FUNCT_2,EQREL_1,RELSET_1,DOMAIN_1,CKB15,CKB16,CKB22;
requirements SUBSET,BOOLE;
begin
reserve A for set;
reserve B for set;
reserve R for Relation;
definition
let B being set;
let A being set;
let R being (Subset of [: A,B :]);
redefine func .: R -> (Function of ( bool A ),( bool B ));

correctness
proof
A1: ( dom ( .: R ) ) = ( bool A ) by CKB22:def 1;
( .: R ) is (Function of ( bool A ),( bool ( rng R ) )) by CKB26:1;
then A2: ( rng ( .: R ) ) c= ( bool ( rng R ) ) by RELAT_1:def 19;
( bool ( rng R ) ) c= ( bool B ) by ZFMISC_1:67;
then ( rng ( .: R ) ) c= ( bool B ) by A2,XBOOLE_1:1;
hence thesis by A1,FUNCT_2:def 1,RELSET_1:4;
end;
end;
