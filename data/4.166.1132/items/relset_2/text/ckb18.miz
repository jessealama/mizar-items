begin
theorem
Th15: (for A being non empty set holds (for X being (Subset of A) holds X = ( union { { x } where x is (Element of A): x in X } )))
proof
let A being non empty set;
let X being (Subset of A);
thus X c= ( union { { x } where x is (Element of A): x in X } )
proof
let a being set;
assume A1: a in X;
set Y = { a };
A2: a in Y by TARSKI:def 1;
Y in { { x } where x is (Element of A): x in X } by A1;
hence thesis by A2,TARSKI:def 4;
end;

let a being set;
assume a in ( union { { x } where x is (Element of A): x in X } );
then consider Y being set such that A3: a in Y and A4: Y in { { x } where x is (Element of A): x in X } by TARSKI:def 4;
(ex x being (Element of A) st (Y = { x } & x in X)) by A4;
hence thesis by A3,TARSKI:def 1;
end;
