theorem
(for A being non empty set holds (for X being (Subset of A) holds { { x } where x is (Element of A): x in X } is (Subset-Family of A)))
proof
let A being non empty set;
let X being (Subset of A);
{ { x } where x is (Element of A): x in X } c= ( bool A )
proof
let a being set;
assume a in { { x } where x is (Element of A): x in X };
then (ex x being (Element of A) st (a = { x } & x in X));
then a c= A by ZFMISC_1:31;
hence thesis;
end;
hence thesis;
end;
