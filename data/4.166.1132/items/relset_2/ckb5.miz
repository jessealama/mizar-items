environ
vocabularies RELAT_1,EQREL_1,XBOOLE_0,SUBSET_1,TARSKI,SETFAM_1,ZFMISC_1,FUNCT_1,RELSET_2;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,SETFAM_1,RELAT_1,FUNCT_1,RELSET_1,FUNCT_2,EQREL_1,CKB1;
definitions XBOOLE_0,TARSKI,EQREL_1,SUBSET_1,RELAT_1;
theorems RELAT_1,TARSKI,SETFAM_1,ZFMISC_1,XBOOLE_1,SUBSET_1,RELSET_1,XBOOLE_0,FUNCT_1,FUNCT_2,MSSUBFAM,EQREL_1,SYSREL,ORDERS_1,CKB2,CKB3,CKB4;
schemes CLASSES1,DOMAIN_1;
registrations XBOOLE_0,SUBSET_1,RELAT_1,PARTFUN1,RELSET_1;
constructors SETFAM_1,FUNCT_2,EQREL_1,RELSET_1,DOMAIN_1;
requirements SUBSET,BOOLE;
begin
reserve X for set;
reserve Y for set;
theorem
Th4: {_{ ( X /\ Y ) }_} = ( {_{ X }_} /\ {_{ Y }_} )
proof
thus {_{ ( X /\ Y ) }_} c= ( {_{ X }_} /\ {_{ Y }_} )
proof
let y being set;
assume y in {_{ ( X /\ Y ) }_};
then consider x being set such that A1: y = { x } and A2: x in ( X /\ Y ) by CKB2:1;
A3: x in X by A2,XBOOLE_0:def 4;
A4: x in Y by A2,XBOOLE_0:def 4;
A5: y in {_{ X }_} by A1,A3,CKB2:1;
y in {_{ Y }_} by A1,A4,CKB2:1;
hence thesis by A5,XBOOLE_0:def 4;
end;

let y being set;
assume A6: y in ( {_{ X }_} /\ {_{ Y }_} );
then A7: y in {_{ X }_} by XBOOLE_0:def 4;
A8: y in {_{ Y }_} by A6,XBOOLE_0:def 4;
consider x being set such that A9: y = { x } and A10: x in X by A7,CKB2:1;
consider x1 being set such that A11: y = { x1 } and A12: x1 in Y by A8,CKB2:1;
x = x1 by A9,A11,ZFMISC_1:3;
then x in ( X /\ Y ) by A10,A12,XBOOLE_0:def 4;
hence thesis by A9,CKB2:1;
end;
