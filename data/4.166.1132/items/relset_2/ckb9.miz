begin
reserve P for Relation;
reserve Q for Relation;
reserve R for Relation;
theorem
( ( P /\ Q ) * R ) c= ( ( P * R ) /\ ( Q * R ) )
proof
let x being set,y being set;
assume [ x,y ] in ( ( P /\ Q ) * R );
then consider z being set such that A1: [ x,z ] in ( P /\ Q ) and A2: [ z,y ] in R by RELAT_1:def 8;
A3: [ x,z ] in P by A1,XBOOLE_0:def 4;
A4: [ x,z ] in Q by A1,XBOOLE_0:def 4;
A5: [ x,y ] in ( P * R ) by A2,A3,RELAT_1:def 8;
[ x,y ] in ( Q * R ) by A2,A4,RELAT_1:def 8;
hence thesis by A5,XBOOLE_0:def 4;
end;
