theorem
Th6: (for C being category holds (for o1,o2 being (object of C) holds ((<^ o1,o2 ^> <> ( {} ) & <^ o2,o1 ^> <> ( {} )) implies (for A being (Morphism of o1,o2) holds (A is  iso iff (A is  retraction & A is  coretraction))))))
proof
let C being category;
let o1 being (object of C);
let o2 being (object of C);
assume that
A1: (<^ o1,o2 ^> <> ( {} ) & <^ o2,o1 ^> <> ( {} ));
let A being (Morphism of o1,o2);
thus (A is  iso implies (A is  retraction & A is  coretraction)) by CKB11:1;
assume A2: (A is  retraction & A is  coretraction);
then ( A " ) is_right_inverse_of A by A1,CKB6:def 1;
then A3: ( A * ( A " ) ) = ( idm o2 ) by CKB1:def 1;
( A " ) is_left_inverse_of A by A1,A2,CKB6:def 1;
then ( ( A " ) * A ) = ( idm o1 ) by CKB1:def 1;
hence thesis by A3,CKB10:def 1;
end;
