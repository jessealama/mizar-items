begin
Lm1:now
let C being  with_units non  empty AltCatStr;
let a being (object of C);
thus ( idm a ) is  epi
proof
let o being (object of C);
assume that
A1: <^ a,o ^> <> ( {} );
let B being (Morphism of a,o);
let C being (Morphism of a,o);
assume that
A2: ( B * ( idm a ) ) = ( C * ( idm a ) );
thus B = ( B * ( idm a ) ) by A1,ALTCAT_1:def 17
.= C by A1,A2,ALTCAT_1:def 17;
end;

thus ( idm a ) is  mono
proof
let o being (object of C);
assume that
A3: <^ o,a ^> <> ( {} );
let B being (Morphism of o,a);
let C being (Morphism of o,a);
assume that
A4: ( ( idm a ) * B ) = ( ( idm a ) * C );
thus B = ( ( idm a ) * B ) by A3,ALTCAT_1:20
.= C by A3,A4,ALTCAT_1:20;
end;

end;
registration
let C being  with_units non  empty AltCatStr;
let o being (object of C);
cluster  mono  epi  retraction  coretraction for (Morphism of o,o);
existence
proof
take ( idm o );
thus thesis by Lm1,CKB5:1;
end;
end;
