begin
definition
let C being category;
let o1 being (object of C);
let o2 being (object of C);
pred o1,o2 are_iso 
means
:Def6: ((<^ o1,o2 ^> <> ( {} ) & <^ o2,o1 ^> <> ( {} )) & (ex A being (Morphism of o1,o2) st A is  iso))
;reflexivity
proof
let o being (object of C);
thus A1: (<^ o,o ^> <> ( {} ) & <^ o,o ^> <> ( {} )) by ALTCAT_1:19;
take ( idm o );
set A = ( idm o );
A2: ( ( A " ) * A ) = ( A * A ) by CKB9:1
.= ( idm o ) by A1,ALTCAT_1:def 17;
( A * ( A " ) ) = ( A * A ) by CKB9:1
.= ( idm o ) by A1,ALTCAT_1:def 17;
hence thesis by A2,CKB10:def 1;
end;
symmetry
proof
let o1 being (object of C);
let o2 being (object of C);
assume that
A3: (<^ o1,o2 ^> <> ( {} ) & <^ o2,o1 ^> <> ( {} ))
and
A4: (ex A being (Morphism of o1,o2) st A is  iso);
thus (<^ o2,o1 ^> <> ( {} ) & <^ o1,o2 ^> <> ( {} )) by A3;
consider A being (Morphism of o1,o2) such that A5: A is  iso by A4;
take A1 = ( A " );
A6: (A is  retraction & A is  coretraction) by A5,CKB11:1;
then A7: ( ( A1 " ) * A1 ) = ( A * ( A " ) ) by A3,CKB8:1
.= ( idm o2 ) by A3,A6,CKB7:1;
( A1 * ( A1 " ) ) = ( ( A " ) * A ) by A3,A6,CKB8:1
.= ( idm o1 ) by A3,A6,CKB7:1;
hence thesis by A7,CKB10:def 1;
end;
end;
