theorem
Th2: (for C being category holds (for o1,o2 being (object of C) holds ((<^ o1,o2 ^> <> ( {} ) & <^ o2,o1 ^> <> ( {} )) implies (for A being (Morphism of o1,o2) holds ((A is  retraction & A is  coretraction) implies (( ( A " ) * A ) = ( idm o1 ) & ( A * ( A " ) ) = ( idm o2 )))))))
proof
let C being category;
let o1 being (object of C);
let o2 being (object of C);
assume that
A1: (<^ o1,o2 ^> <> ( {} ) & <^ o2,o1 ^> <> ( {} ));
let A being (Morphism of o1,o2);
assume (A is  retraction & A is  coretraction);
then (( A " ) is_left_inverse_of A & ( A " ) is_right_inverse_of A) by A1,CKB6:def 1;
hence thesis by CKB1:def 1;
