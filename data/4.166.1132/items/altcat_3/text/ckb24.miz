begin
theorem
Th15: (for C being category holds (for o1,o2 being (object of C) holds ((<^ o1,o2 ^> <> ( {} ) & <^ o2,o1 ^> <> ( {} )) implies (for A being (Morphism of o1,o2) holds (A is  retraction implies A is  epi)))))
proof
let C being category;
let o1 being (object of C);
let o2 being (object of C);
assume A1: (<^ o1,o2 ^> <> ( {} ) & <^ o2,o1 ^> <> ( {} ));
let A being (Morphism of o1,o2);
assume A is  retraction;
then consider R being (Morphism of o2,o1) such that A2: R is_right_inverse_of A by CKB3:def 1;
let o being (object of C);
assume A3: <^ o2,o ^> <> ( {} );
let B being (Morphism of o2,o);
let C being (Morphism of o2,o);
assume A4: ( B * A ) = ( C * A );
thus B = ( B * ( idm o2 ) ) by A3,ALTCAT_1:def 17
.= ( B * ( A * R ) ) by A2,CKB1:def 1
.= ( ( C * A ) * R ) by A1,A3,A4,ALTCAT_1:21
.= ( C * ( A * R ) ) by A1,A3,ALTCAT_1:21
.= ( C * ( idm o2 ) ) by A2,CKB1:def 1
.= C by A3,ALTCAT_1:def 17;
end;
