begin
theorem
(for C being category holds (for o1,o2,o3 being (object of C) holds (for A being (Morphism of o1,o2) holds (for B being (Morphism of o2,o3) holds ((((<^ o1,o2 ^> <> ( {} ) & <^ o2,o3 ^> <> ( {} )) & <^ o3,o1 ^> <> ( {} )) & ( B * A ) is  coretraction) implies A is  coretraction)))))
proof
let C being category;
let o1 being (object of C);
let o2 being (object of C);
let o3 being (object of C);
let A being (Morphism of o1,o2);
let B being (Morphism of o2,o3);
assume A1: ((<^ o1,o2 ^> <> ( {} ) & <^ o2,o3 ^> <> ( {} )) & <^ o3,o1 ^> <> ( {} ));
assume ( B * A ) is  coretraction;
then consider G being (Morphism of o3,o1) such that A2: G is_left_inverse_of ( B * A ) by CKB4:def 1;
A3: ( ( G * B ) * A ) = ( G * ( B * A ) ) by A1,ALTCAT_1:21;
( G * ( B * A ) ) = ( idm o1 ) by A2,CKB1:def 1;
then ( G * B ) is_left_inverse_of A by A3,CKB1:def 1;
hence thesis by CKB4:def 1;
end;
