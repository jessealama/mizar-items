begin
theorem
Th1: (for C being  with_units non  empty AltCatStr holds (for o being (object of C) holds (( idm o ) is  retraction & ( idm o ) is  coretraction)))
proof
let C being  with_units non  empty AltCatStr;
let o being (object of C);
<^ o,o ^> <> ( {} ) by ALTCAT_1:19;
then ( ( idm o ) * ( idm o ) ) = ( idm o ) by ALTCAT_1:def 17;
then ( idm o ) is_left_inverse_of ( idm o ) by CKB1:def 1;
hence thesis by CKB3:def 1,CKB4:def 1;
end;
