environ
vocabularies ALTCAT_1,XBOOLE_0,CAT_1,RELAT_1,CAT_3,BINOP_1,RELAT_2,FUNCT_1,FUNCOP_1,TARSKI,FUNCT_2,SUBSET_1,SETFAM_1,ZFMISC_1,ALTCAT_3;
notations TARSKI,ZFMISC_1,XBOOLE_0,SUBSET_1,SETFAM_1,RELAT_1,FUNCT_1,FUNCT_2,FUNCOP_1,STRUCT_0,ALTCAT_1,CKB1,CKB2,CKB3,CKB4,CKB6,CKB10,CKB14,CKB16,CKB17,CKB41,CKB44,CKB47,CKB49;
definitions TARSKI,CKB1,CKB3,CKB4,CKB6,CKB10,CKB14,CKB16,CKB17,CKB41,CKB44,CKB47,CKB49;
theorems FUNCOP_1,RELAT_1,FUNCT_2,ZFMISC_1,ALTCAT_1,TARSKI,FUNCT_1,REALSET1,XBOOLE_0,XBOOLE_1,CKB1,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB41,CKB42,CKB43,CKB44,CKB45,CKB46,CKB47,CKB48,CKB49;
schemes FUNCT_1;
registrations XBOOLE_0,SETFAM_1,FUNCT_1,RELSET_1,ALTCAT_1,ZFMISC_1,CKB34,CKB35,CKB36,CKB37,CKB38,CKB39,CKB40;
constructors SETFAM_1,REALSET1,ALTCAT_1,RELSET_1,CKB1,CKB3,CKB4,CKB6,CKB10,CKB14,CKB16,CKB17,CKB41,CKB44,CKB47,CKB49;
requirements SUBSET,BOOLE;
begin
theorem
(for C being category holds (for o1,o2,o3 being (object of C) holds (for M1 being (Morphism of o1,o2) holds (for M2 being (Morphism of o2,o3) holds ((M1 is  _zero & M2 is  _zero) implies ( M2 * M1 ) is  _zero)))))
proof
let C being category;
let o1 being (object of C);
let o2 being (object of C);
let o3 being (object of C);
let M1 being (Morphism of o1,o2);
let M2 being (Morphism of o2,o3);
assume that
A1: M1 is  _zero
and
A2: M2 is  _zero;
let o being (object of C);
assume A3: o is  _zero;
then A4: o is  initial by CKB47:def 1;
then consider B1 being (Morphism of o,o2) such that A5: B1 in <^ o,o2 ^> and <^ o,o2 ^> is  trivial by CKB41:def 1;
let A being (Morphism of o1,o);
let B being (Morphism of o,o3);
consider B2 being (Morphism of o,o3) such that A6: B2 in <^ o,o3 ^> and A7: <^ o,o3 ^> is  trivial by A4,CKB41:def 1;
consider y being set such that A8: <^ o,o3 ^> = { y } by A6,A7,ZFMISC_1:131;
A9: o is  terminal by A3,CKB47:def 1;
then consider A1 being (Morphism of o1,o) such that A10: A1 in <^ o1,o ^> and A11: <^ o1,o ^> is  trivial by CKB44:def 1;
consider x being set such that A12: <^ o1,o ^> = { x } by A10,A11,ZFMISC_1:131;
(ex M being (Morphism of o,o) st (M in <^ o,o ^> & <^ o,o ^> is  trivial)) by A9,CKB44:def 1;
then consider z being set such that A13: <^ o,o ^> = { z } by ZFMISC_1:131;
consider A2 being (Morphism of o2,o) such that A14: A2 in <^ o2,o ^> and <^ o2,o ^> is  trivial by A9,CKB44:def 1;
A15: (( idm o ) = z & ( A2 * B1 ) = z) by A13,TARSKI:def 1;
A16: (B = y & B2 = y) by A8,TARSKI:def 1;
A17: (A = x & A1 = x) by A12,TARSKI:def 1;
A18: <^ o2,o3 ^> <> ( {} ) by A6,A14,ALTCAT_1:def 2;
M2 = ( B2 * A2 ) by A2,A3,CKB49:def 1;
hence ( M2 * M1 ) = ( ( B2 * A2 ) * ( B1 * A1 ) ) by A1,A3,CKB49:def 1
.= ( ( ( B2 * A2 ) * B1 ) * A1 ) by A5,A10,A18,ALTCAT_1:21
.= ( ( B * ( idm o ) ) * A ) by A5,A6,A14,A17,A16,A15,ALTCAT_1:21
.= ( B * A ) by A6,ALTCAT_1:def 17;
end;
