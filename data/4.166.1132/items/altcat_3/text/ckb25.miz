begin
theorem
Th16: (for C being category holds (for o1,o2 being (object of C) holds ((<^ o1,o2 ^> <> ( {} ) & <^ o2,o1 ^> <> ( {} )) implies (for A being (Morphism of o1,o2) holds (A is  coretraction implies A is  mono)))))
proof
let C being category;
let o1 being (object of C);
let o2 being (object of C);
assume A1: (<^ o1,o2 ^> <> ( {} ) & <^ o2,o1 ^> <> ( {} ));
let A being (Morphism of o1,o2);
assume A is  coretraction;
then consider R being (Morphism of o2,o1) such that A2: R is_left_inverse_of A by CKB4:def 1;
let o being (object of C);
assume A3: <^ o,o1 ^> <> ( {} );
let B being (Morphism of o,o1);
let C being (Morphism of o,o1);
assume A4: ( A * B ) = ( A * C );
thus B = ( ( idm o1 ) * B ) by A3,ALTCAT_1:20
.= ( ( R * A ) * B ) by A2,CKB1:def 1
.= ( R * ( A * C ) ) by A1,A3,A4,ALTCAT_1:21
.= ( ( R * A ) * C ) by A1,A3,ALTCAT_1:21
.= ( ( idm o1 ) * C ) by A2,CKB1:def 1
.= C by A3,ALTCAT_1:20;
end;
