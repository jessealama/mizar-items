requirements BOOLE,SUBSET,NUMERALS;
begin
reserve l for  natural Ordinal;
reserve i for  natural Ordinal;
reserve j for  natural Ordinal;
reserve k for  natural Ordinal;
theorem
Th51: ((j <> ( {} ) & l <> ( {} )) implies (( i / j ) = ( k / l ) iff ( i *^ l ) = ( j *^ k )))
proof
assume that
A1: j <> ( {} )
and
A2: l <> ( {} );
set x = ( i / j );
set y = ( k / l );
set ny = ( numerator y );
set dy = ( denominator y );
A3: ny = ( RED (k,l) ) by A2,CKB63:1;
set nx = ( numerator x );
set dx = ( denominator x );
A4: dx = ( RED (j,i) ) by A1,CKB63:1;
A5: dy = ( RED (l,k) ) by A2,CKB63:1;
A6: nx = ( RED (i,j) ) by A1,CKB63:1;
hereby
assume ( i / j ) = ( k / l );
then (i = ( ny *^ ( i hcf j ) ) & l = ( dx *^ ( l hcf k ) )) by A6,A5,CKB32:1;
hence ( i *^ l ) = ( ( ( ny *^ ( i hcf j ) ) *^ dx ) *^ ( l hcf k ) ) by ORDINAL3:50
.= ( ( ny *^ ( ( i hcf j ) *^ dx ) ) *^ ( l hcf k ) ) by ORDINAL3:50
.= ( ( ny *^ j ) *^ ( l hcf k ) ) by A4,CKB32:1
.= ( j *^ ( ny *^ ( l hcf k ) ) ) by ORDINAL3:50
.= ( j *^ k ) by A3,CKB32:1;
end;
assume A7: ( i *^ l ) = ( j *^ k );
then dx = ( RED (( j *^ l ),( j *^ k )) ) by A2,A4,CKB39:1;
then A8: dx = dy by A1,A5,CKB39:1;
nx = ( RED (( j *^ k ),( j *^ l )) ) by A2,A6,A7,CKB39:1;
then nx = ny by A1,A3,CKB39:1;
then x = ( ny / dy ) by A8,CKB60:1;
hence thesis by CKB60:1;
end;
