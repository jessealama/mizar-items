environ
vocabularies ORDINAL1,XBOOLE_0,ORDINAL2,TARSKI,SUBSET_1,ORDINAL3,ARYTM_3;
notations TARSKI,XBOOLE_0,SUBSET_1,ORDINAL1,ORDINAL2,ORDINAL3,CKB3,CKB4,CKB11;
definitions TARSKI,ORDINAL1,CKB3,CKB4,CKB11;
theorems TARSKI,XBOOLE_0,ZFMISC_1,ORDINAL1,ORDINAL2,ORDINAL3,XBOOLE_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17;
schemes ORDINAL1;
registrations XBOOLE_0,SUBSET_1,ORDINAL1,ORDINAL2,ORDINAL3,CKB9,CKB10;
constructors SUBSET_1,ORDINAL3,CKB3,CKB4,CKB11;
requirements BOOLE,SUBSET,NUMERALS;
begin
reserve a for  natural Ordinal;
reserve b for  natural Ordinal;
reserve l for  natural Ordinal;
reserve m for  natural Ordinal;
reserve n for  natural Ordinal;
theorem
Th16: (for n holds (for m holds (for l holds ((n divides m & n divides ( m +^ l )) implies n divides l))))
proof
let n;
let m;
let l;
assume n divides m;
then consider a such that A1: m = ( n *^ a ) by CKB12:1;
assume n divides ( m +^ l );
then consider b such that A2: ( m +^ l ) = ( n *^ b ) by CKB12:1;
assume A3: (not n divides l);
l = ( ( n *^ b ) -^ ( n *^ a ) ) by A1,A2,ORDINAL3:52
.= ( ( b -^ a ) *^ n ) by ORDINAL3:63;
hence thesis by A3,CKB11:def 1;
end;
