begin
reserve a for  natural Ordinal;
reserve b for  natural Ordinal;
theorem
Th48: (b <> ( {} ) implies (( numerator ( a / b ) ) = ( RED (a,b) ) & ( denominator ( a / b ) ) = ( RED (b,a) )))
proof
assume A1: b <> ( {} );
per cases ;
suppose A2: ( RED (b,a) ) = 1;

then ( a / b ) = ( RED (a,b) ) by CKB58:def 1;
hence thesis by A2,CKB51:def 1,CKB52:def 1;
end;
suppose A3: ( RED (b,a) ) <> 1;

A4: (not [ ( RED (a,b) ),( RED (b,a) ) ] in ( omega )) by CKB49:1;
( a / b ) = [ ( RED (a,b) ),( RED (b,a) ) ] by A1,A3,CKB58:def 1;
hence thesis by A4,CKB51:def 1,CKB52:def 1;
end;
