environ
vocabularies DIRAF,SUBSET_1,ANALOAF,AFF_1,AFF_2,ANALMETR,CONMETR,CONAFFM,CONMETR1;
notations STRUCT_0,ANALOAF,DIRAF,AFF_1,ANALMETR,AFF_2,CONAFFM,CONMETR,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6;
definitions CKB1,CKB2,CKB3,CKB4,CKB5,CKB6;
theorems AFF_1,AFF_2,ANALMETR,CONAFFM,CONMETR,TRANSLAC,DIRAF,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10;
registrations STRUCT_0,ANALMETR;
constructors AFF_1,AFF_2,CONAFFM,CONMETR,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6;
requirements SUBSET;
begin
reserve X for AffinPlane;
theorem
(X is  satisfying_indirect_Scherungssatz implies X is  satisfying_Scherungssatz)
proof
assume A1: X is  satisfying_indirect_Scherungssatz;
then X is  satisfying_major_indirect_Scherungssatz by CKB7:1;
then A2: X is  satisfying_major_Scherungssatz by CKB10:1;
X is  satisfying_minor_indirect_Scherungssatz by A1,CKB7:1;
then X is  satisfying_minor_Scherungssatz by CKB9:1;
hence thesis by A2,CKB8:1;
end;
