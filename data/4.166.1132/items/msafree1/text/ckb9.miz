begin
theorem
Th4: (for I being non  empty set holds (for X being  disjoint_valued (ManySortedSet of I) holds (for D being  non-empty (ManySortedSet of I) holds (for F1,F2 being (ManySortedFunction of X,D) holds (( Flatten F1 ) = ( Flatten F2 ) implies F1 = F2)))))
proof
let I being non  empty set;
let X being  disjoint_valued (ManySortedSet of I);
let D being  non-empty (ManySortedSet of I);
let F1 being (ManySortedFunction of X,D);
let F2 being (ManySortedFunction of X,D);
assume A1: ( Flatten F1 ) = ( Flatten F2 );
now
let i being set;
assume A2: i in I;
then reconsider Di = ( D . i ) as non  empty set;
reconsider f1 = ( F1 . i ),f2 = ( F2 . i ) as (Function of ( X . i ),Di) by A2,PBOOLE:def 15;
now
let x being set;
assume A3: x in ( X . i );
hence ( f1 . x ) = ( ( Flatten F1 ) . x ) by A2,CKB8:def 1
.= ( f2 . x ) by A1,A2,A3,CKB8:def 1;
end;
hence ( F1 . i ) = ( F2 . i ) by FUNCT_2:12;
end;
hence thesis by PBOOLE:3;
end;
