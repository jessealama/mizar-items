begin
definition
let X being non trivial set;
let F being (BinOp of X);
let x being (Element of X);
pred F is_Bin_Op_Preserv x
means
(( X \ { x } ) is (Preserv of F) & ( ( F || X ) \ { x } ) is (BinOp of ( X \ { x } )))
;correctness;
