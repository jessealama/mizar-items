environ
vocabularies NUMBERS,FUNCT_1,ZFMISC_1,XBOOLE_0,RELAT_1,TARSKI,BINOP_1,SUBSET_1,CARD_1,MCART_1,REALSET1,BSPACE;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,RELAT_1,FUNCT_1,FUNCT_2,BINOP_1,DOMAIN_1,FUNCOP_1,FUNCT_3,CARD_1,XXREAL_0,CKB2,CKB3,CKB7;
