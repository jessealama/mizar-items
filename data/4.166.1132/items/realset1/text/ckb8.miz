begin
theorem
Th4: (for X being set holds (X is non  trivial iff (for x being set holds ( X \ { x } ) is non  empty non  empty set)))
proof
let X being set;
hereby
assume A1: X is non  trivial;
let x being set;
X <> { x } by A1;
then consider y being set such that A2: y in X and A3: x <> y by A1,ZFMISC_1:35;
(not y in { x }) by A3,TARSKI:def 1;
hence ( X \ { x } ) is non  empty non  empty set by A2,XBOOLE_0:def 5;
end;
assume A4: (for x being set holds ( X \ { x } ) is non  empty non  empty set);
( X \ { ( {} ) } ) c= X;
then X is non  empty by A4;
then consider x being set such that W1: x in X by XBOOLE_0:def 1;
( X \ { x } ) is non  empty by A4;
then consider y being set such that W2: y in ( X \ { x } ) by XBOOLE_0:def 1;
take x;
take y;
thus x in X by W1;
thus y in X by W2;
x in { x } by TARSKI:def 1;
then (not x in ( X \ { x } )) by XBOOLE_0:def 5;
hence x <> y by W2;
end;
