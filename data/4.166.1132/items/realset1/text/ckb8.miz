registrations XBOOLE_0,FUNCT_1,RELSET_1,FINSET_1,MEMBERED,CARD_1,ZFMISC_1,SUBSET_1,CKB4,CKB5;
constructors BINOP_1,DOMAIN_1,FUNCT_3,FUNCOP_1,XXREAL_0,CARD_1,MEMBERED,NUMBERS,RELSET_1,CKB2,CKB3,CKB7;
