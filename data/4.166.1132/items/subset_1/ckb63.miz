environ
vocabularies ZFMISC_1,XBOOLE_0,TARSKI,SUBSET_1;
notations TARSKI,XBOOLE_0,ENUMSET1,ZFMISC_1,CKB10,CKB11,CKB16,CKB19,CKB20,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32;
definitions TARSKI,XBOOLE_0,ZFMISC_1,CKB10,CKB19,CKB20,CKB26;
theorems XBOOLE_0,TARSKI,ENUMSET1,ZFMISC_1,XBOOLE_1,CKB10,CKB17,CKB19,CKB20,CKB22,CKB23,CKB24,CKB25,CKB26,CKB33,CKB34,CKB35,CKB36,CKB37,CKB38,CKB39,CKB40,CKB41,CKB42,CKB43,CKB44,CKB45,CKB46,CKB47,CKB48,CKB49,CKB50,CKB51,CKB52,CKB53,CKB54,CKB55,CKB56,CKB57,CKB58,CKB59,CKB60,CKB61,CKB62;
schemes XBOOLE_0;
registrations XBOOLE_0,ZFMISC_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB12,CKB13,CKB14,CKB15,CKB18,CKB21;
constructors TARSKI,XBOOLE_0,ENUMSET1,ZFMISC_1,CKB10,CKB16,CKB19,CKB20,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32;
requirements BOOLE;
begin
reserve X for set;
reserve x for set;
reserve x1 for (Element of X);
reserve x2 for (Element of X);
reserve x3 for (Element of X);
theorem
(X <> ( {} ) implies { x1,x2,x3 } is (Subset of X))
proof
assume A1: X <> ( {} );
then A2: x3 in X by CKB10:def 1;
A3: (x1 in X & x2 in X) by A1,CKB10:def 1;
{ x1,x2,x3 } c= X
proof
let x;
assume x in { x1,x2,x3 };
hence thesis by A3,A2,ENUMSET1:def 1;
end;
then { x1,x2,x3 } in ( bool X ) by ZFMISC_1:def 1;
hence thesis by CKB10:def 1;
end;
