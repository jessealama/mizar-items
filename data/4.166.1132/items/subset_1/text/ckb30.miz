environ
vocabularies ZFMISC_1,XBOOLE_0,TARSKI,SUBSET_1;
notations TARSKI,XBOOLE_0,ENUMSET1,ZFMISC_1,CKB10,CKB11,CKB16,CKB19,CKB20,CKB26,CKB27,CKB28,CKB29;
definitions TARSKI,XBOOLE_0,ZFMISC_1,CKB10,CKB19,CKB20,CKB26;
theorems XBOOLE_0,TARSKI,ENUMSET1,ZFMISC_1,XBOOLE_1,CKB10,CKB17,CKB19,CKB20,CKB22,CKB23,CKB24,CKB25,CKB26;
schemes XBOOLE_0;
registrations XBOOLE_0,ZFMISC_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB12,CKB13,CKB14,CKB15,CKB18,CKB21;
constructors TARSKI,XBOOLE_0,ENUMSET1,ZFMISC_1,CKB10,CKB16,CKB19,CKB20,CKB26,CKB27,CKB28,CKB29;
requirements BOOLE;
begin
reserve E for set;
reserve X for set;
reserve A for (Subset of E);
definition
let E;
let A;
let X;
redefine func A \ X -> (Subset of E);

coherence
proof
A in ( bool E ) by CKB10:def 1;
then (( A \ X ) c= A & A c= E) by XBOOLE_1:36,ZFMISC_1:def 1;
then ( A \ X ) c= E by XBOOLE_1:1;
then ( A \ X ) in ( bool E ) by ZFMISC_1:def 1;
hence ( A \ X ) is (Subset of E) by CKB10:def 1;
end;
end;
