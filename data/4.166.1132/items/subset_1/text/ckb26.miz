environ
vocabularies ZFMISC_1,XBOOLE_0,TARSKI,SUBSET_1;
notations TARSKI,XBOOLE_0,ENUMSET1,ZFMISC_1,CKB10,CKB11,CKB16,CKB19,CKB20;
definitions TARSKI,XBOOLE_0,ZFMISC_1,CKB10,CKB19,CKB20;
theorems XBOOLE_0,TARSKI,ENUMSET1,ZFMISC_1,XBOOLE_1,CKB10,CKB17,CKB19,CKB20,CKB22,CKB23,CKB24,CKB25;
schemes XBOOLE_0;
registrations XBOOLE_0,ZFMISC_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB12,CKB13,CKB14,CKB15,CKB18,CKB21;
constructors TARSKI,XBOOLE_0,ENUMSET1,ZFMISC_1,CKB10,CKB16,CKB19,CKB20;
requirements BOOLE;
begin
reserve E for set;
reserve A for (Subset of E);
definition
let E;
let A;
func A ` -> (Subset of E)
equals
( E \ A );
coherence
proof
( E \ A ) c= E by XBOOLE_1:36;
then ( E \ A ) in ( bool E ) by ZFMISC_1:def 1;
hence thesis by CKB10:def 1;
end;
correctness;
involutiveness
proof
let S being (Subset of E),T being (Subset of E);
assume A1: S = ( E \ T );
T in ( bool E ) by CKB10:def 1;
then T c= E by ZFMISC_1:def 1;
hence T = ( ( {} ) \/ ( E /\ T ) ) by XBOOLE_1:28
.= ( ( E \ E ) \/ ( E /\ T ) ) by XBOOLE_1:37
.= ( E \ S ) by A1,XBOOLE_1:52;
end;
end;
