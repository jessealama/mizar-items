begin
reserve X for set;
reserve x for set;
reserve x1 for (Element of X);
theorem
(X <> ( {} ) implies { x1 } is (Subset of X))
proof
assume X <> ( {} );
then A1: x1 in X by CKB10:def 1;
{ x1 } c= X
proof
let x;
assume x in { x1 };
hence thesis by A1,TARSKI:def 1;
end;
then { x1 } in ( bool X ) by ZFMISC_1:def 1;
hence thesis by CKB10:def 1;
end;
