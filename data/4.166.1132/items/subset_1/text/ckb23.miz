environ
vocabularies ZFMISC_1,XBOOLE_0,TARSKI,SUBSET_1;
notations TARSKI,XBOOLE_0,ENUMSET1,ZFMISC_1,CKB10,CKB11,CKB16,CKB19,CKB20;
definitions TARSKI,XBOOLE_0,ZFMISC_1,CKB10,CKB19,CKB20;
theorems XBOOLE_0,TARSKI,ENUMSET1,ZFMISC_1,XBOOLE_1,CKB10,CKB17,CKB19,CKB20,CKB22;
schemes XBOOLE_0;
registrations XBOOLE_0,ZFMISC_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB12,CKB13,CKB14,CKB15,CKB18,CKB21;
constructors TARSKI,XBOOLE_0,ENUMSET1,ZFMISC_1,CKB10,CKB16,CKB19,CKB20;
requirements BOOLE;
begin
reserve E for set;
reserve A for (Subset of E);
reserve B for (Subset of E);
theorem
Th7: ((for x being (Element of E) holds (x in A implies x in B)) implies A c= B)
proof
assume A1: (for x being (Element of E) holds (x in A implies x in B));
let x being set;
assume A2: x in A;
then x in E by CKB17:1;
then x is (Element of E) by CKB10:def 1;
hence thesis by A1,A2;
end;
