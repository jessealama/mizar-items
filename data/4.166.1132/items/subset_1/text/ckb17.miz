environ
vocabularies ZFMISC_1,XBOOLE_0,TARSKI,SUBSET_1;
notations TARSKI,XBOOLE_0,ENUMSET1,ZFMISC_1,CKB10,CKB11,CKB16;
definitions TARSKI,XBOOLE_0,ZFMISC_1,CKB10;
theorems XBOOLE_0,TARSKI,ENUMSET1,ZFMISC_1,XBOOLE_1,CKB10;
schemes XBOOLE_0;
registrations XBOOLE_0,ZFMISC_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB12,CKB13,CKB14,CKB15;
constructors TARSKI,XBOOLE_0,ENUMSET1,ZFMISC_1,CKB10,CKB16;
requirements BOOLE;
begin
reserve E for set;
theorem
Lm1: (for X being (Subset of E) holds (for x being set holds (x in X implies x in E)))
proof
let X being (Subset of E);
let x being set;
assume that
A1: x in X;
X in ( bool E ) by CKB10:def 1;
then X c= E by ZFMISC_1:def 1;
hence thesis by A1,TARSKI:def 3;
end;
