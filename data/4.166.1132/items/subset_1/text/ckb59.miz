begin
reserve E for set;
reserve A for (Subset of E);
reserve B for (Subset of E);
theorem
(for A holds (for B holds ((for x being (Element of E) holds ((not x in A) iff x in B)) implies A = ( B ` ))))
proof
let A;
let B;
assume (for x being (Element of E) holds ((not x in A) iff x in B));
then (for x being (Element of E) holds (x in A iff (not x in B)));
hence thesis by CKB58:1;
end;
