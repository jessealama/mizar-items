begin
reserve L for Lattice;
reserve p for (Element of L);
reserve q for (Element of L);
reserve x for set;
reserve D1 for non  empty (Subset of L);
reserve D2 for non  empty (Subset of L);
definition
let L;
let D1;
let D2;
func D1 "/\" D2 -> (Subset of L) equals 
{ ( p "/\" q ): (p in D1 & q in D2) };
coherence
proof
set x = the (Element of D1);
set y = the (Element of D2);
reconsider x,y as (Element of L);
( x "/\" y ) in { ( p "/\" q ): (p in D1 & q in D2) };
then reconsider D = { ( p "/\" q ): (p in D1 & q in D2) } as non  empty set;
D c= (the carrier of L)
proof
let x;
assume x in D;
then (ex p st (ex q st ((x = ( p "/\" q ) & p in D1) & q in D2)));
hence thesis;
end;
hence thesis;
end;
end;
