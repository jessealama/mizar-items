begin
scheme Gen199 { A,B() -> set,F(set,set) -> set,P[set,set],Q[set] } : (for s being (Element of A()) holds (for t being (Element of B()) holds (P[ s,t ] implies Q[ F(s,t) ])))
provided
A1: (for st1 being set holds (st1 in { F(s1,t1) where s1 is (Element of A()),t1 is (Element of B()): P[ s1,t1 ] } implies Q[ st1 ]))
proof
let s being (Element of A());
let t being (Element of B());
assume P[ s,t ];
then F(s,t) in { F(s1,t1) where s1 is (Element of A()),t1 is (Element of B()): P[ s1,t1 ] };
hence thesis by A1;
end;
