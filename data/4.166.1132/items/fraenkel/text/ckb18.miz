begin
scheme Gen399 { A,B() -> set,F(set,set) -> set,P[set,set],Q[set] } : { F(s,t) where s is (Element of A()),t is (Element of B()): (s in { s1 where s1 is (Element of A()): Q[ s1 ] } & P[ s,t ]) } = { F(s2,t2) where s2 is (Element of A()),t2 is (Element of B()): (Q[ s2 ] & P[ s2,t2 ]) }
proof
defpred QQ[ set,set ]
 means
(Q[ $1 ] & P[ $1,$2 ]);
defpred PP[ set,set ]
 means
($1 in { s1 where s1 is (Element of A()): Q[ s1 ] } & P[ $1,$2 ]);
A1: (for s being (Element of A()) holds (for t being (Element of B()) holds (PP[ s,t ] iff QQ[ s,t ])))
proof
let s being (Element of A());
let t being (Element of B());
now
assume s in { s1 where s1 is (Element of A()): Q[ s1 ] };
then (ex s1 being (Element of A()) st (s = s1 & Q[ s1 ]));
hence Q[ s ];
end;
hence thesis;
end;
thus { F(s,t) where s is (Element of A()),t is (Element of B()): PP[ s,t ] } = { F(s2,t2) where s2 is (Element of A()),t2 is (Element of B()): QQ[ s2,t2 ] } from CKB4:sch 1(A1);
end;
