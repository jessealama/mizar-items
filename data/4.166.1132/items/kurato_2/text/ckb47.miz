begin
theorem
Th74: (for T being non empty TopSpace holds (for F being (SetSequence of (the carrier of T)) holds (for A being (Subset of T) holds ((for i being Nat holds ( F . i ) = A) implies ( Lim_inf F ) = ( Lim_sup F )))))
proof
let T being non empty TopSpace;
let F being (SetSequence of (the carrier of T));
let A being (Subset of T);
assume A1: (for i being Nat holds ( F . i ) = A);
thus ( Lim_inf F ) c= ( Lim_sup F ) by CKB39:1;
thus ( Lim_sup F ) c= ( Lim_inf F )
proof
let x being set;
assume x in ( Lim_sup F );
then (ex G being (subsequence of F) st x in ( Lim_inf G )) by CKB37:def 1;
hence thesis by A1,CKB17:1;
end;

end;
