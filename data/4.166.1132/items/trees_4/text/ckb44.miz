Th24: (for D1 holds (for D2 holds (for T holds (( dom ( T `1 ) ) = ( dom T ) & ( dom ( T `2 ) ) = ( dom T )))))
proof
let D1;
let D2;
let T;
A1: (( T `1 ) = ( ( pr1 (D1,D2) ) * T ) & ( T `2 ) = ( ( pr2 (D1,D2) ) * T )) by TREES_3:def 12,TREES_3:def 13;
