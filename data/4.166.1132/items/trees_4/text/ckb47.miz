environ
vocabularies NUMBERS,TREES_2,SUBSET_1,RELAT_1,FINSEQ_1,FUNCT_1,TARSKI,TREES_1,XXREAL_0,ARYTM_3,CARD_1,FUNCOP_1,XBOOLE_0,TREES_3,ZFMISC_1,NAT_1,FINSEQ_2,TREES_A,ORDINAL4,FUNCT_6,FINSEQ_4,MCART_1,PARTFUN1,TREES_4;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,NUMBERS,NAT_1,RELAT_1,FUNCT_1,DOMAIN_1,FUNCOP_1,FUNCT_3,FINSEQ_1,FINSEQ_2,TREES_1,TREES_2,FUNCT_6,TREES_3,XXREAL_0,CKB1,CKB2,CKB6,CKB7,CKB12,CKB16,CKB17,CKB18,CKB36,CKB37,CKB38,CKB41;
definitions TARSKI,FINSEQ_1,TREES_1,TREES_2,FINSEQ_2,FUNCT_6,CKB2,CKB6,CKB12,CKB16,CKB17,CKB18,CKB41;
theorems TARSKI,ZFMISC_1,FINSEQ_1,MCART_1,NAT_1,FUNCT_1,FUNCT_2,FUNCOP_1,FUNCT_6,FINSEQ_2,FINSEQ_3,TREES_1,TREES_2,TREES_3,RELAT_1,XBOOLE_0,XBOOLE_1,XREAL_1,XXREAL_0,FUNCT_5,ORDINAL1,CARD_1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB41,CKB43,CKB44,CKB45,CKB46;
schemes CLASSES1,FINSEQ_1,XBOOLE_0,CKB40;
registrations XBOOLE_0,SUBSET_1,RELAT_1,FUNCT_1,ORDINAL1,XREAL_0,FINSEQ_1,TREES_2,TREES_3,CARD_1,FINSEQ_2,FUNCOP_1,RELSET_1,TREES_1,CKB32,CKB33,CKB34,CKB35,CKB39,CKB42;
constructors BINOP_1,DOMAIN_1,XXREAL_0,XREAL_0,NAT_1,FUNCT_5,FINSEQ_2,FUNCT_6,TREES_3,RELSET_1,CKB6,CKB7,CKB12,CKB16,CKB17,CKB18,CKB36,CKB37,CKB38,CKB41;
requirements NUMERALS,REAL,BOOLE,SUBSET,ARITHM;
begin
reserve x for set;
reserve y for set;
reserve i for (Element of ( NAT ));
reserve n for (Element of ( NAT ));
reserve p for FinSequence;
reserve q for FinSequence;
Lm1:now
let x;
let y;
let p being (FinSequence of x);
assume (y in ( dom p ) or y in ( dom p ));
then A1: ( p . y ) in ( rng p ) by FUNCT_1:def 3;
( rng p ) c= x by FINSEQ_1:def 4;
hence ( p . y ) in x by A1;
end;
Lm3:now
let n;
let x;
let p being (FinSequence of x);
assume n < ( len p );
then A1: ( p . ( n + 1 ) ) in ( rng p ) by CKB5:1;
( rng p ) c= x by FINSEQ_1:def 4;
hence ( p . ( n + 1 ) ) in x by A1;
end;
reserve x for set;
reserve D1 for non  empty set;
reserve D2 for non  empty set;
reserve T for (DecoratedTree of D1,D2);
reserve d1 for (Element of D1);
reserve d2 for (Element of D2);
reserve F for non  empty (DTree-set of D1,D2);
reserve F1 for non  empty (DTree-set of D1);
theorem
Th27: (for D1 holds (for D2 holds (for d1 holds (for d2 holds (for F holds (for F1 holds (for p being (FinSequence of F) holds (for p1 being (FinSequence of F1) holds ((( dom p1 ) = ( dom p ) & (for i holds (i in ( dom p ) implies (for T holds (T = ( p . i ) implies ( p1 . i ) = ( T `1 )))))) implies ( ( [ d1,d2 ] -tree p ) `1 ) = ( d1 -tree p1 ))))))))))
proof
let D1;
let D2;
let d1;
let d2;
let F;
let F1;
let p being (FinSequence of F);
let p1 being (FinSequence of F1);
assume that
A1: ( dom p1 ) = ( dom p )
and
A2: (for i holds (i in ( dom p ) implies (for T holds (T = ( p . i ) implies ( p1 . i ) = ( T `1 )))));
set W = ( [ d1,d2 ] -tree p );
set W1 = ( d1 -tree p1 );
A3: ( len ( doms p ) ) = ( len p ) by TREES_3:38;
A4: ( len ( doms p1 ) ) = ( len p1 ) by TREES_3:38;
A5: ( len p ) = ( len p1 ) by A1,FINSEQ_3:29;
then A6: ( dom ( doms p ) ) = ( dom ( doms p1 ) ) by A3,A4,FINSEQ_3:29;
A7: ( dom ( doms p ) ) = ( dom p ) by A3,FINSEQ_3:29;
now
let i being Nat;
assume A8: i in ( dom p );
then reconsider T = ( p . i ) as (Element of F) by Lm1;
A9: ( p1 . i ) = ( T `1 ) by A2,A8;
A10: ( ( doms p ) . i ) = ( dom T ) by A8,FUNCT_6:22;
( ( doms p1 ) . i ) = ( dom ( T `1 ) ) by A1,A8,A9,FUNCT_6:22;
hence ( ( doms p ) . i ) = ( ( doms p1 ) . i ) by A10,CKB44:1;
end;
then A11: ( doms p ) = ( doms p1 ) by A6,A7,FINSEQ_1:13;
( dom ( W `1 ) ) = ( dom W ) by CKB44:1
.= ( tree ( doms p ) ) by CKB19:1;
hence ( dom ( W `1 ) ) = ( dom W1 ) by A11,CKB19:1;
let x being (Node of ( W `1 ));
reconsider a = x as (Node of W) by CKB44:1;
A12: ( ( W `1 ) . x ) = ( ( W . a ) `1 ) by TREES_3:39;
per cases ;
suppose x = ( {} );

then (( W . x ) = [ d1,d2 ] & ( W1 . x ) = d1) by CKB16:def 1;
hence thesis by A12,MCART_1:7;
end;
suppose x <> ( {} );

then consider n being (Element of ( NAT )),T being DecoratedTree,q being (Node of T) such that A13: n < ( len p ) and A14: T = ( p . ( n + 1 ) ) and A15: a = ( <* n *> ^ q ) by CKB20:1;
reconsider T as (Element of F) by A13,A14,Lm3;
reconsider q as (Node of ( T `1 )) by CKB44:1;
A16: ( p1 . ( n + 1 ) ) = ( T `1 ) by A2,A13,A14,CKB5:1;
A17: ( W . a ) = ( T . q ) by A13,A14,A15,CKB21:1;
( W1 . a ) = ( ( T `1 ) . q ) by A5,A13,A15,A16,CKB21:1;
hence thesis by A12,A17,TREES_3:39;
end;
end;
