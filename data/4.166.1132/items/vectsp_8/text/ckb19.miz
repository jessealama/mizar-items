registrations XBOOLE_0,SUBSET_1,RELAT_1,FUNCT_1,FUNCT_2,STRUCT_0,LATTICES,VECTSP_2,VECTSP_4,VECTSP_5,GRCAT_1,MOD_2,RELSET_1,CKB3;
constructors SETFAM_1,BINOP_1,REALSET1,VECTSP_5,VECTSP_7,MOD_2,LATTICE3,LATTICE4,RELSET_1,GRCAT_1,CKB1,CKB2,CKB4,CKB5,CKB6,CKB7,CKB10;
requirements BOOLE,SUBSET;
begin
reserve F for Field;
reserve VS for  strict (VectSp of F);
reserve x for set;
theorem
Th10: (for x being set holds (for VS being  strict (VectSp of F) holds (for S being (Subset of VS) holds ((S is non  empty & S is  linearly-closed) implies (x in ( Lin S ) implies x in S)))))
proof
let x being set;
let VS being  strict (VectSp of F);
let S being (Subset of VS);
assume (S is non  empty & S is  linearly-closed);
then consider W being  strict (Subspace of VS) such that A1: S = (the carrier of W) by VECTSP_4:34;
assume A2: x in ( Lin S );
( Lin S ) = W by A1,VECTSP_7:11;
hence thesis by A2,A1,STRUCT_0:def 5;
end;
