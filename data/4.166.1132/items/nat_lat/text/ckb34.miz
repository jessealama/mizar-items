begin
reserve n for Nat;
reserve m for Nat;
reserve m for NatPlus;
reserve n for NatPlus;
definition
func lcmlatplus -> (BinOp of ( NATPLUS )) means 
:Def13: ( it . (m,n) ) = ( m lcm n );
existence
proof
deffunc O(NatPlus,NatPlus) = ( @ ( $1 lcm $2 ) );
consider f being (BinOp of ( NATPLUS )) such that A3: (for m,n being NatPlus holds ( f . (m,n) ) = O(m,n)) from BINOP_1:sch 4;
take f;
let m being NatPlus;
let n being NatPlus;
A4: (m > ( 0 ) & n > ( 0 )) by CKB25:def 1;
thus ( f . (m,n) ) = O(m,n) by A3
.= ( m lcm n ) by A4,CKB31:def 1,NEWTON:59;
end;
uniqueness
proof
deffunc O(NatPlus,NatPlus) = ( $1 lcm $2 );
thus (for o1,o2 being (BinOp of ( NATPLUS )) holds (((for a,b being NatPlus holds ( o1 . (a,b) ) = O(a,b)) & (for a,b being NatPlus holds ( o2 . (a,b) ) = O(a,b))) implies o1 = o2)) from BINOP_2:sch 2;
end;
end;
