environ
vocabularies NUMBERS,NAT_1,BINOP_1,FUNCT_1,INT_2,SUBSET_1,ARYTM_3,STRUCT_0,XBOOLE_0,LATTICES,ORDINAL1,EQREL_1,PBOOLE,CARD_1,TARSKI,XREAL_0,XXREAL_0,QC_LANG1,XCMPLX_0,ZFMISC_1,RELAT_1,REALSET1,NAT_LAT,MEMBERED;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,NUMBERS,ORDINAL1,XREAL_0,REALSET1,NAT_1,NAT_D,BINOP_1,RELAT_1,FUNCT_1,MEMBERED,XXREAL_0,STRUCT_0,LATTICES;
definitions TARSKI,XBOOLE_0,LATTICES,BINOP_1,REALSET1,ORDINAL1;
theorems NEWTON,ZFMISC_1,LATTICES,FUNCT_1,FUNCT_2,XBOOLE_1,RELAT_1,ORDINAL1,BINOP_1,RELSET_1;
schemes BINOP_1,BINOP_2,SUBSET_1;
