begin
reserve n for Nat;
reserve m for Nat;
definition
func hcflat -> (BinOp of ( NAT )) means 
:Def3: ( it . (m,n) ) = ( m gcd n );
existence
proof
deffunc O(Nat,Nat) = ( $1 gcd $2 );
consider o being (BinOp of ( NAT )) such that A1: (for a,b being (Element of ( NAT )) holds ( o . (a,b) ) = O(a,b)) from BINOP_1:sch 4;
take o;
let m;
let n;
(m in ( NAT ) & n in ( NAT )) by ORDINAL1:def 12;
hence thesis by A1;
end;
uniqueness
proof
let f1 being (BinOp of ( NAT ));
let f2 being (BinOp of ( NAT ));
assume that
A2: ( f1 . (m,n) ) = ( m gcd n )
and
A3: ( f2 . (m,n) ) = ( m gcd n );
now
let m being (Element of ( NAT ));
let n being (Element of ( NAT ));
thus ( f1 . (m,n) ) = ( m gcd n ) by A2
.= ( f2 . (m,n) ) by A3;
end;
hence thesis by BINOP_1:2;
end;
end;
