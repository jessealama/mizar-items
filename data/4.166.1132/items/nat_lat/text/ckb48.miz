environ
vocabularies NUMBERS,NAT_1,BINOP_1,FUNCT_1,INT_2,SUBSET_1,ARYTM_3,STRUCT_0,XBOOLE_0,LATTICES,ORDINAL1,EQREL_1,PBOOLE,CARD_1,TARSKI,XREAL_0,XXREAL_0,QC_LANG1,XCMPLX_0,ZFMISC_1,RELAT_1,REALSET1,NAT_LAT,MEMBERED;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,NUMBERS,ORDINAL1,XREAL_0,REALSET1,NAT_1,NAT_D,BINOP_1,RELAT_1,FUNCT_1,MEMBERED,XXREAL_0,STRUCT_0,LATTICES,CKB1,CKB2,CKB3,CKB10,CKB11,CKB25,CKB27,CKB30,CKB31,CKB33,CKB34,CKB35,CKB37,CKB47;
definitions TARSKI,XBOOLE_0,LATTICES,BINOP_1,REALSET1,ORDINAL1,CKB1,CKB2,CKB3,CKB10,CKB11,CKB25,CKB31,CKB33,CKB34,CKB35,CKB37,CKB47;
theorems NEWTON,ZFMISC_1,LATTICES,FUNCT_1,FUNCT_2,XBOOLE_1,RELAT_1,ORDINAL1,BINOP_1,RELSET_1,CKB1,CKB2,CKB3,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB31,CKB33,CKB34,CKB35,CKB37,CKB41,CKB42,CKB43,CKB44,CKB45,CKB47;
schemes BINOP_1,BINOP_2,SUBSET_1;
registrations ORDINAL1,RELSET_1,FINSET_1,XREAL_0,MEMBERED,STRUCT_0,LATTICES,CKB4,CKB14,CKB15,CKB16,CKB26,CKB28,CKB29,CKB32,CKB36,CKB38,CKB46,CKB5,CKB6,CKB39,CKB40;
constructors PARTFUN1,BINOP_1,FINSET_1,XXREAL_0,NAT_D,MEMBERED,REALSET1,LATTICES,RELSET_1,CKB1,CKB2,CKB3,CKB10,CKB11,CKB25,CKB27,CKB31,CKB33,CKB34,CKB35,CKB37,CKB47;
requirements REAL,NUMERALS,SUBSET,BOOLE;
begin
registration
let L being Lattice;
cluster  strict for (SubLattice of L);
existence
proof
set S = LattStr (# (the carrier of L),(the L_join of L),(the L_meet of L) #);
A1:now
let a being (Element of S);
let b being (Element of S);
let c being (Element of S);
reconsider a9 = a,b9 = b,c9 = c as (Element of L);
thus ( a "\/" ( b "\/" c ) ) = ( a9 "\/" ( b9 "\/" c9 ) )
.= ( ( a9 "\/" b9 ) "\/" c9 ) by LATTICES:def 5
.= ( ( a "\/" b ) "\/" c );
end;
A2:now
let a being (Element of S);
let b being (Element of S);
reconsider a9 = a,b9 = b as (Element of L);
thus ( ( a "/\" b ) "\/" b ) = ( ( a9 "/\" b9 ) "\/" b9 )
.= b by LATTICES:def 8;
end;
A3:now
let a being (Element of S);
let b being (Element of S);
reconsider a9 = a,b9 = b as (Element of L);
thus ( a "/\" ( a "\/" b ) ) = ( a9 "/\" ( a9 "\/" b9 ) )
.= a by LATTICES:def 9;
end;
A4:now
let a being (Element of S);
let b being (Element of S);
let c being (Element of S);
reconsider a9 = a,b9 = b,c9 = c as (Element of L);
thus ( a "/\" ( b "/\" c ) ) = ( a9 "/\" ( b9 "/\" c9 ) )
.= ( ( a9 "/\" b9 ) "/\" c9 ) by LATTICES:def 7
.= ( ( a "/\" b ) "/\" c );
end;
A5: (for a,b being (Element of S) holds (for a9,b9 being (Element of L) holds ((a = a9 & b = b9) implies (( a "\/" b ) = ( a9 "\/" b9 ) & ( a "/\" b ) = ( a9 "/\" b9 )))));
A6:now
let a being (Element of S);
let b being (Element of S);
reconsider a9 = a,b9 = b as (Element of L);
thus ( a "/\" b ) = ( b9 "/\" a9 ) by A5
.= ( b "/\" a );
end;
now
let a being (Element of S);
let b being (Element of S);
reconsider a9 = a,b9 = b as (Element of L);
thus ( a "\/" b ) = ( b9 "\/" a9 ) by A5
.= ( b "\/" a );
end;
then S is  join-commutative  join-associative  meet-absorbing  meet-commutative  meet-associative  join-absorbing by A1,A2,A6,A4,A3,LATTICES:def 4,LATTICES:def 5,LATTICES:def 6,LATTICES:def 7,LATTICES:def 8,LATTICES:def 9;
then reconsider S as Lattice;
((the L_join of S) = ( (the L_join of L) || (the carrier of S) ) & (the L_meet of S) = ( (the L_meet of L) || (the carrier of S) )) by RELSET_1:19;
then S is (SubLattice of L) by CKB47:def 1;
hence thesis;
end;
end;
