begin
reserve n for Nat;
reserve m for Nat;
definition
func lcmlat -> (BinOp of ( NAT )) means 
:Def4: ( it . (m,n) ) = ( m lcm n );
existence
proof
deffunc O(Nat,Nat) = ( $1 lcm $2 );
consider o being (BinOp of ( NAT )) such that A4: (for a,b being (Element of ( NAT )) holds ( o . (a,b) ) = O(a,b)) from BINOP_1:sch 4;
take o;
let m;
let n;
(m in ( NAT ) & n in ( NAT )) by ORDINAL1:def 12;
hence thesis by A4;
end;
uniqueness
proof
let f1 being (BinOp of ( NAT ));
let f2 being (BinOp of ( NAT ));
assume that
A5: ( f1 . (m,n) ) = ( m lcm n )
and
A6: ( f2 . (m,n) ) = ( m lcm n );
now
let m being (Element of ( NAT ));
let n being (Element of ( NAT ));
thus ( f1 . (m,n) ) = ( m lcm n ) by A5
.= ( f2 . (m,n) ) by A6;
end;
hence thesis by BINOP_1:2;
end;
end;
