begin
reserve n for (Element of ( NAT ));
theorem
Th62: (for k being (Element of ( NAT )) holds (for l being (QC-variable_list of k) holds (for a being free_QC-variable holds (for x being bound_QC-variable holds ( still_not-bound_in ( Subst (l,( a .--> x )) ) ) c= ( ( still_not-bound_in l ) \/ { x } )))))
proof
let k being (Element of ( NAT ));
let l being (QC-variable_list of k);
let a being free_QC-variable;
let x being bound_QC-variable;
let y being set;
A1: ( still_not-bound_in l ) = { ( l . n ): ((1 <= n & n <= ( len l )) & ( l . n ) in ( bound_QC-variables )) } by QC_LANG1:def 27;
assume A2: y in ( still_not-bound_in ( Subst (l,( a .--> x )) ) );
then reconsider y9 = y as (Element of ( still_not-bound_in ( Subst (l,( a .--> x )) ) ));
( still_not-bound_in ( Subst (l,( a .--> x )) ) ) = { ( ( Subst (l,( a .--> x )) ) . n ): ((1 <= n & n <= ( len ( Subst (l,( a .--> x )) ) )) & ( ( Subst (l,( a .--> x )) ) . n ) in ( bound_QC-variables )) } by QC_LANG1:def 27;
then consider n such that A3: y9 = ( ( Subst (l,( a .--> x )) ) . n ) and A4: 1 <= n and A5: n <= ( len ( Subst (l,( a .--> x )) ) ) and A6: ( ( Subst (l,( a .--> x )) ) . n ) in ( bound_QC-variables ) by A2;
A7: n <= ( len l ) by A5,CQC_LANG:def 1;
per cases ;
suppose ( l . n ) = a;

then y9 = x by A3,A4,A7,CQC_LANG:3;
then y9 in { x } by TARSKI:def 1;
hence thesis by XBOOLE_0:def 3;
end;
suppose ( l . n ) <> a;

then ( l . n ) = ( ( Subst (l,( a .--> x )) ) . n ) by A4,A7,CQC_LANG:3;
then y9 in ( still_not-bound_in l ) by A1,A3,A4,A6,A7;
hence thesis by XBOOLE_0:def 3;
end;
