environ
vocabularies CARD_1,FUNCOP_1,ORDINAL1,NUMBERS,SUBSET_1,FUNCT_2,FUNCT_1,XBOOLE_0,ARYTM_0,ARYTM_3,RELAT_1,ARYTM_2,ZFMISC_1,ARYTM_1,TARSKI,XCMPLX_0;
notations TARSKI,XBOOLE_0,SUBSET_1,ZFMISC_1,FUNCT_1,FUNCT_2,FUNCT_4,ORDINAL1,ARYTM_2,ARYTM_1,NUMBERS,ARYTM_0,CKB1,CKB2,CKB5,CKB6,CKB7,CKB8,CKB13,CKB14,CKB15,CKB16;
definitions CKB1,CKB2,CKB6,CKB7,CKB8,CKB13,CKB14,CKB15,CKB16;
theorems ARYTM_0,XBOOLE_0,FUNCT_4,FUNCT_2,ZFMISC_1,ARYTM_2,TARSKI,ARYTM_1,XBOOLE_1,NUMBERS,ORDINAL1,CKB1,CKB2,CKB6,CKB7,CKB8,CKB9,CKB10,CKB13,CKB14,CKB15,CKB16;
registrations ZFMISC_1,FUNCT_1,FUNCT_2,NUMBERS,XBOOLE_0,CKB3,CKB4,CKB11,CKB12,CKB17,CKB18;
constructors FUNCT_4,ARYTM_1,ARYTM_0,CKB1,CKB2,CKB7,CKB8,CKB13,CKB14,CKB15,CKB16;
