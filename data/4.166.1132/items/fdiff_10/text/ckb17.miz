definitions SQUARE_1,LIMFUNC1,XCMPLX_0,SIN_COS,FDIFF_9;
theorems FDIFF_2,TAYLOR_1,XBOOLE_1,XCMPLX_1,FDIFF_1,FDIFF_7,RFUNCT_1,FUNCT_1,SIN_COS,RELAT_1,FDIFF_8,FDIFF_9,VALUED_1,XXREAL_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16;
