definition
let p;
let n;
func p /^ n -> XFinSequence
means
:Def2: (( len it ) = ( ( len p ) -' n ) & (for m holds (m in ( dom it ) implies ( it . m ) = ( p . ( m + n ) ))));
existence
proof
thus (ex p1 being XFinSequence st (( len p1 ) = ( ( len p ) -' n ) & (for m holds (m in ( dom p1 ) implies ( p1 . m ) = ( p . ( m + n ) )))))
proof
deffunc F(Nat) = ( p . ( $1 + n ) );
set k = ( ( len p ) -' n );
consider q such that A1: (( len q ) = k & (for m2 being Nat holds (m2 in k implies ( q . m2 ) = F(m2)))) from AFINSQ_1:sch 2;
take q;
thus thesis by A1;
end;

end;
uniqueness
proof
let p1 being XFinSequence,p2 being XFinSequence;
thus (((( len p1 ) = ( ( len p ) -' n ) & (for m being Nat holds (m in ( dom p1 ) implies ( p1 . m ) = ( p . ( m + n ) )))) & (( len p2 ) = ( ( len p ) -' n ) & (for m being Nat holds (m in ( dom p2 ) implies ( p2 . m ) = ( p . ( m + n ) ))))) implies p1 = p2)
proof
assume that
A2: ( len p1 ) = ( ( len p ) -' n )
and
A3: (for m holds (m in ( dom p1 ) implies ( p1 . m ) = ( p . ( m + n ) )))
and
A4: ( len p2 ) = ( ( len p ) -' n )
and
A5: (for m holds (m in ( dom p2 ) implies ( p2 . m ) = ( p . ( m + n ) )));
now
let m;
assume A6: m in ( dom p1 );
then ( p1 . m ) = ( p . ( m + n ) ) by A3;
hence ( p1 . m ) = ( p2 . m ) by A2,A4,A5,A6;
end;
hence thesis by A2,A4,AFINSQ_1:8;
end;

