reserve p for XFinSequence;
theorem
( ( p | n ) ^ ( p /^ n ) ) = p
proof
set pn = ( p /^ n );
now
per cases ;
case A1: ( len p ) <= n;
then A2: ( p | n ) = p by AFINSQ_1:52;
( p /^ n ) = ( {} ) by A1,CKB37:1;
hence thesis by A2,AFINSQ_1:29;
end;
case A3: n < ( len p );
set g = ( p | n );
A4: ( len g ) = n by A3,AFINSQ_1:54;
A5: ( len pn ) = ( ( len p ) - n ) by A3,CKB38:1;
A6:now
let m;
assume A7: m < ( len p );
now
per cases ;
case m < n;
then A8: m in n by NAT_1:44;
hence ( ( ( p | n ) ^ ( p /^ n ) ) . m ) = ( ( p | n ) . m ) by A4,AFINSQ_1:def 3
.= ( p . m ) by A3,A8,AFINSQ_1:53;
end;
case n <= m;
then ( max (( 0 ),( m - n )) ) = ( m - n ) by FINSEQ_2:4;
then reconsider k = ( m - n ) as (Element of ( NAT )) by FINSEQ_2:5;
k < ( len pn ) by A5,A7,XREAL_1:9;
then A9: k in ( dom pn ) by NAT_1:44;
m = ( ( len ( p | n ) ) + k ) by A4;
hence ( ( ( p | n ) ^ ( p /^ n ) ) . m ) = ( pn . k ) by A9,AFINSQ_1:def 3
.= ( p . ( k + n ) ) by A9,CKB36:def 1
.= ( p . m );
end;
end;
hence ( ( ( p | n ) ^ ( p /^ n ) ) . m ) = ( p . m );
end;
( len ( g ^ ( p /^ n ) ) ) = ( n + ( ( len p ) - n ) ) by A5,A4,AFINSQ_1:17
.= ( len p );
hence thesis by A6,AFINSQ_1:9;
end;
end;
hence thesis;
end;
