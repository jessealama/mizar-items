constructors SERIES_1,PARTFUN3,WELLORD2,SETWISEO,REAL_1,FINSOP_1,NAT_D,RECDEF_1,BINOP_2,RELSET_1,ORDINAL4,AFINSQ_1,MATRIX_7,SQUARE_1,FINSEQ_4,BINOP_1,CKB33,CKB36,CKB47,CKB55,CKB60,CKB61;
requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve i for Nat;
theorem
(for X,Y being  finite  natural-membered set holds ((X <N< Y & i in ( card X )) implies ( ( Sgm0 ( X \/ Y ) ) . i ) in X))
proof
let X being  finite  natural-membered set,Y being  finite  natural-membered set;
assume that
A1: X <N< Y
and
A2: i in ( card X );
set f = ( ( Sgm0 ( X \/ Y ) ) | ( card X ) );
set f0 = ( Sgm0 ( X \/ Y ) );
set Z = { v where v is (Element of ( X \/ Y )): (ex k2 being Nat st (v = ( f . k2 ) & k2 in ( card X ))) };
A3: ( rng f0 ) = ( X \/ Y ) by CKB55:def 1;
( len ( Sgm0 ( X \/ Y ) ) ) = ( card ( X \/ Y ) ) by CKB57:1;
then A4: ( card X ) <= ( len ( Sgm0 ( X \/ Y ) ) ) by NAT_1:43,XBOOLE_1:7;
then A5: ( len f ) = ( card X ) by AFINSQ_1:54;
A6: Z c= ( rng f )
proof
let y being set;
assume y in Z;
then (ex v0 being (Element of ( X \/ Y )) st (y = v0 & (ex k2 being Nat st (v0 = ( f . k2 ) & k2 in ( card X )))));
hence thesis by A5,FUNCT_1:def 3;
end;
then reconsider Z0 = Z as  finite set;
A7: ( rng f ) c= ( rng ( Sgm0 ( X \/ Y ) ) ) by RELAT_1:70;
( rng f ) c= Z
proof
let y being set;
assume A8: y in ( rng f );
then consider x being set such that A9: x in ( dom f ) and A10: y = ( f . x ) by FUNCT_1:def 3;
reconsider y0 = y as (Element of ( X \/ Y )) by A7,A8,CKB55:def 1;
(ex k2 being Nat st (y0 = ( f . k2 ) & k2 in ( card X ))) by A5,A9,A10;
hence thesis;
end;
then A11: ( rng f ) = Z by A6,XBOOLE_0:def 10;
A12: ( X \/ Y ) <> ( {} ) by A2,CARD_1:27,XBOOLE_1:15;
A13:now
assume that
A14: (not Z c= X)
and
A15: (not X c= Z);
consider v1 being set such that A16: v1 in Z and A17: (not v1 in X) by A14,TARSKI:def 3;
consider v10 being (Element of ( X \/ Y )) such that A18: v1 = v10 and A19: (ex k2 being Nat st (v10 = ( f . k2 ) & k2 in ( card X ))) by A16;
A20: v10 in Y by A12,A17,A18,XBOOLE_0:def 3;
reconsider nv10 = v10 as Nat;
consider v2 being set such that A21: v2 in X and A22: (not v2 in Z) by A15,TARSKI:def 3;
X c= ( X \/ Y ) by XBOOLE_1:7;
then consider x2 being set such that A23: x2 in ( dom f0 ) and A24: v2 = ( f0 . x2 ) by A3,A21,FUNCT_1:def 3;
reconsider x20 = x2 as Nat by A23;
now
assume x20 < ( card X );
then A25: x20 in ( card X ) by NAT_1:44;
( card X ) <= ( card ( X \/ Y ) ) by NAT_1:43,XBOOLE_1:7;
then ( card X ) <= ( len f0 ) by CKB57:1;
then ( f . x20 ) = ( f0 . x20 ) by A25,AFINSQ_1:53;
hence contradiction by A5,A11,A22,A24,A25,FUNCT_1:def 3;
end;
then A26: ( len f ) <= x20 by A4,AFINSQ_1:54;
consider k20 being Nat such that A27: v10 = ( f . k20 ) and A28: k20 in ( card X ) by A19;
A29: ( f . k20 ) = ( f0 . k20 ) by A4,A28,AFINSQ_1:53;
reconsider nv2 = v2 as Nat by A24;
k20 < ( len f ) by A5,A28,NAT_1:44;
then A30: k20 < x20 by A26,XXREAL_0:2;
x20 < ( len f0 ) by A23,NAT_1:44;
then nv10 < nv2 by A27,A24,A30,A29,CKB55:def 1;
hence contradiction by A1,A21,A20,CKB60:def 1;
end;
f is  one-to-one by FUNCT_1:52;
then A31: ( dom f ),( f .: ( dom f ) ) are_equipotent  by CARD_1:33;
( f .: ( dom f ) ) = ( rng f ) by RELAT_1:113;
then A32: ( card Z ) = ( card ( len f ) ) by A11,A31,CARD_1:5;
then A33: ( card Z ) = ( card X ) by A4,AFINSQ_1:54;
A34:now
per cases  by A13;
case Z0 c= X;
hence Z0 = X by A4,A32,CKB1:1,AFINSQ_1:54;
end;
case X c= Z0;
hence Z0 = X by A33,CKB1:1;
end;
end;
( f . i ) = ( f0 . i ) by A2,A4,AFINSQ_1:53;
hence thesis by A2,A5,A11,A34,FUNCT_1:def 3;
end;
