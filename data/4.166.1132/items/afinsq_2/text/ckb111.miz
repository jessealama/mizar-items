.= ( ( Partial_Sums S ) . ( k + 1 ) ) by A9,A12,A13,NAT_1:44,SERIES_1:def 1;
end;

end;
( ( Partial_Sums S ) . ( 0 ) ) = ( S . ( 0 ) ) by SERIES_1:def 1;
then A14: P[ ( 0 ) ] by A3,A5,FUNCT_1:47;
(for m holds P[ m ]) from NAT_1:sch 2(A14,A8);
