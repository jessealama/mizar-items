constructors BINOP_1,XXREAL_0,XREAL_0,NAT_1,FINSEQ_4,TREES_4,TREES_9,QC_LANG2,RELSET_1,CKB3,CKB6,CKB21,CKB38,CKB39,CKB41;
requirements NUMERALS,REAL,BOOLE,SUBSET,ARITHM;
begin
reserve F for (Element of ( QC-WFF ));
reserve G1 for (Subformula of F);
reserve G2 for (Subformula of F);
reserve t1 for (Entry_Point_in_Subformula_Tree of G1);
reserve t2 for (Entry_Point_in_Subformula_Tree of G2);
theorem
(G2 is_subformula_of G1 implies (for t1 holds (ex t2 st t1 is_a_prefix_of t2)))
proof
assume A1: G2 is_subformula_of G1;
now
let t1;
consider H being (Element of ( QC-WFF )) such that A2: H = G2;
reconsider H as (Subformula of G1) by A1,A2,CKB38:def 1;
set s = the (Entry_Point_in_Subformula_Tree of H);
( ( tree_of_subformulae G1 ) . s ) = H by CKB39:def 1;
then s in ( G1 -entry_points_in_subformula_tree_of G2 ) by A2,CKB21:def 1;
then ( t1 ^ s ) is (Entry_Point_in_Subformula_Tree of G2) by CKB44:1;
then consider t2 such that A3: t2 = ( t1 ^ s );
take t2;
thus t1 is_a_prefix_of t2 by A3,TREES_1:1;
end;
hence thesis;
end;
