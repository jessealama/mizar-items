environ
vocabularies XBOOLE_0,SUBSET_1,ZFMISC_1,TARSKI,FINSET_1,FINSUB_1;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,FINSET_1,CKB1,CKB2,CKB3,CKB4,CKB9,CKB10,CKB16,CKB17;
definitions TARSKI,XBOOLE_0,CKB1,CKB2,CKB3,CKB4;
theorems TARSKI,ZFMISC_1,XBOOLE_0,XBOOLE_1,CKB1,CKB2,CKB3,CKB4,CKB8,CKB11,CKB12,CKB13,CKB14,CKB15,CKB18,CKB19;
schemes XBOOLE_0;
registrations XBOOLE_0,SUBSET_1,FINSET_1,CKB5,CKB6,CKB7,CKB20;
constructors TARSKI,SUBSET_1,FINSET_1,CKB1,CKB2,CKB3,CKB4,CKB9,CKB10,CKB16,CKB17;
requirements SUBSET,BOOLE;
begin
reserve X for set;
reserve Y for set;
reserve A for non  empty  preBoolean set;
theorem
(for A,B being non  empty  preBoolean set holds ( A /\ B ) is non  empty  preBoolean)
proof
let A being non  empty  preBoolean set;
let B being non  empty  preBoolean set;
(( {} ) in A & ( {} ) in B) by CKB18:1;
then reconsider C = ( A /\ B ) as non  empty set by XBOOLE_0:def 4;
now
let X being set;
let Y being set;
assume A1: (X in C & Y in C);
then A2: (X in B & Y in B) by XBOOLE_0:def 4;
then A3: ( X \/ Y ) in B by CKB8:1;
A4: ( X \ Y ) in B by A2,CKB8:1;
A5: (X in A & Y in A) by A1,XBOOLE_0:def 4;
then ( X \/ Y ) in A by CKB8:1;
hence ( X \/ Y ) in C by A3,XBOOLE_0:def 4;
( X \ Y ) in A by A5,CKB8:1;
hence ( X \ Y ) in C by A4,XBOOLE_0:def 4;
end;
hence thesis by CKB8:1;
end;
