environ
vocabularies XBOOLE_0,SUBSET_1,ZFMISC_1,TARSKI,FINSET_1,FINSUB_1;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,FINSET_1,CKB1,CKB2,CKB3,CKB4,CKB9,CKB10;
definitions TARSKI,XBOOLE_0,CKB1,CKB2,CKB3,CKB4;
theorems TARSKI,ZFMISC_1,XBOOLE_0,XBOOLE_1,CKB1,CKB2,CKB3,CKB4,CKB8,CKB11,CKB12,CKB13;
schemes XBOOLE_0;
registrations XBOOLE_0,SUBSET_1,FINSET_1,CKB5,CKB6,CKB7;
constructors TARSKI,SUBSET_1,FINSET_1,CKB1,CKB2,CKB3,CKB4,CKB9,CKB10;
requirements SUBSET,BOOLE;
begin
reserve X for set;
reserve Y for set;
reserve A for non  empty  preBoolean set;
theorem
(for A being non  empty set holds ((for X,Y being (Element of A) holds (( X \+\ Y ) in A & ( X /\ Y ) in A)) implies A is  preBoolean))
proof
let A being non  empty set;
assume that
A1: (for X,Y being (Element of A) holds (( X \+\ Y ) in A & ( X /\ Y ) in A));
now
let X being set;
let Y being set;
assume that
A2: X in A
and
A3: Y in A;
reconsider Z1 = ( X \+\ Y ),Z2 = ( X /\ Y ) as (Element of A) by A1,A2,A3;
( X \/ Y ) = ( Z1 \+\ Z2 ) by XBOOLE_1:94;
hence ( X \/ Y ) in A by A1;
( X \ Y ) = ( X \+\ Z2 ) by XBOOLE_1:100;
hence ( X \ Y ) in A by A1,A2;
end;
hence thesis by CKB8:1;
end;
