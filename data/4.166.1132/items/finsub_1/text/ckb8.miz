begin
theorem
Th10: (for A being set holds (A is  preBoolean iff (for X,Y being set holds ((X in A & Y in A) implies (( X \/ Y ) in A & ( X \ Y ) in A)))))
proof
let A being set;
thus (A is  preBoolean implies (for X,Y being set holds ((X in A & Y in A) implies (( X \/ Y ) in A & ( X \ Y ) in A)))) by CKB1:def 1,CKB3:def 1;
assume A1: (for X,Y being set holds ((X in A & Y in A) implies (( X \/ Y ) in A & ( X \ Y ) in A)));
A2: A is  diff-closed
proof
let X being set,Y being set;
assume (X in A & Y in A);
hence thesis by A1;
end;
A is  cup-closed
proof
let X being set,Y being set;
assume (X in A & Y in A);
hence thesis by A1;
end;
hence thesis by A2;
end;
