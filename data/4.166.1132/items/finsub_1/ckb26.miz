environ
vocabularies XBOOLE_0,SUBSET_1,ZFMISC_1,TARSKI,FINSET_1,FINSUB_1;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,FINSET_1,CKB1,CKB2,CKB3,CKB4,CKB9,CKB10,CKB16,CKB17,CKB22;
definitions TARSKI,XBOOLE_0,CKB1,CKB2,CKB3,CKB4,CKB22;
theorems TARSKI,ZFMISC_1,XBOOLE_0,XBOOLE_1,CKB1,CKB2,CKB3,CKB4,CKB8,CKB11,CKB12,CKB13,CKB14,CKB15,CKB18,CKB19,CKB21,CKB22,CKB25;
schemes XBOOLE_0;
registrations XBOOLE_0,SUBSET_1,FINSET_1,CKB5,CKB6,CKB7,CKB20,CKB23,CKB24;
constructors TARSKI,SUBSET_1,FINSET_1,CKB1,CKB2,CKB3,CKB4,CKB9,CKB10,CKB16,CKB17,CKB22;
requirements SUBSET,BOOLE;
begin
reserve X for set;
theorem
(for A,B being set holds ( Fin ( A /\ B ) ) = ( ( Fin A ) /\ ( Fin B ) ))
proof
let A being set,B being set;
(( Fin ( A /\ B ) ) c= ( Fin A ) & ( Fin ( A /\ B ) ) c= ( Fin B )) by CKB25:1,XBOOLE_1:17;
hence ( Fin ( A /\ B ) ) c= ( ( Fin A ) /\ ( Fin B ) ) by XBOOLE_1:19;
now
let X;
assume A1: X in ( ( Fin A ) /\ ( Fin B ) );
then X in ( Fin B ) by XBOOLE_0:def 4;
then A2: X c= B by CKB22:def 1;
A3: X in ( Fin A ) by A1,XBOOLE_0:def 4;
then X c= A by CKB22:def 1;
then X c= ( A /\ B ) by A2,XBOOLE_1:19;
hence X in ( Fin ( A /\ B ) ) by A3,CKB22:def 1;
end;
hence thesis by TARSKI:def 3;
end;
