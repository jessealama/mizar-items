begin
theorem
Th11: (for X being non empty set holds (for Y being RealNormSpace holds (for f,h being (VECTOR of ( R_VectorSpace_of_BoundedFunctions (X,Y) )) holds (for f9,h9 being  bounded (Function of X,(the carrier of Y)) holds ((f9 = f & h9 = h) implies (for a being Real holds (h = ( a * f ) iff (for x being (Element of X) holds ( h9 . x ) = ( a * ( f9 . x ) )))))))))
proof
let X being non empty set;
let Y being RealNormSpace;
let f being (VECTOR of ( R_VectorSpace_of_BoundedFunctions (X,Y) )),h being (VECTOR of ( R_VectorSpace_of_BoundedFunctions (X,Y) ));
let f9 being  bounded (Function of X,(the carrier of Y)),h9 being  bounded (Function of X,(the carrier of Y));
assume that
A1: (f9 = f & h9 = h);
let a being Real;
A2: ( R_VectorSpace_of_BoundedFunctions (X,Y) ) is (Subspace of ( RealVectSpace (X,Y) )) by CKB24:1,RSSPACE:11;
then reconsider f1 = f,h1 = h as (VECTOR of ( RealVectSpace (X,Y) )) by RLSUB_1:10;
A3:now
assume A4: h = ( a * f );
let x being (Element of X);
h1 = ( a * f1 ) by A2,A4,RLSUB_1:14;
hence ( h9 . x ) = ( a * ( f9 . x ) ) by A1,LOPBAN_1:12;
end;
now
assume (for x being (Element of X) holds ( h9 . x ) = ( a * ( f9 . x ) ));
then h1 = ( a * f1 ) by A1,LOPBAN_1:12;
hence h = ( a * f ) by A2,RLSUB_1:14;
end;
hence thesis by A3;
end;
