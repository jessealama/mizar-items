begin
theorem
Th10: (for X being non  empty set holds (for Y being RealNormSpace holds (for f,g,h being (VECTOR of ( R_VectorSpace_of_BoundedFunctions (X,Y) )) holds (for f9,g9,h9 being  bounded (Function of X,(the carrier of Y)) holds (((f9 = f & g9 = g) & h9 = h) implies (h = ( f + g ) iff (for x being (Element of X) holds ( h9 . x ) = ( ( f9 . x ) + ( g9 . x ) ))))))))
proof
let X being non  empty set;
let Y being RealNormSpace;
let f being (VECTOR of ( R_VectorSpace_of_BoundedFunctions (X,Y) ));
let g being (VECTOR of ( R_VectorSpace_of_BoundedFunctions (X,Y) ));
let h being (VECTOR of ( R_VectorSpace_of_BoundedFunctions (X,Y) ));
A1: ( R_VectorSpace_of_BoundedFunctions (X,Y) ) is (Subspace of ( RealVectSpace (X,Y) )) by CKB24:1,RSSPACE:11;
then reconsider f1 = f as (VECTOR of ( RealVectSpace (X,Y) )) by RLSUB_1:10;
reconsider h1 = h as (VECTOR of ( RealVectSpace (X,Y) )) by A1,RLSUB_1:10;
reconsider g1 = g as (VECTOR of ( RealVectSpace (X,Y) )) by A1,RLSUB_1:10;
let f9 being  bounded (Function of X,(the carrier of Y));
let g9 being  bounded (Function of X,(the carrier of Y));
let h9 being  bounded (Function of X,(the carrier of Y));
assume that
A2: ((f9 = f & g9 = g) & h9 = h);
A3:now
assume A4: h = ( f + g );
let x being (Element of X);
h1 = ( f1 + g1 ) by A1,A4,RLSUB_1:13;
hence ( h9 . x ) = ( ( f9 . x ) + ( g9 . x ) ) by A2,LOPBAN_1:11;
end;
now
assume (for x being (Element of X) holds ( h9 . x ) = ( ( f9 . x ) + ( g9 . x ) ));
then h1 = ( f1 + g1 ) by A2,LOPBAN_1:11;
hence h = ( f + g ) by A1,RLSUB_1:13;
end;
hence thesis by A3;
end;
