begin
registration
cluster ( the_set_of_BoundedRealSequences ) ->  linearly-closed;
coherence
proof
set W = ( the_set_of_BoundedRealSequences );
A1: (for a being Real holds (for v being (VECTOR of ( Linear_Space_of_RealSequences )) holds (v in W implies ( a * v ) in W)))
proof
let a being Real;
let v being (VECTOR of ( Linear_Space_of_RealSequences ));
assume v in W;
then A2: ( seq_id v ) is  bounded by CKB3:def 1;
( seq_id ( a * v ) ) = ( seq_id ( a (#) ( seq_id v ) ) ) by RSSPACE:3,RSSPACE:def 7
.= ( a (#) ( seq_id v ) ) by RSSPACE:1;
then ( seq_id ( a * v ) ) is  bounded by A2,SEQM_3:37;
hence thesis by CKB3:def 1,RSSPACE:def 7;
end;
(for v,u being (VECTOR of ( Linear_Space_of_RealSequences )) holds ((v in ( the_set_of_BoundedRealSequences ) & u in ( the_set_of_BoundedRealSequences )) implies ( v + u ) in ( the_set_of_BoundedRealSequences )))
proof
let v being (VECTOR of ( Linear_Space_of_RealSequences ));
let u being (VECTOR of ( Linear_Space_of_RealSequences ));
assume (v in W & u in W);
then A3: (( seq_id v ) is  bounded & ( seq_id u ) is  bounded) by CKB3:def 1;
( seq_id ( v + u ) ) = ( seq_id ( ( seq_id v ) + ( seq_id u ) ) ) by RSSPACE:2,RSSPACE:def 7
.= ( ( seq_id v ) + ( seq_id u ) ) by RSSPACE:1;
hence thesis by A3,CKB3:def 1,RSSPACE:def 7;
end;
hence thesis by A1,RLSUB_1:def 1;
end;
end;
