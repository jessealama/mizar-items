theorem
Lm1: (for rseq being Real_Sequence holds (for K being  real number holds ((for n being (Element of ( NAT )) holds ( rseq . n ) <= K) implies ( upper_bound ( rng rseq ) ) <= K)))
proof
let rseq being Real_Sequence;
let K being  real number;
assume that
A1: (for n being (Element of ( NAT )) holds ( rseq . n ) <= K);
now
let s being  real number;
assume s in ( rng rseq );
then (ex n being set st (n in ( NAT ) & ( rseq . n ) = s)) by FUNCT_2:11;
hence s <= K by A1;
end;
hence thesis by SEQ_4:45;
end;
