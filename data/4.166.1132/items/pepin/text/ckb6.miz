begin
reserve m for Nat;
reserve p for Nat;
theorem
Th2: (for p being Nat holds (p is  prime implies (m,p are_relative_prime  or ( m gcd p ) = p)))
proof
let p being Nat;
A1: ( m gcd p ) divides p by NAT_D:def 5;
assume p is  prime;
then (( m gcd p ) = 1 or ( m gcd p ) = p) by A1,INT_2:def 4;
hence thesis by INT_2:def 3;
end;
