theorem
Th32: (for seq holds ((for k holds ( seq . k ) = ( ( Partial_Sums ( Conj (k,z,w) ) ) . k )) implies (seq is  convergent & ( lim seq ) = ( 0. X ))))
proof
deffunc U((Element of ( NAT ))) = ( ( Partial_Sums ||. ( Conj ($1,z,w) ) .|| ) . $1 );
(ex rseq being Real_Sequence st (for k holds ( rseq . k ) = U(k))) from SEQ_1:sch 1;
then consider rseq being Real_Sequence such that A1: (for k holds ( rseq . k ) = ( ( Partial_Sums ||. ( Conj (k,z,w) ) .|| ) . k ));
let seq;
assume that
A2: (for k holds ( seq . k ) = ( ( Partial_Sums ( Conj (k,z,w) ) ) . k ));
A3:now
let k;
||. ( seq . k ) .|| = ||. ( ( Partial_Sums ( Conj (k,z,w) ) ) . k ) .|| by A2;
hence ||. ( seq . k ) .|| <= ( ( Partial_Sums ||. ( Conj (k,z,w) ) .|| ) . k ) by CKB12:1;
end;
A4:now
let k;
||. ( seq . k ) .|| <= ( ( Partial_Sums ||. ( Conj (k,z,w) ) .|| ) . k ) by A3;
hence ||. ( seq . k ) .|| <= ( rseq . k ) by A1;
end;
A5:now
let p being  real number;
assume p > ( 0 );
then consider n such that A6: (for k holds (n <= k implies ( abs ( ( Partial_Sums ||. ( Conj (k,z,w) ) .|| ) . k ) ) < p)) by CKB40:1;
take n;
now
let k;
assume that
A7: n <= k;
( abs ( ( rseq . k ) - ( 0 ) ) ) = ( abs ( ( Partial_Sums ||. ( Conj (k,z,w) ) .|| ) . k ) ) by A1;
hence ( abs ( ( rseq . k ) - ( 0 ) ) ) < p by A6,A7;
end;
hence (for k holds (n <= k implies ( abs ( ( rseq . k ) - ( 0 ) ) ) < p));
end;
then A8: rseq is  convergent by SEQ_2:def 6;
then ( lim rseq ) = ( 0 ) by A5,SEQ_2:def 7;
hence thesis by A4,A8,CKB14:1;
end;
