constructors ARYTM_0,REAL_1,SQUARE_1,NAT_1,SEQ_2,COMSEQ_3,NAT_D,SIN_COS,BHSP_4,LOPBAN_4,CSSPACE3,CLOPBAN3,SEQ_1,SERIES_1,SEQ_4,NORMSP_1,RELSET_1,CKB15;
requirements SUBSET,REAL,BOOLE,NUMERALS,ARITHM;
begin
reserve k for (Element of ( NAT ));
reserve n for (Element of ( NAT ));
scheme ExNormSpaceCASE { CNS() -> non empty Complex_Banach_Algebra,F((Element of ( NAT )),(Element of ( NAT ))) -> (Point of CNS()) } : (for k holds (ex seq being (sequence of CNS()) st (for n holds ((n <= k implies ( seq . n ) = F(k,n)) & (n > k implies ( seq . n ) = ( 0. CNS() ))))))
proof
let k;
defpred P[ set,set ]
 means
(ex n st ((n = $1 & (n <= k implies $2 = F(k,n))) & (n > k implies $2 = ( 0. CNS() ))));
A1:now
let x being set;
assume x in ( NAT );
then consider n such that A2: n = x;
reconsider y = ( ( CHK (n,k) ) * F(k,n) ) as set;
A3:now
assume n > k;
hence ( ( CHK (n,k) ) * F(k,n) ) = ( ( 0c ) * F(k,n) ) by SIN_COS:def 1
.= ( 0. CNS() ) by CLVECT_1:1;
end;
take y;
now
assume n <= k;
hence ( ( CHK (n,k) ) * F(k,n) ) = ( ( 1r ) * F(k,n) ) by COMPLEX1:def 4,SIN_COS:def 1
.= F(k,n) by CLVECT_1:def 5;
end;
hence P[ x,y ] by A2,A3;
end;
consider f being Function such that A4: ( dom f ) = ( NAT ) and A5: (for x being set holds (x in ( NAT ) implies P[ x,( f . x ) ])) from CLASSES1:sch 1(A1);
now
let x being set;
assume x in ( NAT );
then (ex n st ((n = x & (n <= k implies ( f . x ) = F(k,n))) & (n > k implies ( f . x ) = ( 0. CNS() )))) by A5;
hence ( f . x ) in (the carrier of CNS());
end;
then reconsider f as (sequence of CNS()) by A4,FUNCT_2:3;
take seq = f;
let n;
(ex l being (Element of ( NAT )) st ((l = n & (l <= k implies ( seq . n ) = F(k,l))) & (l > k implies ( seq . n ) = ( 0. CNS() )))) by A5;
hence thesis;
end;
