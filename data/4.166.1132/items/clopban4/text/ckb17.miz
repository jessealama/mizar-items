assume that
A1: (for k holds ((k <= n implies ( seq1 . k ) = ( ( ( ( Coef n ) . k ) * ( z #N k ) ) * ( w #N ( n -' k ) ) )) & (k > n implies ( seq1 . k ) = ( 0. X ))))
