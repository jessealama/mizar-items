registrations ORDINAL1,RELSET_1,NUMBERS,XCMPLX_0,XXREAL_0,XREAL_0,MEMBERED,STRUCT_0,CLOPBAN2,CLOPBAN3,VALUED_0,SEQ_2,XBOOLE_0,SEQ_4,NEWTON,NAT_1;
constructors ARYTM_0,REAL_1,SQUARE_1,NAT_1,SEQ_2,COMSEQ_3,NAT_D,SIN_COS,BHSP_4,LOPBAN_4,CSSPACE3,CLOPBAN3,SEQ_1,SERIES_1,SEQ_4,NORMSP_1,RELSET_1;
requirements SUBSET,REAL,BOOLE,NUMERALS,ARITHM;
begin
reserve X for Complex_Banach_Algebra;
reserve z for (Element of X);
reserve m for (Element of ( NAT ));
reserve n for (Element of ( NAT ));
reserve s for (sequence of X);
theorem
Th6: (s is  convergent implies ( lim ( z * s ) ) = ( z * ( lim s ) ))
proof
assume A1: s is  convergent;
set g1 = ( lim s );
set g = ( z * g1 );
A2: ( ( 0 ) + ( 0 ) ) < ( ||. z .|| + 1 ) by CLVECT_1:105,XREAL_1:8;
A3: ( 0 ) <= ||. z .|| by CLVECT_1:105;
A4:now
let p being Real;
assume A5: ( 0 ) < p;
then consider n such that A6: (for m holds (n <= m implies ||. ( ( s . m ) - g1 ) .|| < ( p / ( ||. z .|| + 1 ) ))) by A1,A2,CLVECT_1:def 16;
take n;
let m;
assume n <= m;
then A7: ||. ( ( s . m ) - g1 ) .|| < ( p / ( ||. z .|| + 1 ) ) by A6;
( 0 ) <= ||. ( ( s . m ) - g1 ) .|| by CLVECT_1:105;
then A8: ( ||. z .|| * ||. ( ( s . m ) - g1 ) .|| ) <= ( ||. z .|| * ( p / ( ||. z .|| + 1 ) ) ) by A3,A7,XREAL_1:66;
||. ( z * ( ( s . m ) - g1 ) ) .|| <= ( ||. z .|| * ||. ( ( s . m ) - g1 ) .|| ) by CLOPBAN3:38;
then A9: ||. ( z * ( ( s . m ) - g1 ) ) .|| <= ( ||. z .|| * ( p / ( ||. z .|| + 1 ) ) ) by A8,XXREAL_0:2;
A10: ||. ( ( ( z * s ) . m ) - g ) .|| = ||. ( ( z * ( s . m ) ) - ( z * g1 ) ) .|| by CLOPBAN3:def 4
.= ||. ( z * ( ( s . m ) - g1 ) ) .|| by CLOPBAN3:38;
( ( 0 ) + ||. z .|| ) < ( ||. z .|| + 1 ) by XREAL_1:8;
then A11: ( ||. z .|| * ( p / ( ||. z .|| + 1 ) ) ) < ( ( ||. z .|| + 1 ) * ( p / ( ||. z .|| + 1 ) ) ) by A3,A5,XREAL_1:97;
( ( ||. z .|| + 1 ) * ( p / ( ||. z .|| + 1 ) ) ) = p by A2,XCMPLX_1:87;
hence ||. ( ( ( z * s ) . m ) - g ) .|| < p by A10,A9,A11,XXREAL_0:2;
end;
( z * s ) is  convergent by A1,CKB6:1;
hence thesis by A4,CLVECT_1:def 16;
end;
