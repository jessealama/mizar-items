environ
vocabularies FUNCT_1,PBOOLE,RELAT_1,FUNCOP_1,CARD_3,TARSKI,FUNCT_6,MEMBER_1,XBOOLE_0,FINSEQ_1,MSUALG_1,STRUCT_0,PROB_2,MSAFREE,ZFMISC_1,CARD_1,FINSEQ_2,MCART_1,SUBSET_1,ORDINAL4,MSUALG_6,CAT_5,INSTALG1,NUMBERS,MARGREL1,CAT_1,PUA2MSS1,GRAPH_1,PARTFUN1,MSUALG_3,CATALG_1;
notations TARSKI,XBOOLE_0,ENUMSET1,ZFMISC_1,SUBSET_1,NUMBERS,NAT_1,MCART_1,RELAT_1,FUNCT_1,RELSET_1,PARTFUN1,FUNCT_2,FUNCOP_1,MSAFREE1,FINSEQ_1,FINSEQ_2,CARD_3,FINSEQ_4,FUNCT_6,STRUCT_0,PBOOLE,MSUALG_1,MSUALG_3,MSAFREE,PUA2MSS1,MSUALG_6,INSTALG1,GRAPH_1,CAT_1,XXREAL_0,CKB1,CKB5,CKB10,CKB13,CKB14,CKB17,CKB18,CKB23;
definitions TARSKI,STRUCT_0,MSUALG_1,MSUALG_3,PUA2MSS1,MSUALG_6,INSTALG1,PROB_2,MSAFREE1,XBOOLE_0,FUNCOP_1,PBOOLE,CKB1,CKB5,CKB10,CKB14,CKB18;
theorems TARSKI,ZFMISC_1,RELAT_1,FUNCT_1,FUNCT_2,FINSEQ_1,FINSEQ_2,FINSEQ_3,FUNCT_3,MCART_1,ENUMSET1,FUNCT_6,FINSEQ_4,MONOID_1,CARD_3,PBOOLE,PRALG_2,MSUALG_3,INSTALG1,CAT_1,RELSET_1,XBOOLE_0,XBOOLE_1,FUNCOP_1,PARTFUN1,CARD_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB10,CKB14,CKB18,CKB19;
schemes FUNCT_1,CLASSES1,PBOOLE,XBOOLE_0;
registrations XBOOLE_0,SUBSET_1,RELAT_1,FUNCT_1,ORDINAL1,FUNCT_2,FUNCOP_1,XREAL_0,FINSEQ_1,FINSEQ_2,STRUCT_0,MSUALG_1,RELSET_1,MSAFREE,INSTALG1,MSAFREE1,MSSUBFAM,PBOOLE,CARD_1,CKB6,CKB7,CKB8,CKB9,CKB11,CKB12,CKB15,CKB16,CKB20,CKB21,CKB22;
constructors ENUMSET1,XXREAL_0,NAT_1,FINSEQ_4,MSSUBFAM,CAT_1,MSUALG_3,MSATERM,MSAFREE1,PUA2MSS1,MSUALG_6,INSTALG1,XREAL_0,RELSET_1,CKB1,CKB5,CKB10,CKB14,CKB18,CKB23;
requirements NUMERALS,BOOLE,SUBSET,ARITHM;
begin
definition
let S being ManySortedSign;
func underlay S means 
:Def8: (for x being set holds (x in it iff (ex a being set st (ex f being Function st ([ a,f ] in ( (the carrier of S) \/ (the carrier' of S) ) & x in ( rng f ))))));
existence
proof
set A = ( (the carrier of S) \/ (the carrier' of S) );
take X = ( proj2 ( union ( SubFuncs ( proj2 A ) ) ) );
let x being set;
hereby
assume x in X;
then consider a being set such that A1: [ a,x ] in ( union ( SubFuncs ( proj2 A ) ) ) by RELAT_1:def 5;
consider b being set such that A2: [ a,x ] in b and A3: b in ( SubFuncs ( proj2 A ) ) by A1,TARSKI:def 4;
reconsider b as Function by A3,FUNCT_6:def 1;
b in ( proj2 A ) by A3,FUNCT_6:def 1;
then consider c being set such that A4: [ c,b ] in A by RELAT_1:def 5;
take c;
take b;
thus ([ c,b ] in A & x in ( rng b )) by A2,A4,RELAT_1:def 5;
end;
given a being set,f being Function such that
A5: [ a,f ] in ( (the carrier of S) \/ (the carrier' of S) )
and
A6: x in ( rng f );

consider b being set such that A7: [ b,x ] in f by A6,RELAT_1:def 5;
f in ( proj2 A ) by A5,RELAT_1:def 5;
then f in ( SubFuncs ( proj2 A ) ) by FUNCT_6:def 1;
then [ b,x ] in ( union ( SubFuncs ( proj2 A ) ) ) by A7,TARSKI:def 4;
hence thesis by RELAT_1:def 5;
end;
uniqueness
proof
defpred P[ set ]
 means
(ex a being set st (ex f being Function st ([ a,f ] in ( (the carrier of S) \/ (the carrier' of S) ) & $1 in ( rng f ))));
thus (for X1,X2 being set holds (((for x being set holds (x in X1 iff P[ x ])) & (for x being set holds (x in X2 iff P[ x ]))) implies X1 = X2)) from XBOOLE_0:sch 3;
end;
end;
