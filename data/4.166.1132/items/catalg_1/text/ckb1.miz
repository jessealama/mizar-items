requirements NUMERALS,BOOLE,SUBSET,ARITHM;
begin
definition
let I being set;
let A being Function;
let f being Function;
func f -MSF (I,A) -> (ManySortedFunction of I) means 
:Def1: (for i being set holds (i in I implies ( it . i ) = ( f | ( A . i ) )));
existence
proof
deffunc f(set) = ( f | ( A . $1 ) );
consider F being (ManySortedSet of I) such that A1: (for i being set holds (i in I implies ( F . i ) = f(i))) from PBOOLE:sch 4;
F is  Function-yielding
proof
let x being set;
assume x in ( dom F );
then x in I by PARTFUN1:def 2;
then ( F . x ) = ( f | ( A . x ) ) by A1;
hence thesis;
end;
hence thesis by A1;
end;
uniqueness
proof
let g1 being (ManySortedFunction of I);
let g2 being (ManySortedFunction of I);
assume that
A2: (for i being set holds (i in I implies ( g1 . i ) = ( f | ( A . i ) )))
and
A3: (for i being set holds (i in I implies ( g2 . i ) = ( f | ( A . i ) )));
now
let i being set;
assume A4: i in I;
then ( g1 . i ) = ( f | ( A . i ) ) by A2;
hence ( g1 . i ) = ( g2 . i ) by A3,A4;
end;
hence thesis by PBOOLE:3;
end;
end;
