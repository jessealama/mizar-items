begin
scheme CatAlgEx { X,Y() -> non  empty set,H(set,set) -> set,R(set,set,set,set,set) -> set,I(set) -> set } : (ex A being  strict MSAlgebra over ( CatSign X() ) st (((for a,b being (Element of X()) holds ( (the Sorts of A) . ( homsym (a,b) ) ) = H(a,b)) & (for a being (Element of X()) holds ( ( Den (( idsym a ),A) ) . ( {} ) ) = I(a))) & (for a,b,c being (Element of X()) holds (for f,g being (Element of Y()) holds ((f in H(a,b) & g in H(b,c)) implies ( ( Den (( compsym (a,b,c) ),A) ) . <* g,f *> ) = R(a,b,c,g,f))))))
provided
A1: (for a,b being (Element of X()) holds H(a,b) c= Y())
and
A2: (for a being (Element of X()) holds I(a) in H(a,a))
and
A3: (for a,b,c being (Element of X()) holds (for f,g being (Element of Y()) holds ((f in H(a,b) & g in H(b,c)) implies R(a,b,c,g,f) in H(a,c))))
proof
defpred Z[ set,set ]
 means
((( $1 `1 ) = 1 & (ex a being (Element of X()) st ($1 = ( idsym a ) & (ex F being (Function of { ( {} ) },H(a,a)) st (F = $2 & ( F . ( {} ) ) = I(a)))))) or (( $1 `1 ) = 2 & (ex a,b,c being (Element of X()) st ($1 = ( compsym (a,b,c) ) & (ex F being (Function of ( product <* H(b,c),H(a,b) *> ),H(a,c)) st (F = $2 & (for f,g being set holds ((f in H(a,b) & g in H(b,c)) implies ( F . <* g,f *> ) = R(a,b,c,g,f)))))))));
set CS = ( CatSign X() );
A4: (for o being set holds (o in (the carrier' of CS) implies (ex u being set st Z[ o,u ])))
proof
let o being set;
assume A5: o in (the carrier' of CS);
A6:now
assume ( o `1 ) = 1;
then consider a being (Element of X()) such that A7: o = ( idsym a ) by A5,CKB52:1;
set F = ( ( {} ) :-> I(a) );
reconsider u = F as set;
take u;
take a;
thus o = ( idsym a ) by A7;
I(a) in H(a,a) by A2;
then { I(a) } c= H(a,a) by ZFMISC_1:31;
then (( dom F ) = { ( {} ) } & ( rng F ) c= H(a,a)) by FUNCOP_1:13,XBOOLE_1:1;
then reconsider F as (Function of { ( {} ) },H(a,a)) by FUNCT_2:2;
take F;
( {} ) in { ( {} ) } by TARSKI:def 1;
hence (F = u & ( F . ( {} ) ) = I(a)) by FUNCOP_1:7;
end;
A8:now
assume ( o `1 ) = 2;
then consider a,b,c being (Element of X()) such that A9: o = ( compsym (a,b,c) ) by A5,CKB53:1;
defpred P[ set,set ]
 means
(ex f,g being set st (((f in H(a,b) & g in H(b,c)) & $1 = <* g,f *>) & $2 = R(a,b,c,g,f)));
A10:now
let x being set;
assume x in ( product <* H(b,c),H(a,b) *> );
then consider g,f being set such that A11: (g in H(b,c) & f in H(a,b)) and A12: x = <* g,f *> by FINSEQ_3:124;
take u = R(a,b,c,g,f);
(H(a,b) c= Y() & H(b,c) c= Y()) by A1;
hence u in H(a,c) by A3,A11;
thus P[ x,u ] by A11,A12;
end;
consider F being Function such that A13: (( dom F ) = ( product <* H(b,c),H(a,b) *> ) & ( rng F ) c= H(a,c)) and A14: (for x being set holds (x in ( product <* H(b,c),H(a,b) *> ) implies P[ x,( F . x ) ])) from FUNCT_1:sch 5(A10);
reconsider u = F as set;
take u;
take a;
take b;
take c;
thus o = ( compsym (a,b,c) ) by A9;
reconsider F as (Function of ( product <* H(b,c),H(a,b) *> ),H(a,c)) by A13,FUNCT_2:2;
take F;
thus F = u;
let f being set;
let g being set;
assume (f in H(a,b) & g in H(b,c));
then <* g,f *> in ( product <* H(b,c),H(a,b) *> ) by FINSEQ_3:124;
then consider f9,g9 being set such that f9 in H(a,b) and g9 in H(b,c) and A15: <* g,f *> = <* g9,f9 *> and A16: ( F . <* g,f *> ) = R(a,b,c,g9,f9) by A14;
A17: (( <* g,f *> . 1 ) = g & ( <* g,f *> . 2 ) = f) by FINSEQ_1:44;
( <* g,f *> . 1 ) = g9 by A15,FINSEQ_1:44;
hence ( F . <* g,f *> ) = R(a,b,c,g,f) by A15,A16,A17,FINSEQ_1:44;
end;
(( o `1 ) = 1 or ( o `1 ) = 2) by A5,CKB51:1;
hence thesis by A6,A8;
end;
consider O being Function such that A18: ( dom O ) = (the carrier' of CS) and A19: (for o being set holds (o in (the carrier' of CS) implies Z[ o,( O . o ) ])) from CLASSES1:sch 1(A4);
reconsider O as (ManySortedSet of (the carrier' of CS)) by A18,PARTFUN1:def 2,RELAT_1:def 18;
defpred P[ set,set ]
 means
(ex a,b being (Element of X()) st ($1 = ( homsym (a,b) ) & $2 = H(a,b)));
A20:now
let s being set;
assume s in (the carrier of CS);
then consider a,b being (Element of X()) such that A21: s = ( homsym (a,b) ) by CKB50:1;
take u = H(a,b);
thus P[ s,u ] by A21;
end;
consider S being Function such that A22: ( dom S ) = (the carrier of CS) and A23: (for s being set holds (s in (the carrier of CS) implies P[ s,( S . s ) ])) from CLASSES1:sch 1(A20);
reconsider S as (ManySortedSet of (the carrier of CS)) by A22,PARTFUN1:def 2,RELAT_1:def 18;
O is (ManySortedFunction of ( ( S # ) * (the Arity of CS) ),( S * (the ResultSort of CS) ))
proof
let o being set;
assume o in (the carrier' of CS);
then reconsider o as (OperSymbol of CS);
A24: ( ( ( S # ) * (the Arity of CS) ) . o ) = ( ( S # ) . ( the_arity_of o ) ) by FUNCT_2:15
.= ( product ( S * ( the_arity_of o ) ) ) by FINSEQ_2:def 5;
A25: ( ( S * (the ResultSort of CS) ) . o ) = ( S . ( the_result_sort_of o ) ) by FUNCT_2:15;
per cases  by CKB51:1;
suppose (( o `1 ) = 1 & 1 <> 2);

then consider a being (Element of X()) such that A26: (o = ( idsym a ) & (ex F being (Function of { ( {} ) },H(a,a)) st (F = ( O . o ) & ( F . ( {} ) ) = I(a)))) by A19;
A27: (( the_arity_of ( idsym a ) ) = ( {} ) & ( {} ) = ( S * ( {} ) )) by CKB10:def 1;
A28: ( the_result_sort_of ( idsym a ) ) = ( homsym (a,a) ) by CKB10:def 1;
consider c,b being (Element of X()) such that A29: ( homsym (a,a) ) = ( homsym (c,b) ) and A30: ( S . ( homsym (a,a) ) ) = H(c,b) by A23;
(a = b & a = c) by A29,CKB48:1;
hence thesis by A24,A25,A26,A30,A27,A28,CARD_3:10;
end;
suppose (( o `1 ) = 2 & 1 <> 2);

then consider a,b,c being (Element of X()) such that A31: o = ( compsym (a,b,c) ) and A32: (ex F being (Function of ( product <* H(b,c),H(a,b) *> ),H(a,c)) st (F = ( O . o ) & (for f,g being set holds ((f in H(a,b) & g in H(b,c)) implies ( F . <* g,f *> ) = R(a,b,c,g,f))))) by A19;
A33: ( the_result_sort_of ( compsym (a,b,c) ) ) = ( homsym (a,c) ) by CKB10:def 1;
consider ax,cx being (Element of X()) such that A34: ( homsym (a,c) ) = ( homsym (ax,cx) ) and A35: ( S . ( homsym (a,c) ) ) = H(ax,cx) by A23;
(ax = a & cx = c) by A34,CKB48:1;
then A36: ( ( S * (the ResultSort of CS) ) . o ) = H(a,c) by A31,A35,A33,FUNCT_2:15;
A37: ( the_arity_of ( compsym (a,b,c) ) ) = <* ( homsym (b,c) ),( homsym (a,b) ) *> by CKB10:def 1;
consider a9,b9 being (Element of X()) such that A38: ( homsym (a,b) ) = ( homsym (a9,b9) ) and A39: ( S . ( homsym (a,b) ) ) = H(a9,b9) by A23;
consider b99,c9 being (Element of X()) such that A40: ( homsym (b,c) ) = ( homsym (b99,c9) ) and A41: ( S . ( homsym (b,c) ) ) = H(b99,c9) by A23;
A42: (b99 = b & c9 = c) by A40,CKB48:1;
(a9 = a & b9 = b) by A38,CKB48:1;
then ( ( ( S # ) * (the Arity of CS) ) . o ) = ( product <* H(b,c),H(a,b) *> ) by A22,A24,A31,A39,A41,A42,A37,FINSEQ_2:125;
hence thesis by A32,A36;
end;
end;
then reconsider O as (ManySortedFunction of ( ( S # ) * (the Arity of CS) ),( S * (the ResultSort of CS) ));
take A = MSAlgebra (# S,O #);
hereby
let a being (Element of X());
let b being (Element of X());
consider a9,b9 being (Element of X()) such that A43: ( homsym (a,b) ) = ( homsym (a9,b9) ) and A44: ( S . ( homsym (a,b) ) ) = H(a9,b9) by A23;
a = a9 by A43,CKB48:1;
hence ( (the Sorts of A) . ( homsym (a,b) ) ) = H(a,b) by A43,A44,CKB48:1;
end;
hereby
let a being (Element of X());
( ( idsym a ) `1 ) = 1 by MCART_1:7;
then (ex b being (Element of X()) st (( idsym a ) = ( idsym b ) & (ex F being (Function of { ( {} ) },H(b,b)) st (F = ( O . ( idsym a ) ) & ( F . ( {} ) ) = I(b))))) by A19;
hence ( ( Den (( idsym a ),A) ) . ( {} ) ) = I(a) by CKB47:1;
end;
let a being (Element of X());
let b being (Element of X());
let c being (Element of X());
set o = ( compsym (a,b,c) );
( o `1 ) = 2 by MCART_1:7;
then consider a9,b9,c9 being (Element of X()) such that A45: o = ( compsym (a9,b9,c9) ) and A46: (ex F being (Function of ( product <* H(b9,c9),H(a9,b9) *> ),H(a9,c9)) st (F = ( O . o ) & (for f,g being set holds ((f in H(a9,b9) & g in H(b9,c9)) implies ( F . <* g,f *> ) = R(a9,b9,c9,g,f))))) by A19;
A47: c = c9 by A45,CKB49:1;
let f being (Element of Y());
let g being (Element of Y());
assume A48: (f in H(a,b) & g in H(b,c));
(a = a9 & b = b9) by A45,CKB49:1;
hence thesis by A46,A47,A48;
end;
