hence (z in R3 or z in ( LSeg (f,k) )) by A13,A14;
end;
end;
hence thesis by XBOOLE_0:def 3;
end;
then R2 = ( R3 \/ ( LSeg (f,k) ) ) by A7,XBOOLE_0:def 10;
hence thesis by A5,A3,TOPS_1:49;
end;

