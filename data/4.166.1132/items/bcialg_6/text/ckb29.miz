begin
reserve X for BCI-algebra;
reserve a for (Element of ( AtomSet X ));
reserve i for Integer;
reserve j for Integer;
theorem
Th22: ( ( a |^ i ) \ ( a |^ j ) ) = ( a |^ ( i - j ) )
proof
per cases ;
suppose A1: i > ( 0 );

per cases ;
suppose j > ( 0 );

hence thesis by A1,CKB28:1;
end;
suppose A2: j = ( 0 );

( a |^ ( i - ( 0 ) ) ) = ( ( a |^ i ) \ ( 0. X ) ) by BCIALG_1:2
.= ( ( a |^ i ) \ ( a |^ ( 0 ) ) ) by CKB2:def 1;
hence thesis by A2;
end;
suppose A3: j < ( 0 );

set m = ( - j );
reconsider i,m as (Element of ( NAT )) by A1,A3,INT_1:3;
( a |^ j ) = ( ( BCI-power X ) . (( a ` ),( abs j )) ) by A3,CKB3:def 1
.= ( ( a ` ) |^ m ) by A3,ABSVALUE:def 1
.= ( ( a |^ m ) ` ) by CKB23:1;
then ( ( a |^ i ) \ ( a |^ j ) ) = ( a |^ ( i + m ) ) by CKB19:1;
hence thesis;
end;
end;
suppose A4: i = ( 0 );

per cases ;
suppose j >= ( 0 );

then reconsider j as (Element of ( NAT )) by INT_1:3;
( ( a |^ ( 0 ) ) \ ( a |^ j ) ) = ( ( a |^ j ) ` ) by CKB2:def 1
.= ( ( a ` ) |^ j ) by CKB23:1
.= ( a |^ ( - j ) ) by CKB14:1;
hence thesis by A4;
end;
suppose A5: j < ( 0 );

set m = ( - j );
reconsider m as (Element of ( NAT )) by A5,INT_1:3;
( a |^ j ) = ( ( BCI-power X ) . (( a ` ),( abs j )) ) by A5,CKB3:def 1
.= ( ( a ` ) |^ m ) by A5,ABSVALUE:def 1
.= ( ( a |^ m ) ` ) by CKB23:1;
then ( ( a |^ ( 0 ) ) \ ( a |^ j ) ) = ( a |^ ( ( 0 ) + m ) ) by CKB19:1;
hence thesis by A4;
end;
end;
suppose A6: i < ( 0 );

then reconsider m = ( - i ) as (Element of ( NAT )) by INT_1:3;
A7: ( - i ) > ( 0 ) by A6,XREAL_1:58;
per cases ;
suppose A8: j >= ( 0 );

set n = ( - ( i - j ) );
reconsider n,j as (Element of ( NAT )) by A6,A8,INT_1:3;
reconsider b = ( a ` ) as (Element of ( AtomSet X )) by BCIALG_1:34;
A9: ( ( ( a ` ) |^ j ) ` ) = ( ( b ` ) |^ j ) by CKB23:1
.= ( a |^ j ) by BCIALG_1:29;
A10: ( a |^ i ) = ( ( BCI-power X ) . (( a ` ),( abs i )) ) by A6,CKB3:def 1
.= ( ( a ` ) |^ m ) by A6,ABSVALUE:def 1;
( a |^ ( i - j ) ) = ( ( BCI-power X ) . (( a ` ),( abs ( i - j ) )) ) by A6,CKB3:def 1
.= ( ( a ` ) |^ n ) by A6,ABSVALUE:def 1
.= ( b |^ ( j + m ) )
.= ( ( ( a ` ) |^ m ) \ ( ( ( a ` ) |^ j ) ` ) ) by CKB19:1;
hence thesis by A10,A9;
end;
suppose A11: j < ( 0 );

reconsider b = ( a ` ) as (Element of ( AtomSet X )) by BCIALG_1:34;
A12: ( - j ) > ( 0 ) by A11,XREAL_1:58;
reconsider n = ( - j ) as (Element of ( NAT )) by A11,INT_1:3;
A13: ( a |^ j ) = ( ( BCI-power X ) . (( a ` ),( abs j )) ) by A11,CKB3:def 1
.= ( ( a ` ) |^ n ) by A11,ABSVALUE:def 1;
( a |^ i ) = ( ( BCI-power X ) . (( a ` ),( abs i )) ) by A6,CKB3:def 1
.= ( ( a ` ) |^ m ) by A6,ABSVALUE:def 1;
then A14: ( ( a |^ i ) \ ( a |^ j ) ) = ( b |^ ( m - n ) ) by A7,A12,A13,CKB28:1;
per cases ;
suppose m >= n;

then reconsider q = ( m - n ) as (Element of ( NAT )) by INT_1:3,XREAL_1:48;
( ( a |^ i ) \ ( a |^ j ) ) = ( a |^ ( - q ) ) by A14,CKB14:1;
hence thesis;
end;
suppose A15: m < n;

then ( n - m ) > ( 0 ) by XREAL_1:50;
then reconsider p = ( n - m ) as (Element of ( NAT )) by INT_1:3;
A16: ( m - n ) < ( 0 ) by A15,XREAL_1:49;
then ( ( a |^ i ) \ ( a |^ j ) ) = ( ( BCI-power X ) . (( b ` ),( abs ( m - n ) )) ) by A14,CKB3:def 1
.= ( ( BCI-power X ) . (( b ` ),( - ( m - n ) )) ) by A16,ABSVALUE:def 1
.= ( a |^ p ) by BCIALG_1:29;
hence thesis;
end;
end;
end;
