begin
theorem
Th34: (for X being BCI-algebra holds (for Y being (SubAlgebra of X) holds (for x,y being (Element of X) holds (for x9,y9 being (Element of Y) holds ((x = x9 & y = y9) implies ( x \ y ) = ( x9 \ y9 ))))))
proof
let X being BCI-algebra;
let Y being (SubAlgebra of X);
let x being (Element of X),y being (Element of X);
let x9 being (Element of Y),y9 being (Element of Y);
assume that
A1: (x = x9 & y = y9);
A2: [ x9,y9 ] in [: (the carrier of Y),(the carrier of Y) :] by ZFMISC_1:87;
( x9 \ y9 ) = ( ( (the InternalDiff of X) || (the carrier of Y) ) . (x9,y9) ) by BCIALG_1:def 10
.= ( x \ y ) by A1,A2,FUNCT_1:49;
hence thesis;
end;
