registrations XBOOLE_0,RELSET_1,REALSET1,STRUCT_0,BCIALG_1,BCIALG_2,FUNCT_2,PARTFUN1,NAT_1,XREAL_0,ORDINAL1,XXREAL_0,INT_1;
constructors BINOP_1,REALSET1,BCIALG_2,REAL_1,BINOP_2,FINSEQOP,SUPINF_1,SEQ_1,NAT_D,RELSET_1,CKB1,CKB2,CKB3;
requirements BOOLE,SUBSET,NUMERALS,ARITHM,REAL;
begin
reserve X for BCI-algebra;
reserve x for (Element of X);
reserve n for Nat;
theorem
((x in ( BCK-part X ) & n >= 1) implies ( x |^ n ) = x)
proof
assume that
A1: x in ( BCK-part X )
and
A2: n >= 1;
defpred P[ Nat ]
 means
( x |^ $1 ) = x;
A3: (ex y being (Element of X) st (y = x & ( 0. X ) <= y)) by A1;
A4:now
let n;
assume n >= 1;
assume P[ n ];
then ( x |^ ( n + 1 ) ) = ( x \ ( x ` ) ) by CKB6:1
.= ( x \ ( 0. X ) ) by A3,BCIALG_1:def 11
.= x by BCIALG_1:2;
hence P[ ( n + 1 ) ];
end;
A5: P[ 1 ] by CKB8:1;
(for n holds (n >= 1 implies P[ n ])) from NAT_1:sch 8(A5,A4);
hence thesis by A2;
end;
