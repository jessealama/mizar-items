begin
theorem
Th40: (for L being  lower-bounded  continuous LATTICE holds (for x being (Element of L) holds (for N being non  empty  countable (Subset of L) holds ((for n,y being (Element of L) holds (((not y <= x) & n in N) implies (not ( y "/\" n ) <= x))) implies (for y being (Element of L) holds ((not y <= x) implies (ex p being  irreducible (Element of L) st (x <= p & (not p in ( uparrow ( { y } "/\" N ) ))))))))))
proof
let L being  lower-bounded  continuous LATTICE;
let x being (Element of L);
let N being non  empty  countable (Subset of L);
assume that
A1: (for n,y being (Element of L) holds (((not y <= x) & n in N) implies (not ( y "/\" n ) <= x)));
set V = ( ( downarrow x ) ` );
A2: ( V "/\" N ) c= V
proof
let q being set;
assume q in ( V "/\" N );
then consider v,n being (Element of L) such that A3: q = ( v "/\" n ) and A4: v in V and A5: n in N;
(not v in ( downarrow x )) by A4,XBOOLE_0:def 5;
then (not v <= x) by WAYBEL_0:17;
then (not ( v "/\" n ) <= x) by A1,A5;
then (not ( v "/\" n ) in ( downarrow x )) by WAYBEL_0:17;
hence thesis by A3,XBOOLE_0:def 5;
end;
x <= x;
then x in ( downarrow x ) by WAYBEL_0:17;
then A6: (not x in V) by XBOOLE_0:def 5;
let y being (Element of L);
assume (not y <= x);
then (not y in ( downarrow x )) by WAYBEL_0:17;
then y in V by XBOOLE_0:def 5;
hence thesis by A2,A6,CKB46:1;
end;
