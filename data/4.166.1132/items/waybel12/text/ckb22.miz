theorem
Th23: (for L being non  empty RelStr holds (for V,S,T being (Subset of L) holds (((S is_coarser_than T & V is  upper) & T c= V) implies S c= V)))
proof
let L being non  empty RelStr;
let V being (Subset of L);
let S being (Subset of L);
let T being (Subset of L);
assume that
A1: S is_coarser_than T
and
A2: (V is  upper & T c= V);
let q being set;
assume A3: q in S;
then reconsider S1 = S as non  empty (Subset of L);
reconsider b = q as (Element of S1) by A3;
(ex a being (Element of L) st (a in T & a <= b)) by A1,YELLOW_4:def 2;
hence thesis by A2,WAYBEL_0:def 20;
end;
