begin
theorem
Th43: (for L being  lower-bounded  continuous LATTICE holds (for D being non  empty  countable  dense (Subset of L) holds (for u being (Element of L) holds (u <> ( Bottom L ) implies (ex p being  irreducible (Element of L) st (p <> ( Top L ) & (not p in ( uparrow ( { u } "/\" D ) ))))))))
proof
let L being  lower-bounded  continuous LATTICE;
let D being non  empty  countable  dense (Subset of L);
let u being (Element of L);
assume that
A1: u <> ( Bottom L );
A2: (for d,y being (Element of L) holds (((not y <= ( Bottom L )) & d in D) implies (not ( y "/\" d ) <= ( Bottom L ))))
proof
let d being (Element of L);
let y being (Element of L);
assume that
A3: (not y <= ( Bottom L ));
assume d in D;
then d is  dense by CKB52:def 1;
then A4: ( y "/\" d ) <> ( Bottom L ) by A3,CKB48:def 1;
( Bottom L ) <= ( y "/\" d ) by YELLOW_0:44;
hence thesis by A4,ORDERS_2:2;
end;
( Bottom L ) <= u by YELLOW_0:44;
then (not u <= ( Bottom L )) by A1,ORDERS_2:2;
then consider p being  irreducible (Element of L) such that ( Bottom L ) <= p and A5: (not p in ( uparrow ( { u } "/\" D ) )) by A2,CKB47:1;
take p;
thus p <> ( Top L ) by A5,CKB12:1;
thus thesis by A5;
end;
