theorem
Th35: (for L being Semilattice holds (for A being (Subset of L) holds (for f,g being (Function of ( NAT ),(the carrier of L)) holds ((( rng f ) = A & (for n being (Element of ( NAT )) holds ( g . n ) = ( "/\" ({ ( f . m ) where m is (Element of ( NAT )): m <= n },L) ))) implies A is_coarser_than ( rng g )))))
proof
let L being Semilattice;
let A being (Subset of L);
let f being (Function of ( NAT ),(the carrier of L)),g being (Function of ( NAT ),(the carrier of L));
assume that
A1: ( rng f ) = A
and
A2: (for n being (Element of ( NAT )) holds ( g . n ) = ( "/\" ({ ( f . m ) where m is (Element of ( NAT )): m <= n },L) ));
let a being (Element of L);
assume a in A;
then consider n being set such that A3: n in ( dom f ) and A4: ( f . n ) = a by A1,FUNCT_1:def 3;
reconsider n as (Element of ( NAT )) by A3;
reconsider T = { ( f . m ) where m is (Element of ( NAT )): m <= n } as non empty  finite (Subset of L) by CKB1:1;
take b = ( "/\" (T,L) );
(( dom g ) = ( NAT ) & ( g . n ) = b) by A2,FUNCT_2:def 1;
hence b in ( rng g ) by FUNCT_1:def 3;
( f . n ) in T;
then A5: { ( f . n ) } c= T by ZFMISC_1:31;
( ex_inf_of { ( f . n ) },L &  ex_inf_of T,L) by YELLOW_0:55;
then b <= ( "/\" ({ ( f . n ) },L) ) by A5,YELLOW_0:35;
hence b <= a by A4,YELLOW_0:39;
end;
