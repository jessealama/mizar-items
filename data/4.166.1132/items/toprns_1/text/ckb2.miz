registrations ORDINAL1,RELSET_1,XXREAL_0,XREAL_0,MEMBERED,STRUCT_0,MONOID_0,EUCLID,XBOOLE_0,VALUED_1,FUNCT_2,NUMBERS,VALUED_0,NAT_1;
