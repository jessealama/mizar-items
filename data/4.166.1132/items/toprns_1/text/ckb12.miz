begin
reserve r for Real;
theorem
LmDef3: (for N,n being Nat holds (for seq being (Real_Sequence of N) holds ( ( r * seq ) . n ) = ( r * ( seq . n ) )))
proof
let N being Nat,n being Nat;
let seq being (Real_Sequence of N);
reconsider m = n as (Element of ( NAT )) by ORDINAL1:def 12;
A1: ( dom ( r * seq ) ) = ( NAT ) by FUNCT_2:def 1;
thus ( ( r * seq ) . n ) = ( ( r * seq ) /. m )
.= ( r * ( seq /. m ) ) by A1,VFUNCT_1:def 4
.= ( r * ( seq . n ) );
end;
