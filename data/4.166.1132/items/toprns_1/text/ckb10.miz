reserve seq9 for (Real_Sequence of N);
definition
let N being Nat;
let seq being (Real_Sequence of N);
func |.seq .| -> Real_Sequence means 
:Def7: (for n holds ( it . n ) = |. ( seq . n ) .|);
existence
proof
deffunc U((Element of ( NAT ))) = |. ( seq . $1 ) .|;
thus (ex s being Real_Sequence st (for n holds ( s . n ) = U(n))) from SEQ_1:sch 1;
end;
uniqueness
proof
let seq8 being Real_Sequence;
let seq9 being Real_Sequence;
assume that
A1: (for n holds ( seq8 . n ) = |. ( seq . n ) .|)
and
A2: (for n holds ( seq9 . n ) = |. ( seq . n ) .|);
let n;
( seq9 . n ) = |. ( seq . n ) .| by A2;
hence ( seq8 . n ) = ( seq9 . n ) by A1;
end;
end;
