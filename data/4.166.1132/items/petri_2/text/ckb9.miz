registrations SUBSET_1,RELAT_1,PARTFUN1,RELSET_1,XBOOLE_0,FUNCT_2,FINSET_1,REALSET1,STRUCT_0,PARTIT_2,FUNCT_1,PETRI,ZFMISC_1;
constructors RELSET_1,DOMAIN_1,PETRI,RELSET_2,FUNCOP_1,FUNCT_4,CARD_1,STRUCT_0,PARTIT_2,CKB1,CKB2,CKB5;
requirements SUBSET,BOOLE;
begin
theorem
Lm4: (for A being non  trivial set holds (for B being set holds (for Bo1,Bo2 being (Subset of B) holds (for yo1 being (Function of Bo1,A) holds (for yo2 being (Function of Bo2,A) holds (Bo1 <> Bo2 implies { y where y is (Function of B,A): ( y | Bo1 ) = yo1 } <> { y where y is (Function of B,A): ( y | Bo2 ) = yo2 }))))))
proof
let A being non  trivial set;
let B being set;
let Bo1 being (Subset of B);
let Bo2 being (Subset of B);
let yo1 being (Function of Bo1,A);
let yo2 being (Function of Bo2,A);
assume A1: Bo1 <> Bo2;
per cases ;
suppose (not Bo2 c= Bo1);

then consider f being (Function of B,A) such that A2: ( f | Bo1 ) = yo1 and A3: ( f | Bo2 ) <> yo2 by CKB7:1;
(not f in { y where y is (Function of B,A): ( y | Bo2 ) = yo2 })
proof
assume f in { y where y is (Function of B,A): ( y | Bo2 ) = yo2 };
then (ex y being (Function of B,A) st (f = y & ( y | Bo2 ) = yo2));
hence contradiction by A3;
end;
hence thesis by A2;
end;
suppose Bo2 c= Bo1;

then consider f being (Function of B,A) such that A4: ( f | Bo2 ) = yo2 and A5: ( f | Bo1 ) <> yo1 by A1,CKB8:1;
(not f in { y where y is (Function of B,A): ( y | Bo1 ) = yo1 })
proof
assume f in { y where y is (Function of B,A): ( y | Bo1 ) = yo1 };
then (ex y being (Function of B,A) st (f = y & ( y | Bo1 ) = yo1));
hence contradiction by A5;
end;
hence thesis by A4;
end;
end;
