registrations SUBSET_1,RELAT_1,PARTFUN1,RELSET_1,XBOOLE_0,FUNCT_2,FINSET_1,REALSET1,STRUCT_0,PARTIT_2,FUNCT_1,PETRI,ZFMISC_1;
constructors RELSET_1,DOMAIN_1,PETRI,RELSET_2,FUNCOP_1,FUNCT_4,CARD_1,STRUCT_0,PARTIT_2,CKB1,CKB2;
requirements SUBSET,BOOLE;
begin
theorem
(for A1,A2 being non  empty set holds (for B being set holds (for D1 being (thin_cylinder of A1,B) holds (A1 c= A2 implies (ex D2 being (thin_cylinder of A2,B) st D1 c= D2)))))
proof
let A1 being non  empty set;
let A2 being non  empty set;
let B being set;
let D1 being (thin_cylinder of A1,B);
consider Bo being (Subset of B),yo1 being (Function of Bo,A1) such that A1: Bo is  finite and A2: D1 = { y where y is (Function of B,A1): ( y | Bo ) = yo1 } by CKB3:1;
assume A3: A1 c= A2;
then reconsider yo2 = yo1 as (Function of Bo,A2) by FUNCT_2:7;
set D2 = { y where y is (Function of B,A2): ( y | Bo ) = yo2 };
A4:now
let x being set;
assume x in D1;
then consider y1 being (Function of B,A1) such that A5: x = y1 and A6: ( y1 | Bo ) = yo1 by A2;
reconsider y2 = y1 as (Function of B,A2) by A3,FUNCT_2:7;
( y2 | Bo ) = yo1 by A6;
hence x in D2 by A5;
end;
D2 = ( cylinder0 (A2,B,Bo,yo2) ) by CKB1:def 1;
then reconsider D2 as (thin_cylinder of A2,B) by A1,CKB2:def 1;
take D2;
thus thesis by A4,TARSKI:def 3;
end;
