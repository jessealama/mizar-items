begin
definition
let A being non empty set;
let B being set;
func thin_cylinders (A,B) -> non empty (Subset-Family of ( Funcs (B,A) ))
equals
{ D where D is (Subset of ( Funcs (B,A) )): D is (thin_cylinder of A,B) };
correctness
proof
set F = the (thin_cylinder of A,B);
A1:now
let z being set;
assume z in { D where D is (Subset of ( Funcs (B,A) )): D is (thin_cylinder of A,B) };
then (ex D being (Subset of ( Funcs (B,A) )) st (z = D & D is (thin_cylinder of A,B)));
hence z in ( bool ( Funcs (B,A) ) );
end;
F in { D where D is (Subset of ( Funcs (B,A) )): D is (thin_cylinder of A,B) };
hence thesis by A1,TARSKI:def 3;
end;
end;
