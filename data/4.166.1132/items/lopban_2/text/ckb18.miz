theorem
Th12: (for X being RealNormSpace holds (for f,g being (Element of ( BoundedLinearOperators (X,X) )) holds (for a being Real holds ( a * ( f * g ) ) = ( ( a * f ) * g ))))
proof
let X being RealNormSpace;
let f being (Element of ( BoundedLinearOperators (X,X) ));
let g being (Element of ( BoundedLinearOperators (X,X) ));
let a being Real;
set RRL = RLSStruct (# ( BoundedLinearOperators (X,X) ),( Zero_ (( BoundedLinearOperators (X,X) ),( R_VectorSpace_of_LinearOperators (X,X) )) ),( Add_ (( BoundedLinearOperators (X,X) ),( R_VectorSpace_of_LinearOperators (X,X) )) ),( Mult_ (( BoundedLinearOperators (X,X) ),( R_VectorSpace_of_LinearOperators (X,X) )) ) #);
reconsider gg = g as (Element of RRL);
A1: ( 1 * g ) = ( 1 * gg )
.= g by RLVECT_1:def 8;
( a * ( f * g ) ) = ( ( a * 1 ) * ( f * g ) )
.= ( ( a * f ) * ( 1 * g ) ) by CKB17:1;
hence thesis by A1;
end;
