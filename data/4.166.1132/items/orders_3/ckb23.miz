begin
theorem
Lm2: (for A,B,C being non empty RelStr holds (for f being (Function of A,B) holds (for g being (Function of B,C) holds ((f is  monotone & g is  monotone) implies (ex gf being (Function of A,C) st (gf = ( g * f ) & gf is  monotone))))))
proof
let A being non empty RelStr,B being non empty RelStr,C being non empty RelStr;
let f being (Function of A,B);
let g being (Function of B,C);
assume that
A1: f is  monotone
and
A2: g is  monotone;
reconsider gf = ( g * f ) as (Function of A,C);
take gf;
now
let p1 being (Element of A),p2 being (Element of A);
reconsider p19 = ( f . p1 ),p29 = ( f . p2 ) as (Element of B);
( dom f ) = (the carrier of A) by FUNCT_2:def 1;
then A3: (( g . ( f . p1 ) ) = ( ( g * f ) . p1 ) & ( g . ( f . p2 ) ) = ( ( g * f ) . p2 )) by FUNCT_1:13;
assume p1 <= p2;
then A4: p19 <= p29 by A1,CKB22:def 1;
let r1 being (Element of C),r2 being (Element of C);
assume (r1 = ( gf . p1 ) & r2 = ( gf . p2 ));
hence r1 <= r2 by A2,A3,A4,CKB22:def 1;
end;
hence thesis by CKB22:def 1;
end;
