begin
registration
let V being  right_zeroed non  empty VectSpStr over ( F_Complex );
let W being (VectSp of ( F_Complex ));
let f being  additiveFAF  cmplxhomogeneousFAF (Form of V,W);
cluster ( RQ*Form f ) -> non  degenerated-on-right;
coherence
proof
set K = ( F_Complex );
set qf = ( RQ*Form f );
set R = ( RKer ( f *' ) );
set qW = ( VectQuot (W,R) );
A1: ( rightker f ) = ( rightker ( f *' ) ) by CKB110:1;
thus ( rightker qf ) c= { ( 0. qW ) }
proof
let x being set;
assume x in ( rightker qf );
then consider ww being (Vector of qW) such that A2: x = ww and A3: (for v being (Vector of V) holds ( qf . (v,ww) ) = ( 0. K ));
consider w being (Vector of W) such that A4: ww = ( w + R ) by VECTSP10:22;
now
let v being (Vector of V);
thus ( f . (v,w) ) = ( qf . (v,ww) ) by A4,CKB115:1
.= ( 0. K ) by A3;
end;
then w in ( rightker f );
then w in (the carrier of R) by A1,BILINEAR:def 19;
then w in R by STRUCT_0:def 5;
then ( w + R ) = ( zeroCoset (W,R) ) by VECTSP_4:49
.= ( 0. qW ) by VECTSP10:21;
hence thesis by A2,A4,TARSKI:def 1;
end;

let x being set;
assume x in { ( 0. qW ) };
then A5: x = ( 0. qW ) by TARSKI:def 1;
(for v being (Vector of V) holds ( qf . (v,( 0. qW )) ) = ( 0. K )) by BILINEAR:29;
hence thesis by A5;
end;
end;
