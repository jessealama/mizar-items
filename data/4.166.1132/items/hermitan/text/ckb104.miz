proof
set q = ( quasinorm f );
thus ( quasinorm f ) is  subadditive
proof
let v being (Vector of V);
let w being (Vector of V);
set fvw = ( signnorm (f,( v + w )) );
set fv = ( signnorm (f,v) );
set fw = ( signnorm (f,w) );
A1: ( q . v ) = ( sqrt fv ) by CKB103:def 1;
( 0 ) <= ( Re ( f . (v,v) ) ) by CKB50:def 1;
then A2: ( 0 ) <= ( q . v ) by A1,SQUARE_1:def 2;
A3: ( q . w ) = ( sqrt fw ) by CKB103:def 1;
( 0 ) <= ( Re ( f . (w,w) ) ) by CKB50:def 1;
then A4: ( 0 ) <= ( q . w ) by A3,SQUARE_1:def 2;
(( 0 ) <= ( Re ( f . (( v + w ),( v + w )) ) ) & ( q . ( v + w ) ) = ( sqrt fvw )) by CKB50:def 1,CKB103:def 1;
then ( q . ( v + w ) ) <= ( sqrt ( ( ( q . v ) + ( q . w ) ) ^2 ) ) by A1,A3,CKB99:1,SQUARE_1:26;
hence ( q . ( v + w ) ) <= ( ( q . v ) + ( q . w ) ) by A2,A4,SQUARE_1:22;
end;

let v being (Vector of V);
let r being (Scalar of V);
A5: (( 0 ) <= ( |. r .| ^2 ) & ( 0 ) <= ( Re ( f . (v,v) ) )) by CKB50:def 1,XREAL_1:63;
A6: ( f . (( r * v ),( r * v )) ) = ( r * ( f . (v,( r * v )) ) ) by BILINEAR:31
.= ( r * ( ( r *' ) * ( f . (v,v) ) ) ) by CKB47:1
.= ( ( r * ( r *' ) ) * ( f . (v,v) ) )
.= ( [** ( |. r .| ^2 ),( 0 ) **] * ( f . (v,v) ) ) by CKB14:1
.= ( [** ( |. r .| ^2 ),( 0 ) **] * [** ( Re ( f . (v,v) ) ),( Im ( f . (v,v) ) ) **] ) by COMPLEX1:13
.= ( [** ( |. r .| ^2 ),( 0 ) **] * [** ( Re ( f . (v,v) ) ),( 0 ) **] ) by CKB49:def 1
.= [** ( ( |. r .| ^2 ) * ( Re ( f . (v,v) ) ) ),( 0 ) **];
thus ( q . ( r * v ) ) = ( sqrt ( signnorm (f,( r * v )) ) ) by CKB103:def 1
.= ( sqrt ( ( |. r .| ^2 ) * ( Re ( f . (v,v) ) ) ) ) by A6,COMPLEX1:12
.= ( ( sqrt ( |. r .| ^2 ) ) * ( sqrt ( Re ( f . (v,v) ) ) ) ) by A5,SQUARE_1:29
.= ( |. r .| * ( sqrt ( signnorm (f,v) ) ) ) by COMPLEX1:46,SQUARE_1:22
.= ( |. r .| * ( q . v ) ) by CKB103:def 1;
end;
