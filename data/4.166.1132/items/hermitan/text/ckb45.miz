environ
vocabularies NUMBERS,CARD_1,ARYTM_3,RELAT_1,XCMPLX_0,ORDINAL1,ARYTM_1,SUBSET_1,COMPLEX1,SQUARE_1,XXREAL_0,COMPLFLD,HAHNBAN1,GROUP_1,SUPINF_2,MESFUNC1,XBOOLE_0,VECTSP_1,HAHNBAN,FUNCT_1,RLVECT_1,ALGSTR_0,MSSUBFAM,STRUCT_0,UNIALG_1,ZFMISC_1,VECTSP10,TARSKI,RLSUB_1,GROUP_6,BILINEAR,XREAL_0,REAL_1,HERMITAN;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,ORDINAL1,NUMBERS,XCMPLX_0,XXREAL_0,XREAL_0,REAL_1,STRUCT_0,ALGSTR_0,FUNCT_1,RLVECT_1,BINOP_1,GROUP_1,VECTSP_1,RELSET_1,FUNCT_2,SQUARE_1,COMPLEX1,VECTSP_4,COMPLFLD,HAHNBAN1,VECTSP10,BILINEAR,GRCAT_1,CKB17,CKB21,CKB26,CKB42;
definitions HAHNBAN1,TARSKI,XBOOLE_0,VECTSP_4,VECTSP10,BILINEAR,FUNCT_1,SQUARE_1,BINOP_1,COMPLEX1,VECTSP_1,ALGSTR_0,GRCAT_1,CKB17,CKB26,CKB42;
theorems COMPLEX1,SQUARE_1,ABSVALUE,COMPLFLD,XREAL_0,FUNCT_2,HAHNBAN1,VECTSP_1,RLVECT_1,VECTSP_4,VECTSP_6,TARSKI,VECTSP10,BILINEAR,DOMAIN_1,ZFMISC_1,FUNCT_1,XCMPLX_1,XREAL_1,BINOP_1,XXREAL_0,FUNCOP_1,STRUCT_0,GRCAT_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB26,CKB32,CKB33,CKB34,CKB35,CKB36,CKB37,CKB38,CKB39,CKB40,CKB41,CKB42,CKB43;
schemes FUNCT_2,BINOP_1;
registrations XBOOLE_0,RELSET_1,NUMBERS,XCMPLX_0,XXREAL_0,XREAL_0,SQUARE_1,MEMBERED,STRUCT_0,VECTSP_1,COMPLFLD,HAHNBAN1,VECTSP10,BILINEAR,CKB18,CKB19,CKB20,CKB22,CKB23,CKB24,CKB25,CKB27,CKB28,CKB29,CKB30,CKB31,CKB44;
constructors REAL_1,SQUARE_1,BINOP_2,BORSUK_1,BILINEAR,FUNCOP_1,SUPINF_1,CKB17,CKB26,CKB42;
requirements NUMERALS,SUBSET,REAL,BOOLE,ARITHM;
begin
registration
let V being (VectSp of ( F_Complex ));
let f being (antilinear-Functional of V);
cluster ( QcFunctional f ) -> non  degenerated;
coherence
proof
set qf = ( QcFunctional f );
set W = ( Ker ( f *' ) );
set qV = ( VectQuot (V,W) );
set K = ( F_Complex );
A1: (the carrier of W) = ( ker ( f *' ) ) by VECTSP10:def 11
.= ( ker f ) by CKB39:1;
A2: (the carrier of qV) = ( CosetSet (V,W) ) by VECTSP10:def 6;
thus ( ker qf ) c= { ( 0. qV ) }
proof
let x being set;
assume x in ( ker qf );
then consider w being (Vector of qV) such that A3: x = w and A4: ( qf . w ) = ( 0. K );
w in ( CosetSet (V,W) ) by A2;
then consider A being (Coset of W) such that A5: w = A;
consider v being (Vector of V) such that A6: A = ( v + W ) by VECTSP_4:def 6;
( f . v ) = ( 0. K ) by A1,A4,A5,A6,VECTSP10:def 12;
then v in ( ker f );
then v in W by A1,STRUCT_0:def 5;
then w = ( zeroCoset (V,W) ) by A5,A6,VECTSP_4:49
.= ( 0. qV ) by VECTSP10:21;
hence thesis by A3,TARSKI:def 1;
end;

thus { ( 0. qV ) } c= ( ker qf )
proof
let x being set;
assume x in { ( 0. qV ) };
then A7: x = ( 0. qV ) by TARSKI:def 1;
( qf . ( 0. qV ) ) = ( 0. K ) by HAHNBAN1:def 9;
hence thesis by A7;
end;

end;
end;
