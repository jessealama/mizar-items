begin
reserve x for set;
definition
func FinTrees -> (Subset of ( Trees )) means 
:Def2: (x in it iff x is  finite  finite Tree);
existence
proof
set X = { T where T is (Element of ( Trees )): T is  finite  finite Tree };
X c= ( Trees )
proof
let x;
assume x in X;
then (ex T being (Element of ( Trees )) st (x = T & T is  finite  finite Tree));
hence thesis;
end;
then reconsider X as (Subset of ( Trees ));
take X;
let x;
thus (x in X implies x is  finite  finite Tree)
proof
assume x in X;
then (ex T being (Element of ( Trees )) st (x = T & T is  finite  finite Tree));
hence thesis;
end;

assume x is  finite  finite Tree;
then reconsider T = x as  finite Tree;
T in ( Trees ) by CKB2:def 1;
hence thesis;
end;
uniqueness
proof
defpred X[ set ]
 means
$1 is  finite  finite Tree;
let T1 being (Subset of ( Trees ));
let T2 being (Subset of ( Trees ));
assume that
A1: (x in T1 iff X[ x ])
and
A2: (x in T2 iff X[ x ]);
thus thesis from XBOOLE_0:sch 2(A1,A2);
end;
