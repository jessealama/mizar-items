begin
definition
let p being FinSequence;
assume that
A1: p is  DTree-yielding;
func roots p -> FinSequence
means
(( dom it ) = ( dom p ) & (for i being (Element of ( NAT )) holds (i in ( dom p ) implies (ex T being DecoratedTree st (T = ( p . i ) & ( it . i ) = ( T . ( {} ) ))))));
existence
proof
defpred P[ Nat,set ]
 means
(ex T being DecoratedTree st (T = ( p . $1 ) & $2 = ( T . ( {} ) )));
A2: ( Seg ( len p ) ) = ( dom p ) by FINSEQ_1:def 3;
A3: (for k being Nat holds (k in ( Seg ( len p ) ) implies (ex x being set st P[ k,x ])))
proof
let k being Nat;
assume k in ( Seg ( len p ) );
then A4: ( p . k ) in ( rng p ) by A2,FUNCT_1:def 3;
( rng p ) is  constituted-DTrees by A1,CKB55:def 1;
then reconsider T = ( p . k ) as DecoratedTree by A4;
take ( T . ( {} ) );
take T;
thus thesis;
end;
consider q being FinSequence such that A5: (( dom q ) = ( Seg ( len p ) ) & (for k being Nat holds (k in ( Seg ( len p ) ) implies P[ k,( q . k ) ]))) from FINSEQ_1:sch 1(A3);
take q;
thus ( dom q ) = ( dom p ) by A5,FINSEQ_1:def 3;
thus thesis by A2,A5;
end;
uniqueness
proof
defpred P[ Nat,set ]
 means
(ex T being DecoratedTree st (T = ( p . $1 ) & $2 = ( T . ( {} ) )));
let x1 being FinSequence,x2 being FinSequence;
assume that
A6: ( dom x1 ) = ( dom p )
and
A7: (for i being (Element of ( NAT )) holds (i in ( dom p ) implies P[ i,( x1 . i ) ]))
and
A8: ( dom x2 ) = ( dom p )
and
A9: (for i being (Element of ( NAT )) holds (i in ( dom p ) implies P[ i,( x2 . i ) ]));
now
let k being Nat;
assume A10: k in ( dom p );
then A11: P[ k,( x1 . k ) ] by A7;
P[ k,( x2 . k ) ] by A9,A10;
hence ( x1 . k ) = ( x2 . k ) by A11;
end;
hence thesis by A6,A8,FINSEQ_1:13;
end;
end;
