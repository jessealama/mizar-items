begin
reserve i for Nat;
reserve n for Nat;
reserve D for non empty set;
reserve F for (FinSequence_of_Matrix of D);
definition
let D;
let F;
let n;
redefine func F /^ n -> (FinSequence_of_Matrix of D);

coherence
proof
now
let i;
assume that
A1: i in ( dom ( F /^ n ) );
( i + n ) in ( dom F ) by A1,FINSEQ_5:26;
then A2: ( F . ( n + i ) ) = ( F /. ( n + i ) ) by PARTFUN1:def 6;
( ( F /^ n ) . i ) = ( ( F /^ n ) /. i ) by A1,PARTFUN1:def 6;
hence ( ( F /^ n ) . i ) is (Matrix of D) by A1,A2,FINSEQ_5:27;
end;
hence thesis by CKB15:def 1;
end;
end;
