constructors XXREAL_0,GROUP_4,VECTSP_7,MATRIX11,MATRIX13,LAPLACE,RFINSEQ,POLYNOM1,WSIERP_1,SEQ_1,VALUED_0,VALUED_1,REAL_1,NAT_D,BINARITH,RELSET_1,FVSUM_1,CKB7;
requirements ARITHM,NUMERALS,REAL,BOOLE,SUBSET;
begin
reserve i for Nat;
reserve j for Nat;
reserve f for (FinSequence of ( NAT ));
theorem
(for i holds (for j holds (((((i <= ( len f ) & j <= ( len f )) & ( Sum ( f | i ) ) = ( Sum ( f | j ) )) & (i in ( dom f ) implies ( f . i ) <> ( 0 ))) & (j in ( dom f ) implies ( f . j ) <> ( 0 ))) implies i = j)))
proof
A1:now
let i;
let j;
assume that
A2: i <= ( len f )
and
j <= ( len f )
and
A3: ( Sum ( f | i ) ) = ( Sum ( f | j ) )
and
A4: (i in ( dom f ) implies ( f . i ) <> ( 0 ))
and
(j in ( dom f ) implies ( f . j ) <> ( 0 ));
assume A5: i > j;
then reconsider i1 = ( i - 1 ) as (Element of ( NAT )) by NAT_1:20;
A6: j in ( NAT ) by ORDINAL1:def 12;
A7: i = ( i1 + 1 );
then j <= i1 by A5,NAT_1:13;
then A8: ( Sum ( f | j ) ) <= ( Sum ( f | i1 ) ) by A6,POLYNOM3:18;
A9: i >= 1 by A7,NAT_1:14;
then i in ( dom f ) by A2,FINSEQ_3:25;
then ( f | ( i1 + 1 ) ) = ( ( f | i1 ) ^ <* ( f . i ) *> ) by FINSEQ_5:10;
then A10: ( Sum ( f | j ) ) = ( ( Sum ( f | i1 ) ) + ( f . i ) ) by A3,RVSUM_1:74;
then ( Sum ( f | i1 ) ) <= ( Sum ( f | j ) ) by NAT_1:11;
then ( Sum ( f | i1 ) ) = ( Sum ( f | j ) ) by A8,XXREAL_0:1;
hence contradiction by A2,A4,A9,A10,FINSEQ_3:25;
end;
let i;
let j;
assume that
A11: i <= ( len f )
and
A12: j <= ( len f )
and
A13: ( Sum ( f | i ) ) = ( Sum ( f | j ) )
and
A14: (i in ( dom f ) implies ( f . i ) <> ( 0 ))
and
A15: (j in ( dom f ) implies ( f . j ) <> ( 0 ));
A16: j <= i by A1,A11,A12,A13,A14,A15;
i <= j by A1,A11,A12,A13,A14,A15;
hence thesis by A16,XXREAL_0:1;
end;
