theorem
( a * ( block_diagonal (G,a1) ) ) = ( block_diagonal (( mlt (( ( len G ) |-> a ),G) ),( a * a1 )) )
proof
defpred P[ Nat ]
 means
(for a holds (for a1 holds (for G holds (( len G ) = $1 implies ( a * ( block_diagonal (G,a1) ) ) = ( block_diagonal (( mlt (( ( len G ) |-> a ),G) ),( a * a1 )) )))));
A1: (for n holds (P[ n ] implies P[ ( n + 1 ) ]))
proof
let n;
assume that
A2: P[ n ];
set n1 = ( n + 1 );
let a;
let a1;
let G;
assume that
A3: ( len G ) = n1;
A4: G = ( ( G | n ) ^ <* ( G . n1 ) *> ) by A3,FINSEQ_3:55;
n <= n1 by NAT_1:13;
then A5: ( len ( G | n ) ) = n by A3,FINSEQ_1:59;
1 in ( Seg 1 );
then 1 in ( dom <* a *> ) by FINSEQ_1:def 8;
then A6: ( <* a *> . 1 ) = ( <* a *> /. 1 ) by PARTFUN1:def 6;
A7: ( <* a *> . 1 ) = a by FINSEQ_1:def 8;
A8: ( len <* a *> ) = 1 by FINSEQ_1:39;
A9: ( len <* ( G . n1 ) *> ) = 1 by FINSEQ_1:39;
A10: ( len ( n |-> a ) ) = n by CARD_1:def 7;
( n1 |-> a ) = ( ( n |-> a ) ^ <* a *> ) by FINSEQ_2:60;
hence ( block_diagonal (( mlt (( ( len G ) |-> a ),G) ),( a * a1 )) ) = ( block_diagonal (( ( mlt (( n |-> a ),( G | n )) ) ^ ( mlt (<* a *>,<* ( G . n1 ) *>) ) ),( a * a1 )) ) by A3,A4,A5,A10,A9,A8,CKB104:1
.= ( block_diagonal (( ( mlt (( n |-> a ),( G | n )) ) ^ <* ( a * ( G . n1 ) ) *> ),( a * a1 )) ) by A6,A7,CKB103:1
.= ( block_diagonal (( <* ( block_diagonal (( mlt (( n |-> a ),( G | n )) ),( a * a1 )) ) *> ^ <* ( a * ( G . n1 ) ) *> ),( a * a1 )) ) by CKB57:1
.= ( block_diagonal (<* ( a * ( block_diagonal (( G | n ),a1) ) ),( a * ( G . n1 ) ) *>,( a * a1 )) ) by A2,A5
.= ( a * ( block_diagonal (<* ( block_diagonal (( G | n ),a1) ),( G . n1 ) *>,a1) ) ) by CKB105:1
.= ( a * ( block_diagonal (G,a1) ) ) by A4,CKB57:1;
end;
A11: P[ ( 0 ) ]
proof
let a;
let a1;
let G;
assume that
A12: ( len G ) = ( 0 );
( dom ( mlt (( ( len G ) |-> a ),G) ) ) = ( dom G ) by CKB100:def 1;
then ( len ( mlt (( ( len G ) |-> a ),G) ) ) = ( len G ) by FINSEQ_3:29;
then ( Len ( mlt (( ( len G ) |-> a ),G) ) ) = ( {} ) by A12;
then ( len ( block_diagonal (( mlt (( ( len G ) |-> a ),G) ),( a * a1 )) ) ) = ( 0 ) by CKB42:def 1,RVSUM_1:72;
then A13: ( block_diagonal (( mlt (( ( len G ) |-> a ),G) ),( a * a1 )) ) = ( {} );
( Len G ) = ( {} ) by A12;
then ( len ( block_diagonal (G,a1) ) ) = ( 0 ) by CKB42:def 1,RVSUM_1:72;
then ( len ( a * ( block_diagonal (G,a1) ) ) ) = ( 0 ) by MATRIX_3:def 5;
hence thesis by A13;
end;
(for n holds P[ n ]) from NAT_1:sch 2(A11,A1);
