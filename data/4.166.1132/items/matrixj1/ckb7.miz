begin
reserve j for Nat;
reserve n for Nat;
reserve k for Nat;
reserve f for (FinSequence of ( NAT ));
definition
let f;
let i being Nat;
assume that
A1: i in ( Seg ( Sum f ) );
func min (f,i) -> (Element of ( NAT ))
means
:Def1: ((i <= ( Sum ( f | it ) ) & it in ( dom f )) & (for j holds (i <= ( Sum ( f | j ) ) implies it <= j)));
existence
proof
defpred P[ Nat ]
 means
(i <= ( Sum ( f | $1 ) ) & $1 >= 1);
A2: ( f | ( len f ) ) = f by FINSEQ_1:58;
A3: i <= ( Sum f ) by A1,FINSEQ_1:1;
( Sum f ) <> ( 0 ) by A1;
then A4: ( len f ) >= 1 by FINSEQ_1:20,RVSUM_1:72;
then A5: (ex k st P[ k ]) by A3,A2;
consider k such that A6: P[ k ] and A7: (for n holds (P[ n ] implies k <= n)) from NAT_1:sch 5(A5);
reconsider k as (Element of ( NAT )) by ORDINAL1:def 12;
take k;
thus i <= ( Sum ( f | k ) ) by A6;
k <= ( len f ) by A4,A3,A2,A7;
hence k in ( dom f ) by A6,FINSEQ_3:25;
let j;
assume that
A8: i <= ( Sum ( f | j ) );
per cases ;
suppose j = ( 0 );

then ( f | j ) = ( <*> ( REAL ) );
hence thesis by A1,A8,FINSEQ_1:1,RVSUM_1:72;
end;
suppose j > ( 0 );

hence thesis by A7,A8,NAT_1:14;
end;
end;
uniqueness
proof
let n1 being (Element of ( NAT )),n2 being (Element of ( NAT ));
assume that
A9: i <= ( Sum ( f | n1 ) )
and
n1 in ( dom f )
and
A10: (for j holds (i <= ( Sum ( f | j ) ) implies n1 <= j))
and
A11: i <= ( Sum ( f | n2 ) )
and
n2 in ( dom f )
and
A12: (for j holds (i <= ( Sum ( f | j ) ) implies n2 <= j));
A13: n2 <= n1 by A9,A12;
n1 <= n2 by A10,A11;
hence thesis by A13,XXREAL_0:1;
end;
end;
