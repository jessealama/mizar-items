begin
theorem
Th3: (for F being Field holds (for V being (VectSp of F) holds (for W being (Subspace of V) holds W is (Subspace of ( (Omega). V )))))
proof
let F being Field;
let V being (VectSp of F);
let W being (Subspace of V);
thus (the carrier of W) c= (the carrier of ( (Omega). V )) by VECTSP_4:def 2;
thus ( 0. W ) = ( 0. V ) by VECTSP_4:def 2
.= ( 0. ( (Omega). V ) );
thus thesis by VECTSP_4:def 2;
end;
