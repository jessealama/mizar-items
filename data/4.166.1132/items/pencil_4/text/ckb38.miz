constructors XXREAL_0,REALSET1,VECTSP_5,VECTSP_7,VECTSP_9,PENCIL_1,NAT_D,CKB7,CKB9,CKB11,CKB14,CKB22,CKB28,CKB32;
requirements REAL,BOOLE,SUBSET,NUMERALS,ARITHM;
begin
definition
let X being set;
func PairSet X
means
:Def8: (for z being set holds (z in it iff (ex x,y being set st ((x in X & y in X) & z = { x,y }))));
existence
proof
defpred P[ set ]
 means
(ex x,y being set st ((x in X & y in X) & $1 = { x,y }));
consider S being set such that A1: (for z being set holds (z in S iff (z in ( bool X ) & P[ z ]))) from XBOOLE_0:sch 1;
take S;
let z being set;
thus (z in S implies P[ z ]) by A1;
assume A2: P[ z ];
then z c= X by ZFMISC_1:32;
hence thesis by A1,A2;
end;
uniqueness
proof
let p1 being set,p2 being set;
assume that
A3: (for z being set holds (z in p1 iff (ex x,y being set st ((x in X & y in X) & z = { x,y }))))
and
A4: (for z being set holds (z in p2 iff (ex x,y being set st ((x in X & y in X) & z = { x,y }))));
now
let z being set;
(z in p1 iff (ex x,y being set st ((x in X & y in X) & z = { x,y }))) by A3;
hence (z in p1 iff z in p2) by A4;
end;
hence thesis by TARSKI:1;
end;
end;
