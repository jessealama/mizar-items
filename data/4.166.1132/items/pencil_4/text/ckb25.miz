begin
theorem
Th21: (for F being Field holds (for V being  finite-dimensional (VectSp of F) holds (for k being Nat holds ((1 <= k & k < ( dim V )) implies ( PencilSpace (V,k) ) is  with_non_trivial_blocks))))
proof
let F being Field;
let V being  finite-dimensional (VectSp of F);
let k being Nat;
assume that
A1: (1 <= k & k < ( dim V ));
set S = ( PencilSpace (V,k) );
thus S is  with_non_trivial_blocks
proof
let X being (Block of S);
S is non void by A1,CKB23:1;
then (ex W1,W2 being (Subspace of V) st (((W1 is (Subspace of W2) & ( ( dim W1 ) + 1 ) = k) & ( dim W2 ) = ( k + 1 )) & X = ( pencil (W1,W2,k) ))) by CKB14:def 1;
then X is non trivial by CKB21:1;
hence thesis by PENCIL_1:4;
end;

end;
