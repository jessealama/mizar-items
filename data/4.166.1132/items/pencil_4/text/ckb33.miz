theorem
Th28: (for F being Field holds (for V being  finite-dimensional (VectSp of F) holds (for m,n being Nat holds (n <= ( dim V ) implies ( GrassmannSpace (V,m,n) ) is non  void))))
proof
let F being Field;
let V being  finite-dimensional (VectSp of F);
let m being Nat;
let n being Nat;
assume n <= ( dim V );
then (the topology of ( GrassmannSpace (V,m,n) )) is non  empty by CKB29:1;
hence thesis by PENCIL_1:def 4;
end;
