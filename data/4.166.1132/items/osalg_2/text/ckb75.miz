let o2 being (BinOp of ( OSSub U0 ));
assume that
A3: (for x,y being (Element of ( OSSub U0 )) holds (for U1,U2 being  strict (OSSubAlgebra of U0) holds ((x = U1 & y = U2) implies ( o1 . (x,y) ) = ( U1 /\ U2 ))))
