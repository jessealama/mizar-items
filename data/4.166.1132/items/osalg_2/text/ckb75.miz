definition
let S1;
let U0 being  non-empty (OSAlgebra of S1);
func OSAlg_meet U0 -> (BinOp of ( OSSub U0 )) means 
:Def17: (for x,y being (Element of ( OSSub U0 )) holds (for U1,U2 being  strict (OSSubAlgebra of U0) holds ((x = U1 & y = U2) implies ( it . (x,y) ) = ( U1 /\ U2 ))));
existence
proof
defpred P[ (Element of ( OSSub U0 )),(Element of ( OSSub U0 )),set ]
 means
(for U1,U2 being  strict (OSSubAlgebra of U0) holds (($1 = U1 & $2 = U2) implies $3 = ( U1 /\ U2 )));
A1: (for x,y being (Element of ( OSSub U0 )) holds (ex z being (Element of ( OSSub U0 )) st P[ x,y,z ]))
proof
let x being (Element of ( OSSub U0 ));
let y being (Element of ( OSSub U0 ));
reconsider U1 = x,U2 = y as  strict (OSSubAlgebra of U0) by CKB70:def 1;
reconsider z = ( U1 /\ U2 ) as (Element of ( OSSub U0 )) by CKB70:def 1;
take z;
thus thesis;
end;
