environ
vocabularies XBOOLE_0,METRIC_1,SUBSET_1,FUNCT_1,ZFMISC_1,MCART_1,STRUCT_0,NUMBERS,ARYTM_3,CARD_1,XXREAL_0,REAL_1,METRIC_3,SQUARE_1,XREAL_0,ORDINAL1,RELAT_1,ARYTM_1,XCMPLX_0,COMPLEX1;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,DOMAIN_1,ORDINAL1,NUMBERS,XCMPLX_0,XREAL_0,COMPLEX1,REAL_1,FUNCT_2,BINOP_1,STRUCT_0,METRIC_1,MCART_1,XXREAL_0,SQUARE_1;
definitions SQUARE_1;
theorems BINOP_1,DOMAIN_1,ZFMISC_1,METRIC_1,MCART_1,XREAL_1,SQUARE_1,COMPLEX1,XXREAL_0;
schemes BINOP_1;
registrations XBOOLE_0,SUBSET_1,RELSET_1,NUMBERS,MEMBERED,STRUCT_0,METRIC_1,RELAT_1,XCMPLX_0,XREAL_0,SQUARE_1;
constructors XXREAL_0,REAL_1,MEMBERED,METRIC_1,SQUARE_1,DOMAIN_1,COMPLEX1;
requirements SUBSET,BOOLE,ARITHM,NUMERALS,REAL;
begin
reserve X for non  empty MetrSpace;
reserve Y for non  empty MetrSpace;
reserve Z for non  empty MetrSpace;
scheme LambdaMCART { X,Y,Z() -> non  empty set,F(set,set,set,set) -> (Element of Z()) } : (ex f being (Function of [: [: X(),Y() :],[: X(),Y() :] :],Z()) st (for x1,y1 being (Element of X()) holds (for x2,y2 being (Element of Y()) holds (for x,y being (Element of [: X(),Y() :]) holds ((x = [ x1,x2 ] & y = [ y1,y2 ]) implies ( f . (x,y) ) = F(x1,y1,x2,y2))))))
proof
deffunc G((Element of [: X(),Y() :]),(Element of [: X(),Y() :])) = F(( $1 `1 ),( $2 `1 ),( $1 `2 ),( $2 `2 ));
consider f being (Function of [: [: X(),Y() :],[: X(),Y() :] :],Z()) such that A1: (for x,y being (Element of [: X(),Y() :]) holds ( f . (x,y) ) = G(x,y)) from BINOP_1:sch 4;
take f;
let x1 being (Element of X());
let y1 being (Element of X());
let x2 being (Element of Y());
let y2 being (Element of Y());
let x being (Element of [: X(),Y() :]);
let y being (Element of [: X(),Y() :]);
assume that
A2: x = [ x1,x2 ]
and
A3: y = [ y1,y2 ];
y = [ ( y `1 ),( y `2 ) ] by MCART_1:21;
then A4: (y1 = ( y `1 ) & y2 = ( y `2 )) by A3,ZFMISC_1:27;
x = [ ( x `1 ),( x `2 ) ] by MCART_1:21;
then (x1 = ( x `1 ) & x2 = ( x `2 )) by A2,ZFMISC_1:27;
hence thesis by A1,A4;
end;
