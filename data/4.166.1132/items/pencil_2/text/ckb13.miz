begin
theorem
(for S being TopStruct holds (for X,Y being (Subset of S) holds (X,Y are_joinable  implies (ex f being  one-to-one (FinSequence of ( bool (the carrier of S) )) st (((X = ( f . 1 ) & Y = ( f . ( len f ) )) & (for W being (Subset of S) holds (W in ( rng f ) implies W is  closed_under_lines  strong))) & (for i being (Element of ( NAT )) holds ((1 <= i & i < ( len f )) implies 2 c= ( card ( ( f . i ) /\ ( f . ( i + 1 ) ) ) ))))))))
proof
defpred P[ set,set ]
 means
2 c= ( card ( $1 /\ $2 ) );
let S being TopStruct;
let X being (Subset of S);
let Y being (Subset of S);
assume X,Y are_joinable ;
then consider f being (FinSequence of ( bool (the carrier of S) )) such that A1: (X = ( f . 1 ) & Y = ( f . ( len f ) )) and A2: (for W being (Subset of S) holds (W in ( rng f ) implies W is  closed_under_lines  strong)) and A3: (for i being (Element of ( NAT )) holds ((1 <= i & i < ( len f )) implies 2 c= ( card ( ( f . i ) /\ ( f . ( i + 1 ) ) ) ))) by CKB12:def 1;
A4: (for i being (Element of ( NAT )) holds (for d1,d2 being set holds ((((1 <= i & i < ( len f )) & d1 = ( f . i )) & d2 = ( f . ( i + 1 ) )) implies P[ d1,d2 ]))) by A3;
consider g being  one-to-one (FinSequence of ( bool (the carrier of S) )) such that A5: (X = ( g . 1 ) & Y = ( g . ( len g ) )) and A6: ( rng g ) c= ( rng f ) and A7: (for i being (Element of ( NAT )) holds ((1 <= i & i < ( len g )) implies P[ ( g . i ),( g . ( i + 1 ) ) ])) from CKB8:sch 1(A1,A4);
take g;
thus thesis by A2,A5,A6,A7;
end;
