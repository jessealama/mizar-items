constructors XXREAL_0,RFINSEQ,NAT_D,PZFMISC1,TOPS_2,T_0TOPSP,POLYNOM1,PENCIL_1,REAL_1,RELSET_1,PBOOLE,FUNCT_7,CKB1;
requirements REAL,BOOLE,SUBSET,NUMERALS,ARITHM;
begin
theorem
Th7: (for I being non empty set holds (for A being  1-sorted-yielding (ManySortedSet of I) holds (for L being (ManySortedSubset of ( Carrier A )) holds (for i being (Element of I) holds (for S being (Subset of ( A . i )) holds ( L +* (i,S) ) is (ManySortedSubset of ( Carrier A )))))))
proof
let I being non empty set;
let A being  1-sorted-yielding (ManySortedSet of I);
let L being (ManySortedSubset of ( Carrier A ));
let i being (Element of I);
let S being (Subset of ( A . i ));
A1: L c= ( Carrier A ) by PBOOLE:def 18;
A2: ( dom L ) = I by PARTFUN1:def 2;
( L +* (i,S) ) c= ( Carrier A )
proof
let a being set;
assume a in I;
then reconsider b = a as (Element of I);
per cases ;
suppose A3: a = i;

then ( ( L +* (i,S) ) . b ) = S by A2,FUNCT_7:31;
then ( ( L +* (i,S) ) . b ) c= (the carrier of ( A . b )) by A3;
hence thesis by YELLOW_6:2;
end;
suppose a <> i;

then ( ( L +* (i,S) ) . a ) = ( L . b ) by FUNCT_7:32;
hence thesis by A1,PBOOLE:def 2;
end;
end;
hence thesis by PBOOLE:def 18;
end;
