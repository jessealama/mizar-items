theorem
Lm7: (for fs being FinSequence holds (1 <= ( len fs ) implies (fs = ( <* ( fs . 1 ) *> ^ ( Del (fs,1) ) ) & fs = ( ( Del (fs,( len fs )) ) ^ <* ( fs . ( len fs ) ) *> ))))
proof
let fs being FinSequence;
set fs1 = ( <* ( fs . 1 ) *> ^ ( Del (fs,1) ) );
set fs2 = ( ( Del (fs,( len fs )) ) ^ <* ( fs . ( len fs ) ) *> );
A1: ( len <* ( fs . 1 ) *> ) = 1 by FINSEQ_1:39;
assume A2: 1 <= ( len fs );
then A3: ( len fs ) in ( dom fs ) by FINSEQ_3:25;
A4: 1 in ( dom fs ) by A2,FINSEQ_3:25;
then A5: ( len fs ) = ( ( len <* ( fs . 1 ) *> ) + ( len ( Del (fs,1) ) ) ) by A1,WSIERP_1:def 1
.= ( len fs1 ) by FINSEQ_1:22;
(for b being Nat holds ((1 <= b & b <= ( len fs )) implies ( fs . b ) = ( fs1 . b )))
proof
let b being Nat;
assume that
A6: 1 <= b
and
A7: b <= ( len fs );
now
per cases  by A6,XXREAL_0:1;
suppose A8: b = 1;

1 in ( dom <* ( fs . 1 ) *> ) by A1,FINSEQ_3:25;
hence ( fs1 . b ) = ( <* ( fs . 1 ) *> . 1 ) by A8,FINSEQ_1:def 7
.= ( fs . b ) by A8,FINSEQ_1:40;
end;
suppose A9: b > 1;

then A10: ( b - 1 ) > ( 0 ) by XREAL_1:50;
then reconsider e = ( b - 1 ) as (Element of ( NAT )) by INT_1:3;
A11: e >= 1 by A10,NAT_1:14;
thus ( fs1 . b ) = ( ( Del (fs,1) ) . e ) by A1,A5,A7,A9,FINSEQ_1:24
.= ( fs . ( e + 1 ) ) by A4,A11,WSIERP_1:def 1
.= ( fs . b );
end;
end;
hence thesis;
end;
hence fs1 = fs by A5,FINSEQ_1:14;
( len <* ( fs . ( len fs ) ) *> ) = 1 by FINSEQ_1:39;
then A12: ( len fs ) = ( ( len <* ( fs . ( len fs ) ) *> ) + ( len ( Del (fs,( len fs )) ) ) ) by A3,WSIERP_1:def 1
.= ( len fs2 ) by FINSEQ_1:22;
A13: ( ( len ( Del (fs,( len fs )) ) ) + 1 ) = ( len fs ) by A3,WSIERP_1:def 1;
then A14: ( len ( Del (fs,( len fs )) ) ) = ( ( len fs ) - 1 );
(for b being Nat holds ((1 <= b & b <= ( len fs )) implies ( fs . b ) = ( fs2 . b )))
proof
let b being Nat;
assume that
A15: 1 <= b
and
A16: b <= ( len fs );
now
per cases  by A16,XXREAL_0:1;
suppose A17: b = ( len fs );

reconsider e = ( b - ( b - 1 ) ) as (Element of ( NAT ));
thus ( fs2 . b ) = ( <* ( fs . ( len fs ) ) *> . e ) by A13,A12,A17,FINSEQ_1:24,XREAL_1:146
.= ( fs . b ) by A17,FINSEQ_1:40;
end;
suppose A18: b < ( len fs );

then ( b + 1 ) <= ( len fs ) by NAT_1:13;
then b <= ( len ( Del (fs,( len fs )) ) ) by A14,XREAL_1:19;
then b in ( dom ( Del (fs,( len fs )) ) ) by A15,FINSEQ_3:25;
hence ( fs2 . b ) = ( ( Del (fs,( len fs )) ) . b ) by FINSEQ_1:def 7
.= ( fs . b ) by A3,A18,WSIERP_1:def 1;
end;
end;
hence thesis;
end;
hence fs2 = fs by A12,FINSEQ_1:14;
end;
