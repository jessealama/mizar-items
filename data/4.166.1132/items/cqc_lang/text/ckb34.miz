constructors ENUMSET1,PARTFUN1,BINOP_1,FUNCOP_1,FUNCT_4,XXREAL_0,MEMBERED,QC_LANG2,QC_LANG3,FINSEQ_2,RELSET_1,CKB4,CKB7,CKB9,CKB17,CKB20,CKB21,CKB22,CKB26,CKB27,CKB28,CKB30,CKB32;
requirements NUMERALS,SUBSET,BOOLE;
begin
reserve k for (Element of ( NAT ));
reserve x for bound_QC-variable;
reserve r for (Element of ( CQC-WFF ));
reserve s for (Element of ( CQC-WFF ));
scheme CQCFuncEx { D() -> non empty set,V() -> (Element of D()),A(set,set,set) -> (Element of D()),N(set) -> (Element of D()),C(set,set) -> (Element of D()),Q(set,set) -> (Element of D()) } : (ex F being (Function of ( CQC-WFF ),D()) st (( F . ( VERUM ) ) = V() & (for r holds (for s holds (for x holds (for k holds (for l being (CQC-variable_list of k) holds (for P being (QC-pred_symbol of k) holds (((( F . ( P ! l ) ) = A(k,P,l) & ( F . ( 'not' r ) ) = N(( F . r ))) & ( F . ( r '&' s ) ) = C(( F . r ),( F . s ))) & ( F . ( All (x,r) ) ) = Q(x,( F . r )))))))))))
proof
deffunc q((Element of ( QC-WFF )),(Element of D())) = Q(( bound_in $1 ),$2);
deffunc a((Element of ( QC-WFF ))) = A(( the_arity_of ( the_pred_symbol_of $1 ) ),( the_pred_symbol_of $1 ),( the_arguments_of $1 ));
consider F being (Function of ( QC-WFF ),D()) such that A1: (( F . ( VERUM ) ) = V() & (for p being (Element of ( QC-WFF )) holds ((((p is  atomic implies ( F . p ) = a(p)) & (p is  negative implies ( F . p ) = N(( F . ( the_argument_of p ) )))) & (p is  conjunctive implies ( F . p ) = C(( F . ( the_left_argument_of p ) ),( F . ( the_right_argument_of p ) )))) & (p is  universal implies ( F . p ) = q(p,( F . ( the_scope_of p ) )))))) from QC_LANG1:sch 3;
reconsider G = ( F | ( CQC-WFF ) ) as (Function of ( CQC-WFF ),D()) by FUNCT_2:32;
take G;
thus ( G . ( VERUM ) ) = V() by A1,FUNCT_1:49;
let r;
let s;
let x;
let k;
let l being (CQC-variable_list of k);
let P being (QC-pred_symbol of k);
A2: ( the_arity_of P ) = k by QC_LANG1:9;
A3: ( P ! l ) is  atomic by QC_LANG1:def 16;
then A4: (( the_arguments_of ( P ! l ) ) = l & ( the_pred_symbol_of ( P ! l ) ) = P) by QC_LANG1:def 20,QC_LANG1:def 21;
thus ( G . ( P ! l ) ) = ( F . ( P ! l ) ) by FUNCT_1:49
.= A(k,P,l) by A1,A3,A4,A2;
set r9 = ( G . r );
set s9 = ( G . s );
A5: r9 = ( F . r ) by FUNCT_1:49;
A6: ( r '&' s ) is  conjunctive by QC_LANG1:def 18;
then A7: (( the_left_argument_of ( r '&' s ) ) = r & ( the_right_argument_of ( r '&' s ) ) = s) by QC_LANG1:def 23,QC_LANG1:def 24;
A8: ( 'not' r ) is  negative by QC_LANG1:def 17;
then A9: ( the_argument_of ( 'not' r ) ) = r by QC_LANG1:def 22;
thus ( G . ( 'not' r ) ) = ( F . ( 'not' r ) ) by FUNCT_1:49
.= N(r9) by A1,A5,A8,A9;
A10: s9 = ( F . s ) by FUNCT_1:49;
thus ( G . ( r '&' s ) ) = ( F . ( r '&' s ) ) by FUNCT_1:49
.= C(r9,s9) by A1,A5,A10,A6,A7;
A11: ( All (x,r) ) is  universal by QC_LANG1:def 19;
then A12: (( bound_in ( All (x,r) ) ) = x & ( the_scope_of ( All (x,r) ) ) = r) by QC_LANG1:def 25,QC_LANG1:def 26;
thus ( G . ( All (x,r) ) ) = ( F . ( All (x,r) ) ) by FUNCT_1:49
.= Q(x,r9) by A1,A5,A11,A12;
end;
