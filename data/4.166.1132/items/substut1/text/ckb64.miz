begin
reserve x1 for bound_QC-variable;
reserve x2 for bound_QC-variable;
reserve B for (Element of [: ( QC-Sub-WFF ),( bound_QC-variables ) :]);
reserve SQ for (second_Q_comp of B);
definition
let A being set;
assume that
A1: A is  Sub_universal;
func Sub_the_bound_of A -> bound_QC-variable means 
(ex B st (ex SQ st ((A = ( Sub_All (B,SQ) ) & ( B `2 ) = it) & B is  quantifiable)));
existence
proof
consider B,SQ such that A2: (A = ( Sub_All (B,SQ) ) & B is  quantifiable) by A1,CKB58:def 1;
take ( B `2 );
thus thesis by A2;
end;
uniqueness
proof
let x1;
let x2;
assume that
A3: (ex B st (ex SQ st ((A = ( Sub_All (B,SQ) ) & ( B `2 ) = x1) & B is  quantifiable)))
and
A4: (ex B st (ex SQ st ((A = ( Sub_All (B,SQ) ) & ( B `2 ) = x2) & B is  quantifiable)));
consider B1 being (Element of [: ( QC-Sub-WFF ),( bound_QC-variables ) :]),SQ1 being (second_Q_comp of B1) such that A5: A = ( Sub_All (B1,SQ1) ) and A6: ( B1 `2 ) = x1 and A7: B1 is  quantifiable by A3;
consider B2 being (Element of [: ( QC-Sub-WFF ),( bound_QC-variables ) :]),SQ2 being (second_Q_comp of B2) such that A8: A = ( Sub_All (B2,SQ2) ) and A9: ( B2 `2 ) = x2 and A10: B2 is  quantifiable by A4;
A11: [ ( All (( B2 `2 ),( ( B2 `1 ) `1 )) ),SQ2 ] = A by A8,A10,CKB50:def 1;
[ ( All (( B1 `2 ),( ( B1 `1 ) `1 )) ),SQ1 ] = A by A5,A7,CKB50:def 1;
then ( All (( B1 `2 ),( ( B1 `1 ) `1 )) ) = ( All (( B2 `2 ),( ( B2 `1 ) `1 )) ) by A11,ZFMISC_1:27;
hence thesis by A6,A9,QC_LANG2:5;
end;
