begin
theorem
Th10: (for T being non empty  normal TopSpace holds (for A,B being  closed (Subset of T) holds ((A <> ( {} ) & A misses B) implies (for G being (Rain of A,B) holds (for x being Real holds (x in ( DYADIC ) implies (ex y being (Subset of T) st (for n being (Element of ( NAT )) holds (x in ( dyadic n ) implies y = ( ( G . n ) . x ))))))))))
proof
let T being non empty  normal TopSpace;
let A being  closed (Subset of T),B being  closed (Subset of T);
assume A1: (A <> ( {} ) & A misses B);
let G being (Rain of A,B);
let x being Real;
assume A2: x in ( DYADIC );
reconsider s = ( inf_number_dyadic x ) as (Element of ( NAT ));
( G . s ) is (Drizzle of A,B,s) by A1,CKB8:def 1;
then reconsider y = ( ( G . s ) . x ) as (Subset of T) by A2,CKB10:1,FUNCT_2:5;
take y;
let n being (Element of ( NAT ));
assume x in ( dyadic n );
then consider m being Nat such that A3: n = ( s + m ) by CKB12:1,NAT_1:10;
m in ( NAT ) by ORDINAL1:def 12;
hence thesis by A1,A2,A3,CKB13:1;
end;
