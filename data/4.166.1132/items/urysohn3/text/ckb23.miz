begin
theorem
Th17: (for T being non  empty TopSpace holds (for A,B being (Subset of T) holds (for G being (Rain of A,B) holds (for p being (Point of T) holds (for S being non  empty (Subset of ( ExtREAL )) holds (S = ( Rainbow (p,G) ) implies (for e1 being R_eal holds (e1 = 1 implies (( 0. ) <= ( sup S ) & ( sup S ) <= e1)))))))))
proof
reconsider a = ( 0 ),b = 1 as R_eal by XXREAL_0:def 1;
let T being non  empty TopSpace;
let A being (Subset of T);
let B being (Subset of T);
let G being (Rain of A,B);
let p being (Point of T);
let S being non  empty (Subset of ( ExtREAL ));
consider s being set such that A1: s in S by XBOOLE_0:def 1;
reconsider s as R_eal by A1;
assume S = ( Rainbow (p,G) );
then S c= ( DYADIC ) by CKB21:1;
then A2: S c= [. a,b .] by URYSOHN2:28,XBOOLE_1:1;
let e1 being R_eal;
assume e1 = 1;
then (for x being  ext-real number holds (x in S implies x <= e1)) by A2,XXREAL_1:1;
then A3: e1 is (UpperBound of S) by XXREAL_2:def 1;
( 0. ) <= s by A2,A1,XXREAL_1:1;
hence thesis by A3,A1,XXREAL_2:4,XXREAL_2:def 3;
end;
