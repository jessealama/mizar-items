Th12: (for T being non  empty  normal TopSpace holds (for A,B being  closed (Subset of T) holds ((A <> ( {} ) & A misses B) implies (for G being (Rain of A,B) holds (for r being Real holds (for C being (Subset of T) holds ((C = ( ( Tempest G ) . r ) & r in ( DOM )) implies C is  open)))))))
proof
let T being non  empty  normal TopSpace;
let A being  closed (Subset of T);
let B being  closed (Subset of T);
assume A1: (A <> ( {} ) & A misses B);
let G being (Rain of A,B);
let r being Real;
let C being (Subset of T);
