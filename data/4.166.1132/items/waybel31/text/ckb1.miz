begin
scheme UparrowUnion { L1() -> RelStr,P[set] } : (for S being (Subset-Family of L1()) holds (S = { X where X is (Subset of L1()): P[ X ] } implies ( uparrow ( union S ) ) = ( union { ( uparrow X ) where X is (Subset of L1()): P[ X ] } )))
proof
let S being (Subset-Family of L1());
assume A1: S = { X where X is (Subset of L1()): P[ X ] };
A2: ( union { ( uparrow X ) where X is (Subset of L1()): P[ X ] } ) c= ( uparrow ( union S ) )
proof
let z being set;
assume z in ( union { ( uparrow X ) where X is (Subset of L1()): P[ X ] } );
then consider Z being set such that A3: z in Z and A4: Z in { ( uparrow X ) where X is (Subset of L1()): P[ X ] } by TARSKI:def 4;
consider X1 being (Subset of L1()) such that A5: Z = ( uparrow X1 ) and A6: P[ X1 ] by A4;
reconsider z1 = z as (Element of L1()) by A3,A5;
consider y1 being (Element of L1()) such that A7: y1 <= z1 and A8: y1 in X1 by A3,A5,WAYBEL_0:def 16;
X1 in S by A1,A6;
then y1 in ( union S ) by A8,TARSKI:def 4;
hence thesis by A7,WAYBEL_0:def 16;
end;
( uparrow ( union S ) ) c= ( union { ( uparrow X ) where X is (Subset of L1()): P[ X ] } )
proof
let z being set;
assume A9: z in ( uparrow ( union S ) );
then reconsider z1 = z as (Element of L1());
consider y1 being (Element of L1()) such that A10: y1 <= z1 and A11: y1 in ( union S ) by A9,WAYBEL_0:def 16;
consider Z being set such that A12: y1 in Z and A13: Z in S by A11,TARSKI:def 4;
consider X1 being (Subset of L1()) such that A14: Z = X1 and A15: P[ X1 ] by A1,A13;
A16: ( uparrow X1 ) in { ( uparrow X ) where X is (Subset of L1()): P[ X ] } by A15;
z in ( uparrow X1 ) by A10,A12,A14,WAYBEL_0:def 16;
hence thesis by A16,TARSKI:def 4;
end;
hence thesis by A2,XBOOLE_0:def 10;
end;
