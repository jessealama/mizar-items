begin
definition
let L1 being  continuous sup-Semilattice;
func CLweight L1 -> Cardinal
equals
( meet { ( card B1 ) where B1 is  with_bottom (CLbasis of L1): (not contradiction) } );
coherence
proof
set X = { ( card B2 ) where B2 is  with_bottom (CLbasis of L1): (not contradiction) };
defpred P[ Ordinal ]
 means
$1 in { ( card B1 ) where B1 is  with_bottom (CLbasis of L1): (not contradiction) };
A1: (ex A being Ordinal st P[ A ])
proof
take ( card ( [#] L1 ) );
( [#] L1 ) is (CLbasis of L1) by YELLOW15:25;
hence thesis;
end;
