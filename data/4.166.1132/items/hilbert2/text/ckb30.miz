begin
reserve n for (Element of ( NAT ));
reserve p for (Element of ( HP-WFF ));
reserve q for (Element of ( HP-WFF ));
reserve r for (Element of ( HP-WFF ));
reserve s for (Element of ( HP-WFF ));
reserve r9 for (Element of ( HP-WFF ));
reserve s9 for (Element of ( HP-WFF ));
reserve t for FinSequence;
reserve t1 for FinSequence;
theorem
Th17: (p = ( q ^ t ) implies p = q)
proof
defpred P[ Nat ]
 means
(for p holds (for q holds (for t holds ((( len p ) = $1 & p = ( q ^ t )) implies p = q))));
A1: (for n being Nat holds ((for k being Nat holds (k < n implies P[ k ])) implies P[ n ]))
proof
let n being Nat;
assume that
A2: (for k being Nat holds (k < n implies (for p holds (for q holds (for t holds ((( len p ) = k & p = ( q ^ t )) implies p = q))))));
let p;
let q;
let t;
assume that
A3: ( len p ) = n
and
A4: p = ( q ^ t );
( len q ) >= 1 by CKB23:1;
then A5: ( p . 1 ) = ( q . 1 ) by A4,FINSEQ_1:64;
per cases  by CKB22:1;
suppose p is  conjunctive;

then consider r,s such that A6: p = ( r '&' s ) by CKB18:def 1;
( q . 1 ) = ( ( <* 2 *> ^ ( r ^ s ) ) . 1 ) by A5,A6,FINSEQ_1:32
.= 2 by FINSEQ_1:41;
then q is  conjunctive by CKB25:1;
then consider r9,s9 such that A7: q = ( r9 '&' s9 ) by CKB18:def 1;
( <* 2 *> ^ ( ( r9 ^ s9 ) ^ t ) ) = ( ( <* 2 *> ^ ( r9 ^ s9 ) ) ^ t ) by FINSEQ_1:32
.= ( ( <* 2 *> ^ r ) ^ s ) by A4,A6,A7,FINSEQ_1:32
.= ( <* 2 *> ^ ( r ^ s ) ) by FINSEQ_1:32;
then ( ( r9 ^ s9 ) ^ t ) = ( r ^ s ) by CKB2:1;
then A8: ( r9 ^ ( s9 ^ t ) ) = ( r ^ s ) by FINSEQ_1:32;
now
per cases ;
suppose A9: ( len r ) <= ( len r9 );

n = ( ( len q ) + ( len t ) ) by A3,A4,FINSEQ_1:22;
then ( len q ) <= n by NAT_1:11;
then A10: ( len r9 ) < n by A7,CKB28:1,XXREAL_0:2;
(ex t1 st ( r ^ t1 ) = r9) by A8,A9,FINSEQ_1:47;
then r = r9 by A2,A10;
then A11: ( s9 ^ t ) = s by A8,FINSEQ_1:33;
( len s ) < n by A3,A6,CKB28:1;
then s9 = s by A2,A11;
then t = ( {} ) by A11,FINSEQ_1:87;
hence thesis by A4,FINSEQ_1:34;
end;
suppose ( len r ) >= ( len r9 );

then A12: (ex t1 st ( r9 ^ t1 ) = r) by A8,FINSEQ_1:47;
( len r ) < n by A3,A6,CKB28:1;
then r = r9 by A2,A12;
then A13: ( s9 ^ t ) = s by A8,FINSEQ_1:33;
( len s ) < n by A3,A6,CKB28:1;
then s9 = s by A2,A13;
then t = ( {} ) by A13,FINSEQ_1:87;
hence thesis by A4,FINSEQ_1:34;
end;
end;
hence thesis;
end;
suppose p is  conditional;

then consider r,s such that A14: p = ( r => s ) by CKB19:def 1;
( q . 1 ) = ( ( <* 1 *> ^ ( r ^ s ) ) . 1 ) by A5,A14,FINSEQ_1:32
.= 1 by FINSEQ_1:41;
then q is  conditional by CKB24:1;
then consider r9,s9 such that A15: q = ( r9 => s9 ) by CKB19:def 1;
( <* 1 *> ^ ( ( r9 ^ s9 ) ^ t ) ) = ( ( <* 1 *> ^ ( r9 ^ s9 ) ) ^ t ) by FINSEQ_1:32
.= ( ( <* 1 *> ^ r ) ^ s ) by A4,A14,A15,FINSEQ_1:32
.= ( <* 1 *> ^ ( r ^ s ) ) by FINSEQ_1:32;
then ( ( r9 ^ s9 ) ^ t ) = ( r ^ s ) by CKB2:1;
then A16: ( r9 ^ ( s9 ^ t ) ) = ( r ^ s ) by FINSEQ_1:32;
now
per cases ;
suppose A17: ( len r ) <= ( len r9 );

n = ( ( len q ) + ( len t ) ) by A3,A4,FINSEQ_1:22;
then ( len q ) <= n by NAT_1:11;
then A18: ( len r9 ) < n by A15,CKB29:1,XXREAL_0:2;
(ex t1 st ( r ^ t1 ) = r9) by A16,A17,FINSEQ_1:47;
then r = r9 by A2,A18;
then A19: ( s9 ^ t ) = s by A16,FINSEQ_1:33;
( len s ) < n by A3,A14,CKB29:1;
then s9 = s by A2,A19;
then t = ( {} ) by A19,FINSEQ_1:87;
hence thesis by A4,FINSEQ_1:34;
end;
suppose ( len r ) >= ( len r9 );

then A20: (ex t1 st ( r9 ^ t1 ) = r) by A16,FINSEQ_1:47;
( len r ) < n by A3,A14,CKB29:1;
then r = r9 by A2,A20;
then A21: ( s9 ^ t ) = s by A16,FINSEQ_1:33;
( len s ) < n by A3,A14,CKB29:1;
then s9 = s by A2,A21;
then t = ( {} ) by A21,FINSEQ_1:87;
hence thesis by A4,FINSEQ_1:34;
end;
end;
hence thesis;
end;
suppose A22: p is  simple;

A23: q <> ( {} ) by CKB23:1,CARD_1:27;
(ex n st p = ( prop n )) by A22,CKB20:def 1;
hence thesis by A4,A23,FINSEQ_1:88;
end;
suppose A24: p = ( VERUM );

q <> ( {} ) by CKB23:1,CARD_1:27;
hence thesis by A4,A24,FINSEQ_1:88;
end;
end;
A25: (for n being Nat holds P[ n ]) from NAT_1:sch 4(A1);
( len p ) = ( len p );
hence thesis by A25;
end;
