reserve Y for non empty TopSpace;
reserve f for (Function of X,Y);
theorem
(f is  sp-continuous iff f is  ps-continuous  (sp,ps)-continuous)
proof
hereby
assume A1: f is  sp-continuous;
thus f is  ps-continuous
proof
let V being (Subset of Y);
assume V is  open;
then ( f " V ) in ( SPO X ) by A1,CKB47:def 1;
hence thesis;
end;

thus f is  (sp,ps)-continuous
proof
let G being (Subset of Y);
assume G is  open;
then ( f " G ) in ( SPO X ) by A1,CKB47:def 1;
then ( f " G ) in ( ( PSO X ) /\ ( D(sp,ps) X ) ) by CKB42:1;
hence thesis by XBOOLE_0:def 4;
end;

end;
assume A2: f is  ps-continuous  (sp,ps)-continuous;
let V being (Subset of Y);
assume V is  open;
then ( f " V ) in ( PSO X ) by A2,CKB46:def 1;
hence thesis;
end;
