theorem
(f is  p-continuous iff f is  sp-continuous  (p,sp)-continuous)
proof
hereby
assume A1: f is  p-continuous;
thus f is  sp-continuous
proof
let V being (Subset of Y);
assume V is  open;
then ( f " V ) in ( PO X ) by A1,CKB44:def 1;
hence thesis;
end;

thus f is  (p,sp)-continuous
proof
let G being (Subset of Y);
assume G is  open;
then ( f " G ) in ( PO X ) by A1,CKB44:def 1;
then ( f " G ) in ( ( SPO X ) /\ ( D(p,sp) X ) ) by CKB39:1;
hence thesis by XBOOLE_0:def 4;
end;

end;
assume A2: f is  sp-continuous  (p,sp)-continuous;
let V being (Subset of Y);
assume V is  open;
then ( f " V ) in ( SPO X ) by A2,CKB47:def 1;
hence thesis;
end;
