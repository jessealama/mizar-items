constructors FUNCT_4,REAL_1,SQUARE_1,RFUNCT_1,MEASURE6,INTEGRA1,FUZZY_2,SEQ_1,XXREAL_2,SEQ_4,RELSET_1,RELAT_1,FUZZY_1,BINOP_2,RVSUM_1,BINOP_1;
requirements NUMERALS,REAL,SUBSET,BOOLE;
begin
reserve x for set;
reserve C1 for non empty set;
theorem
Th1: (for F being (Membership_Func of C1) holds ((( rng F ) is  bounded & (for x holds (x in ( dom F ) implies ( F . x ) <= ( upper_bound ( rng F ) )))) & (for x holds (x in ( dom F ) implies ( F . x ) >= ( lower_bound ( rng F ) )))))
proof
let F being (Membership_Func of C1);
A1: [. ( 0 ),1 .] is non empty  closed_interval non empty  closed_interval (Subset of ( REAL )) by MEASURE5:14;
A2: ( rng F ) c= [. ( 0 ),1 .] by RELAT_1:def 19;
then A3: ( rng F ) is  bounded by A1,XXREAL_2:45;
A4: (for x holds (x in ( dom F ) implies ( F . x ) >= ( lower_bound ( rng F ) )))
proof
let x;
assume x in ( dom F );
then A5: ( F . x ) in ( rng F ) by FUNCT_1:def 3;
( rng F ) is  bounded_below by A3,XXREAL_2:def 11;
hence thesis by A5,SEQ_4:def 2;
end;
(for x holds (x in ( dom F ) implies ( F . x ) <= ( upper_bound ( rng F ) )))
proof
let x;
assume x in ( dom F );
then A6: ( F . x ) in ( rng F ) by FUNCT_1:def 3;
( rng F ) is  bounded_above by A3,XXREAL_2:def 11;
hence thesis by A6,SEQ_4:def 1;
end;
hence thesis by A2,A1,A4,XXREAL_2:45;
end;
