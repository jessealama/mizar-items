begin
reserve c for set;
reserve x for set;
reserve y for set;
reserve z for set;
reserve C1 for non  empty set;
reserve C2 for non  empty set;
reserve C3 for non  empty set;
theorem
(for f,g being (RMembership_Func of C1,C2) holds (for h,k being (RMembership_Func of C2,C3) holds ((f c= g & h c= k) implies ( f (#) h ) c= ( g (#) k ))))
proof
let f being (RMembership_Func of C1,C2);
let g being (RMembership_Func of C1,C2);
let h being (RMembership_Func of C2,C3);
let k being (RMembership_Func of C2,C3);
assume that
A1: f c= g
and
A2: h c= k;
let c being (Element of [: C1,C3 :]);
consider x,z being set such that A3: x in C1 and A4: z in C3 and A5: c = [ x,z ] by ZFMISC_1:def 2;
(for y being set holds (y in C2 implies (( f . [ x,y ] ) <= ( g . [ x,y ] ) & ( h . [ y,z ] ) <= ( k . [ y,z ] ))))
proof
let y being set;
assume A6: y in C2;
then A7: [ y,z ] in [: C2,C3 :] by A4,ZFMISC_1:87;
[ x,y ] in [: C1,C2 :] by A3,A6,ZFMISC_1:87;
hence thesis by A1,A2,A7,FUZZY_1:def 2;
end;
hence thesis by A3,A4,A5,CKB29:1;
end;
