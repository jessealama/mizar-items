environ
vocabularies NUMBERS,XBOOLE_0,FUZZY_1,RELAT_1,SUBSET_1,FUNCT_1,XXREAL_2,XXREAL_0,ORDINAL2,XXREAL_1,CARD_1,INTEGRA1,TARSKI,XREAL_0,ORDINAL1,ARYTM_3,ARYTM_1,FUZZY_2,ZFMISC_1,PARTFUN1,VALUED_1,SEQ_4,FUZZY_4,MEASURE5;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,ORDINAL1,NUMBERS,XCMPLX_0,XXREAL_0,XREAL_0,SQUARE_1,BINOP_1,RELAT_1,RELSET_1,PARTFUN1,XXREAL_2,FUNCT_4,SEQ_1,INTEGRA1,RCOMP_1,MEASURE6,FUZZY_1,FUZZY_2,SEQ_4,MEASURE5;
definitions TARSKI,FUZZY_1,FUZZY_2,BINOP_1,RELAT_1;
theorems FUZZY_1,FUNCT_3,PARTFUN1,FUNCT_1,INTEGRA1,INTEGRA2,ZFMISC_1,SEQ_4,SUBSET_1,XBOOLE_0,FUZZY_2,XXREAL_0,FUNCT_4,XXREAL_2,XREAL_1,RELSET_1,MEMBERED,FUNCT_2,RELAT_1,MEASURE5,CKB2;
schemes PARTFUN1,BINOP_1;
registrations XBOOLE_0,SUBSET_1,NUMBERS,XXREAL_0,XREAL_0,MEMBERED,INTEGRA1,RELAT_1,VALUED_0,RELSET_1,MEASURE5,CKB1;
constructors FUNCT_4,REAL_1,SQUARE_1,RFUNCT_1,MEASURE6,INTEGRA1,FUZZY_2,SEQ_1,XXREAL_2,SEQ_4,RELSET_1,RELAT_1,FUZZY_1,BINOP_2,RVSUM_1,BINOP_1;
requirements NUMERALS,REAL,SUBSET,BOOLE;
begin
reserve c for set;
reserve x for set;
reserve y for set;
reserve C1 for non  empty set;
theorem
(for F,G being (Membership_Func of C1) holds ((for x holds (x in C1 implies ( F . x ) <= ( G . x ))) implies ( upper_bound ( rng F ) ) <= ( upper_bound ( rng G ) )))
proof
let F being (Membership_Func of C1);
let G being (Membership_Func of C1);
( rng F ) is  bounded by CKB2:1;
then A1: ( rng F ) is  bounded_above by XXREAL_2:def 11;
assume A2: (for c holds (c in C1 implies ( F . c ) <= ( G . c )));
A3: (for s being  real number holds (( 0 ) < s implies (ex y st (y in ( dom F ) & ( ( upper_bound ( rng F ) ) - s ) <= ( G . y )))))
proof
let s being  real number;
assume ( 0 ) < s;
then consider r being  real number such that A4: r in ( rng F ) and A5: ( ( upper_bound ( rng F ) ) - s ) < r by A1,SEQ_4:def 1;
consider y being set such that A6: y in ( dom F ) and A7: r = ( F . y ) by A4,FUNCT_1:def 3;
r <= ( G . y ) by A2,A6,A7;
hence thesis by A5,A6,XXREAL_0:2;
end;
(for s being  real number holds (( 0 ) < s implies ( ( upper_bound ( rng F ) ) - s ) <= ( upper_bound ( rng G ) )))
proof
let s being  real number;
assume ( 0 ) < s;
then consider y such that A8: y in ( dom F ) and A9: ( ( upper_bound ( rng F ) ) - s ) <= ( G . y ) by A3;
y in C1 by A8;
then y in ( dom G ) by FUNCT_2:def 1;
then ( G . y ) <= ( upper_bound ( rng G ) ) by CKB2:1;
hence thesis by A9,XXREAL_0:2;
end;
hence thesis by XREAL_1:57;
end;
