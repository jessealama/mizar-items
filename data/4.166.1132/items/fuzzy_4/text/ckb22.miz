constructors FUNCT_4,REAL_1,SQUARE_1,RFUNCT_1,MEASURE6,INTEGRA1,FUZZY_2,SEQ_1,XXREAL_2,SEQ_4,RELSET_1,RELAT_1,FUZZY_1,BINOP_2,RVSUM_1,BINOP_1,CKB5,CKB8,CKB17,CKB18;
requirements NUMERALS,REAL,SUBSET,BOOLE;
begin
reserve C1 for non empty set;
reserve C2 for non empty set;
reserve C3 for non empty set;
theorem
(for f,g being (RMembership_Func of C1,C2) holds (for h being (RMembership_Func of C2,C3) holds ( ( max (f,g) ) (#) h ) = ( max (( f (#) h ),( g (#) h )) )))
proof
let f being (RMembership_Func of C1,C2),g being (RMembership_Func of C1,C2);
let h being (RMembership_Func of C2,C3);
A1: ( dom ( max (( f (#) h ),( g (#) h )) ) ) = [: C1,C3 :] by FUNCT_2:def 1;
A2: (for c being (Element of [: C1,C3 :]) holds (c in [: C1,C3 :] implies ( ( ( max (f,g) ) (#) h ) . c ) = ( ( max (( f (#) h ),( g (#) h )) ) . c )))
proof
let c being (Element of [: C1,C3 :]);
consider x being set,z being set such that A3: x in C1 and A4: z in C3 and A5: c = [ x,z ] by ZFMISC_1:def 2;
( ( ( max (f,g) ) (#) h ) . c ) = ( ( ( max (f,g) ) (#) h ) . (x,z) ) by A5
.= ( upper_bound ( rng ( min (( max (f,g) ),h,x,z) ) ) ) by A5,CKB18:def 1
.= ( max (( upper_bound ( rng ( min (f,h,x,z) ) ) ),( upper_bound ( rng ( min (g,h,x,z) ) ) )) ) by A3,A4,CKB21:1
.= ( max (( ( f (#) h ) . (x,z) ),( upper_bound ( rng ( min (g,h,x,z) ) ) )) ) by A5,CKB18:def 1
.= ( max (( ( f (#) h ) . (x,z) ),( ( g (#) h ) . (x,z) )) ) by A5,CKB18:def 1
.= ( ( max (( f (#) h ),( g (#) h )) ) . c ) by A5,FUZZY_1:def 4;
hence thesis;
end;
( dom ( ( max (f,g) ) (#) h ) ) = [: C1,C3 :] by FUNCT_2:def 1;
hence thesis by A1,A2,PARTFUN1:5;
end;
