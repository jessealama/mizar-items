registrations XBOOLE_0,SUBSET_1,NUMBERS,XXREAL_0,XREAL_0,MEMBERED,INTEGRA1,RELAT_1,VALUED_0,RELSET_1,MEASURE5,CKB1;
constructors FUNCT_4,REAL_1,SQUARE_1,RFUNCT_1,MEASURE6,INTEGRA1,FUZZY_2,SEQ_1,XXREAL_2,SEQ_4,RELSET_1,RELAT_1,FUZZY_1,BINOP_2,RVSUM_1,BINOP_1,CKB5,CKB8;
requirements NUMERALS,REAL,SUBSET,BOOLE;
begin
reserve c for set;
reserve x for set;
reserve y for set;
reserve C1 for non  empty set;
reserve C2 for non  empty set;
theorem
(for f,g being (RMembership_Func of C1,C2) holds (f c= g implies ( converse f ) c= ( converse g )))
proof
let f being (RMembership_Func of C1,C2);
let g being (RMembership_Func of C1,C2);
assume A1: f c= g;
let c being (Element of [: C2,C1 :]);
(ex y st (ex x st ((y in C2 & x in C1) & c = [ y,x ]))) by ZFMISC_1:def 2;
then consider x,y being set such that A2: x in C1 and A3: y in C2 and A4: c = [ y,x ];
[ x,y ] in [: C1,C2 :] by A2,A3,ZFMISC_1:87;
then ( f . [ x,y ] ) <= ( g . [ x,y ] ) by A1,FUZZY_1:def 2;
hence thesis by A2,A3,A4,CKB13:1;
end;
