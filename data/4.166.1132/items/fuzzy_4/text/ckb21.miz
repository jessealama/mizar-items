begin
reserve y for set;
reserve C1 for non empty set;
reserve C2 for non empty set;
reserve C3 for non empty set;
theorem
Lm2: (for f,g being (RMembership_Func of C1,C2) holds (for h being (RMembership_Func of C2,C3) holds (for x,z being set holds ((x in C1 & z in C3) implies ( upper_bound ( rng ( min (( max (f,g) ),h,x,z) ) ) ) = ( max (( upper_bound ( rng ( min (f,h,x,z) ) ) ),( upper_bound ( rng ( min (g,h,x,z) ) ) )) )))))
proof
let f being (RMembership_Func of C1,C2),g being (RMembership_Func of C1,C2);
let h being (RMembership_Func of C2,C3);
let x being set,z being set;
assume that
A1: x in C1
and
A2: z in C3;
A3: (for y being (Element of C2) holds (y in C2 implies ( ( min (( max (f,g) ),h,x,z) ) . y ) = ( ( max (( min (f,h,x,z) ),( min (g,h,x,z) )) ) . y )))
proof
let y being (Element of C2);
assume y in C2;
A4: [ x,y ] in [: C1,C2 :] by A1,ZFMISC_1:87;
( ( min (( max (f,g) ),h,x,z) ) . y ) = ( min (( ( max (f,g) ) . [ x,y ] ),( h . [ y,z ] )) ) by A1,A2,CKB17:def 1
.= ( min (( max (( f . [ x,y ] ),( g . [ x,y ] )) ),( h . [ y,z ] )) ) by A4,FUZZY_1:def 4
.= ( max (( min (( f . [ x,y ] ),( h . [ y,z ] )) ),( min (( g . [ x,y ] ),( h . [ y,z ] )) )) ) by XXREAL_0:38
.= ( max (( min (( f . [ x,y ] ),( h . [ y,z ] )) ),( ( min (g,h,x,z) ) . y )) ) by A1,A2,CKB17:def 1
.= ( max (( ( min (f,h,x,z) ) . y ),( ( min (g,h,x,z) ) . y )) ) by A1,A2,CKB17:def 1;
hence thesis by FUZZY_1:def 4;
end;
set F = ( min (f,h,x,z) );
set G = ( min (g,h,x,z) );
A5: ( dom ( max (( min (f,h,x,z) ),( min (g,h,x,z) )) ) ) = C2 by FUNCT_2:def 1;
( rng ( max (F,G) ) ) is  bounded by CKB2:1;
then A6: ( rng ( max (F,G) ) ) is  bounded_above by XXREAL_2:def 11;
A7: (for y holds (y in ( dom ( max (F,G) ) ) implies ( ( max (F,G) ) . y ) <= ( upper_bound ( rng ( max (F,G) ) ) )))
proof
let y;
assume y in ( dom ( max (F,G) ) );
then ( ( max (F,G) ) . y ) in ( rng ( max (F,G) ) ) by FUNCT_1:def 3;
hence thesis by A6,SEQ_4:def 1;
end;
( rng G ) is  bounded by CKB2:1;
then A8: ( rng G ) is  bounded_above by XXREAL_2:def 11;
A9: (for y holds (y in ( dom G ) implies ( G . y ) <= ( upper_bound ( rng G ) )))
proof
let y;
assume y in ( dom G );
then ( G . y ) in ( rng G ) by FUNCT_1:def 3;
hence thesis by A8,SEQ_4:def 1;
end;
A10: (for s being  real number holds (( 0 ) < s implies (ex y st (y in ( dom G ) & ( ( upper_bound ( rng G ) ) - s ) <= ( ( max (F,G) ) . y )))))
proof
let s being  real number;
assume ( 0 ) < s;
then consider r being  real number such that A11: r in ( rng G ) and A12: ( ( upper_bound ( rng G ) ) - s ) < r by A8,SEQ_4:def 1;
consider y being set such that A13: y in ( dom G ) and A14: r = ( G . y ) by A11,FUNCT_1:def 3;
( G . y ) <= ( max (( F . y ),( G . y )) ) by XXREAL_0:25;
then r <= ( ( max (F,G) ) . y ) by A13,A14,FUZZY_1:def 4;
hence thesis by A12,A13,XXREAL_0:2;
end;
(for s being  real number holds (( 0 ) < s implies ( ( upper_bound ( rng G ) ) - s ) <= ( upper_bound ( rng ( max (F,G) ) ) )))
proof
let s being  real number;
assume ( 0 ) < s;
then consider y such that A15: y in ( dom G ) and A16: ( ( upper_bound ( rng G ) ) - s ) <= ( ( max (F,G) ) . y ) by A10;
y in C2 by A15;
then y in ( dom ( max (F,G) ) ) by FUNCT_2:def 1;
then ( ( max (F,G) ) . y ) <= ( upper_bound ( rng ( max (F,G) ) ) ) by A7;
hence thesis by A16,XXREAL_0:2;
end;
then A17: ( upper_bound ( rng G ) ) <= ( upper_bound ( rng ( max (F,G) ) ) ) by XREAL_1:57;
( rng F ) is  bounded by CKB2:1;
then A18: ( rng F ) is  bounded_above by XXREAL_2:def 11;
A19: (for s being  real number holds (( 0 ) < s implies (ex y st (y in ( dom F ) & ( ( upper_bound ( rng F ) ) - s ) <= ( ( max (F,G) ) . y )))))
proof
let s being  real number;
assume ( 0 ) < s;
then consider r being  real number such that A20: r in ( rng F ) and A21: ( ( upper_bound ( rng F ) ) - s ) < r by A18,SEQ_4:def 1;
consider y being set such that A22: y in ( dom F ) and A23: r = ( F . y ) by A20,FUNCT_1:def 3;
( F . y ) <= ( max (( F . y ),( G . y )) ) by XXREAL_0:25;
then r <= ( ( max (F,G) ) . y ) by A22,A23,FUZZY_1:def 4;
hence thesis by A21,A22,XXREAL_0:2;
end;
(for s being  real number holds (( 0 ) < s implies ( ( upper_bound ( rng F ) ) - s ) <= ( upper_bound ( rng ( max (F,G) ) ) )))
proof
let s being  real number;
assume ( 0 ) < s;
then consider y such that A24: y in ( dom F ) and A25: ( ( upper_bound ( rng F ) ) - s ) <= ( ( max (F,G) ) . y ) by A19;
y in C2 by A24;
then y in ( dom ( max (F,G) ) ) by FUNCT_2:def 1;
then ( ( max (F,G) ) . y ) <= ( upper_bound ( rng ( max (F,G) ) ) ) by A7;
hence thesis by A25,XXREAL_0:2;
end;
then ( upper_bound ( rng F ) ) <= ( upper_bound ( rng ( max (F,G) ) ) ) by XREAL_1:57;
then A26: ( upper_bound ( rng ( max (F,G) ) ) ) >= ( max (( upper_bound ( rng F ) ),( upper_bound ( rng G ) )) ) by A17,XXREAL_0:28;
A27: (for y holds (y in ( dom F ) implies ( F . y ) <= ( upper_bound ( rng F ) )))
proof
let y;
assume y in ( dom F );
then ( F . y ) in ( rng F ) by FUNCT_1:def 3;
hence thesis by A18,SEQ_4:def 1;
end;
(for s being  real number holds (( 0 ) < s implies ( ( upper_bound ( rng ( max (F,G) ) ) ) - s ) <= ( max (( upper_bound ( rng F ) ),( upper_bound ( rng G ) )) )))
proof
let s being  real number;
assume ( 0 ) < s;
then consider r being  real number such that A28: r in ( rng ( max (F,G) ) ) and A29: ( ( upper_bound ( rng ( max (F,G) ) ) ) - s ) < r by A6,SEQ_4:def 1;
consider y being set such that A30: y in ( dom ( max (F,G) ) ) and A31: r = ( ( max (F,G) ) . y ) by A28,FUNCT_1:def 3;
A32: y in C2 by A30;
then y in ( dom G ) by FUNCT_2:def 1;
then A33: ( G . y ) <= ( upper_bound ( rng G ) ) by A9;
y in ( dom F ) by A32,FUNCT_2:def 1;
then ( F . y ) <= ( upper_bound ( rng F ) ) by A27;
then A34: ( max (( F . y ),( G . y )) ) <= ( max (( upper_bound ( rng F ) ),( upper_bound ( rng G ) )) ) by A33,XXREAL_0:26;
( ( upper_bound ( rng ( max (F,G) ) ) ) - s ) <= ( max (( F . y ),( G . y )) ) by A29,A30,A31,FUZZY_1:def 4;
hence thesis by A34,XXREAL_0:2;
end;
then A35: ( upper_bound ( rng ( max (F,G) ) ) ) <= ( max (( upper_bound ( rng F ) ),( upper_bound ( rng G ) )) ) by XREAL_1:57;
( dom ( min (( max (f,g) ),h,x,z) ) ) = C2 by FUNCT_2:def 1;
then ( min (( max (f,g) ),h,x,z) ) = ( max (( min (f,h,x,z) ),( min (g,h,x,z) )) ) by A5,A3,PARTFUN1:5;
hence thesis by A35,A26,XXREAL_0:1;
end;
