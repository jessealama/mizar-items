environ
vocabularies NUMBERS,XBOOLE_0,FUZZY_1,RELAT_1,SUBSET_1,FUNCT_1,XXREAL_2,XXREAL_0,ORDINAL2,XXREAL_1,CARD_1,INTEGRA1,TARSKI,XREAL_0,ORDINAL1,ARYTM_3,ARYTM_1,FUZZY_2,ZFMISC_1,PARTFUN1,VALUED_1,SEQ_4,FUZZY_4,MEASURE5;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,ORDINAL1,NUMBERS,XCMPLX_0,XXREAL_0,XREAL_0,SQUARE_1,BINOP_1,RELAT_1,RELSET_1,PARTFUN1,XXREAL_2,FUNCT_4,SEQ_1,INTEGRA1,RCOMP_1,MEASURE6,FUZZY_1,FUZZY_2,SEQ_4,MEASURE5,CKB5,CKB7,CKB8,CKB17,CKB18;
definitions TARSKI,FUZZY_1,FUZZY_2,BINOP_1,RELAT_1,CKB8,CKB17,CKB18;
theorems FUZZY_1,FUNCT_3,PARTFUN1,FUNCT_1,INTEGRA1,INTEGRA2,ZFMISC_1,SEQ_4,SUBSET_1,XBOOLE_0,FUZZY_2,XXREAL_0,FUNCT_4,XXREAL_2,XREAL_1,RELSET_1,MEMBERED,FUNCT_2,RELAT_1,MEASURE5,CKB2,CKB3,CKB4,CKB6,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28;
schemes PARTFUN1,BINOP_1;
registrations XBOOLE_0,SUBSET_1,NUMBERS,XXREAL_0,XREAL_0,MEMBERED,INTEGRA1,RELAT_1,VALUED_0,RELSET_1,MEASURE5,CKB1;
constructors FUNCT_4,REAL_1,SQUARE_1,RFUNCT_1,MEASURE6,INTEGRA1,FUZZY_2,SEQ_1,XXREAL_2,SEQ_4,RELSET_1,RELAT_1,FUZZY_1,BINOP_2,RVSUM_1,BINOP_1,CKB5,CKB8,CKB17,CKB18;
requirements NUMERALS,REAL,SUBSET,BOOLE;
begin
reserve x for set;
reserve y for set;
reserve z for set;
reserve C1 for non  empty set;
reserve C2 for non  empty set;
reserve C3 for non  empty set;
theorem
Th18: (for f,g being (RMembership_Func of C1,C2) holds (for h,k being (RMembership_Func of C2,C3) holds (for x,z being set holds (((x in C1 & z in C3) & (for y being set holds (y in C2 implies (( f . [ x,y ] ) <= ( g . [ x,y ] ) & ( h . [ y,z ] ) <= ( k . [ y,z ] ))))) implies ( ( f (#) h ) . [ x,z ] ) <= ( ( g (#) k ) . [ x,z ] )))))
proof
let f being (RMembership_Func of C1,C2);
let g being (RMembership_Func of C1,C2);
let h being (RMembership_Func of C2,C3);
let k being (RMembership_Func of C2,C3);
let x being set;
let z being set;
assume that
A1: x in C1
and
A2: z in C3
and
A3: (for y being set holds (y in C2 implies (( f . [ x,y ] ) <= ( g . [ x,y ] ) & ( h . [ y,z ] ) <= ( k . [ y,z ] ))));
A4: [ x,z ] in [: C1,C3 :] by A1,A2,ZFMISC_1:87;
then A5: ( ( g (#) k ) . (x,z) ) = ( upper_bound ( rng ( min (g,k,x,z) ) ) ) by CKB18:def 1;
( rng ( min (f,h,x,z) ) ) is  bounded by CKB2:1;
then A6: ( rng ( min (f,h,x,z) ) ) is  bounded_above by XXREAL_2:def 11;
A7: (for s being  real number holds (s > ( 0 ) implies ( ( upper_bound ( rng ( min (f,h,x,z) ) ) ) - s ) <= ( upper_bound ( rng ( min (g,k,x,z) ) ) )))
proof
let s being  real number;
assume s > ( 0 );
then consider r being  real number such that A8: r in ( rng ( min (f,h,x,z) ) ) and A9: ( ( upper_bound ( rng ( min (f,h,x,z) ) ) ) - s ) < r by A6,SEQ_4:def 1;
consider y being set such that A10: y in ( dom ( min (f,h,x,z) ) ) and A11: r = ( ( min (f,h,x,z) ) . y ) by A8,FUNCT_1:def 3;
A12: ( h . [ y,z ] ) <= ( k . [ y,z ] ) by A3,A10;
( f . [ x,y ] ) <= ( g . [ x,y ] ) by A3,A10;
then ( min (( f . [ x,y ] ),( h . [ y,z ] )) ) <= ( min (( g . [ x,y ] ),( k . [ y,z ] )) ) by A12,XXREAL_0:18;
then A13: ( ( min (f,h,x,z) ) . y ) <= ( min (( g . [ x,y ] ),( k . [ y,z ] )) ) by A1,A2,A10,CKB17:def 1;
y in C2 by A10;
then A14: y in ( dom ( min (g,k,x,z) ) ) by FUNCT_2:def 1;
( min (( g . [ x,y ] ),( k . [ y,z ] )) ) = ( ( min (g,k,x,z) ) . y ) by A1,A2,A10,CKB17:def 1;
then ( min (( g . [ x,y ] ),( k . [ y,z ] )) ) <= ( upper_bound ( rng ( min (g,k,x,z) ) ) ) by A14,CKB2:1;
then ( ( min (f,h,x,z) ) . y ) <= ( upper_bound ( rng ( min (g,k,x,z) ) ) ) by A13,XXREAL_0:2;
hence thesis by A9,A11,XXREAL_0:2;
end;
( ( f (#) h ) . (x,z) ) = ( upper_bound ( rng ( min (f,h,x,z) ) ) ) by A4,CKB18:def 1;
hence thesis by A5,A7,XREAL_1:57;
end;
