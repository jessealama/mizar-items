environ
vocabularies NUMBERS,XBOOLE_0,FUZZY_1,RELAT_1,SUBSET_1,FUNCT_1,XXREAL_2,XXREAL_0,ORDINAL2,XXREAL_1,CARD_1,INTEGRA1,TARSKI,XREAL_0,ORDINAL1,ARYTM_3,ARYTM_1,FUZZY_2,ZFMISC_1,PARTFUN1,VALUED_1,SEQ_4,FUZZY_4,MEASURE5;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,ORDINAL1,NUMBERS,XCMPLX_0,XXREAL_0,XREAL_0,SQUARE_1,BINOP_1,RELAT_1,RELSET_1,PARTFUN1,XXREAL_2,FUNCT_4,SEQ_1,INTEGRA1,RCOMP_1,MEASURE6,FUZZY_1,FUZZY_2,SEQ_4,MEASURE5,CKB5,CKB7,CKB8,CKB17,CKB18;
definitions TARSKI,FUZZY_1,FUZZY_2,BINOP_1,RELAT_1,CKB8,CKB17,CKB18;
theorems FUZZY_1,FUNCT_3,PARTFUN1,FUNCT_1,INTEGRA1,INTEGRA2,ZFMISC_1,SEQ_4,SUBSET_1,XBOOLE_0,FUZZY_2,XXREAL_0,FUNCT_4,XXREAL_2,XREAL_1,RELSET_1,MEMBERED,FUNCT_2,RELAT_1,MEASURE5,CKB2,CKB3,CKB4,CKB6,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22;
schemes PARTFUN1,BINOP_1;
registrations XBOOLE_0,SUBSET_1,NUMBERS,XXREAL_0,XREAL_0,MEMBERED,INTEGRA1,RELAT_1,VALUED_0,RELSET_1,MEASURE5,CKB1;
constructors FUNCT_4,REAL_1,SQUARE_1,RFUNCT_1,MEASURE6,INTEGRA1,FUZZY_2,SEQ_1,XXREAL_2,SEQ_4,RELSET_1,RELAT_1,FUZZY_1,BINOP_2,RVSUM_1,BINOP_1,CKB5,CKB8,CKB17,CKB18;
requirements NUMERALS,REAL,SUBSET,BOOLE;
begin
reserve y for set;
reserve C1 for non empty set;
reserve C2 for non empty set;
reserve C3 for non empty set;
theorem
Lm3: (for f being (RMembership_Func of C1,C2) holds (for g,h being (RMembership_Func of C2,C3) holds (for x,z being set holds ((x in C1 & z in C3) implies ( upper_bound ( rng ( min (f,( min (g,h) ),x,z) ) ) ) <= ( min (( upper_bound ( rng ( min (f,g,x,z) ) ) ),( upper_bound ( rng ( min (f,h,x,z) ) ) )) )))))
proof
let f being (RMembership_Func of C1,C2);
let g being (RMembership_Func of C2,C3),h being (RMembership_Func of C2,C3);
let x being set,z being set;
assume that
A1: x in C1
and
A2: z in C3;
set F = ( min (f,( min (g,h) ),x,z) );
set G = ( min (f,g,x,z) );
set H = ( min (f,h,x,z) );
( rng F ) is  bounded by CKB2:1;
then A3: ( rng F ) is  bounded_above by XXREAL_2:def 11;
A4: (for s being  real number holds (( 0 ) < s implies (ex y st (y in ( dom F ) & ( ( upper_bound ( rng F ) ) - s ) <= ( G . y )))))
proof
let s being  real number;
assume ( 0 ) < s;
then consider r being  real number such that A5: r in ( rng F ) and A6: ( ( upper_bound ( rng F ) ) - s ) < r by A3,SEQ_4:def 1;
consider y being set such that A7: y in ( dom F ) and A8: r = ( F . y ) by A5,FUNCT_1:def 3;
A9: [ y,z ] in [: C2,C3 :] by A2,A7,ZFMISC_1:87;
( F . y ) = ( min (( f . [ x,y ] ),( ( min (g,h) ) . [ y,z ] )) ) by A1,A2,A7,CKB17:def 1
.= ( min (( f . [ x,y ] ),( min (( g . [ y,z ] ),( h . [ y,z ] )) )) ) by A9,FUZZY_1:def 3
.= ( min (( min (( f . [ x,y ] ),( g . [ y,z ] )) ),( h . [ y,z ] )) ) by XXREAL_0:33
.= ( min (( G . y ),( h . [ y,z ] )) ) by A1,A2,A7,CKB17:def 1;
then r <= ( G . y ) by A8,XXREAL_0:17;
hence thesis by A6,A7,XXREAL_0:2;
end;
A10: (for s being  real number holds (( 0 ) < s implies (ex y st (y in ( dom F ) & ( ( upper_bound ( rng F ) ) - s ) <= ( H . y )))))
proof
let s being  real number;
assume ( 0 ) < s;
then consider r being  real number such that A11: r in ( rng F ) and A12: ( ( upper_bound ( rng F ) ) - s ) < r by A3,SEQ_4:def 1;
consider y being set such that A13: y in ( dom F ) and A14: r = ( F . y ) by A11,FUNCT_1:def 3;
A15: [ y,z ] in [: C2,C3 :] by A2,A13,ZFMISC_1:87;
( F . y ) = ( min (( f . [ x,y ] ),( ( min (g,h) ) . [ y,z ] )) ) by A1,A2,A13,CKB17:def 1
.= ( min (( f . [ x,y ] ),( min (( g . [ y,z ] ),( h . [ y,z ] )) )) ) by A15,FUZZY_1:def 3
.= ( min (( min (( f . [ x,y ] ),( h . [ y,z ] )) ),( g . [ y,z ] )) ) by XXREAL_0:33
.= ( min (( H . y ),( g . [ y,z ] )) ) by A1,A2,A13,CKB17:def 1;
then r <= ( H . y ) by A14,XXREAL_0:17;
hence thesis by A12,A13,XXREAL_0:2;
end;
( rng H ) is  bounded by CKB2:1;
then A16: ( rng H ) is  bounded_above by XXREAL_2:def 11;
A17: (for y holds (y in ( dom H ) implies ( H . y ) <= ( upper_bound ( rng H ) )))
proof
let y;
assume y in ( dom H );
then ( H . y ) in ( rng H ) by FUNCT_1:def 3;
hence thesis by A16,SEQ_4:def 1;
end;
(for s being  real number holds (( 0 ) < s implies ( ( upper_bound ( rng F ) ) - s ) <= ( upper_bound ( rng H ) )))
proof
let s being  real number;
assume ( 0 ) < s;
then consider y such that A18: y in ( dom F ) and A19: ( ( upper_bound ( rng F ) ) - s ) <= ( H . y ) by A10;
y in C2 by A18;
then y in ( dom H ) by FUNCT_2:def 1;
then ( H . y ) <= ( upper_bound ( rng H ) ) by A17;
hence thesis by A19,XXREAL_0:2;
end;
then A20: ( upper_bound ( rng F ) ) <= ( upper_bound ( rng H ) ) by XREAL_1:57;
( rng G ) is  bounded by CKB2:1;
then A21: ( rng G ) is  bounded_above by XXREAL_2:def 11;
A22: (for y holds (y in ( dom G ) implies ( G . y ) <= ( upper_bound ( rng G ) )))
proof
let y;
assume y in ( dom G );
then ( G . y ) in ( rng G ) by FUNCT_1:def 3;
hence thesis by A21,SEQ_4:def 1;
end;
(for s being  real number holds (( 0 ) < s implies ( ( upper_bound ( rng F ) ) - s ) <= ( upper_bound ( rng G ) )))
proof
let s being  real number;
assume ( 0 ) < s;
then consider y such that A23: y in ( dom F ) and A24: ( ( upper_bound ( rng F ) ) - s ) <= ( G . y ) by A4;
y in C2 by A23;
then y in ( dom G ) by FUNCT_2:def 1;
then ( G . y ) <= ( upper_bound ( rng G ) ) by A22;
hence thesis by A24,XXREAL_0:2;
end;
then ( upper_bound ( rng F ) ) <= ( upper_bound ( rng G ) ) by XREAL_1:57;
hence thesis by A20,XXREAL_0:20;
end;
