environ
vocabularies NUMBERS,XBOOLE_0,FUZZY_1,RELAT_1,SUBSET_1,FUNCT_1,XXREAL_2,XXREAL_0,ORDINAL2,XXREAL_1,CARD_1,INTEGRA1,TARSKI,XREAL_0,ORDINAL1,ARYTM_3,ARYTM_1,FUZZY_2,ZFMISC_1,PARTFUN1,VALUED_1,SEQ_4,FUZZY_4,MEASURE5;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,ORDINAL1,NUMBERS,XCMPLX_0,XXREAL_0,XREAL_0,SQUARE_1,BINOP_1,RELAT_1,RELSET_1,PARTFUN1,XXREAL_2,FUNCT_4,SEQ_1,INTEGRA1,RCOMP_1,MEASURE6,FUZZY_1,FUZZY_2,SEQ_4,MEASURE5,CKB5,CKB7,CKB8,CKB17,CKB18;
definitions TARSKI,FUZZY_1,FUZZY_2,BINOP_1,RELAT_1,CKB8,CKB17,CKB18;
theorems FUZZY_1,FUNCT_3,PARTFUN1,FUNCT_1,INTEGRA1,INTEGRA2,ZFMISC_1,SEQ_4,SUBSET_1,XBOOLE_0,FUZZY_2,XXREAL_0,FUNCT_4,XXREAL_2,XREAL_1,RELSET_1,MEMBERED,FUNCT_2,RELAT_1,MEASURE5,CKB2,CKB3,CKB4,CKB6,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30;
schemes PARTFUN1,BINOP_1;
registrations XBOOLE_0,SUBSET_1,NUMBERS,XXREAL_0,XREAL_0,MEMBERED,INTEGRA1,RELAT_1,VALUED_0,RELSET_1,MEASURE5,CKB1;
constructors FUNCT_4,REAL_1,SQUARE_1,RFUNCT_1,MEASURE6,INTEGRA1,FUZZY_2,SEQ_1,XXREAL_2,SEQ_4,RELSET_1,RELAT_1,FUZZY_1,BINOP_2,RVSUM_1,BINOP_1,CKB5,CKB8,CKB17,CKB18;
requirements NUMERALS,REAL,SUBSET,BOOLE;
begin
reserve c for set;
reserve x for set;
reserve y for set;
reserve z for set;
reserve z1 for set;
reserve z2 for set;
reserve C1 for non  empty set;
reserve C2 for non  empty set;
definition
let C1 being non  empty set;
let C2 being non  empty set;
func Imf (C1,C2) -> (RMembership_Func of C1,C2) means 
:Def4: (for x holds (for y holds ([ x,y ] in [: C1,C2 :] implies ((x = y implies ( it . (x,y) ) = 1) & (x <> y implies ( it . (x,y) ) = ( 0 ))))));
existence
proof
defpred P[ set,set,set ]
 means
(($1 = $2 implies $3 = 1) & ((not $1 = $2) implies $3 = ( 0 )));
A1: (for x holds (for y holds (for z1 holds (for z2 holds ((((x in C1 & y in C2) & P[ x,y,z1 ]) & P[ x,y,z2 ]) implies z1 = z2)))));
A2: (for x holds (for y holds (for z holds (((x in C1 & y in C2) & P[ x,y,z ]) implies z in ( REAL )))));
consider IT being (PartFunc of [: C1,C2 :],( REAL )) such that A3: ((for x holds (for y holds ([ x,y ] in ( dom IT ) iff ((x in C1 & y in C2) & (ex z st P[ x,y,z ]))))) & (for x holds (for y holds ([ x,y ] in ( dom IT ) implies P[ x,y,( IT . (x,y) ) ])))) from BINOP_1:sch 5(A2,A1);
[: C1,C2 :] c= ( dom IT )
proof
let x being set;
let y being set;
A4: ((not x = y) implies (ex z st ((x = y implies z = 1) & ((not x = y) implies z = ( 0 )))));
assume A5: [ x,y ] in [: C1,C2 :];
then A6: y in C2 by ZFMISC_1:87;
x in C1 by A5,ZFMISC_1:87;
hence thesis by A3,A6,A4;
end;
then A7: ( dom IT ) = [: C1,C2 :] by XBOOLE_0:def 10;
( rng IT ) c= [. ( 0 ),1 .]
proof
let c being set;
assume c in ( rng IT );
then consider z being (Element of [: C1,C2 :]) such that A8: z in ( dom IT ) and A9: c = ( IT . z ) by PARTFUN1:3;
consider x,y being set such that x in C1 and y in C2 and A10: z = [ x,y ] by ZFMISC_1:def 2;
A11: (1 in [. ( 0 ),1 .] & ( 0 ) in [. ( 0 ),1 .])
proof
reconsider A = [. ( 0 ),1 .] as non  empty  closed_interval (Subset of ( REAL )) by MEASURE5:14;
A12: A = [. ( lower_bound A ),( upper_bound A ) .] by INTEGRA1:4;
then A13: 1 = ( upper_bound A ) by INTEGRA1:5;
( 0 ) = ( lower_bound A ) by A12,INTEGRA1:5;
hence thesis by A13,INTEGRA2:1;
end;
then A14: (x <> y implies ( IT . (x,y) ) in [. ( 0 ),1 .]) by A3,A8,A10;
(x = y implies ( IT . (x,y) ) in [. ( 0 ),1 .]) by A3,A8,A10,A11;
hence thesis by A9,A10,A14;
end;
then IT is (RMembership_Func of C1,C2) by A7,FUNCT_2:def 1,RELAT_1:def 19;
hence thesis by A3,A7;
end;
uniqueness
proof
let F being (RMembership_Func of C1,C2);
let G being (RMembership_Func of C1,C2);
assume that
A15: (for x holds (for y holds ([ x,y ] in [: C1,C2 :] implies ((x = y implies ( F . (x,y) ) = 1) & (x <> y implies ( F . (x,y) ) = ( 0 ))))))
and
A16: (for x holds (for y holds ([ x,y ] in [: C1,C2 :] implies ((x = y implies ( G . (x,y) ) = 1) & (x <> y implies ( G . (x,y) ) = ( 0 ))))));
A17: (for x holds (for y holds ((x in C1 & y in C2) implies ( F . (x,y) ) = ( G . (x,y) ))))
proof
let x being set;
let y being set;
assume that
A18: x in C1
and
A19: y in C2;
A20: [ x,y ] in [: C1,C2 :] by A18,A19,ZFMISC_1:87;
then A21: ((not x = y) implies ( F . (x,y) ) = ( 0 )) by A15;
(x = y implies ( F . (x,y) ) = 1) by A15,A20;
hence thesis by A16,A20,A21;
end;
A22: ( dom G ) = [: C1,C2 :] by FUNCT_2:def 1;
( dom F ) = [: C1,C2 :] by FUNCT_2:def 1;
hence thesis by A22,A17,FUNCT_3:6;
end;
end;
