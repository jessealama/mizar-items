begin
reserve x for  ext-real number;
reserve s for  ext-real number;
registration
cluster  bounded_above ->  right_end for non  empty  integer-membered non  empty  integer-membered set;
coherence
proof
let X being non  empty  integer-membered set;
assume X is  bounded_above;
then reconsider Y = X as  bounded_above non  empty  integer-membered set;
set s = ( sup Y );
A1: [\ s /] <= s by INT_1:def 6;
[\ s /] is (UpperBound of X)
proof
let x;
assume x in X;
hence thesis by CKB14:1,INT_1:54;
end;
then s <= [\ s /] by CKB4:def 1;
then reconsider s as Integer by A1,XXREAL_0:1;
assume A2: (not ( sup X ) in X);
( s - 1 ) is (UpperBound of X)
proof
let x;
assume A3: x in X;
then reconsider i = x as Integer;
i <= s by A3,CKB14:1;
then i < s by A2,A3,XXREAL_0:1;
hence thesis by INT_1:52;
end;
then ( s - 1 ) >= s by CKB4:def 1;
hence contradiction by XREAL_1:146;
end;
end;
