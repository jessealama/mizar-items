environ
vocabularies NUMBERS,INT_1,SUBSET_1,XXREAL_0,FINSET_1,ARYTM_3,XBOOLE_0,ARYTM_1,NAT_1,FUNCT_1,RELAT_1,TARSKI,ORDINAL1,MEMBERED,ORDINAL2,FINSUB_1,SETWISEO,CARD_1,XREAL_0,RAT_1,XXREAL_1,XXREAL_2,MEASURE5;
notations TARSKI,XBOOLE_0,SUBSET_1,RELAT_1,FUNCT_1,ORDINAL1,FINSET_1,FINSUB_1,SETWISEO,NUMBERS,MEMBERED,XXREAL_0,XCMPLX_0,XREAL_0,INT_1,NAT_1,RAT_1,XXREAL_1,CKB2,CKB3,CKB4;
definitions TARSKI,SETWISEO,XXREAL_0,MEMBERED,CKB2,CKB3,CKB4;
theorems XXREAL_0,MEMBERED,TARSKI,XBOOLE_0,XXREAL_1,XBOOLE_1,FINSUB_1,XREAL_0,XREAL_1,INT_1,NAT_1,FINSET_1,ZFMISC_1,FUNCT_1,SETFAM_1,CKB2,CKB3,CKB4;
schemes XXREAL_1,NAT_1,SETWISEO,CLASSES1,CKB1;
registrations XXREAL_0,MEMBERED,XXREAL_1,XBOOLE_0,FINSET_1,NUMBERS,SETWISEO,FINSUB_1,XREAL_0,INT_1,NAT_1,CARD_1;
constructors XXREAL_0,MEMBERED,XREAL_0,INT_1,FINSET_1,RAT_1,XXREAL_1,SETWISEO,XCMPLX_0,REAL_1,NAT_1,CKB2,CKB3,CKB4;
requirements BOOLE,SUBSET,NUMERALS,ARITHM,REAL;
begin
reserve x for  ext-real number;
reserve y for  ext-real number;
reserve z for  ext-real number;
reserve r for  ext-real number;
reserve s for  ext-real number;
definition
let X being  ext-real-membered set;
func inf X ->  ext-real number means 
:Def4: (it is (LowerBound of X) & (for x being (LowerBound of X) holds x <= it));
existence
proof
defpred Q[ set ]
 means
$1 in X;
defpred P[ set ]
 means
$1 is (LowerBound of X);
A9: (for r holds (for s holds ((P[ r ] & Q[ s ]) implies r <= s))) by CKB3:def 1;
consider s such that A10: (for r holds (P[ r ] implies r <= s)) and A11: (for r holds (Q[ r ] implies s <= r)) from XXREAL_1:sch 1(A9);
take s;
thus s is (LowerBound of X) by A11,CKB3:def 1;
thus thesis by A10;
end;
uniqueness
proof
let y;
let z;
assume that
A12: y is (LowerBound of X)
and
A13: (for x being (LowerBound of X) holds x <= y)
and
A14: z is (LowerBound of X)
and
A15: (for x being (LowerBound of X) holds x <= z);
A16: z <= y by A13,A14;
y <= z by A12,A15;
hence thesis by A16,XXREAL_0:1;
end;
end;
