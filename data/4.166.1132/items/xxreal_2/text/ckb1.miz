notations TARSKI,XBOOLE_0,SUBSET_1,RELAT_1,FUNCT_1,ORDINAL1,FINSET_1,FINSUB_1,SETWISEO,NUMBERS,MEMBERED,XXREAL_0,XCMPLX_0,XREAL_0,INT_1,NAT_1,RAT_1,XXREAL_1;
definitions TARSKI,SETWISEO,XXREAL_0,MEMBERED;
theorems XXREAL_0,MEMBERED,TARSKI,XBOOLE_0,XXREAL_1,XBOOLE_1,FINSUB_1,XREAL_0,XREAL_1,INT_1,NAT_1,FINSET_1,ZFMISC_1,FUNCT_1,SETFAM_1;
