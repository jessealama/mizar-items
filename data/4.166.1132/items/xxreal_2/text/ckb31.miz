begin
reserve x for  ext-real number;
reserve y for  ext-real number;
reserve z for  ext-real number;
theorem
( max (x,y) ) = ( max { x,y } )
proof
now
per cases ;
case A1: x <= y;
A2: (for z being (UpperBound of { x,y }) holds y <= z) by CKB30:1;
y is (UpperBound of { x,y }) by A1,CKB29:1;
hence ( max { x,y } ) = y by A2,CKB4:def 1;
end;
case A3: y < x;
A4: (for z being (UpperBound of { x,y }) holds x <= z) by CKB30:1;
x is (UpperBound of { x,y }) by A3,CKB29:1;
hence ( max { x,y } ) = x by A4,CKB4:def 1;
end;
end;
hence thesis by XXREAL_0:def 10;
end;
