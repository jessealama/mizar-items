environ
vocabularies NUMBERS,INT_1,SUBSET_1,XXREAL_0,FINSET_1,ARYTM_3,XBOOLE_0,ARYTM_1,NAT_1,FUNCT_1,RELAT_1,TARSKI,ORDINAL1,MEMBERED,ORDINAL2,FINSUB_1,SETWISEO,CARD_1,XREAL_0,RAT_1,XXREAL_1,XXREAL_2,MEASURE5;
notations TARSKI,XBOOLE_0,SUBSET_1,RELAT_1,FUNCT_1,ORDINAL1,FINSET_1,FINSUB_1,SETWISEO,NUMBERS,MEMBERED,XXREAL_0,XCMPLX_0,XREAL_0,INT_1,NAT_1,RAT_1,XXREAL_1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7;
definitions TARSKI,SETWISEO,XXREAL_0,MEMBERED,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7;
theorems XXREAL_0,MEMBERED,TARSKI,XBOOLE_0,XXREAL_1,XBOOLE_1,FINSUB_1,XREAL_0,XREAL_1,INT_1,NAT_1,FINSET_1,ZFMISC_1,FUNCT_1,SETFAM_1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB13,CKB14,CKB15,CKB16,CKB17;
schemes XXREAL_1,NAT_1,SETWISEO,CLASSES1,CKB1;
registrations XXREAL_0,MEMBERED,XXREAL_1,XBOOLE_0,FINSET_1,NUMBERS,SETWISEO,FINSUB_1,XREAL_0,INT_1,NAT_1,CARD_1,CKB12;
constructors XXREAL_0,MEMBERED,XREAL_0,INT_1,FINSET_1,RAT_1,XXREAL_1,SETWISEO,XCMPLX_0,REAL_1,NAT_1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7;
requirements BOOLE,SUBSET,NUMERALS,ARITHM,REAL;
begin
reserve x for  ext-real number;
reserve y for  ext-real number;
reserve z for  ext-real number;
reserve A for  ext-real-membered set;
reserve B for  ext-real-membered set;
theorem
Th8: (for x being (UpperBound of A) holds (for y being (UpperBound of B) holds ( max (x,y) ) is (UpperBound of ( A \/ B ))))
proof
let x being (UpperBound of A);
let y being (UpperBound of B);
set m = ( max (x,y) );
let z;
assume A1: z in ( A \/ B );
per cases  by A1,XBOOLE_0:def 3;
suppose A2: z in A;

A3: x <= m by XXREAL_0:25;
z <= x by A2,CKB2:def 1;
hence thesis by A3,XXREAL_0:2;
end;
suppose A4: z in B;

A5: y <= m by XXREAL_0:25;
z <= y by A4,CKB2:def 1;
hence thesis by A5,XXREAL_0:2;
end;
end;
