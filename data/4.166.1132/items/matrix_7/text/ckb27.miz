begin
reserve k for Nat;
reserve i for Nat;
definition
let G being Group;
let f being (FinSequence of G);
func f " -> (FinSequence of G) means 
:Def4: (( len it ) = ( len f ) & (for i being (Element of ( NAT )) holds (i in ( dom f ) implies ( it /. i ) = ( ( f /. i ) " ))));
existence
proof
deffunc F(Nat) = ( ( f /. $1 ) " );
(ex p being FinSequence st (( len p ) = ( len f ) & (for k being Nat holds (k in ( dom p ) implies ( p . k ) = F(k))))) from FINSEQ_1:sch 2;
then consider p being FinSequence such that A1: ( len p ) = ( len f ) and A2: (for k being Nat holds (k in ( dom p ) implies ( p . k ) = F(k)));
( rng p ) c= (the carrier of G)
proof
let y being set;
assume y in ( rng p );
then consider x being set such that A3: x in ( dom p ) and A4: y = ( p . x ) by FUNCT_1:def 3;
reconsider k = x as (Element of ( NAT )) by A3;
( p . k ) = ( ( f /. k ) " ) by A2,A3;
hence thesis by A4;
end;
then reconsider q = p as (FinSequence of G) by FINSEQ_1:def 4;
A5: ( dom p ) = ( dom f ) by A1,FINSEQ_3:29;
(for i being (Element of ( NAT )) holds (i in ( dom f ) implies ( q /. i ) = ( ( f /. i ) " )))
proof
let i being (Element of ( NAT ));
assume A6: i in ( dom f );
hence ( q /. i ) = ( p . i ) by A5,PARTFUN1:def 6
.= ( ( f /. i ) " ) by A2,A5,A6;
end;
hence thesis by A1;
end;
uniqueness
proof
thus (for g1,g2 being (FinSequence of G) holds (((( len g1 ) = ( len f ) & (for i being (Element of ( NAT )) holds (i in ( dom f ) implies ( g1 /. i ) = ( ( f /. i ) " )))) & (( len g2 ) = ( len f ) & (for j being (Element of ( NAT )) holds (j in ( dom f ) implies ( g2 /. j ) = ( ( f /. j ) " ))))) implies g1 = g2))
proof
let g1 being (FinSequence of G);
let g2 being (FinSequence of G);
assume that
A7: ( len g1 ) = ( len f )
and
A8: (for i being (Element of ( NAT )) holds (i in ( dom f ) implies ( g1 /. i ) = ( ( f /. i ) " )))
and
A9: ( len g2 ) = ( len f )
and
A10: (for j being (Element of ( NAT )) holds (j in ( dom f ) implies ( g2 /. j ) = ( ( f /. j ) " )));
A11: ( dom g1 ) = ( dom f ) by A7,FINSEQ_3:29;
(for k being Nat holds ((1 <= k & k <= ( len g1 )) implies ( g1 . k ) = ( g2 . k )))
proof
let k being Nat;
assume A12: (1 <= k & k <= ( len g1 ));
A13: k in ( NAT ) by ORDINAL1:def 12;
then k in ( Seg ( len g2 ) ) by A7,A9,A12;
then k in ( dom g2 ) by FINSEQ_1:def 3;
then A14: ( g2 . k ) = ( g2 /. k ) by PARTFUN1:def 6;
k in ( Seg ( len g1 ) ) by A13,A12;
then A15: k in ( dom g1 ) by FINSEQ_1:def 3;
then (( g1 . k ) = ( g1 /. k ) & ( g1 /. k ) = ( ( f /. k ) " )) by A8,A11,PARTFUN1:def 6;
hence thesis by A10,A11,A15,A14;
end;
hence thesis by A7,A9,FINSEQ_1:14;
end;

