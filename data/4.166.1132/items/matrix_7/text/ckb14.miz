begin
reserve n for Nat;
definition
let n being Nat;
let K being Field;
let M being (Matrix of n,K);
let a being (Element of K);
redefine func a * M -> (Matrix of n,K);

coherence
proof
A1: ( width ( a * M ) ) = ( width M ) by MATRIX_3:def 5;
A2: ( len M ) = n by MATRIX_1:def 2;
A3: ( len ( a * M ) ) = ( len M ) by MATRIX_3:def 5;
A4: ( len M ) = n by MATRIX_1:def 2;
now
per cases ;
case A5: ( len M ) > ( 0 );
then n = ( width M ) by A2,MATRIX_1:20;
hence thesis by A2,A3,A1,A5,MATRIX_1:20;
end;
case ( len M ) <= ( 0 );
then A6: ( len ( a * M ) ) = ( 0 ) by MATRIX_3:def 5;
then A7: ( Seg ( len ( a * M ) ) ) = ( {} );
(for p being (FinSequence of K) holds (p in ( rng ( a * M ) ) implies ( len p ) = ( 0 )))
proof
let p being (FinSequence of K);
assume p in ( rng ( a * M ) );
then (ex x being set st (x in ( dom ( a * M ) ) & p = ( ( a * M ) . x ))) by FUNCT_1:def 3;
hence thesis by A7,FINSEQ_1:def 3;
end;
hence thesis by A4,A3,A6,MATRIX_1:def 2;
end;
end;
hence thesis;
end;
end;
