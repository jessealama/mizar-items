begin
reserve A for non  empty set;
reserve x for (Element of A);
reserve y for (Element of A);
reserve z for (Element of A);
reserve f for (Permutation of A);
reserve AFS for AffinSpace;
reserve x for (Element of AFS);
reserve y for (Element of AFS);
reserve z for (Element of AFS);
reserve f for (Permutation of (the carrier of AFS));
theorem
Th93: (f is  dilatation implies ((f = ( id (the carrier of AFS) ) or (for x holds ( f . x ) <> x)) iff (for x holds (for y holds x,( f . x ) // y,( f . y )))))
proof
assume A1: f is  dilatation;
A2:now
assume A3: (for x holds (for y holds x,( f . x ) // y,( f . y )));
assume f <> ( id (the carrier of AFS) );
then consider y such that A4: ( f . y ) <> ( ( id (the carrier of AFS) ) . y ) by FUNCT_2:63;
given x such that
A5: ( f . x ) = x;

x <> y by A5,A4,FUNCT_1:18;
then consider z such that A6: (not  LIN x,y,z) by AFF_1:13;
x,z // ( f . x ),( f . z ) by A1,CKB87:1;
then  LIN x,z,( f . z ) by A5,AFF_1:def 1;
then A7:  LIN z,( f . z ),x by AFF_1:6;
 LIN z,( f . z ),z by AFF_1:7;
then A8: z,( f . z ) // x,z by A7,AFF_1:10;
A9: ( f . y ) <> y by A4,FUNCT_1:18;
x,y // ( f . x ),( f . y ) by A1,CKB87:1;
then A10:  LIN x,y,( f . y ) by A5,AFF_1:def 1;
then  LIN y,x,( f . y ) by AFF_1:6;
then A11: y,x // y,( f . y ) by AFF_1:def 1;
A12:  LIN y,( f . y ),x by A10,AFF_1:6;
A13:now
assume z = ( f . z );
then z,y // z,( f . y ) by A1,CKB87:1;
then  LIN z,y,( f . y ) by AFF_1:def 1;
then ( LIN y,( f . y ),y &  LIN y,( f . y ),z) by AFF_1:6,AFF_1:7;
hence contradiction by A9,A12,A6,AFF_1:8;
end;
y,( f . y ) // z,( f . z ) by A3;
then y,( f . y ) // x,z by A13,A8,AFF_1:5;
then y,x // x,z by A9,A11,AFF_1:5;
then x,y // x,z by AFF_1:4;
hence contradiction by A6,AFF_1:def 1;
end;
now
assume A14: (f = ( id (the carrier of AFS) ) or (for z holds ( f . z ) <> z));
let x;
let y;
A15: x,y // ( f . x ),( f . y ) by A1,CKB87:1;
A16:now
assume A17: (for z holds ( f . z ) <> z);
assume A18: (not x,( f . x ) // y,( f . y ));
then consider z such that A19:  LIN x,( f . x ),z and A20:  LIN y,( f . y ),z by A15,CKB94:1;
set t = ( f . z );
 LIN x,( f . x ),t by A1,A19,CKB93:1;
then A21: x,( f . x ) // z,t by A19,AFF_1:10;
 LIN y,( f . y ),t by A1,A20,CKB93:1;
then A22: y,( f . y ) // z,t by A20,AFF_1:10;
z <> t by A17;
hence contradiction by A18,A21,A22,AFF_1:5;
end;
now
assume f = ( id (the carrier of AFS) );
then ( f . y ) = y by FUNCT_1:18;
hence x,( f . x ) // y,( f . y ) by AFF_1:3;
end;
hence x,( f . x ) // y,( f . y ) by A14,A16;
end;
hence thesis by A2;
end;
