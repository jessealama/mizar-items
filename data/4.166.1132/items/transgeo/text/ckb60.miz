reserve a for (Element of OAS);
reserve b for (Element of OAS);
reserve c for (Element of OAS);
reserve d for (Element of OAS);
reserve x for (Element of OAS);
reserve z for (Element of OAS);
reserve t for (Element of OAS);
theorem
Th61: (a,b '||' c,d implies (a,c '||' b,d or (ex x st ( LIN a,c,x &  LIN b,d,x))))
proof
assume A1: a,b '||' c,d;
assume A2: (not a,c '||' b,d);
A3:now
consider z such that A4: a,b '||' c,z and A5: a,c '||' b,z by DIRAF:26;
assume A6: a <> b;
A7:now
c,d '||' c,z by A1,A6,A4,DIRAF:23;
then  LIN c,d,z by DIRAF:def 5;
then  LIN d,c,z by DIRAF:30;
then d,c '||' d,z by DIRAF:def 5;
then z,d '||' d,c by DIRAF:22;
then consider t such that A8: b,d '||' d,t and A9: b,z '||' c,t by A2,A5,DIRAF:27;
assume b <> z;
then a,c '||' c,t by A5,A9,DIRAF:23;
then c,a '||' c,t by DIRAF:22;
then  LIN c,a,t by DIRAF:def 5;
then A10:  LIN a,c,t by DIRAF:30;
d,b '||' d,t by A8,DIRAF:22;
then  LIN d,b,t by DIRAF:def 5;
then  LIN b,d,t by DIRAF:30;
hence thesis by A10;
end;
now
assume b = z;
then b,a '||' b,c by A4,DIRAF:22;
then  LIN b,a,c by DIRAF:def 5;
then A11:  LIN a,c,b by DIRAF:30;
 LIN b,d,b by DIRAF:31;
hence thesis by A11;
end;
hence thesis by A7;
end;
now
assume a = b;
then ( LIN a,c,a &  LIN b,d,a) by DIRAF:31;
hence thesis;
end;
hence thesis by A3;
end;
