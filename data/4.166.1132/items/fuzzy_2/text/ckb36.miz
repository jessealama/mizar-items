begin
reserve C for non empty set;
reserve c for (Element of C);
reserve f for (Membership_Func of C);
reserve h for (Membership_Func of C);
reserve g for (Membership_Func of C);
theorem
( ( f ++ g ) * ( f ++ h ) ) c= ( f ++ ( g * h ) )
proof
let c;
set x = ( f . c );
set y = ( g . c );
set z = ( h . c );
( f * f ) c= f by CKB31:1;
then ( ( f * f ) . c ) <= ( f . c ) by FUZZY_1:def 2;
then ( x * x ) <= x by CKB29:def 1;
then A1: ( ( x * x ) - x ) <= ( 0 ) by XREAL_1:47;
( 0 ) <= ( ( 1_minus ( g ++ h ) ) . c ) by CKB1:1;
then A2: ( ( ( 1_minus ( g ++ h ) ) . c ) * ( ( - x ) + ( x * x ) ) ) <= ( ( 0 ) * ( ( - x ) + ( x * x ) ) ) by A1,XREAL_1:65;
( ( ( ( f ++ g ) * ( f ++ h ) ) . c ) - ( ( f ++ ( g * h ) ) . c ) ) = ( ( ( ( f ++ g ) . c ) * ( ( f ++ h ) . c ) ) - ( ( f ++ ( g * h ) ) . c ) ) by CKB29:def 1
.= ( ( ( ( x + y ) - ( x * y ) ) * ( ( f ++ h ) . c ) ) - ( ( f ++ ( g * h ) ) . c ) ) by CKB30:def 1
.= ( ( ( ( x + y ) - ( x * y ) ) * ( ( x + z ) - ( x * z ) ) ) - ( ( f ++ ( g * h ) ) . c ) ) by CKB30:def 1
.= ( ( ( ( x + y ) - ( x * y ) ) * ( ( x + z ) - ( x * z ) ) ) - ( ( x + ( ( g * h ) . c ) ) - ( x * ( ( g * h ) . c ) ) ) ) by CKB30:def 1
.= ( ( ( ( x + y ) - ( x * y ) ) * ( ( x + z ) - ( x * z ) ) ) - ( ( x + ( y * z ) ) - ( x * ( ( g * h ) . c ) ) ) ) by CKB29:def 1
.= ( ( ( ( x - ( x * y ) ) + y ) * ( ( x + z ) - ( x * z ) ) ) - ( ( x + ( y * z ) ) - ( ( y * z ) * x ) ) ) by CKB29:def 1
.= ( ( x * ( ( ( y + z ) - ( y * z ) ) - 1 ) ) + ( ( ( ( ( - y ) + ( y * z ) ) - z ) + 1 ) * ( x * x ) ) )
.= ( ( x * ( ( ( g ++ h ) . c ) - 1 ) ) + ( ( ( - ( ( y + z ) - ( y * z ) ) ) + 1 ) * ( x * x ) ) ) by CKB30:def 1
.= ( ( x * ( - ( ( - ( ( g ++ h ) . c ) ) + 1 ) ) ) + ( ( ( - ( ( g ++ h ) . c ) ) + 1 ) * ( x * x ) ) ) by CKB30:def 1
.= ( ( 1 - ( ( g ++ h ) . c ) ) * ( ( - x ) + ( x * x ) ) )
.= ( ( ( 1_minus ( g ++ h ) ) . c ) * ( ( - x ) + ( x * x ) ) ) by FUZZY_1:def 5;
hence thesis by A2,XREAL_1:50;
end;
