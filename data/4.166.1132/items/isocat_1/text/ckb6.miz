begin
reserve A for Category;
reserve B for Category;
theorem
Th9: (for F1,F2 being (Functor of A,B) holds (F1 is_transformable_to F2 implies (for t being (transformation of F1,F2) holds (t is  invertible implies (for a being (Object of A) holds ( F1 . a ),( F2 . a ) are_isomorphic )))))
proof
let F1 being (Functor of A,B);
let F2 being (Functor of A,B);
assume that
A1: F1 is_transformable_to F2;
let t being (transformation of F1,F2);
assume that
A2: t is  invertible;
let a being (Object of A);
thus ( Hom (( F1 . a ),( F2 . a )) ) <> ( {} ) by A1,NATTRA_1:def 2;
( t . a ) is  invertible by A2,NATTRA_1:def 10;
hence thesis;
end;
