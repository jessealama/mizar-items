theorem
Th6: (for X being RealBanachSpace holds (for Y being RealNormSpace holds (for vseq being (sequence of ( R_NormSpace_of_BoundedLinearOperators (X,Y) )) holds (for tseq being (Function of X,Y) holds ((for x being (Point of X) holds (( vseq # x ) is  convergent & ( tseq . x ) = ( lim ( vseq # x ) ))) implies ((tseq is  bounded  bounded (LinearOperator of X,Y) & (for x being (Point of X) holds ||. ( tseq . x ) .|| <= ( ( lim_inf ||. vseq .|| ) * ||. x .|| ))) & (for ttseq being (Point of ( R_NormSpace_of_BoundedLinearOperators (X,Y) )) holds (ttseq = tseq implies ||. ttseq .|| <= ( lim_inf ||. vseq .|| )))))))))
proof
let X being RealBanachSpace;
let Y being RealNormSpace;
let vseq being (sequence of ( R_NormSpace_of_BoundedLinearOperators (X,Y) ));
let tseq being (Function of X,Y);
set T = ( rng vseq );
set RNS = ( R_NormSpace_of_BoundedLinearOperators (X,Y) );
assume A1: (for x being (Point of X) holds (( vseq # x ) is  convergent & ( tseq . x ) = ( lim ( vseq # x ) )));
A2: (for x,y being (Point of X) holds ( tseq . ( x + y ) ) = ( ( tseq . x ) + ( tseq . y ) ))
proof
let x being (Point of X);
let y being (Point of X);
A3: (( vseq # y ) is  convergent & ( tseq . y ) = ( lim ( vseq # y ) )) by A1;
A4: ( tseq . ( x + y ) ) = ( lim ( vseq # ( x + y ) ) ) by A1;
now
let n being (Element of ( NAT ));
(( vseq . n ) is  bounded  bounded (LinearOperator of X,Y) & ( ( vseq # ( x + y ) ) . n ) = ( ( vseq . n ) . ( x + y ) )) by CKB8:def 1,LOPBAN_1:def 9;
then A5: ( ( vseq # ( x + y ) ) . n ) = ( ( ( vseq . n ) . x ) + ( ( vseq . n ) . y ) ) by GRCAT_1:def 8;
( ( vseq . n ) . y ) = ( ( vseq # y ) . n ) by CKB8:def 1;
hence ( ( vseq # ( x + y ) ) . n ) = ( ( ( vseq # x ) . n ) + ( ( vseq # y ) . n ) ) by A5,CKB8:def 1;
end;
then A6: ( vseq # ( x + y ) ) = ( ( vseq # x ) + ( vseq # y ) ) by NORMSP_1:def 2;
(( vseq # x ) is  convergent & ( tseq . x ) = ( lim ( vseq # x ) )) by A1;
hence thesis by A3,A6,A4,NORMSP_1:25;
end;
A7: (for x being (Point of X) holds (ex K being  real number st (( 0 ) <= K & (for f being (Point of RNS) holds (f in T implies ||. ( f . x ) .|| <= K)))))
proof
let x being (Point of X);
( vseq # x ) is  convergent by A1;
then ||. ( vseq # x ) .|| is  bounded by NORMSP_1:23,SEQ_2:13;
then consider K being  real number such that A8: (for n being (Element of ( NAT )) holds ( ||. ( vseq # x ) .|| . n ) < K) by SEQ_2:def 3;
A9: (for f being (Point of RNS) holds (f in T implies ||. ( f . x ) .|| <= K))
proof
let f being (Point of RNS);
assume f in T;
then consider n being set such that A10: n in ( NAT ) and A11: f = ( vseq . n ) by FUNCT_2:11;
reconsider n as (Element of ( NAT )) by A10;
( ( vseq . n ) . x ) = ( ( vseq # x ) . n ) by CKB8:def 1;
then ||. ( f . x ) .|| = ( ||. ( vseq # x ) .|| . n ) by A11,NORMSP_0:def 4;
hence thesis by A8;
end;
( ||. ( vseq # x ) .|| . ( 0 ) ) < K by A8;
then ||. ( ( vseq # x ) . ( 0 ) ) .|| < K by NORMSP_0:def 4;
then ( 0 ) <= K;
hence thesis by A9;
end;
vseq in ( Funcs (( NAT ),(the carrier of RNS)) ) by FUNCT_2:8;
then (ex f0 being Function st ((vseq = f0 & ( dom f0 ) = ( NAT )) & ( rng f0 ) c= (the carrier of RNS))) by FUNCT_2:def 2;
then consider L being  real number such that A12: ( 0 ) <= L and A13: (for f being (Point of RNS) holds (f in T implies ||. f .|| <= L)) by A7,CKB7:1;
A14: ( L + ( 0 ) ) < ( 1 + L ) by XREAL_1:8;
(for n being (Element of ( NAT )) holds ( abs ( ||. vseq .|| . n ) ) < ( 1 + L ))
proof
let n being (Element of ( NAT ));
||. ( vseq . n ) .|| <= L by A13,FUNCT_2:4;
then ( ||. vseq .|| . n ) <= L by NORMSP_0:def 4;
then A15: ( ||. vseq .|| . n ) < ( 1 + L ) by A14,XXREAL_0:2;
( 0 ) <= ||. ( vseq . n ) .||;
then ( 0 ) <= ( ||. vseq .|| . n ) by NORMSP_0:def 4;
hence thesis by A15,ABSVALUE:def 1;
end;
then A16: ||. vseq .|| is  bounded by A12,SEQ_2:3;
A17: (for x being (Point of X) holds ||. ( tseq . x ) .|| <= ( ( lim_inf ||. vseq .|| ) * ||. x .|| ))
proof
let x being (Point of X);
A18: ( ||. x .|| (#) ||. vseq .|| ) is  bounded by A16,SEQM_3:37;
A19: (for n being (Element of ( NAT )) holds ||. ( ( vseq # x ) . n ) .|| <= ( ||. ( vseq . n ) .|| * ||. x .|| ))
proof
let n being (Element of ( NAT ));
(( ( vseq . n ) . x ) = ( ( vseq # x ) . n ) & ( vseq . n ) is  bounded  bounded (LinearOperator of X,Y)) by CKB8:def 1,LOPBAN_1:def 9;
hence thesis by LOPBAN_1:32;
end;
A20: (for n being (Element of ( NAT )) holds ( ||. ( vseq # x ) .|| . n ) <= ( ( ||. x .|| (#) ||. vseq .|| ) . n ))
proof
let n being (Element of ( NAT ));
A21: ||. ( vseq . n ) .|| = ( ||. vseq .|| . n ) by NORMSP_0:def 4;
(( ||. ( vseq # x ) .|| . n ) = ||. ( ( vseq # x ) . n ) .|| & ||. ( ( vseq # x ) . n ) .|| <= ( ||. ( vseq . n ) .|| * ||. x .|| )) by A19,NORMSP_0:def 4;
hence thesis by A21,SEQ_1:9;
end;
A22: ( lim_inf ( ||. x .|| (#) ||. vseq .|| ) ) = ( ( lim_inf ||. vseq .|| ) * ||. x .|| ) by A16,CKB1:1;
A23: (( vseq # x ) is  convergent & ( tseq . x ) = ( lim ( vseq # x ) )) by A1;
then ||. ( vseq # x ) .|| is  convergent by LOPBAN_1:20;
then A24: ( lim ||. ( vseq # x ) .|| ) = ( lim_inf ||. ( vseq # x ) .|| ) by RINFSUP1:89;
||. ( vseq # x ) .|| is  bounded by A23,LOPBAN_1:20,SEQ_2:13;
then ( lim_inf ||. ( vseq # x ) .|| ) <= ( lim_inf ( ||. x .|| (#) ||. vseq .|| ) ) by A18,A20,RINFSUP1:91;
hence thesis by A23,A22,A24,LOPBAN_1:20;
end;
now
let s being  real number;
assume A25: ( 0 ) < s;
(for k being (Element of ( NAT )) holds ( ( 0 ) - s ) < ( ||. vseq .|| . ( ( 0 ) + k ) ))
proof
let k being (Element of ( NAT ));
||. ( vseq . k ) .|| = ( ||. vseq .|| . k ) by NORMSP_0:def 4;
then ( 0 ) <= ( ||. vseq .|| . k );
hence thesis by A25;
end;
hence (ex n being (Element of ( NAT )) st (for k being (Element of ( NAT )) holds ( ( 0 ) - s ) < ( ||. vseq .|| . ( n + k ) )));
end;
then A26: ( 0 ) <= ( lim_inf ||. vseq .|| ) by A16,RINFSUP1:82;
A27: (for x being (Point of X) holds (for r being (Element of ( REAL )) holds ( tseq . ( r * x ) ) = ( r * ( tseq . x ) )))
proof
let x being (Point of X);
let r being (Element of ( REAL ));
A28: ( tseq . x ) = ( lim ( vseq # x ) ) by A1;
A29:now
let n being (Element of ( NAT ));
(( vseq . n ) is  bounded  bounded (LinearOperator of X,Y) & ( ( vseq # ( r * x ) ) . n ) = ( ( vseq . n ) . ( r * x ) )) by CKB8:def 1,LOPBAN_1:def 9;
then ( ( vseq # ( r * x ) ) . n ) = ( r * ( ( vseq . n ) . x ) ) by LOPBAN_1:def 5;
hence ( ( vseq # ( r * x ) ) . n ) = ( r * ( ( vseq # x ) . n ) ) by CKB8:def 1;
end;
( tseq . ( r * x ) ) = ( lim ( vseq # ( r * x ) ) ) by A1;
then ( tseq . ( r * x ) ) = ( lim ( r * ( vseq # x ) ) ) by A29,NORMSP_1:def 5;
hence thesis by A1,A28,NORMSP_1:28;
end;
then reconsider tseq1 = tseq as  bounded (LinearOperator of X,Y) by A2,A17,A26,GRCAT_1:def 8,LOPBAN_1:def 5,LOPBAN_1:def 8;
(for ttseq being (Point of ( R_NormSpace_of_BoundedLinearOperators (X,Y) )) holds (ttseq = tseq implies ||. ttseq .|| <= ( lim_inf ||. vseq .|| )))
proof
(for k being  real number holds (k in { ||. ( tseq . x1 ) .|| where x1 is (Point of X): ||. x1 .|| <= 1 } implies k <= ( lim_inf ||. vseq .|| )))
proof
let k being  real number;
assume k in { ||. ( tseq . x1 ) .|| where x1 is (Point of X): ||. x1 .|| <= 1 };
then consider x being (Point of X) such that A30: (k = ||. ( tseq . x ) .|| & ||. x .|| <= 1);
(k <= ( ( lim_inf ||. vseq .|| ) * ||. x .|| ) & ( ( lim_inf ||. vseq .|| ) * ||. x .|| ) <= ( lim_inf ||. vseq .|| )) by A17,A26,A30,XREAL_1:153;
hence thesis by XXREAL_0:2;
end;
then A31: ( upper_bound ( PreNorms tseq1 ) ) <= ( lim_inf ||. vseq .|| ) by SEQ_4:45;
let ttseq being (Point of ( R_NormSpace_of_BoundedLinearOperators (X,Y) ));
assume ttseq = tseq;
hence thesis by A31,LOPBAN_1:30;
end;
hence thesis by A2,A27,A17,A26,GRCAT_1:def 8,LOPBAN_1:def 5,LOPBAN_1:def 8;
end;
