func Impl f -> (Element of ( CQC-WFF )) means 
:Def4: (ex F being (FinSequence of ( CQC-WFF )) st (((it = ( F . ( len f ) ) & ( len F ) = ( len f )) & (( F . 1 ) = ( Begin f ) or ( len f ) = ( 0 ))) & (for n holds ((1 <= n & n < ( len f )) implies (ex p st (ex q st ((p = ( f . ( n + 1 ) ) & q = ( F . n )) & ( F . ( n + 1 ) ) = ( p => q ))))))));
existence
proof
defpred P[ (Element of ( NAT )),set,set ]
 means
(ex p st (ex q st ((p = ( f . ( $1 + 1 ) ) & q = $2) & $3 = ( p => q ))));
A2: (for n holds ((1 <= n & n < ( len f )) implies (for x being (Element of ( CQC-WFF )) holds (ex y being (Element of ( CQC-WFF )) st P[ n,x,y ]))))
proof
let n;
assume that
1 <= n
and
A3: n < ( len f );
(1 <= ( n + 1 ) & ( n + 1 ) <= ( len f )) by A3,NAT_1:11,NAT_1:13;
then ( n + 1 ) in ( dom f ) by FINSEQ_3:25;
then reconsider p = ( f . ( n + 1 ) ) as (Element of ( CQC-WFF )) by FINSEQ_2:11;
let x being (Element of ( CQC-WFF ));
take ( p => x );
take p;
take x;
thus thesis;
end;
consider F being (FinSequence of ( CQC-WFF )) such that A4: ((( len F ) = ( len f ) & (( F . 1 ) = ( Begin f ) or ( len f ) = ( 0 ))) & (for n holds ((1 <= n & n < ( len f )) implies P[ n,( F . n ),( F . ( n + 1 ) ) ]))) from RECDEF_1:sch 4(A2);
( len f ) in ( dom F ) by A1,A4,FINSEQ_3:25;
then reconsider p = ( F . ( len f ) ) as (Element of ( CQC-WFF )) by FINSEQ_2:11;
take p;
thus thesis by A4;
end;
uniqueness
proof
defpred P[ (Element of ( NAT )),set,set ]
 means
(ex p st (ex q st ((p = ( f . ( $1 + 1 ) ) & q = $2) & $3 = ( p => q ))));
let p1;
let p2;
assume that
A5: (ex F being (FinSequence of ( CQC-WFF )) st (((p1 = ( F . ( len f ) ) & ( len F ) = ( len f )) & (( F . 1 ) = ( Begin f ) or ( len f ) = ( 0 ))) & (for n holds ((1 <= n & n < ( len f )) implies P[ n,( F . n ),( F . ( n + 1 ) ) ]))))
and
A6: (ex F being (FinSequence of ( CQC-WFF )) st (((p2 = ( F . ( len f ) ) & ( len F ) = ( len f )) & (( F . 1 ) = ( Begin f ) or ( len f ) = ( 0 ))) & (for n holds ((1 <= n & n < ( len f )) implies P[ n,( F . n ),( F . ( n + 1 ) ) ]))));
consider H being (FinSequence of ( CQC-WFF )) such that A7: p2 = ( H . ( len f ) ) and A8: ((( len H ) = ( len f ) & (( H . 1 ) = ( Begin f ) or ( len f ) = ( 0 ))) & (for n holds ((1 <= n & n < ( len f )) implies P[ n,( H . n ),( H . ( n + 1 ) ) ]))) by A6;
consider G being (FinSequence of ( CQC-WFF )) such that A9: p1 = ( G . ( len f ) ) and A10: ((( len G ) = ( len f ) & (( G . 1 ) = ( Begin f ) or ( len f ) = ( 0 ))) & (for n holds ((1 <= n & n < ( len f )) implies P[ n,( G . n ),( G . ( n + 1 ) ) ]))) by A5;
A11: (for n holds ((1 <= n & n < ( len f )) implies (for x,y1,y2 being (Element of ( CQC-WFF )) holds ((P[ n,x,y1 ] & P[ n,x,y2 ]) implies y1 = y2))));
G = H from RECDEF_1:sch 8(A11,A10,A8);
hence thesis by A9,A7;
end;
