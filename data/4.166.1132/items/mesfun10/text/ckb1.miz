begin
reserve seq1 for ExtREAL_sequence;
reserve seq2 for ExtREAL_sequence;
theorem
Th1: ((for n being Nat holds ( seq1 . n ) <= ( seq2 . n )) implies ( inf ( rng seq1 ) ) <= ( inf ( rng seq2 ) ))
proof
assume A1: (for n being Nat holds ( seq1 . n ) <= ( seq2 . n ));
now
let x being  ext-real number;
A2:now
let n being (Element of ( NAT ));
( dom seq1 ) = ( NAT ) by FUNCT_2:def 1;
then A3: ( seq1 . n ) in ( rng seq1 ) by FUNCT_1:def 3;
A4: ( seq1 . n ) <= ( seq2 . n ) by A1;
( inf ( rng seq1 ) ) is (LowerBound of ( rng seq1 )) by XXREAL_2:def 4;
then ( inf ( rng seq1 ) ) <= ( seq1 . n ) by A3,XXREAL_2:def 2;
hence ( inf ( rng seq1 ) ) <= ( seq2 . n ) by A4,XXREAL_0:2;
end;
assume x in ( rng seq2 );
then (ex z being set st (z in ( dom seq2 ) & x = ( seq2 . z ))) by FUNCT_1:def 3;
hence ( inf ( rng seq1 ) ) <= x by A2;
end;
then ( inf ( rng seq1 ) ) is (LowerBound of ( rng seq2 )) by XXREAL_2:def 2;
hence thesis by XXREAL_2:def 4;
end;
