begin
reserve seq for ExtREAL_sequence;
reserve a for R_eal;
theorem
Th4: ((for n being Nat holds ( seq . n ) >= a) implies ( inf seq ) >= a)
proof
assume A1: (for n being Nat holds ( seq . n ) >= a);
now
let x being  ext-real number;
assume x in ( rng seq );
then (ex z being set st (z in ( dom seq ) & x = ( seq . z ))) by FUNCT_1:def 3;
hence x >= a by A1;
end;
then a is (LowerBound of ( rng seq )) by XXREAL_2:def 2;
hence thesis by XXREAL_2:def 4;
end;
