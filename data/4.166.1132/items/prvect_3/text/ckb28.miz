registration
let G being  scalar-distributive non  empty RLSStruct;
let F being  scalar-distributive non  empty RLSStruct;
cluster [: G,F :] ->  scalar-distributive;
correctness
proof
let a0 being  real number;
let b0 being  real number;
let x being (VECTOR of [: G,F :]);
reconsider a = a0,b = b0 as (Element of ( REAL )) by XREAL_0:def 1;
consider x1 being (Point of G),x2 being (Point of F) such that A1: x = [ x1,x2 ] by CKB8:1;
A2: (( ( a + b ) * x1 ) = ( ( a0 * x1 ) + ( b0 * x1 ) ) & ( ( a + b ) * x2 ) = ( ( a0 * x2 ) + ( b0 * x2 ) )) by RLVECT_1:def 6;
thus ( ( a0 + b0 ) * x ) = [ ( ( a + b ) * x1 ),( ( a + b ) * x2 ) ] by A1,CKB10:def 1
.= ( ( prod_ADD (G,F) ) . ([ ( a * x1 ),( a * x2 ) ],[ ( b * x1 ),( b * x2 ) ]) ) by A2,CKB9:def 1
.= ( ( prod_ADD (G,F) ) . (( ( prod_MLT (G,F) ) . (a,[ x1,x2 ]) ),[ ( b * x1 ),( b * x2 ) ]) ) by CKB10:def 1
.= ( ( a0 * x ) + ( b0 * x ) ) by A1,CKB10:def 1;
end;
end;
