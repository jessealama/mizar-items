begin
theorem
(for G,F being  add-associative  right_zeroed  right_complementable non empty RLSStruct holds (for x being (Point of [: G,F :]) holds (for x1 being (Point of G) holds (for x2 being (Point of F) holds (x = [ x1,x2 ] implies ( - x ) = [ ( - x1 ),( - x2 ) ])))))
proof
let G being  add-associative  right_zeroed  right_complementable non empty RLSStruct,F being  add-associative  right_zeroed  right_complementable non empty RLSStruct;
let x being (Point of [: G,F :]);
let x1 being (Point of G);
let x2 being (Point of F);
assume A1: x = [ x1,x2 ];
reconsider y = [ ( - x1 ),( - x2 ) ] as (Point of [: G,F :]);
( x + y ) = [ ( x1 + ( - x1 ) ),( x2 + ( - x2 ) ) ] by A1,CKB9:def 1
.= [ ( 0. G ),( x2 + ( - x2 ) ) ] by RLVECT_1:def 10
.= ( 0. [: G,F :] ) by RLVECT_1:def 10;
hence thesis by RLVECT_1:def 10;
end;
