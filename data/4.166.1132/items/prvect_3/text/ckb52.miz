requirements SUBSET,REAL,BOOLE,NUMERALS,ARITHM;
begin
registration
let X being  complete RealNormSpace;
let Y being  complete RealNormSpace;
cluster [: X,Y :] ->  complete;
coherence
proof
A1: ( dom <* X,Y *> ) = { 1,2 } by FINSEQ_1:2,FINSEQ_1:89;
now
let i being (Element of ( dom <* X,Y *> ));
(i = 1 or i = 2) by A1,TARSKI:def 2;
hence ( <* X,Y *> . i ) is  complete by FINSEQ_1:44;
end;
then A2: ( product <* X,Y *> ) is  complete by PRVECT_2:14;
consider I being (Function of [: X,Y :],( product <* X,Y *> )) such that A3: ((((((I is  one-to-one & I is  onto) & (for x being (Point of X) holds (for y being (Point of Y) holds ( I . (x,y) ) = <* x,y *>))) & (for v,w being (Point of [: X,Y :]) holds ( I . ( v + w ) ) = ( ( I . v ) + ( I . w ) ))) & (for v being (Point of [: X,Y :]) holds (for r being (Element of ( REAL )) holds ( I . ( r * v ) ) = ( r * ( I . v ) )))) & ( 0. ( product <* X,Y *> ) ) = ( I . ( 0. [: X,Y :] ) )) & (for v being (Point of [: X,Y :]) holds ||. ( I . v ) .|| = ||. v .||)) by CKB45:1;
A4:now
let v being (Point of [: X,Y :]),w being (Point of [: X,Y :]);
thus ( I . ( v - w ) ) = ( I . ( v + ( ( - 1 ) * w ) ) ) by RLVECT_1:16
.= ( ( I . v ) + ( I . ( ( - 1 ) * w ) ) ) by A3
.= ( ( I . v ) + ( ( - 1 ) * ( I . w ) ) ) by A3
.= ( ( I . v ) - ( I . w ) ) by RLVECT_1:16;
end;
A5:now
let v being (Point of [: X,Y :]),w being (Point of [: X,Y :]);
thus ||. ( ( I . v ) - ( I . w ) ) .|| = ||. ( I . ( v - w ) ) .|| by A4
.= ||. ( v - w ) .|| by A3;
end;
now
let seq being (sequence of [: X,Y :]);
assume A6: seq is  Cauchy_sequence_by_Norm;
reconsider Iseq = ( I * seq ) as (sequence of ( product <* X,Y *> ));
now
let r being Real;
assume r > ( 0 );
then consider k being (Element of ( NAT )) such that A7: (for n,m being (Element of ( NAT )) holds ((n >= k & m >= k) implies ||. ( ( seq . n ) - ( seq . m ) ) .|| < r)) by A6,RSSPACE3:8;
take k;
hereby
let n being (Element of ( NAT )),m being (Element of ( NAT ));
assume (n >= k & m >= k);
then A8: ||. ( ( seq . n ) - ( seq . m ) ) .|| < r by A7;
( NAT ) = ( dom seq ) by FUNCT_2:def 1;
then (( Iseq . n ) = ( I . ( seq . n ) ) & ( Iseq . m ) = ( I . ( seq . m ) )) by FUNCT_1:13;
hence ||. ( ( Iseq . n ) - ( Iseq . m ) ) .|| < r by A8,A5;
end;
end;
then Iseq is  Cauchy_sequence_by_Norm by RSSPACE3:8;
then A9: Iseq is  convergent by A2,LOPBAN_1:def 15;
(( dom ( I " ) ) = ( rng I ) & ( rng ( I " ) ) = ( dom I )) by A3,FUNCT_1:33;
then (( dom ( I " ) ) = (the carrier of ( product <* X,Y *> )) & ( rng ( I " ) ) = (the carrier of [: X,Y :])) by A3,FUNCT_2:def 1,FUNCT_2:def 3;
then reconsider Lseq = ( ( I " ) . ( lim Iseq ) ) as (Point of [: X,Y :]) by FUNCT_1:3;
( rng I ) = (the carrier of ( product <* X,Y *> )) by A3,FUNCT_2:def 3;
then A10: ( I . Lseq ) = ( lim Iseq ) by A3,FUNCT_1:35;
now
let r being Real;
assume ( 0 ) < r;
then consider m being (Element of ( NAT )) such that A11: (for n being (Element of ( NAT )) holds (m <= n implies ||. ( ( Iseq . n ) - ( lim Iseq ) ) .|| < r)) by A9,NORMSP_1:def 7;
take m;
thus (for n being (Element of ( NAT )) holds (m <= n implies ||. ( ( seq . n ) - Lseq ) .|| < r))
proof
let n being (Element of ( NAT ));
assume m <= n;
then A12: ||. ( ( Iseq . n ) - ( lim Iseq ) ) .|| < r by A11;
( NAT ) = ( dom seq ) by FUNCT_2:def 1;
then ( Iseq . n ) = ( I . ( seq . n ) ) by FUNCT_1:13;
hence ||. ( ( seq . n ) - Lseq ) .|| < r by A10,A5,A12;
end;

end;
hence seq is  convergent by NORMSP_1:def 6;
end;
hence thesis by LOPBAN_1:def 15;
end;
end;
