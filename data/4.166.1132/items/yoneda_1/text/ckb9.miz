environ
vocabularies CAT_1,ENS_1,FUNCT_1,GRAPH_1,RELAT_1,SUBSET_1,MCART_1,NATTRA_1,STRUCT_0,XBOOLE_0,PZFMISC1,TARSKI,OPPCAT_1,CAT_2,FUNCT_2,YONEDA_1;
notations TARSKI,XBOOLE_0,SUBSET_1,MCART_1,RELAT_1,FUNCT_1,BINOP_1,FUNCT_2,STRUCT_0,GRAPH_1,CAT_1,CAT_2,OPPCAT_1,ENS_1,NATTRA_1,CKB1,CKB3,CKB5,CKB7,CKB8;
definitions TARSKI,BINOP_1,GRAPH_1,CKB1,CKB3,CKB5,CKB7,CKB8;
theorems ZFMISC_1,MCART_1,FUNCT_1,FUNCT_2,CAT_1,CAT_2,OPPCAT_1,ENS_1,NATTRA_1,ISOCAT_1,RELAT_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8;
schemes FUNCT_2;
registrations XBOOLE_0,FUNCT_1,RELSET_1,ENS_1,STRUCT_0,FUNCT_2;
constructors PARTFUN1,DOMAIN_1,NATTRA_1,ENS_1,RELSET_1,CKB1,CKB3,CKB5,CKB7,CKB8;
requirements SUBSET,BOOLE;
begin
reserve A for Category;
theorem
(for F being (Functor of A,( Functors (A,( EnsHom A )) )) holds ((( Obj F ) is  one-to-one & F is  faithful) implies F is  one-to-one))
proof
let F being (Functor of A,( Functors (A,( EnsHom A )) ));
assume A1: ( Obj F ) is  one-to-one;
assume A2: F is  faithful;
(for x1,x2 being set holds (((x1 in ( dom F ) & x2 in ( dom F )) & ( F . x1 ) = ( F . x2 )) implies x1 = x2))
proof
let x1 being set;
let x2 being set;
assume that
A3: (x1 in ( dom F ) & x2 in ( dom F ))
and
A4: ( F . x1 ) = ( F . x2 );
reconsider m1 = x1,m2 = x2 as (Morphism of A) by A3,FUNCT_2:def 1;
set o1 = ( dom m1 );
set o2 = ( cod m1 );
set o3 = ( dom m2 );
set o4 = ( cod m2 );
reconsider m19 = m1 as (Morphism of o1,o2) by CAT_1:4;
reconsider m29 = m2 as (Morphism of o3,o4) by CAT_1:4;
A5: ( Hom (o1,o2) ) <> ( {} ) by CAT_1:2;
then A6: ( Hom (( F . o1 ),( F . o2 )) ) <> ( {} ) by CAT_1:84;
A7: ( ( Obj F ) . o4 ) = ( F . o4 ) by CAT_1:def 17;
A8: ( ( Obj F ) . o3 ) = ( F . o3 ) by CAT_1:def 17;
A9: ( Hom (o3,o4) ) <> ( {} ) by CAT_1:2;
then A10: ( Hom (( F . o3 ),( F . o4 )) ) <> ( {} ) by CAT_1:84;
A11: ( F . m19 ) = ( F . m2 ) by A4,A5,NATTRA_1:def 1
.= ( F . m29 ) by A9,NATTRA_1:def 1;
( ( Obj F ) . o1 ) = ( F . o1 ) by CAT_1:def 17
.= ( dom ( F . m29 ) ) by A11,A6,CAT_1:5
.= ( ( Obj F ) . o3 ) by A10,A8,CAT_1:5;
then A12: (m2 is (Morphism of ( dom m2 ),( cod m2 )) & o1 = o3) by A1,CAT_1:4,FUNCT_2:19;
( ( Obj F ) . o2 ) = ( F . o2 ) by CAT_1:def 17
.= ( cod ( F . m29 ) ) by A11,A6,CAT_1:5
.= ( ( Obj F ) . o4 ) by A10,A7,CAT_1:5;
then (m1 is (Morphism of ( dom m1 ),( cod m1 )) & m2 is (Morphism of o1,o2)) by A1,A12,CAT_1:4,FUNCT_2:19;
hence thesis by A2,A4,A5,CAT_1:def 21;
end;
hence thesis by FUNCT_1:def 4;
end;
