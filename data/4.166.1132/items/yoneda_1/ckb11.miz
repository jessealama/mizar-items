environ
vocabularies CAT_1,ENS_1,FUNCT_1,GRAPH_1,RELAT_1,SUBSET_1,MCART_1,NATTRA_1,STRUCT_0,XBOOLE_0,PZFMISC1,TARSKI,OPPCAT_1,CAT_2,FUNCT_2,YONEDA_1;
notations TARSKI,XBOOLE_0,SUBSET_1,MCART_1,RELAT_1,FUNCT_1,BINOP_1,FUNCT_2,STRUCT_0,GRAPH_1,CAT_1,CAT_2,OPPCAT_1,ENS_1,NATTRA_1,CKB1,CKB3,CKB5,CKB7,CKB8,CKB10;
definitions TARSKI,BINOP_1,GRAPH_1,CKB1,CKB3,CKB5,CKB7,CKB8,CKB10;
theorems ZFMISC_1,MCART_1,FUNCT_1,FUNCT_2,CAT_1,CAT_2,OPPCAT_1,ENS_1,NATTRA_1,ISOCAT_1,RELAT_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10;
