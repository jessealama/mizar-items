begin
theorem
Th22: (for F being  finite set holds (for A being (FinSequence of ( bool F )) holds (for B being (Reduction of A) holds (for C being (Reduction of B) holds C is (Reduction of A)))))
proof
let F being  finite set;
let A being (FinSequence of ( bool F ));
let B being (Reduction of A);
let C being (Reduction of B);
A1: (for i being (Element of ( NAT )) holds (i in ( dom A ) implies ( C . i ) c= ( A . i )))
proof
let i being (Element of ( NAT ));
assume A2: i in ( dom A );
then i in ( dom B ) by CKB32:def 1;
then A3: ( C . i ) c= ( B . i ) by CKB32:def 1;
( B . i ) c= ( A . i ) by A2,CKB32:def 1;
hence thesis by A3,XBOOLE_1:1;
end;
( dom B ) = ( dom C ) by CKB32:def 1;
hence thesis by A1,CKB32:def 1;
end;
