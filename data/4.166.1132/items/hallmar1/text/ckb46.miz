begin
theorem
Th28: (for F being non empty  finite set holds (for A being non empty (FinSequence of ( bool F )) holds (for X being set holds (for B being (Reduction of A) holds (X is_a_system_of_different_representatives_of B implies X is_a_system_of_different_representatives_of A)))))
proof
let F being non empty  finite set;
let A being non empty (FinSequence of ( bool F ));
let X being set;
let B being (Reduction of A);
assume that
A1: X is_a_system_of_different_representatives_of B;
X is_a_system_of_different_representatives_of A
proof
consider f being (FinSequence of F) such that A2: f = X and A3: ( dom B ) = ( dom f ) and A4: (for i being (Element of ( NAT )) holds (i in ( dom f ) implies ( f . i ) in ( B . i ))) and A5: f is  one-to-one by A1,CKB22:def 1;
A6: (for i being (Element of ( NAT )) holds (i in ( dom f ) implies ( f . i ) in ( A . i )))
proof
let i being (Element of ( NAT ));
assume that
A7: i in ( dom f );
A8: ( f . i ) in ( B . i ) by A4,A7;
( dom B ) = ( dom A ) by CKB32:def 1;
then ( B . i ) c= ( A . i ) by A3,A7,CKB32:def 1;
hence thesis by A8;
end;
( dom A ) = ( dom B ) by CKB32:def 1;
hence thesis by A2,A3,A5,A6,CKB22:def 1;
end;
hence thesis;
end;
