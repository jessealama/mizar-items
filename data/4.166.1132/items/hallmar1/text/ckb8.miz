theorem
Th3: (for F being set holds (for A being (FinSequence of ( bool F )) holds (for J being set holds ( union (A,J) ) c= F)))
proof
let F being set;
let A being (FinSequence of ( bool F ));
let J being set;
let x being set;
assume x in ( union (A,J) );
then consider j being set such that j in J and A1: j in ( dom A ) and A2: x in ( A . j ) by CKB7:def 1;
(( rng A ) c= ( bool F ) & ( A . j ) in ( rng A )) by A1,FINSEQ_1:def 4,FUNCT_1:3;
hence thesis by A2;
end;
