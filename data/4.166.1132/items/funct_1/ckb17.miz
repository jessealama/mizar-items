environ
vocabularies RELAT_1,XBOOLE_0,ZFMISC_1,SUBSET_1,TARSKI,SETFAM_1,FUNCT_1;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,RELAT_1,SETFAM_1,CKB1,CKB4,CKB7,CKB10;
definitions TARSKI,XBOOLE_0,RELAT_1,SETFAM_1,ZFMISC_1,CKB1,CKB7,CKB10;
theorems TARSKI,XBOOLE_0,ZFMISC_1,RELAT_1,XBOOLE_1,SUBSET_1,CKB1,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB15,CKB16;
schemes TARSKI,XBOOLE_0,CKB6,CKB13,CKB14;
registrations XBOOLE_0,RELAT_1,ZFMISC_1,CKB2,CKB3,CKB5;
constructors SETFAM_1,RELAT_1,CKB1,CKB7;
requirements SUBSET,BOOLE;
begin
reserve x for set;
reserve y for set;
reserve f for Function;
reserve g for Function;
theorem
(((( dom f ) = ( dom g ) & ( rng f ) = { y }) & ( rng g ) = { y }) implies f = g)
proof
assume that
A1: ( dom f ) = ( dom g )
and
A2: ( rng f ) = { y }
and
A3: ( rng g ) = { y };
(x in ( dom f ) implies ( f . x ) = ( g . x ))
proof
assume A4: x in ( dom f );
then ( f . x ) in ( rng f ) by CKB10:def 1;
then A5: ( f . x ) = y by A2,TARSKI:def 1;
( g . x ) in ( rng g ) by A1,A4,CKB10:def 1;
hence thesis by A3,A5,TARSKI:def 1;
end;
hence thesis by A1,CKB9:1;
end;
