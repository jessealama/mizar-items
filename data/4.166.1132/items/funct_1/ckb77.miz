environ
vocabularies RELAT_1,XBOOLE_0,ZFMISC_1,SUBSET_1,TARSKI,SETFAM_1,FUNCT_1;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,RELAT_1,SETFAM_1,CKB1,CKB4,CKB7,CKB10,CKB20,CKB37,CKB50;
definitions TARSKI,XBOOLE_0,RELAT_1,SETFAM_1,ZFMISC_1,CKB1,CKB7,CKB10,CKB37,CKB50;
theorems TARSKI,XBOOLE_0,ZFMISC_1,RELAT_1,XBOOLE_1,SUBSET_1,CKB1,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB15,CKB16,CKB17,CKB18,CKB19,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB37,CKB38,CKB39,CKB40,CKB41,CKB42,CKB43,CKB45,CKB46,CKB50,CKB51,CKB52,CKB53,CKB54,CKB55,CKB56,CKB57,CKB58,CKB59,CKB62,CKB63,CKB64,CKB65,CKB66,CKB67,CKB69,CKB70,CKB71,CKB72,CKB73,CKB74,CKB75;
schemes TARSKI,XBOOLE_0,CKB6,CKB13,CKB14;
registrations XBOOLE_0,RELAT_1,ZFMISC_1,CKB2,CKB3,CKB5,CKB21,CKB29,CKB44,CKB47,CKB48,CKB49,CKB60,CKB61,CKB68,CKB76;
constructors SETFAM_1,RELAT_1,CKB1,CKB7,CKB37,CKB50;
requirements SUBSET,BOOLE;
begin
reserve Y for set;
reserve x for set;
reserve y for set;
reserve f for Function;
reserve g for Function;
theorem
Th85: (g = ( Y | f ) iff ((for x holds (x in ( dom g ) iff (x in ( dom f ) & ( f . x ) in Y))) & (for x holds (x in ( dom g ) implies ( g . x ) = ( f . x )))))
proof
hereby
assume A1: g = ( Y | f );
hereby
let x;
hereby
assume x in ( dom g );
then A2: [ x,( g . x ) ] in g by CKB7:def 1;
then A3: [ x,( g . x ) ] in f by A1,RELAT_1:def 12;
hence x in ( dom f ) by RELAT_1:def 4;
then ( f . x ) = ( g . x ) by A3,CKB7:def 1;
hence ( f . x ) in Y by A1,A2,RELAT_1:def 12;
end;
assume x in ( dom f );
then A4: [ x,( f . x ) ] in f by CKB7:def 1;
assume ( f . x ) in Y;
then [ x,( f . x ) ] in g by A1,A4,RELAT_1:def 12;
hence x in ( dom g ) by RELAT_1:def 4;
end;
let x;
assume x in ( dom g );
then [ x,( g . x ) ] in g by CKB7:def 1;
then A5: [ x,( g . x ) ] in f by A1,RELAT_1:def 12;
then x in ( dom f ) by RELAT_1:def 4;
hence ( f . x ) = ( g . x ) by A5,CKB7:def 1;
end;
assume that
A6: (for x holds (x in ( dom g ) iff (x in ( dom f ) & ( f . x ) in Y)))
and
A7: (for x holds (x in ( dom g ) implies ( g . x ) = ( f . x )));
now
let x;
let y;
hereby
assume A8: [ x,y ] in g;
then A9: x in ( dom g ) by RELAT_1:def 4;
then A10: y = ( g . x ) by A8,CKB7:def 1
.= ( f . x ) by A7,A9;
hence y in Y by A6,A9;
x in ( dom f ) by A6,A9;
hence [ x,y ] in f by A10,CKB7:def 1;
end;
assume A11: y in Y;
assume A12: [ x,y ] in f;
then A13: y = ( f . x ) by CKB8:1;
x in ( dom f ) by A12,RELAT_1:def 4;
then A14: x in ( dom g ) by A6,A11,A13;
then y = ( g . x ) by A7,A13;
hence [ x,y ] in g by A14,CKB7:def 1;
end;
hence thesis by RELAT_1:def 12;
end;
