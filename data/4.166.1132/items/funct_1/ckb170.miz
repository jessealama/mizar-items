begin
reserve X for set;
reserve x for set;
reserve y for set;
reserve z for set;
scheme NonUniqBoundFuncEx { X() -> set,Y() -> set,P[set,set] } : (ex f being Function st ((( dom f ) = X() & ( rng f ) c= Y()) & (for x holds (x in X() implies P[ x,( f . x ) ]))))
provided
A1: (for x holds (x in X() implies (ex y st (y in Y() & P[ x,y ]))))
proof
per cases ;
suppose A19: X() = ( {} );

take ( {} );
thus thesis by A19,XBOOLE_1:2;
end;
suppose A10: X() <> ( {} );

defpred Q[ set,set ]
 means
(for y holds (y in $2 iff (y in Y() & P[ $1,y ])));
A2: (for e,u1,u2 being set holds (((e in X() & Q[ e,u1 ]) & Q[ e,u2 ]) implies u1 = u2))
proof
let e being set,u1 being set,u2 being set;
assume that
e in X();
assume A3: (for y holds (y in u1 iff (y in Y() & P[ e,y ])));
defpred A[ set ]
 means
($1 in Y() & P[ e,$1 ]);
A4: (for x being set holds (x in u1 iff A[ x ])) by A3;
assume A5: (for y holds (y in u2 iff (y in Y() & P[ e,y ])));
A6: (for x being set holds (x in u2 iff A[ x ])) by A5;
u1 = u2 from XBOOLE_0:sch 2(A4,A6);
hence thesis;
end;
A7: (for x holds (x in X() implies (ex y st Q[ x,y ])))
proof
let x;
assume that
x in X();
defpred R[ set ]
 means
P[ x,$1 ];
consider X such that A8: (y in X iff (y in Y() & R[ y ])) from XBOOLE_0:sch 1;
take X;
thus thesis by A8;
end;
consider G being Function such that A9: (( dom G ) = X() & (for x holds (x in X() implies Q[ x,( G . x ) ]))) from CKB13:sch 1(A2,A7);
reconsider D = ( rng G ) as non empty set by A9,A10,RELAT_1:42;
now
let X;
assume X in D;
then consider x such that A11: (x in ( dom G ) & X = ( G . x )) by CKB10:def 1;
((for y holds (y in X iff (y in Y() & P[ x,y ]))) & (ex y st (y in Y() & P[ x,y ]))) by A1,A9,A11;
hence X <> ( {} );
end;
then consider F being Function such that A12: ( dom F ) = D and A13: (for X holds (X in D implies ( F . X ) in X)) by CKB169:1;
A14: ( dom ( F * G ) ) = X() by A9,A12,RELAT_1:27;
take f = ( F * G );
thus ( dom f ) = X() by A9,A12,RELAT_1:27;
( rng F ) c= Y()
proof
let x;
assume x in ( rng F );
then consider y such that A15: y in ( dom F ) and A16: x = ( F . y ) by CKB10:def 1;
A17: (ex z st (z in ( dom G ) & y = ( G . z ))) by A12,A15,CKB10:def 1;
x in y by A12,A13,A15,A16;
hence thesis by A9,A17;
end;
hence ( rng f ) c= Y() by A12,RELAT_1:28;
let x;
assume A18: x in X();
then (( f . x ) = ( F . ( G . x ) ) & ( G . x ) in D) by A9,A14,CKB24:1,CKB10:def 1;
then ( f . x ) in ( G . x ) by A13;
hence thesis by A9,A18;
end;
end;
