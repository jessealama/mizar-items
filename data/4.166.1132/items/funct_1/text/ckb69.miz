environ
vocabularies RELAT_1,XBOOLE_0,ZFMISC_1,SUBSET_1,TARSKI,SETFAM_1,FUNCT_1;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,RELAT_1,SETFAM_1,CKB1,CKB4,CKB7,CKB10,CKB20,CKB37,CKB50;
definitions TARSKI,XBOOLE_0,RELAT_1,SETFAM_1,ZFMISC_1,CKB1,CKB7,CKB10,CKB37,CKB50;
theorems TARSKI,XBOOLE_0,ZFMISC_1,RELAT_1,XBOOLE_1,SUBSET_1,CKB1,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB15,CKB16,CKB17,CKB18,CKB19,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB37,CKB38,CKB39,CKB40,CKB41,CKB42,CKB43,CKB45,CKB46,CKB50,CKB51,CKB52,CKB53,CKB54,CKB55,CKB56,CKB57,CKB58,CKB59,CKB62,CKB63,CKB64,CKB65,CKB66,CKB67;
schemes TARSKI,XBOOLE_0,CKB6,CKB13,CKB14;
registrations XBOOLE_0,RELAT_1,ZFMISC_1,CKB2,CKB3,CKB5,CKB21,CKB29,CKB44,CKB47,CKB48,CKB49,CKB60,CKB61,CKB68;
constructors SETFAM_1,RELAT_1,CKB1,CKB7,CKB37,CKB50;
requirements SUBSET,BOOLE;
begin
reserve X for set;
reserve x for set;
reserve y for set;
reserve f for Function;
reserve g for Function;
theorem
((( dom g ) = ( ( dom f ) /\ X ) & (for x holds (x in ( dom g ) implies ( g . x ) = ( f . x )))) implies g = ( f | X ))
proof
assume that
A1: ( dom g ) = ( ( dom f ) /\ X )
and
A2: (for x holds (x in ( dom g ) implies ( g . x ) = ( f . x )));
now
let x;
let y;
hereby
assume A3: [ x,y ] in g;
then A4: x in ( dom g ) by RELAT_1:def 4;
hence x in X by A1,XBOOLE_0:def 4;
A5: x in ( dom f ) by A1,A4,XBOOLE_0:def 4;
y = ( g . x ) by A3,A4,CKB7:def 1
.= ( f . x ) by A2,A4;
hence [ x,y ] in f by A5,CKB7:def 1;
end;
assume A6: x in X;
assume A7: [ x,y ] in f;
then A8: x in ( dom f ) by RELAT_1:def 4;
then A9: x in ( dom g ) by A1,A6,XBOOLE_0:def 4;
y = ( f . x ) by A7,A8,CKB7:def 1
.= ( g . x ) by A2,A9;
hence [ x,y ] in g by A9,CKB7:def 1;
end;
hence thesis by RELAT_1:def 11;
end;
