begin
reserve x for set;
reserve y for set;
reserve f for Function;
reserve g for Function;
theorem
(for B being non  empty  functional set holds (for f being Function holds (f = ( union B ) implies (( dom f ) = ( union { ( dom g ) where g is (Element of B): (not contradiction) } ) & ( rng f ) = ( union { ( rng g ) where g is (Element of B): (not contradiction) } )))))
proof
let B being non  empty  functional set;
let f being Function;
assume that
A1: f = ( union B );
set X = { ( dom g ) where g is (Element of B): (not contradiction) };
now
let x being set;
hereby
assume x in ( dom f );
then [ x,( f . x ) ] in f by CKB8:1;
then consider g being set such that A2: [ x,( f . x ) ] in g and A3: g in B by A1,TARSKI:def 4;
reconsider g as Function by A3;
take Z = ( dom g );
thus (x in Z & Z in X) by A2,A3,CKB8:1;
end;
given Z being set such that
A4: x in Z
and
A5: Z in X;

consider g being (Element of B) such that A6: Z = ( dom g ) by A5;
[ x,( g . x ) ] in g by A4,A6,CKB8:1;
then [ x,( g . x ) ] in f by A1,TARSKI:def 4;
hence x in ( dom f ) by CKB8:1;
end;
hence ( dom f ) = ( union X ) by TARSKI:def 4;
set X = { ( rng g ) where g is (Element of B): (not contradiction) };
now
let y being set;
hereby
assume y in ( rng f );
then consider x being set such that A7: (x in ( dom f ) & y = ( f . x )) by CKB10:def 1;
[ x,y ] in f by A7,CKB8:1;
then consider g being set such that A8: [ x,y ] in g and A9: g in B by A1,TARSKI:def 4;
reconsider g as Function by A9;
take Z = ( rng g );
(x in ( dom g ) & y = ( g . x )) by A8,CKB8:1;
hence (y in Z & Z in X) by A9,CKB10:def 1;
end;
given Z being set such that
A10: y in Z
and
A11: Z in X;

consider g being (Element of B) such that A12: Z = ( rng g ) by A11;
consider x being set such that A13: (x in ( dom g ) & y = ( g . x )) by A10,A12,CKB10:def 1;
[ x,y ] in g by A13,CKB8:1;
then [ x,y ] in f by A1,TARSKI:def 4;
hence y in ( rng f ) by RELAT_1:6;
end;
hence thesis by TARSKI:def 4;
end;
