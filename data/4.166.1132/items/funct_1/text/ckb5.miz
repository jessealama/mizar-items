environ
vocabularies RELAT_1,XBOOLE_0,ZFMISC_1,SUBSET_1,TARSKI,SETFAM_1,FUNCT_1;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,RELAT_1,SETFAM_1,CKB1,CKB4;
definitions TARSKI,XBOOLE_0,RELAT_1,SETFAM_1,ZFMISC_1,CKB1;
theorems TARSKI,XBOOLE_0,ZFMISC_1,RELAT_1,XBOOLE_1,SUBSET_1,CKB1;
schemes TARSKI,XBOOLE_0;
registrations XBOOLE_0,RELAT_1,ZFMISC_1,CKB2,CKB3;
constructors SETFAM_1,RELAT_1,CKB1;
requirements SUBSET,BOOLE;
begin
registration
let a being set;
let b being set;
cluster { [ a,b ] } ->  Function-like;
coherence
proof
set X = { [ a,b ] };
A1: [: { a },{ b } :] = X by ZFMISC_1:29;
(for x,y1,y2 being set holds (([ x,y1 ] in X & [ x,y2 ] in X) implies y1 = y2))
proof
let x being set,y1 being set,y2 being set;
assume that
A2: [ x,y1 ] in X
and
A3: [ x,y2 ] in X;
y1 = b by A1,A2,ZFMISC_1:28;
hence thesis by A1,A3,ZFMISC_1:28;
end;
hence thesis by CKB1:def 1;
end;
end;
