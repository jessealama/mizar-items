environ
vocabularies RELAT_1,XBOOLE_0,ZFMISC_1,SUBSET_1,TARSKI,SETFAM_1,FUNCT_1;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,RELAT_1,SETFAM_1,CKB1,CKB4,CKB7;
definitions TARSKI,XBOOLE_0,RELAT_1,SETFAM_1,ZFMISC_1,CKB1,CKB7;
theorems TARSKI,XBOOLE_0,ZFMISC_1,RELAT_1,XBOOLE_1,SUBSET_1,CKB1,CKB7,CKB8;
schemes TARSKI,XBOOLE_0,CKB6;
registrations XBOOLE_0,RELAT_1,ZFMISC_1,CKB2,CKB3,CKB5;
constructors SETFAM_1,RELAT_1,CKB1,CKB7;
requirements SUBSET,BOOLE;
begin
reserve x for set;
reserve y for set;
reserve f for Function;
reserve g for Function;
theorem
Th9: ((( dom f ) = ( dom g ) & (for x holds (x in ( dom f ) implies ( f . x ) = ( g . x )))) implies f = g)
proof
assume that
A1: ( dom f ) = ( dom g )
and
A2: (for x holds (x in ( dom f ) implies ( f . x ) = ( g . x )));
let x;
let y;
thus ([ x,y ] in f implies [ x,y ] in g)
proof
assume A3: [ x,y ] in f;
then A4: x in ( dom f ) by RELAT_1:def 4;
then ( f . x ) = y by A3,CKB7:def 1;
then ( g . x ) = y by A2,A4;
hence thesis by A1,A4,CKB7:def 1;
end;

assume A5: [ x,y ] in g;
then A6: x in ( dom g ) by RELAT_1:def 4;
then ( g . x ) = y by A5,CKB7:def 1;
then ( f . x ) = y by A1,A2,A6;
hence thesis by A1,A6,CKB7:def 1;
end;
