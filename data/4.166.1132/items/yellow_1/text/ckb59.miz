constructors SETFAM_1,WELLORD2,TOLER_1,TOPS_2,LATTICE3,YELLOW_0,PRALG_1,ORDERS_3,CARD_3,RELSET_1,CKB5,CKB6,CKB10,CKB42,CKB45,CKB46,CKB48;
requirements SUBSET,BOOLE,NUMERALS;
begin
reserve X for set;
reserve x for set;
reserve y for set;
registration
let X being non  empty set;
let Y being non  empty  with_infima  antisymmetric RelStr;
cluster ( Y |^ X ) ->  with_infima;
coherence
proof
set IT = ( Y |^ X );
let x being (Element of IT);
let y being (Element of IT);
reconsider y9 = y as (Function of X,(the carrier of Y)) by CKB53:1;
reconsider x9 = x as (Function of X,(the carrier of Y)) by CKB53:1;
defpred P[ set,set ]
 means
(ex xa,ya being (Element of Y) st ((xa = ( x9 . $1 ) & ya = ( y9 . $1 )) & $2 = ( xa "/\" ya )));
A1: (for x being set holds (x in X implies (ex y being set st (y in (the carrier of Y) & P[ x,y ]))))
proof
let a being set;
assume a in X;
then reconsider xa = ( x9 . a ),ya = ( y9 . a ) as (Element of Y) by FUNCT_2:5;
take y = ( xa "/\" ya );
thus y in (the carrier of Y);
take xa;
take ya;
thus thesis;
end;
consider f being (Function of X,(the carrier of Y)) such that A2: (for a being set holds (a in X implies P[ a,( f . a ) ])) from FUNCT_2:sch 1(A1);
f in ( Funcs (X,(the carrier of Y)) ) by FUNCT_2:8;
then reconsider z = f as (Element of IT) by CKB51:1;
take z;
A3: z <= y
proof
reconsider y1 = y,z1 = z as (Element of ( product ( X --> Y ) ));
A4: (ex f,g being Function st ((f = z1 & g = y1) & (for i being set holds (i in X implies (ex R being RelStr st (ex xi,yi being (Element of R) st (((R = ( ( X --> Y ) . i ) & xi = ( f . i )) & yi = ( g . i )) & xi <= yi)))))))
proof
take f;
take y9;
thus (f = z1 & y9 = y1);
let i being set;
assume i in X;
then reconsider i as (Element of X);
reconsider R = ( ( X --> Y ) . i ) as RelStr;
reconsider xi = ( f . i ),yi = ( y9 . i ) as (Element of R) by FUNCOP_1:7;
take R;
take xi;
take yi;
(R = Y & (ex xa,ya being (Element of Y) st ((xa = ( x9 . i ) & ya = ( y9 . i )) & ( f . i ) = ( xa "/\" ya )))) by A2,FUNCOP_1:7;
hence thesis by YELLOW_0:23;
end;
z1 in (the carrier of ( product ( X --> Y ) ));
then z1 in ( product ( Carrier ( X --> Y ) ) ) by CKB46:def 1;
hence thesis by A4,CKB46:def 1;
end;
A5: (for z9 being (Element of IT) holds ((z9 <= x & z9 <= y) implies z9 <= z))
proof
let z9 being (Element of IT);
assume that
A6: z9 <= x
and
A7: z9 <= y;
z9 <= z
proof
reconsider z2 = z9,z3 = z,z4 = y,z5 = x as (Element of ( product ( X --> Y ) ));
reconsider J = z2,K = z3 as (Function of X,(the carrier of Y)) by CKB53:1;
z9 in (the carrier of ( product ( X --> Y ) ));
then A8: z2 in ( product ( Carrier ( X --> Y ) ) ) by CKB46:def 1;
then consider f1,g1 being Function such that A9: (f1 = z2 & g1 = z5) and A10: (for i being set holds (i in X implies (ex R being RelStr st (ex xi,yi being (Element of R) st (((R = ( ( X --> Y ) . i ) & xi = ( f1 . i )) & yi = ( g1 . i )) & xi <= yi))))) by A6,CKB46:def 1;
consider f2,g2 being Function such that A11: (f2 = z2 & g2 = z4) and A12: (for i being set holds (i in X implies (ex R being RelStr st (ex xi,yi being (Element of R) st (((R = ( ( X --> Y ) . i ) & xi = ( f2 . i )) & yi = ( g2 . i )) & xi <= yi))))) by A7,A8,CKB46:def 1;
(ex f,g being Function st ((f = z2 & g = z3) & (for i being set holds (i in X implies (ex R being RelStr st (ex xi,yi being (Element of R) st (((R = ( ( X --> Y ) . i ) & xi = ( f . i )) & yi = ( g . i )) & xi <= yi)))))))
proof
take J;
take K;
thus (J = z2 & K = z3);
let i being set;
assume i in X;
then reconsider i as (Element of X);
reconsider R = ( ( X --> Y ) . i ) as RelStr;
reconsider xi = ( J . i ),yi = ( K . i ) as (Element of R) by FUNCOP_1:7;
take R;
take xi;
take yi;
A13: (R = Y & (ex xa,ya being (Element of Y) st ((xa = ( x9 . i ) & ya = ( y9 . i )) & ( f . i ) = ( xa "/\" ya )))) by A2,FUNCOP_1:7;
((ex R1 being RelStr st (ex xi1,yi1 being (Element of R1) st (((R1 = ( ( X --> Y ) . i ) & xi1 = ( f1 . i )) & yi1 = ( g1 . i )) & xi1 <= yi1))) & (ex R2 being RelStr st (ex xi2,yi2 being (Element of R2) st (((R2 = ( ( X --> Y ) . i ) & xi2 = ( f2 . i )) & yi2 = ( g2 . i )) & xi2 <= yi2)))) by A10,A12;
hence thesis by A9,A11,A13,YELLOW_0:23;
end;
hence thesis by A8,CKB46:def 1;
end;
hence thesis;
end;
z <= x
proof
reconsider x1 = x,z1 = z as (Element of ( product ( X --> Y ) ));
A14: (ex f,g being Function st ((f = z1 & g = x1) & (for i being set holds (i in X implies (ex R being RelStr st (ex xi,yi being (Element of R) st (((R = ( ( X --> Y ) . i ) & xi = ( f . i )) & yi = ( g . i )) & xi <= yi)))))))
proof
take f;
take x9;
thus (f = z1 & x9 = x1);
let i being set;
assume i in X;
then reconsider i as (Element of X);
reconsider R = ( ( X --> Y ) . i ) as RelStr;
reconsider xi = ( f . i ),yi = ( x9 . i ) as (Element of R) by FUNCOP_1:7;
take R;
take xi;
take yi;
(R = Y & (ex xa,ya being (Element of Y) st ((xa = ( x9 . i ) & ya = ( y9 . i )) & ( f . i ) = ( xa "/\" ya )))) by A2,FUNCOP_1:7;
hence thesis by YELLOW_0:23;
end;
z1 in (the carrier of ( product ( X --> Y ) ));
then z1 in ( product ( Carrier ( X --> Y ) ) ) by CKB46:def 1;
hence thesis by A14,CKB46:def 1;
end;
hence thesis by A3,A5;
end;
end;
