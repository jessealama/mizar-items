theorems TARSKI,LATTICES,LATTICE3,YELLOW_0,STRUCT_0,RELAT_1,ORDERS_2,RELAT_2,RELSET_1,ZFMISC_1,WELLORD2,SETFAM_1,PRE_TOPC,TOPS_2,FUNCT_1,FUNCOP_1,PBOOLE,CARD_3,FUNCT_2,PRALG_1,ORDERS_3,XBOOLE_0,XBOOLE_1,PARTFUN1,ORDERS_1,CKB6,CKB9,CKB10,CKB13,CKB14;
schemes RELSET_1,FUNCT_2;
registrations XBOOLE_0,SUBSET_1,RELSET_1,PARTFUN1,FUNCOP_1,STRUCT_0,LATTICES,PRE_TOPC,ORDERS_2,LATTICE3,YELLOW_0,PRE_CIRC,CARD_1,ORDINAL1,CKB1,CKB2,CKB3,CKB4,CKB7,CKB8,CKB11,CKB12;
constructors SETFAM_1,WELLORD2,TOLER_1,TOPS_2,LATTICE3,YELLOW_0,PRALG_1,ORDERS_3,CARD_3,RELSET_1,CKB5,CKB6,CKB10;
requirements SUBSET,BOOLE,NUMERALS;
begin
reserve X for set;
theorem
Th4: ( BoolePoset X ) = ( InclPoset ( bool X ) )
proof
set B = ( BoolePoset X );
(the carrier of B) = ( bool X ) by LATTICE3:def 1;
then reconsider In = (the InternalRel of B) as (Relation of ( bool X ));
(for x being set holds (x in ( bool X ) implies (ex y being set st [ x,y ] in In)))
proof
let x being set;
assume x in ( bool X );
then reconsider x9 = x as (Element of B) by LATTICE3:def 1;
take y = x9;
x9 <= y;
hence thesis by ORDERS_2:def 5;
end;
then A1: ( dom In ) = ( bool X ) by RELSET_1:9;
A2:now
let Y being set;
let Z being set;
assume (Y in ( bool X ) & Z in ( bool X ));
then reconsider Y9 = Y,Z9 = Z as (Element of B) by LATTICE3:def 1;
thus ([ Y,Z ] in (the InternalRel of B) implies Y c= Z)
proof
assume [ Y,Z ] in (the InternalRel of B);
then Y9 <= Z9 by ORDERS_2:def 5;
hence thesis by CKB13:1;
end;

thus (Y c= Z implies [ Y,Z ] in (the InternalRel of B))
proof
assume Y c= Z;
then Y9 <= Z9 by CKB13:1;
hence thesis by ORDERS_2:def 5;
end;

end;
(for y being set holds (y in ( bool X ) implies (ex x being set st [ x,y ] in In)))
proof
let y being set;
assume y in ( bool X );
then reconsider y9 = y as (Element of B) by LATTICE3:def 1;
take x = y9;
x <= y9;
hence thesis by ORDERS_2:def 5;
end;
then ( field (the InternalRel of B) ) = ( ( bool X ) \/ ( bool X ) ) by A1,RELSET_1:10;
then (the InternalRel of B) = ( RelIncl ( bool X ) ) by A2,WELLORD2:def 1;
hence thesis by LATTICE3:def 1;
end;
