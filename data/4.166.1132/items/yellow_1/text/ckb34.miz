registrations XBOOLE_0,SUBSET_1,RELSET_1,PARTFUN1,FUNCOP_1,STRUCT_0,LATTICES,PRE_TOPC,ORDERS_2,LATTICE3,YELLOW_0,PRE_CIRC,CARD_1,ORDINAL1,CKB1,CKB2,CKB3,CKB4,CKB7,CKB8,CKB11,CKB12;
constructors SETFAM_1,WELLORD2,TOLER_1,TOPS_2,LATTICE3,YELLOW_0,PRALG_1,ORDERS_3,CARD_3,RELSET_1,CKB5,CKB6,CKB10;
requirements SUBSET,BOOLE,NUMERALS;
begin
reserve X for set;
theorem
(for T being non  empty TopSpace holds (for X being (Subset of ( InclPoset (the topology of T) )) holds ( sup X ) = ( union X )))
proof
let T being non  empty TopSpace;
set L = ( InclPoset (the topology of T) );
let X being (Subset of L);
reconsider X as (Subset-Family of T) by XBOOLE_1:1;
reconsider Un = ( union X ) as (Element of L) by PRE_TOPC:def 1;
A1:now
let b being (Element of L);
assume A2: b is_>=_than X;
now
let Z being set;
assume A3: Z in X;
then reconsider Z9 = Z as (Element of L);
Z9 <= b by A2,A3,LATTICE3:def 9;
hence Z c= b by CKB14:1;
end;
then Un c= b by ZFMISC_1:76;
hence Un <= b by CKB14:1;
end;
now
let b being (Element of L);
assume b in X;
then b c= Un by ZFMISC_1:74;
hence b <= Un by CKB14:1;
end;
then Un is_>=_than X by LATTICE3:def 9;
hence thesis by A1,YELLOW_0:30;
end;
