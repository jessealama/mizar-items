registrations XBOOLE_0,SUBSET_1,RELSET_1,PARTFUN1,FUNCOP_1,STRUCT_0,LATTICES,PRE_TOPC,ORDERS_2,LATTICE3,YELLOW_0,PRE_CIRC,CARD_1,ORDINAL1,CKB1,CKB2,CKB3,CKB4,CKB7,CKB8,CKB11,CKB12;
constructors SETFAM_1,WELLORD2,TOLER_1,TOPS_2,LATTICE3,YELLOW_0,PRALG_1,ORDERS_3,CARD_3,RELSET_1,CKB5,CKB6,CKB10;
requirements SUBSET,BOOLE,NUMERALS;
begin
reserve X for set;
theorem
(for X being non  empty set holds ((for x,y being set holds ((x in X & y in X) implies ( x \/ y ) in X)) implies ( InclPoset X ) is  with_suprema))
proof
let X being non  empty set;
set L = ( InclPoset X );
assume A1: (for x,y being set holds ((x in X & y in X) implies ( x \/ y ) in X));
now
let a being (Element of L);
let b being (Element of L);
(ex c being (Element of L) st ({ a,b } is_<=_than c & (for d being (Element of L) holds ({ a,b } is_<=_than d implies c <= d))))
proof
take c = ( a "\/" b );
A2: ( a \/ b ) = c by A1,CKB19:1;
then b c= c by XBOOLE_1:7;
then A3: b <= c by CKB14:1;
a c= c by A2,XBOOLE_1:7;
then a <= c by CKB14:1;
hence { a,b } is_<=_than c by A3,YELLOW_0:8;
let d being (Element of L);
assume A4: { a,b } is_<=_than d;
b in { a,b } by TARSKI:def 2;
then b <= d by A4,LATTICE3:def 9;
then A5: b c= d by CKB14:1;
a in { a,b } by TARSKI:def 2;
then a <= d by A4,LATTICE3:def 9;
then a c= d by CKB14:1;
then ( a \/ b ) c= d by A5,XBOOLE_1:8;
then c c= d by A1,CKB19:1;
hence thesis by CKB14:1;
end;
hence  ex_sup_of { a,b },L by YELLOW_0:15;
end;
hence thesis by YELLOW_0:20;
end;
