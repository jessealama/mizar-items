environ
vocabularies LATTICES,LATTICE3,SUBSET_1,STRUCT_0,EQREL_1,XBOOLE_0,PBOOLE,XXREAL_0,REWRITE1,WELLORD2,ORDERS_1,RELAT_1,ZFMISC_1,RELAT_2,ORDERS_2,TARSKI,SETFAM_1,CAT_1,YELLOW_0,WELLORD1,ORDINAL2,PRE_TOPC,RCOMP_1,PRALG_1,FUNCT_1,FUNCOP_1,CARD_3,RLVECT_2,NEWTON,FUNCT_2,SEQM_3,ORDERS_3,YELLOW_1,CARD_1;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,RELAT_1,RELAT_2,RELSET_1,SETFAM_1,FUNCT_1,ORDERS_1,CARD_3,PBOOLE,WELLORD2,TOLER_1,PARTFUN1,FUNCT_2,STRUCT_0,ORDERS_2,TOPS_2,LATTICES,ORDERS_3,LATTICE3,PRE_TOPC,FUNCOP_1,PRALG_1,YELLOW_0,CKB5,CKB6,CKB10,CKB42,CKB45,CKB46,CKB48;
definitions LATTICE3,XBOOLE_0,TOPS_2,TARSKI,RELAT_1,PRALG_1,CKB6,CKB10,CKB42,CKB46,CKB48;
theorems TARSKI,LATTICES,LATTICE3,YELLOW_0,STRUCT_0,RELAT_1,ORDERS_2,RELAT_2,RELSET_1,ZFMISC_1,WELLORD2,SETFAM_1,PRE_TOPC,TOPS_2,FUNCT_1,FUNCOP_1,PBOOLE,CARD_3,FUNCT_2,PRALG_1,ORDERS_3,XBOOLE_0,XBOOLE_1,PARTFUN1,ORDERS_1,CKB6,CKB9,CKB10,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB37,CKB38,CKB39,CKB41,CKB42,CKB46,CKB48,CKB49,CKB50,CKB51,CKB53;
schemes RELSET_1,FUNCT_2;
registrations XBOOLE_0,SUBSET_1,RELSET_1,PARTFUN1,FUNCOP_1,STRUCT_0,LATTICES,PRE_TOPC,ORDERS_2,LATTICE3,YELLOW_0,PRE_CIRC,CARD_1,ORDINAL1,CKB1,CKB2,CKB3,CKB4,CKB7,CKB8,CKB11,CKB12,CKB40,CKB43,CKB44,CKB47,CKB52,CKB54,CKB55,CKB56,CKB57,CKB58,CKB59;
constructors SETFAM_1,WELLORD2,TOLER_1,TOPS_2,LATTICE3,YELLOW_0,PRALG_1,ORDERS_3,CARD_3,RELSET_1,CKB5,CKB6,CKB10,CKB42,CKB45,CKB46,CKB48;
requirements SUBSET,BOOLE,NUMERALS;
begin
registration
let X being non empty set;
let Y being non empty  with_suprema  antisymmetric RelStr;
cluster ( Y |^ X ) ->  with_suprema;
coherence
proof
set IT = ( Y |^ X );
let x being (Element of IT),y being (Element of IT);
reconsider y9 = y as (Function of X,(the carrier of Y)) by CKB53:1;
reconsider x9 = x as (Function of X,(the carrier of Y)) by CKB53:1;
defpred P[ set,set ]
 means
(ex xa,ya being (Element of Y) st ((xa = ( x9 . $1 ) & ya = ( y9 . $1 )) & $2 = ( xa "\/" ya )));
A1: (for x being set holds (x in X implies (ex y being set st (y in (the carrier of Y) & P[ x,y ]))))
proof
let a being set;
assume a in X;
then reconsider xa = ( x9 . a ),ya = ( y9 . a ) as (Element of Y) by FUNCT_2:5;
take y = ( xa "\/" ya );
thus y in (the carrier of Y);
take xa;
take ya;
thus thesis;
end;
consider f being (Function of X,(the carrier of Y)) such that A2: (for a being set holds (a in X implies P[ a,( f . a ) ])) from FUNCT_2:sch 1(A1);
f in ( Funcs (X,(the carrier of Y)) ) by FUNCT_2:8;
then reconsider z = f as (Element of IT) by CKB51:1;
take z;
A3: y <= z
proof
reconsider y1 = y,z1 = z as (Element of ( product ( X --> Y ) ));
A4: (ex f,g being Function st ((f = y1 & g = z1) & (for i being set holds (i in X implies (ex R being RelStr st (ex xi,yi being (Element of R) st (((R = ( ( X --> Y ) . i ) & xi = ( f . i )) & yi = ( g . i )) & xi <= yi)))))))
proof
take y9;
take f;
thus (y9 = y1 & f = z1);
let i being set;
assume i in X;
then reconsider i as (Element of X);
reconsider R = ( ( X --> Y ) . i ) as RelStr;
reconsider yi = ( f . i ),xi = ( y9 . i ) as (Element of R) by FUNCOP_1:7;
take R;
take xi;
take yi;
(R = Y & (ex xa,ya being (Element of Y) st ((xa = ( x9 . i ) & ya = ( y9 . i )) & ( f . i ) = ( xa "\/" ya )))) by A2,FUNCOP_1:7;
hence thesis by YELLOW_0:22;
end;
y1 in (the carrier of ( product ( X --> Y ) ));
then y1 in ( product ( Carrier ( X --> Y ) ) ) by CKB46:def 1;
hence thesis by A4,CKB46:def 1;
end;
A5: (for z9 being (Element of IT) holds ((z9 >= x & z9 >= y) implies z9 >= z))
proof
let z9 being (Element of IT);
assume that
A6: z9 >= x
and
A7: z9 >= y;
z9 >= z
proof
reconsider z2 = z9,z3 = z,z4 = y,z5 = x as (Element of ( product ( X --> Y ) ));
z4 in (the carrier of ( product ( X --> Y ) ));
then z4 in ( product ( Carrier ( X --> Y ) ) ) by CKB46:def 1;
then consider f2 being Function,g2 being Function such that A8: (f2 = z4 & g2 = z2) and A9: (for i being set holds (i in X implies (ex R being RelStr st (ex xi,yi being (Element of R) st (((R = ( ( X --> Y ) . i ) & xi = ( f2 . i )) & yi = ( g2 . i )) & xi <= yi))))) by A7,CKB46:def 1;
reconsider K = z3,J = z2 as (Function of X,(the carrier of Y)) by CKB53:1;
z5 in (the carrier of ( product ( X --> Y ) ));
then z5 in ( product ( Carrier ( X --> Y ) ) ) by CKB46:def 1;
then consider f1 being Function,g1 being Function such that A10: (f1 = z5 & g1 = z2) and A11: (for i being set holds (i in X implies (ex R being RelStr st (ex xi,yi being (Element of R) st (((R = ( ( X --> Y ) . i ) & xi = ( f1 . i )) & yi = ( g1 . i )) & xi <= yi))))) by A6,CKB46:def 1;
A12: (ex f,g being Function st ((f = z3 & g = z2) & (for i being set holds (i in X implies (ex R being RelStr st (ex xi,yi being (Element of R) st (((R = ( ( X --> Y ) . i ) & xi = ( f . i )) & yi = ( g . i )) & xi <= yi)))))))
proof
take K;
take J;
thus (K = z3 & J = z2);
let i being set;
assume i in X;
then reconsider i as (Element of X);
reconsider R = ( ( X --> Y ) . i ) as RelStr;
reconsider yi = ( J . i ),xi = ( K . i ) as (Element of R) by FUNCOP_1:7;
take R;
take xi;
take yi;
A13: (R = Y & (ex xa,ya being (Element of Y) st ((xa = ( x9 . i ) & ya = ( y9 . i )) & ( f . i ) = ( xa "\/" ya )))) by A2,FUNCOP_1:7;
((ex R1 being RelStr st (ex xi1,yi1 being (Element of R1) st (((R1 = ( ( X --> Y ) . i ) & xi1 = ( f1 . i )) & yi1 = ( g1 . i )) & xi1 <= yi1))) & (ex R2 being RelStr st (ex xi2,yi2 being (Element of R2) st (((R2 = ( ( X --> Y ) . i ) & xi2 = ( f2 . i )) & yi2 = ( g2 . i )) & xi2 <= yi2)))) by A11,A9;
hence thesis by A10,A8,A13,YELLOW_0:22;
end;
z3 in (the carrier of ( product ( X --> Y ) ));
then z3 in ( product ( Carrier ( X --> Y ) ) ) by CKB46:def 1;
hence thesis by A12,CKB46:def 1;
end;
hence thesis;
end;
x <= z
proof
reconsider x1 = x,z1 = z as (Element of ( product ( X --> Y ) ));
A14: (ex f,g being Function st ((f = x1 & g = z1) & (for i being set holds (i in X implies (ex R being RelStr st (ex xi,yi being (Element of R) st (((R = ( ( X --> Y ) . i ) & xi = ( f . i )) & yi = ( g . i )) & xi <= yi)))))))
proof
take x9;
take f;
thus (x9 = x1 & f = z1);
let i being set;
assume i in X;
then reconsider i as (Element of X);
reconsider R = ( ( X --> Y ) . i ) as RelStr;
reconsider yi = ( f . i ),xi = ( x9 . i ) as (Element of R) by FUNCOP_1:7;
take R;
take xi;
take yi;
(R = Y & (ex xa,ya being (Element of Y) st ((xa = ( x9 . i ) & ya = ( y9 . i )) & ( f . i ) = ( xa "\/" ya )))) by A2,FUNCOP_1:7;
hence thesis by YELLOW_0:22;
end;
x1 in (the carrier of ( product ( X --> Y ) ));
then x1 in ( product ( Carrier ( X --> Y ) ) ) by CKB46:def 1;
hence thesis by A14,CKB46:def 1;
end;
hence thesis by A3,A5;
end;
end;
