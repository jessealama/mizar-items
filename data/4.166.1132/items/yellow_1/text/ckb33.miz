requirements SUBSET,BOOLE,NUMERALS;
begin
reserve X for set;
theorem
(for Y being (Subset of ( BoolePoset X )) holds ( sup Y ) = ( union Y ))
proof
set L = ( BoolePoset X );
let Y being (Subset of L);
A1: (the carrier of L) = ( bool X ) by LATTICE3:def 1;
then ( union Y ) c= ( union ( bool X ) ) by ZFMISC_1:77;
then reconsider Un = ( union Y ) as (Element of L) by A1,ZFMISC_1:81;
A2:now
let b being (Element of L);
assume A3: b is_>=_than Y;
now
let Z being set;
assume A4: Z in Y;
then reconsider Z9 = Z as (Element of L);
Z9 <= b by A3,A4,LATTICE3:def 9;
hence Z c= b by CKB13:1;
end;
then Un c= b by ZFMISC_1:76;
hence Un <= b by CKB13:1;
end;
now
let b being (Element of L);
assume b in Y;
then b c= Un by ZFMISC_1:74;
hence b <= Un by CKB13:1;
end;
then Un is_>=_than Y by LATTICE3:def 9;
hence thesis by A2,YELLOW_0:30;
end;
