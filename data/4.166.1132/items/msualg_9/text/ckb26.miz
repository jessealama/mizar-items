requirements SUBSET,BOOLE;
begin
reserve S for non  empty non  void ManySortedSign;
theorem
Th20: (for A,B,C being  non-empty MSAlgebra over S holds (for F1 being (ManySortedFunction of A,B) holds (for F2 being (ManySortedFunction of A,C) holds ((F1 is_epimorphism A,B & F2 is_homomorphism A,C) implies (for G being (ManySortedFunction of B,C) holds (( G ** F1 ) = F2 implies G is_homomorphism B,C))))))
proof
let A being  non-empty MSAlgebra over S;
let B being  non-empty MSAlgebra over S;
let C being  non-empty MSAlgebra over S;
let F1 being (ManySortedFunction of A,B);
let F2 being (ManySortedFunction of A,C);
assume that
A1: F1 is_epimorphism A,B
and
A2: F2 is_homomorphism A,C;
let G being (ManySortedFunction of B,C);
assume that
A3: ( G ** F1 ) = F2;
let o being (OperSymbol of S);
assume that
( Args (o,B) ) <> ( {} );
let x being (Element of ( Args (o,B) ));
F1 is  "onto" by A1,MSUALG_3:def 8;
then consider y being (Element of ( Args (o,A) )) such that A4: ( F1 # y ) = x by CKB24:1;
set r = ( the_result_sort_of o );
F1 is_homomorphism A,B by A1,MSUALG_3:def 8;
then A5: ( ( F1 . r ) . ( ( Den (o,A) ) . y ) ) = ( ( Den (o,B) ) . x ) by A4,MSUALG_3:def 7;
A6: ( ( F2 . r ) . ( ( Den (o,A) ) . y ) ) = ( ( ( G . r ) * ( F1 . r ) ) . ( ( Den (o,A) ) . y ) ) by A3,MSUALG_3:2
.= ( ( G . r ) . ( ( Den (o,B) ) . x ) ) by A5,CKB25:1,FUNCT_2:15;
( ( F2 . r ) . ( ( Den (o,A) ) . y ) ) = ( ( Den (o,C) ) . ( ( G ** F1 ) # y ) ) by A2,A3,MSUALG_3:def 7
.= ( ( Den (o,C) ) . ( G # x ) ) by A4,MSUALG_3:8;
hence ( ( G . ( the_result_sort_of o ) ) . ( ( Den (o,B) ) . x ) ) = ( ( Den (o,C) ) . ( G # x ) ) by A6;
end;
