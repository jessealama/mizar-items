theorem
Th26: (for A being  non-empty MSAlgebra over S holds (for F being (ManySortedFunction of A,( Trivial_Algebra S )) holds F is_epimorphism A,( Trivial_Algebra S )))
proof
let A being  non-empty MSAlgebra over S;
let F being (ManySortedFunction of A,( Trivial_Algebra S ));
set I = (the carrier of S);
consider XX being (ManySortedSet of I) such that A1: { XX } = ( I --> { ( 0 ) } ) by CKB12:1;
thus F is_homomorphism A,( Trivial_Algebra S )
proof
let o being (OperSymbol of S);
assume that
( Args (o,A) ) <> ( {} );
let x being (Element of ( Args (o,A) ));
thus ( ( F . ( the_result_sort_of o ) ) . ( ( Den (o,A) ) . x ) ) = ( 0 ) by CKB35:1
.= ( ( Den (o,( Trivial_Algebra S )) ) . ( F # x ) ) by CKB35:1;
end;

(the Sorts of ( Trivial_Algebra S )) = { XX } by A1,MSAFREE2:def 12;
hence thesis by CKB16:1;
end;
