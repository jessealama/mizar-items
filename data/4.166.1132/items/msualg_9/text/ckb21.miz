begin
reserve S for non empty non void ManySortedSign;
theorem
(for I being non empty set holds (for s being (Element of S) holds (for A being (MSAlgebra-Family of I,S) holds (for f,g being (Element of ( product ( Carrier (A,s) ) )) holds ((for a being (Element of I) holds ( ( proj (( Carrier (A,s) ),a) ) . f ) = ( ( proj (( Carrier (A,s) ),a) ) . g )) implies f = g)))))
proof
let I being non empty set;
let s being (Element of S);
let A being (MSAlgebra-Family of I,S);
let f being (Element of ( product ( Carrier (A,s) ) )),g being (Element of ( product ( Carrier (A,s) ) ));
assume that
A1: (for a being (Element of I) holds ( ( proj (( Carrier (A,s) ),a) ) . f ) = ( ( proj (( Carrier (A,s) ),a) ) . g ));
now
( dom f ) = ( dom ( Carrier (A,s) ) ) by CARD_3:9;
hence ( dom f ) = ( dom g ) by CARD_3:9;
A2: ( dom f ) = I by PARTFUN1:def 2;
let x being set;
assume that
A3: x in ( dom f );
A4: ( dom ( proj (( Carrier (A,s) ),x) ) ) = ( product ( Carrier (A,s) ) ) by CARD_3:def 16;
hence ( f . x ) = ( ( proj (( Carrier (A,s) ),x) ) . f ) by CARD_3:def 16
.= ( ( proj (( Carrier (A,s) ),x) ) . g ) by A1,A3,A2
.= ( g . x ) by A4,CARD_3:def 16;
end;
hence thesis by FUNCT_1:2;
end;
