begin
reserve x for set;
reserve y for set;
reserve C for Category;
reserve a for (Object of C);
reserve b for (Object of C);
reserve k for (Morphism of C);
theorem
a is_a_coproduct_wrt ( y .--> ( id a ) )
proof
set F = ( y .--> ( id a ) );
( cod ( id a ) ) = a by CAT_1:19;
hence F is (Injections_family of a,{ y }) by CKB82:1;
let b;
let F9 being (Injections_family of b,{ y });
assume that
A1: ( doms F ) = ( doms F9 );
take h = ( F9 /. y );
A2: y in { y } by TARSKI:def 1;
then A3: ( cod h ) = b by CKB80:1;
( dom h ) = ( ( doms F ) /. y ) by A1,A2,CKB7:def 1
.= ( dom ( F /. y ) ) by A2,CKB7:def 1
.= ( dom ( id a ) ) by A2,CKB5:1
.= a by CAT_1:19;
hence h in ( Hom (a,b) ) by A3;
let k;
assume k in ( Hom (a,b) );
then A4: ( dom k ) = a by CAT_1:1;
thus ((for x holds (x in { y } implies ( k * ( F /. x ) ) = ( F9 /. x ))) implies h = k)
proof
assume A5: (for x holds (x in { y } implies ( k * ( F /. x ) ) = ( F9 /. x )));
thus k = ( k * ( id a ) ) by A4,CAT_1:22
.= ( k * ( F /. y ) ) by A2,CKB5:1
.= h by A2,A5;
end;

assume A6: h = k;
let x;
assume A7: x in { y };
hence ( F9 /. x ) = k by A6,TARSKI:def 1
.= ( k * ( id a ) ) by A4,CAT_1:22
.= ( k * ( F /. x ) ) by A7,CKB5:1;
end;
