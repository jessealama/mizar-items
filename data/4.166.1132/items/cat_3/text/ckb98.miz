reserve p2 for (Morphism of C);
theorem
(c is_a_product_wrt p1,p2 iff ( c opp ) is_a_coproduct_wrt ( p1 opp ),( p2 opp ))
proof
set i1 = ( p1 opp );
set i2 = ( p2 opp );
thus (c is_a_product_wrt p1,p2 implies ( c opp ) is_a_coproduct_wrt ( p1 opp ),( p2 opp ))
proof
assume that
A1: (( dom p1 ) = c & ( dom p2 ) = c)
and
A2: (for d holds (for f holds (for g holds ((f in ( Hom (d,( cod p1 )) ) & g in ( Hom (d,( cod p2 )) )) implies (ex h st (h in ( Hom (d,c) ) & (for k holds (k in ( Hom (d,c) ) implies ((( p1 * k ) = f & ( p2 * k ) = g) iff h = k)))))))));
(( dom p1 ) = ( c opp ) & ( dom p2 ) = ( c opp )) by A1,OPPCAT_1:def 2;
hence A3: (( cod i1 ) = ( c opp ) & ( cod i2 ) = ( c opp )) by OPPCAT_1:8;
let d being (Object of ( C opp ));
let f being (Morphism of ( C opp )),g being (Morphism of ( C opp ));
assume that
A4: f in ( Hom (( dom i1 ),d) )
and
A5: g in ( Hom (( dom i2 ),d) );
( opp g ) in ( Hom (( opp d ),( opp ( dom i2 ) )) ) by A5,OPPCAT_1:13;
then ( opp g ) in ( Hom (( opp d ),( cod ( opp i2 ) )) ) by OPPCAT_1:11;
then A6: ( opp g ) in ( Hom (( opp d ),( cod p2 )) ) by OPPCAT_1:6;
( opp f ) in ( Hom (( opp d ),( opp ( dom i1 ) )) ) by A4,OPPCAT_1:13;
then ( opp f ) in ( Hom (( opp d ),( cod ( opp i1 ) )) ) by OPPCAT_1:11;
then ( opp f ) in ( Hom (( opp d ),( cod p1 )) ) by OPPCAT_1:6;
then consider h such that A7: h in ( Hom (( opp d ),c) ) and A8: (for k holds (k in ( Hom (( opp d ),c) ) implies ((( p1 * k ) = ( opp f ) & ( p2 * k ) = ( opp g )) iff h = k))) by A2,A6;
take ( h opp );
( h opp ) in ( Hom (( c opp ),( ( opp d ) opp )) ) by A7,OPPCAT_1:12;
hence ( h opp ) in ( Hom (( c opp ),d) ) by OPPCAT_1:4;
let k being (Morphism of ( C opp ));
assume A9: k in ( Hom (( c opp ),d) );
then ( opp k ) in ( Hom (( opp d ),( opp ( c opp ) )) ) by OPPCAT_1:13;
then ( opp k ) in ( Hom (( opp d ),c) ) by OPPCAT_1:3;
then ((( p1 * ( opp k ) ) = ( opp f ) & ( p2 * ( opp k ) ) = ( opp g )) iff h = ( opp k )) by A8;
then ((( p1 * ( opp k ) ) = ( f opp ) & ( p2 * ( opp k ) ) = ( g opp )) iff h = ( k opp )) by OPPCAT_1:def 5;
then ((( p1 * ( opp k ) ) = f & ( p2 * ( opp k ) ) = g) iff h = k) by OPPCAT_1:def 4;
then A10: ((( ( opp i1 ) * ( opp k ) ) = f & ( ( opp i2 ) * ( opp k ) ) = g) iff ( h opp ) = k) by OPPCAT_1:6,OPPCAT_1:def 4;
( dom k ) = ( c opp ) by A9,CAT_1:1;
then ((( opp ( k * i1 ) ) = f & ( opp ( k * i2 ) ) = g) iff ( h opp ) = k) by A3,A10,OPPCAT_1:18;
then ((( ( k * i1 ) opp ) = f & ( ( k * i2 ) opp ) = g) iff ( h opp ) = k) by OPPCAT_1:def 5;
hence thesis by OPPCAT_1:def 4;
end;

assume that
A11: (( cod i1 ) = ( c opp ) & ( cod i2 ) = ( c opp ))
and
A12: (for d being (Object of ( C opp )) holds (for f,g being (Morphism of ( C opp )) holds ((f in ( Hom (( dom i1 ),d) ) & g in ( Hom (( dom i2 ),d) )) implies (ex h being (Morphism of ( C opp )) st (h in ( Hom (( c opp ),d) ) & (for k being (Morphism of ( C opp )) holds (k in ( Hom (( c opp ),d) ) implies ((( k * i1 ) = f & ( k * i2 ) = g) iff h = k))))))));
A13: (( dom p1 ) = ( c opp ) & ( dom p2 ) = ( c opp )) by A11,OPPCAT_1:8;
hence (( dom p1 ) = c & ( dom p2 ) = c) by OPPCAT_1:def 2;
let d;
let f;
let g;
assume that
A14: f in ( Hom (d,( cod p1 )) )
and
A15: g in ( Hom (d,( cod p2 )) );
( g opp ) in ( Hom (( ( cod p2 ) opp ),( d opp )) ) by A15,OPPCAT_1:12;
then A16: ( g opp ) in ( Hom (( dom ( p2 opp ) ),( d opp )) ) by OPPCAT_1:10;
( f opp ) in ( Hom (( ( cod p1 ) opp ),( d opp )) ) by A14,OPPCAT_1:12;
then ( f opp ) in ( Hom (( dom ( p1 opp ) ),( d opp )) ) by OPPCAT_1:10;
then consider h being (Morphism of ( C opp )) such that A17: h in ( Hom (( c opp ),( d opp )) ) and A18: (for k being (Morphism of ( C opp )) holds (k in ( Hom (( c opp ),( d opp )) ) implies ((( k * i1 ) = ( f opp ) & ( k * i2 ) = ( g opp )) iff h = k))) by A12,A16;
take ( opp h );
h = ( ( opp h ) opp ) by OPPCAT_1:7;
hence ( opp h ) in ( Hom (d,c) ) by A17,OPPCAT_1:12;
let k;
assume A19: k in ( Hom (d,c) );
then ( k opp ) in ( Hom (( c opp ),( d opp )) ) by OPPCAT_1:12;
then ((( ( k opp ) * i1 ) = ( f opp ) & ( ( k opp ) * i2 ) = ( g opp )) iff h = ( k opp )) by A18;
then A20: ((( ( k opp ) * i1 ) = f & ( ( k opp ) * i2 ) = g) iff ( h opp ) = ( k opp )) by OPPCAT_1:def 4;
( cod k ) = c by A19,CAT_1:1;
then ((( ( p1 * k ) opp ) = f & ( ( p2 * k ) opp ) = g) iff ( h opp ) = k) by A13,A20,OPPCAT_1:16,OPPCAT_1:def 2,OPPCAT_1:def 4;
hence thesis by OPPCAT_1:def 4,OPPCAT_1:def 5;
end;
