environ
vocabularies XBOOLE_0,CAT_1,SUBSET_1,FUNCT_1,PARTFUN1,FUNCOP_1,STRUCT_0,FUNCT_6,RELAT_1,GRAPH_1,FUNCT_4,ARYTM_0,FVSUM_1,ALGSTR_0,WELLORD1,TARSKI,CARD_1,CAT_3;
notations TARSKI,XBOOLE_0,SUBSET_1,RELAT_1,FUNCT_1,PARTFUN1,FUNCOP_1,FUNCT_4,NUMBERS,FUNCT_2,STRUCT_0,GRAPH_1,CAT_1,OPPCAT_1,CKB4,CKB7,CKB8,CKB13,CKB17,CKB21,CKB22,CKB27,CKB32,CKB33,CKB47,CKB51,CKB55,CKB63,CKB70,CKB79,CKB89;
definitions CAT_1,FUNCOP_1,CKB7,CKB8,CKB13,CKB17,CKB21,CKB22,CKB27,CKB32,CKB33,CKB47,CKB51,CKB55,CKB63,CKB70,CKB79,CKB89;
theorems TARSKI,FUNCT_2,FUNCOP_1,FUNCT_4,CAT_1,OPPCAT_1,RELSET_1,CKB2,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB37,CKB38,CKB39,CKB40,CKB41,CKB42,CKB43,CKB44,CKB45,CKB46,CKB47,CKB48,CKB49,CKB50,CKB51,CKB52,CKB53,CKB54,CKB55,CKB56,CKB57,CKB58,CKB59,CKB60,CKB61,CKB62,CKB63,CKB64,CKB65,CKB66,CKB67,CKB68,CKB69,CKB70,CKB71,CKB72,CKB73,CKB74,CKB75,CKB76,CKB77,CKB78,CKB79,CKB80,CKB81,CKB82,CKB83,CKB84,CKB85,CKB86,CKB87,CKB88,CKB89,CKB90;
schemes FUNCT_2,FUNCT_1,CKB1,CKB3;
registrations XBOOLE_0,RELSET_1,STRUCT_0;
constructors PARTFUN1,OPPCAT_1,FUNCOP_1,RELSET_1,CKB4,CKB7,CKB8,CKB13,CKB17,CKB21,CKB22,CKB27,CKB32,CKB33,CKB47,CKB51,CKB55,CKB63,CKB70,CKB79,CKB89;
requirements SUBSET,BOOLE;
begin
reserve I for set;
reserve x for set;
reserve C for Category;
reserve c for (Object of C);
reserve d for (Object of C);
reserve f for (Morphism of C);
reserve g for (Morphism of C);
reserve k for (Morphism of C);
theorem
Th78: (for F being (Injections_family of c,I) holds (for F9 being (Injections_family of d,I) holds (((c is_a_coproduct_wrt F & d is_a_coproduct_wrt F9) & ( doms F ) = ( doms F9 )) implies c,d are_isomorphic )))
proof
let F being (Injections_family of c,I);
let F9 being (Injections_family of d,I);
assume that
A1: c is_a_coproduct_wrt F
and
A2: d is_a_coproduct_wrt F9
and
A3: ( doms F ) = ( doms F9 );
( doms F9 ) = ( doms F9 );
then consider fg being (Morphism of C) such that fg in ( Hom (d,d) ) and A4: (for k holds (k in ( Hom (d,d) ) implies ((for x holds (x in I implies ( k * ( F9 /. x ) ) = ( F9 /. x ))) iff fg = k))) by A2,CKB89:def 1;
consider f such that A5: f in ( Hom (c,d) ) and A6: (for k holds (k in ( Hom (c,d) ) implies ((for x holds (x in I implies ( k * ( F /. x ) ) = ( F9 /. x ))) iff f = k))) by A1,A3,CKB89:def 1;
thus ( Hom (c,d) ) <> ( {} ) by A5;
reconsider f as (Morphism of c,d) by A5,CAT_1:def 4;
A7: ( dom f ) = c by A5,CAT_1:1;
A8:now
set k = ( id c );
thus k in ( Hom (c,c) ) by CAT_1:26;
let x;
assume x in I;
then ( cod ( F /. x ) ) = c by CKB80:1;
hence ( k * ( F /. x ) ) = ( F /. x ) by CAT_1:21;
end;
A9:now
set k = ( id d );
thus k in ( Hom (d,d) ) by CAT_1:26;
let x;
assume x in I;
then ( cod ( F9 /. x ) ) = d by CKB80:1;
hence ( k * ( F9 /. x ) ) = ( F9 /. x ) by CAT_1:21;
end;
( doms F ) = ( doms F );
then consider gf being (Morphism of C) such that gf in ( Hom (c,c) ) and A10: (for k holds (k in ( Hom (c,c) ) implies ((for x holds (x in I implies ( k * ( F /. x ) ) = ( F /. x ))) iff gf = k))) by A1,CKB89:def 1;
consider g such that A11: g in ( Hom (d,c) ) and A12: (for k holds (k in ( Hom (d,c) ) implies ((for x holds (x in I implies ( k * ( F9 /. x ) ) = ( F /. x ))) iff g = k))) by A2,A3,CKB89:def 1;
take f;
take g;
A13: ( cod f ) = d by A5,CAT_1:1;
hence (( dom g ) = ( cod f ) & ( cod g ) = ( dom f )) by A11,A7,CAT_1:1;
A14: ( dom g ) = d by A11,CAT_1:1;
A15: ( cod g ) = c by A11,CAT_1:1;
now
(( cod ( f * g ) ) = d & ( dom ( f * g ) ) = d) by A13,A14,A7,A15,CAT_1:17;
hence ( f * g ) in ( Hom (d,d) );
let x;
assume A16: x in I;
then ( cod ( F9 /. x ) ) = d by CKB80:1;
hence ( ( f * g ) * ( F9 /. x ) ) = ( f * ( g * ( F9 /. x ) ) ) by A14,A7,A15,CAT_1:18
.= ( f * ( F /. x ) ) by A11,A12,A16
.= ( F9 /. x ) by A5,A6,A16;
end;
hence ( f * g ) = fg by A4
.= ( id ( cod f ) ) by A13,A4,A9;
now
(( cod ( g * f ) ) = c & ( dom ( g * f ) ) = c) by A13,A14,A7,A15,CAT_1:17;
hence ( g * f ) in ( Hom (c,c) );
let x;
assume A17: x in I;
then ( cod ( F /. x ) ) = c by CKB80:1;
hence ( ( g * f ) * ( F /. x ) ) = ( g * ( f * ( F /. x ) ) ) by A13,A14,A7,CAT_1:18
.= ( g * ( F9 /. x ) ) by A5,A6,A17
.= ( F /. x ) by A11,A12,A17;
end;
hence ( g * f ) = gf by A10
.= ( id ( dom f ) ) by A7,A8,A10;
end;
