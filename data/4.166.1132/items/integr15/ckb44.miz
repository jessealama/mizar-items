begin
theorem
(for n being (Element of ( NAT )) holds (for r being Real holds (for A being non empty  closed_interval (Subset of ( REAL )) holds (for f being (PartFunc of ( REAL ),( REAL n )) holds (((A c= ( dom f ) & f is_integrable_on A) & ( f | A ) is  bounded) implies (( r (#) f ) is_integrable_on A & ( integral (( r (#) f ),A) ) = ( r * ( integral (f,A) ) )))))))
proof
let n being (Element of ( NAT ));
let r being Real;
let A being non empty  closed_interval (Subset of ( REAL ));
let f being (PartFunc of ( REAL ),( REAL n ));
assume that
A1: A c= ( dom f )
and
A2: f is_integrable_on A
and
A3: ( f | A ) is  bounded;
A4:now
let i being (Element of ( NAT ));
( dom ( proj (i,n) ) ) = ( REAL n ) by FUNCT_2:def 1;
then ( rng f ) c= ( dom ( proj (i,n) ) );
hence A c= ( dom ( ( proj (i,n) ) * f ) ) by A1,RELAT_1:27;
end;
A5: (for i being (Element of ( NAT )) holds (i in ( Seg n ) implies ( integral (( r (#) ( ( proj (i,n) ) * f ) ),A) ) = ( r * ( integral (( ( proj (i,n) ) * f ),A) ) )))
proof
let i being (Element of ( NAT ));
assume A6: i in ( Seg n );
( ( ( proj (i,n) ) * f ) | A ) = ( ( proj (i,n) ) * ( f | A ) ) by CKB30:1;
then A7: ( ( ( proj (i,n) ) * f ) | A ) is  bounded by A3,A6,CKB35:def 1;
A8: A c= ( dom ( ( proj (i,n) ) * f ) ) by A4;
( ( proj (i,n) ) * f ) is_integrable_on A by A2,A6,CKB36:def 1;
hence thesis by A7,A8,INTEGRA6:9;
end;
A9: (for i being (Element of ( NAT )) holds (i in ( Seg n ) implies ( ( r * ( integral (f,A) ) ) . i ) = ( r * ( integral (( ( proj (i,n) ) * f ),A) ) )))
proof
let i being (Element of ( NAT ));
assume i in ( Seg n );
then ( r * ( ( integral (f,A) ) . i ) ) = ( r * ( integral (( ( proj (i,n) ) * f ),A) ) ) by CKB37:def 1;
hence thesis by RVSUM_1:45;
end;
A10: (for i being (Element of ( NAT )) holds (i in ( Seg n ) implies ( ( integral (( r (#) f ),A) ) . i ) = ( ( r * ( integral (f,A) ) ) . i )))
proof
let i being (Element of ( NAT ));
A11: ( integral (( r (#) ( ( proj (i,n) ) * f ) ),A) ) = ( integral (( ( proj (i,n) ) * ( r (#) f ) ),A) ) by CKB42:1;
assume A12: i in ( Seg n );
then (( ( integral (( r (#) f ),A) ) . i ) = ( integral (( ( proj (i,n) ) * ( r (#) f ) ),A) ) & ( ( r * ( integral (f,A) ) ) . i ) = ( r * ( integral (( ( proj (i,n) ) * f ),A) ) )) by A9,CKB37:def 1;
hence thesis by A5,A12,A11;
end;
(for i being (Element of ( NAT )) holds (i in ( Seg n ) implies ( ( proj (i,n) ) * ( r (#) f ) ) is_integrable_on A))
proof
let i being (Element of ( NAT ));
assume A13: i in ( Seg n );
( ( ( proj (i,n) ) * f ) | A ) = ( ( proj (i,n) ) * ( f | A ) ) by CKB30:1;
then A14: ( ( ( proj (i,n) ) * f ) | A ) is  bounded by A3,A13,CKB35:def 1;
A15: A c= ( dom ( ( proj (i,n) ) * f ) ) by A4;
( ( proj (i,n) ) * f ) is_integrable_on A by A2,A13,CKB36:def 1;
then ( r (#) ( ( proj (i,n) ) * f ) ) is_integrable_on A by A14,A15,INTEGRA6:9;
hence thesis by CKB42:1;
end;
hence ( r (#) f ) is_integrable_on A by CKB36:def 1;
A16: ( dom ( integral (( r (#) f ),A) ) ) = ( Seg n ) by FINSEQ_1:89;
then ( dom ( integral (( r (#) f ),A) ) ) = ( dom ( r * ( integral (f,A) ) ) ) by FINSEQ_1:89;
hence thesis by A10,A16,PARTFUN1:5;
end;
