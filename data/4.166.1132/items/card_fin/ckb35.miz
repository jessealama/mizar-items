begin
reserve x for set;
reserve x1 for set;
reserve y for set;
reserve z for set;
reserve F for Function;
reserve Ch for Function;
theorem
Th34: (x in ( Ch " { y } ) implies ( ( Intersection (F,( Ch | ( ( dom Ch ) \ { x } ) ),y) ) /\ ( F . x ) ) = ( Intersection (F,Ch,y) ))
proof
set d = ( ( dom Ch ) \ { x } );
set Chd = ( Ch | d );
set I1 = ( Intersection (F,Ch,y) );
set I2 = ( Intersection (F,Chd,y) );
assume x in ( Ch " { y } );
then A1: I1 c= ( F . x ) by CKB34:1;
A2: ( I2 /\ ( F . x ) ) c= I1
proof
let z;
assume that
A3: z in ( I2 /\ ( F . x ) );
now
let x1;
assume that
A4: x1 in ( dom Ch )
and
A5: ( Ch . x1 ) = y;
per cases  by A4,XBOOLE_0:def 5;
suppose A6: x1 in d;

A7: z in I2 by A3,XBOOLE_0:def 4;
A8: (( ( dom Ch ) /\ d ) = ( dom Chd ) & ( ( dom Ch ) /\ d ) = d) by RELAT_1:61,XBOOLE_1:28;
then ( Chd . x1 ) = y by A5,A6,FUNCT_1:47;
hence z in ( F . x1 ) by A6,A8,A7,CKB21:def 1;
end;
suppose x1 in { x };

then x1 = x by TARSKI:def 1;
hence z in ( F . x1 ) by A3,XBOOLE_0:def 4;
end;
end;
hence thesis by A3,CKB21:def 1;
end;
I1 c= I2 by CKB30:1;
then I1 c= ( I2 /\ ( F . x ) ) by A1,XBOOLE_1:19;
hence thesis by A2,XBOOLE_0:def 10;
end;
