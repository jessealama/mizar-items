begin
reserve x for set;
reserve y for set;
reserve z for set;
reserve X9 for set;
reserve F for Function;
reserve Ch for Function;
theorem
Th59: ( ( Intersection (F,Ch,y) ) /\ X9 ) = ( Intersection (( Intersect (F,( ( dom F ) --> X9 )) ),Ch,y) )
proof
set I = ( Intersect (F,( ( dom F ) --> X9 )) );
set Int1 = ( Intersection (F,Ch,y) );
set Int2 = ( Intersection (I,Ch,y) );
thus ( Int1 /\ X9 ) c= Int2
proof
let x;
assume that
A1: x in ( Int1 /\ X9 );
A2: (for z holds ((z in ( dom Ch ) & ( Ch . z ) = y) implies x in ( I . z )))
proof
A3: x in Int1 by A1,XBOOLE_0:def 4;
let z;
assume (z in ( dom Ch ) & ( Ch . z ) = y);
then A4: x in ( F . z ) by A3,CKB21:def 1;
then A5: z in ( dom F ) by FUNCT_1:def 2;
x in X9 by A1,XBOOLE_0:def 4;
then x in ( ( F . z ) /\ X9 ) by A4,XBOOLE_0:def 4;
hence thesis by A5,CKB61:1;
end;
x in X9 by A1,XBOOLE_0:def 4;
then x in ( ( union ( rng F ) ) /\ X9 ) by A1,XBOOLE_0:def 4;
then x in ( union ( rng I ) ) by CKB62:1;
hence thesis by A2,CKB21:def 1;
end;

thus Int2 c= ( Int1 /\ X9 )
proof
let x;
assume that
A6: x in Int2;
x in ( union ( rng I ) ) by A6;
then A7: x in ( ( union ( rng F ) ) /\ X9 ) by CKB62:1;
then A8: x in X9 by XBOOLE_0:def 4;
A9: (for z holds ((z in ( dom Ch ) & ( Ch . z ) = y) implies x in ( F . z )))
proof
A10: ( dom I ) = ( dom F ) by CKB61:1;
let z;
assume (z in ( dom Ch ) & ( Ch . z ) = y);
then A11: x in ( I . z ) by A6,CKB21:def 1;
then z in ( dom I ) by FUNCT_1:def 2;
then x in ( ( F . z ) /\ X9 ) by A11,A10,CKB61:1;
hence thesis by XBOOLE_0:def 4;
end;
x in ( union ( rng F ) ) by A7,XBOOLE_0:def 4;
then x in Int1 by A9,CKB21:def 1;
hence thesis by A8,XBOOLE_0:def 4;
end;

end;
