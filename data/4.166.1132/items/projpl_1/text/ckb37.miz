environ
vocabularies INCSP_1,INCPROJ,ZFMISC_1,ANALOAF,PARSP_2,SUBSET_1,MCART_1,RELAT_1,PROJPL_1;
notations ZFMISC_1,SUBSET_1,MCART_1,DOMAIN_1,INCSP_1,INCPROJ,CKB1,CKB2,CKB3,CKB4,CKB6,CKB10,CKB11,CKB12,CKB15,CKB25,CKB26,CKB35;
definitions CKB2,CKB3,CKB4,CKB6,CKB11,CKB15,CKB25,CKB26,CKB35;
theorems INCPROJ,PROJRED1,MCART_1,ZFMISC_1,INCSP_1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB11,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36;
registrations XBOOLE_0,INCPROJ;
constructors DOMAIN_1,INCPROJ,CKB2,CKB3,CKB4,CKB6,CKB11,CKB15,CKB25,CKB26,CKB35;
begin
reserve G for IncProjectivePlane;
reserve a for (POINT of G);
reserve q for (POINT of G);
reserve A for (LINE of G);
reserve B for (LINE of G);
theorem
((((A <> B & a on A) & q |' A) & a <> ( A * B )) implies (( ( q * a ) * B ) on B & ( ( q * a ) * B ) |' A))
proof
assume that
A1: A <> B
and
A2: a on A
and
A3: q |' A
and
A4: a <> ( A * B );
set D = ( q * a );
A5: (a on D & q on D) by A2,A3,CKB27:1;
set d = ( D * B );
A6: G is  configuration by CKB34:1;
A7: a |' B by A1,A2,A4,CKB36:1;
then A8: ( q * a ) <> B by A2,A3,CKB27:1;
hence ( ( q * a ) * B ) on B by CKB36:1;
assume A9: d on A;
d on D by A8,CKB36:1;
then a = d by A2,A3,A6,A9,A5,CKB6:def 1;
hence thesis by A7,A8,CKB36:1;
end;
