begin
reserve G for IncProjStr;
reserve a for (POINT of G);
reserve b for (POINT of G);
reserve c for (POINT of G);
reserve p for (POINT of G);
reserve q for (POINT of G);
reserve M for (LINE of G);
reserve P for (LINE of G);
theorem
Th17: (((ex a st (ex b st (ex c st a,b,c is_a_triangle ))) & (for p holds (for q holds (ex M st { p,q } on M)))) implies (ex p st (ex P st p |' P)))
proof
assume that
A1: (ex a st (ex b st (ex c st a,b,c is_a_triangle )))
and
A2: (for p holds (for q holds (ex M st { p,q } on M)));
consider a,b,c such that A3: a,b,c is_a_triangle  by A1;
consider P such that A4: { a,b } on P by A2;
take c;
take P;
(a on P & b on P) by A4,INCSP_1:1;
hence thesis by A3,CKB13:1;
end;
