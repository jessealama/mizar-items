environ
vocabularies INCSP_1,INCPROJ,ZFMISC_1,ANALOAF,PARSP_2,SUBSET_1,MCART_1,RELAT_1,PROJPL_1;
notations ZFMISC_1,SUBSET_1,MCART_1,DOMAIN_1,INCSP_1,INCPROJ,CKB1,CKB2,CKB3,CKB4,CKB6,CKB10,CKB11,CKB12,CKB15,CKB25,CKB26,CKB35;
definitions CKB2,CKB3,CKB4,CKB6,CKB11,CKB15,CKB25,CKB26,CKB35;
theorems INCPROJ,PROJRED1,MCART_1,ZFMISC_1,INCSP_1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB11,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB37,CKB38,CKB39,CKB40,CKB41;
registrations XBOOLE_0,INCPROJ;
constructors DOMAIN_1,INCPROJ,CKB2,CKB3,CKB4,CKB6,CKB11,CKB15,CKB25,CKB26,CKB35;
begin
theorem
(for G being IncProjectivePlane holds (for e,m,m9 being (POINT of G) holds (for I being (LINE of G) holds ((((m on I & m9 on I) & m <> m9) & e |' I) implies (( m * e ) <> ( m9 * e ) & ( e * m ) <> ( e * m9 ))))))
proof
let G being IncProjectivePlane;
let e being (POINT of G),m being (POINT of G),m9 being (POINT of G);
let I being (LINE of G);
assume that
A1: m on I
and
A2: m9 on I
and
A3: m <> m9
and
A4: e |' I;
set L1 = ( m * e );
set L2 = ( m9 * e );
A5:now
m on L1 by A1,A4,CKB27:1;
then A6: m on I,L1 by A1,CKB3:def 1;
e on L1 by A1,A4,CKB27:1;
then A7: m = ( I * L1 ) by A4,A6,CKB35:def 1;
assume A8: ( m * e ) = ( m9 * e );
m9 on L2 by A2,A4,CKB27:1;
then A9: m9 on I,L2 by A2,CKB3:def 1;
e on L2 by A2,A4,CKB27:1;
hence contradiction by A3,A4,A8,A9,A7,CKB35:def 1;
end;
now
assume A10: ( e * m ) = ( e * m9 );
( m * e ) = ( e * m ) by A1,A4,CKB27:1;
hence contradiction by A2,A4,A5,A10,CKB27:1;
end;
hence thesis by A5;
end;
