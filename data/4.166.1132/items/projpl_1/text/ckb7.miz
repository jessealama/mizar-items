environ
vocabularies INCSP_1,INCPROJ,ZFMISC_1,ANALOAF,PARSP_2,SUBSET_1,MCART_1,RELAT_1,PROJPL_1;
notations ZFMISC_1,SUBSET_1,MCART_1,DOMAIN_1,INCSP_1,INCPROJ,CKB1,CKB2,CKB3,CKB4,CKB6;
definitions CKB2,CKB3,CKB4,CKB6;
theorems INCPROJ,PROJRED1,MCART_1,ZFMISC_1,INCSP_1,CKB2,CKB3,CKB4,CKB5,CKB6;
registrations XBOOLE_0,INCPROJ;
constructors DOMAIN_1,INCPROJ,CKB2,CKB3,CKB4,CKB6;
begin
reserve G for IncProjStr;
reserve p for (POINT of G);
reserve q for (POINT of G);
reserve P for (LINE of G);
reserve Q for (LINE of G);
theorem
Th2: (G is  configuration iff (for p holds (for q holds (for P holds (for Q holds (({ p,q } on P & { p,q } on Q) implies (p = q or P = Q)))))))
proof
hereby
assume A1: G is  configuration;
let p;
let q;
let P;
let Q;
assume that
A2: { p,q } on P
and
A3: { p,q } on Q;
A4: (p on Q & q on Q) by A3,INCSP_1:1;
(p on P & q on P) by A2,INCSP_1:1;
hence (p = q or P = Q) by A1,A4,CKB6:def 1;
end;
hereby
assume A5: (for p holds (for q holds (for P holds (for Q holds (({ p,q } on P & { p,q } on Q) implies (p = q or P = Q))))));
now
let p;
let q;
let P;
let Q;
assume (((p on P & q on P) & p on Q) & q on Q);
then ({ p,q } on P & { p,q } on Q) by INCSP_1:1;
hence (p = q or P = Q) by A5;
end;
hence G is  configuration by CKB6:def 1;
end;
end;
