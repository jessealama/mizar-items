begin
theorem
(for G being IncProjSp holds (for a,b,q,q1 being (POINT of G) holds (((((q on ( a * b ) & q on ( a * q1 )) & q <> a) & q1 <> a) & a <> b) implies q1 on ( a * b ))))
proof
let G being IncProjSp;
let a being (POINT of G),b being (POINT of G),q being (POINT of G),q1 being (POINT of G);
assume that
A1: ((q on ( a * b ) & q on ( a * q1 )) & q <> a)
and
A2: q1 <> a
and
A3: a <> b;
(a on ( a * b ) & a on ( a * q1 )) by A2,A3,CKB27:1;
then ( a * b ) = ( a * q1 ) by A1,INCPROJ:def 4;
hence thesis by A2,CKB27:1;
end;
