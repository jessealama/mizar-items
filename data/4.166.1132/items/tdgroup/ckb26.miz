environ
vocabularies SUBSET_1,VECTSP_1,ARYTM_3,NUMBERS,REAL_1,SUPINF_2,XBOOLE_0,ALGSTR_0,STRUCT_0,RLVECT_1,PBOOLE,RELAT_1,ZFMISC_1,ANALOAF,CARD_1,ARYTM_1,TDGROUP;
notations TARSKI,ZFMISC_1,SUBSET_1,NUMBERS,REAL_1,STRUCT_0,ALGSTR_0,ANALOAF,RELSET_1,BINOP_2,RLVECT_1,VECTSP_1,CKB3,CKB7,CKB8,CKB10,CKB11,CKB12,CKB15;
definitions VECTSP_1,STRUCT_0,CKB3,CKB11,CKB12,CKB15;
theorems VECTSP_1,RELAT_1,ZFMISC_1,DOMAIN_1,RLVECT_1,ANALOAF,STRUCT_0,CKB1,CKB2,CKB3,CKB4,CKB9,CKB11,CKB12,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25;
schemes RELSET_1;
registrations RELSET_1,STRUCT_0,VECTSP_1,ANALOAF,REAL_1,MEMBERED,CKB5,CKB6,CKB13;
constructors BINOP_2,ANALOAF,REAL_1,DOMAIN_1,VECTSP_1,MEMBERED,CKB3,CKB11,CKB12,CKB15;
requirements NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve ADG for Uniquely_Two_Divisible_Group;
reserve a for (Element of ADG);
reserve b for (Element of ADG);
reserve c for (Element of ADG);
reserve d for (Element of ADG);
theorem
Th20: ((ex a,b being (Element of ADG) st a <> b) implies ((((((((ex a,b being (Element of ( AV ADG )) st a <> b) & (for a,b,c being (Element of ( AV ADG )) holds (a,b // c,c implies a = b))) & (for a,b,c,d,p,q being (Element of ( AV ADG )) holds ((a,b // p,q & c,d // p,q) implies a,b // c,d))) & (for a,b,c being (Element of ( AV ADG )) holds (ex d being (Element of ( AV ADG )) st a,b // c,d))) & (for a,b,c,a9,b9,c9 being (Element of ( AV ADG )) holds ((a,b // a9,b9 & a,c // a9,c9) implies b,c // b9,c9))) & (for a,c being (Element of ( AV ADG )) holds (ex b being (Element of ( AV ADG )) st a,b // b,c))) & (for a,b,c,b9 being (Element of ( AV ADG )) holds ((a,b // b,c & a,b9 // b9,c) implies b = b9))) & (for a,b,c,d being (Element of ( AV ADG )) holds (a,b // c,d implies a,c // b,d))))
proof
set A = ( AV ADG );
assume (ex a,b being (Element of ADG) st a <> b);
hence (ex a,b being (Element of A) st a <> b);
A1: A = AffinStruct (# (the carrier of ADG),( CONGRD ADG ) #);
A2: (for a9,b9,c9,d9 being (Element of A) holds (for a holds (for b holds (for c holds (for d holds ((((a = a9 & b = b9) & c = c9) & d = d9) implies (a,b ==> c,d iff a9,b9 // c9,d9)))))))
proof
let a9 being (Element of A),b9 being (Element of A),c9 being (Element of A),d9 being (Element of A);
let a;
let b;
let c;
let d;
assume that
A3: (((a = a9 & b = b9) & c = c9) & d = d9);
A4:now
assume a9,b9 // c9,d9;
then [ [ a,b ],[ c,d ] ] in ( CONGRD ADG ) by A3,ANALOAF:def 2;
hence a,b ==> c,d by A1,CKB15:def 1;
end;
now
assume a,b ==> c,d;
then [ [ a,b ],[ c,d ] ] in ( CONGRD ADG ) by A1,CKB15:def 1;
hence a9,b9 // c9,d9 by A3,ANALOAF:def 2;
end;
hence thesis by A4;
end;
thus (for a,b,c being (Element of A) holds (a,b // c,c implies a = b))
proof
let a being (Element of A),b being (Element of A),c being (Element of A);
assume that
A5: a,b // c,c;
reconsider a9 = a,b9 = b,c9 = c as (Element of ADG);
a9,b9 ==> c9,c9 by A2,A5;
hence thesis by CKB19:1;
end;

thus (for a,b,c,d,p,q being (Element of A) holds ((a,b // p,q & c,d // p,q) implies a,b // c,d))
proof
let a being (Element of A),b being (Element of A),c being (Element of A),d being (Element of A),p being (Element of A),q being (Element of A);
reconsider a9 = a,b9 = b,c9 = c,d9 = d,p9 = p,q9 = q as (Element of ADG);
assume (a,b // p,q & c,d // p,q);
then (a9,b9 ==> p9,q9 & c9,d9 ==> p9,q9) by A2;
then a9,b9 ==> c9,d9 by CKB20:1;
hence thesis by A2;
end;

thus (for a,b,c being (Element of A) holds (ex d being (Element of A) st a,b // c,d))
proof
let a being (Element of A),b being (Element of A),c being (Element of A);
reconsider a9 = a,b9 = b,c9 = c as (Element of ADG);
consider d9 being (Element of ADG) such that A6: a9,b9 ==> c9,d9 by CKB21:1;
reconsider d = d9 as (Element of A);
take d;
thus thesis by A2,A6;
end;

thus (for a,b,c,a9,b9,c9 being (Element of A) holds ((a,b // a9,b9 & a,c // a9,c9) implies b,c // b9,c9))
proof
let a being (Element of A),b being (Element of A),c being (Element of A),a9 being (Element of A),b9 being (Element of A),c9 being (Element of A);
reconsider p = a,q = b,r = c,p9 = a9,q9 = b9,r9 = c9 as (Element of ADG);
assume (a,b // a9,b9 & a,c // a9,c9);
then (p,q ==> p9,q9 & p,r ==> p9,r9) by A2;
then q,r ==> q9,r9 by CKB22:1;
hence thesis by A2;
end;

thus (for a,c being (Element of A) holds (ex b being (Element of A) st a,b // b,c))
proof
let a being (Element of A),c being (Element of A);
reconsider a9 = a,c9 = c as (Element of ADG);
consider b9 being (Element of ADG) such that A7: a9,b9 ==> b9,c9 by CKB23:1;
reconsider b = b9 as (Element of A);
take b;
thus thesis by A2,A7;
end;

thus (for a,b,c,b9 being (Element of A) holds ((a,b // b,c & a,b9 // b9,c) implies b = b9))
proof
let a being (Element of A),b being (Element of A),c being (Element of A),b9 being (Element of A);
reconsider a9 = a,p = b,c9 = c,p9 = b9 as (Element of ADG);
assume (a,b // b,c & a,b9 // b9,c);
then (a9,p ==> p,c9 & a9,p9 ==> p9,c9) by A2;
hence thesis by CKB24:1;
end;

thus (for a,b,c,d being (Element of A) holds (a,b // c,d implies a,c // b,d))
proof
let a being (Element of A),b being (Element of A),c being (Element of A),d being (Element of A);
reconsider a9 = a,b9 = b,c9 = c,d9 = d as (Element of ADG);
assume a,b // c,d;
then a9,b9 ==> c9,d9 by A2;
then a9,c9 ==> b9,d9 by CKB25:1;
hence thesis by A2;
end;

end;
