begin
reserve ADG for Uniquely_Two_Divisible_Group;
theorem
((ex a,b being (Element of ADG) st a <> b) implies ( AV ADG ) is AffVect)
proof
A1: ((for a,b,c being (Element of ( AV ADG )) holds (a,b // c,c implies a = b)) & (for a,b,c,b9 being (Element of ( AV ADG )) holds ((a,b // b,c & a,b9 // b9,c) implies b = b9))) by CKB26:1;
assume A2: (ex a,b being (Element of ADG) st a <> b);
then A3: ((for a,b,c,a9,b9,c9 being (Element of ( AV ADG )) holds ((a,b // a9,b9 & a,c // a9,c9) implies b,c // b9,c9)) & (for a,c being (Element of ( AV ADG )) holds (ex b being (Element of ( AV ADG )) st a,b // b,c))) by CKB26:1;
A4: (for a,b,c,d being (Element of ( AV ADG )) holds (a,b // c,d implies a,c // b,d)) by A2,CKB26:1;
((for a,b,c,d,p,q being (Element of ( AV ADG )) holds ((a,b // p,q & c,d // p,q) implies a,b // c,d)) & (for a,b,c being (Element of ( AV ADG )) holds (ex d being (Element of ( AV ADG )) st a,b // c,d))) by A2,CKB26:1;
hence thesis by A2,A3,A1,A4,CKB27:def 1,STRUCT_0:def 10;
end;
