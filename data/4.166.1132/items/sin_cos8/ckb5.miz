environ
vocabularies NUMBERS,XREAL_0,ORDINAL1,SUBSET_1,SIN_COS2,XXREAL_0,CARD_1,FUNCT_1,SIN_COS,ARYTM_1,ARYTM_3,SQUARE_1,RELAT_1,SIN_COS5,NEWTON;
notations ORDINAL1,SUBSET_1,NUMBERS,XCMPLX_0,XXREAL_0,XREAL_0,SQUARE_1,NEWTON,SEQ_1,SIN_COS,SIN_COS2,SIN_COS5;
definitions SQUARE_1;
theorems XREAL_1,SQUARE_1,SIN_COS,NEWTON,XCMPLX_1,SIN_COS2,SIN_COS5,POLYEQ_2,SIN_COS7,TAYLOR_1,CKB1,CKB2,CKB3,CKB4;
registrations XXREAL_0,XREAL_0,SQUARE_1,NAT_1,MEMBERED,SIN_COS,XBOOLE_0,VALUED_0,RELSET_1;
constructors REAL_1,SQUARE_1,NAT_1,BINOP_2,SEQ_1,SIN_COS,SIN_COS2,SIN_COS5,VALUED_1,NEWTON;
