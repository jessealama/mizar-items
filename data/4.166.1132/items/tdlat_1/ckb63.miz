reconsider G = ( ( D-Union T ) || ( Open_Domains_of T ) ) as (Function of [: ( Open_Domains_of T ),( Open_Domains_of T ) :],( Domains_of T )) by FUNCT_2:32;
(for A being (Element of ( Open_Domains_of T )) holds (for B being (Element of ( Open_Domains_of T )) holds ( F . (A,B) ) = ( G . (A,B) )))
proof
let A being (Element of ( Open_Domains_of T ));
