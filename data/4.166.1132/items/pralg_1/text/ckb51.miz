begin
reserve x for set;
definition
let I being set;
let A being (ManySortedSet of I);
func EmptySeq A -> (ManySortedSet of I)
means
:Def20: (for i being set holds (i in I implies ( it . i ) = ( {} ( A . i ) )));
existence
proof
deffunc F(set) = ( {} ( A . $1 ) );
consider f being Function such that A1: (( dom f ) = I & (for x holds (x in I implies ( f . x ) = F(x)))) from FUNCT_1:sch 3;
reconsider f as (ManySortedSet of I) by A1,PARTFUN1:def 2,RELAT_1:def 18;
take f;
thus thesis by A1;
end;
uniqueness
proof
let X being (ManySortedSet of I),Y being (ManySortedSet of I);
assume that
A2: (for i being set holds (i in I implies ( X . i ) = ( {} ( A . i ) )))
and
A3: (for i being set holds (i in I implies ( Y . i ) = ( {} ( A . i ) )));
(for i being set holds (i in I implies ( X . i ) = ( Y . i )))
proof
let i being set;
assume A4: i in I;
then ( X . i ) = ( {} ( A . i ) ) by A2;
hence thesis by A3,A4;
end;
hence thesis by PBOOLE:3;
end;
end;
