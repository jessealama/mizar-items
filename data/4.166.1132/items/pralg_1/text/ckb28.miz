registrations XBOOLE_0,SUBSET_1,RELAT_1,FUNCT_1,ORDINAL1,PARTFUN1,FUNCOP_1,FINSEQ_1,FINSEQ_2,CARD_3,PBOOLE,STRUCT_0,UNIALG_1,CARD_1,RELSET_1,MARGREL1,CKB15,CKB20,CKB24,CKB25,CKB26;
constructors BINOP_1,DOMAIN_1,FUNCT_4,FUNCT_5,CARD_3,PBOOLE,UNIALG_2,DTCONSTR,STRUCT_0,RELSET_1,CKB1,CKB2,CKB3,CKB4,CKB6,CKB7,CKB14,CKB17,CKB21,CKB22,CKB23,CKB27;
requirements BOOLE,SUBSET;
begin
reserve U1 for Universal_Algebra;
reserve U2 for Universal_Algebra;
reserve U3 for Universal_Algebra;
reserve x for set;
reserve y for set;
registration
let J being non empty set;
cluster  equal-signature  Univ_Alg-yielding for (ManySortedSet of J);
existence
proof
set U1 = the Universal_Algebra;
set f = ( J --> U1 );
A1: ( dom f ) = J by FUNCOP_1:13;
reconsider f as (ManySortedSet of J);
take f;
(for x holds (for y holds ((x in ( dom f ) & y in ( dom f )) implies (for U1 holds (for U2 holds ((U1 = ( f . x ) & U2 = ( f . y )) implies ( signature U1 ) = ( signature U2 )))))))
proof
let x;
let y;
assume that
A2: x in ( dom f )
and
A3: y in ( dom f );
let U2;
let U3;
assume A4: (U2 = ( f . x ) & U3 = ( f . y ));
( f . x ) = U1 by A1,A2,FUNCOP_1:7;
hence thesis by A1,A3,A4,FUNCOP_1:7;
end;
hence f is  equal-signature by CKB27:def 1;
(for x holds (x in ( dom f ) implies ( f . x ) is Universal_Algebra)) by A1,FUNCOP_1:7;
hence thesis by CKB22:def 1;
end;
end;
