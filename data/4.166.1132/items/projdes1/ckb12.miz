environ
vocabularies INCSP_1,ANPROJ_2,SUBSET_1,PARSP_2,AFF_2,PROJDES1;
notations STRUCT_0,COLLSP,ANPROJ_2,CKB7;
definitions CKB7;
theorems ANPROJ_2,COLLSP,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11;
registrations ANPROJ_2;
constructors ANPROJ_2,CKB7;
begin
reserve FCPS for  up-3-dimensional CollProjectiveSpace;
reserve a for (Element of FCPS);
reserve b for (Element of FCPS);
reserve c for (Element of FCPS);
reserve p for (Element of FCPS);
reserve q for (Element of FCPS);
reserve x for (Element of FCPS);
reserve y for (Element of FCPS);
reserve z for (Element of FCPS);
theorem
Lm4: ((((not a,b,c is_collinear ) & a,b,c,p are_coplanar ) & a,b,c,q are_coplanar ) implies a,b,p,q are_coplanar )
proof
assume that
A1: (not a,b,c is_collinear )
and
A2: a,b,c,p are_coplanar 
and
A3: a,b,c,q are_coplanar ;
consider x such that A4: a,b,x is_collinear  and A5: c,p,x is_collinear  by A2,CKB7:def 1;
consider y such that A6: a,b,y is_collinear  and A7: c,q,y is_collinear  by A3,CKB7:def 1;
A8:now
assume A9: a <> b;
(b,a,x is_collinear  & b,a,y is_collinear ) by A4,A6,CKB1:1;
then b,x,y is_collinear  by A9,COLLSP:6;
then A10: x,y,b is_collinear  by CKB1:1;
a,x,y is_collinear  by A4,A6,A9,COLLSP:6;
then A11: x,y,a is_collinear  by CKB1:1;
A12:now
p,c,x is_collinear  by A5,CKB1:1;
then consider z such that A13: p,q,z is_collinear  and A14: x,y,z is_collinear  by A7,ANPROJ_2:def 9;
assume x <> y;
then a,b,z is_collinear  by A11,A10,A14,ANPROJ_2:def 8;
hence thesis by A13,CKB7:def 1;
end;
now
assume x = y;
then A15: x,c,q is_collinear  by A7,CKB1:1;
x,c,p is_collinear  by A5,CKB1:1;
then x,p,q is_collinear  by A1,A4,A15,COLLSP:6;
then p,q,x is_collinear  by CKB1:1;
hence thesis by A4,CKB7:def 1;
end;
hence thesis by A12;
end;
now
assume a = b;
then a,b,p is_collinear  by ANPROJ_2:def 7;
hence thesis by CKB8:1;
end;
hence thesis by A8;
end;
