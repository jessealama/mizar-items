environ
vocabularies INCSP_1,ANPROJ_2,SUBSET_1,PARSP_2,AFF_2,PROJDES1;
notations STRUCT_0,COLLSP,ANPROJ_2,CKB7,CKB28;
definitions CKB7,CKB28;
theorems ANPROJ_2,COLLSP,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29;
registrations ANPROJ_2;
constructors ANPROJ_2,CKB7,CKB28;
begin
reserve FCPS for  up-3-dimensional CollProjectiveSpace;
reserve a for (Element of FCPS);
reserve a9 for (Element of FCPS);
reserve b for (Element of FCPS);
reserve b9 for (Element of FCPS);
reserve c for (Element of FCPS);
reserve c9 for (Element of FCPS);
reserve o for (Element of FCPS);
reserve p for (Element of FCPS);
reserve q for (Element of FCPS);
reserve r for (Element of FCPS);
theorem
Th26: ((((((((((((((((((not o,a,b is_collinear ) & (not o,b,c is_collinear )) & (not o,a,c is_collinear )) & o,a,a9 is_collinear ) & o,b,b9 is_collinear ) & o,c,c9 is_collinear ) & a,b,p is_collinear ) & a9,b9,p is_collinear ) & a <> a9) & b,c,r is_collinear ) & b9,c9,r is_collinear ) & a,c,q is_collinear ) & b <> b9) & a9,c9,q is_collinear ) & o <> a9) & o <> b9) & o <> c9) implies r,q,p is_collinear )
proof
assume that
A1: (not o,a,b is_collinear )
and
A2: (not o,b,c is_collinear )
and
A3: (not o,a,c is_collinear )
and
A4: ((o,a,a9 is_collinear  & o,b,b9 is_collinear ) & o,c,c9 is_collinear )
and
A5: a,b,p is_collinear 
and
A6: (a9,b9,p is_collinear  & a <> a9)
and
A7: b,c,r is_collinear 
and
A8: b9,c9,r is_collinear 
and
A9: a,c,q is_collinear 
and
A10: ((((b <> b9 & a9,c9,q is_collinear ) & o <> a9) & o <> b9) & o <> c9);
A11:now
A12: (b <> c & b,c,b is_collinear ) by A2,ANPROJ_2:def 7;
assume A13: a,b,c is_collinear ;
then b,c,a is_collinear  by CKB1:1;
then A14: a,b,r is_collinear  by A7,A12,ANPROJ_2:def 8;
A15: (c <> a & a,c,a is_collinear ) by A3,ANPROJ_2:def 7;
a,c,b is_collinear  by A13,CKB1:1;
then A16: a,b,q is_collinear  by A9,A15,ANPROJ_2:def 8;
a <> b by A1,ANPROJ_2:def 7;
hence thesis by A5,A14,A16,ANPROJ_2:def 8;
end;
A17: (not o,c,a is_collinear ) by A3,CKB1:1;
now
assume (not a,b,c is_collinear );
then o,a,b,c constitute_a_quadrangle  by A1,A2,A17,CKB28:def 1;
then p,r,q is_collinear  by A4,A5,A6,A7,A8,A9,A10,CKB29:1;
hence thesis by CKB1:1;
end;
hence thesis by A11;
end;
