environ
vocabularies INCSP_1,ANPROJ_2,SUBSET_1,PARSP_2,AFF_2,PROJDES1;
notations STRUCT_0,COLLSP,ANPROJ_2,CKB7;
definitions CKB7;
theorems ANPROJ_2,COLLSP,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7;
registrations ANPROJ_2;
constructors ANPROJ_2,CKB7;
begin
reserve FCPS for  up-3-dimensional CollProjectiveSpace;
reserve a for (Element of FCPS);
reserve b for (Element of FCPS);
reserve c for (Element of FCPS);
reserve d for (Element of FCPS);
theorem
Th10: ((((a,b,c is_collinear  or b,c,d is_collinear ) or c,d,a is_collinear ) or d,a,b is_collinear ) implies a,b,c,d are_coplanar )
proof
A1:now
A2: c,d,c is_collinear  by ANPROJ_2:def 7;
assume a,b,c is_collinear ;
hence thesis by A2,CKB7:def 1;
end;
A3:now
A4: a,b,a is_collinear  by ANPROJ_2:def 7;
assume c,d,a is_collinear ;
hence thesis by A4,CKB7:def 1;
end;
A5:now
assume d,a,b is_collinear ;
then A6: a,b,d is_collinear  by CKB1:1;
c,d,d is_collinear  by ANPROJ_2:def 7;
hence thesis by A6,CKB7:def 1;
end;
A7:now
assume b,c,d is_collinear ;
then A8: c,d,b is_collinear  by CKB1:1;
a,b,b is_collinear  by ANPROJ_2:def 7;
hence thesis by A8,CKB7:def 1;
end;
assume (((a,b,c is_collinear  or b,c,d is_collinear ) or c,d,a is_collinear ) or d,a,b is_collinear );
hence thesis by A1,A7,A3,A5;
end;
