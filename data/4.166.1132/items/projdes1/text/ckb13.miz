environ
vocabularies INCSP_1,ANPROJ_2,SUBSET_1,PARSP_2,AFF_2,PROJDES1;
notations STRUCT_0,COLLSP,ANPROJ_2,CKB7;
definitions CKB7;
theorems ANPROJ_2,COLLSP,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12;
registrations ANPROJ_2;
constructors ANPROJ_2,CKB7;
begin
reserve FCPS for  up-3-dimensional CollProjectiveSpace;
reserve a for (Element of FCPS);
reserve b for (Element of FCPS);
reserve c for (Element of FCPS);
reserve p for (Element of FCPS);
reserve q for (Element of FCPS);
reserve r for (Element of FCPS);
reserve s for (Element of FCPS);
theorem
Th12: ((((((not a,b,c is_collinear ) & a,b,c,p are_coplanar ) & a,b,c,q are_coplanar ) & a,b,c,r are_coplanar ) & a,b,c,s are_coplanar ) implies p,q,r,s are_coplanar )
proof
assume that
A1: (not a,b,c is_collinear )
and
A2: a,b,c,p are_coplanar 
and
A3: a,b,c,q are_coplanar 
and
A4: a,b,c,r are_coplanar 
and
A5: a,b,c,s are_coplanar ;
A6: a,b,p,q are_coplanar  by A1,A2,A3,CKB12:1;
A7: a,b,q,r are_coplanar  by A1,A3,A4,CKB12:1;
A8: a,b,p,r are_coplanar  by A1,A2,A4,CKB12:1;
A9: a,b,q,s are_coplanar  by A1,A3,A5,CKB12:1;
A10:now
A11: q,a,b,r are_coplanar  by A7,CKB11:1;
assume A12: (not a,b,q is_collinear );
then A13: (not q,a,b is_collinear ) by CKB1:1;
A14: q,a,b,p are_coplanar  by A6,CKB11:1;
then A15: q,a,p,r are_coplanar  by A13,A11,CKB12:1;
A16: q,a,b,s are_coplanar  by A9,CKB11:1;
then A17: q,a,p,s are_coplanar  by A13,A14,CKB12:1;
A18:now
assume (not q,a,p is_collinear );
then A19: (not q,p,a is_collinear ) by CKB1:1;
(q,p,a,r are_coplanar  & q,p,a,s are_coplanar ) by A15,A17,CKB11:1;
then q,p,r,s are_coplanar  by A19,CKB12:1;
hence thesis by CKB11:1;
end;
A20: q,a,r,s are_coplanar  by A13,A11,A16,CKB12:1;
A21:now
assume (not q,a,r is_collinear );
then A22: (not q,r,a is_collinear ) by CKB1:1;
(q,r,a,p are_coplanar  & q,r,a,s are_coplanar ) by A15,A20,CKB11:1;
then q,r,p,s are_coplanar  by A22,CKB12:1;
hence thesis by CKB11:1;
end;
A23: q <> a by A12,ANPROJ_2:def 7;
now
assume (q,a,p is_collinear  & q,a,r is_collinear );
then q,p,r is_collinear  by A23,COLLSP:6;
then p,q,r is_collinear  by CKB1:1;
hence thesis by CKB8:1;
end;
hence thesis by A18,A21;
end;
A24: a,b,r,s are_coplanar  by A1,A4,A5,CKB12:1;
A25:now
A26: r,a,b,q are_coplanar  by A7,CKB11:1;
assume A27: (not a,b,r is_collinear );
then A28: (not r,a,b is_collinear ) by CKB1:1;
A29: r,a,b,p are_coplanar  by A8,CKB11:1;
then A30: r,a,p,q are_coplanar  by A28,A26,CKB12:1;
A31: r,a,b,s are_coplanar  by A24,CKB11:1;
then A32: r,a,p,s are_coplanar  by A28,A29,CKB12:1;
A33:now
assume (not r,a,p is_collinear );
then A34: (not r,p,a is_collinear ) by CKB1:1;
(r,p,a,q are_coplanar  & r,p,a,s are_coplanar ) by A30,A32,CKB11:1;
then r,p,q,s are_coplanar  by A34,CKB12:1;
hence thesis by CKB11:1;
end;
A35: r,a,q,s are_coplanar  by A28,A26,A31,CKB12:1;
A36:now
assume (not r,a,q is_collinear );
then A37: (not r,q,a is_collinear ) by CKB1:1;
(r,q,a,p are_coplanar  & r,q,a,s are_coplanar ) by A30,A35,CKB11:1;
then r,q,p,s are_coplanar  by A37,CKB12:1;
hence thesis by CKB11:1;
end;
A38: r <> a by A27,ANPROJ_2:def 7;
now
assume (r,a,p is_collinear  & r,a,q is_collinear );
then r,p,q is_collinear  by A38,COLLSP:6;
then p,q,r is_collinear  by CKB1:1;
hence thesis by CKB8:1;
end;
hence thesis by A33,A36;
end;
A39: a,b,p,s are_coplanar  by A1,A2,A5,CKB12:1;
A40:now
A41: p,a,b,r are_coplanar  by A8,CKB11:1;
assume A42: (not a,b,p is_collinear );
then A43: (not p,a,b is_collinear ) by CKB1:1;
A44: p,a,b,q are_coplanar  by A6,CKB11:1;
then A45: p,a,q,r are_coplanar  by A43,A41,CKB12:1;
A46: p,a,b,s are_coplanar  by A39,CKB11:1;
then A47: p,a,q,s are_coplanar  by A43,A44,CKB12:1;
A48:now
assume (not p,a,q is_collinear );
then A49: (not p,q,a is_collinear ) by CKB1:1;
(p,q,a,r are_coplanar  & p,q,a,s are_coplanar ) by A45,A47,CKB11:1;
hence thesis by A49,CKB12:1;
end;
A50: p,a,r,s are_coplanar  by A43,A41,A46,CKB12:1;
A51:now
assume (not p,a,r is_collinear );
then A52: (not p,r,a is_collinear ) by CKB1:1;
(p,r,a,q are_coplanar  & p,r,a,s are_coplanar ) by A45,A50,CKB11:1;
then p,r,q,s are_coplanar  by A52,CKB12:1;
hence thesis by CKB11:1;
end;
A53: p <> a by A42,ANPROJ_2:def 7;
now
assume (p,a,q is_collinear  & p,a,r is_collinear );
then p,q,r is_collinear  by A53,COLLSP:6;
hence thesis by CKB8:1;
end;
hence thesis by A48,A51;
end;
A54: a <> b by A1,ANPROJ_2:def 7;
now
assume ((a,b,p is_collinear  & a,b,q is_collinear ) & a,b,r is_collinear );
then p,q,r is_collinear  by A54,ANPROJ_2:def 8;
hence thesis by CKB8:1;
end;
hence thesis by A40,A10,A25;
end;
