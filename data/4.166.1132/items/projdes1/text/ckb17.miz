environ
vocabularies INCSP_1,ANPROJ_2,SUBSET_1,PARSP_2,AFF_2,PROJDES1;
notations STRUCT_0,COLLSP,ANPROJ_2,CKB7;
definitions CKB7;
theorems ANPROJ_2,COLLSP,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16;
registrations ANPROJ_2;
constructors ANPROJ_2,CKB7;
begin
reserve FCPS for  up-3-dimensional CollProjectiveSpace;
reserve a for (Element of FCPS);
reserve b for (Element of FCPS);
reserve c for (Element of FCPS);
reserve p for (Element of FCPS);
reserve q for (Element of FCPS);
reserve r for (Element of FCPS);
theorem
Th14: ((((p <> q & p,q,r is_collinear ) & a,b,c,p are_coplanar ) & a,b,c,q are_coplanar ) implies a,b,c,r are_coplanar )
proof
assume that
A1: p <> q
and
A2: p,q,r is_collinear 
and
A3: a,b,c,p are_coplanar 
and
A4: a,b,c,q are_coplanar ;
A5: q,p,r is_collinear  by A2,CKB1:1;
now
assume A6: (not a,b,c is_collinear );
then a,b,p,q are_coplanar  by A3,A4,CKB12:1;
then A7: a,b,p,r are_coplanar  by A1,A2,CKB16:1;
A8:now
b,a,b is_collinear  by ANPROJ_2:def 7;
then A9: a,b,p,b are_coplanar  by CKB8:1;
a,a,b is_collinear  by ANPROJ_2:def 7;
then A10: a,b,p,a are_coplanar  by CKB8:1;
assume A11: (not a,b,p is_collinear );
a,b,p,c are_coplanar  by A3,CKB11:1;
hence thesis by A7,A11,A10,A9,CKB13:1;
end;
a,b,q,p are_coplanar  by A3,A4,A6,CKB12:1;
then A12: a,b,q,r are_coplanar  by A1,A5,CKB16:1;
A13:now
b,a,b is_collinear  by ANPROJ_2:def 7;
then A14: a,b,q,b are_coplanar  by CKB8:1;
a,a,b is_collinear  by ANPROJ_2:def 7;
then A15: a,b,q,a are_coplanar  by CKB8:1;
assume A16: (not a,b,q is_collinear );
a,b,q,c are_coplanar  by A4,CKB11:1;
hence thesis by A12,A16,A15,A14,CKB13:1;
end;
now
assume (a,b,p is_collinear  & a,b,q is_collinear );
then a,b,r is_collinear  by A1,A2,COLLSP:9;
then r,a,b is_collinear  by CKB1:1;
hence thesis by CKB8:1;
end;
hence thesis by A8,A13;
end;
hence thesis by CKB8:1;
end;
