environ
vocabularies INCSP_1,ANPROJ_2,SUBSET_1,PARSP_2,AFF_2,PROJDES1;
notations STRUCT_0,COLLSP,ANPROJ_2,CKB7;
definitions CKB7;
theorems ANPROJ_2,COLLSP,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13;
registrations ANPROJ_2;
constructors ANPROJ_2,CKB7;
begin
reserve FCPS for  up-3-dimensional CollProjectiveSpace;
reserve a for (Element of FCPS);
reserve b for (Element of FCPS);
reserve c for (Element of FCPS);
reserve p for (Element of FCPS);
reserve q for (Element of FCPS);
reserve r for (Element of FCPS);
theorem
Lm5: (((((not a,b,c is_collinear ) & a,b,c,p are_coplanar ) & a,b,c,q are_coplanar ) & a,b,c,r are_coplanar ) implies a,p,q,r are_coplanar )
proof
assume that
A1: (not a,b,c is_collinear )
and
A2: a,b,c,p are_coplanar 
and
A3: a,b,c,q are_coplanar 
and
A4: a,b,c,r are_coplanar ;
now
A5:now
a,b,r,q are_coplanar  by A1,A3,A4,CKB12:1;
then A6: a,r,b,q are_coplanar  by CKB11:1;
a,b,r,p are_coplanar  by A1,A2,A4,CKB12:1;
then A7: a,r,b,p are_coplanar  by CKB11:1;
assume (not a,r,b is_collinear );
then a,r,p,q are_coplanar  by A7,A6,CKB12:1;
hence thesis by CKB11:1;
end;
A8:now
a,b,p,r are_coplanar  by A1,A2,A4,CKB12:1;
then A9: a,p,b,r are_coplanar  by CKB11:1;
a,b,p,q are_coplanar  by A1,A2,A3,CKB12:1;
then A10: a,p,b,q are_coplanar  by CKB11:1;
assume (not a,p,b is_collinear );
hence thesis by A10,A9,CKB12:1;
end;
A11:now
a,b,q,r are_coplanar  by A1,A3,A4,CKB12:1;
then A12: a,q,b,r are_coplanar  by CKB11:1;
a,b,q,p are_coplanar  by A1,A2,A3,CKB12:1;
then A13: a,q,b,p are_coplanar  by CKB11:1;
assume (not a,q,b is_collinear );
then a,q,p,r are_coplanar  by A13,A12,CKB12:1;
hence thesis by CKB11:1;
end;
assume A14: (not p,q,r is_collinear );
a <> b by A1,ANPROJ_2:def 7;
then (((not a,b,p is_collinear ) or (not a,b,q is_collinear )) or (not a,b,r is_collinear )) by A14,ANPROJ_2:def 8;
hence thesis by A8,A11,A5,CKB1:1;
end;
hence thesis by CKB8:1;
end;
