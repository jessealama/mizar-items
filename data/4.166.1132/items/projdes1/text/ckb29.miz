environ
vocabularies INCSP_1,ANPROJ_2,SUBSET_1,PARSP_2,AFF_2,PROJDES1;
notations STRUCT_0,COLLSP,ANPROJ_2,CKB7,CKB28;
definitions CKB7,CKB28;
theorems ANPROJ_2,COLLSP,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28;
registrations ANPROJ_2;
constructors ANPROJ_2,CKB7,CKB28;
begin
reserve FCPS for  up-3-dimensional CollProjectiveSpace;
reserve a for (Element of FCPS);
reserve a9 for (Element of FCPS);
reserve b for (Element of FCPS);
reserve b9 for (Element of FCPS);
reserve c for (Element of FCPS);
reserve c9 for (Element of FCPS);
reserve d for (Element of FCPS);
reserve d9 for (Element of FCPS);
reserve o for (Element of FCPS);
reserve p for (Element of FCPS);
reserve q for (Element of FCPS);
reserve r for (Element of FCPS);
reserve s for (Element of FCPS);
reserve t for (Element of FCPS);
reserve u for (Element of FCPS);
theorem
Lm7: (((((((((((((((o <> a9 & o <> b9) & o <> c9) & a <> a9) & b <> b9) & o,a,b,c constitute_a_quadrangle ) & o,a,a9 is_collinear ) & o,b,b9 is_collinear ) & o,c,c9 is_collinear ) & a,b,p is_collinear ) & a9,b9,p is_collinear ) & b,c,q is_collinear ) & b9,c9,q is_collinear ) & a,c,r is_collinear ) & a9,c9,r is_collinear ) implies p,q,r is_collinear )
proof
assume that
A1: o <> a9
and
A2: o <> b9
and
A3: o <> c9
and
A4: a <> a9
and
A5: b <> b9
and
A6: o,a,b,c constitute_a_quadrangle 
and
A7: o,a,a9 is_collinear 
and
A8: o,b,b9 is_collinear 
and
A9: o,c,c9 is_collinear 
and
A10: a,b,p is_collinear 
and
A11: a9,b9,p is_collinear 
and
A12: b,c,q is_collinear 
and
A13: b9,c9,q is_collinear 
and
A14: a,c,r is_collinear 
and
A15: a9,c9,r is_collinear ;
A16: (not o,b,c is_collinear ) by A6,CKB28:def 1;
A17: (not a,b,c is_collinear ) by A6,CKB28:def 1;
A18: (not o,c,a is_collinear ) by A6,CKB28:def 1;
A19: (not o,a,b is_collinear ) by A6,CKB28:def 1;
A20:now
assume A21: a,b,c,o are_coplanar ;
then A22: b,c,a,o are_coplanar  by CKB11:1;
A23: a,c,b,o are_coplanar  by A21,CKB11:1;
consider d such that A24: (not a,b,c,d are_coplanar ) by A17,CKB20:1;
A25: (not a,c,b,d are_coplanar ) by A24,CKB11:1;
A26: a,b,c,c are_coplanar  by CKB21:1;
A27: (not b,c,a,d are_coplanar ) by A24,CKB11:1;
consider d9 such that A28: o <> d9 and A29: d <> d9 and A30: o,d,d9 is_collinear  by ANPROJ_2:def 10;
a,o,a9 is_collinear  by A7,CKB1:1;
then consider s such that A31: a,d,s is_collinear  and A32: a9,d9,s is_collinear  by A30,ANPROJ_2:def 9;
A33: d,a,s is_collinear  by A31,CKB1:1;
b,o,b9 is_collinear  by A8,CKB1:1;
then consider t such that A34: (b,d,t is_collinear  & b9,d9,t is_collinear ) by A30,ANPROJ_2:def 9;
c,o,c9 is_collinear  by A9,CKB1:1;
then consider u such that A35: c,d,u is_collinear  and A36: c9,d9,u is_collinear  by A30,ANPROJ_2:def 9;
A37: s,t,u,s are_coplanar  by CKB21:1;
(not a,c,o is_collinear ) by A18,CKB1:1;
then A38: (not a,c,d,o are_coplanar ) by A25,A23,CKB25:1;
then (not a9,c9,d9 is_collinear ) by A1,A3,A7,A9,A28,A30,CKB26:1;
then r,u,s is_collinear  by A4,A7,A14,A15,A31,A32,A35,A36,A38,CKB24:1;
then A39: s,u,r is_collinear  by CKB1:1;
(not a,b,o is_collinear ) by A19,CKB1:1;
then A40: (not a,b,d,o are_coplanar ) by A21,A24,CKB25:1;
then (not d,o,a is_collinear ) by CKB8:1;
then A41: (not o,d,a is_collinear ) by CKB1:1;
A42: s,t,u,u are_coplanar  by CKB21:1;
(not b,c,o is_collinear ) by A16,CKB1:1;
then A43: (not b,c,d,o are_coplanar ) by A27,A22,CKB25:1;
then (not b9,c9,d9 is_collinear ) by A2,A3,A8,A9,A28,A30,CKB26:1;
then q,u,t is_collinear  by A5,A8,A12,A13,A34,A35,A36,A43,CKB24:1;
then A44: u,t,q is_collinear  by CKB1:1;
A45: s,t,u,t are_coplanar  by CKB21:1;
d9,a9,s is_collinear  by A32,CKB1:1;
then s <> a by A4,A7,A28,A30,A41,CKB5:1;
then A46: (not a,b,c,s are_coplanar ) by A24,A33,CKB22:1;
A47: a,b,c,b are_coplanar  by CKB21:1;
(not a9,b9,d9 is_collinear ) by A1,A2,A7,A8,A28,A30,A40,CKB26:1;
then p,t,s is_collinear  by A4,A7,A10,A11,A31,A32,A34,A40,CKB24:1;
then A48: t,s,p is_collinear  by CKB1:1;
A49: a,b,c,a are_coplanar  by CKB21:1;
A50: (not s,t,u is_collinear ) by A1,A2,A7,A8,A21,A24,A29,A30,A31,A32,A34,A35,A40,CKB27:1;
then t <> u by ANPROJ_2:def 7;
then A51: s,t,u,q are_coplanar  by A44,A45,A42,CKB17:1;
c <> a by A18,ANPROJ_2:def 7;
then A52: a,b,c,r are_coplanar  by A14,A49,A26,CKB17:1;
b <> c by A16,ANPROJ_2:def 7;
then A53: a,b,c,q are_coplanar  by A12,A47,A26,CKB17:1;
a <> b by A19,ANPROJ_2:def 7;
then A54: a,b,c,p are_coplanar  by A10,A49,A47,CKB17:1;
u <> s by A50,ANPROJ_2:def 7;
then A55: s,t,u,r are_coplanar  by A39,A37,A42,CKB17:1;
s <> t by A50,ANPROJ_2:def 7;
then s,t,u,p are_coplanar  by A48,A37,A45,CKB17:1;
hence thesis by A17,A50,A46,A54,A53,A52,A51,A55,CKB23:1;
end;
now
assume A56: (not a,b,c,o are_coplanar );
then (not a9,b9,c9 is_collinear ) by A1,A2,A3,A7,A8,A9,CKB26:1;
hence thesis by A4,A7,A10,A11,A12,A13,A14,A15,A56,CKB24:1;
end;
hence thesis by A20;
end;
