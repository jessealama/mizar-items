environ
vocabularies INCSP_1,ANPROJ_2,SUBSET_1,PARSP_2,AFF_2,PROJDES1;
notations STRUCT_0,COLLSP,ANPROJ_2,CKB7;
definitions CKB7;
theorems ANPROJ_2,COLLSP,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25;
registrations ANPROJ_2;
constructors ANPROJ_2,CKB7;
begin
reserve FCPS for  up-3-dimensional CollProjectiveSpace;
reserve a for (Element of FCPS);
reserve a9 for (Element of FCPS);
reserve b for (Element of FCPS);
reserve b9 for (Element of FCPS);
reserve c for (Element of FCPS);
reserve c9 for (Element of FCPS);
reserve o for (Element of FCPS);
theorem
Th23: ((((((((not a,b,c,o are_coplanar ) & o,a,a9 is_collinear ) & o,b,b9 is_collinear ) & o,c,c9 is_collinear ) & o <> a9) & o <> b9) & o <> c9) implies ((not a9,b9,c9 is_collinear ) & (not a9,b9,c9,o are_coplanar )))
proof
assume that
A1: ((not a,b,c,o are_coplanar ) & o,a,a9 is_collinear )
and
A2: o,b,b9 is_collinear 
and
A3: o,c,c9 is_collinear 
and
A4: o <> a9
and
A5: o <> b9
and
A6: o <> c9;
(a,o,a9 is_collinear  & (not o,b,c,a are_coplanar )) by A1,CKB1:1,CKB11:1;
then (not o,b,c,a9 are_coplanar ) by A4,CKB22:1;
then A7: (not o,a9,b,c are_coplanar ) by CKB11:1;
c,o,c9 is_collinear  by A3,CKB1:1;
then (not o,a9,b,c9 are_coplanar ) by A6,A7,CKB22:1;
then A8: (not o,a9,c9,b are_coplanar ) by CKB11:1;
b,o,b9 is_collinear  by A2,CKB1:1;
then (not o,a9,c9,b9 are_coplanar ) by A5,A8,CKB22:1;
then (not a9,b9,c9,o are_coplanar ) by CKB11:1;
hence thesis by CKB8:1;
end;
