begin
theorem
(for FT being  filled non empty RelStr holds (for B being (Subset of FT) holds (FT is  symmetric implies (B is  connected iff (not (ex C being (Subset of FT) st (((C <> ( {} ) & ( B \ C ) <> ( {} )) & C c= B) & ( C ^b ) misses ( B \ C ))))))))
proof
let FT being  filled non empty RelStr;
let B being (Subset of FT);
assume A1: FT is  symmetric;
thus (B is  connected implies (not (ex C being (Subset of FT) st (((C <> ( {} ) & ( B \ C ) <> ( {} )) & C c= B) & ( C ^b ) misses ( B \ C )))))
proof
assume A2: B is  connected;
(for C being (Subset of FT) holds ((C c= B & ( C ^b ) misses ( B \ C )) implies (C = ( {} ) or ( B \ C ) = ( {} ))))
proof
let C being (Subset of FT);
assume that
A3: C c= B
and
A4: ( C ^b ) misses ( B \ C );
C misses ( ( B \ C ) ^b ) by A1,A4,CKB9:1;
then A5: C,( B \ C ) are_separated  by A4,CKB1:def 1;
( C \/ ( B \ C ) ) = ( C \/ B ) by XBOOLE_1:39
.= B by A3,XBOOLE_1:12;
then (C = B or ( B \ C ) = B) by A1,A2,A5,CKB13:1;
hence thesis by A3,XBOOLE_1:37,XBOOLE_1:38;
end;
hence thesis;
end;

thus ((not (ex C being (Subset of FT) st (((C <> ( {} ) & ( B \ C ) <> ( {} )) & C c= B) & ( C ^b ) misses ( B \ C )))) implies B is  connected)
proof
assume A6: (not (ex C being (Subset of FT) st (((C <> ( {} ) & ( B \ C ) <> ( {} )) & C c= B) & ( C ^b ) misses ( B \ C ))));
(for A,B2 being (Subset of FT) holds ((B = ( A \/ B2 ) & A,B2 are_separated ) implies (A = B or B2 = B)))
proof
let A being (Subset of FT),B2 being (Subset of FT);
assume that
A7: B = ( A \/ B2 )
and
A8: A,B2 are_separated ;
A9: ( ( A \/ B2 ) \ A ) = ( B2 \ A ) by XBOOLE_1:40;
( A ^b ) misses B2 by A8,CKB1:def 1;
then ( A ^b ) misses ( B \ A ) by A7,A9,XBOOLE_1:36,XBOOLE_1:63;
then (A = ( {} ) or ( B \ A ) = ( {} )) by A6,A7,XBOOLE_1:7;
then A10: (B = B2 or B c= A) by A7,XBOOLE_1:37;
A c= B by A7,XBOOLE_1:7;
hence thesis by A10,XBOOLE_0:def 10;
end;
hence thesis by A1,CKB13:1;
end;

