environ
vocabularies NUMBERS,XBOOLE_0,ORDERS_2,SUBSET_1,CONNSP_1,FIN_TOPO,XXREAL_0,FINTOPO3,TARSKI,ARYTM_3,ARYTM_1,CARD_1,RELAT_2,FUNCT_1,STRUCT_0,RELAT_1,NAT_1,FINSEQ_1,ZFMISC_1,FINTOPO4;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,RELAT_1,NUMBERS,XCMPLX_0,XXREAL_0,NAT_1,FUNCT_1,RELSET_1,FUNCT_2,FINSEQ_1,STRUCT_0,ORDERS_2,FIN_TOPO,FINTOPO3,NAT_D,ENUMSET1,LIMFUNC1,CKB1,CKB15;
definitions TARSKI,FIN_TOPO,FINSEQ_1,CKB1,CKB15;
theorems TARSKI,FINSEQ_1,NAT_1,FUNCT_1,FUNCT_2,XBOOLE_0,XBOOLE_1,FIN_TOPO,FINTOPO3,RELAT_1,ENUMSET1,XREAL_1,XXREAL_0,ORDINAL1,RELSET_1,XREAL_0,NAT_D,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18;
schemes NAT_1,FUNCT_2,RELSET_1;
registrations XBOOLE_0,SUBSET_1,RELSET_1,XXREAL_0,XREAL_0,NAT_1,STRUCT_0,FIN_TOPO,ORDINAL1,FINSEQ_1;
constructors ENUMSET1,NAT_1,EQREL_1,LIMFUNC1,NAT_D,FIN_TOPO,FINTOPO3,FINSEQ_1,RELSET_1,CKB1,CKB15;
requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
theorem
(for FT1,FT2 being non  empty RelStr holds (for A being (Subset of FT1) holds (for B being (Subset of FT2) holds (for f being (Function of FT1,FT2) holds (((A is  connected & f is_continuous ( 0 )) & B = ( f .: A )) implies B is  connected)))))
proof
let FT1 being non  empty RelStr;
let FT2 being non  empty RelStr;
let A being (Subset of FT1);
let B being (Subset of FT2);
let f being (Function of FT1,FT2);
assume that
A1: A is  connected
and
A2: f is_continuous ( 0 )
and
A3: B = ( f .: A );
(for B2,C2 being (Subset of FT2) holds ((((B = ( B2 \/ C2 ) & B2 <> ( {} )) & C2 <> ( {} )) & B2 misses C2) implies ( B2 ^b ) meets C2))
proof
let B2 being (Subset of FT2);
let C2 being (Subset of FT2);
assume that
A4: B = ( B2 \/ C2 )
and
A5: B2 <> ( {} )
and
A6: C2 <> ( {} )
and
A7: B2 misses C2;
reconsider C1 = ( f " C2 ) as (Subset of FT1);
reconsider C10 = ( C1 /\ A ) as (Subset of FT1);
reconsider B1 = ( f " B2 ) as (Subset of FT1);
reconsider B10 = ( B1 /\ A ) as (Subset of FT1);
A8: C10 c= C1 by XBOOLE_1:17;
set x6 = the (Element of C2);
x6 in B by A4,A6,XBOOLE_0:def 3;
then consider z6 being set such that A9: z6 in ( dom f ) and A10: z6 in A and A11: x6 = ( f . z6 ) by A3,FUNCT_1:def 6;
z6 in ( f " C2 ) by A6,A9,A11,FUNCT_1:def 7;
then A12: C10 <> ( {} ) by A10,XBOOLE_0:def 4;
set x5 = the (Element of B2);
x5 in B by A4,A5,XBOOLE_0:def 3;
then consider z5 being set such that A13: z5 in ( dom f ) and A14: z5 in A and A15: x5 = ( f . z5 ) by A3,FUNCT_1:def 6;
A c= (the carrier of FT1);
then A16: A c= ( dom f ) by FUNCT_2:def 1;
( B2 /\ C2 ) = ( {} ) by A7,XBOOLE_0:def 7;
then ( f " ( B2 /\ C2 ) ) = ( {} ) by RELAT_1:136;
then (B10 c= B1 & ( ( f " B2 ) /\ ( f " C2 ) ) = ( {} )) by FUNCT_1:68,XBOOLE_1:17;
then ( B10 /\ C10 ) = ( {} ) by A8,XBOOLE_1:3,XBOOLE_1:27;
then A17: B10 misses C10 by XBOOLE_0:def 7;
( ( f " B2 ) \/ ( f " C2 ) ) = ( f " ( f .: A ) ) by A3,A4,RELAT_1:140;
then A c= ( B1 \/ C1 ) by A16,FUNCT_1:76;
then A c= ( A /\ ( B1 \/ C1 ) ) by XBOOLE_1:19;
then A18: A c= ( B10 \/ C10 ) by XBOOLE_1:23;
(B10 c= A & C10 c= A) by XBOOLE_1:17;
then ( B10 \/ C10 ) c= A by XBOOLE_1:8;
then A19: A = ( B10 \/ C10 ) by A18,XBOOLE_0:def 10;
z5 in ( f " B2 ) by A5,A13,A15,FUNCT_1:def 7;
then B10 <> ( {} ) by A14,XBOOLE_0:def 4;
then ( B10 ^b ) meets C10 by A1,A19,A12,A17,FIN_TOPO:def 16;
then A20: ( ( B10 ^b ) /\ C10 ) <> ( {} ) by XBOOLE_0:def 7;
reconsider B20 = ( f .: B1 ) as (Subset of FT2);
A21: ( dom f ) = (the carrier of FT1) by FUNCT_2:def 1;
( f .: B1 ) c= B2
proof
let y being set;
assume y in ( f .: B1 );
then (ex x2 being set st ((x2 in ( dom f ) & x2 in B1) & y = ( f . x2 ))) by FUNCT_1:def 6;
hence thesis by FUNCT_1:def 7;
end;
then A22: ( B20 ^b ) c= ( B2 ^b ) by FIN_TOPO:14;
( f .: ( B1 ^b ) ) c= ( B20 ^b ) by A2,CKB18:1;
then ( f .: ( B1 ^b ) ) c= ( B2 ^b ) by A22,XBOOLE_1:1;
then A23: (( ( f .: ( B1 ^b ) ) /\ ( f .: C1 ) ) c= ( ( f .: ( B1 ^b ) ) /\ C2 ) & ( ( f .: ( B1 ^b ) ) /\ C2 ) c= ( ( B2 ^b ) /\ C2 )) by FUNCT_1:75,XBOOLE_1:26;
( B10 ^b ) c= ( B1 ^b ) by FIN_TOPO:14,XBOOLE_1:17;
then ( ( B1 ^b ) /\ C1 ) <> ( {} ) by A8,A20,XBOOLE_1:3,XBOOLE_1:27;
then ( f .: ( ( B1 ^b ) /\ C1 ) ) <> ( {} ) by A21,RELAT_1:119;
then ( ( f .: ( B1 ^b ) ) /\ ( f .: C1 ) ) <> ( {} ) by RELAT_1:121,XBOOLE_1:3;
then ( ( B2 ^b ) /\ C2 ) <> ( {} ) by A23;
hence thesis by XBOOLE_0:def 7;
end;
hence thesis by FIN_TOPO:def 16;
end;
