begin
theorem
(for FT being  filled non empty RelStr holds (for A,B being (Subset of FT) holds (FT is  symmetric implies (A,B are_separated  iff A misses ( B ^b )))))
proof
let FT being  filled non empty RelStr;
let A being (Subset of FT),B being (Subset of FT);
assume A1: FT is  symmetric;
thus (A,B are_separated  implies A misses ( B ^b )) by CKB1:def 1;
thus (A misses ( B ^b ) implies A,B are_separated )
proof
assume A2: A misses ( B ^b );
then ( A ^b ) misses B by A1,CKB9:1;
hence thesis by A2,CKB1:def 1;
end;

end;
