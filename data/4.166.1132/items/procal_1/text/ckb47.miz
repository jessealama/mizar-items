begin
reserve p for (Element of ( CQC-WFF ));
reserve q for (Element of ( CQC-WFF ));
reserve r for (Element of ( CQC-WFF ));
theorem
Lm7: (( p => q ) in ( TAUT ) implies ( ( r 'or' p ) => ( r 'or' q ) ) in ( TAUT ))
proof
assume ( p => q ) in ( TAUT );
then A1: ( ( 'not' r ) => ( p => q ) ) in ( TAUT ) by LUKASI_1:13;
( ( ( 'not' r ) => ( p => q ) ) => ( ( ( 'not' r ) => p ) => ( ( 'not' r ) => q ) ) ) in ( TAUT ) by LUKASI_1:11;
then ( ( ( 'not' r ) => p ) => ( ( 'not' r ) => q ) ) in ( TAUT ) by A1,CQC_THE1:46;
then ( ( r 'or' p ) => ( ( 'not' r ) => q ) ) in ( TAUT ) by CKB2:1;
hence thesis by CKB2:1;
end;
