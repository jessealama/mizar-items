environ
vocabularies SUBSET_1,CQC_LANG,XBOOLEAN,CQC_THE1;
notations SUBSET_1,CQC_LANG,CQC_THE1;
theorems CQC_THE1,QC_LANG2,LUKASI_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27;
registrations CQC_LANG;
constructors CQC_THE1;
begin
reserve p for (Element of ( CQC-WFF ));
reserve q for (Element of ( CQC-WFF ));
reserve r for (Element of ( CQC-WFF ));
theorem
Th25: ( ( ( p 'or' q ) 'or' r ) => ( p 'or' ( q 'or' r ) ) ) in ( TAUT )
proof
(( ( 'not' p ) => ( ( ( 'not' r ) => q ) => ( ( 'not' q ) => r ) ) ) in ( TAUT ) & ( ( ( 'not' p ) => ( ( ( 'not' r ) => q ) => ( ( 'not' q ) => r ) ) ) => ( ( ( 'not' p ) => ( ( 'not' r ) => q ) ) => ( ( 'not' p ) => ( ( 'not' q ) => r ) ) ) ) in ( TAUT )) by LUKASI_1:11,LUKASI_1:13,LUKASI_1:31;
then A1: ( ( ( 'not' p ) => ( ( 'not' r ) => q ) ) => ( ( 'not' p ) => ( ( 'not' q ) => r ) ) ) in ( TAUT ) by CQC_THE1:46;
(( ( ( p 'or' q ) 'or' r ) => ( r 'or' ( p 'or' q ) ) ) in ( TAUT ) & ( ( r 'or' ( p 'or' q ) ) => ( ( 'not' r ) => ( p 'or' q ) ) ) in ( TAUT )) by CKB6:1,CKB9:1;
then ( ( ( p 'or' q ) 'or' r ) => ( ( 'not' r ) => ( p 'or' q ) ) ) in ( TAUT ) by LUKASI_1:3;
then A2: ( ( ( p 'or' q ) 'or' r ) => ( ( 'not' r ) => ( ( 'not' p ) => q ) ) ) in ( TAUT ) by CKB2:1;
( ( ( 'not' r ) => ( ( 'not' p ) => q ) ) => ( ( 'not' p ) => ( ( 'not' r ) => q ) ) ) in ( TAUT ) by LUKASI_1:8;
then ( ( ( p 'or' q ) 'or' r ) => ( ( 'not' p ) => ( ( 'not' r ) => q ) ) ) in ( TAUT ) by A2,LUKASI_1:3;
then ( ( ( p 'or' q ) 'or' r ) => ( ( 'not' p ) => ( ( 'not' q ) => r ) ) ) in ( TAUT ) by A1,LUKASI_1:3;
then ( ( ( p 'or' q ) 'or' r ) => ( ( 'not' p ) => ( q 'or' r ) ) ) in ( TAUT ) by CKB2:1;
hence thesis by CKB2:1;
end;
