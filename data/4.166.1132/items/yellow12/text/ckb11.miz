begin
theorem
Th9: (for L1 being Semilattice holds (for L2 being non  empty RelStr holds (for x,y being (Element of L1) holds (for x1,y1 being (Element of L2) holds (((( the RelStr of L1 ) = ( the RelStr of L2 ) & x = x1) & y = y1) implies ( x "/\" y ) = ( x1 "/\" y1 ))))))
proof
let L1 being Semilattice;
let L2 being non  empty RelStr;
let x being (Element of L1);
let y being (Element of L1);
let x1 being (Element of L2);
let y1 being (Element of L2);
assume that
A1: ( the RelStr of L1 ) = ( the RelStr of L2 )
and
A2: (x = x1 & y = y1);
A3: L2 is  with_infima  with_infima Poset by A1,WAYBEL_8:12,WAYBEL_8:13,WAYBEL_8:14,YELLOW_7:14;
A4:  ex_inf_of { x,y },L1 by YELLOW_0:21;
thus ( x "/\" y ) = ( inf { x,y } ) by YELLOW_0:40
.= ( inf { x1,y1 } ) by A1,A2,A4,YELLOW_0:27
.= ( x1 "/\" y1 ) by A3,YELLOW_0:40;
end;
