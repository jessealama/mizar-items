requirements SUBSET,BOOLE;
begin
reserve X for set;
reserve Y for set;
Lm1:now
let S being set,T being set,x1 being set,x2 being set;
assume that
A1: (x1 in S & x2 in T);
A2: ( dom <: ( pr2 (S,T) ),( pr1 (S,T) ) :> ) = ( ( dom ( pr2 (S,T) ) ) /\ ( dom ( pr1 (S,T) ) ) ) by FUNCT_3:def 7
.= ( ( dom ( pr2 (S,T) ) ) /\ [: S,T :] ) by FUNCT_3:def 4
.= ( [: S,T :] /\ [: S,T :] ) by FUNCT_3:def 5
.= [: S,T :];
[ x1,x2 ] in [: S,T :] by A1,ZFMISC_1:87;
hence ( <: ( pr2 (S,T) ),( pr1 (S,T) ) :> . (x1,x2) ) = [ ( ( pr2 (S,T) ) . (x1,x2) ),( ( pr1 (S,T) ) . (x1,x2) ) ] by A2,FUNCT_3:def 7
.= [ x2,( ( pr1 (S,T) ) . (x1,x2) ) ] by A1,FUNCT_3:def 5
.= [ x2,x1 ] by A1,FUNCT_3:def 4;
end;
theorem
(for A being (Subset of X) holds (for B being (Subset of Y) holds ( <: ( pr2 (X,Y) ),( pr1 (X,Y) ) :> .: [: A,B :] ) = [: B,A :]))
proof
let A being (Subset of X);
let B being (Subset of Y);
set f = <: ( pr2 (X,Y) ),( pr1 (X,Y) ) :>;
A1: ( dom f ) = [: X,Y :] by CKB5:1;
thus ( f .: [: A,B :] ) c= [: B,A :] by CKB6:1;
let y being set;
assume y in [: B,A :];
then consider y1 being set,y2 being set such that A2: (y1 in B & y2 in A) and A3: y = [ y1,y2 ] by ZFMISC_1:def 2;
([ y2,y1 ] in [: A,B :] & ( f . (y2,y1) ) = [ y1,y2 ]) by A2,Lm1,ZFMISC_1:87;
hence thesis by A1,A3,FUNCT_1:def 6;
end;
