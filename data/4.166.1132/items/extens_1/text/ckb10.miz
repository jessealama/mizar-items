requirements SUBSET,BOOLE;
begin
reserve I for set;
theorem
(for A,B being (ManySortedSet of I) holds (for F being (ManySortedFunction of A,B) holds (F is  "onto" iff ( rngs F ) = B)))
proof
let A being (ManySortedSet of I),B being (ManySortedSet of I);
let F being (ManySortedFunction of A,B);
A1: ( dom F ) = I by PARTFUN1:def 2;
thus (F is  "onto" implies ( rngs F ) = B)
proof
assume A2: F is  "onto";
now
let i being set;
assume A3: i in I;
then reconsider f = ( F . i ) as (Function of ( A . i ),( B . i )) by PBOOLE:def 15;
( rng f ) = ( B . i ) by A2,A3,MSUALG_3:def 3;
hence ( ( rngs F ) . i ) = ( B . i ) by A1,A3,FUNCT_6:22;
end;
hence thesis by PBOOLE:3;
end;

assume A4: ( rngs F ) = B;
let i being set;
assume i in I;
hence thesis by A1,A4,FUNCT_6:22;
end;
