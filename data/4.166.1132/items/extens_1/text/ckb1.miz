begin
theorem
Lm1: (for I being set holds (for A,B,C being (ManySortedSet of I) holds (for F being (ManySortedFunction of A,B) holds (for G being (ManySortedFunction of B,C) holds ( G ** F ) is (ManySortedSet of I)))))
proof
let I being set;
let A being (ManySortedSet of I),B being (ManySortedSet of I),C being (ManySortedSet of I);
let F being (ManySortedFunction of A,B);
let G being (ManySortedFunction of B,C);
( dom ( G ** F ) ) = ( ( dom F ) /\ ( dom G ) ) by PBOOLE:def 19
.= ( I /\ ( dom G ) ) by PARTFUN1:def 2
.= ( I /\ I ) by PARTFUN1:def 2
.= I;
hence thesis by PARTFUN1:def 2,RELAT_1:def 18;
end;
