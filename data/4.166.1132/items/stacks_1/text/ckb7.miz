theorems TARSKI,XBOOLE_1,FINSEQ_1,FINSEQ_3,FINSEQ_5,NAT_1,NAT_D,FUNCT_1,PARTFUN1,FUNCT_2,FUNCT_3,FUNCOP_1,WSIERP_1,BINOP_1,POLYALG1,EQREL_1,WELLORD2,RELSET_1,ORDINAL1,XREAL_1,NAT_2,ZFMISC_1,SETFAM_1,RELAT_1,XBOOLE_0,ORDERS_1,MATRIX_7,FUNCT_7,FILTER_1,FUNCT_4,REWRITE1,XXREAL_0,MSUALG_8,HILBERT2,RLVECT_3,FINSEQOP,FINSEQ_2,CKB3,CKB5;
schemes XBOOLE_0,RELSET_1,NAT_1,FUNCT_2,BINOP_1,RECDEF_1,ALTCAT_1,FUNCT_1,FINSEQ_1,FINSEQ_2,CKB4,CKB6;
registrations XBOOLE_0,RELSET_1,STRUCT_0,RELAT_1,FUNCT_1,FINSEQ_1,PARTFUN1,FUNCT_2,NAT_1,ORDINAL1,XXREAL_0,XREAL_0,CARD_1,EQREL_1,SUBSET_1,REWRITE1;
constructors BINOP_1,DOMAIN_1,XXREAL_0,RELSET_1,FILTER_1,FUNCT_7,REWRITE1,ABCMIZ_1,POLYNOM3,NAT_D,CKB1,CKB2,CKB5;
requirements BOOLE,SUBSET,NUMERALS,ARITHM,REAL;
begin
definition
let C being non empty set;
let D being non empty set;
let R being (Equivalence_Relation of D);
let b being (Function of [: C,D :],D);
assume A1: b is (BinOp of C,D,R);
func b /\/ R -> (Function of [: C,( Class R ) :],( Class R ))
means
:Def4: (for x,y,y1 being set holds (((x in C & y in ( Class R )) & y1 in y) implies ( it . (x,y) ) = ( Class (R,( b . (x,y1) )) )));
existence
proof
now
let X being set;
assume X in ( Class R );
then (ex x being set st (x in D & X = ( Class (R,x) ))) by EQREL_1:def 3;
hence X <> ( {} ) by EQREL_1:20;
end;
then consider g being Function such that A2: ( dom g ) = ( Class R ) and A3: (for X being set holds (X in ( Class R ) implies ( g . X ) in X)) by FUNCT_1:111;
A4: ( rng g ) c= D
proof
let x being set;
assume x in ( rng g );
then consider y being set such that A5: y in ( dom g ) and A6: x = ( g . y ) by FUNCT_1:def 3;
x in y by A2,A3,A5,A6;
hence thesis by A2,A5;
end;
deffunc F((Element of D)) = ( EqClass (R,$1) );
consider f being (Function of D,( Class R )) such that A7: (for x being (Element of D) holds ( f . x ) = F(x)) from FUNCT_2:sch 4;
reconsider g as (Function of ( Class R ),D) by A2,A4,FUNCT_2:def 1,RELSET_1:4;
deffunc F((Element of C),(Element of ( Class R ))) = ( f . ( b . ($1,( g . $2 )) ) );
consider bR being (Function of [: C,( Class R ) :],( Class R )) such that A8: (for x being (Element of C) holds (for y being (Element of ( Class R )) holds ( bR . (x,y) ) = F(x,y))) from CKB6:sch 1;
take bR;
let x being set,y being set,y1 being set;
assume that
A9: x in C
and
A10: y in ( Class R )
and
A12: y1 in y;
reconsider x9 = x as (Element of C) by A9;
reconsider y9 = y as (Element of ( Class R )) by A10;
reconsider y19 = y1 as (Element of D) by A10,A12;
A13: (ex y2 being set st (y2 in D & y9 = ( Class (R,y2) ))) by EQREL_1:def 3;
( g . y9 ) in y by A3;
then [ ( g . y9 ),y19 ] in R by A12,A13,EQREL_1:22;
then [ ( b . (x9,( g . y9 )) ),( b . (x9,y19) ) ] in R by A1,CKB5:def 1;
then A16: ( b . (x9,( g . y9 )) ) in ( EqClass (R,( b . (x9,y19) )) ) by EQREL_1:19;
A17: ( f . ( b . (x9,( g . y9 )) ) ) = ( EqClass (R,( b . (x9,( g . y9 )) )) ) by A7;
( bR . (x9,y9) ) = ( f . ( b . (x9,( g . y9 )) ) ) by A8;
hence thesis by A16,A17,EQREL_1:23;
end;
uniqueness
proof
let b1 being (Function of [: C,( Class R ) :],( Class R )),b2 being (Function of [: C,( Class R ) :],( Class R ));
assume that
A18: (for x,y,y1 being set holds (((x in C & y in ( Class R )) & y1 in y) implies ( b1 . (x,y) ) = ( Class (R,( b . (x,y1) )) )))
and
A19: (for x,y,y1 being set holds (((x in C & y in ( Class R )) & y1 in y) implies ( b2 . (x,y) ) = ( Class (R,( b . (x,y1) )) )));
now
let x being (Element of C);
let y being (Element of ( Class R ));
consider y1 being set such that A22: y1 in D and A23: y = ( Class (R,y1) ) by EQREL_1:def 3;
( b1 . (x,y) ) = ( Class (R,( b . (x,y1) )) ) by A18,A22,A23,EQREL_1:20;
hence ( b1 . (x,y) ) = ( b2 . (x,y) ) by A19,A22,A23,EQREL_1:20;
end;
hence thesis by BINOP_1:2;
end;
end;
