environ
vocabularies NUMBERS,RLVECT_1,REAL_1,PARSP_1,ANALOAF,ANALMETR,RELAT_1,ARYTM_3,SUPINF_2,CARD_1,SUBSET_1,STRUCT_0,DIRAF,ARYTM_1,PBOOLE,SYMSP_1,MCART_1,ZFMISC_1,BINOP_1,FUNCT_1,MIDSP_1,XBOOLE_0,GEOMTRAP;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,DOMAIN_1,NUMBERS,XXREAL_0,REAL_1,STRUCT_0,DIRAF,RELSET_1,RLVECT_1,MIDSP_1,AFF_1,ANALOAF,BINOP_1,ANALMETR,CKB1,CKB6,CKB27;
definitions STRUCT_0,RLVECT_1,CKB1,CKB6,CKB27;
theorems RLVECT_1,RELAT_1,DIRAF,ANALOAF,ANALMETR,AFF_1,ZFMISC_1,RLSUB_2,XCMPLX_0,XCMPLX_1,XREAL_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30;
schemes RELSET_1,BINOP_1,BINOP_2;
registrations SUBSET_1,RELSET_1,MEMBERED,STRUCT_0,ANALOAF,DIRAF,ANALMETR;
constructors BINOP_1,DOMAIN_1,XXREAL_0,REAL_1,MEMBERED,MIDSP_1,AFF_1,ANALMETR,CKB1,CKB6,CKB27;
requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve V for RealLinearSpace;
reserve u for (VECTOR of V);
reserve v1 for (VECTOR of V);
reserve v2 for (VECTOR of V);
reserve w for (VECTOR of V);
reserve y for (VECTOR of V);
theorem
Th20: (( Gen w,y & v1,u,u,v2 are_DTr_wrt w,y) implies (v1 = u & u = v2))
proof
assume that
A1:  Gen w,y
and
A2: v1,u,u,v2 are_DTr_wrt w,y;
set a = ( v1 # u );
set b = ( u # v2 );
A3: v1,u,a,b are_Ort_wrt w,y by A2,CKB27:def 1;
A4: u,v2,a,b are_Ort_wrt w,y by A2,CKB27:def 1;
A5: v1,u // u,v2 by A2,CKB27:def 1;
per cases ;
suppose v1 = v2;

hence thesis by A2,CKB30:1;
end;
suppose v1 <> v2;

then A6: a <> b by CKB9:1;
u,v2 // u,b by CKB14:1;
then A7: u,v2 '||' u,b by CKB1:def 1;
A8: a,u // u,b by A5,CKB16:1;
then u,b // a,b by CKB15:1;
then A9: u,b '||' a,b by CKB1:def 1;
A10: u = v2
proof
assume A11: u <> v2;
A12: u <> b
proof
assume u = b;
then ( u # v2 ) = ( u # u );
hence contradiction by A11,CKB9:1;
end;
u,b,a,b are_Ort_wrt w,y by A1,A4,A7,A11,CKB26:1;
then u,b,u,b are_Ort_wrt w,y by A1,A6,A9,CKB26:1;
hence contradiction by A1,A12,CKB24:1;
end;
v1,u // a,u by CKB14:1;
then A13: v1,u '||' a,u by CKB1:def 1;
a,u // a,b by A8,CKB15:1;
then A14: a,u '||' a,b by CKB1:def 1;
v1 = u
proof
assume A15: v1 <> u;
A16: u <> a
proof
assume u = a;
then ( v1 # u ) = ( u # u );
hence contradiction by A15,CKB9:1;
end;
a,u,a,b are_Ort_wrt w,y by A1,A3,A13,A15,CKB26:1;
then a,u,a,u are_Ort_wrt w,y by A1,A6,A14,CKB26:1;
hence contradiction by A1,A16,CKB24:1;
end;
hence thesis by A10;
end;
end;
