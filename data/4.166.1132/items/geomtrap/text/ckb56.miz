end;
hence (( PProJ (w,y,u,( v + v1 )) ) = ( ( PProJ (w,y,u,v) ) + ( PProJ (w,y,u,v1) ) ) & ( PProJ (w,y,u,( v - v1 )) ) = ( ( PProJ (w,y,u,v) ) - ( PProJ (w,y,u,v1) ) ));
