proof
let f being (PartFunc of ( REAL ),( REAL ));
set X = ( dom f );
assume f is  Lipschitzian;
then consider r such that A1: ( 0 ) < r and A2: (for x1 holds (for x2 holds ((x1 in X & x2 in X) implies ( abs ( ( f . x1 ) - ( f . x2 ) ) ) <= ( r * ( abs ( x1 - x2 ) ) )))) by CKB44:def 1;
now
let x0;
assume that
A3: x0 in X;
(for r holds (( 0 ) < r implies (ex s st (( 0 ) < s & (for x1 holds ((x1 in ( dom f ) & ( abs ( x1 - x0 ) ) < s) implies ( abs ( ( f . x1 ) - ( f . x0 ) ) ) < r))))))
proof
let g;
assume that
A4: ( 0 ) < g;
set s = ( g / r );
take s9 = s;
A5:now
let x1;
assume that
A6: x1 in ( dom f )
and
A7: ( abs ( x1 - x0 ) ) < s;
( r * ( abs ( x1 - x0 ) ) ) < ( ( g / r ) * r ) by A1,A7,XREAL_1:68;
then A8: ( r * ( abs ( x1 - x0 ) ) ) < g by A1,XCMPLX_1:87;
( abs ( ( f . x1 ) - ( f . x0 ) ) ) <= ( r * ( abs ( x1 - x0 ) ) ) by A2,A3,A6;
hence ( abs ( ( f . x1 ) - ( f . x0 ) ) ) < g by A8,XXREAL_0:2;
end;
s9 = ( g * ( r " ) ) by XCMPLX_0:def 9;
hence thesis by A1,A4,A5,XREAL_1:129;
end;
hence f is_continuous_in x0 by CKB4:1;
end;
hence thesis by CKB14:def 1;
