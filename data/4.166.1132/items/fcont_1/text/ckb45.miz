let x1;
let x2;
assume A6: (x1 in ( dom ( f | X ) ) & x2 in ( dom ( f | X ) ));
then (( ( f | X ) . x1 ) = ( f . x1 ) & ( ( f | X ) . x2 ) = ( f . x2 )) by FUNCT_1:47;
hence thesis by A5,A6;
end;
