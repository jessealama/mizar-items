begin
reserve s1 for Real_Sequence;
reserve f1 for (PartFunc of ( REAL ),( REAL ));
reserve f2 for (PartFunc of ( REAL ),( REAL ));
registration
let f1 being  continuous (PartFunc of ( REAL ),( REAL ));
let f2 being  continuous (PartFunc of ( REAL ),( REAL ));
cluster ( f1 - f2 ) ->  continuous for (PartFunc of ( REAL ),( REAL ));
coherence
proof
set X = ( dom ( f1 - f2 ) );
X c= ( ( dom f1 ) /\ ( dom f2 ) ) by VALUED_1:12;
then A9: (X c= ( dom f1 ) & X c= ( dom f2 )) by XBOOLE_1:18;
A10: (( f1 | X ) is  continuous & ( f2 | X ) is  continuous);
now
let s1;
assume that
A11: ( rng s1 ) c= X
and
A12: s1 is  convergent
and
A13: ( lim s1 ) in X;
A14: (( f1 /* s1 ) is  convergent & ( f2 /* s1 ) is  convergent) by A9,A10,A11,A12,A13,CKB15:1;
then A15: ( ( f1 /* s1 ) - ( f2 /* s1 ) ) is  convergent by SEQ_2:11;
A16: ( rng s1 ) c= ( ( dom f1 ) /\ ( dom f2 ) ) by A11,VALUED_1:12;
(( f1 . ( lim s1 ) ) = ( lim ( f1 /* s1 ) ) & ( f2 . ( lim s1 ) ) = ( lim ( f2 /* s1 ) )) by A9,A10,A11,A12,A13,CKB15:1;
then ( ( f1 - f2 ) . ( lim s1 ) ) = ( ( lim ( f1 /* s1 ) ) - ( lim ( f2 /* s1 ) ) ) by A13,VALUED_1:13
.= ( lim ( ( f1 /* s1 ) - ( f2 /* s1 ) ) ) by A14,SEQ_2:12
.= ( lim ( ( f1 - f2 ) /* s1 ) ) by A16,RFUNCT_2:8;
hence (( ( f1 - f2 ) /* s1 ) is  convergent & ( ( f1 - f2 ) . ( lim s1 ) ) = ( lim ( ( f1 - f2 ) /* s1 ) )) by A16,A15,RFUNCT_2:8;
end;
then ( ( f1 - f2 ) | X ) is  continuous by CKB15:1;
hence thesis by RELAT_1:69;
end;
