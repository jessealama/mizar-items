constructors PARTFUN1,REAL_1,SQUARE_1,NAT_1,COMPLEX1,VALUED_1,SEQ_2,SEQM_3,SEQ_4,RCOMP_1,PARTFUN2,RFUNCT_1,RFUNCT_2,SEQ_1,RECDEF_1,RELSET_1,CKB1;
requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve x0 for  real number;
reserve f for (PartFunc of ( REAL ),( REAL ));
theorem
((ex N being (Neighbourhood of x0) st ( ( dom f ) /\ N ) = { x0 }) implies f is_continuous_in x0)
proof
given N being (Neighbourhood of x0) such that
A1: ( ( dom f ) /\ N ) = { x0 };

x0 in ( ( dom f ) /\ N ) by A1,TARSKI:def 1;
then A2: x0 in ( dom f ) by XBOOLE_0:def 4;
now
let N1 being (Neighbourhood of ( f . x0 ));
take N;
A3: ( f . x0 ) in N1 by RCOMP_1:16;
( f .: N ) = ( Im (f,x0) ) by A1,RELAT_1:112
.= { ( f . x0 ) } by A2,FUNCT_1:59;
hence ( f .: N ) c= N1 by A3,ZFMISC_1:31;
end;
hence thesis by CKB6:1;
end;
