constructors PARTFUN1,REAL_1,SQUARE_1,NAT_1,COMPLEX1,VALUED_1,SEQ_2,SEQM_3,SEQ_4,RCOMP_1,PARTFUN2,RFUNCT_1,RFUNCT_2,SEQ_1,RECDEF_1,RELSET_1,CKB1,CKB14;
requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve r for  real number;
reserve x0 for  real number;
reserve x1 for  real number;
reserve f for (PartFunc of ( REAL ),( REAL ));
registration
cluster  constant ->  continuous for (PartFunc of ( REAL ),( REAL ));
coherence
proof
let f being (PartFunc of ( REAL ),( REAL ));
assume A1: f is  constant;
now
reconsider s = 1 as  real number;
let x0;
let r;
assume that
A2: x0 in ( dom f )
and
A3: ( 0 ) < r;
take s;
thus ( 0 ) < s;
let x1;
assume A4: x1 in ( dom f );
assume ( abs ( x1 - x0 ) ) < s;
( f . x1 ) = ( f . x0 ) by A1,A2,A4,FUNCT_1:def 10;
hence ( abs ( ( f . x1 ) - ( f . x0 ) ) ) < r by A3,ABSVALUE:2;
end;
then ( f | ( dom f ) ) is  continuous by CKB16:1;
hence thesis by RELAT_1:69;
end;
end;
