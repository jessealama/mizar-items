registrations ORDINAL1,RELSET_1,NUMBERS,XXREAL_0,XREAL_0,NAT_1,MEMBERED,VALUED_0,VALUED_1,FUNCT_2,SEQ_4;
constructors REAL_1,NAT_1,COMPLEX1,SEQ_2,SEQM_3,PROB_1,RCOMP_1,PARTFUN1,RFUNCT_1,RFUNCT_2,LIMFUNC1,LIMFUNC2,VALUED_1,SEQ_1,RECDEF_1,RELSET_1;
requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve r for Real;
reserve x0 for Real;
reserve n for (Element of ( NAT ));
reserve k for (Element of ( NAT ));
reserve seq for Real_Sequence;
reserve f for (PartFunc of ( REAL ),( REAL ));
theorem
Th3: (((seq is  convergent & ( lim seq ) = x0) & ( rng seq ) c= ( ( dom f ) \ { x0 } )) implies (for r holds (( 0 ) < r implies (ex n st (for k holds (n <= k implies ((( 0 ) < ( abs ( x0 - ( seq . k ) ) ) & ( abs ( x0 - ( seq . k ) ) ) < r) & ( seq . k ) in ( dom f ))))))))
proof
assume that
A1: seq is  convergent
and
A2: ( lim seq ) = x0
and
A3: ( rng seq ) c= ( ( dom f ) \ { x0 } );
let r;
assume ( 0 ) < r;
then consider n such that A4: (for k holds (n <= k implies ( abs ( ( seq . k ) - x0 ) ) < r)) by A1,A2,SEQ_2:def 7;
take n;
let k;
assume n <= k;
then ( abs ( ( seq . k ) - x0 ) ) < r by A4;
then A5: ( abs ( - ( x0 - ( seq . k ) ) ) ) < r;
now
let n;
( seq . n ) in ( rng seq ) by VALUED_0:28;
then (not ( seq . n ) in { x0 }) by A3,XBOOLE_0:def 5;
hence ( ( seq . n ) - x0 ) <> ( 0 ) by TARSKI:def 1;
end;
then ( ( seq . k ) - x0 ) <> ( 0 );
then ( 0 ) < ( abs ( - ( x0 - ( seq . k ) ) ) ) by COMPLEX1:47;
hence ( 0 ) < ( abs ( x0 - ( seq . k ) ) ) by COMPLEX1:52;
thus ( abs ( x0 - ( seq . k ) ) ) < r by A5,COMPLEX1:52;
( seq . k ) in ( rng seq ) by VALUED_0:28;
hence thesis by A3,XBOOLE_0:def 5;
end;
