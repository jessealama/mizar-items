requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve r for Real;
reserve r1 for Real;
reserve r2 for Real;
reserve g for Real;
reserve g1 for Real;
reserve g2 for Real;
reserve x0 for Real;
reserve f for (PartFunc of ( REAL ),( REAL ));
theorem
Th33: (f is_convergent_in x0 implies ((((f is_left_convergent_in x0 & f is_right_convergent_in x0) & ( lim_left (f,x0) ) = ( lim_right (f,x0) )) & ( lim (f,x0) ) = ( lim_left (f,x0) )) & ( lim (f,x0) ) = ( lim_right (f,x0) )))
proof
assume A1: f is_convergent_in x0;
A2: ( lim (f,x0) ) = ( lim (f,x0) );
A3:now
let s being Real_Sequence;
assume that
A4: s is  convergent
and
A5: ( lim s ) = x0
and
A6: ( rng s ) c= ( ( dom f ) /\ ( right_open_halfline x0 ) );
( rng s ) c= ( ( dom f ) \ { x0 } ) by A6,CKB5:1;
hence (( f /* s ) is  convergent & ( lim ( f /* s ) ) = ( lim (f,x0) )) by A1,A2,A4,A5,CKB35:def 1;
end;
A7:now
let s being Real_Sequence;
assume that
A8: s is  convergent
and
A9: ( lim s ) = x0
and
A10: ( rng s ) c= ( ( dom f ) /\ ( left_open_halfline x0 ) );
( rng s ) c= ( ( dom f ) \ { x0 } ) by A10,CKB5:1;
hence (( f /* s ) is  convergent & ( lim ( f /* s ) ) = ( lim (f,x0) )) by A1,A2,A8,A9,CKB35:def 1;
end;
A11: (for r1 holds (for r2 holds ((r1 < x0 & x0 < r2) implies (ex g1 st (ex g2 st (((((r1 < g1 & g1 < x0) & g1 in ( dom f )) & g2 < r2) & x0 < g2) & g2 in ( dom f ))))))) by A1,CKB13:def 1;
then (for r holds (r < x0 implies (ex g st ((r < g & g < x0) & g in ( dom f ))))) by CKB12:1;
hence f is_left_convergent_in x0 by A7,LIMFUNC2:def 1;
then A12: ( lim_left (f,x0) ) = ( lim (f,x0) ) by A7,LIMFUNC2:def 7;
(for r holds (x0 < r implies (ex g st ((g < r & x0 < g) & g in ( dom f ))))) by A11,CKB12:1;
hence f is_right_convergent_in x0 by A3,LIMFUNC2:def 4;
hence thesis by A12,A3,LIMFUNC2:def 8;
end;
