registrations ORDINAL1,RELSET_1,NUMBERS,XXREAL_0,XREAL_0,NAT_1,MEMBERED,VALUED_0,VALUED_1,FUNCT_2,SEQ_4;
constructors REAL_1,NAT_1,COMPLEX1,SEQ_2,SEQM_3,PROB_1,RCOMP_1,PARTFUN1,RFUNCT_1,RFUNCT_2,LIMFUNC1,LIMFUNC2,VALUED_1,SEQ_1,RECDEF_1,RELSET_1;
requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve g for Real;
reserve x0 for Real;
reserve n for (Element of ( NAT ));
reserve k for (Element of ( NAT ));
reserve seq for Real_Sequence;
theorem
Th7: (((seq is  convergent & ( lim seq ) = x0) & ( 0 ) < g) implies (ex k st (for n holds (k <= n implies (( x0 - g ) < ( seq . n ) & ( seq . n ) < ( x0 + g ))))))
proof
assume that
A1: seq is  convergent
and
A2: ( lim seq ) = x0
and
A3: ( 0 ) < g;
( x0 - g ) < ( lim seq ) by A2,A3,CKB1:1;
then consider k1 being (Element of ( NAT )) such that A4: (for n holds (k1 <= n implies ( x0 - g ) < ( seq . n ))) by A1,LIMFUNC2:1;
( lim seq ) < ( x0 + g ) by A2,A3,CKB1:1;
then consider k2 being (Element of ( NAT )) such that A5: (for n holds (k2 <= n implies ( seq . n ) < ( x0 + g ))) by A1,LIMFUNC2:2;
take k = ( max (k1,k2) );
let n;
assume A6: k <= n;
k1 <= k by XXREAL_0:25;
then k1 <= n by A6,XXREAL_0:2;
hence ( x0 - g ) < ( seq . n ) by A4;
k2 <= k by XXREAL_0:25;
then k2 <= n by A6,XXREAL_0:2;
hence thesis by A5;
end;
