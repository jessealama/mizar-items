begin
reserve fi for Ordinal-Sequence;
reserve A for Ordinal;
reserve A1 for Ordinal;
reserve B for Ordinal;
reserve D for Ordinal;
theorem
Lm7: (A <> ( {} ) implies (for fi holds ((( dom fi ) = A & (for B holds (B in A implies ( fi . B ) = ( exp (1,B) )))) implies 1 is_limes_of fi)))
proof
assume that
A1: A <> ( {} );
let fi;
assume that
A2: ( dom fi ) = A
and
A3: (for B holds (B in A implies ( fi . B ) = ( exp (1,B) )));
per cases ;
case 1 = ( {} );
hence thesis;
end;
case 1 <> ( {} );
let A1 being Ordinal;
let A2 being Ordinal;
assume that
A4: A1 in 1
and
A5: 1 in A2;
take B = ( {} );
thus B in ( dom fi ) by A1,A2,ORDINAL3:8;
let D;
assume that
B c= D
and
A6: D in ( dom fi );
( exp (1,D) ) = ( fi . D ) by A2,A3,A6;
hence thesis by A4,A5,ORDINAL2:46;
end;
end;
