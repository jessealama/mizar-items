theorem
Lm6: ((A <> ( {} ) & A is  limit_ordinal) implies (for fi holds ((( dom fi ) = A & (for B holds (B in A implies ( fi . B ) = ( exp (( {} ),B) )))) implies ( {} ) is_limes_of fi)))
proof
assume that
A1: A <> ( {} )
and
A2: A is  limit_ordinal;
let fi;
assume that
A3: ( dom fi ) = A
and
A4: (for B holds (B in A implies ( fi . B ) = ( exp (( {} ),B) )));
per cases ;
case ( {} ) = ( {} );
take B = 1;
( {} ) in A by A1,ORDINAL3:8;
hence B in ( dom fi ) by A2,A3,CKB3:1,ORDINAL1:28;
let D;
assume that
A5: B c= D
and
A6: D in ( dom fi );
A7: D <> ( {} ) by A5,CKB3:1,ORDINAL1:21;
( exp (( {} ),D) ) = ( fi . D ) by A3,A4,A6;
hence thesis by A7,CKB28:1;
end;
case ( {} ) <> ( {} );
thus thesis;
end;
