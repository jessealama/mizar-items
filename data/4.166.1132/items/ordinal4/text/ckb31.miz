environ
vocabularies ORDINAL2,ORDINAL1,FUNCT_1,XBOOLE_0,RELAT_1,TARSKI,ORDINAL3,SUBSET_1,CLASSES2,ZFMISC_1,CARD_1,ORDINAL4;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,RELAT_1,FUNCT_1,FUNCT_2,ORDINAL1,ORDINAL2,ORDINAL3,CARD_1,CLASSES2,CKB6;
definitions ORDINAL1,TARSKI,ORDINAL2,XBOOLE_0,RELAT_1,CKB6;
theorems ZFMISC_1,FUNCT_1,GRFUNC_1,ORDINAL1,ORDINAL2,ORDINAL3,CARD_2,CLASSES2,RELAT_1,CLASSES1,XBOOLE_0,XBOOLE_1,FUNCT_2,RELSET_1,CARD_1,CKB1,CKB2,CKB3,CKB5,CKB6,CKB7,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30;
schemes ORDINAL1,ORDINAL2,FINSET_1;
registrations XBOOLE_0,FUNCT_1,ORDINAL1,ORDINAL2,ORDINAL3,CARD_1,CLASSES2,RELSET_1,CKB4,CKB8;
constructors WELLORD2,FUNCOP_1,ORDINAL3,CARD_1,CLASSES1,CLASSES2,RELSET_1,CKB6;
requirements SUBSET,BOOLE,NUMERALS;
begin
reserve fi for Ordinal-Sequence;
reserve psi for Ordinal-Sequence;
reserve A for Ordinal;
reserve A1 for Ordinal;
reserve B for Ordinal;
reserve C for Ordinal;
reserve D for Ordinal;
theorem
Lm8: (for A holds ((A <> ( {} ) & A is  limit_ordinal) implies (ex fi st ((( dom fi ) = A & (for B holds (B in A implies ( fi . B ) = ( exp (C,B) )))) & (ex D st D is_limes_of fi)))))
proof
defpred P[ Ordinal ]
 means
(($1 <> ( {} ) & $1 is  limit_ordinal) & (for fi holds ((( dom fi ) = $1 & (for B holds (B in $1 implies ( fi . B ) = ( exp (C,B) )))) implies (for D holds (not D is_limes_of fi)))));
let A;
assume that
A1: A <> ( {} )
and
A2: A is  limit_ordinal
and
A3: (for fi holds ((( dom fi ) = A & (for B holds (B in A implies ( fi . B ) = ( exp (C,B) )))) implies (for D holds (not D is_limes_of fi))));
deffunc F(Ordinal) = ( exp (C,$1) );
A4: (ex A st P[ A ]) by A1,A2,A3;
consider A such that A5: P[ A ] and A6: (for A1 holds (P[ A1 ] implies A c= A1)) from ORDINAL1:sch 1(A4);
consider fi such that A7: (( dom fi ) = A & (for B holds (B in A implies ( fi . B ) = F(B)))) from ORDINAL2:sch 3;
A8:now
assume (C = ( {} ) or C = 1);
then (( {} ) is_limes_of fi or 1 is_limes_of fi) by A5,A7,CKB29:1,CKB30:1;
hence contradiction by A5,A7;
end;
then ( {} ) in C by ORDINAL3:8;
then 1 c= C by CKB3:1,ORDINAL1:21;
then A9: 1 c< C by A8,XBOOLE_0:def 8;
A10: (for B2,B1 being Ordinal holds ((B1 in B2 & B2 in A) implies ( exp (C,B1) ) in ( exp (C,B2) )))
proof
defpred V[ Ordinal ]
 means
(for B1 being Ordinal holds ((B1 in $1 & $1 in A) implies ( exp (C,B1) ) in ( exp (C,$1) )));
A11: (V[ B ] implies V[ ( succ B ) ])
proof
assume A12: (for B1 being Ordinal holds ((B1 in B & B in A) implies ( exp (C,B1) ) in ( exp (C,B) )));
let B1 being Ordinal;
assume that
A13: B1 in ( succ B )
and
A14: ( succ B ) in A;
A15: B1 c= B by A13,ORDINAL1:22;
B in ( succ B ) by ORDINAL1:6;
then A16: B in A by A14,ORDINAL1:10;
A17: ( 1 *^ ( exp (C,B) ) ) = ( exp (C,B) ) by ORDINAL2:39;
A18: ( exp (C,B) ) <> ( {} )
proof
now
per cases ;
suppose B = ( {} );

hence thesis by ORDINAL2:43;
end;
suppose A19: B <> ( {} );

B in ( succ B ) by ORDINAL1:6;
then B in A by A14,ORDINAL1:10;
hence thesis by A12,A19,ORDINAL3:8;
end;
end;
hence thesis;
end;
A20: ( exp (C,( succ B )) ) = ( C *^ ( exp (C,B) ) ) by ORDINAL2:44;
then A21: ( exp (C,B) ) in ( exp (C,( succ B )) ) by A9,A18,A17,ORDINAL1:11,ORDINAL2:40;
now
assume B1 <> B;
then B1 c< B by A15,XBOOLE_0:def 8;
then B1 in B by ORDINAL1:11;
then ( exp (C,B1) ) in ( exp (C,B) ) by A12,A16;
hence thesis by A21,ORDINAL1:10;
end;
hence thesis by A9,A18,A20,A17,ORDINAL1:11,ORDINAL2:40;
end;
A22: (for B holds (((B <> ( {} ) & B is  limit_ordinal) & (for D holds (D in B implies V[ D ]))) implies V[ B ]))
proof
let B;
assume that
A23: B <> ( {} )
and
A24: B is  limit_ordinal
and
A25: (for D holds (D in B implies V[ D ]));
let B1 being Ordinal;
assume that
A26: B1 in B
and
A27: B in A;
consider psi such that A28: ( dom psi ) = B and A29: (for D holds (D in B implies ( psi . D ) = ( exp (C,D) ))) and (ex D st D is_limes_of psi) by A6,A24,A26,A27,ORDINAL1:5;
( psi . B1 ) = ( exp (C,B1) ) by A26,A29;
then A30: ( exp (C,B1) ) in ( rng psi ) by A26,A28,FUNCT_1:def 3;
psi is  increasing
proof
let B1 being Ordinal;
let B2 being Ordinal;
assume that
A31: B1 in B2
and
A32: B2 in ( dom psi );
B2 in A by A27,A28,A32,ORDINAL1:10;
then A33: ( exp (C,B1) ) in ( exp (C,B2) ) by A25,A28,A31,A32;
( psi . B1 ) = ( exp (C,B1) ) by A28,A29,A31,A32,ORDINAL1:10;
hence thesis by A28,A29,A32,A33;
end;
then A34: ( lim psi ) = ( sup psi ) by A23,A24,A28,CKB15:1;
( exp (C,B) ) = ( lim psi ) by A23,A24,A28,A29,ORDINAL2:45;
hence thesis by A34,A30,ORDINAL2:19;
end;
A35: V[ ( {} ) ];
thus (for B holds V[ B ]) from ORDINAL2:sch 1(A35,A11,A22);
end;
fi is  increasing
proof
let B1 being Ordinal;
let B2 being Ordinal;
assume that
A36: B1 in B2
and
A37: B2 in ( dom fi );
A38: ( fi . B1 ) = ( exp (C,B1) ) by A7,A36,A37,ORDINAL1:10;
( exp (C,B1) ) in ( exp (C,B2) ) by A7,A10,A36,A37;
hence thesis by A7,A37,A38;
end;
then ( sup fi ) is_limes_of fi by A5,A7,CKB15:1;
hence contradiction by A5,A7;
end;
