environ
vocabularies XBOOLE_0,LATTICES,ORDERS_2,ZFMISC_1,STRUCT_0,TARSKI,SUBSET_1,LATTICE3,RELAT_2,XXREAL_0,YELLOW_0,REWRITE1,ORDINAL2,RELAT_1,MCART_1,EQREL_1,XXREAL_2,WAYBEL_0,WAYBEL_3,RCOMP_1,WAYBEL_8,WAYBEL_6,CARD_FIL,WAYBEL11,WAYBEL_2;
notations TARSKI,XBOOLE_0,ZFMISC_1,DOMAIN_1,STRUCT_0,MCART_1,ORDERS_2,LATTICE3,YELLOW_0,WAYBEL_0,WAYBEL_1,YELLOW_3,YELLOW_4,WAYBEL_2,WAYBEL_3,WAYBEL_6,WAYBEL_8,WAYBEL11,CKB19;
definitions TARSKI,YELLOW_0,LATTICE3,WAYBEL_0,WAYBEL_1,WAYBEL_2,WAYBEL_3,WAYBEL_6,WAYBEL_8,WAYBEL11,XBOOLE_0;
theorems ZFMISC_1,TARSKI,ORDERS_2,YELLOW_0,YELLOW_3,YELLOW_4,WAYBEL_0,WAYBEL_1,WAYBEL_2,WAYBEL_3,WAYBEL_8,MCART_1,FUNCT_5,DOMAIN_1,XBOOLE_0,RELAT_1,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27;
registrations XBOOLE_0,SUBSET_1,STRUCT_0,LATTICE3,YELLOW_0,WAYBEL_0,YELLOW_3,YELLOW_4,WAYBEL_2,WAYBEL_3,WAYBEL_8,WAYBEL14,RELAT_1,CKB1,CKB2;
constructors DOMAIN_1,WAYBEL_1,YELLOW_4,WAYBEL_3,WAYBEL_6,WAYBEL_8,WAYBEL11,WAYBEL_2,CKB19;
requirements SUBSET,BOOLE;
begin
theorem
(for S,T being  with_suprema  antisymmetric RelStr holds (for X,Y being (Subset of [: S,T :]) holds (( proj1 ( X "\/" Y ) ) = ( ( proj1 X ) "\/" ( proj1 Y ) ) & ( proj2 ( X "\/" Y ) ) = ( ( proj2 X ) "\/" ( proj2 Y ) ))))
proof
let S being  with_suprema  antisymmetric RelStr;
let T being  with_suprema  antisymmetric RelStr;
let X being (Subset of [: S,T :]);
let Y being (Subset of [: S,T :]);
A1: (the carrier of [: S,T :]) = [: (the carrier of S),(the carrier of T) :] by YELLOW_3:def 2;
A2: ( X "\/" Y ) = { ( x "\/" y ) where x is (Element of [: S,T :]),y is (Element of [: S,T :]): (x in X & y in Y) } by YELLOW_4:def 3;
A3: ( ( proj1 X ) "\/" ( proj1 Y ) ) = { ( x "\/" y ) where x is (Element of S),y is (Element of S): (x in ( proj1 X ) & y in ( proj1 Y )) } by YELLOW_4:def 3;
hereby
hereby
let a being set;
assume a in ( proj1 ( X "\/" Y ) );
then consider b being set such that A4: [ a,b ] in ( X "\/" Y ) by RELAT_1:def 4;
consider x,y being (Element of [: S,T :]) such that A5: [ a,b ] = ( x "\/" y ) and A6: x in X and A7: y in Y by A2,A4;
x = [ ( x `1 ),( x `2 ) ] by A1,MCART_1:21;
then A8: ( x `1 ) in ( proj1 X ) by A6,FUNCT_5:2;
y = [ ( y `1 ),( y `2 ) ] by A1,MCART_1:21;
then A9: ( y `1 ) in ( proj1 Y ) by A7,FUNCT_5:2;
a = ( [ a,b ] `1 ) by MCART_1:7
.= ( ( x `1 ) "\/" ( y `1 ) ) by A5,CKB16:1;
hence a in ( ( proj1 X ) "\/" ( proj1 Y ) ) by A8,A9,YELLOW_4:10;
end;
let a being set;
assume a in ( ( proj1 X ) "\/" ( proj1 Y ) );
then consider x,y being (Element of S) such that A10: a = ( x "\/" y ) and A11: x in ( proj1 X ) and A12: y in ( proj1 Y ) by A3;
consider x2 being set such that A13: [ x,x2 ] in X by A11,RELAT_1:def 4;
consider y2 being set such that A14: [ y,y2 ] in Y by A12,RELAT_1:def 4;
reconsider x2,y2 as (Element of T) by A1,A13,A14,ZFMISC_1:87;
( [ x,x2 ] "\/" [ y,y2 ] ) = [ a,( x2 "\/" y2 ) ] by A10,CKB18:1;
then [ a,( x2 "\/" y2 ) ] in ( X "\/" Y ) by A13,A14,YELLOW_4:10;
hence a in ( proj1 ( X "\/" Y ) ) by RELAT_1:def 4;
end;
hereby
let b being set;
assume b in ( proj2 ( X "\/" Y ) );
then consider a being set such that A15: [ a,b ] in ( X "\/" Y ) by RELAT_1:def 5;
consider x,y being (Element of [: S,T :]) such that A16: [ a,b ] = ( x "\/" y ) and A17: x in X and A18: y in Y by A2,A15;
x = [ ( x `1 ),( x `2 ) ] by A1,MCART_1:21;
then A19: ( x `2 ) in ( proj2 X ) by A17,FUNCT_5:2;
y = [ ( y `1 ),( y `2 ) ] by A1,MCART_1:21;
then A20: ( y `2 ) in ( proj2 Y ) by A18,FUNCT_5:2;
b = ( [ a,b ] `2 ) by MCART_1:7
.= ( ( x `2 ) "\/" ( y `2 ) ) by A16,CKB16:1;
hence b in ( ( proj2 X ) "\/" ( proj2 Y ) ) by A19,A20,YELLOW_4:10;
end;
let b being set;
A21: ( ( proj2 X ) "\/" ( proj2 Y ) ) = { ( x "\/" y ) where x is (Element of T),y is (Element of T): (x in ( proj2 X ) & y in ( proj2 Y )) } by YELLOW_4:def 3;
assume b in ( ( proj2 X ) "\/" ( proj2 Y ) );
then consider x,y being (Element of T) such that A22: b = ( x "\/" y ) and A23: x in ( proj2 X ) and A24: y in ( proj2 Y ) by A21;
consider x1 being set such that A25: [ x1,x ] in X by A23,RELAT_1:def 5;
consider y1 being set such that A26: [ y1,y ] in Y by A24,RELAT_1:def 5;
reconsider x1,y1 as (Element of S) by A1,A25,A26,ZFMISC_1:87;
( [ x1,x ] "\/" [ y1,y ] ) = [ ( x1 "\/" y1 ),b ] by A22,CKB18:1;
then [ ( x1 "\/" y1 ),b ] in ( X "\/" Y ) by A25,A26,YELLOW_4:10;
hence thesis by RELAT_1:def 5;
end;
