begin
reserve n for (Element of ( NAT ));
reserve X for set;
definition
let X;
let A1 being (SetSequence of X);
let A2 being (SetSequence of X);
func A1 (\+\) A2 -> (SetSequence of X)
means
:Def4: (for n holds ( it . n ) = ( ( A1 . n ) \+\ ( A2 . n ) ));
existence
proof
deffunc F((Element of ( NAT ))) = ( ( A1 . $1 ) \+\ ( A2 . $1 ) );
(ex f being (SetSequence of X) st (for n holds ( f . n ) = F(n))) from FUNCT_2:sch 4;
hence thesis;
end;
uniqueness
proof
let A3 being (SetSequence of X);
let A4 being (SetSequence of X);
assume that
A7: (for n holds ( A3 . n ) = ( ( A1 . n ) \+\ ( A2 . n ) ))
and
A8: (for n holds ( A4 . n ) = ( ( A1 . n ) \+\ ( A2 . n ) ));
let n;
thus ( A3 . n ) = ( ( A1 . n ) \+\ ( A2 . n ) ) by A7
.= ( A4 . n ) by A8;
end;
commutativity;
end;
