environ
vocabularies SUBSET_1,NUMBERS,PROB_1,SETLIM_1,FUNCT_1,EQREL_1,NAT_1,XXREAL_0,SETFAM_1,RELAT_1,CARD_3,TARSKI,XBOOLE_0,ARYTM_3,CARD_1,SEQM_3,ORDINAL2,SEQ_2,SETLIM_2;
notations TARSKI,SUBSET_1,FUNCT_1,NUMBERS,NAT_1,SETFAM_1,KURATO_0,PROB_1,SETLIM_1,XXREAL_0;
definitions TARSKI,XBOOLE_0,FUNCT_2;
theorems XBOOLE_0,XBOOLE_1,KURATO_0,SETLIM_1,CARD_3,PROB_1,NAT_1;
