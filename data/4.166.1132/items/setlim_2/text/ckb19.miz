environ
vocabularies SUBSET_1,NUMBERS,PROB_1,SETLIM_1,FUNCT_1,EQREL_1,NAT_1,XXREAL_0,SETFAM_1,RELAT_1,CARD_3,TARSKI,XBOOLE_0,ARYTM_3,CARD_1,SEQM_3,ORDINAL2,SEQ_2,SETLIM_2;
notations TARSKI,SUBSET_1,FUNCT_1,NUMBERS,NAT_1,SETFAM_1,KURATO_0,PROB_1,SETLIM_1,XXREAL_0,CKB3,CKB4,CKB5,CKB6;
definitions TARSKI,XBOOLE_0,FUNCT_2,CKB3,CKB4,CKB5,CKB6;
theorems XBOOLE_0,XBOOLE_1,KURATO_0,SETLIM_1,CARD_3,PROB_1,NAT_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18;
schemes FUNCT_2;
registrations SUBSET_1,RELSET_1,XREAL_0,ORDINAL1,FUNCT_2;
constructors SETFAM_1,NAT_1,KURATO_0,SETLIM_1,XREAL_0,RELSET_1,CKB3,CKB4,CKB5,CKB6;
requirements NUMERALS,SUBSET,ARITHM;
begin
reserve n for (Element of ( NAT ));
reserve X for set;
reserve A for (Subset of X);
reserve A1 for (SetSequence of X);
definition
let X;
let A1 being (SetSequence of X);
let A being (Subset of X);
func A (/\) A1 -> (SetSequence of X) means 
:Def5: (for n holds ( it . n ) = ( A /\ ( A1 . n ) ));
existence
proof
deffunc F((Element of ( NAT ))) = ( A /\ ( A1 . $1 ) );
(ex f being (SetSequence of X) st (for n holds ( f . n ) = F(n))) from FUNCT_2:sch 4;
hence thesis;
end;
uniqueness
proof
let A3 being (SetSequence of X);
let A4 being (SetSequence of X);
assume that
A1: (for n holds ( A3 . n ) = ( A /\ ( A1 . n ) ))
and
A2: (for n holds ( A4 . n ) = ( A /\ ( A1 . n ) ));
let n;
thus ( A3 . n ) = ( A /\ ( A1 . n ) ) by A1
.= ( A4 . n ) by A2;
end;
end;
