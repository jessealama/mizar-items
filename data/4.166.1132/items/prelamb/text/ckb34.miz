begin
theorem
Lm1: (for D being non empty set holds (for T being (FinSequence of D) holds (for f being (Function of D,( NAT )) holds ( Sum ( f * T ) ) is Nat)))
proof
let D being non empty set;
let T being (FinSequence of D);
let f being (Function of D,( NAT ));
defpred P[ (FinSequence of ( REAL )) ]
 means
($1 is (FinSequence of ( NAT )) implies ( Sum $1 ) is Nat);
A1: P[ ( <*> ( REAL ) ) ] by RVSUM_1:72;
A2: (for p being (FinSequence of ( REAL )) holds (for x being Real holds (P[ p ] implies P[ ( p ^ <* x *> ) ])))
proof
let p being (FinSequence of ( REAL ));
let x being Real;
assume A3: P[ p ];
assume ( p ^ <* x *> ) is (FinSequence of ( NAT ));
then A4: ( rng ( p ^ <* x *> ) ) c= ( NAT ) by FINSEQ_1:def 4;
( rng p ) c= ( rng ( p ^ <* x *> ) ) by FINSEQ_1:29;
then A5: ( rng p ) c= ( NAT ) by A4,XBOOLE_1:1;
( rng <* x *> ) c= ( rng ( p ^ <* x *> ) ) by FINSEQ_1:30;
then A6: ( rng <* x *> ) c= ( NAT ) by A4,XBOOLE_1:1;
( rng <* x *> ) = { x } by FINSEQ_1:38;
then reconsider n = x as (Element of ( NAT )) by A6,ZFMISC_1:31;
reconsider s = ( Sum p ) as Nat by A3,A5,FINSEQ_1:def 4;
( Sum ( p ^ <* x *> ) ) = ( s + n ) by RVSUM_1:74;
hence thesis;
end;
A7: (for p being (FinSequence of ( REAL )) holds P[ p ]) from FINSEQ_2:sch 2(A1,A2);
( f * T ) is (FinSequence of ( NAT )) by FINSEQ_2:32;
hence thesis by A7;
end;
