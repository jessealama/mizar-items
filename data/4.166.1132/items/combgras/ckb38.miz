constructors SETFAM_1,WELLORD2,DOMAIN_1,XXREAL_0,REAL_1,NAT_1,BINOP_2,INCPROJ,RELSET_1,CKB12,CKB17,CKB18,CKB19,CKB20,CKB23,CKB24,CKB25,CKB27,CKB29,CKB30,CKB31,CKB32,CKB33;
requirements BOOLE,SUBSET,NUMERALS,ARITHM,REAL;
begin
theorem
Th22: (for S being IncProjStr holds (for F being IncProjMap over S,S holds (for K being (Subset of (the Points of S)) holds ((F is  automorphism & K is  maximal_clique) implies (( F .: K ) is  maximal_clique & ( F " K ) is  maximal_clique)))))
proof
let S being IncProjStr;
let F being IncProjMap over S,S;
let K being (Subset of (the Points of S));
assume that
A1: F is  automorphism
and
A2: K is  maximal_clique;
A3: F is  incidence_preserving by A1,CKB29:def 1;
(the point-map of F) is  bijective by A1,CKB29:def 1;
then A4: (the Points of S) = ( rng (the point-map of F) ) by FUNCT_2:def 3;
A5: (the Points of S) = ( dom (the point-map of F) ) by FUNCT_2:52;
A6: (for U being (Subset of (the Points of S)) holds ((U is  clique & ( F " K ) c= U) implies U = ( F " K )))
proof
let U being (Subset of (the Points of S));
assume that
A7: U is  clique
and
A8: ( F " K ) c= U;
( F .: ( F " K ) ) c= ( F .: U ) by A8,RELAT_1:123;
then A9: K c= ( F .: U ) by A4,FUNCT_1:77;
A10: U c= ( F " ( F .: U ) ) by A5,FUNCT_1:76;
( F .: U ) is  clique by A3,A7,CKB36:1;
then U c= ( F " K ) by A2,A9,A10,CKB18:def 1;
hence thesis by A8,XBOOLE_0:def 10;
end;
A11: (the line-map of F) is  bijective by A1,CKB29:def 1;
A12: (for U being (Subset of (the Points of S)) holds ((U is  clique & ( F .: K ) c= U) implies U = ( F .: K )))
proof
A13: K c= ( F " ( F .: K ) ) by A5,FUNCT_1:76;
let U being (Subset of (the Points of S));
assume that
A14: U is  clique
and
A15: ( F .: K ) c= U;
( F " ( F .: K ) ) c= ( F " U ) by A15,RELAT_1:143;
then A16: K c= ( F " U ) by A13,XBOOLE_1:1;
( F " U ) is  clique by A11,A3,A14,CKB37:1;
then ( F .: ( F " U ) ) c= ( F .: K ) by A2,A16,CKB18:def 1;
then U c= ( F .: K ) by A4,FUNCT_1:77;
hence thesis by A15,XBOOLE_0:def 10;
end;
A17: K is  clique by A2,CKB18:def 1;
then A18: ( F .: K ) is  clique by A3,CKB36:1;
( F " K ) is  clique by A11,A17,A3,CKB37:1;
hence thesis by A18,A12,A6,CKB18:def 1;
end;
