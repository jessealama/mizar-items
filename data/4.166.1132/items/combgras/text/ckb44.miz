begin
reserve k for (Element of ( NAT ));
reserve X for set;
theorem
(for k being (Element of ( NAT )) holds (for X being non  empty set holds ((( 0 ) < k & ( k + 1 ) c= ( card X )) implies (for T being (Subset of (the Points of ( G_ (k,X) ))) holds (T is  STAR implies (for S being (Subset of X) holds (S = ( meet T ) implies (( card S ) = ( k - 1 ) & T = { A where A is (Subset of X): (( card A ) = k & S c= A) }))))))))
proof
let k being (Element of ( NAT ));
let X being non  empty set;
assume that
A1: (( 0 ) < k & ( k + 1 ) c= ( card X ));
let T being (Subset of (the Points of ( G_ (k,X) )));
assume T is  STAR;
then consider S1 being (Subset of X) such that A2: (( card S1 ) = ( k - 1 ) & T = { A where A is (Subset of X): (( card A ) = k & S1 c= A) }) by CKB19:def 1;
let S being (Subset of X);
assume A3: S = ( meet T );
S1 = ( meet T ) by A1,A2,CKB43:1;
hence thesis by A3,A2;
end;
