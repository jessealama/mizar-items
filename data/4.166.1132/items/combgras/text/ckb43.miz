environ
vocabularies NUMBERS,INCSP_1,SUBSET_1,CARD_1,XBOOLE_0,ARYTM_3,TARSKI,ORDINAL1,FINSET_1,RELAT_1,ARYTM_1,NAT_1,FUNCT_1,XXREAL_0,FDIFF_1,WELLORD2,ZFMISC_1,ANPROJ_2,AFF_2,MOD_4,FUNCT_2,PRE_POLY,SETFAM_1,COMBGRAS;
notations TARSKI,XBOOLE_0,SUBSET_1,ORDINAL1,ENUMSET1,ZFMISC_1,SETFAM_1,DOMAIN_1,FINSET_1,RELAT_1,RELSET_1,WELLORD2,CARD_1,NUMBERS,INCSP_1,XXREAL_0,REAL_1,FUNCT_1,FUNCT_2,INCPROJ,NAT_1,CKB11,CKB12,CKB17,CKB18,CKB19,CKB20,CKB23,CKB24,CKB25,CKB27,CKB29,CKB30,CKB31,CKB32,CKB33,CKB40;
definitions TARSKI,INCSP_1,INCPROJ,FUNCT_1,RELAT_1,CKB12,CKB17,CKB18,CKB19,CKB20,CKB24,CKB25,CKB27,CKB29,CKB30,CKB31,CKB32,CKB33,CKB40;
theorems ENUMSET1,TARSKI,ZFMISC_1,XBOOLE_0,XBOOLE_1,XREAL_1,SETFAM_1,PENCIL_1,INCSP_1,WELLORD2,CARD_1,CARD_2,NAT_1,ORDINAL1,CARD_FIL,STIRL2_1,CARD_FIN,XXREAL_0,INCPROJ,FUNCT_1,FUNCT_2,RELAT_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB37,CKB38,CKB39,CKB40,CKB41,CKB42;
schemes FUNCT_1,NAT_1,CLASSES1;
registrations XBOOLE_0,SUBSET_1,FUNCT_1,ORDINAL1,FUNCT_2,FINSET_1,XXREAL_0,XREAL_0,NAT_1,CARD_1,INCSP_1,RELSET_1,CKB10,CKB23;
constructors SETFAM_1,WELLORD2,DOMAIN_1,XXREAL_0,REAL_1,NAT_1,BINOP_2,INCPROJ,RELSET_1,CKB12,CKB17,CKB18,CKB19,CKB20,CKB23,CKB24,CKB25,CKB27,CKB29,CKB30,CKB31,CKB32,CKB33,CKB40;
requirements BOOLE,SUBSET,NUMERALS,ARITHM,REAL;
begin
theorem
Th26: (for k being (Element of ( NAT )) holds (for X being non empty set holds ((( 0 ) < k & ( k + 1 ) c= ( card X )) implies (for T being (Subset of (the Points of ( G_ (k,X) ))) holds (for S being (Subset of X) holds ((( card S ) = ( k - 1 ) & T = { A where A is (Subset of X): (( card A ) = k & S c= A) }) implies S = ( meet T )))))))
proof
let k being (Element of ( NAT ));
let X being non empty set;
assume that
A1: ( 0 ) < k
and
A2: ( k + 1 ) c= ( card X );
A3: ( k - 1 ) is (Element of ( NAT )) by A1,NAT_1:20;
let T being (Subset of (the Points of ( G_ (k,X) )));
let S being (Subset of X);
assume that
A4: ( card S ) = ( k - 1 )
and
A5: T = { A where A is (Subset of X): (( card A ) = k & S c= A) };
A6: S is  finite by A1,A4,NAT_1:20;
A7: T <> ( {} )
proof
( X \ S ) <> ( {} )
proof
assume ( X \ S ) = ( {} );
then X c= S by XBOOLE_1:37;
then ( card X ) = ( k - 1 ) by A4,XBOOLE_0:def 10;
then ( 1 + k ) <= ( ( - 1 ) + k ) by A2,A3,NAT_1:39;
hence contradiction by XREAL_1:6;
end;
then consider x being set such that A8: x in ( X \ S ) by XBOOLE_0:def 1;
{ x } c= X by A8,ZFMISC_1:31;
then A9: (S c= ( S \/ { x } ) & ( S \/ { x } ) c= X) by XBOOLE_1:7,XBOOLE_1:8;
(not x in S) by A8,XBOOLE_0:def 5;
then ( card ( S \/ { x } ) ) = ( ( k - 1 ) + 1 ) by A4,A6,CARD_2:41;
then ( S \/ { x } ) in T by A5,A9;
hence thesis;
end;
A10: ( meet T ) c= S
proof
let y being set;
assume that
A11: y in ( meet T );
y in S
proof
consider a1 being set such that A12: a1 in T by A7,XBOOLE_0:def 1;
A13: (ex A1 being (Subset of X) st ((a1 = A1 & ( card A1 ) = k) & S c= A1)) by A5,A12;
then A14: a1 is  finite;
( X \ a1 ) <> ( {} )
proof
assume ( X \ a1 ) = ( {} );
then X c= a1 by XBOOLE_1:37;
then ( card X ) = k by A13,XBOOLE_0:def 10;
then ( 1 + k ) <= ( ( 0 ) + k ) by A2,NAT_1:39;
hence contradiction by XREAL_1:6;
end;
then consider y2 being set such that A15: y2 in ( X \ a1 ) by XBOOLE_0:def 1;
assume A16: (not y in S);
A17: S misses { y }
proof
assume (not S misses { y });
then ( S /\ { y } ) <> ( {} ) by XBOOLE_0:def 7;
then consider z being set such that A18: z in ( S /\ { y } ) by XBOOLE_0:def 1;
(z in { y } & z in S) by A18,XBOOLE_0:def 4;
hence contradiction by A16,TARSKI:def 1;
end;
then S c= ( a1 \ { y } ) by A13,XBOOLE_1:86;
then A19: S c= ( ( a1 \ { y } ) \/ { y2 } ) by XBOOLE_1:10;
A20: y in a1 by A11,A12,SETFAM_1:def 1;
then y2 <> y by A15,XBOOLE_0:def 5;
then A21: (not y in { y2 }) by TARSKI:def 1;
(( card { y } ) = 1 & { y } c= a1) by A20,CARD_1:30,ZFMISC_1:31;
then A22: ( card ( a1 \ { y } ) ) = ( k - 1 ) by A13,A14,CARD_2:44;
then (not y in ( a1 \ { y } )) by A4,A16,A13,A14,A17,CARD_FIN:1,XBOOLE_1:86;
then A23: (not y in ( ( a1 \ { y } ) \/ { y2 } )) by A21,XBOOLE_0:def 3;
A24: { y2 } c= X by A15,ZFMISC_1:31;
( a1 \ { y } ) c= X by A13,XBOOLE_1:1;
then A25: ( ( a1 \ { y } ) \/ { y2 } ) c= X by A24,XBOOLE_1:8;
(not y2 in ( a1 \ { y } )) by A15,XBOOLE_0:def 5;
then ( card ( ( a1 \ { y } ) \/ { y2 } ) ) = ( ( k - 1 ) + 1 ) by A14,A22,CARD_2:41;
then ( ( a1 \ { y } ) \/ { y2 } ) in T by A5,A25,A19;
hence contradiction by A11,A23,SETFAM_1:def 1;
end;
hence thesis;
end;
(for a1 being set holds (a1 in T implies S c= a1))
proof
let a1 being set;
assume a1 in T;
then (ex A1 being (Subset of X) st ((a1 = A1 & ( card A1 ) = k) & S c= A1)) by A5;
hence thesis;
end;
then S c= ( meet T ) by A7,SETFAM_1:5;
hence thesis by A10,XBOOLE_0:def 10;
end;
