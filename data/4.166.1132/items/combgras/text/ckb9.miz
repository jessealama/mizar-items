begin
theorem
Th9: (for P1,P2 being IncProjStr holds (( the IncProjStr of P1 ) = ( the IncProjStr of P2 ) implies (for A1 being (Subset of (the Points of P1)) holds (for A2 being (Subset of (the Points of P2)) holds (A1 = A2 implies (for L1 being (LINE of P1) holds (for L2 being (LINE of P2) holds (L1 = L2 implies (A1 on L1 implies A2 on L2)))))))))
proof
let P1 being IncProjStr;
let P2 being IncProjStr;
assume A1: ( the IncProjStr of P1 ) = ( the IncProjStr of P2 );
let A1 being (Subset of (the Points of P1));
let A2 being (Subset of (the Points of P2));
assume A2: A1 = A2;
let L1 being (LINE of P1);
let L2 being (LINE of P2);
assume that
A3: L1 = L2
and
A4: A1 on L1;
thus A2 on L2
proof
let A being (POINT of P2);
consider B being (POINT of P1) such that A5: A = B by A1;
assume A in A2;
then B on L1 by A2,A4,A5,INCSP_1:def 4;
then [ A,L2 ] in (the Inc of P2) by A1,A3,A5,INCSP_1:def 1;
hence thesis by INCSP_1:def 1;
end;

end;
