then x in ( ( dom f1 ) /\ ( left_open_halfline r ) ) by A10;
hence thesis by XBOOLE_0:def 4;
end;
A15:now
let n;
( f1 . ( s2 . n ) ) <= ( f2 . ( s2 . n ) ) by A4,A10;
then ( ( f1 /* s2 ) . n ) <= ( f2 . ( s2 . n ) ) by A14,FUNCT_2:108;
hence ( ( f1 /* s2 ) . n ) <= ( ( f2 /* s2 ) . n ) by A12,FUNCT_2:108;
end;
( lim_in-infty f2 ) = ( lim_in-infty f2 );
then A16: ( f2 /* s2 ) is  convergent by A2,A11,A12,CKB95:def 1;
( lim_in-infty f1 ) = ( lim_in-infty f1 );
then A17: ( f1 /* s2 ) is  convergent by A1,A11,A14,CKB95:def 1;
( lim ( f1 /* s2 ) ) = ( lim_in-infty f1 ) by A1,A11,A14,CKB95:def 1;
hence thesis by A17,A16,A13,A15,SEQ_2:18;
end;
suppose A18: (( ( dom f2 ) /\ ( left_open_halfline r ) ) c= ( ( dom f1 ) /\ ( left_open_halfline r ) ) & (for g holds (g in ( ( dom f2 ) /\ ( left_open_halfline r ) ) implies ( f1 . g ) <= ( f2 . g ))));

