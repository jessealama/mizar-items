requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve n for (Element of ( NAT ));
reserve m for (Element of ( NAT ));
reserve f for (PartFunc of ( REAL ),( REAL ));
reserve x for set;
reserve r for Real;
reserve r1 for Real;
reserve g for Real;
set s1 = ( incl ( NAT ) );
theorem
(f is  divergent_in-infty_to+infty iff ((for r holds (ex g st (g < r & g in ( dom f )))) & (for g holds (ex r st (for r1 holds ((r1 < r & r1 in ( dom f )) implies g < ( f . r1 )))))))
proof
thus (f is  divergent_in-infty_to+infty implies ((for r holds (ex g st (g < r & g in ( dom f )))) & (for g holds (ex r st (for r1 holds ((r1 < r & r1 in ( dom f )) implies g < ( f . r1 )))))))
proof
deffunc U((Element of ( NAT ))) = ( - $1 );
assume A1: f is  divergent_in-infty_to+infty;
assume ((not (for r holds (ex g st (g < r & g in ( dom f ))))) or (ex g st (for r holds (ex r1 st ((r1 < r & r1 in ( dom f )) & g >= ( f . r1 ))))));
then consider g such that A2: (for r holds (ex r1 st ((r1 < r & r1 in ( dom f )) & g >= ( f . r1 )))) by A1,CKB58:def 1;
defpred X[ (Element of ( NAT )), real number ]
 means
(($2 < ( - $1 ) & $2 in ( dom f )) & g >= ( f . $2 ));
A3: (for n holds (ex r st X[ n,r ])) by A2;
consider s being Real_Sequence such that A4: (for n holds X[ n,( s . n ) ]) from FUNCT_2:sch 3(A3);
now
let x being set;
assume x in ( rng s );
then (ex n st ( s . n ) = x) by FUNCT_2:113;
hence x in ( dom f ) by A4;
end;
then A5: ( rng s ) c= ( dom f ) by TARSKI:def 3;
consider s1 being Real_Sequence such that A6: (for n holds ( s1 . n ) = U(n)) from SEQ_1:sch 1;
now
let n;
( s . n ) < ( - n ) by A4;
hence ( s . n ) <= ( s1 . n ) by A6;
end;
then s is  divergent_to-infty by A6,CKB31:1,CKB53:1;
then ( f /* s ) is  divergent_to+infty by A1,A5,CKB58:def 1;
then consider n such that A7: (for m holds (n <= m implies g < ( ( f /* s ) . m ))) by CKB14:def 1;
g < ( ( f /* s ) . n ) by A7;
then g < ( f . ( s . n ) ) by A5,FUNCT_2:108;
hence contradiction by A4;
end;

assume that
A8: (for r holds (ex g st (g < r & g in ( dom f ))))
and
A9: (for g holds (ex r st (for r1 holds ((r1 < r & r1 in ( dom f )) implies g < ( f . r1 )))));
now
let s being Real_Sequence;
assume that
A10: s is  divergent_to-infty
and
A11: ( rng s ) c= ( dom f );
now
let g;
consider r such that A12: (for r1 holds ((r1 < r & r1 in ( dom f )) implies g < ( f . r1 ))) by A9;
consider n such that A13: (for m holds (n <= m implies ( s . m ) < r)) by A10,CKB15:def 1;
take n;
let m;
A14: ( s . m ) in ( rng s ) by VALUED_0:28;
assume n <= m;
then g < ( f . ( s . m ) ) by A11,A12,A13,A14;
hence g < ( ( f /* s ) . m ) by A11,FUNCT_2:108;
end;
hence ( f /* s ) is  divergent_to+infty by CKB14:def 1;
end;
hence thesis by A8,CKB58:def 1;
end;
