theorem
Th52: ((seq is  divergent_to+infty or seq is  divergent_to-infty) implies ( abs seq ) is  divergent_to+infty)
proof
assume A1: (seq is  divergent_to+infty or seq is  divergent_to-infty);
let r being Real;
now
per cases  by A1;
suppose seq is  divergent_to+infty;

then consider n such that A2: (for m holds (n <= m implies ( abs r ) < ( seq . m ))) by CKB14:def 1;
take n;
let m;
assume n <= m;
then (r <= ( abs r ) & ( abs r ) < ( seq . m )) by A2,ABSVALUE:4;
then A3: r < ( seq . m ) by XXREAL_0:2;
( seq . m ) <= ( abs ( seq . m ) ) by ABSVALUE:4;
then ( seq . m ) <= ( ( abs seq ) . m ) by SEQ_1:12;
hence r < ( ( abs seq ) . m ) by A3,XXREAL_0:2;
end;
suppose seq is  divergent_to-infty;

then consider n such that A4: (for m holds (n <= m implies ( seq . m ) < ( - r ))) by CKB15:def 1;
take n;
let m;
( - ( abs ( seq . m ) ) ) <= ( seq . m ) by ABSVALUE:4;
then A5: ( - ( ( abs seq ) . m ) ) <= ( seq . m ) by SEQ_1:12;
assume n <= m;
then ( seq . m ) < ( - r ) by A4;
then ( - ( ( abs seq ) . m ) ) < ( - r ) by A5,XXREAL_0:2;
hence r < ( ( abs seq ) . m ) by XREAL_1:24;
end;
end;
hence thesis;
end;
