Lm4: (for n holds ( s1 . n ) = n) by FUNCT_1:18;
theorem
(((f1 is  convergent_in+infty & f2 is  convergent_in+infty) & (ex r st ((( ( dom f1 ) /\ ( right_open_halfline r ) ) c= ( ( dom f2 ) /\ ( right_open_halfline r ) ) & (for g holds (g in ( ( dom f1 ) /\ ( right_open_halfline r ) ) implies ( f1 . g ) <= ( f2 . g )))) or (( ( dom f2 ) /\ ( right_open_halfline r ) ) c= ( ( dom f1 ) /\ ( right_open_halfline r ) ) & (for g holds (g in ( ( dom f2 ) /\ ( right_open_halfline r ) ) implies ( f1 . g ) <= ( f2 . g ))))))) implies ( lim_in+infty f1 ) <= ( lim_in+infty f2 ))
proof
assume that
A1: f1 is  convergent_in+infty
and
A2: f2 is  convergent_in+infty;
given r such that
A3: ((( ( dom f1 ) /\ ( right_open_halfline r ) ) c= ( ( dom f2 ) /\ ( right_open_halfline r ) ) & (for g holds (g in ( ( dom f1 ) /\ ( right_open_halfline r ) ) implies ( f1 . g ) <= ( f2 . g )))) or (( ( dom f2 ) /\ ( right_open_halfline r ) ) c= ( ( dom f1 ) /\ ( right_open_halfline r ) ) & (for g holds (g in ( ( dom f2 ) /\ ( right_open_halfline r ) ) implies ( f1 . g ) <= ( f2 . g )))));

now
per cases  by A3;
suppose A4: (( ( dom f1 ) /\ ( right_open_halfline r ) ) c= ( ( dom f2 ) /\ ( right_open_halfline r ) ) & (for g holds (g in ( ( dom f1 ) /\ ( right_open_halfline r ) ) implies ( f1 . g ) <= ( f2 . g ))));

defpred X[ (Element of ( NAT )), real number ]
 means
($1 < $2 & $2 in ( ( dom f1 ) /\ ( right_open_halfline r ) ));
A5:now
let n;
( 0 ) <= ( abs r ) by COMPLEX1:46;
then A6: ( n + ( 0 ) ) <= ( n + ( abs r ) ) by XREAL_1:7;
consider g such that A7: ( n + ( abs r ) ) < g and A8: g in ( dom f1 ) by A1,CKB54:def 1;
take g;
(( 0 ) <= n & r <= ( abs r )) by ABSVALUE:4,NAT_1:2;
then ( ( 0 ) + r ) <= ( n + ( abs r ) ) by XREAL_1:7;
then r < g by A7,XXREAL_0:2;
then g in { g2: r < g2 };
then g in ( right_open_halfline r ) by XXREAL_1:230;
hence X[ n,g ] by A7,A8,A6,XBOOLE_0:def 4,XXREAL_0:2;
end;
consider s2 being Real_Sequence such that A9: (for n holds X[ n,( s2 . n ) ]) from FUNCT_2:sch 3(A5);
now
let n;
n < ( s2 . n ) by A9;
hence ( s1 . n ) <= ( s2 . n ) by FUNCT_1:18;
end;
then A10: s2 is  divergent_to+infty by Lm4,CKB30:1,CKB52:1;
A11: ( rng s2 ) c= ( dom f2 )
proof
let x being  real number;
assume x in ( rng s2 );
then (ex n st x = ( s2 . n )) by FUNCT_2:113;
then x in ( ( dom f1 ) /\ ( right_open_halfline r ) ) by A9;
hence thesis by A4,XBOOLE_0:def 4;
end;
then A12: ( lim ( f2 /* s2 ) ) = ( lim_in+infty f2 ) by A2,A10,CKB94:def 1;
A13: ( rng s2 ) c= ( dom f1 )
proof
