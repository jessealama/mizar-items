assume x in ( rng s2 );
then (ex n st x = ( s2 . n )) by FUNCT_2:113;
then x in ( ( dom f1 ) /\ ( right_open_halfline r ) ) by A9;
hence thesis by XBOOLE_0:def 4;
end;
A14:now
let n;
( f1 . ( s2 . n ) ) <= ( f2 . ( s2 . n ) ) by A4,A9;
then ( ( f1 /* s2 ) . n ) <= ( f2 . ( s2 . n ) ) by A13,FUNCT_2:108;
hence ( ( f1 /* s2 ) . n ) <= ( ( f2 /* s2 ) . n ) by A11,FUNCT_2:108;
end;
( lim_in+infty f2 ) = ( lim_in+infty f2 );
then A15: ( f2 /* s2 ) is  convergent by A2,A10,A11,CKB94:def 1;
( lim_in+infty f1 ) = ( lim_in+infty f1 );
then A16: ( f1 /* s2 ) is  convergent by A1,A10,A13,CKB94:def 1;
( lim ( f1 /* s2 ) ) = ( lim_in+infty f1 ) by A1,A10,A13,CKB94:def 1;
hence thesis by A16,A15,A12,A14,SEQ_2:18;
end;
suppose A17: (( ( dom f2 ) /\ ( right_open_halfline r ) ) c= ( ( dom f1 ) /\ ( right_open_halfline r ) ) & (for g holds (g in ( ( dom f2 ) /\ ( right_open_halfline r ) ) implies ( f1 . g ) <= ( f2 . g ))));

defpred X[ (Element of ( NAT )), real number ]
 means
($1 < $2 & $2 in ( ( dom f2 ) /\ ( right_open_halfline r ) ));
A18:now
let n;
( 0 ) <= ( abs r ) by COMPLEX1:46;
then A19: ( n + ( 0 ) ) <= ( n + ( abs r ) ) by XREAL_1:7;
consider g such that A20: ( n + ( abs r ) ) < g and A21: g in ( dom f2 ) by A2,CKB54:def 1;
take g;
(( 0 ) <= n & r <= ( abs r )) by ABSVALUE:4,NAT_1:2;
then ( ( 0 ) + r ) <= ( n + ( abs r ) ) by XREAL_1:7;
then r < g by A20,XXREAL_0:2;
then g in { g2: r < g2 };
then g in ( right_open_halfline r ) by XXREAL_1:230;
hence X[ n,g ] by A20,A21,A19,XBOOLE_0:def 4,XXREAL_0:2;
end;
consider s2 being Real_Sequence such that A22: (for n holds X[ n,( s2 . n ) ]) from FUNCT_2:sch 3(A18);
now
let n;
n < ( s2 . n ) by A22;
hence ( s1 . n ) <= ( s2 . n ) by FUNCT_1:18;
end;
then A23: s2 is  divergent_to+infty by Lm4,CKB30:1,CKB52:1;
A24: ( rng s2 ) c= ( dom f1 )
proof
let x being  real number;
assume x in ( rng s2 );
