environ
vocabularies NUMBERS,XREAL_0,ORDINAL1,SUBSET_1,SEQ_1,PARTFUN1,CARD_1,ARYTM_3,XXREAL_0,ARYTM_1,RELAT_1,TARSKI,XBOOLE_0,VALUED_1,PROB_1,XXREAL_1,VALUED_0,XXREAL_2,FUNCT_1,SEQ_2,ORDINAL2,COMPLEX1,FUNCOP_1,REAL_1,NAT_1,SQUARE_1,FUNCT_3,SEQM_3,FUNCT_2,ORDINAL4,LIMFUNC1;
notations TARSKI,XBOOLE_0,SUBSET_1,ORDINAL1,NUMBERS,XCMPLX_0,XREAL_0,COMPLEX1,REAL_1,NAT_1,FUNCT_2,FUNCOP_1,VALUED_0,VALUED_1,SEQ_1,SEQ_2,SEQM_3,PROB_1,RELSET_1,SQUARE_1,PARTFUN1,FUNCT_3,RFUNCT_1,XXREAL_0,XXREAL_1,RECDEF_1,CKB4,CKB5,CKB6,CKB7,CKB8,CKB14,CKB15,CKB54,CKB55,CKB56,CKB57,CKB58,CKB59;
definitions SQUARE_1,TARSKI,SEQ_2,XBOOLE_0,PROB_1,MEMBERED,VALUED_1,CKB6,CKB7,CKB8,CKB14,CKB15,CKB54,CKB55,CKB56,CKB57,CKB58,CKB59;
