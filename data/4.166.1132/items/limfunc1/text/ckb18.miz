begin
reserve n for (Element of ( NAT ));
reserve m for (Element of ( NAT ));
reserve seq1 for Real_Sequence;
reserve seq2 for Real_Sequence;
reserve r for Real;
theorem
Th35: ((seq1 is  divergent_to+infty & seq2 is  divergent_to+infty) implies ( seq1 + seq2 ) is  divergent_to+infty)
proof
assume that
A1: seq1 is  divergent_to+infty
and
A2: seq2 is  divergent_to+infty;
let r being Real;
consider n1 being (Element of ( NAT )) such that A3: (for m holds (n1 <= m implies ( r / 2 ) < ( seq1 . m ))) by A1,CKB14:def 1;
consider n2 being (Element of ( NAT )) such that A4: (for m holds (n2 <= m implies ( r / 2 ) < ( seq2 . m ))) by A2,CKB14:def 1;
take n = ( max (n1,n2) );
let m;
assume that
A5: n <= m;
n2 <= n by XXREAL_0:25;
then n2 <= m by A5,XXREAL_0:2;
then A6: ( r / 2 ) < ( seq2 . m ) by A4;
n1 <= n by XXREAL_0:25;
then n1 <= m by A5,XXREAL_0:2;
then ( r / 2 ) < ( seq1 . m ) by A3;
then ( ( r / 2 ) + ( r / 2 ) ) < ( ( seq1 . m ) + ( seq2 . m ) ) by A6,XREAL_1:8;
hence thesis by SEQ_1:7;
end;
