begin
reserve r for  real number;
reserve r1 for  real number;
reserve g for  real number;
theorem
Lm1: ((( 0 ) < g & r <= r1) implies (( r - g ) < r1 & r < ( r1 + g )))
proof
assume A1: (( 0 ) < g & r <= r1);
then ( r - g ) < ( r1 - ( 0 ) ) by XREAL_1:15;
hence ( r - g ) < r1;
( r + ( 0 ) ) < ( r1 + g ) by A1,XREAL_1:8;
hence thesis;
end;
