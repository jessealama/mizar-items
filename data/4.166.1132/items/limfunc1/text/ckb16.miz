begin
reserve n for (Element of ( NAT ));
reserve m for (Element of ( NAT ));
reserve k for (Element of ( NAT ));
reserve seq for Real_Sequence;
theorem
((seq is  divergent_to+infty or seq is  divergent_to-infty) implies (ex n st (for m holds (n <= m implies ( seq ^\ m ) is  non-zero))))
proof
assume A1: (seq is  divergent_to+infty or seq is  divergent_to-infty);
now
per cases  by A1;
suppose seq is  divergent_to+infty;

then consider n such that A2: (for m holds (n <= m implies ( 0 ) < ( seq . m ))) by CKB14:def 1;
take n;
let m;
assume that
A3: n <= m;
now
let k;
( 0 ) < ( seq . ( k + m ) ) by A2,A3,NAT_1:12;
hence ( 0 ) <> ( ( seq ^\ m ) . k ) by NAT_1:def 3;
end;
hence ( seq ^\ m ) is  non-zero by SEQ_1:5;
end;
suppose seq is  divergent_to-infty;

then consider n such that A4: (for m holds (n <= m implies ( seq . m ) < ( 0 ))) by CKB15:def 1;
take n;
let m;
assume that
A5: n <= m;
now
let k;
( seq . ( k + m ) ) < ( 0 ) by A4,A5,NAT_1:12;
hence ( ( seq ^\ m ) . k ) <> ( 0 ) by NAT_1:def 3;
end;
hence ( seq ^\ m ) is  non-zero by SEQ_1:5;
end;
end;
hence thesis;
end;
