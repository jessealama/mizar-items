begin
reserve x for set;
reserve y for set;
reserve k for (Element of ( NAT ));
reserve GF for  add-associative  right_zeroed  right_complementable  Abelian  associative  well-unital  distributive non  empty doubleLoopStr;
reserve V for  Abelian  add-associative  right_zeroed  right_complementable  vector-distributive  scalar-distributive  scalar-associative  scalar-unital non  empty VectSpStr over GF;
reserve F for (FinSequence of V);
reserve f for (Function of V,GF);
definition
let GF being non  empty addLoopStr;
let V being non  empty VectSpStr over GF;
let F being (FinSequence of (the carrier of V));
let f being (Function of V,GF);
func f (#) F -> (FinSequence of V) means 
:Def8: (( len it ) = ( len F ) & (for i being Nat holds (i in ( dom it ) implies ( it . i ) = ( ( f . ( F /. i ) ) * ( F /. i ) ))));
existence
proof
deffunc G(Nat) = ( ( f . ( F /. $1 ) ) * ( F /. $1 ) );
consider G being FinSequence such that A1: ( len G ) = ( len F ) and A2: (for n being Nat holds (n in ( dom G ) implies ( G . n ) = G(n))) from FINSEQ_1:sch 2;
( rng G ) c= (the carrier of V)
proof
let x;
assume x in ( rng G );
then consider y such that A3: y in ( dom G ) and A4: ( G . y ) = x by FUNCT_1:def 3;
y in ( Seg ( len F ) ) by A1,A3,FINSEQ_1:def 3;
then reconsider y as (Element of ( NAT ));
( G . y ) = ( ( f . ( F /. y ) ) * ( F /. y ) ) by A2,A3;
hence thesis by A4;
end;
then reconsider G as (FinSequence of (the carrier of V)) by FINSEQ_1:def 4;
take G;
thus thesis by A1,A2;
end;
uniqueness
proof
let H1 being (FinSequence of (the carrier of V));
let H2 being (FinSequence of (the carrier of V));
assume that
A5: ( len H1 ) = ( len F )
and
A6: (for i being Nat holds (i in ( dom H1 ) implies ( H1 . i ) = ( ( f . ( F /. i ) ) * ( F /. i ) )))
and
A7: ( len H2 ) = ( len F )
and
A8: (for i being Nat holds (i in ( dom H2 ) implies ( H2 . i ) = ( ( f . ( F /. i ) ) * ( F /. i ) )));
now
let k being Nat;
assume (1 <= k & k <= ( len H1 ));
then A9: k in ( Seg ( len H1 ) ) by FINSEQ_1:1;
then k in ( dom H1 ) by FINSEQ_1:def 3;
then A10: ( H1 . k ) = ( ( f . ( F /. k ) ) * ( F /. k ) ) by A6;
k in ( dom H2 ) by A5,A7,A9,FINSEQ_1:def 3;
hence ( H1 . k ) = ( H2 . k ) by A8,A10;
end;
hence thesis by A5,A7,FINSEQ_1:14;
end;
end;
