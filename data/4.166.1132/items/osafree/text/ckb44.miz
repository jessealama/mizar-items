environ
vocabularies OSALG_1,MSUALG_2,OSALG_2,MSUALG_1,TARSKI,STRUCT_0,RELAT_1,SEQM_3,PBOOLE,MSUALG_3,REALSET1,ZFMISC_1,XBOOLE_0,CARD_3,MSAFREE,SUBSET_1,FINSEQ_1,MARGREL1,FUNCT_1,XXREAL_0,PARTFUN1,LANG1,TDGROUP,DTCONSTR,TREES_3,TREES_4,NAT_1,TREES_2,OSALG_4,CARD_5,NATTRA_1,CARD_LAR,QC_LANG1,EQREL_1,RELAT_2,MSUALG_4,COH_SP,MCART_1,UNIALG_2,MEMBER_1,FUNCT_7,OSAFREE;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,NAT_1,RELAT_1,RELSET_1,FUNCT_1,MCART_1,PARTFUN1,FUNCT_2,FINSEQ_1,FINSEQ_2,PBOOLE,RELAT_2,TREES_2,CARD_3,EQREL_1,STRUCT_0,LANG1,TREES_3,TREES_4,DTCONSTR,ORDERS_2,ORDERS_3,MSUALG_1,MSUALG_2,MSUALG_3,MSUALG_4,OSALG_1,OSALG_2,OSALG_3,OSALG_4,MSAFREE,FUNCT_7,CKB1,CKB3,CKB4,CKB5,CKB7,CKB12,CKB13,CKB15,CKB21,CKB22,CKB23,CKB25,CKB31,CKB32,CKB38,CKB41,CKB42,CKB43;
definitions TARSKI,XBOOLE_0,PBOOLE,MSUALG_3,OSALG_1,OSALG_3,OSALG_4,CKB1,CKB3,CKB4,CKB5,CKB7,CKB12,CKB13,CKB15,CKB21,CKB22,CKB23,CKB31,CKB38,CKB41,CKB42,CKB43;
theorems TARSKI,FUNCT_1,FUNCT_2,MCART_1,ZFMISC_1,PBOOLE,CARD_3,MSUALG_1,MSUALG_2,MSUALG_3,RELAT_1,LANG1,DTCONSTR,FINSEQ_1,TREES_4,TREES_1,DOMAIN_1,RELSET_1,XBOOLE_0,XBOOLE_1,OSALG_1,OSALG_2,OSALG_3,OSALG_4,MSAFREE,MSUALG_6,MSAFREE2,CARD_5,FINSEQ_2,FINSEQ_3,MSUALG_9,MSUALG_4,EQREL_1,FUNCT_5,ORDERS_2,RELAT_2,PARTFUN1,ORDERS_1,FUNCT_7,FUNCOP_1,TREES_3,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB9,CKB11,CKB12,CKB13,CKB15,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB33,CKB34,CKB35,CKB36,CKB38,CKB39,CKB41,CKB42,CKB43;
schemes CLASSES1,FUNCT_1,RELSET_1,DTCONSTR,XBOOLE_0,FUNCT_2,PBOOLE;
registrations XBOOLE_0,SUBSET_1,RELAT_1,ORDINAL1,PARTFUN1,FUNCOP_1,FUNCT_1,PBOOLE,TREES_2,TREES_3,STRUCT_0,DTCONSTR,MSUALG_1,MSUALG_3,MSAFREE,MSUALG_4,MSUALG_9,OSALG_1,OSALG_4,RELSET_1,FINSEQ_1,CKB8,CKB10,CKB14,CKB16,CKB24,CKB37,CKB40;
constructors XXREAL_0,NAT_1,FINSEQOP,FUNCT_7,MSUALG_3,MSAFREE,ORDERS_3,OSALG_2,OSALG_3,OSALG_4,RELSET_1,CKB1,CKB3,CKB4,CKB5,CKB7,CKB12,CKB13,CKB15,CKB21,CKB22,CKB23,CKB25,CKB31,CKB38,CKB41,CKB42,CKB43;
requirements BOOLE,SUBSET;
begin
definition
let S being  locally_directed OrderSortedSign;
let X being  non-empty (ManySortedSet of S);
func LCongruence X ->  monotone (OSCongruence of ( ParsedTermsOSA X ))
means
:Def18: (for R being  monotone (OSCongruence of ( ParsedTermsOSA X )) holds it c= R);
existence
proof
set PTA = ( ParsedTermsOSA X );
set D = ( DTConOSA X );
set SPTA = (the Sorts of PTA);
defpred MC[ set,set,set ]
 means
(for R being  monotone (OSCongruence of PTA) holds [ $1,$2 ] in ( R . $3 ));
deffunc F(set) = { [ x,y ] where x is (Element of ( TS D )),y is (Element of ( TS D )): ((x in ( SPTA . $1 ) & y in ( SPTA . $1 )) & (for R being  monotone (OSCongruence of PTA) holds [ x,y ] in ( R . $1 ))) };
consider f being (ManySortedSet of (the carrier of S)) such that A1: (for i being set holds (i in (the carrier of S) implies ( f . i ) = F(i))) from PBOOLE:sch 4;
A2: (for i being set holds (i in (the carrier of S) implies ( f . i ) is (Equivalence_Relation of ( SPTA . i ))))
proof
let i being set;
assume that
A3: i in (the carrier of S);
reconsider s = i as (Element of S) by A3;
A4: ( f . i ) = { [ x,y ] where x is (Element of ( TS D )),y is (Element of ( TS D )): ((x in ( SPTA . i ) & y in ( SPTA . i )) & (for R being  monotone (OSCongruence of PTA) holds [ x,y ] in ( R . i ))) } by A1,A3;
now
let z being set;
assume z in ( f . i );
then (ex x,y being (Element of ( TS D )) st (((z = [ x,y ] & x in ( SPTA . i )) & y in ( SPTA . i )) & MC[ x,y,i ])) by A4;
hence z in [: ( SPTA . i ),( SPTA . i ) :] by ZFMISC_1:87;
end;
then reconsider fi = ( f . i ) as (Relation of ( SPTA . i )) by TARSKI:def 3;
now
let x being set,y being set,z being set;
assume that
A5: x in ( SPTA . s )
and
A6: y in ( SPTA . s )
and
A7: z in ( SPTA . s )
and
A8: [ x,y ] in fi
and
A9: [ y,z ] in fi;
thus [ x,z ] in fi
proof
reconsider t1 = x,t2 = y,t3 = z as (Element of ( TS D )) by A5,A6,A7,CKB34:1;
A10: (ex t6,t7 being (Element of ( TS D )) st ((([ t2,t3 ] = [ t6,t7 ] & t6 in ( SPTA . i )) & t7 in ( SPTA . i )) & MC[ t6,t7,i ])) by A4,A9;
A11: (ex t4,t5 being (Element of ( TS D )) st ((([ t1,t2 ] = [ t4,t5 ] & t4 in ( SPTA . i )) & t5 in ( SPTA . i )) & MC[ t4,t5,i ])) by A4,A8;
now
let R being  monotone (OSCongruence of PTA);
A12: [ t2,t3 ] in ( R . s ) by A10;
( field ( R . s ) ) = ( SPTA . s ) by ORDERS_1:12;
then A13: ( R . s ) is_transitive_in ( SPTA . s ) by RELAT_2:def 16;
[ t1,t2 ] in ( R . s ) by A11;
hence [ t1,t3 ] in ( R . s ) by A5,A6,A7,A12,A13,RELAT_2:def 8;
end;
hence thesis by A4,A5,A7;
end;

end;
then A14: fi is_transitive_in ( SPTA . s ) by RELAT_2:def 8;
now
let x being set,y being set;
assume that
A15: x in ( SPTA . s )
and
A16: y in ( SPTA . s )
and
A17: [ x,y ] in fi;
thus [ y,x ] in fi
proof
reconsider t1 = x,t2 = y as (Element of ( TS D )) by A15,A16,CKB34:1;
A18: (ex t3,t4 being (Element of ( TS D )) st ((([ t1,t2 ] = [ t3,t4 ] & t3 in ( SPTA . i )) & t4 in ( SPTA . i )) & MC[ t3,t4,i ])) by A4,A17;
now
let R being  monotone (OSCongruence of PTA);
( field ( R . s ) ) = ( SPTA . s ) by ORDERS_1:12;
then A19: ( R . s ) is_symmetric_in ( SPTA . s ) by RELAT_2:def 11;
[ t1,t2 ] in ( R . s ) by A18;
hence [ t2,t1 ] in ( R . s ) by A15,A16,A19,RELAT_2:def 3;
end;
hence thesis by A4,A15,A16;
end;

end;
then A20: fi is_symmetric_in ( SPTA . s ) by RELAT_2:def 3;
now
let x being set;
assume that
A21: x in ( SPTA . s );
thus [ x,x ] in fi
proof
reconsider t1 = x as (Element of ( TS D )) by A21,CKB34:1;
now
let R being  monotone (OSCongruence of PTA);
( field ( R . s ) ) = ( SPTA . s ) by ORDERS_1:12;
then ( R . s ) is_reflexive_in ( SPTA . s ) by RELAT_2:def 9;
hence [ t1,t1 ] in ( R . s ) by A21,RELAT_2:def 1;
end;
hence thesis by A4,A21;
end;

end;
then A22: fi is_reflexive_in ( SPTA . s ) by RELAT_2:def 1;
then A23: ( field fi ) = ( SPTA . s ) by ORDERS_1:13;
( dom fi ) = ( SPTA . s ) by A22,ORDERS_1:13;
hence thesis by A23,A20,A14,PARTFUN1:def 2,RELAT_2:def 11,RELAT_2:def 16;
end;
then (for i being set holds (i in (the carrier of S) implies ( f . i ) is (Relation of ( SPTA . i ),( SPTA . i ))));
then reconsider f as (ManySortedRelation of (the Sorts of PTA)) by MSUALG_4:def 1;
reconsider f as (ManySortedRelation of PTA);
(for i being set holds (for R being (Relation of ( SPTA . i )) holds ((i in (the carrier of S) & ( f . i ) = R) implies R is (Equivalence_Relation of ( SPTA . i ))))) by A2;
then f is  MSEquivalence_Relation-like by MSUALG_4:def 2;
then reconsider f as  MSEquivalence-like (ManySortedRelation of PTA) by MSUALG_4:def 3;
f is  os-compatible
proof
let s1 being (Element of S),s2 being (Element of S);
assume that
A24: s1 <= s2;
A25: ( f . s1 ) = { [ x,y ] where x is (Element of ( TS D )),y is (Element of ( TS D )): ((x in ( SPTA . s1 ) & y in ( SPTA . s1 )) & MC[ x,y,s1 ]) } by A1;
A26: ( f . s2 ) = { [ x,y ] where x is (Element of ( TS D )),y is (Element of ( TS D )): ((x in ( SPTA . s2 ) & y in ( SPTA . s2 )) & MC[ x,y,s2 ]) } by A1;
let x being set,y being set;
assume that
A27: x in ( SPTA . s1 )
and
A28: y in ( SPTA . s1 );
hereby
assume [ x,y ] in ( f . s1 );
then consider t1 being (Element of ( TS D )),t2 being (Element of ( TS D )) such that A29: [ x,y ] = [ t1,t2 ] and A30: t1 in ( SPTA . s1 ) and A31: t2 in ( SPTA . s1 ) and A32: MC[ t1,t2,s1 ] by A25;
now
let R being  monotone (OSCongruence of PTA);
A33: R is  os-compatible by OSALG_4:def 2;
[ t1,t2 ] in ( R . s1 ) by A32;
then [ t1,t2 ] in ( R . s2 ) by A24,A30,A31,A33,OSALG_4:def 1;
hence ((t1 in ( SPTA . s2 ) & t2 in ( SPTA . s2 )) & [ t1,t2 ] in ( R . s2 )) by ZFMISC_1:87;
end;
hence [ x,y ] in ( f . s2 ) by A26,A29;
end;
assume [ x,y ] in ( f . s2 );
then consider t1 being (Element of ( TS D )),t2 being (Element of ( TS D )) such that A34: [ x,y ] = [ t1,t2 ] and t1 in ( SPTA . s2 ) and t2 in ( SPTA . s2 ) and A35: MC[ t1,t2,s2 ] by A26;
A36: y = t2 by A34,ZFMISC_1:27;
A37:now
let R being  monotone (OSCongruence of PTA);
A38: R is  os-compatible by OSALG_4:def 2;
[ t1,t2 ] in ( R . s2 ) by A35;
hence [ t1,t2 ] in ( R . s1 ) by A24,A27,A28,A34,A38,OSALG_4:def 1;
end;
x = t1 by A34,ZFMISC_1:27;
hence [ x,y ] in ( f . s1 ) by A25,A27,A28,A36,A37;
end;
then reconsider f as  MSEquivalence-like (OrderSortedRelation of PTA) by OSALG_4:def 2;
f is  monotone
proof
let o1 being (OperSymbol of S),o2 being (OperSymbol of S);
assume that
A39: o1 <= o2;
set w2 = ( the_arity_of o2 );
set rs2 = ( the_result_sort_of o2 );
let x1 being (Element of ( Args (o1,PTA) ));
let x2 being (Element of ( Args (o2,PTA) ));
assume that
A40: (for y being Nat holds (y in ( dom x1 ) implies [ ( x1 . y ),( x2 . y ) ] in ( f . ( w2 /. y ) )));
set D1 = ( ( Den (o1,PTA) ) . x1 );
set D2 = ( ( Den (o2,PTA) ) . x2 );
A41:now
let R being  monotone (OSCongruence of PTA);
A42:now
let y being Nat;
assume y in ( dom x1 );
then A43: [ ( x1 . y ),( x2 . y ) ] in ( f . ( w2 /. y ) ) by A40;
( f . ( w2 /. y ) ) = { [ x,z ] where x is (Element of ( TS D )),z is (Element of ( TS D )): ((x in ( SPTA . ( w2 /. y ) ) & z in ( SPTA . ( w2 /. y ) )) & MC[ x,z,( w2 /. y ) ]) } by A1;
then (ex x,z being (Element of ( TS D )) st ((([ ( x1 . y ),( x2 . y ) ] = [ x,z ] & x in ( SPTA . ( w2 /. y ) )) & z in ( SPTA . ( w2 /. y ) )) & MC[ x,z,( w2 /. y ) ])) by A43;
hence [ ( x1 . y ),( x2 . y ) ] in ( R . ( w2 /. y ) );
end;
then A44: [ D1,D2 ] in ( R . rs2 ) by A39,OSALG_4:def 26;
then A45: D2 in ( SPTA . rs2 ) by ZFMISC_1:87;
D1 in ( SPTA . rs2 ) by A44,ZFMISC_1:87;
hence ((((D1 in ( SPTA . rs2 ) & D2 in ( SPTA . rs2 )) & [ D1,D2 ] in ( R . rs2 )) & D1 is (Element of ( TS D ))) & D2 is (Element of ( TS D ))) by A39,A42,A45,CKB34:1,OSALG_4:def 26;
end;
( f . rs2 ) = { [ x,y ] where x is (Element of ( TS D )),y is (Element of ( TS D )): ((x in ( SPTA . rs2 ) & y in ( SPTA . rs2 )) & MC[ x,y,rs2 ]) } by A1;
hence thesis by A41;
end;
then reconsider f as  monotone  MSEquivalence-like (OrderSortedRelation of PTA);
take f;
let R being  monotone (OSCongruence of PTA);
let i being set;
assume i in (the carrier of S);
then reconsider s = i as (Element of S);
A46: ( f . s ) = { [ x,y ] where x is (Element of ( TS D )),y is (Element of ( TS D )): ((x in ( SPTA . s ) & y in ( SPTA . s )) & MC[ x,y,s ]) } by A1;
let z being set;
assume z in ( f . i );
then (ex x,y being (Element of ( TS D )) st (((z = [ x,y ] & x in ( SPTA . s )) & y in ( SPTA . s )) & MC[ x,y,s ])) by A46;
hence thesis;
end;
uniqueness
proof
set PTA = ( ParsedTermsOSA X );
let L1 being  monotone (OSCongruence of PTA),L2 being  monotone (OSCongruence of PTA);
assume that
A47: (for R being  monotone (OSCongruence of ( ParsedTermsOSA X )) holds L1 c= R)
and
A48: (for R being  monotone (OSCongruence of ( ParsedTermsOSA X )) holds L2 c= R);
A49: L2 c= L1 by A48;
L1 c= L2 by A47;
hence thesis by A49,PBOOLE:def 10;
end;
end;
