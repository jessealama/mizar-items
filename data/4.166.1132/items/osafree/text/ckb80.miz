begin
definition
let S being  locally_directed OrderSortedSign;
let X being  non-empty (ManySortedSet of S);
func NHReverse X -> (ManySortedFunction of ( OSFreeGen X ),( PTVars X ))
means
(for s being (Element of S) holds ( it . s ) = ( NHReverse (s,X) ));
existence
proof
defpred P[ set,set ]
 means
(for s being (Element of S) holds (s = $1 implies $2 = ( NHReverse (s,X) )));
set I = (the carrier of S);
set FG = ( OSFreeGen X );
A1: (for i being set holds (i in I implies (ex u being set st P[ i,u ])))
proof
let i being set;
assume i in I;
then reconsider s = i as (SortSymbol of S);
take ( NHReverse (s,X) );
let s1 being (Element of S);
assume s1 = i;
hence thesis;
end;
consider H being Function such that A2: (( dom H ) = I & (for i being set holds (i in I implies P[ i,( H . i ) ]))) from CLASSES1:sch 1(A1);
reconsider H as (ManySortedSet of I) by A2,PARTFUN1:def 2,RELAT_1:def 18;
(for x being set holds (x in ( dom H ) implies ( H . x ) is Function))
proof
let i being set;
assume i in ( dom H );
then reconsider s = i as (SortSymbol of S) by A2;
( H . s ) = ( NHReverse (s,X) ) by A2;
hence thesis;
end;
then reconsider H as (ManySortedFunction of I) by FUNCOP_1:def 6;
(for i being set holds (i in I implies ( H . i ) is (Function of ( FG . i ),( ( PTVars X ) . i ))))
proof
let i being set;
assume i in I;
then reconsider s = i as (SortSymbol of S);
A3: ( ( PTVars X ) . s ) = ( PTVars (s,X) ) by CKB63:def 1;
A4: ( H . i ) = ( NHReverse (s,X) ) by A2;
( ( OSFreeGen X ) . s ) = ( OSFreeGen (s,X) ) by CKB68:def 1;
hence thesis by A3,A4;
end;
then reconsider H as (ManySortedFunction of FG,( PTVars X )) by PBOOLE:def 15;
take H;
let s being (Element of S);
thus thesis by A2;
end;
uniqueness
proof
let A being (ManySortedFunction of ( OSFreeGen X ),( PTVars X )),B being (ManySortedFunction of ( OSFreeGen X ),( PTVars X ));
assume that
A5: (for s being (Element of S) holds ( A . s ) = ( NHReverse (s,X) ))
and
A6: (for s being (Element of S) holds ( B . s ) = ( NHReverse (s,X) ));
(for i being set holds (i in (the carrier of S) implies ( A . i ) = ( B . i )))
proof
let i being set;
assume i in (the carrier of S);
then reconsider s = i as (SortSymbol of S);
( A . s ) = ( NHReverse (s,X) ) by A5;
hence thesis by A6;
end;
hence thesis by PBOOLE:3;
end;
end;
