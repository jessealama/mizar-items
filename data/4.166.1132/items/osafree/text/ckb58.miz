begin
theorem
Th28: (for S being  locally_directed OrderSortedSign holds (for X being  non-empty (ManySortedSet of S) holds ( PTCongruence X ) is  monotone))
proof
let S being  locally_directed OrderSortedSign;
let X being  non-empty (ManySortedSet of S);
set PTA = ( ParsedTermsOSA X );
set P = ( PTCongruence X );
thus P is  monotone
proof
let o1 being (OperSymbol of S),o2 being (OperSymbol of S);
assume that
A1: o1 <= o2;
A2: o1 ~= o2 by A1,OSALG_1:def 20;
A3: ( the_result_sort_of o1 ) <= ( the_result_sort_of o2 ) by A1,OSALG_1:def 20;
let x1 being (Element of ( Args (o1,PTA) ));
let x2 being (Element of ( Args (o2,PTA) ));
assume that
A4: (for y being Nat holds (y in ( dom x1 ) implies [ ( x1 . y ),( x2 . y ) ] in ( P . ( ( the_arity_of o2 ) /. y ) )));
( the_arity_of o1 ) <= ( the_arity_of o2 ) by A1,OSALG_1:def 20;
then A5: ( len ( the_arity_of o1 ) ) = ( len ( the_arity_of o2 ) ) by OSALG_1:def 6;
then ( dom ( the_arity_of o2 ) ) = ( dom ( the_arity_of o1 ) ) by FINSEQ_3:29;
then ( dom ( the_arity_of o2 ) ) = ( dom x1 ) by MSUALG_3:6;
hence thesis by A4,A2,A3,A5,CKB57:1;
end;

