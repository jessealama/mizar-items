constructors XXREAL_0,NAT_1,FINSEQOP,FUNCT_7,MSUALG_3,MSAFREE,ORDERS_3,OSALG_2,OSALG_3,OSALG_4,RELSET_1,CKB1,CKB3,CKB4,CKB5,CKB7,CKB12,CKB13,CKB15,CKB21,CKB22,CKB23,CKB25,CKB31,CKB38,CKB41,CKB42,CKB43,CKB44,CKB45,CKB46,CKB47,CKB48;
requirements BOOLE,SUBSET;
begin
reserve S for OrderSortedSign;
reserve X for (ManySortedSet of S);
reserve x for set;
definition
let S being  locally_directed OrderSortedSign;
let X being  non-empty (ManySortedSet of S);
func PTCongruence X ->  MSEquivalence-like (OrderSortedRelation of ( ParsedTermsOSA X )) means 
:Def23: (for i being set holds (i in (the carrier of S) implies ( it . i ) = { [ x,y ] where x is (Element of ( TS ( DTConOSA X ) )),y is (Element of ( TS ( DTConOSA X ) )): [ x,i ] in ( ( PTClasses X ) . y ) }));
existence
proof
set PTA = ( ParsedTermsOSA X );
set SPTA = (the Sorts of PTA);
set D = ( DTConOSA X );
set F = ( PTClasses X );
deffunc F(set) = { [ x,y ] where x is (Element of ( TS D )),y is (Element of ( TS D )): [ x,$1 ] in ( F . y ) };
consider R being (ManySortedSet of (the carrier of S)) such that A1: (for i being set holds (i in (the carrier of S) implies ( R . i ) = F(i))) from PBOOLE:sch 4;
(for i being set holds (i in (the carrier of S) implies ( R . i ) is (Relation of ( SPTA . i ))))
proof
let i being set;
assume that
A2: i in (the carrier of S);
reconsider s = i as (Element of S) by A2;
A3: ( R . i ) = { [ x,y ] where x is (Element of ( TS D )),y is (Element of ( TS D )): [ x,i ] in ( F . y ) } by A1,A2;
( R . s ) c= [: ( SPTA . s ),( SPTA . s ) :]
proof
let z being set;
assume z in ( R . s );
then consider x,y being (Element of ( TS D )) such that A4: z = [ x,y ] and A5: [ x,s ] in ( F . y ) by A3;
[ y,s ] in ( F . x ) by A5,CKB49:1;
then [ x,s ] in ( F . x ) by CKB50:1;
then A6: x in ( SPTA . s ) by CKB49:1;
[ y,s ] in ( F . y ) by A5,CKB50:1;
then y in ( SPTA . s ) by CKB49:1;
hence thesis by A4,A6,ZFMISC_1:87;
end;
hence thesis;
end;
then reconsider R as (ManySortedRelation of PTA) by MSUALG_4:def 1;
(for s1,s2 being (Element of S) holds (s1 <= s2 implies (for x,y being set holds ((x in ( SPTA . s1 ) & y in ( SPTA . s1 )) implies ([ x,y ] in ( R . s1 ) iff [ x,y ] in ( R . s2 ))))))
proof
let s1 being (Element of S);
let s2 being (Element of S);
assume that
A7: s1 <= s2;
A8: ( R . s1 ) = { [ x,y ] where x is (Element of ( TS D )),y is (Element of ( TS D )): [ x,s1 ] in ( F . y ) } by A1;
A9: ( R . s2 ) = { [ x,y ] where x is (Element of ( TS D )),y is (Element of ( TS D )): [ x,s2 ] in ( F . y ) } by A1;
let x being set;
let y being set;
assume that
A10: x in ( SPTA . s1 )
and
A11: y in ( SPTA . s1 );
hereby
assume [ x,y ] in ( R . s1 );
then consider t1,t2 being (Element of ( TS D )) such that A12: [ x,y ] = [ t1,t2 ] and A13: [ t1,s1 ] in ( F . t2 ) by A8;
A14: y = t2 by A12,ZFMISC_1:27;
x = t1 by A12,ZFMISC_1:27;
then [ t1,s2 ] in ( F . t2 ) by A7,A10,A11,A13,A14,CKB51:1;
hence [ x,y ] in ( R . s2 ) by A9,A12;
end;
assume [ x,y ] in ( R . s2 );
then consider t1,t2 being (Element of ( TS D )) such that A15: [ x,y ] = [ t1,t2 ] and A16: [ t1,s2 ] in ( F . t2 ) by A9;
A17: y = t2 by A15,ZFMISC_1:27;
x = t1 by A15,ZFMISC_1:27;
then [ t1,s1 ] in ( F . t2 ) by A7,A10,A11,A16,A17,CKB51:1;
hence thesis by A8,A15;
end;
then A18: R is  os-compatible by OSALG_4:def 1;
defpred IRREL[ (Element of ( TS D )),(Element of S) ]
 means
((ex s1 being (Element of S) st (ex x being set st ((s1 <= $2 & x in ( X . s1 )) & $1 = ( root-tree [ x,s1 ] )))) or (ex o being (OperSymbol of S) st ([ o,(the carrier of S) ] = ( $1 . ( {} ) ) & ( the_result_sort_of o ) <= $2)));
reconsider R as (OrderSortedRelation of PTA) by A18,OSALG_4:def 2;
(for i being set holds (for Ri being (Relation of ( SPTA . i )) holds ((i in (the carrier of S) & ( R . i ) = Ri) implies Ri is  total  symmetric  transitive  total  symmetric  transitive (Relation of ( SPTA . i )))))
proof
let i being set;
let Ri being (Relation of ( SPTA . i ));
assume that
A19: i in (the carrier of S)
and
A20: ( R . i ) = Ri;
reconsider s = i as (Element of S) by A19;
A21: Ri = { [ x,y ] where x is (Element of ( TS D )),y is (Element of ( TS D )): [ x,i ] in ( F . y ) } by A1,A19,A20;
(for x,y being set holds (((x in ( SPTA . i ) & y in ( SPTA . i )) & [ x,y ] in Ri) implies [ y,x ] in Ri))
proof
let x being set;
let y being set;
assume that
x in ( SPTA . i )
and
y in ( SPTA . i )
and
A22: [ x,y ] in Ri;
consider t1,t2 being (Element of ( TS D )) such that A23: [ x,y ] = [ t1,t2 ] and A24: [ t1,s ] in ( F . t2 ) by A21,A22;
A25: x = t1 by A23,ZFMISC_1:27;
A26: y = t2 by A23,ZFMISC_1:27;
[ t2,s ] in ( F . t1 ) by A24,CKB49:1;
hence thesis by A21,A25,A26;
end;
then A27: Ri is_symmetric_in ( SPTA . i ) by RELAT_2:def 3;
now
let x being set;
let y being set;
let z being set;
assume that
x in ( SPTA . i )
and
y in ( SPTA . i )
and
z in ( SPTA . i )
and
A28: [ x,y ] in Ri
and
A29: [ y,z ] in Ri;
consider t1,t2 being (Element of ( TS D )) such that A30: [ x,y ] = [ t1,t2 ] and A31: [ t1,s ] in ( F . t2 ) by A21,A28;
A32: [ t2,s ] in ( F . t1 ) by A31,CKB49:1;
consider t22,t3 being (Element of ( TS D )) such that A33: [ y,z ] = [ t22,t3 ] and A34: [ t22,s ] in ( F . t3 ) by A21,A29;
A35: y = t22 by A33,ZFMISC_1:27;
y = t2 by A30,ZFMISC_1:27;
then [ t3,s ] in ( F . t2 ) by A34,A35,CKB49:1;
then A36: [ t1,s ] in ( F . t3 ) by A32,CKB52:1;
A37: z = t3 by A33,ZFMISC_1:27;
x = t1 by A30,ZFMISC_1:27;
hence [ x,z ] in Ri by A21,A37,A36;
end;
then A38: Ri is_transitive_in ( SPTA . i ) by RELAT_2:def 8;
A39: ( SPTA . s ) = { a where a is (Element of ( TS ( DTConOSA X ) )): IRREL[ a,s ] } by CKB26:1;
now
let x being set;
assume that
A40: x in ( SPTA . i );
consider t being (Element of ( TS D )) such that A41: x = t and IRREL[ t,s ] by A39,A40;
[ t,s ] in ( F . t ) by A40,A41,CKB49:1;
hence [ x,x ] in Ri by A21,A41;
end;
then A42: Ri is_reflexive_in ( SPTA . i ) by RELAT_2:def 1;
then A43: ( field Ri ) = ( SPTA . i ) by ORDERS_1:13;
( dom Ri ) = ( SPTA . i ) by A42,ORDERS_1:13;
hence thesis by A43,A27,A38,PARTFUN1:def 2,RELAT_2:def 11,RELAT_2:def 16;
end;
then R is  MSEquivalence_Relation-like by MSUALG_4:def 2;
then reconsider R as  MSEquivalence-like (OrderSortedRelation of PTA) by MSUALG_4:def 3;
take R;
thus thesis by A1;
end;
uniqueness
proof
set D = ( DTConOSA X );
set F = ( PTClasses X );
let R1 being  MSEquivalence-like (OrderSortedRelation of ( ParsedTermsOSA X ));
let R2 being  MSEquivalence-like (OrderSortedRelation of ( ParsedTermsOSA X ));
assume that
A44: (for i being set holds (i in (the carrier of S) implies ( R1 . i ) = { [ x,y ] where x is (Element of ( TS D )),y is (Element of ( TS D )): [ x,i ] in ( F . y ) }))
and
A45: (for i being set holds (i in (the carrier of S) implies ( R2 . i ) = { [ x,y ] where x is (Element of ( TS D )),y is (Element of ( TS D )): [ x,i ] in ( F . y ) }));
now
let i being set;
assume that
A46: i in (the carrier of S);
( R1 . i ) = { [ x,y ] where x is (Element of ( TS D )),y is (Element of ( TS D )): [ x,i ] in ( F . y ) } by A44,A46;
hence ( R1 . i ) = ( R2 . i ) by A45,A46;
end;
hence R1 = R2 by PBOOLE:3;
end;
end;
