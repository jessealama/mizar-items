constructors XXREAL_0,NAT_1,FINSEQOP,FUNCT_7,MSUALG_3,MSAFREE,ORDERS_3,OSALG_2,OSALG_3,OSALG_4,RELSET_1,CKB1,CKB3,CKB4,CKB5,CKB7,CKB12,CKB13,CKB15,CKB21,CKB22,CKB23,CKB25,CKB31,CKB38;
requirements BOOLE,SUBSET;
begin
theorem
Th19: (for S being  locally_directed OrderSortedSign holds (for X being  non-empty (ManySortedSet of S) holds (for o being (OperSymbol of S) holds (for x being (FinSequence of ( TS ( DTConOSA X ) )) holds (( LeastSorts x ) <= ( the_arity_of o ) iff x in ( Args (o,( ParsedTermsOSA X )) ))))))
proof
let S being  locally_directed OrderSortedSign;
let X being  non-empty (ManySortedSet of S);
let o being (OperSymbol of S);
let x being (FinSequence of ( TS ( DTConOSA X ) ));
set PTA = ( ParsedTermsOSA X );
set D = ( DTConOSA X );
set w = ( the_arity_of o );
set LSx = ( LeastSorts x );
reconsider SPTA = (the Sorts of PTA) as (OrderSortedSet of S);
A1: ( dom LSx ) = ( dom x ) by CKB38:def 1;
hereby
assume A2: ( LeastSorts x ) <= w;
then A3: ( len LSx ) = ( len w ) by OSALG_1:def 6;
then A4: ( dom LSx ) = ( dom w ) by FINSEQ_3:29;
A5: (for k being Nat holds (k in ( dom x ) implies ( x . k ) in ( (the Sorts of PTA) . ( w /. k ) )))
proof
let k being Nat;
assume that
A6: k in ( dom x );
consider t2 being (Element of ( TS ( DTConOSA X ) )) such that A7: t2 = ( x . k ) and A8: ( LSx . k ) = ( LeastSort t2 ) by A6,CKB38:def 1;
reconsider wk = ( w /. k ) as (Element of S);
( w /. k ) = ( w . k ) by A1,A4,A6,PARTFUN1:def 6;
then ( LeastSort t2 ) <= wk by A1,A2,A6,A8,OSALG_1:def 6;
then A9: ( SPTA . ( LeastSort t2 ) ) c= ( SPTA . wk ) by OSALG_1:def 16;
t2 in ( (the Sorts of PTA) . ( LeastSort t2 ) ) by CKB31:def 1;
hence thesis by A7,A9;
end;
( len x ) = ( len w ) by A1,A3,FINSEQ_3:29;
hence x in ( Args (o,PTA) ) by A5,MSAFREE2:5;
end;
assume A10: x in ( Args (o,PTA) );
then A11: ( dom x ) = ( dom w ) by MSUALG_6:2;
hence ( len LSx ) = ( len w ) by A1,FINSEQ_3:29;
let i being set;
assume that
A12: i in ( dom LSx );
reconsider k = i as Nat by A12;
i in ( dom w ) by A11,A12,CKB38:def 1;
then A13: ( x . k ) in ( (the Sorts of PTA) . ( w /. k ) ) by A10,MSUALG_6:2;
i in ( dom x ) by A12,CKB38:def 1;
then A14: (ex t2 being (Element of ( TS D )) st (t2 = ( x . k ) & ( LSx . k ) = ( LeastSort t2 ))) by CKB38:def 1;
let s1 being (Element of S),s2 being (Element of S);
assume that
A15: s1 = ( LSx . i )
and
A16: s2 = ( w . i );
( w /. k ) = ( w . k ) by A1,A11,A12,PARTFUN1:def 6;
hence thesis by A15,A16,A14,A13,CKB31:def 1;
end;
