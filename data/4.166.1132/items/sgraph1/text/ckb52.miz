requirements NUMERALS,REAL,BOOLE,SUBSET,ARITHM;
begin
definition
let n being (Element of ( NAT ));
let m being (Element of ( NAT ));
func K_ (m,n) -> (SimpleGraph of ( NAT ))
means
(ex ee being (Subset of ( TWOELEMENTSETS ( Seg ( m + n ) ) )) st (ee = { { i,j } where i is (Element of ( NAT )),j is (Element of ( NAT )): (i in ( Seg m ) & j in ( nat_interval (( m + 1 ),( m + n )) )) } & it = SimpleGraphStruct (# ( Seg ( m + n ) ),ee #)));
existence
proof
set VV = ( Seg ( m + n ) );
set V1 = ( Seg m );
set V2 = ( nat_interval (( m + 1 ),( m + n )) );
set EE = { { i,j } where i is (Element of ( NAT )),j is (Element of ( NAT )): (i in V1 & j in V2) };
m <= ( m + n ) by NAT_1:11;
then A1: V1 c= VV by FINSEQ_1:5;
A2: V2 c= VV by CKB8:1,NAT_1:11;
EE c= ( TWOELEMENTSETS VV )
proof
let e being set;
assume e in EE;
then consider i0 being (Element of ( NAT )),j0 being (Element of ( NAT )) such that A3: e = { i0,j0 } and A4: (i0 in V1 & j0 in V2);
(i0 in VV & j0 in VV) by A1,A2,A4;
then (for e0 being set holds (e0 in e implies e0 in VV)) by A3,TARSKI:def 2;
then A5: e c= VV by TARSKI:def 3;
m < ( m + 1 ) by NAT_1:13;
then V1 misses V2 by CKB9:1;
then ( V1 /\ V2 ) = ( {} ) by XBOOLE_0:def 7;
then i0 <> j0 by A4,XBOOLE_0:def 4;
hence thesis by A1,A2,A3,A4,A5,CKB14:1;
end;
then reconsider EE as  finite (Subset of ( TWOELEMENTSETS VV )) by CKB19:1,FINSET_1:1;
set IT = SimpleGraphStruct (# VV,EE #);
IT in ( SIMPLEGRAPHS ( NAT ) );
then reconsider IT as (SimpleGraph of ( NAT )) by CKB26:def 1;
reconsider EE as (Subset of ( TWOELEMENTSETS ( Seg ( m + n ) ) ));
take IT;
take EE;
thus thesis;
end;
uniqueness;
end;
