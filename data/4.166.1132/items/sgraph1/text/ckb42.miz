begin
reserve X for set;
definition
let X being set;
let G being (SimpleGraph of X);
let v being set;
func degree (G,v) -> (Element of ( NAT )) means 
:Def11: (ex X being  finite set st ((for z being set holds (z in X iff (z in (the SEdges of G) & v in z))) & it = ( card X )));
existence
proof
defpred X[ set ]
 means
v in $1;
consider X being set such that A1: (for z being set holds (z in X iff (z in (the SEdges of G) & X[ z ]))) from XBOOLE_0:sch 1;
A2: X c= (the SEdges of G)
proof
let z being set;
assume z in X;
hence thesis by A1;
end;
(the SEdges of G) is  finite by CKB31:1;
then reconsider X as  finite set by A2;
take ( card X );
take X;
thus thesis by A1;
end;
uniqueness
proof
let IT1 being (Element of ( NAT ));
let IT2 being (Element of ( NAT ));
assume ((ex X1 being  finite set st ((for z being set holds (z in X1 iff (z in (the SEdges of G) & v in z))) & IT1 = ( card X1 ))) & (ex X2 being  finite set st ((for z being set holds (z in X2 iff (z in (the SEdges of G) & v in z))) & IT2 = ( card X2 ))));
then consider X1,X2 being  finite set such that A3: (for z being set holds (z in X1 iff (z in (the SEdges of G) & v in z))) and A4: IT1 = ( card X1 ) and A5: (for z being set holds (z in X2 iff (z in (the SEdges of G) & v in z))) and A6: IT2 = ( card X2 );
A7: X2 c= X1
proof
let z being set;
assume z in X2;
then (z in (the SEdges of G) & v in z) by A5;
hence thesis by A3;
end;
X1 c= X2
proof
let z being set;
assume z in X1;
then (z in (the SEdges of G) & v in z) by A3;
hence thesis by A5;
end;
hence thesis by A4,A6,A7,XBOOLE_0:def 10;
end;
