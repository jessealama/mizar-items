begin
theorem
Th6: (for k,m,n being (Element of ( NAT )) holds (k < m implies ( Seg k ) misses ( nat_interval (m,n) )))
proof
let k being (Element of ( NAT )),m being (Element of ( NAT )),n being (Element of ( NAT ));
assume A1: k < m;
now
let e being set;
assume A2: e in ( ( Seg k ) /\ ( nat_interval (m,n) ) );
then e in ( nat_interval (m,n) ) by XBOOLE_0:def 4;
then A3: (ex j being (Element of ( NAT )) st ((e = j & m <= j) & j <= n));
e in ( Seg k ) by A2,XBOOLE_0:def 4;
then (ex i being (Element of ( NAT )) st ((e = i & 1 <= i) & i <= k)) by CKB1:1;
hence e in ( {} ) by A1,A3,XXREAL_0:2;
end;
then ( ( Seg k ) /\ ( nat_interval (m,n) ) ) c= ( {} ) by TARSKI:def 3;
then ( ( Seg k ) /\ ( nat_interval (m,n) ) ) = ( {} );
hence thesis by XBOOLE_0:def 7;
end;
