begin
theorem
(for X being non empty set holds (for g being (SimpleGraph of X) holds (for v being set holds (v in (the carrier of g) implies (ex VV being  finite set st (VV = (the carrier of g) & ( degree (g,v) ) < ( card VV )))))))
proof
let X being non empty set;
let g being (SimpleGraph of X);
let v being set;
reconsider VV = (the carrier of g) as  finite set by CKB31:1;
consider ww being  finite set such that A1: ww = { w where w is (Element of X): (w in VV & { v,w } in (the SEdges of g)) } and A2: ( degree (g,v) ) = ( card ww ) by CKB43:1;
assume A3: v in (the carrier of g);
A4:now
assume ww = VV;
then A5: (ex w being (Element of X) st ((v = w & w in VV) & { v,w } in (the SEdges of g))) by A3,A1;
{ v,v } = { v } by ENUMSET1:29;
then consider x being set,y being set such that x in VV and y in VV and A6: x <> y and A7: { v } = { x,y } by A5,CKB14:1;
v = x by A7,ZFMISC_1:4;
hence ww <> VV by A6,A7,ZFMISC_1:4;
end;
take VV;
ww c= VV
proof
let e being set;
assume e in ww;
then (ex w being (Element of X) st ((e = w & w in VV) & { v,w } in (the SEdges of g))) by A1;
hence thesis;
end;
then ww c< VV by A4,XBOOLE_0:def 8;
hence thesis by A2,CARD_2:48;
end;
