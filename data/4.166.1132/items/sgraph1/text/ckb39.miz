begin
reserve X for set;
theorem
Th36: (for OTHER being set holds (OTHER is_SetOfSimpleGraphs_of X implies ( SIMPLEGRAPHS X ) c= OTHER))
proof
let OTHER being set;
defpred X[ set ]
 means
$1 in OTHER;
assume A1: OTHER is_SetOfSimpleGraphs_of X;
A2: (for g being (SimpleGraph of X) holds (for v being set holds ((((g in ( SIMPLEGRAPHS X ) & X[ g ]) & v in X) & (not v in (the carrier of g))) implies X[ SimpleGraphStruct (# ( (the carrier of g) \/ { v } ),( {} ( TWOELEMENTSETS ( (the carrier of g) \/ { v } ) ) ) #) ])))
proof
let g being (SimpleGraph of X);
let v being set;
assume that
g in ( SIMPLEGRAPHS X )
and
A3: ((g in OTHER & v in X) & (not v in (the carrier of g)));
set SVg = (the carrier of g);
SVg is (Subset of X) by CKB31:1;
hence thesis by A1,A3,CKB37:def 1;
end;
A4: (for g being (SimpleGraph of X) holds (for e being set holds (((X[ g ] & e in ( TWOELEMENTSETS (the carrier of g) )) & (not e in (the SEdges of g))) implies (ex sege being (Subset of ( TWOELEMENTSETS (the carrier of g) )) st (sege = ( (the SEdges of g) \/ { e } ) & X[ SimpleGraphStruct (# (the carrier of g),sege #) ])))))
proof
let g being (SimpleGraph of X);
let e being set;
assume that
A5: g in OTHER
and
A6: e in ( TWOELEMENTSETS (the carrier of g) )
and
A7: (not e in (the SEdges of g));
set SVg = (the carrier of g);
set SEg = (the SEdges of g);
consider v1 being set,v2 being set such that A8: ((v1 in SVg & v2 in SVg) & v1 <> v2) and A9: e = { v1,v2 } by A6,CKB14:1;
SVg is  finite  finite (Subset of X) by CKB31:1;
then consider v1v2 being  finite (Subset of ( TWOELEMENTSETS SVg )) such that A10: (v1v2 = ( SEg \/ { { v1,v2 } } ) & SimpleGraphStruct (# SVg,v1v2 #) in OTHER) by A1,A5,A7,A8,A9,CKB37:def 1;
take v1v2;
thus thesis by A9,A10;
end;
let e being set;
assume A11: e in ( SIMPLEGRAPHS X );
A12: X[ SimpleGraphStruct (# ( {} ),( {} ( TWOELEMENTSETS ( {} ) ) ) #) ] by A1,CKB37:def 1;
(for e being set holds (e in ( SIMPLEGRAPHS X ) implies X[ e ])) from CKB33:sch 1(A12,A2,A4);
hence thesis by A11;
end;
