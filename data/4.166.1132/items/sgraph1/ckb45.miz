begin
reserve X for set;
theorem
(for g being (SimpleGraph of X) holds (for v,e being set holds ((e in (the SEdges of g) & ( degree (g,v) ) = ( 0 )) implies (not v in e))))
proof
let g being (SimpleGraph of X);
let v being set,e being set;
assume that
A1: e in (the SEdges of g)
and
A2: ( degree (g,v) ) = ( 0 );
consider Y being  finite set such that A3: (for z being set holds (z in Y iff (z in (the SEdges of g) & v in z))) and A4: ( degree (g,v) ) = ( card Y ) by CKB42:def 1;
assume v in e;
then Y is non empty by A1,A3;
hence contradiction by A2,A4;
end;
