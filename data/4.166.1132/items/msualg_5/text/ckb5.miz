begin
reserve I for set;
reserve d for set;
reserve M for (ManySortedSet of I);
registration
let I;
let M;
cluster  MSEquivalence_Relation-like for (ManySortedRelation of M);
existence
proof
deffunc F(set) = ( id ( M . $1 ) );
consider f being (ManySortedSet of I) such that A1: (for d holds (d in I implies ( f . d ) = F(d))) from PBOOLE:sch 4;
(for i being set holds (i in I implies ( f . i ) is (Relation of ( M . i ),( M . i ))))
proof
let i being set;
assume i in I;
then ( f . i ) = ( id ( M . i ) ) by A1;
hence thesis;
end;
then reconsider f as (ManySortedRelation of M) by MSUALG_4:def 1;
take f;
(for i being set holds (for R being (Relation of ( M . i )) holds ((i in I & ( f . i ) = R) implies R is (Equivalence_Relation of ( M . i )))))
proof
let i being set;
let R being (Relation of ( M . i ));
assume that
A3: i in I
and
A4: ( f . i ) = R;
R = ( id ( M . i ) ) by A1,A3,A4;
hence thesis;
end;
hence thesis by MSUALG_4:def 2;
end;
end;
