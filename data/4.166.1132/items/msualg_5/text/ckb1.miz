requirements NUMERALS,REAL,BOOLE,SUBSET;
begin
reserve X for set;
definition
let X being set;
let R being (Relation of X);
func EqCl R -> (Equivalence_Relation of X) means 
:Def1: (R c= it & (for EqR2 being (Equivalence_Relation of X) holds (R c= EqR2 implies it c= EqR2)));
existence by EQREL_1:12;
uniqueness
proof
let C1 being (Equivalence_Relation of X);
let C2 being (Equivalence_Relation of X);
assume that
A1: R c= C1
and
A2: (for EqR2 being (Equivalence_Relation of X) holds (R c= EqR2 implies C1 c= EqR2))
and
A3: R c= C2
and
A4: (for EqR2 being (Equivalence_Relation of X) holds (R c= EqR2 implies C2 c= EqR2));
A5: C2 c= C1 by A1,A4;
C1 c= C2 by A2,A3;
hence C1 = C2 by A5,XBOOLE_0:def 10;
end;
end;
