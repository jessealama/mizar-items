begin
reserve I for non empty set;
reserve M for (ManySortedSet of I);
theorem
Th13: (for EqR1,EqR2 being (Equivalence_Relation of M) holds ( EqR1 /\ EqR2 ) is (Equivalence_Relation of M))
proof
let EqR1 being (Equivalence_Relation of M),EqR2 being (Equivalence_Relation of M);
(for i being set holds (i in I implies ( ( EqR1 /\ EqR2 ) . i ) is (Relation of ( M . i ),( M . i ))))
proof
let i being set;
assume A1: i in I;
then reconsider U19 = ( EqR1 . i ) as (Relation of ( M . i ),( M . i )) by MSUALG_4:def 1;
reconsider U29 = ( EqR2 . i ) as (Relation of ( M . i ),( M . i )) by A1,MSUALG_4:def 1;
( ( EqR1 /\ EqR2 ) . i ) = ( U19 /\ U29 ) by A1,PBOOLE:def 5;
hence thesis;
end;
then reconsider U3 = ( EqR1 /\ EqR2 ) as (ManySortedRelation of M) by MSUALG_4:def 1;
(for i being set holds (for R being (Relation of ( M . i )) holds ((i in I & ( U3 . i ) = R) implies R is (Equivalence_Relation of ( M . i )))))
proof
let i being set;
let R being (Relation of ( M . i ));
assume that
A2: i in I
and
A3: ( U3 . i ) = R;
reconsider U29 = ( EqR2 . i ) as (Relation of ( M . i )) by A2,MSUALG_4:def 1;
reconsider U29 as (Equivalence_Relation of ( M . i )) by A2,MSUALG_4:def 2;
reconsider U19 = ( EqR1 . i ) as (Relation of ( M . i )) by A2,MSUALG_4:def 1;
reconsider U19 as (Equivalence_Relation of ( M . i )) by A2,MSUALG_4:def 2;
( U19 /\ U29 ) is (Equivalence_Relation of ( M . i ));
hence thesis by A2,A3,PBOOLE:def 5;
end;
hence thesis by MSUALG_4:def 2;
end;
