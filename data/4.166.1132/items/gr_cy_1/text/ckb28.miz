begin
reserve p for Nat;
reserve n for Nat;
registration
let n being non  zero  natural number;
cluster ( INT.Group n ) ->  finite  associative  Group-like;
coherence
proof
set G = ( INT.Group n );
thus (the carrier of ( INT.Group n )) is  finite;
reconsider e = ( 0 ) as (Element of G) by NAT_1:44;
thus (for h,g,f being (Element of G) holds ( ( h * g ) * f ) = ( h * ( g * f ) ))
proof
let h being (Element of G);
let g being (Element of G);
let f being (Element of G);
reconsider A = h,B = g,C = f as (Element of ( Segm n ));
A1: ( g * f ) = ( ( B + C ) mod n ) by CKB26:def 1;
( h * g ) = ( ( A + B ) mod n ) by CKB26:def 1;
hence ( ( h * g ) * f ) = ( ( ( ( A + B ) mod n ) + C ) mod n ) by CKB26:def 1
.= ( ( ( A + B ) + C ) mod n ) by NAT_D:22
.= ( ( A + ( B + C ) ) mod n )
.= ( ( A + ( ( B + C ) mod n ) ) mod n ) by NAT_D:22
.= ( h * ( g * f ) ) by A1,CKB26:def 1;
end;

take e;
let h being (Element of G);
reconsider A = h as (Element of ( Segm n ));
A2: A < n by NAT_1:44;
then consider p being Nat such that A3: n = ( A + p ) by NAT_1:10;
thus ( h * e ) = ( ( A + ( 0 ) ) mod n ) by CKB26:def 1
.= h by A2,NAT_D:24;
thus ( e * h ) = ( ( ( 0 ) + A ) mod n ) by CKB26:def 1
.= h by A2,NAT_D:24;
( p mod n ) < n by NAT_D:1;
then reconsider g = ( p mod n ) as (Element of G) by NAT_1:44;
reconsider B = g as (Element of ( Segm n ));
take g;
A4: p <= n by A3,NAT_1:12;
thus ( h * g ) = e
proof
per cases  by A4,XXREAL_0:1;
suppose A5: p = n;

( h * g ) = ( ( A + B ) mod n ) by CKB26:def 1
.= ( ( ( 0 ) * n ) mod n ) by A3,A5,NAT_D:25
.= e by NAT_D:13;
hence thesis;
end;
suppose A6: p < n;

( h * g ) = ( ( A + B ) mod n ) by CKB26:def 1
.= ( n mod n ) by A3,A6,NAT_D:24
.= e by NAT_D:25;
hence thesis;
end;
end;

thus ( g * h ) = e
proof
per cases  by A4,XXREAL_0:1;
suppose p = n;

then ( g * h ) = ( ( ( n mod n ) + ( 0 ) ) mod n ) by A3,CKB26:def 1
.= ( ( ( 0 ) * n ) mod n ) by NAT_D:25
.= e by NAT_D:13;
hence thesis;
end;
suppose A7: p < n;

( g * h ) = ( ( A + B ) mod n ) by CKB26:def 1
.= ( n mod n ) by A3,A7,NAT_D:24
.= e by NAT_D:25;
hence thesis;
end;
end;

