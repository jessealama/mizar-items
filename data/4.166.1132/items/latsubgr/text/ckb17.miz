begin
theorem
(for G being Group holds (for H being  strict (Subgroup of G) holds (for g1,g2 being (Element of G) holds ((g1 in ( ( carr G ) . H ) & g2 in ( ( carr G ) . H )) implies ( g1 * g2 ) in ( ( carr G ) . H )))))
proof
let G being Group;
let H being  strict (Subgroup of G);
let g1 being (Element of G);
let g2 being (Element of G);
assume (g1 in ( ( carr G ) . H ) & g2 in ( ( carr G ) . H ));
then (g1 in H & g2 in H) by CKB14:1;
then ( g1 * g2 ) in H by GROUP_2:50;
hence thesis by CKB14:1;
end;
