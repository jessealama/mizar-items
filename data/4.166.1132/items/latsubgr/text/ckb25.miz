begin
theorem
Th28: (for G being Group holds (for H1,H2 being (Subgroup of G) holds (for h1,h2 being (Element of ( lattice G )) holds ((h1 = H1 & h2 = H2) implies ( h1 "/\" h2 ) = ( H1 /\ H2 )))))
proof
let G being Group;
let H1 being (Subgroup of G),H2 being (Subgroup of G);
let h1 being (Element of ( lattice G )),h2 being (Element of ( lattice G ));
A1: ( h1 "/\" h2 ) = ( ( SubMeet G ) . (h1,h2) ) by LATTICES:def 2;
assume A2: (h1 = H1 & h2 = H2);
then (H1 is  strict & H2 is  strict) by GROUP_3:def 1;
hence thesis by A2,A1,GROUP_4:def 11;
end;
