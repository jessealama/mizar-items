begin
theorem
Th3: (for G being Group holds (for A being (Subset of G) holds (for H being  strict (Subgroup of G) holds (A = (the carrier of H) implies ( gr A ) = H))))
proof
let G being Group;
let A being (Subset of G);
let H being  strict (Subgroup of G);
assume that
A1: A = (the carrier of H);
( gr ( carr H ) ) = H by GROUP_4:31;
hence thesis by A1,GROUP_2:def 9;
end;
