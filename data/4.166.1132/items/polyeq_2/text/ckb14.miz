begin
theorem
(for x,y being  real number holds (( x + y ) <> ( 0 ) implies ( ( x + y ) |^ 4 ) = ( ( ( x |^ 4 ) + ( ( ( ( 4 * y ) * ( x |^ 3 ) ) + ( ( 6 * ( y ^2 ) ) * ( x ^2 ) ) ) + ( ( 4 * ( y |^ 3 ) ) * x ) ) ) + ( y |^ 4 ) )))
proof
let x being  real number;
let y being  real number;
set g = ( ( ( ( x |^ 3 ) + ( ( ( 3 * y ) * ( x ^2 ) ) + ( ( 3 * ( y ^2 ) ) * x ) ) ) + ( y |^ 3 ) ) * x );
set h = ( ( ( ( x |^ 3 ) + ( ( ( 3 * y ) * ( x ^2 ) ) + ( ( 3 * ( y ^2 ) ) * x ) ) ) + ( y |^ 3 ) ) * y );
set p = ( y |^ 3 );
set q = ( x |^ 3 );
set u = ( x |^ 4 );
set v = ( y |^ 4 );
A1: g = ( ( ( ( x |^ 3 ) * x ) + ( ( ( ( 3 * y ) * ( x ^2 ) ) + ( ( 3 * ( y ^2 ) ) * x ) ) * x ) ) + ( ( y |^ 3 ) * x ) );
A2: ( y |^ 3 ) = ( ( y ^2 ) * y ) by CKB12:1;
assume ( x + y ) <> ( 0 );
then A3: ( ( x + y ) |^ 4 ) = ( g + h ) by CKB13:1;
h = ( ( ( ( x |^ 3 ) * y ) + ( ( ( ( 3 * y ) * ( x ^2 ) ) + ( ( 3 * ( y ^2 ) ) * x ) ) * y ) ) + ( ( y |^ 3 ) * y ) )
.= ( ( ( ( x |^ 3 ) * y ) + ( ( ( ( 3 * y ) * ( x ^2 ) ) + ( ( 3 * ( y ^2 ) ) * x ) ) * y ) ) + ( y |^ 4 ) ) by CKB12:1;
then ( ( x + y ) |^ 4 ) = ( ( ( u + ( ( ( 3 * y ) * ( ( x ^2 ) * x ) ) - ( ( - ( ( 3 * ( y ^2 ) ) * x ) ) * x ) ) ) + ( p * x ) ) + ( ( ( q * y ) + ( ( ( ( 3 * y ) * ( x ^2 ) ) + ( ( 3 * ( y ^2 ) ) * x ) ) * y ) ) + v ) ) by A3,A1,CKB12:1
.= ( ( ( u + ( ( ( 3 * y ) * q ) - ( - ( ( 3 * ( y ^2 ) ) * ( x ^2 ) ) ) ) ) + ( p * x ) ) + ( ( ( q * y ) + ( ( ( ( 3 * y ) * ( x ^2 ) ) + ( ( 3 * ( y ^2 ) ) * x ) ) * y ) ) + v ) ) by CKB12:1
.= ( ( ( u + ( ( 3 * y ) * q ) ) + ( ( ( 3 * ( y ^2 ) ) * ( x ^2 ) ) + ( p * x ) ) ) + ( ( ( q * y ) + ( ( ( 3 * ( y ^2 ) ) * ( x ^2 ) ) + ( ( ( 3 * ( y ^2 ) ) * x ) * y ) ) ) + v ) );
hence thesis by A2;
end;
