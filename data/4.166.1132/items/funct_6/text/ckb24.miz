begin
reserve x for set;
definition
let f being Function;
func doms f -> Function
means
:Def2: (( dom it ) = ( f " ( SubFuncs ( rng f ) ) ) & (for x holds (x in ( f " ( SubFuncs ( rng f ) ) ) implies ( it . x ) = ( proj1 ( f . x ) ))));
existence
proof
deffunc F(set) = ( proj1 ( f . $1 ) );
(ex F being Function st (( dom F ) = ( f " ( SubFuncs ( rng f ) ) ) & (for x holds (x in ( f " ( SubFuncs ( rng f ) ) ) implies ( F . x ) = F(x))))) from FUNCT_1:sch 3;
hence thesis;
end;
uniqueness
proof
let f1 being Function,f2 being Function;
assume that
A1: ( dom f1 ) = ( f " ( SubFuncs ( rng f ) ) )
and
A2: (for x holds (x in ( f " ( SubFuncs ( rng f ) ) ) implies ( f1 . x ) = ( proj1 ( f . x ) )))
and
A3: ( dom f2 ) = ( f " ( SubFuncs ( rng f ) ) )
and
A4: (for x holds (x in ( f " ( SubFuncs ( rng f ) ) ) implies ( f2 . x ) = ( proj1 ( f . x ) )));
now
let x;
assume A5: x in ( f " ( SubFuncs ( rng f ) ) );
then ( f1 . x ) = ( proj1 ( f . x ) ) by A2;
hence ( f1 . x ) = ( f2 . x ) by A4,A5;
end;
hence thesis by A1,A3,FUNCT_1:2;
end;
end;
