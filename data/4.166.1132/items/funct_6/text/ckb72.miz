theorem
(for A,B,C being set holds (for f being Function holds (((A <> ( {} ) & B <> ( {} )) & f in ( Funcs (A,( Funcs (B,C) )) )) implies ( commute ( commute f ) ) = f)))
proof
let A being set;
let B being set;
let C being set;
let f being Function;
assume that
A1: (A <> ( {} ) & B <> ( {} ))
and
A2: f in ( Funcs (A,( Funcs (B,C) )) );
A3: (ex g being Function st ((g = f & ( dom g ) = A) & ( rng g ) c= ( Funcs (B,C) ))) by A2,FUNCT_2:def 2;
set cf = ( commute f );
A4: ( commute f ) in ( Funcs (B,( Funcs (A,C) )) ) by A1,A2,CKB70:1;
then ( commute cf ) in ( Funcs (A,( Funcs (B,C) )) ) by A1,CKB70:1;
then A5: (ex h being Function st ((h = ( commute cf ) & ( dom h ) = A) & ( rng h ) c= ( Funcs (B,C) ))) by FUNCT_2:def 2;
A6: (ex g being Function st ((g = cf & ( dom g ) = B) & ( rng g ) c= ( Funcs (A,C) ))) by A4,FUNCT_2:def 2;
(for x holds (x in A implies ( f . x ) = ( ( commute cf ) . x )))
proof
set ccf = ( commute cf );
set uf = ( uncurry f );
set ucf = ( uncurry cf );
let x;
assume A7: x in A;
then ( f . x ) in ( rng f ) by A3,FUNCT_1:def 3;
then consider g being Function such that A8: (g = ( f . x ) & ( dom g ) = B) and ( rng g ) c= C by A3,FUNCT_2:def 2;
( ccf . x ) in ( rng ccf ) by A5,A7,FUNCT_1:def 3;
then consider h being Function such that A9: h = ( ccf . x ) and A10: ( dom h ) = B and ( rng h ) c= C by A5,FUNCT_2:def 2;
A11: (for y holds (y in B implies (for h being Function holds (h = ( cf . y ) implies (x in ( dom h ) & ( h . x ) = ( g . y ))))))
proof
let y;
assume y in B;
then A12: ([ x,y ] in ( dom uf ) & ( uf . (x,y) ) = ( g . y )) by A3,A7,A8,FUNCT_5:38;
let h being Function;
assume h = ( cf . y );
hence thesis by A12,FUNCT_5:22;
end;
A13: (for y holds (y in B implies ([ y,x ] in ( dom ucf ) & ( ucf . (y,x) ) = ( g . y ))))
proof
let y;
reconsider h = ( cf . y ) as Function;
assume A14: y in B;
then (x in ( dom h ) & ( h . x ) = ( g . y )) by A11;
hence thesis by A6,A14,FUNCT_5:38;
end;
(for y holds (y in B implies ( h . y ) = ( g . y )))
proof
let y;
assume y in B;
then ([ y,x ] in ( dom ucf ) & ( ucf . (y,x) ) = ( g . y )) by A13;
hence thesis by A9,FUNCT_5:22;
end;
hence thesis by A8,A9,A10,FUNCT_1:2;
end;
hence thesis by A3,A5,FUNCT_1:2;
end;
