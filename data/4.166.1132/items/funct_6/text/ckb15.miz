begin
reserve X for set;
reserve Y for set;
reserve Z for set;
reserve f for Function;
reserve g for Function;
theorem
Th24: (f in ( PFuncs (X,( PFuncs (Y,Z) )) ) implies (( uncurry f ) in ( PFuncs ([: X,Y :],Z) ) & ( uncurry' f ) in ( PFuncs ([: Y,X :],Z) )))
proof
assume f in ( PFuncs (X,( PFuncs (Y,Z) )) );
then A1: (ex g st ((f = g & ( dom g ) c= X) & ( rng g ) c= ( PFuncs (Y,Z) ))) by PARTFUN1:def 3;
then (( dom ( uncurry f ) ) c= [: ( dom f ),Y :] & [: ( dom f ),Y :] c= [: X,Y :]) by FUNCT_5:37,ZFMISC_1:96;
then A2: ( dom ( uncurry f ) ) c= [: X,Y :] by XBOOLE_1:1;
(( dom ( uncurry' f ) ) c= [: Y,( dom f ) :] & [: Y,( dom f ) :] c= [: Y,X :]) by A1,FUNCT_5:37,ZFMISC_1:96;
then A3: ( dom ( uncurry' f ) ) c= [: Y,X :] by XBOOLE_1:1;
(( rng ( uncurry f ) ) c= Z & ( rng ( uncurry' f ) ) c= Z) by A1,FUNCT_5:40;
hence thesis by A2,A3,PARTFUN1:def 3;
end;
