begin
reserve x for set;
reserve y for set;
reserve z for set;
reserve f for Function;
theorem
Th37: (f <> ( {} ) implies (x in ( meet f ) iff (for y holds (y in ( dom f ) implies x in ( f . y )))))
proof
assume A1: f <> ( {} );
thus (x in ( meet f ) implies (for y holds (y in ( dom f ) implies x in ( f . y ))))
proof
assume A2: x in ( meet f );
let y;
assume y in ( dom f );
then ( f . y ) in ( rng f ) by FUNCT_1:def 3;
then ( meet f ) c= ( f . y ) by SETFAM_1:3;
hence thesis by A2;
end;

assume A3: (for y holds (y in ( dom f ) implies x in ( f . y )));
now
let z;
assume z in ( rng f );
then (ex y st (y in ( dom f ) & z = ( f . y ))) by FUNCT_1:def 3;
hence x in z by A3;
end;
hence thesis by A1,SETFAM_1:def 1;
end;
