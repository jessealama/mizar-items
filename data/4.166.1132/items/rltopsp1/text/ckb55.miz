begin
reserve r for Real;
theorem
(for X being non  empty RLSStruct holds (for F being  circled-membered (Subset-Family of X) holds ( meet F ) is  circled))
proof
let X being non  empty RLSStruct;
let F being  circled-membered (Subset-Family of X);
let r being Real;
assume that
A1: ( abs r ) <= 1;
let x being set;
assume x in ( r * ( meet F ) );
then consider x9 being (Point of X) such that A2: x = ( r * x9 ) and A3: x9 in ( meet F );
A4:now
let Y being set;
assume A5: Y in F;
then reconsider Y9 = Y as (Subset of X);
x9 in Y by A3,A5,SETFAM_1:def 1;
then A6: ( r * x9 ) in ( r * Y9 );
Y9 is  circled by A5,CKB52:def 1;
then ( r * Y9 ) c= Y9 by A1,CKB40:def 1;
hence x in Y by A2,A6;
end;
F <> ( {} ) by A3,SETFAM_1:def 1;
hence thesis by A4,SETFAM_1:def 1;
end;
