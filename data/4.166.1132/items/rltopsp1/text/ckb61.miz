environ
vocabularies NUMBERS,REAL_1,XBOOLE_0,RLVECT_1,SUBSET_1,PRE_TOPC,RELAT_1,XCMPLX_0,SETFAM_1,TARSKI,ALGSTR_0,ARYTM_3,ZFMISC_1,STRUCT_0,SUPINF_2,ARYTM_1,CONNSP_2,TOPS_1,CONVEX1,CARD_1,XXREAL_0,RELAT_2,COMPLEX1,BINOP_1,FUNCT_1,RCOMP_1,XREAL_0,ORDINAL1,ORDINAL2,TOPS_2,YELLOW13,XXREAL_2,FINSET_1,RVSUM_1,EQREL_1,COMPTS_1,RLTOPSP1,FUNCT_2;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,FINSET_1,SETFAM_1,NUMBERS,XCMPLX_0,XXREAL_0,XREAL_0,ORDINAL1,COMPLEX1,RELAT_1,FUNCT_1,RELSET_1,FUNCT_2,BINOP_1,REAL_1,EQREL_1,DOMAIN_1,STRUCT_0,ALGSTR_0,PRE_TOPC,TOPS_1,TOPS_2,CONNSP_2,COMPTS_1,YELLOW13,RLVECT_1,RUSUB_4,CONVEX1,CKB21,CKB29,CKB32,CKB35,CKB37,CKB40,CKB52,CKB56,CKB59,CKB60;
definitions TARSKI,XBOOLE_0,STRUCT_0,RLVECT_1,PRE_TOPC,COMPTS_1,CONNSP_2,TOPS_2,YELLOW13,CONVEX1,SUBSET_1,ALGSTR_0,SETFAM_1,CKB21,CKB29,CKB32,CKB35,CKB37,CKB40,CKB52,CKB59,CKB60;
theorems TARSKI,XBOOLE_0,XBOOLE_1,ZFMISC_1,SUBSET_1,SETFAM_1,XCMPLX_1,XREAL_0,XCMPLX_0,FUNCT_2,FUNCT_1,ABSVALUE,FINSET_1,EQREL_1,WELLORD2,PRE_TOPC,COMPTS_1,CONNSP_2,TOPS_1,URYSOHN1,YELLOW13,RLVECT_1,RUSUB_4,CONVEX1,TOPGRP_1,TOPS_2,YELLOW_8,XREAL_1,TSEP_1,COMPLEX1,XXREAL_0,CKB1,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB31,CKB32,CKB34,CKB35,CKB36,CKB37,CKB39,CKB40,CKB42,CKB44,CKB45,CKB47,CKB48,CKB50,CKB52,CKB54,CKB55,CKB59,CKB60;
schemes BINOP_1,FUNCT_2,SUBSET_1,EQREL_1,FINSET_1;
registrations XBOOLE_0,SUBSET_1,RELSET_1,PARTFUN1,NUMBERS,XCMPLX_0,XXREAL_0,XREAL_0,REAL_1,EQREL_1,STRUCT_0,TOPS_1,RLVECT_1,CONVEX1,TOPGRP_1,ORDINAL1,BORSUK_1,COMPTS_1,PRE_TOPC,FUNCT_2,CKB2,CKB23,CKB30,CKB33,CKB38,CKB41,CKB43,CKB46,CKB49,CKB51,CKB53,CKB56,CKB57,CKB58;
constructors SETFAM_1,BINOP_1,DOMAIN_1,XXREAL_0,REAL_1,COMPLEX1,EQREL_1,TOPS_1,TOPS_2,CONNSP_2,RUSUB_4,CONVEX1,URYSOHN1,YELLOW13,COMPTS_1,RELSET_1,CKB29,CKB32,CKB35,CKB37,CKB40,CKB52,CKB56,CKB59,CKB60;
requirements BOOLE,REAL,NUMERALS,SUBSET,ARITHM;
begin
registration
cluster  strict  add-continuous  Mult-continuous  TopSpace-like  Abelian  add-associative  right_zeroed  right_complementable  vector-distributive  scalar-distributive  scalar-associative  scalar-unital for non empty non empty RLTopStruct;
existence
proof
set ZS = { ( 0 ) };
set T = { ( {} ),ZS };
T c= ( bool ZS )
proof
let x being set;
assume x in T;
then (x = ( {} ) or x = ZS) by TARSKI:def 2;
then x c= ZS by XBOOLE_1:2;
hence thesis;
end;
then reconsider T as (Subset-Family of ZS);
reconsider O = ( 0 ) as (Element of ZS) by TARSKI:def 1;
deffunc A((Element of ZS),(Element of ZS)) = O;
consider F being (BinOp of ZS) such that A1: (for x,y being (Element of ZS) holds ( F . (x,y) ) = A(x,y)) from BINOP_1:sch 4;
deffunc M( real number,(Element of ZS)) = O;
consider G being (Function of [: ( REAL ),ZS :],ZS) such that A2: (for a being (Element of ( REAL )) holds (for x being (Element of ZS) holds ( G . (a,x) ) = M(a,x))) from BINOP_1:sch 4;
take W = RLTopStruct (# ZS,O,F,G,T #);
thus W is  strict;
thus W is  add-continuous
proof
reconsider V1 = { O },V2 = { O } as (Subset of W);
let x1 being (Point of W),x2 being (Point of W);
let V being (Subset of W);
assume that
A3: V is  open
and
A4: ( x1 + x2 ) in V;
take V1;
take V2;
V1 in T by TARSKI:def 2;
hence (V1 is  open & V2 is  open) by PRE_TOPC:def 2;
thus (x1 in V1 & x2 in V2);
V in T by A3,PRE_TOPC:def 2;
then V = (the carrier of W) by A4,TARSKI:def 2;
hence thesis;
end;

thus W is  Mult-continuous
proof
reconsider V9 = { O } as (Subset of W);
let a being Real;
let x being (Point of W);
let V being (Subset of W);
assume that
A5: V is  open
and
A6: ( a * x ) in V;
take 1;
take V9;
V9 in T by TARSKI:def 2;
hence V9 is  open by PRE_TOPC:def 2;
thus x in V9;
let s being Real;
assume that
( abs ( s - a ) ) < 1;
V in T by A5,PRE_TOPC:def 2;
then (V = ( {} ) or V = ZS) by TARSKI:def 2;
hence thesis by A6;
end;

thus W is  TopSpace-like
proof
thus (the carrier of W) in (the topology of W) by TARSKI:def 2;
hereby
let a being (Subset-Family of W);
assume a c= (the topology of W);
then (((a = ( {} ) or a = { ( {} ) }) or a = { ZS }) or a = { ( {} ),ZS }) by ZFMISC_1:36;
then ((( union a ) = ( {} ) or ( union a ) = ZS) or ( union a ) = ( ( {} ) \/ ZS )) by ZFMISC_1:2,ZFMISC_1:25,ZFMISC_1:75;
hence ( union a ) in (the topology of W) by TARSKI:def 2;
end;
let a being (Subset of W),b being (Subset of W);
assume that
A7: a in (the topology of W)
and
A8: b in (the topology of W);
A9: (b = ( {} ) or b = ZS) by A8,TARSKI:def 2;
(a = ( {} ) or a = ZS) by A7,TARSKI:def 2;
hence thesis by A9,TARSKI:def 2;
end;

thus (for x,y being (Point of W) holds ( x + y ) = ( y + x ))
proof
let x being (Point of W),y being (Point of W);
reconsider X = x,Y = y as (Element of ZS);
( x + y ) = A(X,Y) by A1;
hence thesis by A1;
end;

thus (for x,y,z being (Point of W) holds ( ( x + y ) + z ) = ( x + ( y + z ) ))
proof
let x being (Point of W),y being (Point of W),z being (Point of W);
reconsider X = x,Y = y,Z = z as (Element of ZS);
( ( x + y ) + z ) = A(A(X,Y),Z) by A1;
hence thesis by A1;
end;

thus (for x being (Point of W) holds ( x + ( 0. W ) ) = x)
proof
let x being (Point of W);
reconsider X = x as (Element of ZS);
( x + ( 0. W ) ) = A(X,O) by A1;
hence thesis by TARSKI:def 1;
end;

thus (for x being (Point of W) holds x is  right_complementable)
proof
reconsider y = O as (Point of W);
let x being (Point of W);
take y;
thus thesis by A1;
end;

thus (for a being  real number holds (for x,y being (Point of W) holds ( a * ( x + y ) ) = ( ( a * x ) + ( a * y ) )))
proof
let a being  real number;
reconsider a as Real by XREAL_0:def 1;
let x being (Point of W),y being (Point of W);
reconsider X = x,Y = y as (Element of ZS);
( ( a * x ) + ( a * y ) ) = M(a,A(X,Y)) by A1
.= ( G . (a,A(X,Y)) ) by A2
.= ( G . (a,( F . (x,y) )) ) by A1;
hence thesis;
end;

thus (for a,b being  real number holds (for x being (Point of W) holds ( ( a + b ) * x ) = ( ( a * x ) + ( b * x ) )))
proof
let a being  real number,b being  real number;
let x being (Point of W);
reconsider a,b as Real by XREAL_0:def 1;
set c = ( a + b );
reconsider X = x as (Element of ZS);
( c * x ) = M(c,X) by A2;
hence thesis by A1;
end;

thus (for a,b being  real number holds (for x being (Point of W) holds ( ( a * b ) * x ) = ( a * ( b * x ) )))
proof
let a being  real number,b being  real number;
let x being (Point of W);
reconsider a,b as Real by XREAL_0:def 1;
set c = ( a * b );
reconsider X = x as (Element of ZS);
( c * x ) = M(c,X) by A2;
hence thesis by A2;
end;

thus (for x being (Point of W) holds ( 1 * x ) = x)
proof
reconsider A9 = 1 as (Element of ( REAL ));
let x being (Point of W);
reconsider X = x as (Element of ZS);
( 1 * x ) = M(A9,X) by A2;
hence thesis by TARSKI:def 1;
end;

end;
end;
