theorem
(for T being  complete LATTICE holds (for S being  full non empty (SubRelStr of T) holds (S is  sups-inheriting implies S is  complete)))
proof
let T being  complete LATTICE;
let S being  full non empty (SubRelStr of T);
assume A1: S is  sups-inheriting;
now
let X being set;
set Y = ( X /\ (the carrier of S) );
reconsider Y as (Subset of S) by XBOOLE_1:17;
A2:  ex_sup_of Y,T by YELLOW_0:17;
then ( "\/" (Y,T) ) in (the carrier of S) by A1,YELLOW_0:def 19;
then  ex_sup_of Y,S by A2,YELLOW_0:64;
hence  ex_sup_of X,S by YELLOW_0:50;
end;
