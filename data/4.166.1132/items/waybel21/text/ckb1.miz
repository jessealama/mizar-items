requirements SUBSET,BOOLE;
begin
definition
let S being Semilattice;
let T being Semilattice;
assume that
A1: (S is  upper-bounded implies T is  upper-bounded);
mode SemilatticeHomomorphism of S,T
 -> (Function of S,T)
means :Def1: (for X being  finite (Subset of S) holds it preserves_inf_of X);
existence
proof
reconsider f = ( (the carrier of S) --> ( Top T ) ) as (Function of S,T);
take f;
let X being  finite (Subset of S);
assume that
A2:  ex_inf_of X,S;
per cases ;
suppose A3: X = ( {} );

then A4: ( f .: X ) = ( {} ) by RELAT_1:116;
hence  ex_inf_of ( f .: X ),T by A1,A2,A3,WAYBEL20:5,YELLOW_0:43;
thus ( f . ( inf X ) ) = ( inf ( f .: X ) ) by A4,FUNCOP_1:7;
end;
suppose X <> ( {} );

then reconsider A = X as non  empty (Subset of S);
set a = the (Element of A);
reconsider a as (Element of S);
A5: ( dom f ) = (the carrier of S) by FUNCOP_1:13;
( f . a ) = ( Top T ) by FUNCOP_1:7;
then ( Top T ) in ( f .: X ) by A5,FUNCT_1:def 6;
then A6: { ( Top T ) } c= ( f .: X ) by ZFMISC_1:31;
( f .: X ) c= { ( Top T ) } by FUNCOP_1:81;
then A7: { ( Top T ) } = ( f .: X ) by A6,XBOOLE_0:def 10;
( f . ( inf X ) ) = ( Top T ) by FUNCOP_1:7;
hence thesis by A7,YELLOW_0:38,YELLOW_0:39;
end;
end;
