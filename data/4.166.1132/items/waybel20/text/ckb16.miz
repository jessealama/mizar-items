begin
theorem
Th15: (for L,M being non  empty RelStr holds ((L,M are_isomorphic  & L is  reflexive) implies M is  reflexive))
proof
let L being non  empty RelStr;
let M being non  empty RelStr;
assume that
A1: L,M are_isomorphic 
and
A2: L is  reflexive;
let x being (Element of M);
M,L are_isomorphic  by A1,WAYBEL_1:6;
then consider f being (Function of M,L) such that A3: f is  isomorphic by WAYBEL_1:def 8;
reconsider fx = ( f . x ) as (Element of L);
fx <= fx by A2,YELLOW_0:def 1;
hence thesis by A3,WAYBEL_0:66;
end;
