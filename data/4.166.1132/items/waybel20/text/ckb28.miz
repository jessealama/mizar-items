begin
theorem
(for L1,L2,L3 being non  empty RelStr holds (for f being (Function of L1,L2) holds (for g being (Function of L2,L3) holds ((f is  sups-preserving & g is  sups-preserving) implies ( g * f ) is  sups-preserving))))
proof
let L1 being non  empty RelStr;
let L2 being non  empty RelStr;
let L3 being non  empty RelStr;
let f being (Function of L1,L2);
let g being (Function of L2,L3);
assume that
A1: f is  sups-preserving
and
A2: g is  sups-preserving;
set gf = ( g * f );
let X being (Subset of L1);
assume that
A3:  ex_sup_of X,L1;
set fX = ( f .: X );
set gfX = ( gf .: X );
A4: f preserves_sup_of X by A1,WAYBEL_0:def 33;
then A5: (gfX = ( g .: ( f .: X ) ) &  ex_sup_of fX,L2) by A3,RELAT_1:126,WAYBEL_0:def 31;
A6: ( dom f ) = (the carrier of L1) by FUNCT_2:def 1;
A7: g preserves_sup_of fX by A2,WAYBEL_0:def 33;
hence  ex_sup_of gfX,L3 by A5,WAYBEL_0:def 31;
thus ( sup gfX ) = ( g . ( sup fX ) ) by A7,A5,WAYBEL_0:def 31
.= ( g . ( f . ( sup X ) ) ) by A3,A4,WAYBEL_0:def 31
.= ( gf . ( sup X ) ) by A6,FUNCT_1:13;
end;
