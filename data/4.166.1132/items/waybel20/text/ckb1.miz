begin
theorem
Th1: (for X being set holds (for S being (Subset of ( id X )) holds ( proj1 S ) = ( proj2 S )))
proof
let X being set;
let S being (Subset of ( id X ));
now
let x being set;
hereby
assume x in ( proj1 S );
then consider y being set such that A1: [ x,y ] in S by RELAT_1:def 4;
x = y by A1,RELAT_1:def 10;
hence x in ( proj2 S ) by A1,RELAT_1:def 5;
end;
assume x in ( proj2 S );
then consider y being set such that A2: [ y,x ] in S by RELAT_1:def 5;
x = y by A2,RELAT_1:def 10;
hence x in ( proj1 S ) by A2,RELAT_1:def 4;
end;
hence thesis by TARSKI:1;
end;
