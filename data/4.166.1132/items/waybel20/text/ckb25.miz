begin
theorem
Th25: (for S,T being non  empty RelStr holds (for f being (Function of S,T) holds (f is  monotone implies (for X being (Subset of S) holds (X is  filtered implies ( f .: X ) is  filtered)))))
proof
let S being non  empty RelStr;
let T being non  empty RelStr;
let f being (Function of S,T);
assume A1: f is  monotone;
let X being (Subset of S);
assume that
A2: X is  filtered;
let x being (Element of T);
let y being (Element of T);
assume x in ( f .: X );
then consider a being set such that A3: a in (the carrier of S) and A4: a in X and A5: x = ( f . a ) by FUNCT_2:64;
assume y in ( f .: X );
then consider b being set such that A6: b in (the carrier of S) and A7: b in X and A8: y = ( f . b ) by FUNCT_2:64;
reconsider a,b as (Element of S) by A3,A6;
consider c being (Element of S) such that A9: c in X and A10: (c <= a & c <= b) by A2,A4,A7,WAYBEL_0:def 2;
take z = ( f . c );
thus z in ( f .: X ) by A9,FUNCT_2:35;
thus thesis by A1,A5,A8,A10,WAYBEL_1:def 2;
end;
