environ
vocabularies SUBSET_1,ARYTM_2,ARYTM_3,XBOOLE_0,ARYTM_1;
notations TARSKI,SUBSET_1,ARYTM_3,ARYTM_2,CKB11,CKB33;
definitions CKB11,CKB33;
theorems ARYTM_2,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB37,CKB38,CKB39,CKB40;
constructors ARYTM_2,CKB11,CKB33;
requirements SUBSET;
begin
reserve x for (Element of ( REAL+ ));
reserve y for (Element of ( REAL+ ));
reserve z for (Element of ( REAL+ ));
theorem
((x <=' y & z <=' y) implies ( ( y -' z ) - x ) = ( ( y -' x ) - z ))
proof
assume that
A1: x <=' y
and
A2: z <=' y;
per cases ;
suppose A3: ( x + z ) <=' y;

then z <=' ( y -' x ) by A1,CKB20:1;
then A4: ( ( y -' x ) -' z ) = ( ( y -' x ) - z ) by CKB33:def 1;
x <=' ( y -' z ) by A2,A3,CKB20:1;
then ( ( y -' z ) -' x ) = ( ( y -' z ) - x ) by CKB33:def 1;
hence thesis by A4,CKB22:1;
end;
suppose that A5: (not ( x + z ) <=' y)
and
A6: y <=' x;
A7: (not x <=' ( y -' z )) by A2,A5,CKB20:1;
A8: (not z <=' ( y -' x )) by A1,A5,CKB20:1;
A9: x = y by A1,A6,CKB5:1;
then ( x -' ( x -' z ) ) = z by A2,CKB18:1
.= ( z -' ( x -' x ) ) by CKB12:1,CKB16:1;
hence ( ( y -' z ) - x ) = [ ( {} ),( z -' ( y -' x ) ) ] by A7,A9,CKB33:def 1
.= ( ( y -' x ) - z ) by A8,CKB33:def 1;
end;
suppose that A10: (not ( x + z ) <=' y)
and
A11: y <=' z;
A12: (not x <=' ( y -' z )) by A2,A10,CKB20:1;
A13: (not z <=' ( y -' x )) by A1,A10,CKB20:1;
A14: z = y by A2,A11,CKB5:1;
( x -' ( z -' z ) ) = x by CKB12:1,CKB16:1
.= ( z -' ( z -' x ) ) by A1,A14,CKB18:1;
hence ( ( y -' z ) - x ) = [ ( {} ),( z -' ( y -' x ) ) ] by A12,A14,CKB33:def 1
.= ( ( y -' x ) - z ) by A13,CKB33:def 1;
end;
suppose that A15: (not ( x + z ) <=' y)
and
A16: ((not y <=' x) & (not y <=' z));
A17: (not z <=' ( y -' x )) by A1,A15,CKB20:1;
((not x <=' ( y -' z )) & ( x -' ( y -' z ) ) = ( z -' ( y -' x ) )) by A15,A16,CKB20:1,CKB27:1;
hence ( ( y -' z ) - x ) = [ ( {} ),( z -' ( y -' x ) ) ] by CKB33:def 1
.= ( ( y -' x ) - z ) by A17,CKB33:def 1;
end;
end;
