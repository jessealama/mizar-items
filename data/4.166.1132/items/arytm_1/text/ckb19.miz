environ
vocabularies SUBSET_1,ARYTM_2,ARYTM_3,XBOOLE_0,ARYTM_1;
notations TARSKI,SUBSET_1,ARYTM_3,ARYTM_2,CKB11;
definitions CKB11;
theorems ARYTM_2,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18;
constructors ARYTM_2,CKB11;
requirements SUBSET;
begin
reserve x for (Element of ( REAL+ ));
reserve y for (Element of ( REAL+ ));
reserve z for (Element of ( REAL+ ));
theorem
Lm7: (( z -' y ) <=' x iff z <=' ( x + y ))
proof
per cases ;
suppose y <=' z;

then ( ( z -' y ) + y ) = z by CKB11:def 1;
hence thesis by CKB8:1;
end;
suppose A1: (not y <=' z);

A2: y <=' ( x + y ) by ARYTM_2:19;
( z -' y ) = ( {} ) by A1,CKB11:def 1;
hence thesis by A1,A2,CKB4:1,CKB7:1;
end;
end;
