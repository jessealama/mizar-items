begin
reserve X for set;
reserve A for Ordinal;
theorem
Th9: (for A holds (for X holds (X c= A implies ( RelIncl X ) is  well-ordering)))
proof
let A;
let X;
assume that
A1: X c= A;
( ( RelIncl A ) |_2 X ) is  well-ordering by CKB7:1,WELLORD1:25;
hence thesis by A1,CKB8:1;
end;
