begin
theorem
(for X,Y being non empty set holds (for f being (PartFunc of X,( REAL )) holds ((( f | X ) is  bounded_above & Y c= X) implies ( ( f | Y ) | Y ) is  bounded_above)))
proof
let X being non empty set,Y being non empty set;
let f being (PartFunc of X,( REAL ));
assume ( f | X ) is  bounded_above;
then consider a being  real number such that A1: (for x being set holds (x in ( X /\ ( dom f ) ) implies a >= ( f . x ))) by RFUNCT_1:70;
assume A2: Y c= X;
(for x being set holds (x in ( Y /\ ( dom ( f | Y ) ) ) implies a >= ( ( f | Y ) . x )))
proof
let x being set;
A3: ( ( dom f ) /\ Y ) c= ( ( dom f ) /\ X ) by A2,XBOOLE_1:26;
assume x in ( Y /\ ( dom ( f | Y ) ) );
then A4: x in ( dom ( f | Y ) ) by XBOOLE_0:def 4;
then x in ( ( dom f ) /\ Y ) by RELAT_1:61;
then a >= ( f . x ) by A1,A3;
hence thesis by A4,FUNCT_1:47;
end;
hence thesis by RFUNCT_1:70;
end;
