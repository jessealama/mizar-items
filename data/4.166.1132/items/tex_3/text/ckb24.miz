environ
vocabularies XBOOLE_0,PRE_TOPC,SUBSET_1,TSEP_2,STRUCT_0,TOPS_3,TOPS_1,ZFMISC_1,RCOMP_1,TARSKI,SETFAM_1,NATTRA_1,TDLAT_3;
notations TARSKI,XBOOLE_0,SUBSET_1,STRUCT_0,PRE_TOPC,TOPS_1,BORSUK_1,TSEP_1,TSEP_2,TDLAT_3,TOPS_3,TEX_2,CKB11,CKB23;
definitions TSEP_2,SUBSET_1,STRUCT_0,CKB11,CKB23;
theorems PRE_TOPC,TOPS_1,BORSUK_1,TSEP_1,TMAP_1,TSEP_2,TOPS_3,TEX_1,TEX_2,XBOOLE_0,XBOOLE_1,SUBSET_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23;
registrations XBOOLE_0,STRUCT_0,PRE_TOPC,BORSUK_1,TDLAT_3,TEX_1,TEX_2,TOPS_1,CKB13,CKB14,CKB15,CKB16;
constructors TOPS_1,REALSET2,BORSUK_1,TSEP_1,TDLAT_3,TOPS_3,TEX_2,TSEP_2,CKB11,CKB23;
requirements BOOLE,SUBSET;
begin
reserve X for non  empty TopSpace;
reserve A for (Subset of X);
theorem
Th16: (for X0 being (SubSpace of X) holds (for A being (Subset of X) holds (A = (the carrier of X0) implies (X0 is  everywhere_dense iff A is  everywhere_dense))))
proof
let X0 being (SubSpace of X);
let A being (Subset of X);
assume A1: A = (the carrier of X0);
hence (X0 is  everywhere_dense implies A is  everywhere_dense) by CKB23:def 1;
assume A is  everywhere_dense;
then (for B being (Subset of X) holds (B = (the carrier of X0) implies B is  everywhere_dense)) by A1;
hence thesis by CKB23:def 1;
end;
