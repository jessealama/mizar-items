environ
vocabularies XBOOLE_0,PRE_TOPC,SUBSET_1,TSEP_2,STRUCT_0,TOPS_3,TOPS_1,ZFMISC_1,RCOMP_1,TARSKI,SETFAM_1,NATTRA_1,TDLAT_3;
notations TARSKI,XBOOLE_0,SUBSET_1,STRUCT_0,PRE_TOPC,TOPS_1,BORSUK_1,TSEP_1,TSEP_2,TDLAT_3,TOPS_3,TEX_2,CKB11,CKB23,CKB46,CKB57;
definitions TSEP_2,SUBSET_1,STRUCT_0,CKB11,CKB23,CKB46,CKB57;
theorems PRE_TOPC,TOPS_1,BORSUK_1,TSEP_1,TMAP_1,TSEP_2,TOPS_3,TEX_1,TEX_2,XBOOLE_0,XBOOLE_1,SUBSET_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB40,CKB41,CKB42,CKB43,CKB44,CKB45,CKB46,CKB47,CKB52,CKB53,CKB54,CKB55,CKB56,CKB57,CKB58,CKB63,CKB64,CKB65,CKB66,CKB67,CKB71,CKB72,CKB73,CKB74,CKB75,CKB76,CKB77,CKB78,CKB84,CKB85,CKB88,CKB89,CKB90,CKB91,CKB92,CKB93,CKB94,CKB105,CKB106,CKB107,CKB108,CKB111,CKB112,CKB113,CKB114,CKB115,CKB118,CKB119,CKB120,CKB121,CKB122,CKB123;
registrations XBOOLE_0,STRUCT_0,PRE_TOPC,BORSUK_1,TDLAT_3,TEX_1,TEX_2,TOPS_1,CKB13,CKB14,CKB15,CKB16,CKB25,CKB26,CKB27,CKB28,CKB29,CKB36,CKB37,CKB38,CKB39,CKB48,CKB49,CKB50,CKB51,CKB59,CKB60,CKB61,CKB62,CKB68,CKB69,CKB70,CKB79,CKB80,CKB81,CKB82,CKB83,CKB86,CKB87,CKB95,CKB96,CKB97,CKB98,CKB99,CKB100,CKB101,CKB102,CKB103,CKB104,CKB109,CKB110,CKB116,CKB117;
constructors TOPS_1,REALSET2,BORSUK_1,TSEP_1,TDLAT_3,TOPS_3,TEX_2,TSEP_2,CKB11,CKB23,CKB46,CKB57;
requirements BOOLE,SUBSET;
begin
reserve X for non  empty TopSpace;
reserve B for (Subset of X);
reserve X1 for non  empty (SubSpace of X);
reserve X2 for non  empty (SubSpace of X);
reserve X for non  almost_discrete non  empty TopSpace;
theorem
(for X0 being  everywhere_dense non  empty (SubSpace of X) holds (X0 is  dense  open or (ex X1 being  dense  open  proper  strict non  empty (SubSpace of X) st (ex X2 being  nowhere_dense  strict non  empty (SubSpace of X) st (X1 misses X2 & ( X1 union X2 ) = ( the TopStruct of X0 ))))))
proof
let X0 being  everywhere_dense non  empty (SubSpace of X);
reconsider D = (the carrier of X0) as non  empty (Subset of X) by TSEP_1:1;
D is  everywhere_dense by CKB24:1;
then consider C,B being (Subset of X) such that A1: C is  open  dense and A2: B is  nowhere_dense and A3: ( C \/ B ) = D and A4: C misses B by TOPS_3:49;
C <> ( {} ) by A1,TOPS_3:17;
then consider X1 being  dense  open  strict non  empty (SubSpace of X) such that A5: C = (the carrier of X1) by A1,CKB40:1;
assume A6: (X0 is non  dense or X0 is non  open);
now
assume C is non  proper;
then A7: C = (the carrier of X) by SUBSET_1:def 6;
C c= D by A3,XBOOLE_1:7;
then D = ( [#] X ) by A7,XBOOLE_0:def 10;
then D is  dense  open;
hence contradiction by A6,TSEP_1:16;
end;
then reconsider X1 as  dense  open  proper  strict non  empty (SubSpace of X) by A5,TEX_2:8;
now
per cases  by A6;
suppose A8: X0 is non  dense;

assume B = ( {} );
thus contradiction by A8;
end;
suppose A9: X0 is non  open;

assume B = ( {} );
hence contradiction by A1,A3,A9,TSEP_1:16;
end;
end;
then consider X2 being  nowhere_dense  strict non  empty (SubSpace of X) such that A10: B = (the carrier of X2) by A2,CKB111:1;
take X1;
take X2;
thus X1 misses X2 by A4,A5,A10,TSEP_1:def 3;
(the carrier of ( X1 union X2 )) = (the carrier of X0) by A3,A5,A10,TSEP_1:def 2;
hence thesis by TSEP_1:5;
end;
