environ
vocabularies XBOOLE_0,PRE_TOPC,SUBSET_1,TSEP_2,STRUCT_0,TOPS_3,TOPS_1,ZFMISC_1,RCOMP_1,TARSKI,SETFAM_1,NATTRA_1,TDLAT_3;
notations TARSKI,XBOOLE_0,SUBSET_1,STRUCT_0,PRE_TOPC,TOPS_1,BORSUK_1,TSEP_1,TSEP_2,TDLAT_3,TOPS_3,TEX_2;
definitions TSEP_2,SUBSET_1,STRUCT_0;
theorems PRE_TOPC,TOPS_1,BORSUK_1,TSEP_1,TMAP_1,TSEP_2,TOPS_3,TEX_1,TEX_2,XBOOLE_0,XBOOLE_1,SUBSET_1,CKB1,CKB2;
registrations XBOOLE_0,STRUCT_0,PRE_TOPC,BORSUK_1,TDLAT_3,TEX_1,TEX_2,TOPS_1;
constructors TOPS_1,REALSET2,BORSUK_1,TSEP_1,TDLAT_3,TOPS_3,TEX_2,TSEP_2;
requirements BOOLE,SUBSET;
begin
reserve X for non  empty TopSpace;
reserve A for (Subset of X);
reserve B for (Subset of X);
theorem
Lm2: (((A is  everywhere_dense & B is  dense) or (A is  dense & B is  everywhere_dense)) implies A meets B)
proof
assume ((A is  everywhere_dense & B is  dense) or (A is  dense & B is  everywhere_dense));
then ( A /\ B ) <> ( {} ) by TOPS_3:17,TOPS_3:45;
hence thesis by XBOOLE_0:def 7;
end;
