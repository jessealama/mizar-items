environ
vocabularies XBOOLE_0,PRE_TOPC,SUBSET_1,TSEP_2,STRUCT_0,TOPS_3,TOPS_1,ZFMISC_1,RCOMP_1,TARSKI,SETFAM_1,NATTRA_1,TDLAT_3;
notations TARSKI,XBOOLE_0,SUBSET_1,STRUCT_0,PRE_TOPC,TOPS_1,BORSUK_1,TSEP_1,TSEP_2,TDLAT_3,TOPS_3,TEX_2,CKB11,CKB23;
definitions TSEP_2,SUBSET_1,STRUCT_0,CKB11,CKB23;
theorems PRE_TOPC,TOPS_1,BORSUK_1,TSEP_1,TMAP_1,TSEP_2,TOPS_3,TEX_1,TEX_2,XBOOLE_0,XBOOLE_1,SUBSET_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35;
registrations XBOOLE_0,STRUCT_0,PRE_TOPC,BORSUK_1,TDLAT_3,TEX_1,TEX_2,TOPS_1,CKB13,CKB14,CKB15,CKB16,CKB25,CKB26,CKB27,CKB28,CKB29,CKB36,CKB37,CKB38;
constructors TOPS_1,REALSET2,BORSUK_1,TSEP_1,TDLAT_3,TOPS_3,TEX_2,TSEP_2,CKB11,CKB23;
requirements BOOLE,SUBSET;
begin
reserve X for non  empty TopSpace;
reserve A for (Subset of X);
registration
let X being non  empty TopSpace;
cluster  dense  open  strict non  empty for (SubSpace of X);
existence
proof
X is (SubSpace of X) by TSEP_1:2;
then reconsider A0 = (the carrier of X) as (Subset of X) by TSEP_1:1;
A0 = ( [#] X );
then A0 is  dense;
then consider X0 being  dense  strict non  empty (SubSpace of X) such that A1: A0 = (the carrier of X0) by CKB17:1;
take X0;
now
let A being (Subset of X);
assume A = (the carrier of X0);
then A = ( [#] X ) by A1;
hence A is  open;
end;
hence thesis by TSEP_1:def 1;
end;
end;
