environ
vocabularies XBOOLE_0,PRE_TOPC,SUBSET_1,TSEP_2,STRUCT_0,TOPS_3,TOPS_1,ZFMISC_1,RCOMP_1,TARSKI,SETFAM_1,NATTRA_1,TDLAT_3;
notations TARSKI,XBOOLE_0,SUBSET_1,STRUCT_0,PRE_TOPC,TOPS_1,BORSUK_1,TSEP_1,TSEP_2,TDLAT_3,TOPS_3,TEX_2,CKB11,CKB23;
definitions TSEP_2,SUBSET_1,STRUCT_0,CKB11,CKB23;
theorems PRE_TOPC,TOPS_1,BORSUK_1,TSEP_1,TMAP_1,TSEP_2,TOPS_3,TEX_1,TEX_2,XBOOLE_0,XBOOLE_1,SUBSET_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB30,CKB31,CKB32,CKB33;
registrations XBOOLE_0,STRUCT_0,PRE_TOPC,BORSUK_1,TDLAT_3,TEX_1,TEX_2,TOPS_1,CKB13,CKB14,CKB15,CKB16,CKB25,CKB26,CKB27,CKB28,CKB29;
constructors TOPS_1,REALSET2,BORSUK_1,TSEP_1,TDLAT_3,TOPS_3,TEX_2,TSEP_2,CKB11,CKB23;
requirements BOOLE,SUBSET;
begin
reserve X for non  empty TopSpace;
reserve A for (Subset of X);
theorem
(for X1 being  everywhere_dense non  empty (SubSpace of X) holds (for X2 being  everywhere_dense non  empty (SubSpace of X1) holds X2 is  everywhere_dense  everywhere_dense (SubSpace of X)))
proof
let X1 being  everywhere_dense non  empty (SubSpace of X);
let X2 being  everywhere_dense non  empty (SubSpace of X1);
reconsider C = (the carrier of X1) as (Subset of X) by BORSUK_1:1;
now
let A being (Subset of X);
assume A1: A = (the carrier of X2);
then reconsider B = A as (Subset of X1) by BORSUK_1:1;
A2: C is  everywhere_dense by CKB23:def 1;
B is  everywhere_dense by A1,CKB23:def 1;
hence A is  everywhere_dense by A2,TOPS_3:64;
end;
hence thesis by CKB23:def 1,TSEP_1:7;
end;
