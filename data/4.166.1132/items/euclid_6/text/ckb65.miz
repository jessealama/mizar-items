environ
vocabularies NUMBERS,PRE_TOPC,EUCLID,XREAL_0,ORDINAL1,COMPLEX1,ARYTM_1,CARD_1,SUPINF_2,SUBSET_1,VALUED_0,FINSEQ_1,SIN_COS,COMPLEX2,RELAT_1,XXREAL_0,ARYTM_3,REAL_1,EUCLID_3,COMPTRIG,FINSEQ_6,XCMPLX_0,MCART_1,FUNCT_1,PROB_2,SQUARE_1,RVSUM_1,RLTOPSP1,JGRAPH_6,XBOOLE_0,BORSUK_1,XXREAL_1,TOPS_2,STRUCT_0,PARTFUN1,ORDINAL2,TARSKI,ZFMISC_1,PARSP_2,PROJPL_1,TOPMETR,PCOMPS_1,METRIC_1,SIN_COS6,EUCLID_6;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,RELAT_1,FUNCT_1,ORDINAL1,NUMBERS,XREAL_0,XXREAL_0,PARTFUN1,STRUCT_0,PRE_TOPC,SQUARE_1,VALUED_0,FINSEQ_1,FINSEQ_2,RVSUM_1,RLVECT_1,RLTOPSP1,EUCLID,SIN_COS,JGRAPH_6,COMPTRIG,COMPLEX2,TOPMETR,RCOMP_1,EUCLID_3,TOPS_2,XCMPLX_0,COMPLEX1,REAL_1,METRIC_1,PCOMPS_1,SIN_COS6,CKB21,CKB22,CKB39,CKB40;
definitions TARSKI,EUCLID,COMPLEX1,EUCLID_3,SQUARE_1,VALUED_1,ALGSTR_0,RLTOPSP1,CKB21,CKB22,CKB39;
theorems TARSKI,XCMPLX_1,SQUARE_1,FUNCT_1,COMPLEX1,XBOOLE_0,FUNCT_2,EUCLID,COMPLEX2,XREAL_0,EUCLID_2,TOPREAL1,XREAL_1,COMPTRIG,EUCLID_3,SIN_COS,ZFMISC_1,TOPREAL9,XXREAL_0,SIN_COS6,XXREAL_1,RCOMP_1,JGRAPH_6,JORDAN5A,JORDAN2C,RELSET_1,TOPS_2,JORDAN6,BORSUK_1,JGRAPH_1,TOPMETR,METRIC_1,UNIFORM1,TOPREAL3,JGRAPH_2,TOPREAL5,PRE_TOPC,RFUNCT_2,XBOOLE_1,PARTFUN1,VALUED_1,RELAT_1,RLVECT_1,RLTOPSP1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB37,CKB38,CKB39,CKB41,CKB42,CKB43,CKB44,CKB45,CKB46,CKB47,CKB48,CKB49,CKB50,CKB51,CKB52,CKB53,CKB54,CKB55,CKB56,CKB57,CKB58,CKB59,CKB60,CKB61,CKB62,CKB63,CKB64;
schemes CLASSES1;
registrations XBOOLE_0,RELSET_1,NUMBERS,XXREAL_0,XREAL_0,MEMBERED,MONOID_0,STRUCT_0,EUCLID,XCMPLX_0,TOPMETR,SQUARE_1,FUNCT_2,PRE_TOPC,METRIC_1,BORSUK_1,SIN_COS6,VALUED_0,RVSUM_1,RLTOPSP1;
constructors REAL_1,SQUARE_1,BINOP_2,MONOID_0,SEQ_1,ARYTM_0,RCOMP_1,SIN_COS,JGRAPH_6,COMPTRIG,COMPLEX2,TOPREAL1,EUCLID_3,TOPS_2,SIN_COS6,RFUNCT_2,FUNCSDOM,FUNCT_4,FUNCOP_1,SETFAM_1,CONVEX1,JORDAN2C,FINSEQOP,PCOMPS_1,COMPLEX1,CKB21,CKB22,CKB39;
requirements BOOLE,SUBSET,REAL,NUMERALS,ARITHM;
begin
reserve p1 for (Point of ( TOP-REAL 2 ));
reserve p2 for (Point of ( TOP-REAL 2 ));
reserve p3 for (Point of ( TOP-REAL 2 ));
reserve p4 for (Point of ( TOP-REAL 2 ));
reserve p for (Point of ( TOP-REAL 2 ));
reserve a for  real number;
reserve b for  real number;
reserve r for  real number;
theorem
((((((p1 in ( circle (a,b,r) ) & p2 in ( circle (a,b,r) )) & p3 in ( circle (a,b,r) )) & p4 in ( circle (a,b,r) )) & p in ( LSeg (p1,p3) )) & p in ( LSeg (p2,p4) )) implies ( |. ( p1 - p ) .| * |. ( p - p3 ) .| ) = ( |. ( p2 - p ) .| * |. ( p - p4 ) .| ))
proof
assume that
A1: p1 in ( circle (a,b,r) )
and
A2: p2 in ( circle (a,b,r) )
and
A3: p3 in ( circle (a,b,r) )
and
A4: p4 in ( circle (a,b,r) );
A5: |. ( p1 - p ) .| = |. ( p - p1 ) .| by CKB2:1;
A6: |. ( p3 - p ) .| = |. ( p - p3 ) .| by CKB2:1;
A7: (|. ( p2 - p ) .| = |. ( p - p2 ) .| & |. ( p4 - p ) .| = |. ( p - p4 ) .|) by CKB2:1;
assume that
A8: p in ( LSeg (p1,p3) )
and
A9: p in ( LSeg (p2,p4) );
per cases ;
suppose A10: (not p1,p2,p3,p4 are_mutually_different );

per cases  by A10,ZFMISC_1:def 6;
suppose p1 = p2;

hence thesis by A1,A3,A4,A8,A9,CKB61:1;
end;
suppose p1 = p3;

hence thesis by A1,A2,A4,A8,A9,CKB62:1;
end;
suppose p1 = p4;

hence thesis by A1,A2,A3,A8,A9,A7,CKB61:1;
end;
suppose p2 = p3;

hence thesis by A1,A2,A4,A8,A9,A5,A6,CKB61:1;
end;
suppose p2 = p4;

hence thesis by A1,A2,A3,A8,A9,CKB62:1;
end;
suppose p3 = p4;

hence thesis by A1,A2,A3,A8,A9,A5,A7,CKB61:1;
end;
end;
suppose A11: p1,p2,p3,p4 are_mutually_different ;

then A12: p3 <> p4 by ZFMISC_1:def 6;
A13: p1 <> p2 by A11,ZFMISC_1:def 6;
A14: p2 <> p3 by A11,ZFMISC_1:def 6;
A15: ( ( LSeg (p1,p3) ) \ { p1,p3 } ) c= ( inside_of_circle (a,b,r) ) by A1,A3,TOPREAL9:60;
A16: ( inside_of_circle (a,b,r) ) misses ( circle (a,b,r) ) by TOPREAL9:54;
A17: ( ( LSeg (p2,p4) ) \ { p2,p4 } ) c= ( inside_of_circle (a,b,r) ) by A2,A4,TOPREAL9:60;
A18: (p <> p2 & p <> p3)
proof
assume A19: (p = p2 or p = p3);
per cases  by A19;
suppose A20: p = p3;

(not p3 in { p2,p4 }) by A14,A12,TARSKI:def 2;
then p3 in ( ( LSeg (p2,p4) ) \ { p2,p4 } ) by A9,A20,XBOOLE_0:def 5;
then p3 in ( ( inside_of_circle (a,b,r) ) /\ ( circle (a,b,r) ) ) by A3,A17,XBOOLE_0:def 4;
hence contradiction by A16,XBOOLE_0:def 7;
end;
suppose A21: p = p2;

(not p2 in { p1,p3 }) by A13,A14,TARSKI:def 2;
then p2 in ( ( LSeg (p1,p3) ) \ { p1,p3 } ) by A8,A21,XBOOLE_0:def 5;
then p2 in ( ( inside_of_circle (a,b,r) ) /\ ( circle (a,b,r) ) ) by A2,A15,XBOOLE_0:def 4;
hence contradiction by A16,XBOOLE_0:def 7;
end;
end;
then A22: p2,p,p3 are_mutually_different  by A14,ZFMISC_1:def 5;
A23: p1 <> p4 by A11,ZFMISC_1:def 6;
A24: (p <> p1 & p <> p4)
proof
assume A25: (p = p1 or p = p4);
per cases  by A25;
suppose A26: p = p1;

(not p1 in { p2,p4 }) by A13,A23,TARSKI:def 2;
then p1 in ( ( LSeg (p2,p4) ) \ { p2,p4 } ) by A9,A26,XBOOLE_0:def 5;
then p1 in ( ( inside_of_circle (a,b,r) ) /\ ( circle (a,b,r) ) ) by A1,A17,XBOOLE_0:def 4;
hence contradiction by A16,XBOOLE_0:def 7;
end;
suppose A27: p = p4;

(not p4 in { p1,p3 }) by A23,A12,TARSKI:def 2;
then p4 in ( ( LSeg (p1,p3) ) \ { p1,p3 } ) by A8,A27,XBOOLE_0:def 5;
then p4 in ( ( inside_of_circle (a,b,r) ) /\ ( circle (a,b,r) ) ) by A4,A15,XBOOLE_0:def 4;
hence contradiction by A16,XBOOLE_0:def 7;
end;
end;
then A28: ( angle (p4,p,p1) ) = ( angle (p2,p,p3) ) by A8,A9,A18,CKB34:1;
A29: p2 <> p4 by A11,ZFMISC_1:def 6;
A30: ( angle (p3,p2,p) ) <> ( PI )
proof
assume ( angle (p3,p2,p) ) = ( PI );
then ( angle (p3,p2,p4) ) = ( PI ) by A9,A18,CKB28:1;
hence contradiction by A2,A3,A4,A14,A29,CKB60:1;
end;
A31: p1 <> p3 by A11,ZFMISC_1:def 6;
A32: ( angle (p,p3,p2) ) <> ( PI )
proof
assume ( angle (p,p3,p2) ) = ( PI );
then ( angle (p1,p3,p2) ) = ( PI ) by A8,A18,CKB27:1;
hence contradiction by A1,A2,A3,A31,A14,CKB60:1;
end;
A33: ( angle (p,p1,p4) ) <> ( PI )
proof
assume ( angle (p,p1,p4) ) = ( PI );
then ( angle (p3,p1,p4) ) = ( PI ) by A8,A24,CKB27:1;
hence contradiction by A1,A3,A4,A31,A23,CKB60:1;
end;
A34: ( angle (p,p3,p2) ) = ( angle (p1,p3,p2) ) by A8,A18,CKB27:1;
A35: ( angle (p1,p4,p) ) = ( angle (p1,p4,p2) ) by A9,A24,CKB28:1;
A36: ( angle (p4,p,p1) ) <> ( PI )
proof
assume ( angle (p4,p,p1) ) = ( PI );
then p in ( LSeg (p1,p4) ) by CKB30:1;
hence contradiction by A1,A2,A4,A9,A13,A24,CKB55:1;
end;
then ( angle (p2,p,p3) ) <> ( PI ) by A8,A9,A24,A18,CKB34:1;
then A37: p2,p,p3 is_a_triangle  by A22,A32,A30,CKB41:1;
A38: ( angle (p1,p4,p) ) <> ( PI )
proof
assume ( angle (p1,p4,p) ) = ( PI );
then ( angle (p1,p4,p2) ) = ( PI ) by A9,A24,CKB28:1;
hence contradiction by A1,A2,A4,A23,A29,CKB60:1;
end;
p4,p,p1 are_mutually_different  by A23,A24,ZFMISC_1:def 5;
then A39: p4,p,p1 is_a_triangle  by A36,A33,A38,CKB41:1;
( angle (p1,p4,p2) ) = ( angle (p1,p3,p2) ) by A1,A2,A3,A4,A8,A9,A11,CKB63:1;
hence thesis by A5,A6,A7,A35,A34,A28,A39,A37,CKB45:1;
end;
end;
