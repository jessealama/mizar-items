requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve n for Nat;
reserve k for Nat;
reserve Ak for (Subset of ( TOP-REAL k ));
theorem
Th8: (for A being (Subset of ( TOP-REAL ( k + n ) )) holds (A = { ( v ^ ( n |-> ( 0 ) ) ) where v is (Element of ( TOP-REAL k )): (not contradiction) } implies (for B being (Subset of ( ( TOP-REAL ( k + n ) ) | A )) holds (B = { v where v is (Point of ( TOP-REAL ( k + n ) )): (( v | k ) in Ak & v in A) } implies (Ak is  open iff B is  open)))))
proof
set kn = ( k + n );
set TRn = ( TOP-REAL kn );
set TRk = ( TOP-REAL k );
let A being (Subset of TRn);
assume that
A1: A = { ( v ^ ( n |-> ( 0 ) ) ) where v is (Element of TRk): (not contradiction) };
A2: ( the TopStruct of TRk ) = ( TopSpaceMetr ( Euclid k ) ) by EUCLID:def 8;
then reconsider p = Ak as (Subset of ( TopSpaceMetr ( Euclid k ) ));
set TRA = ( TRn | A );
let B being (Subset of ( TRn | A ));
assume that
A3: B = { v where v is (Element of TRn): (( v | k ) in Ak & v in A) };
A4: ( [#] TRA ) = A by PRE_TOPC:def 5;
A5: kn >= k by NAT_1:11;
hereby
set PP = { v where v is (Element of TRn): ( v | k ) in Ak };
PP c= ( [#] TRn )
proof
let x being set;
assume x in PP;
then (ex v being (Element of TRn) st (x = v & ( v | k ) in Ak));
hence thesis;
end;
then reconsider PP as (Subset of TRn);
A6: ( PP /\ A ) c= B
proof
let x being set;
assume A7: x in ( PP /\ A );
then x in PP by XBOOLE_0:def 4;
then consider v being (Element of TRn) such that A8: (x = v & ( v | k ) in Ak);
x in A by A7,XBOOLE_0:def 4;
hence thesis by A3,A8;
end;
assume Ak is  open;
then PP is  open by A5,CKB12:1;
then PP in (the topology of TRn) by PRE_TOPC:def 2;
then A9: ( PP /\ ( [#] TRA ) ) in (the topology of TRA) by PRE_TOPC:def 4;
B c= ( PP /\ A )
proof
let x being set;
assume x in B;
then consider v being (Element of TRn) such that A10: x = v and A11: ( v | k ) in Ak and A12: v in A by A3;
v in PP by A11;
hence thesis by A10,A12,XBOOLE_0:def 4;
end;
then B = ( PP /\ A ) by A6,XBOOLE_0:def 10;
hence B is  open by A4,A9,PRE_TOPC:def 2;
end;
assume B is  open;
then B in (the topology of TRA) by PRE_TOPC:def 2;
then consider Q being (Subset of TRn) such that A13: Q in (the topology of TRn) and A14: ( Q /\ ( [#] TRA ) ) = B by PRE_TOPC:def 4;
A15: ( the TopStruct of TRn ) = ( TopSpaceMetr ( Euclid kn ) ) by EUCLID:def 8;
then reconsider q = Q as (Subset of ( TopSpaceMetr ( Euclid kn ) ));
A16: q is  open by A13,A15,PRE_TOPC:def 2;
(for e being (Point of ( Euclid k )) holds (e in p implies (ex r being  real number st (r > ( 0 ) & ( OpenHypercube (e,r) ) c= p))))
proof
let e being (Point of ( Euclid k ));
A17: ( len ( n |-> ( 0 ) ) ) = n by CARD_1:def 7;
A18: ( @ ( @ ( e ^ ( n |-> ( 0 ) ) ) ) ) = ( e ^ ( n |-> ( 0 ) ) );
A19: ( len e ) = k by CARD_1:def 7;
then ( len ( e ^ ( n |-> ( 0 ) ) ) ) = kn by A17,FINSEQ_1:22;
then reconsider e0 = ( e ^ ( n |-> ( 0 ) ) ) as (Point of ( Euclid kn )) by A18,TOPREAL7:16;
( dom e ) = ( Seg k ) by A19,FINSEQ_1:def 3;
then A20: e = ( e0 | k ) by FINSEQ_1:21;
( n |-> ( 0 ) ) = ( @ ( @ ( n |-> ( 0 ) ) ) );
then reconsider N = ( n |-> ( 0 ) ) as (Element of ( Euclid n )) by A17,TOPREAL7:16;
e is (Element of TRk) by CKB1:1;
then A21: e0 in A by A1;
assume e in p;
then e0 in B by A3,A21,A20;
then e0 in q by A14,XBOOLE_0:def 4;
then consider m being non zero (Element of ( NAT )) such that A22: ( OpenHypercube (e0,( 1 / m )) ) c= q by A16,EUCLID_9:23;
set r = ( 1 / m );
take r;
thus r > ( 0 ) by XREAL_1:139;
let x being set;
N in ( OpenHypercube (N,r) ) by EUCLID_9:11,XREAL_1:139;
then A23: N in ( product ( Intervals (N,r) ) ) by EUCLID_9:def 4;
assume A24: x in ( OpenHypercube (e,r) );
then reconsider w = x as (Point of TRk) by A2;
A25: ( ( Intervals (e,r) ) ^ ( Intervals (N,r) ) ) = ( Intervals (( e ^ N ),r) ) by CKB2:1;
w in ( product ( Intervals (e,r) ) ) by A24,EUCLID_9:def 4;
then ( w ^ N ) in ( product ( Intervals (e0,r) ) ) by A23,A25,CKB3:1;
then A26: ( w ^ N ) in ( OpenHypercube (e0,r) ) by EUCLID_9:def 4;
( w ^ N ) in A by A1;
then ( w ^ N ) in B by A4,A14,A22,A26,XBOOLE_0:def 4;
then A27: (ex v being (Element of TRn) st ((( w ^ N ) = v & ( v | k ) in Ak) & v in A)) by A3;
( len w ) = k by CARD_1:def 7;
then ( ( w ^ N ) | k ) = ( ( w ^ N ) | ( dom w ) ) by FINSEQ_1:def 3
.= w by FINSEQ_1:21;
hence thesis by A27;
end;
then p is  open by EUCLID_9:24;
then Ak in (the topology of ( TOP-REAL k )) by A2,PRE_TOPC:def 2;
hence thesis by PRE_TOPC:def 2;
