begin
theorem
(for L being  lower-bounded  antisymmetric  transitive  with_infima RelStr holds (for a,b,c being (Element of L) holds ((a misses b or a misses c) implies a misses ( b "/\" c ))))
proof
let L being  lower-bounded  antisymmetric  transitive  with_infima RelStr;
let a being (Element of L),b being (Element of L),c being (Element of L);
assume A1: (a misses b or a misses c);
per cases  by A1;
suppose A2: a misses b;

( a "/\" ( b "/\" c ) ) = ( ( a "/\" b ) "/\" c ) by LATTICE3:16
.= ( ( Bottom L ) "/\" c ) by A2,CKB15:def 1
.= ( Bottom L ) by CKB32:1;
hence thesis by CKB15:def 1;
end;
suppose A3: a misses c;

( a "/\" ( b "/\" c ) ) = ( ( a "/\" c ) "/\" b ) by LATTICE3:16
.= ( ( Bottom L ) "/\" b ) by A3,CKB15:def 1
.= ( Bottom L ) by CKB32:1;
hence thesis by CKB15:def 1;
end;
end;
