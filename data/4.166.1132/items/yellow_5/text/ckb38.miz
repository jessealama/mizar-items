begin
theorem
(for L being  lower-bounded LATTICE holds (for a,b,c being (Element of L) holds (((a <= b & a <= c) & b misses c) implies a = ( Bottom L ))))
proof
let L being  lower-bounded LATTICE;
let a being (Element of L);
let b being (Element of L);
let c being (Element of L);
assume that
A1: a <= b
and
A2: a <= c
and
A3: b misses c;
( b "/\" c ) = ( Bottom L ) by A3,CKB15:def 1;
then (( Bottom L ) <= ( a "/\" c ) & ( a "/\" c ) <= ( Bottom L )) by A1,CKB6:1,YELLOW_0:44;
then ( a "/\" c ) = ( Bottom L ) by YELLOW_0:def 3;
hence thesis by A2,CKB10:1;
end;
