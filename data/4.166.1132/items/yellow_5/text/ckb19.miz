begin
theorem
(for L being  antisymmetric  transitive  with_infima  with_suprema RelStr holds (for a,b,c being (Element of L) holds (a <= c implies ( a \ b ) <= c)))
proof
let L being  antisymmetric  transitive  with_infima  with_suprema RelStr;
let a being (Element of L);
let b being (Element of L);
let c being (Element of L);
A1: ( a "/\" ( 'not' b ) ) <= a by YELLOW_0:23;
assume a <= c;
hence thesis by A1,YELLOW_0:def 2;
end;
