begin
theorem
(for L being  lower-bounded  antisymmetric  transitive  with_infima RelStr holds (for a,b,c being (Element of L) holds ((a misses c & b <= c) implies a misses b)))
proof
let L being  lower-bounded  antisymmetric  transitive  with_infima RelStr;
let a being (Element of L),b being (Element of L),c being (Element of L);
assume that
A1: a misses c
and
A2: b <= c;
( a "/\" c ) = ( Bottom L ) by A1,CKB15:def 1;
then A3: ( a "/\" b ) <= ( Bottom L ) by A2,CKB6:1;
( Bottom L ) <= ( a "/\" b ) by YELLOW_0:44;
then ( a "/\" b ) = ( Bottom L ) by A3,YELLOW_0:def 3;
hence thesis by CKB15:def 1;
end;
