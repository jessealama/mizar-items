environ
vocabularies RELAT_2,LATTICE3,ORDERS_2,SUBSET_1,EQREL_1,XXREAL_0,LATTICES,XBOOLE_0,WAYBEL_0,XBOOLEAN;
notations STRUCT_0,LATTICE3,WAYBEL_0,WAYBEL_1,YELLOW_0,ORDERS_2,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18;
definitions CKB12,CKB13,CKB15;
theorems LATTICE3,YELLOW_0,WAYBEL_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB15,CKB19,CKB20,CKB21;
registrations LATTICE3,YELLOW_0,WAYBEL_1;
constructors LATTICE3,WAYBEL_1,CKB12,CKB13,CKB14,CKB15,CKB18;
begin
theorem
(for L being LATTICE holds (for a being (Element of L) holds (a meets a iff a <> ( Bottom L ))))
proof
let L being LATTICE;
let a being (Element of L);
thus (a meets a implies a <> ( Bottom L ))
proof
assume a meets a;
then ( a "/\" a ) <> ( Bottom L ) by CKB15:def 1;
hence thesis by CKB2:1;
end;

thus (a <> ( Bottom L ) implies a meets a)
proof
assume a <> ( Bottom L );
then ( a "/\" a ) <> ( Bottom L ) by CKB2:1;
hence thesis by CKB15:def 1;
end;

end;
