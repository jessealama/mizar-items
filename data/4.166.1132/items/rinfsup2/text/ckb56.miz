begin
reserve n for (Element of ( NAT ));
reserve m for (Element of ( NAT ));
reserve seq for ExtREAL_sequence;
theorem
Lm6: (for seq being ExtREAL_sequence holds ((( lim_inf seq ) = ( lim_sup seq ) & ( lim_inf seq ) = ( -infty )) implies ((seq is  convergent & ( lim seq ) = ( lim_inf seq )) & ( lim seq ) = ( lim_sup seq ))))
proof
let seq being ExtREAL_sequence;
assume that
A1: ( lim_inf seq ) = ( lim_sup seq )
and
A2: ( lim_inf seq ) = ( -infty );
A3: ( superior_realsequence seq ) is  convergent_to_-infty by A1,A2,CKB49:1;
now
let g being  real number;
assume g < ( 0 );
then consider n being Nat such that A4: (for m being Nat holds (n <= m implies ( ( superior_realsequence seq ) . m ) <= g)) by A3,MESFUNC5:def 10;
now
let m being Nat;
m is (Element of ( NAT )) by ORDINAL1:def 12;
then A5: ( seq . m ) <= ( ( superior_realsequence seq ) . m ) by CKB15:1;
assume n <= m;
then ( ( superior_realsequence seq ) . m ) <= g by A4;
hence ( seq . m ) <= g by A5,XXREAL_0:2;
end;
hence (ex n being Nat st (for m being Nat holds (n <= m implies ( seq . m ) <= g)));
end;
then A6: seq is  convergent_to_-infty by MESFUNC5:def 10;
then seq is  convergent by MESFUNC5:def 11;
hence thesis by A1,A2,A6,MESFUNC5:def 12;
end;
