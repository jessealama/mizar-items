requirements NUMERALS,REAL,BOOLE,SUBSET,ARITHM;
begin
reserve k for (Element of ( NAT ));
reserve seq for ExtREAL_sequence;
theorem
Th25: (seq is  non-increasing implies (( seq ^\ k ) is  non-increasing & ( inf seq ) = ( inf ( seq ^\ k ) )))
proof
set seq0 = ( seq ^\ k );
assume A1: seq is  non-increasing;
now
let n being (Element of ( NAT )),m being (Element of ( NAT ));
assume m <= n;
then ( k + m ) <= ( k + n ) by XREAL_1:6;
then ( seq . ( k + n ) ) <= ( seq . ( k + m ) ) by A1,CKB14:1;
then ( seq0 . n ) <= ( seq . ( k + m ) ) by NAT_1:def 3;
hence ( seq0 . n ) <= ( seq0 . m ) by NAT_1:def 3;
end;
hence ( seq ^\ k ) is  non-increasing by CKB14:1;
now
let y being  ext-real number;
assume y in ( rng seq );
then consider n being set such that A2: n in ( dom seq ) and A3: y = ( seq . n ) by FUNCT_1:def 3;
reconsider n as (Element of ( NAT )) by A2;
( seq0 . n ) = ( seq . ( n + k ) ) by NAT_1:def 3;
then A4: ( inf seq0 ) <= ( seq . ( n + k ) ) by CKB36:1;
n <= ( n + k ) by NAT_1:11;
then ( seq . ( n + k ) ) <= ( seq . n ) by A1,CKB14:1;
hence ( inf ( rng seq0 ) ) <= y by A3,A4,XXREAL_0:2;
end;
then ( inf ( rng seq0 ) ) is (LowerBound of ( rng seq )) by XXREAL_2:def 2;
then A5: ( inf ( rng seq0 ) ) <= ( inf ( rng seq ) ) by XXREAL_2:def 4;
now
let y being  ext-real number;
assume y in ( rng seq0 );
then consider n being set such that A6: n in ( dom seq0 ) and A7: y = ( seq0 . n ) by FUNCT_1:def 3;
reconsider n as (Element of ( NAT )) by A6;
( seq0 . n ) = ( seq . ( n + k ) ) by NAT_1:def 3;
then ( inf seq ) <= ( seq0 . n ) by CKB36:1;
hence ( inf ( rng seq ) ) <= y by A7;
end;
then ( inf ( rng seq ) ) is (LowerBound of ( rng seq0 )) by XXREAL_2:def 2;
then ( inf ( rng seq ) ) <= ( inf ( rng seq0 ) ) by XXREAL_2:def 4;
hence thesis by A5,XXREAL_0:1;
end;
