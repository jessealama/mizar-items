reserve seq for ExtREAL_sequence;
theorem
Th32: (for seq being ExtREAL_sequence holds ((for n being (Element of ( NAT )) holds ( +infty ) <= ( seq . n )) implies seq is  convergent_to_+infty))
proof
let seq being ExtREAL_sequence;
assume A1: (for n being (Element of ( NAT )) holds ( +infty ) <= ( seq . n ));
now
let g being  real number;
assume ( 0 ) < g;
now
let m being Nat;
assume ( 0 ) <= m;
m is (Element of ( NAT )) by ORDINAL1:def 12;
then A2: ( +infty ) <= ( seq . m ) by A1;
g <= ( +infty ) by XXREAL_0:3;
hence g <= ( seq . m ) by A2,XXREAL_0:2;
end;
hence (ex n being Nat st (for m being Nat holds (n <= m implies g <= ( seq . m ))));
end;
hence thesis by MESFUNC5:def 9;
end;
