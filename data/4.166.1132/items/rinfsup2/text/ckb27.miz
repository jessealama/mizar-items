begin
reserve n for (Element of ( NAT ));
reserve m for (Element of ( NAT ));
reserve seq for ExtREAL_sequence;
reserve rseq for Real_Sequence;
theorem
Th14: ((seq = rseq & rseq is  convergent) implies ((seq is  convergent_to_finite_number & seq is  convergent) & ( lim seq ) = ( lim rseq )))
proof
assume that
A1: seq = rseq
and
A2: rseq is  convergent;
A3:now
let e being  real number;
assume ( 0 ) < e;
then consider n being (Element of ( NAT )) such that A4: (for m being (Element of ( NAT )) holds (n <= m implies ( abs ( ( rseq . m ) - ( lim rseq ) ) ) < e)) by A2,SEQ_2:def 7;
set N = n;
now
let m being Nat;
A5: m is (Element of ( NAT )) by ORDINAL1:def 12;
A6: ( ( rseq . m ) - ( lim rseq ) ) = ( ( seq . m ) - ( R_EAL ( lim rseq ) ) ) by A1,SUPINF_2:3;
assume N <= m;
then ( abs ( ( rseq . m ) - ( lim rseq ) ) ) < e by A4,A5;
hence |. ( ( seq . m ) - ( R_EAL ( lim rseq ) ) ) .| < e by A6,EXTREAL2:1;
end;
hence (ex N being Nat st (for m being Nat holds (N <= m implies |. ( ( seq . m ) - ( R_EAL ( lim rseq ) ) ) .| < e)));
end;
then A7: seq is  convergent_to_finite_number by MESFUNC5:def 8;
then seq is  convergent by MESFUNC5:def 11;
hence thesis by A3,A7,MESFUNC5:def 12;
end;
