begin
theorem
Th48: (for G being  finite _Graph holds (for m,n being Nat holds (for x being (Vertex of G) holds (n in ( ( ( ( LexBFS:CSeq G ) . m ) `2 ) . x ) implies (ex y being (Vertex of G) st ((( LexBFS:PickUnnumbered ( ( LexBFS:CSeq G ) . ( ( G .order() ) -' n ) ) ) = y & (not y in ( dom ( ( ( LexBFS:CSeq G ) . ( ( G .order() ) -' n ) ) `1 ) ))) & x in ( G .AdjacentSet { y } )))))))
proof
let G being  finite _Graph;
let m being Nat;
let n being Nat;
set CS = ( LexBFS:CSeq G );
set CSM = ( CS . m );
set V2M = ( CSM `2 );
set CSN = ( CS . ( ( G .order() ) -' n ) );
set VLN = ( CSN `1 );
set V2N = ( CSN `2 );
set on1 = ( ( ( G .order() ) -' n ) + 1 );
set CN1 = ( CS . on1 );
set V21 = ( CN1 `2 );
let x being (Vertex of G);
assume that
A1: n in ( V2M . x );
A2: ( V2M . x ) c= ( ( Seg ( G .order() ) ) \ ( Seg ( ( G .order() ) -' m ) ) ) by CKB93:1;
then A3: ( ( G .order() ) -' m ) < n by A1,CKB12:1;
n <= ( G .order() ) by A1,A2,CKB12:1;
then A4: ( ( G .order() ) -' n ) = ( ( G .order() ) - n ) by XREAL_1:233;
then A5: ( ( G .order() ) -' n ) < ( G .order() ) by A3,XREAL_1:44;
then A6: ( ( G .order() ) -' ( ( G .order() ) -' n ) ) = ( ( G .order() ) - ( ( G .order() ) - n ) ) by A4,XREAL_1:233;
then consider w being (Vertex of G) such that A7: w = ( LexBFS:PickUnnumbered CSN ) and A8: (for v being set holds (((v in ( G .AdjacentSet { w } ) & (not v in ( dom VLN ))) implies ( V21 . v ) = ( ( V2N . v ) \/ { n } )) & (((not v in ( G .AdjacentSet { w } )) or v in ( dom VLN )) implies ( V21 . v ) = ( V2N . v )))) by A3,A4,CKB92:1,XREAL_1:44;
( V2N . x ) c= ( ( Seg ( G .order() ) ) \ ( Seg ( ( G .order() ) -' ( ( G .order() ) -' n ) ) ) ) by CKB93:1;
then A9: (not n in ( V2N . x )) by A6,CKB12:1;
A10:now
per cases ;
suppose m <= ( G .order() );

then ( ( G .order() ) -' m ) = ( ( G .order() ) - m ) by XREAL_1:233;
then ( ( ( G .order() ) - m ) + m ) < ( n + m ) by A3,XREAL_1:6;
then ( ( G .order() ) - n ) < ( ( m + n ) - n ) by XREAL_1:9;
hence on1 <= m by A4,NAT_1:13;
end;
suppose ( G .order() ) < m;

then ( ( G .order() ) -' n ) < m by A5,XXREAL_0:2;
hence on1 <= m by NAT_1:13;
end;
end;
A11: ( ( G .order() ) -' n ) < on1 by XREAL_1:39;
assume A12: (not (ex y being (Vertex of G) st ((( LexBFS:PickUnnumbered CSN ) = y & (not y in ( dom VLN ))) & x in ( G .AdjacentSet { y } ))));
( dom VLN ) <> ( the_Vertices_of G ) by A5,CKB85:1;
then (not x in ( G .AdjacentSet { w } )) by A12,A7,CKB78:1;
then (not n in ( V21 . x )) by A9,A8;
hence contradiction by A1,A6,A10,A11,CKB97:1;
end;
