begin
definition
let G being  finite _Graph;
func MCS:CSeq G -> (MCS:LabelingSeq of G)
means
:Def26: (( it . ( 0 ) ) = ( MCS:Init G ) & (for n being Nat holds ( it . ( n + 1 ) ) = ( MCS:Step ( it . n ) )));
existence
proof
defpred P[ set,set,set ]
 means
((($2 is (MCS:Labeling of G) & $1 is Nat) & (ex nn being Nat st (ex Gn,Gn1 being (MCS:Labeling of G) st ((($1 = nn & $2 = Gn) & $3 = Gn1) & Gn1 = ( MCS:Step Gn ))))) or (((not $2 is (MCS:Labeling of G)) or (not $1 is Nat)) & $2 = $3));
now
let n being set,x being set;
now
per cases ;
suppose A1: (x is (MCS:Labeling of G) & n is Nat);

then reconsider Gn = x as (MCS:Labeling of G);
(ex SGn being (MCS:Labeling of G) st SGn = ( MCS:Step Gn ));
hence (ex y being set st P[ n,x,y ]) by A1;
end;
suppose ((not x is (MCS:Labeling of G)) or (not n is Nat));

hence (ex y being set st P[ n,x,y ]);
end;
end;
hence (ex y being set st P[ n,x,y ]);
end;
then A2: (for n being (Element of ( NAT )) holds (for x being set holds (ex y being set st P[ n,x,y ])));
consider IT being Function such that A3: ((( dom IT ) = ( NAT ) & ( IT . ( 0 ) ) = ( MCS:Init G )) & (for n being (Element of ( NAT )) holds P[ n,( IT . n ),( IT . ( n + 1 ) ) ])) from RECDEF_1:sch 1(A2);
reconsider IT as (ManySortedSet of ( NAT )) by A3,PARTFUN1:def 2,RELAT_1:def 18;
defpred P2[ Nat ]
 means
( IT . $1 ) is (MCS:Labeling of G);
A4:now
let n being Nat;
assume A5: P2[ n ];
n in ( NAT ) by ORDINAL1:def 12;
then (ex nn being Nat st (ex Gn,Gn1 being (MCS:Labeling of G) st (((n = nn & ( IT . n ) = Gn) & ( IT . ( n + 1 ) ) = Gn1) & Gn1 = ( MCS:Step Gn )))) by A3,A5;
hence P2[ ( n + 1 ) ];
end;
A6: P2[ ( 0 ) ] by A3;
(for n being Nat holds P2[ n ]) from NAT_1:sch 2(A6,A4);
then reconsider IT as (MCS:LabelingSeq of G) by CKB113:def 1;
take IT;
thus ( IT . ( 0 ) ) = ( MCS:Init G ) by A3;
let n being Nat;
n in ( NAT ) by ORDINAL1:def 12;
then (ex nn being Nat st (ex Gn,Gn1 being (MCS:Labeling of G) st (((n = nn & ( IT . n ) = Gn) & ( IT . ( n + 1 ) ) = Gn1) & Gn1 = ( MCS:Step Gn )))) by A3;
hence thesis;
end;
uniqueness
proof
let IT1 being (MCS:LabelingSeq of G),IT2 being (MCS:LabelingSeq of G);
assume that
A7: ( IT1 . ( 0 ) ) = ( MCS:Init G )
and
A8: (for n being Nat holds ( IT1 . ( n + 1 ) ) = ( MCS:Step ( IT1 . n ) ))
and
A9: ( IT2 . ( 0 ) ) = ( MCS:Init G )
and
A10: (for n being Nat holds ( IT2 . ( n + 1 ) ) = ( MCS:Step ( IT2 . n ) ));
defpred P[ Nat ]
 means
( IT1 . $1 ) = ( IT2 . $1 );
now
let n being Nat;
assume P[ n ];
then ( IT1 . ( n + 1 ) ) = ( MCS:Step ( IT2 . n ) ) by A8
.= ( IT2 . ( n + 1 ) ) by A10;
hence P[ ( n + 1 ) ];
end;
then A11: (for n being (Element of ( NAT )) holds (P[ n ] implies P[ ( n + 1 ) ]));
A12: P[ ( 0 ) ] by A7,A9;
(for n being (Element of ( NAT )) holds P[ n ]) from NAT_1:sch 1(A12,A11);
then (for n being set holds (n in ( NAT ) implies ( IT1 . n ) = ( IT2 . n )));
hence IT1 = IT2 by PBOOLE:3;
end;
end;
