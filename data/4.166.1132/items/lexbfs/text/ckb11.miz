begin
definition
let f being Function;
let g being Function;
func f .\/ g -> Function means 
:Def2: (( dom it ) = ( ( dom f ) \/ ( dom g ) ) & (for x being set holds (x in ( ( dom f ) \/ ( dom g ) ) implies ( it . x ) = ( ( f . x ) \/ ( g . x ) ))));
existence
proof
defpred P[ set,set ]
 means
( ( f . $1 ) \/ ( g . $1 ) ) = $2;
set A = ( ( dom f ) \/ ( dom g ) );
A1: (for x being set holds (x in A implies (ex y being set st P[ x,y ])));
(ex f being Function st (( dom f ) = A & (for x being set holds (x in A implies P[ x,( f . x ) ])))) from CLASSES1:sch 1(A1);
then consider IT being Function such that A2: ( dom IT ) = A and A3: (for x being set holds (x in A implies P[ x,( IT . x ) ]));
take IT;
thus ( dom IT ) = ( ( dom f ) \/ ( dom g ) ) by A2;
thus thesis by A3;
end;
uniqueness
proof
let IT1 being Function;
let IT2 being Function;
assume that
A4: ( dom IT1 ) = ( ( dom f ) \/ ( dom g ) )
and
A5: (for x being set holds (x in ( ( dom f ) \/ ( dom g ) ) implies ( IT1 . x ) = ( ( f . x ) \/ ( g . x ) )))
and
A6: ( dom IT2 ) = ( ( dom f ) \/ ( dom g ) )
and
A7: (for x being set holds (x in ( ( dom f ) \/ ( dom g ) ) implies ( IT2 . x ) = ( ( f . x ) \/ ( g . x ) )));
now
let x being set;
assume that
A8: x in ( dom IT1 );
( IT1 . x ) = ( ( f . x ) \/ ( g . x ) ) by A4,A5,A8;
hence ( IT1 . x ) = ( IT2 . x ) by A4,A7,A8;
end;
hence thesis by A4,A6,FUNCT_1:2;
end;
end;
