begin
theorem
Th4: (for n,k,m being Nat holds (n <= m implies ( ( Seg k ) \ ( Seg ( k -' n ) ) ) c= ( ( Seg k ) \ ( Seg ( k -' m ) ) )))
proof
let n being Nat;
let k being Nat;
let m being Nat;
assume that
A1: n <= m;
per cases ;
suppose A2: k < m;

A3: (for x being set holds (x in ( ( Seg k ) \ ( Seg ( k -' n ) ) ) implies x in ( Seg k ))) by XBOOLE_0:def 5;
( k -' m ) = ( 0 ) by A2,NAT_2:8;
then ( Seg ( k -' m ) ) = ( {} );
hence thesis by A3,TARSKI:def 3;
end;
suppose A4: m <= k;

now
let x being set;
assume that
A5: x in ( ( Seg k ) \ ( Seg ( k -' n ) ) );
reconsider y = x as (Element of ( NAT )) by A5;
A6: ( k -' n ) < y by A5,CKB12:1;
per cases  by A1,XXREAL_0:1;
suppose m = n;

hence x in ( ( Seg k ) \ ( Seg ( k -' m ) ) ) by A5;
end;
suppose n < m;

then ( k -' m ) < ( k -' n ) by A4,CKB6:1;
then A7: ( k -' m ) < y by A6,XXREAL_0:2;
y <= k by A5,CKB12:1;
hence x in ( ( Seg k ) \ ( Seg ( k -' m ) ) ) by A7,CKB12:1;
end;
end;
hence thesis by TARSKI:def 3;
end;
end;
