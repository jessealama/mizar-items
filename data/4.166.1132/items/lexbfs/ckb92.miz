begin
theorem
Th42: (for G being  finite _Graph holds (for n being Nat holds (n < ( G .order() ) implies (ex w being (Vertex of G) st (w = ( LexBFS:PickUnnumbered ( ( LexBFS:CSeq G ) . n ) ) & (for v being set holds (((v in ( G .AdjacentSet { w } ) & (not v in ( dom ( ( ( LexBFS:CSeq G ) . n ) `1 ) ))) implies ( ( ( ( LexBFS:CSeq G ) . ( n + 1 ) ) `2 ) . v ) = ( ( ( ( ( LexBFS:CSeq G ) . n ) `2 ) . v ) \/ { ( ( G .order() ) -' n ) } )) & (((not v in ( G .AdjacentSet { w } )) or v in ( dom ( ( ( LexBFS:CSeq G ) . n ) `1 ) )) implies ( ( ( ( LexBFS:CSeq G ) . ( n + 1 ) ) `2 ) . v ) = ( ( ( ( LexBFS:CSeq G ) . n ) `2 ) . v )))))))))
proof
let G being  finite _Graph;
let n being Nat;
assume that
A1: n < ( G .order() );
set CS = ( LexBFS:CSeq G );
set CSN = ( CS . n );
set VLN = ( CSN `1 );
set V2N = ( CSN `2 );
set CN1 = ( CS . ( n + 1 ) );
set V21 = ( CN1 `2 );
set w = ( LexBFS:PickUnnumbered CSN );
take w;
A2: CN1 = ( LexBFS:Step CSN ) by CKB72:def 1;
( card ( dom VLN ) ) = n by A1,CKB80:1;
then A3: CN1 = ( LexBFS:Update (CSN,w,n) ) by A1,A2,CKB67:def 1;
now
let v being set;
assume A4: ((not v in ( G .AdjacentSet { w } )) or v in ( dom VLN ));
per cases  by A4;
suppose (not v in ( G .AdjacentSet { w } ));

hence ( V21 . v ) = ( V2N . v ) by A3,CKB64:1;
end;
suppose v in ( dom VLN );

hence ( V21 . v ) = ( V2N . v ) by A3,CKB65:1;
end;
end;
hence thesis by A3,CKB66:1;
end;
