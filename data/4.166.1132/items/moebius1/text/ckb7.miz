theorem
Lm2: (for n being  natural number holds (n <> 1 implies (ex p being Prime st p divides n)))
proof
let n being  natural number;
assume A1: n <> 1;
per cases ;
suppose n is  zero;

hence thesis by INT_2:28,NAT_D:6;
end;
suppose n is non  zero;

then (ex p being (Element of ( NAT )) st (p is  prime & p divides n)) by A1,CKB4:1,INT_2:31;
hence thesis;
end;
end;
