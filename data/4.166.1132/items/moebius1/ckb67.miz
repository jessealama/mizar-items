theorem
Th46: (for p being Prime holds ( ( PFactors p ) * <* p *> ) = <* p *>)
proof
let p being Prime;
set f = <* p *>;
set g = ( PFactors p );
A1: ( dom ( g * f ) ) = { 1 }
proof
thus ( dom ( g * f ) ) c= { 1 }
proof
let x being set;
assume x in ( dom ( g * f ) );
then x in ( dom f ) by FUNCT_1:11;
hence thesis by FINSEQ_1:2,FINSEQ_1:38;
end;

let x being set;
assume A2: x in { 1 };
then x = 1 by TARSKI:def 1;
then ( f . x ) = p by FINSEQ_1:def 8;
then ( f . x ) in ( SetPrimes ) by NEWTON:def 6;
then A3: ( f . x ) in ( dom g ) by PARTFUN1:def 2;
x in ( dom f ) by A2,FINSEQ_1:2,FINSEQ_1:38;
hence thesis by A3,FUNCT_1:11;
end;
A4: (for x being set holds (x in ( dom ( g * f ) ) implies ( ( g * f ) . x ) = ( f . x )))
proof
let x being set;
( ( pfexp p ) . p ) <> ( 0 ) by NAT_3:38;
then A5: p in ( support ( pfexp p ) ) by PRE_POLY:def 7;
assume A6: x in ( dom ( g * f ) );
then A7: x = 1 by A1,TARSKI:def 1;
then ( ( g * f ) . 1 ) = ( g . ( f . 1 ) ) by A6,FUNCT_1:12
.= ( g . p ) by FINSEQ_1:def 8
.= p by A5,CKB64:def 1
.= ( f . 1 ) by FINSEQ_1:def 8;
hence thesis by A7;
end;
( dom f ) = { 1 } by FINSEQ_1:2,FINSEQ_1:38;
hence thesis by A1,A4,FUNCT_1:2;
end;
