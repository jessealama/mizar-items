environ
vocabularies NUMBERS,XBOOLE_0,ORDERS_2,SUBSET_1,RELAT_2,FIN_TOPO,TARSKI,CONNSP_1,PRE_TOPC,STRUCT_0,RELAT_1,RCOMP_1,FINSEQ_1,ORDINAL2,XXREAL_0,NAT_1,ARYTM_3,FUNCT_1,ORDINAL4,PARTFUN1,CARD_1,ARYTM_1,BORSUK_2,RFINSEQ,FINTOPO3,FINTOPO6;
notations TARSKI,XBOOLE_0,SUBSET_1,XCMPLX_0,NUMBERS,XXREAL_0,NAT_1,DOMAIN_1,RELAT_1,FUNCT_1,RELSET_1,PARTFUN1,FINSEQ_1,STRUCT_0,ORDERS_2,FIN_TOPO,FINTOPO3,NAT_D,ENUMSET1,FINTOPO4,PRE_TOPC,RFINSEQ,CKB6,CKB11,CKB15,CKB36,CKB54;
definitions TARSKI,XBOOLE_0,STRUCT_0,SUBSET_1,FIN_TOPO,FINSEQ_1,CKB6,CKB11,CKB15,CKB36,CKB54;
theorems NAT_1,ZFMISC_1,FUNCT_1,FINSEQ_1,RELSET_1,XBOOLE_0,XBOOLE_1,FIN_TOPO,FINTOPO3,ENUMSET1,FINTOPO4,FINSEQ_6,JORDAN4,TARSKI,XREAL_1,SUBSET_1,PRE_TOPC,RFINSEQ,FINSEQ_3,FINSEQ_5,REVROT_1,ORDINAL1,XXREAL_0,PARTFUN1,XREAL_0,NAT_D,CKB2,CKB3,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB15,CKB16,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB36,CKB37,CKB38,CKB39,CKB40,CKB41,CKB42,CKB43,CKB44,CKB45,CKB46,CKB47,CKB48,CKB49,CKB50,CKB51,CKB52,CKB53,CKB54;
schemes NAT_1,RELSET_1;
registrations XBOOLE_0,RELAT_1,FUNCT_1,XXREAL_0,XREAL_0,NAT_1,MEMBERED,FINSEQ_1,STRUCT_0,FIN_TOPO,CKB1,CKB4,CKB13,CKB14,CKB17,CKB35;
constructors DOMAIN_1,NAT_1,EQREL_1,RFINSEQ,RFUNCT_3,NAT_D,FIN_TOPO,FINTOPO3,FINTOPO4,REAL_1,RELSET_1,CKB6,CKB11,CKB15,CKB36,CKB54;
requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve FT for non empty RelStr;
reserve A for (Subset of FT);
reserve C for (Subset of FT);
theorem
(for D being non empty (Subset of FT) holds ((FT is  filled  symmetric & D = ( ( [#] FT ) \ A )) implies (((FT is  connected & A is  connected) & C is_a_component_of D) implies ( ( [#] FT ) \ C ) is  connected)))
proof
let D being non empty (Subset of FT);
assume that
A1: FT is  filled  symmetric
and
A2: D = ( ( [#] FT ) \ A )
and
A3: FT is  connected
and
A4: A is  connected
and
A5: C is_a_component_of D;
consider C1 being (Subset of ( FT | D )) such that A6: C1 = C and A7: C1 is_a_component_of ( FT | D ) by A5,CKB54:def 1;
reconsider C2 = C1 as (Subset of FT) by A6;
C1 c= ( [#] ( FT | D ) );
then C1 c= ( ( [#] FT ) \ A ) by A2,CKB15:def 1;
then A8: ( ( ( [#] FT ) \ A ) ` ) c= ( C2 ` ) by SUBSET_1:12;
then A9: A c= ( C2 ` ) by PRE_TOPC:3;
A10: A c= ( ( [#] FT ) \ C2 ) by A8,PRE_TOPC:3;
A11: C1 is  connected by A7,CKB36:def 1;
now
A misses C1 by A9,SUBSET_1:23;
then A12: ( A /\ C1 ) = ( {} ) by XBOOLE_0:def 7;
let P being (Subset of FT),Q being (Subset of FT);
assume that
A13: ( ( [#] FT ) \ C ) = ( P \/ Q )
and
A14: P misses Q
and
A15: P,Q are_separated ;
A16: C is  connected by A6,A11,CKB49:1;
A17:now
assume A18: A c= Q;
P c= ( Q ` ) by A14,SUBSET_1:23;
then (Q misses ( Q ` ) & ( A /\ P ) c= ( Q /\ ( Q ` ) )) by A18,XBOOLE_1:27,XBOOLE_1:79;
then A19: ( A /\ P ) c= ( {} ) by XBOOLE_0:def 7;
( ( C \/ P ) /\ A ) = ( ( A /\ C ) \/ ( A /\ P ) ) by XBOOLE_1:23
.= ( {} ) by A6,A12,A19,XBOOLE_1:3;
then ( C \/ P ) misses A by XBOOLE_0:def 7;
then ( C \/ P ) c= ( A ` ) by SUBSET_1:23;
then ( C \/ P ) c= ( [#] ( FT | D ) ) by A2,CKB15:def 1;
then reconsider C1P1 = ( C \/ P ) as (Subset of ( FT | D ));
( C \/ P ) is  connected by A1,A3,A13,A15,A16,CKB48:1;
then A20: C1P1 is  connected by CKB49:1;
C c= ( C1 \/ P ) by A6,XBOOLE_1:7;
then C1P1 = C1 by A6,A7,A20,CKB36:def 1;
then A21: P c= C by A6,XBOOLE_1:7;
P c= ( ( [#] FT ) \ C ) by A13,XBOOLE_1:7;
then (C misses ( C ` ) & P c= ( C /\ ( ( [#] FT ) \ C ) )) by A21,XBOOLE_1:19,XBOOLE_1:79;
then P c= ( {} ) by XBOOLE_0:def 7;
hence P = ( {} FT ) by XBOOLE_1:3;
end;
A22: P misses ( P ` ) by XBOOLE_1:79;
A23: Q c= ( ( [#] FT ) \ C ) by A13,XBOOLE_1:7;
now
assume A24: A c= P;
Q c= ( P ` ) by A14,SUBSET_1:23;
then ( A /\ Q ) c= ( P /\ ( P ` ) ) by A24,XBOOLE_1:27;
then A25: ( A /\ Q ) c= ( {} ) by A22,XBOOLE_0:def 7;
( ( C \/ Q ) /\ A ) = ( ( A /\ C ) \/ ( A /\ Q ) ) by XBOOLE_1:23
.= ( {} ) by A6,A12,A25,XBOOLE_1:3;
then ( C \/ Q ) misses A by XBOOLE_0:def 7;
then ( C \/ Q ) c= ( A ` ) by SUBSET_1:23;
then ( C \/ Q ) c= ( [#] ( FT | D ) ) by A2,CKB15:def 1;
then reconsider C1Q1 = ( C \/ Q ) as (Subset of ( FT | D ));
( C \/ Q ) is  connected by A1,A3,A13,A15,A16,CKB48:1;
then A26: C1Q1 is  connected by CKB49:1;
C1 c= ( C1 \/ Q ) by XBOOLE_1:7;
then C1Q1 = C1 by A6,A7,A26,CKB36:def 1;
then Q c= C by A6,XBOOLE_1:7;
then (C misses ( C ` ) & Q c= ( C /\ ( ( [#] FT ) \ C ) )) by A23,XBOOLE_1:19,XBOOLE_1:79;
then Q c= ( {} ) by XBOOLE_0:def 7;
hence Q = ( {} FT ) by XBOOLE_1:3;
end;
hence (P = ( {} FT ) or Q = ( {} FT )) by A1,A4,A6,A10,A13,A15,A17,CKB44:1;
end;
hence thesis by A1,CKB10:1;
end;
