registrations XBOOLE_0,RELAT_1,FUNCT_1,XXREAL_0,XREAL_0,NAT_1,MEMBERED,FINSEQ_1,STRUCT_0,FIN_TOPO,CKB1,CKB4,CKB13,CKB14,CKB17;
constructors DOMAIN_1,NAT_1,EQREL_1,RFINSEQ,RFUNCT_3,NAT_D,FIN_TOPO,FINTOPO3,FINTOPO4,REAL_1,RELSET_1,CKB6,CKB11,CKB15;
requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve FT for non  empty RelStr;
theorem
(for X9 being non  empty (SubSpace of FT) holds (for P1,Q1 being (Subset of FT) holds (for P,Q being (Subset of X9) holds ((P = P1 & Q = Q1) implies (P,Q are_separated  implies P1,Q1 are_separated )))))
proof
let X9 being non  empty (SubSpace of FT);
let P1 being (Subset of FT);
let Q1 being (Subset of FT);
let P being (Subset of X9);
let Q being (Subset of X9);
assume that
A1: (P = P1 & Q = Q1);
reconsider P2 = P,Q2 = Q as (Subset of FT) by CKB19:1;
assume A2: P,Q are_separated ;
then ( P ^b ) misses Q by FINTOPO4:def 1;
then A3: ( ( P ^b ) /\ Q ) = ( {} ) by XBOOLE_0:def 7;
P misses ( Q ^b ) by A2,FINTOPO4:def 1;
then A4: ( P /\ ( Q ^b ) ) = ( {} ) by XBOOLE_0:def 7;
( P /\ ( Q ^b ) ) = ( P /\ ( ( [#] X9 ) /\ ( Q2 ^b ) ) ) by CKB22:1
.= ( ( P /\ ( [#] X9 ) ) /\ ( Q2 ^b ) ) by XBOOLE_1:16
.= ( P2 /\ ( Q2 ^b ) ) by XBOOLE_1:28;
then A5: P2 misses ( Q2 ^b ) by A4,XBOOLE_0:def 7;
( ( P ^b ) /\ Q ) = ( ( ( P2 ^b ) /\ ( [#] X9 ) ) /\ Q ) by CKB22:1
.= ( ( P2 ^b ) /\ ( Q /\ ( [#] X9 ) ) ) by XBOOLE_1:16
.= ( ( P2 ^b ) /\ Q2 ) by XBOOLE_1:28;
then ( P2 ^b ) misses Q2 by A3,XBOOLE_0:def 7;
hence thesis by A1,A5,FINTOPO4:def 1;
end;
