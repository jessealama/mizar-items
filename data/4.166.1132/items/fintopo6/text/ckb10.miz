begin
reserve FT for non  empty RelStr;
reserve A for (Subset of FT);
reserve B for (Subset of FT);
reserve C for (Subset of FT);
theorem
Th7: (for A being (Subset of FT) holds ((FT is  symmetric & (for A2,B2 being (Subset of FT) holds (((A = ( A2 \/ B2 ) & A2 misses B2) & A2,B2 are_separated ) implies (A2 = ( {} FT ) or B2 = ( {} FT ))))) implies A is  connected))
proof
let A being (Subset of FT);
assume A1: FT is  symmetric;
assume A2: (for A2,B2 being (Subset of FT) holds (((A = ( A2 \/ B2 ) & A2 misses B2) & A2,B2 are_separated ) implies (A2 = ( {} FT ) or B2 = ( {} FT ))));
(for B,C being (Subset of FT) holds ((((A = ( B \/ C ) & B <> ( {} )) & C <> ( {} )) & B misses C) implies (( B ^b ) meets C & B meets ( C ^b ))))
proof
let B being (Subset of FT);
let C being (Subset of FT);
assume (((A = ( B \/ C ) & B <> ( {} )) & C <> ( {} )) & B misses C);
then (not B,C are_separated ) by A2;
then (not (( B ^b ) misses C & B misses ( C ^b ))) by FINTOPO4:def 1;
hence thesis by A1,CKB9:1;
end;
hence thesis by CKB5:1;
end;
