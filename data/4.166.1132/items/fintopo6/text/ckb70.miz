begin
reserve FT for non empty RelStr;
theorem
Lm4: (for f being (FinSequence of FT) holds (for A being (Subset of FT) holds (for x1,x2 being (Element of FT) holds ((((f is  continuous & ( rng f ) c= A) & ( f . 1 ) = x1) & ( f . ( len f ) ) = x2) implies (ex g being (FinSequence of FT) st ((((g is  continuous & ( rng g ) c= A) & ( g . 1 ) = x1) & ( g . ( len g ) ) = x2) & (for h being (FinSequence of FT) holds ((((h is  continuous & ( rng h ) c= A) & ( h . 1 ) = x1) & ( h . ( len h ) ) = x2) implies ( len g ) <= ( len h )))))))))
proof
let f being (FinSequence of FT);
let A being (Subset of FT);
let x1 being (Element of FT),x2 being (Element of FT);
defpred P[ Nat ]
 means
(ex g being (FinSequence of FT) st ((((g is  continuous & ( rng g ) c= A) & ( g . 1 ) = x1) & ( g . ( len g ) ) = x2) & $1 = ( len g )));
assume (((f is  continuous & ( rng f ) c= A) & ( f . 1 ) = x1) & ( f . ( len f ) ) = x2);
then A1: (ex k being Nat st P[ k ]);
(ex k being Nat st (P[ k ] & (for n being Nat holds (P[ n ] implies k <= n)))) from NAT_1:sch 5(A1);
then consider k0 being Nat such that A2: P[ k0 ] and A3: (for n being Nat holds (P[ n ] implies k0 <= n));
consider g0 being (FinSequence of FT) such that A4: (((g0 is  continuous & ( rng g0 ) c= A) & ( g0 . 1 ) = x1) & ( g0 . ( len g0 ) ) = x2) and A5: k0 = ( len g0 ) by A2;
(for h being (FinSequence of FT) holds ((((h is  continuous & ( rng h ) c= A) & ( h . 1 ) = x1) & ( h . ( len h ) ) = x2) implies ( len g0 ) <= ( len h ))) by A3,A5;
hence thesis by A4;
end;
