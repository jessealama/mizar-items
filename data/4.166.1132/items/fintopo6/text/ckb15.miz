environ
vocabularies NUMBERS,XBOOLE_0,ORDERS_2,SUBSET_1,RELAT_2,FIN_TOPO,TARSKI,CONNSP_1,PRE_TOPC,STRUCT_0,RELAT_1,RCOMP_1,FINSEQ_1,ORDINAL2,XXREAL_0,NAT_1,ARYTM_3,FUNCT_1,ORDINAL4,PARTFUN1,CARD_1,ARYTM_1,BORSUK_2,RFINSEQ,FINTOPO3,FINTOPO6;
notations TARSKI,XBOOLE_0,SUBSET_1,XCMPLX_0,NUMBERS,XXREAL_0,NAT_1,DOMAIN_1,RELAT_1,FUNCT_1,RELSET_1,PARTFUN1,FINSEQ_1,STRUCT_0,ORDERS_2,FIN_TOPO,FINTOPO3,NAT_D,ENUMSET1,FINTOPO4,PRE_TOPC,RFINSEQ,CKB6,CKB11;
definitions TARSKI,XBOOLE_0,STRUCT_0,SUBSET_1,FIN_TOPO,FINSEQ_1,CKB6,CKB11;
theorems NAT_1,ZFMISC_1,FUNCT_1,FINSEQ_1,RELSET_1,XBOOLE_0,XBOOLE_1,FIN_TOPO,FINTOPO3,ENUMSET1,FINTOPO4,FINSEQ_6,JORDAN4,TARSKI,XREAL_1,SUBSET_1,PRE_TOPC,RFINSEQ,FINSEQ_3,FINSEQ_5,REVROT_1,ORDINAL1,XXREAL_0,PARTFUN1,XREAL_0,NAT_D,CKB2,CKB3,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12;
schemes NAT_1,RELSET_1;
registrations XBOOLE_0,RELAT_1,FUNCT_1,XXREAL_0,XREAL_0,NAT_1,MEMBERED,FINSEQ_1,STRUCT_0,FIN_TOPO,CKB1,CKB4,CKB13,CKB14;
constructors DOMAIN_1,NAT_1,EQREL_1,RFINSEQ,RFUNCT_3,NAT_D,FIN_TOPO,FINTOPO3,FINTOPO4,REAL_1,RELSET_1,CKB6,CKB11;
requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
definition
let T being non empty RelStr;
let P being non empty (Subset of T);
func T | P ->  strict non empty (SubSpace of T)
means
:Def3: ( [#] it ) = P;
existence
proof
deffunc F(set) = ( ( Im ((the InternalRel of T),$1) ) /\ P );
A1: (for x being (Element of T) holds (x in P implies F(x) c= P)) by XBOOLE_1:17;
consider G being (Relation of P) such that A2: (for y being (Element of T) holds (y in P implies ( Im (G,y) ) = F(y))) from RELSET_1:sch 3(A1);
set FS = RelStr (# P,G #);
(for x being (Element of FS) holds (x in (the carrier of FS) implies ( U_FT x ) = ( ( Im ((the InternalRel of T),x) ) /\ (the carrier of FS) ))) by A2;
then (( [#] FS ) = (the carrier of FS) & FS is  strict non empty  strict non empty (SubSpace of T)) by CKB11:def 1;
hence thesis;
end;
uniqueness
proof
let Z1 being  strict non empty (SubSpace of T),Z2 being  strict non empty (SubSpace of T);
assume that
A3: ( [#] Z1 ) = P
and
A4: ( [#] Z2 ) = P;
reconsider R1 = (the InternalRel of Z1),R2 = (the InternalRel of Z2) as (Relation of P) by A3,A4;
(for z being set holds (z in P implies ( Im (R1,z) ) = ( Im (R2,z) )))
proof
let z being set;
assume A5: z in P;
then reconsider z1 = z as (Element of Z1) by A3;
reconsider z2 = z as (Element of Z2) by A4,A5;
thus ( Im (R1,z) ) = ( U_FT z1 )
.= ( ( Im ((the InternalRel of T),z1) ) /\ (the carrier of Z1) ) by CKB11:def 1
.= ( U_FT z2 ) by A3,A4,CKB11:def 1
.= ( Im (R2,z) );
end;
hence thesis by A3,A4,RELSET_1:31;
end;
end;
