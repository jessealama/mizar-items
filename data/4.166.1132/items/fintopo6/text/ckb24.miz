constructors DOMAIN_1,NAT_1,EQREL_1,RFINSEQ,RFUNCT_3,NAT_D,FIN_TOPO,FINTOPO3,FINTOPO4,REAL_1,RELSET_1,CKB6,CKB11,CKB15;
requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve FT for non empty RelStr;
theorem
(for X9 being non empty (SubSpace of FT) holds (for P,Q being (Subset of FT) holds (for P1,Q1 being (Subset of X9) holds (((P = P1 & Q = Q1) & ( P \/ Q ) c= ( [#] X9 )) implies (P,Q are_separated  implies P1,Q1 are_separated )))))
proof
let X9 being non empty (SubSpace of FT);
let P being (Subset of FT),Q being (Subset of FT);
let P1 being (Subset of X9),Q1 being (Subset of X9);
assume that
A1: (P = P1 & Q = Q1)
and
A2: ( P \/ Q ) c= ( [#] X9 );
(P c= ( P \/ Q ) & Q c= ( P \/ Q )) by XBOOLE_1:7;
then reconsider P2 = P,Q2 = Q as (Subset of X9) by A2,XBOOLE_1:1;
assume A3: P,Q are_separated ;
then P misses ( Q ^b ) by FINTOPO4:def 1;
then A4: ( P /\ ( Q ^b ) ) = ( {} ) by XBOOLE_0:def 7;
( P2 /\ ( Q2 ^b ) ) = ( P2 /\ ( ( [#] X9 ) /\ ( Q ^b ) ) ) by CKB22:1
.= ( ( P2 /\ ( [#] X9 ) ) /\ ( Q ^b ) ) by XBOOLE_1:16
.= ( P /\ ( Q ^b ) ) by XBOOLE_1:28;
then A5: P2 misses ( Q2 ^b ) by A4,XBOOLE_0:def 7;
( P2 ^b ) = ( ( P ^b ) /\ ( [#] X9 ) ) by CKB22:1;
then A6: ( ( P2 ^b ) /\ Q2 ) = ( ( P ^b ) /\ ( Q2 /\ ( [#] X9 ) ) ) by XBOOLE_1:16
.= ( ( P ^b ) /\ Q ) by XBOOLE_1:28;
( P ^b ) misses Q by A3,FINTOPO4:def 1;
then ( ( P ^b ) /\ Q ) = ( {} ) by XBOOLE_0:def 7;
then ( P2 ^b ) misses Q2 by A6,XBOOLE_0:def 7;
hence thesis by A1,A5,FINTOPO4:def 1;
end;
