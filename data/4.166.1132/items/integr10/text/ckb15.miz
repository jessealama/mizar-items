constructors ARYTM_0,REAL_1,PARTFUN2,LIMFUNC1,LIMFUNC2,TAYLOR_1,SEQ_2,RFUNCT_3,FCONT_1,RFUNCT_2,PSCOMP_1,BINOP_2,INTEGRA5,RSSPACE,COMSEQ_3,SIN_COS,INTEGRA7,CFUNCT_1,SEQ_1,FDIFF_1,SEQM_3,RELSET_1,RCOMP_1,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13;
requirements NUMERALS,BOOLE,SUBSET,REAL,ARITHM;
begin
theorem
(for f being (PartFunc of ( REAL ),( REAL )) holds (for a,b being Real holds (((a < b & [' a,b '] c= ( dom f )) & f is_right_ext_Riemann_integrable_on a,b) implies (for r being Real holds (( r (#) f ) is_right_ext_Riemann_integrable_on a,b & ( ext_right_integral (( r (#) f ),a,b) ) = ( r * ( ext_right_integral (f,a,b) ) ))))))
proof
let f being (PartFunc of ( REAL ),( REAL ));
let a being Real,b being Real;
assume that
A1: a < b
and
A2: [' a,b '] c= ( dom f )
and
A3: f is_right_ext_Riemann_integrable_on a,b;
(for r being Real holds (( r (#) f ) is_right_ext_Riemann_integrable_on a,b & ( ext_right_integral (( r (#) f ),a,b) ) = ( r * ( ext_right_integral (f,a,b) ) )))
proof
let r being Real;
consider Intf being (PartFunc of ( REAL ),( REAL )) such that A4: ( dom Intf ) = [. a,b .[ and A5: (for x being Real holds (x in ( dom Intf ) implies ( Intf . x ) = ( integral (f,a,x) ))) and A6: Intf is_left_convergent_in b and A7: ( ext_right_integral (f,a,b) ) = ( lim_left (Intf,b) ) by A3,CKB6:def 1;
set Intfg = ( r (#) Intf );
A8: Intfg is_left_convergent_in b by A6,LIMFUNC2:43;
A9: (( dom Intfg ) = [. a,b .[ & (for x being Real holds (x in ( dom Intfg ) implies ( Intfg . x ) = ( integral (( r (#) f ),a,x) ))))
proof
A10: [' a,b '] = [. a,b .] by A1,INTEGRA5:def 3;
thus A11: ( dom Intfg ) = [. a,b .[ by A4,VALUED_1:def 5;
let x being Real;
assume A12: x in ( dom Intfg );
then A13: a <= x by A11,XXREAL_1:3;
then A14: [' a,x '] = [. a,x .] by INTEGRA5:def 3;
A15: x < b by A11,A12,XXREAL_1:3;
then A16: [. a,x .] c= [. a,b .] by XXREAL_1:34;
A17: (f is_integrable_on [' a,x '] & ( f | [' a,x '] ) is  bounded) by A3,A13,A15,CKB4:def 1;
thus ( Intfg . x ) = ( r * ( Intf . x ) ) by A12,VALUED_1:def 5
.= ( r * ( integral (f,a,x) ) ) by A4,A5,A11,A12
.= ( integral (( r (#) f ),a,x) ) by A2,A13,A14,A10,A16,A17,INTEGRA6:10,XBOOLE_1:1;
end;
(for d being Real holds ((a <= d & d < b) implies (( r (#) f ) is_integrable_on [' a,d '] & ( ( r (#) f ) | [' a,d '] ) is  bounded)))
proof
let d being Real;
assume A18: (a <= d & d < b);
then A19: ([' a,d '] = [. a,d .] & [. a,d .] c= [. a,b .]) by INTEGRA5:def 3,XXREAL_1:34;
A20: (f is_integrable_on [' a,d '] & ( f | [' a,d '] ) is  bounded) by A3,A18,CKB4:def 1;
[' a,b '] = [. a,b .] by A1,INTEGRA5:def 3;
then [' a,d '] c= ( dom f ) by A2,A19,XBOOLE_1:1;
hence thesis by A20,INTEGRA6:9,RFUNCT_1:80;
end;
hence A21: ( r (#) f ) is_right_ext_Riemann_integrable_on a,b by A9,A8,CKB4:def 1;
( lim_left (Intfg,b) ) = ( r * ( ext_right_integral (f,a,b) ) ) by A6,A7,LIMFUNC2:43;
hence thesis by A9,A8,A21,CKB6:def 1;
end;
hence thesis;
end;
