environ
vocabularies XBOOLE_0,SUBSET_1,FUNCT_2,MARGREL1,BVFUNC_1,XBOOLEAN,FUNCT_1;
notations XBOOLE_0,SUBSET_1,XBOOLEAN,MARGREL1,FUNCT_2,BVFUNC_1;
definitions XBOOLEAN;
theorems MARGREL1,BINARITH,BVFUNC_1,XBOOLEAN,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19;
registrations XBOOLE_0,XBOOLEAN,MARGREL1;
constructors BINARITH,BVFUNC_1;
requirements ARITHM;
begin
reserve Y for non empty set;
theorem
(for a,b being (Element of ( Funcs (Y,( BOOLEAN )) )) holds ((( a 'or' b ) = ( I_el Y ) & ( 'not' a ) = ( I_el Y )) implies b = ( I_el Y )))
proof
let a being (Element of ( Funcs (Y,( BOOLEAN )) )),b being (Element of ( Funcs (Y,( BOOLEAN )) ));
assume that
A1: ( a 'or' b ) = ( I_el Y )
and
A2: ( 'not' a ) = ( I_el Y );
(for x being (Element of Y) holds ( b . x ) = ( TRUE ))
proof
let x being (Element of Y);
( ( 'not' a ) . x ) = ( TRUE ) by A2,BVFUNC_1:def 11;
then A3: ( 'not' ( a . x ) ) = ( TRUE ) by MARGREL1:def 19;
( ( a 'or' b ) . x ) = ( TRUE ) by A1,BVFUNC_1:def 11;
then ( ( a . x ) 'or' ( b . x ) ) = ( TRUE ) by BVFUNC_1:def 4;
hence thesis by A3;
end;
hence thesis by BVFUNC_1:def 11;
end;
