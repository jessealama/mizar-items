environ
vocabularies XBOOLE_0,SUBSET_1,FUNCT_2,MARGREL1,BVFUNC_1,XBOOLEAN,FUNCT_1;
notations XBOOLE_0,SUBSET_1,XBOOLEAN,MARGREL1,FUNCT_2,BVFUNC_1;
definitions XBOOLEAN;
theorems MARGREL1,BINARITH,BVFUNC_1,XBOOLEAN,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15;
registrations XBOOLE_0,XBOOLEAN,MARGREL1;
constructors BINARITH,BVFUNC_1;
requirements ARITHM;
begin
reserve Y for non  empty set;
theorem
(for a,b,c being (Element of ( Funcs (Y,( BOOLEAN )) )) holds (( a 'imp' b ) = ( I_el Y ) implies ( ( a 'or' c ) 'imp' ( b 'or' c ) ) = ( I_el Y )))
proof
let a being (Element of ( Funcs (Y,( BOOLEAN )) ));
let b being (Element of ( Funcs (Y,( BOOLEAN )) ));
let c being (Element of ( Funcs (Y,( BOOLEAN )) ));
assume A1: ( a 'imp' b ) = ( I_el Y );
(for x being (Element of Y) holds ( ( ( a 'or' c ) 'imp' ( b 'or' c ) ) . x ) = ( TRUE ))
proof
let x being (Element of Y);
( ( a 'imp' b ) . x ) = ( TRUE ) by A1,BVFUNC_1:def 11;
then A2: ( ( 'not' ( a . x ) ) 'or' ( b . x ) ) = ( TRUE ) by BVFUNC_1:def 8;
( ( ( a 'or' c ) 'imp' ( b 'or' c ) ) . x ) = ( ( 'not' ( ( a 'or' c ) . x ) ) 'or' ( ( b 'or' c ) . x ) ) by BVFUNC_1:def 8
.= ( ( 'not' ( ( a . x ) 'or' ( c . x ) ) ) 'or' ( ( b 'or' c ) . x ) ) by BVFUNC_1:def 4
.= ( ( ( b . x ) 'or' ( c . x ) ) 'or' ( ( 'not' ( a . x ) ) '&' ( 'not' ( c . x ) ) ) ) by BVFUNC_1:def 4
.= ( ( ( ( c . x ) 'or' ( b . x ) ) 'or' ( 'not' ( a . x ) ) ) '&' ( ( ( b . x ) 'or' ( c . x ) ) 'or' ( 'not' ( c . x ) ) ) ) by XBOOLEAN:9
.= ( ( ( c . x ) 'or' ( ( 'not' ( a . x ) ) 'or' ( b . x ) ) ) '&' ( ( ( b . x ) 'or' ( c . x ) ) 'or' ( 'not' ( c . x ) ) ) )
.= ( ( TRUE ) '&' ( ( b . x ) 'or' ( ( c . x ) 'or' ( 'not' ( c . x ) ) ) ) ) by A2
.= ( ( TRUE ) '&' ( ( b . x ) 'or' ( TRUE ) ) ) by XBOOLEAN:102
.= ( TRUE );
hence thesis;
end;
hence thesis by BVFUNC_1:def 11;
end;
