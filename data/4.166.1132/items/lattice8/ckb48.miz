constructors ORDINAL2,FUNCT_6,RFUNCT_3,ABIAN,REALSET2,ORDERS_3,WAYBEL_1,LATTICE5,YELLOW11,RELSET_1,CKB1,CKB2,CKB16,CKB18,CKB23,CKB29,CKB30,CKB36,CKB43,CKB45,CKB46,CKB47;
requirements NUMERALS,REAL,BOOLE,SUBSET,ARITHM;
begin
theorem
Th30: (for A being non empty set holds (for L being  lower-bounded LATTICE holds (for d being (distance_function of A,L) holds (for Aq being non empty set holds (for dq being (distance_function of Aq,L) holds (Aq,dq is_extension2_of A,d implies (for x,y being (Element of A) holds (for a,b being (Element of L) holds (( d . (x,y) ) <= ( a "\/" b ) implies (ex z1,z2 being (Element of Aq) st ((( dq . (x,z1) ) = a & ( dq . (z1,z2) ) = ( ( ( d . (x,y) ) "\/" a ) "/\" b )) & ( dq . (z2,y) ) = a)))))))))))
proof
let A being non empty set;
let L being  lower-bounded LATTICE;
let d being (distance_function of A,L);
let Aq being non empty set;
let dq being (distance_function of Aq,L);
assume Aq,dq is_extension2_of A,d;
then consider q being (QuadrSeq of d) such that A1: Aq = ( NextSet2 d ) and A2: dq = ( NextDelta2 q ) by CKB47:def 1;
let x being (Element of A),y being (Element of A);
let a being (Element of L),b being (Element of L);
A3: ( rng q ) = { [ x9,y9,a9,b9 ] where x9 is (Element of A),y9 is (Element of A),a9 is (Element of L),b9 is (Element of L): ( d . (x9,y9) ) <= ( a9 "\/" b9 ) } by LATTICE5:def 13;
assume ( d . (x,y) ) <= ( a "\/" b );
then [ x,y,a,b ] in ( rng q ) by A3;
then consider o being set such that A4: o in ( dom q ) and A5: ( q . o ) = [ x,y,a,b ] by FUNCT_1:def 3;
reconsider o as Ordinal by A4;
A6: ( q . o ) = ( Quadr2 (q,o) ) by A4,CKB29:def 1;
then A7: x = ( ( Quadr2 (q,o) ) `1 ) by A5,MCART_1:74;
A8: b = ( ( Quadr2 (q,o) ) `4 ) by A5,A6,MCART_1:74;
A9: y = ( ( Quadr2 (q,o) ) `2 ) by A5,A6,MCART_1:74;
reconsider B = ( ConsecutiveSet2 (A,o) ) as non empty set;
{ B } in { { B },{ { B } } } by TARSKI:def 2;
then A10: { B } in ( B \/ { { B },{ { B } } } ) by XBOOLE_0:def 3;
o in ( DistEsti d ) by A4,LATTICE5:25;
then A11: ( succ o ) c= ( DistEsti d ) by ORDINAL1:21;
then A12: ( ConsecutiveDelta2 (q,( succ o )) ) c= ( ConsecutiveDelta2 (q,( DistEsti d )) ) by CKB38:1;
reconsider cd = ( ConsecutiveDelta2 (q,o) ) as (BiFunction of B,L);
reconsider Q = ( Quadr2 (q,o) ) as (Element of [: B,B,(the carrier of L),(the carrier of L) :]);
A13: (x in A & y in A);
A14: { { B } } in { { B },{ { B } } } by TARSKI:def 2;
then A15: { { B } } in ( new_set2 B ) by XBOOLE_0:def 3;
( ConsecutiveSet2 (A,( succ o )) ) = ( new_set2 B ) by CKB25:1;
then ( new_set2 B ) c= ( ConsecutiveSet2 (A,( DistEsti d )) ) by A11,CKB34:1;
then reconsider z1 = { B },z2 = { { B } } as (Element of Aq) by A1,A10,A15;
take z1;
take z2;
A16: cd is  zeroed by CKB39:1;
A c= B by CKB28:1;
then reconsider xo = x,yo = y as (Element of B) by A13;
A17: B c= ( new_set2 B ) by XBOOLE_1:7;
(xo in B & yo in B);
then reconsider x1 = xo,y1 = yo as (Element of ( new_set2 B )) by A17;
A18: ( ConsecutiveDelta2 (q,( succ o )) ) = ( new_bi_fun2 (( BiFun (( ConsecutiveDelta2 (q,o) ),( ConsecutiveSet2 (A,o) ),L) ),( Quadr2 (q,o) )) ) by CKB32:1
.= ( new_bi_fun2 (cd,Q) ) by LATTICE5:def 15;
( dom d ) = [: A,A :] by FUNCT_2:def 1;
then A19: [ xo,yo ] in ( dom d ) by ZFMISC_1:87;
d c= cd by CKB37:1;
then A20: ( cd . (xo,yo) ) = ( d . (x,y) ) by A19,GRFUNC_1:2;
A21: a = ( ( Quadr2 (q,o) ) `3 ) by A5,A6,MCART_1:74;
A22: ( dom ( new_bi_fun2 (cd,Q) ) ) = [: ( new_set2 B ),( new_set2 B ) :] by FUNCT_2:def 1;
then [ x1,{ B } ] in ( dom ( new_bi_fun2 (cd,Q) ) ) by A10,ZFMISC_1:87;
hence ( dq . (x,z1) ) = ( ( new_bi_fun2 (cd,Q) ) . (x1,{ B }) ) by A2,A12,A18,GRFUNC_1:2
.= ( ( cd . (xo,xo) ) "\/" a ) by A7,A21,CKB18:def 1
.= ( ( Bottom L ) "\/" a ) by A16,LATTICE5:def 6
.= a by WAYBEL_1:3;
[ { B },{ { B } } ] in ( dom ( new_bi_fun2 (cd,Q) ) ) by A10,A15,A22,ZFMISC_1:87;
hence ( dq . (z1,z2) ) = ( ( new_bi_fun2 (cd,Q) ) . ({ B },{ { B } }) ) by A2,A12,A18,GRFUNC_1:2
.= ( ( ( d . (x,y) ) "\/" a ) "/\" b ) by A7,A9,A21,A8,A20,CKB18:def 1;
{ { B } } in ( B \/ { { B },{ { B } } } ) by A14,XBOOLE_0:def 3;
then [ { { B } },y1 ] in ( dom ( new_bi_fun2 (cd,Q) ) ) by A22,ZFMISC_1:87;
hence ( dq . (z2,y) ) = ( ( new_bi_fun2 (cd,Q) ) . ({ { B } },y1) ) by A2,A12,A18,GRFUNC_1:2
.= ( ( cd . (yo,yo) ) "\/" a ) by A9,A21,CKB18:def 1
.= ( ( Bottom L ) "\/" a ) by A16,LATTICE5:def 6
.= a by WAYBEL_1:3;
end;
