begin
reserve O1 for Ordinal;
reserve O2 for Ordinal;
theorem
Th18: (for A being non empty set holds (for O being Ordinal holds A c= ( ConsecutiveSet2 (A,O) )))
proof
let A being non empty set;
let O being Ordinal;
defpred X[ Ordinal ]
 means
A c= ( ConsecutiveSet2 (A,$1) );
A1: (for O1 being Ordinal holds (X[ O1 ] implies X[ ( succ O1 ) ]))
proof
let O1 being Ordinal;
( ConsecutiveSet2 (A,( succ O1 )) ) = ( new_set2 ( ConsecutiveSet2 (A,O1) ) ) by CKB25:1;
then A2: ( ConsecutiveSet2 (A,O1) ) c= ( ConsecutiveSet2 (A,( succ O1 )) ) by XBOOLE_1:7;
assume A c= ( ConsecutiveSet2 (A,O1) );
hence thesis by A2,XBOOLE_1:1;
end;
A3: (for O1 holds (((O1 <> ( {} ) & O1 is  limit_ordinal) & (for O2 holds (O2 in O1 implies X[ O2 ]))) implies X[ O1 ]))
proof
deffunc U(Ordinal) = ( ConsecutiveSet2 (A,$1) );
let O2 being Ordinal;
assume that
A4: O2 <> ( {} )
and
A5: O2 is  limit_ordinal
and
(for O1 being Ordinal holds (O1 in O2 implies A c= ( ConsecutiveSet2 (A,O1) )));
A6: ( {} ) in O2 by A4,ORDINAL3:8;
consider Ls being T-Sequence such that A7: (( dom Ls ) = O2 & (for O1 being Ordinal holds (O1 in O2 implies ( Ls . O1 ) = U(O1)))) from ORDINAL2:sch 2;
( Ls . ( {} ) ) = ( ConsecutiveSet2 (A,( {} )) ) by A4,A7,ORDINAL3:8
.= A by CKB24:1;
then A8: A in ( rng Ls ) by A7,A6,FUNCT_1:def 3;
( ConsecutiveSet2 (A,O2) ) = ( union ( rng Ls ) ) by A4,A5,A7,CKB26:1;
hence thesis by A8,ZFMISC_1:74;
end;
A9: X[ ( {} ) ] by CKB24:1;
(for O being Ordinal holds X[ O ]) from ORDINAL2:sch 1(A9,A1,A3);
hence thesis;
end;
