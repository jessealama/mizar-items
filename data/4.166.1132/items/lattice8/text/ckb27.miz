begin
reserve O1 for Ordinal;
reserve O2 for Ordinal;
registration
let A being non empty set;
let O being Ordinal;
cluster ( ConsecutiveSet2 (A,O) ) -> non empty;
coherence
proof
defpred X[ Ordinal ]
 means
( ConsecutiveSet2 (A,$1) ) is non empty;
A1: (for O1 being Ordinal holds (X[ O1 ] implies X[ ( succ O1 ) ]))
proof
let O1 being Ordinal;
assume ( ConsecutiveSet2 (A,O1) ) is non empty;
( ConsecutiveSet2 (A,( succ O1 )) ) = ( new_set2 ( ConsecutiveSet2 (A,O1) ) ) by CKB25:1;
hence thesis;
end;
A2: (for O1 holds (((O1 <> ( {} ) & O1 is  limit_ordinal) & (for O2 holds (O2 in O1 implies X[ O2 ]))) implies X[ O1 ]))
proof
deffunc U(Ordinal) = ( ConsecutiveSet2 (A,$1) );
let O1 being Ordinal;
assume that
A3: O1 <> ( {} )
and
A4: O1 is  limit_ordinal
and
(for O2 being Ordinal holds (O2 in O1 implies ( ConsecutiveSet2 (A,O2) ) is non empty));
A5: ( {} ) in O1 by A3,ORDINAL3:8;
consider Ls being T-Sequence such that A6: (( dom Ls ) = O1 & (for O2 being Ordinal holds (O2 in O1 implies ( Ls . O2 ) = U(O2)))) from ORDINAL2:sch 2;
( Ls . ( {} ) ) = ( ConsecutiveSet2 (A,( {} )) ) by A3,A6,ORDINAL3:8
.= A by CKB24:1;
then A7: A in ( rng Ls ) by A6,A5,FUNCT_1:def 3;
( ConsecutiveSet2 (A,O1) ) = ( union ( rng Ls ) ) by A3,A4,A6,CKB26:1;
then A c= ( ConsecutiveSet2 (A,O1) ) by A7,ZFMISC_1:74;
hence thesis;
end;
A8: X[ ( {} ) ] by CKB24:1;
(for O being Ordinal holds X[ O ]) from ORDINAL2:sch 1(A8,A1,A2);
hence thesis;
end;
end;
