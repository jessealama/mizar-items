constructors ORDINAL2,FUNCT_6,RFUNCT_3,ABIAN,REALSET2,ORDERS_3,WAYBEL_1,LATTICE5,YELLOW11,RELSET_1,CKB1,CKB2,CKB16,CKB18,CKB23,CKB29,CKB30,CKB36;
requirements NUMERALS,REAL,BOOLE,SUBSET,ARITHM;
begin
reserve O1 for Ordinal;
reserve O2 for Ordinal;
theorem
Th25: (for A being non empty set holds (for L being  lower-bounded LATTICE holds (for d being (BiFunction of A,L) holds (for O1,O2 being Ordinal holds (for q being (QuadrSeq of d) holds (O1 c= O2 implies ( ConsecutiveDelta2 (q,O1) ) c= ( ConsecutiveDelta2 (q,O2) )))))))
proof
let A being non empty set;
let L being  lower-bounded LATTICE;
let d being (BiFunction of A,L);
let O1 being Ordinal,O2 being Ordinal;
let q being (QuadrSeq of d);
defpred X[ Ordinal ]
 means
(O1 c= $1 implies ( ConsecutiveDelta2 (q,O1) ) c= ( ConsecutiveDelta2 (q,$1) ));
A1: (for O1 holds (((O1 <> ( {} ) & O1 is  limit_ordinal) & (for O2 holds (O2 in O1 implies X[ O2 ]))) implies X[ O1 ]))
proof
deffunc U(Ordinal) = ( ConsecutiveDelta2 (q,$1) );
let O2 being Ordinal;
assume that
A2: (O2 <> ( {} ) & O2 is  limit_ordinal)
and
(for O3 being Ordinal holds (O3 in O2 implies (O1 c= O3 implies ( ConsecutiveDelta2 (q,O1) ) c= ( ConsecutiveDelta2 (q,O3) ))));
consider L being T-Sequence such that A3: (( dom L ) = O2 & (for O3 being Ordinal holds (O3 in O2 implies ( L . O3 ) = U(O3)))) from ORDINAL2:sch 2;
A4: ( ConsecutiveDelta2 (q,O2) ) = ( union ( rng L ) ) by A2,A3,CKB33:1;
assume A5: O1 c= O2;
per cases ;
suppose O1 = O2;

hence thesis;
end;
suppose O1 <> O2;

then A6: O1 c< O2 by A5,XBOOLE_0:def 8;
then O1 in O2 by ORDINAL1:11;
then A7: ( L . O1 ) in ( rng L ) by A3,FUNCT_1:def 3;
( L . O1 ) = ( ConsecutiveDelta2 (q,O1) ) by A3,A6,ORDINAL1:11;
hence thesis by A4,A7,ZFMISC_1:74;
end;
end;
A8: (for O1 being Ordinal holds (X[ O1 ] implies X[ ( succ O1 ) ]))
proof
let O2 being Ordinal;
assume A9: (O1 c= O2 implies ( ConsecutiveDelta2 (q,O1) ) c= ( ConsecutiveDelta2 (q,O2) ));
assume A10: O1 c= ( succ O2 );
per cases ;
suppose O1 = ( succ O2 );

hence thesis;
end;
suppose A11: O1 <> ( succ O2 );

( ConsecutiveDelta2 (q,( succ O2 )) ) = ( new_bi_fun2 (( BiFun (( ConsecutiveDelta2 (q,O2) ),( ConsecutiveSet2 (A,O2) ),L) ),( Quadr2 (q,O2) )) ) by CKB32:1
.= ( new_bi_fun2 (( ConsecutiveDelta2 (q,O2) ),( Quadr2 (q,O2) )) ) by LATTICE5:def 15;
then A12: ( ConsecutiveDelta2 (q,O2) ) c= ( ConsecutiveDelta2 (q,( succ O2 )) ) by CKB22:1;
O1 c< ( succ O2 ) by A10,A11,XBOOLE_0:def 8;
then O1 in ( succ O2 ) by ORDINAL1:11;
hence thesis by A9,A12,ORDINAL1:22,XBOOLE_1:1;
end;
end;
A13: X[ ( {} ) ];
(for O being Ordinal holds X[ O ]) from ORDINAL2:sch 1(A13,A8,A1);
hence thesis;
end;
