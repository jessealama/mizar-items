definition
let I;
let X;
let Y;
func X \ Y -> (ManySortedSet of I)
means
:Def9: (for i holds (i in I implies ( it . i ) = ( ( X . i ) \ ( Y . i ) )));
existence
proof
deffunc F(set) = ( ( X . $1 ) \ ( Y . $1 ) );
consider f being Function such that A11: ( dom f ) = I and A12: (for i holds (i in I implies ( f . i ) = F(i))) from FUNCT_1:sch 3;
f is (ManySortedSet of I) by A11,PARTFUN1:def 2,RELAT_1:def 18;
hence thesis by A12;
end;
uniqueness
proof
let A1 being (ManySortedSet of I),A2 being (ManySortedSet of I);
assume that
A13: (for i holds (i in I implies ( A1 . i ) = ( ( X . i ) \ ( Y . i ) )))
and
A14: (for i holds (i in I implies ( A2 . i ) = ( ( X . i ) \ ( Y . i ) )));
now
let i;
assume A15: i in I;
hence ( A1 . i ) = ( ( X . i ) \ ( Y . i ) ) by A13
.= ( A2 . i ) by A14,A15;
end;
hence thesis by CKB10:1;
end;
