begin
reserve i for set;
definition
let I being non empty set;
let X being (ManySortedSet of I);
let Y being (ManySortedSet of I);
redefine pred X in Y
;
asymmetry
proof
let X being (ManySortedSet of I),Y being (ManySortedSet of I);
assume A1: (for i holds (i in I implies ( X . i ) in ( Y . i )));
(not (for i holds (i in I implies ( Y . i ) in ( X . i ))))
proof
assume A2: (for i holds (i in I implies ( Y . i ) in ( X . i )));
consider i such that A3: i in I by XBOOLE_0:def 1;
( X . i ) in ( Y . i ) by A1,A3;
hence thesis by A2,A3;
end;
hence thesis by CKB6:def 1;
end;
end;
