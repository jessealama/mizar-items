environ
vocabularies FUNCT_1,RELAT_1,SETFAM_1,XBOOLE_0,SUBSET_1,PARTFUN1,FUNCOP_1,TARSKI,CARD_3,NUMBERS,FINSEQ_2,FINSEQ_1,FUNCT_2,ZFMISC_1,MEMBER_1,FUNCT_4,PBOOLE,VALUED_1,NAT_1,ARYTM_3,ORDINAL1;
notations TARSKI,ZFMISC_1,XBOOLE_0,SUBSET_1,SETFAM_1,RELAT_1,FUNCT_1,ORDINAL1,PARTFUN1,FUNCT_2,FUNCT_4,FUNCOP_1,CKB4,CKB6,CKB7,CKB8,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18;
definitions TARSKI,XBOOLE_0,RELAT_1,FUNCT_1,SETFAM_1,FUNCOP_1,CKB6,CKB7,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB18;
theorems TARSKI,FUNCOP_1,FUNCT_1,RELAT_1,XBOOLE_0,XBOOLE_1,FUNCT_2,PARTFUN1,FUNCT_4,ZFMISC_1,GRFUNC_1,CKB1,CKB2,CKB6,CKB7,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25;
schemes FUNCT_1,CLASSES1,XBOOLE_0,FUNCT_2,CKB5,CKB9;
registrations XBOOLE_0,FUNCT_1,ORDINAL1,RELSET_1,FUNCOP_1,FUNCT_4,CKB3;
constructors SETFAM_1,FUNCOP_1,ORDINAL1,RELSET_1,PARTFUN1,FUNCT_4,CKB6,CKB7,CKB8,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB18;
requirements BOOLE,SUBSET;
begin
reserve i for set;
reserve I for set;
reserve x for (ManySortedSet of I);
reserve X for (ManySortedSet of I);
reserve Y for (ManySortedSet of I);
theorem
Th11: (X overlap Y implies (ex x st (x in X & x in Y)))
proof
deffunc F(set) = ( ( X . $1 ) /\ ( Y . $1 ) );
assume A1: X overlap Y;
A2:now
let i;
assume i in I;
then ( X . i ) meets ( Y . i ) by A1,CKB15:def 1;
hence F(i) <> ( {} ) by XBOOLE_0:def 7;
end;
consider x being (ManySortedSet of I) such that A3: (for i holds (i in I implies ( x . i ) in F(i))) from CKB5:sch 1(A2);
take x;
now
let i;
assume i in I;
then ( x . i ) in ( ( X . i ) /\ ( Y . i ) ) by A3;
hence ( x . i ) in ( X . i ) by XBOOLE_0:def 4;
end;
hence x in X by CKB6:def 1;
now
let i;
assume i in I;
then ( x . i ) in ( ( X . i ) /\ ( Y . i ) ) by A3;
hence ( x . i ) in ( Y . i ) by XBOOLE_0:def 4;
end;
hence thesis by CKB6:def 1;
end;
