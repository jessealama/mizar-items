let F being  Function-yielding Function;
let G being  Function-yielding Function;
func G ** F -> Function
means
:Def24: (( dom it ) = ( ( dom F ) /\ ( dom G ) ) & (for i being set holds (i in ( dom it ) implies ( it . i ) = ( ( G . i ) * ( F . i ) ))));
existence
proof
defpred P[ set,set ]
 means
$2 = ( ( G . $1 ) * ( F . $1 ) );
A1: (for i being set holds (i in ( ( dom F ) /\ ( dom G ) ) implies (ex u being set st P[ i,u ])));
(ex H being Function st (( dom H ) = ( ( dom F ) /\ ( dom G ) ) & (for i being set holds (i in ( ( dom F ) /\ ( dom G ) ) implies P[ i,( H . i ) ])))) from CLASSES1:sch 1(A1);
hence thesis;
end;
uniqueness
proof
let H1 being Function,H2 being Function;
assume that
A3: ( dom H1 ) = ( ( dom F ) /\ ( dom G ) )
and
A4: (for i being set holds (i in ( dom H1 ) implies ( H1 . i ) = ( ( G . i ) * ( F . i ) )))
and
A5: ( dom H2 ) = ( ( dom F ) /\ ( dom G ) )
and
A6: (for i being set holds (i in ( dom H2 ) implies ( H2 . i ) = ( ( G . i ) * ( F . i ) )));
now
let i being set;
reconsider f = ( F . i ) as Function;
reconsider g = ( G . i ) as Function;
assume A7: i in ( ( dom F ) /\ ( dom G ) );
then ( H1 . i ) = ( g * f ) by A3,A4;
hence ( H1 . i ) = ( H2 . i ) by A5,A6,A7;
end;
hence thesis by A3,A5,FUNCT_1:2;
end;
