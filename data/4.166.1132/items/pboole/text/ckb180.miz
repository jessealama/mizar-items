begin
reserve i for set;
reserve I for set;
definition
let I being set;
let A being (ManySortedSet of I);
let F being (ManySortedFunction of I);
func F .:.: A -> (ManySortedSet of I) means 
(for i being set holds (i in I implies ( it . i ) = ( ( F . i ) .: ( A . i ) )));
existence
proof
defpred P[ set,set ]
 means
$2 = ( ( F . $1 ) .: ( A . $1 ) );
A1: (for i being set holds (i in I implies (ex u being set st P[ i,u ])));
consider g being Function such that A2: (( dom g ) = I & (for i being set holds (i in I implies P[ i,( g . i ) ]))) from CLASSES1:sch 1(A1);
reconsider g as (ManySortedSet of I) by A2,PARTFUN1:def 2,RELAT_1:def 18;
take g;
thus thesis by A2;
end;
uniqueness
proof
let B being (ManySortedSet of I);
let B1 being (ManySortedSet of I);
assume that
A3: (for i being set holds (i in I implies ( B . i ) = ( ( F . i ) .: ( A . i ) )))
and
A4: (for i being set holds (i in I implies ( B1 . i ) = ( ( F . i ) .: ( A . i ) )));
now
let i being set;
reconsider f = ( F . i ) as Function;
assume A5: i in I;
then ( B . i ) = ( f .: ( A . i ) ) by A3;
hence ( B . i ) = ( B1 . i ) by A4,A5;
end;
hence thesis by CKB10:1;
end;
end;
