environ
vocabularies NUMBERS,RLVECT_1,REAL_1,CARD_1,ARYTM_3,RELAT_1,ARYTM_1,SUPINF_2,STRUCT_0,ZFMISC_1,XBOOLE_0,SUBSET_1,ANALOAF;
notations TARSKI,XBOOLE_0,ZFMISC_1,DOMAIN_1,XXREAL_0,REAL_1,RELSET_1,NUMBERS,STRUCT_0,RLVECT_1,CKB1;
definitions STRUCT_0,RLVECT_1,CKB1;
theorems RLVECT_1,ZFMISC_1,RELAT_1,FUNCSDOM,RLSUB_2,XCMPLX_0,XCMPLX_1,XREAL_1,STRUCT_0,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18;
schemes RELSET_1;
registrations SUBSET_1,RELSET_1,XXREAL_0,MEMBERED,STRUCT_0,XBOOLE_0,ZFMISC_1;
constructors XXREAL_0,REAL_1,MEMBERED,DOMAIN_1,RLVECT_1,CKB1;
requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve V for RealLinearSpace;
reserve p for (VECTOR of V);
reserve u for (VECTOR of V);
reserve v for (VECTOR of V);
reserve w for (VECTOR of V);
reserve y for (VECTOR of V);
reserve a for Real;
reserve b for Real;
theorem
Th27: ((p <> v & v,p // p,w) implies (ex y st (u,p // p,y & u,v // w,y)))
proof
assume A1: (p <> v & v,p // p,w);
A2:now
assume p <> w;
then consider a,b such that A3: ( a * ( p - v ) ) = ( b * ( w - p ) ) and A4: ( 0 ) < a and A5: ( 0 ) < b by A1,CKB8:1;
set y = ( ( ( ( b " ) * a ) * ( p - u ) ) + p );
A6: ( y - p ) = ( ( ( b " ) * a ) * ( p - u ) ) by RLSUB_2:61
.= ( ( b " ) * ( a * ( p - u ) ) ) by RLVECT_1:def 7;
A7: ( y - w ) = ( ( y - p ) + ( p - w ) ) by CKB2:1
.= ( ( y - p ) - ( w - p ) ) by RLVECT_1:33;
( v - u ) = ( ( p - u ) + ( v - p ) ) by CKB2:1
.= ( ( p - u ) - ( p - v ) ) by RLVECT_1:33;
then ( a * ( v - u ) ) = ( ( a * ( p - u ) ) - ( a * ( p - v ) ) ) by RLVECT_1:34
.= ( ( b * ( y - p ) ) - ( b * ( w - p ) ) ) by A3,A5,A6,CKB7:1
.= ( b * ( y - w ) ) by A7,RLVECT_1:34;
then A8: u,v // w,y by A4,A5,CKB1:def 1;
( 0 ) < ( b " ) by A5,XREAL_1:122;
then A9: ( 0 ) < ( ( b " ) * a ) by A4,XREAL_1:129;
( 1 * ( y - p ) ) = ( y - p ) by RLVECT_1:def 8
.= ( ( ( b " ) * a ) * ( p - u ) ) by RLSUB_2:61;
then u,p // p,y by A9,CKB1:def 1;
hence thesis by A8;
end;
now
assume A10: p = w;
take y = p;
thus (u,p // p,y & u,v // w,y) by A10,CKB1:def 1;
end;
hence thesis by A2;
end;
