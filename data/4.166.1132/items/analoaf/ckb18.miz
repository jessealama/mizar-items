environ
vocabularies NUMBERS,RLVECT_1,REAL_1,CARD_1,ARYTM_3,RELAT_1,ARYTM_1,SUPINF_2,STRUCT_0,ZFMISC_1,XBOOLE_0,SUBSET_1,ANALOAF;
notations TARSKI,XBOOLE_0,ZFMISC_1,DOMAIN_1,XXREAL_0,REAL_1,RELSET_1,NUMBERS,STRUCT_0,RLVECT_1,CKB1;
definitions STRUCT_0,RLVECT_1,CKB1;
theorems RLVECT_1,ZFMISC_1,RELAT_1,FUNCSDOM,RLSUB_2,XCMPLX_0,XCMPLX_1,XREAL_1,STRUCT_0,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17;
schemes RELSET_1;
registrations SUBSET_1,RELSET_1,XXREAL_0,MEMBERED,STRUCT_0,XBOOLE_0,ZFMISC_1;
constructors XXREAL_0,REAL_1,MEMBERED,DOMAIN_1,RLVECT_1,CKB1;
requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve V for RealLinearSpace;
reserve p for (VECTOR of V);
reserve q for (VECTOR of V);
reserve u for (VECTOR of V);
reserve v for (VECTOR of V);
reserve w for (VECTOR of V);
reserve y for (VECTOR of V);
theorem
Th26: ((ex p st (ex q st p <> q)) implies (for u holds (for v holds (for w holds (ex y st ((u,v // w,y & u,w // v,y) & v <> y))))))
proof
given p,q such that
A1: p <> q;

let u;
let v;
let w;
A2:now
assume A3: u <> w;
take y = ( ( v + w ) - u );
A4:now
assume v = y;
then v = ( v + ( w - u ) ) by RLVECT_1:def 3;
then ( w - u ) = ( 0. V ) by RLVECT_1:9;
hence contradiction by A3,RLVECT_1:21;
end;
(u,v // w,y & u,w // v,y) by CKB17:1;
hence thesis by A4;
end;
now
assume A5: u = w;
A6:now
assume u = v;
then A7: (u,v // w,p & u,v // w,q) by CKB1:def 1;
A8: (v <> p or v <> q) by A1;
(u,w // v,p & u,w // v,q) by A5,CKB1:def 1;
hence thesis by A8,A7;
end;
(u,v // w,u & u,w // v,u) by A5,CKB1:def 1;
hence thesis by A6;
end;
hence thesis by A2;
end;
