environ
vocabularies FUNCT_1,XBOOLE_0,ZFMISC_1,SUBSET_1,TARSKI,RELAT_1,PARTFUN1,BINOP_1;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,RELAT_1,FUNCT_1,RELSET_1,PARTFUN1,FUNCT_2,CKB1,CKB2;
definitions FUNCT_1,CKB1;
theorems RELAT_1,FUNCT_1,FUNCT_2,ZFMISC_1,SUBSET_1,CKB1,CKB3;
schemes FUNCT_2,PARTFUN1;
registrations XBOOLE_0,SUBSET_1,FUNCT_1,RELSET_1;
constructors FUNCT_2,RELSET_1,CKB1,CKB2;
requirements SUBSET,BOOLE;
begin
reserve X for set;
reserve Y for set;
reserve Z for set;
theorem
(for f1,f2 being (Function of [: X,Y :],Z) holds ((for a being (Element of X) holds (for b being (Element of Y) holds ( f1 . (a,b) ) = ( f2 . (a,b) ))) implies f1 = f2))
proof
let f1 being (Function of [: X,Y :],Z),f2 being (Function of [: X,Y :],Z);
assume (for a being (Element of X) holds (for b being (Element of Y) holds ( f1 . (a,b) ) = ( f2 . (a,b) )));
then (for x,y being set holds ((x in X & y in Y) implies ( f1 . (x,y) ) = ( f2 . (x,y) )));
hence thesis by CKB3:1;
end;
