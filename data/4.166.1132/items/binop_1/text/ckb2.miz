environ
vocabularies FUNCT_1,XBOOLE_0,ZFMISC_1,SUBSET_1,TARSKI,RELAT_1,PARTFUN1,BINOP_1;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,RELAT_1,FUNCT_1,RELSET_1,PARTFUN1,FUNCT_2,CKB1;
definitions FUNCT_1,CKB1;
theorems RELAT_1,FUNCT_1,FUNCT_2,ZFMISC_1,SUBSET_1,CKB1;
schemes FUNCT_2,PARTFUN1;
registrations XBOOLE_0,SUBSET_1,FUNCT_1,RELSET_1;
constructors FUNCT_2,RELSET_1,CKB1;
requirements SUBSET,BOOLE;
begin
definition
let A being non empty set;
let B being non empty set;
let C being set;
let f being (Function of [: A,B :],C);
let a being (Element of A);
let b being (Element of B);
redefine func f . (a,b) -> (Element of C);

coherence
proof
reconsider ab = [ a,b ] as (Element of [: A,B :]) by ZFMISC_1:def 2;
( f . ab ) is (Element of C);
hence thesis;
end;
end;
