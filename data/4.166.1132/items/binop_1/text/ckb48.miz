requirements SUBSET,BOOLE;
begin
reserve X for set;
reserve Y for set;
reserve Z for set;
reserve x for set;
reserve y for set;
reserve z for set;
theorem
(for f1,f2 being (PartFunc of [: X,Y :],Z) holds ((( dom f1 ) = ( dom f2 ) & (for x holds (for y holds ([ x,y ] in ( dom f1 ) implies ( f1 . (x,y) ) = ( f2 . (x,y) ))))) implies f1 = f2))
proof
let f1 being (PartFunc of [: X,Y :],Z);
let f2 being (PartFunc of [: X,Y :],Z);
assume that
A1: ( dom f1 ) = ( dom f2 )
and
A2: (for x holds (for y holds ([ x,y ] in ( dom f1 ) implies ( f1 . (x,y) ) = ( f2 . (x,y) ))));
(z in ( dom f1 ) implies ( f1 . z ) = ( f2 . z ))
proof
assume A3: z in ( dom f1 );
then consider x,y such that A4: z = [ x,y ] by RELAT_1:def 1;
( f1 . (x,y) ) = ( f2 . (x,y) ) by A2,A3,A4;
hence thesis by A4;
end;
hence thesis by A1,FUNCT_1:2;
end;
