environ
vocabularies FUNCT_1,XBOOLE_0,ZFMISC_1,SUBSET_1,TARSKI,RELAT_1,PARTFUN1,BINOP_1;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,RELAT_1,FUNCT_1,RELSET_1,PARTFUN1,FUNCT_2,CKB1,CKB2,CKB5,CKB6;
definitions FUNCT_1,CKB1;
theorems RELAT_1,FUNCT_1,FUNCT_2,ZFMISC_1,SUBSET_1,CKB1,CKB3,CKB4;
schemes FUNCT_2,PARTFUN1,CKB7,CKB8,CKB9;
registrations XBOOLE_0,SUBSET_1,FUNCT_1,RELSET_1;
constructors FUNCT_2,RELSET_1,CKB1,CKB2;
requirements SUBSET,BOOLE;
begin
reserve X for set;
reserve Y for set;
reserve Z for set;
scheme Lambda2D { X,Y,Z() -> non  empty set,F((Element of X()),(Element of Y())) -> (Element of Z()) } : (ex f being (Function of [: X(),Y() :],Z()) st (for x being (Element of X()) holds (for y being (Element of Y()) holds ( f . (x,y) ) = F(x,y))))
proof
defpred P[ (Element of X()),(Element of Y()),set ]
 means
$3 = F($1,$2);
A1: (for x being (Element of X()) holds (for y being (Element of Y()) holds (ex z being (Element of Z()) st P[ x,y,z ])));
thus (ex f being (Function of [: X(),Y() :],Z()) st (for x being (Element of X()) holds (for y being (Element of Y()) holds P[ x,y,( f . (x,y) ) ]))) from CKB9:sch 1(A1);
end;
