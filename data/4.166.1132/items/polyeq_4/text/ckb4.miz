environ
vocabularies NUMBERS,REAL_1,SUBSET_1,ARYTM_3,CARD_1,XXREAL_0,FUNCT_3,ARYTM_1,SQUARE_1,RELAT_1,NEWTON,POWER,POLYEQ_1,XCMPLX_0,ORDINAL1,XREAL_0,ABIAN;
notations SUBSET_1,NUMBERS,REAL_1,SQUARE_1,POWER,POLYEQ_1,QUIN_1,POLYEQ_2,NEWTON,ORDINAL1,XCMPLX_0,XXREAL_0,XREAL_0,ABIAN;
definitions SQUARE_1;
theorems SQUARE_1,PREPOWER,QUIN_1,POLYEQ_1,POWER,NEWTON,POLYEQ_2,NAT_1,XCMPLX_1,XCMPLX_0,XREAL_1,XXREAL_0,ABIAN,CKB1,CKB2,CKB3;
registrations XCMPLX_0,XREAL_0,REAL_1,SQUARE_1,NAT_1,QUIN_1,NEWTON,ORDINAL1,POWER,ABIAN,INT_1;
constructors REAL_1,SQUARE_1,NAT_1,QUIN_1,NEWTON,PREPOWER,POWER,POLYEQ_1,POLYEQ_2,ABIAN;
requirements SUBSET,NUMERALS,REAL,ARITHM,BOOLE;
begin
reserve x for Real;
reserve a for Real;
reserve n for (Element of ( NAT ));
theorem
Th4: ((((a > ( 0 ) & n is  even) & n >= 1) & ( x |^ n ) = a) implies (x = ( n -root a ) or x = ( - ( n -root a ) )))
proof
assume that
A1: a > ( 0 )
and
A2: n is  even
and
A3: n >= 1;
assume A4: ( x |^ n ) = a;
then A5: x <> ( 0 ) by A1,A3,NEWTON:11;
now
per cases  by A5,XXREAL_0:1;
case x > ( 0 );
hence thesis by A4,A3,POWER:4;
end;
case x < ( 0 );
then A6: ( - x ) > ( 0 ) by XREAL_1:58;
( n -root a ) = ( n -root ( ( - x ) |^ n ) ) by A2,A4,POWER:1;
then ( ( - 1 ) * ( n -root a ) ) = ( ( - 1 ) * ( - x ) ) by A3,A6,POWER:4;
hence thesis;
end;
end;
hence thesis;
end;
