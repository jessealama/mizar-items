A1: ((( ( x |^ n ) + ( y |^ n ) ) = p & ( ( x |^ n ) * ( y |^ n ) ) = q) & ( ( p ^2 ) - ( 4 * q ) ) >= ( 0 ))
and
A2: n is  odd;
((( x |^ n ) = ( ( p + ( sqrt ( ( p ^2 ) - ( 4 * q ) ) ) ) / 2 ) & ( y |^ n ) = ( ( p - ( sqrt ( ( p ^2 ) - ( 4 * q ) ) ) ) / 2 )) or (( x |^ n ) = ( ( p - ( sqrt ( ( p ^2 ) - ( 4 * q ) ) ) ) / 2 ) & ( y |^ n ) = ( ( p + ( sqrt ( ( p ^2 ) - ( 4 * q ) ) ) ) / 2 ))) by A1,CKB17:1;
