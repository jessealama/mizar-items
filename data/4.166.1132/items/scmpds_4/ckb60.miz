proof
set k2 = n;
set ii = ( (a,k1) <>0_goto k2 );
set J = ( Load ii );
now
let n;
let i;
assume that
A1: n in ( dom J )
and
A2: i = ( J . n );
( dom J ) = { ( 0 ) } by FUNCOP_1:13;
then n = ( 0 ) by A1,TARSKI:def 1;
then A3: ii = i by A2,FUNCOP_1:72;
hence (( InsCode i ) <> 1 & ( InsCode i ) <> 3) by SCMPDS_2:16;
A4: (( n + k2 ) >= ( 0 ) & ( InsCode i ) <> 6) by A3,SCMPDS_2:16;
(( InsCode i ) <> ( 0 ) & ( InsCode i ) <> 5) by A3,SCMPDS_2:16;
hence i valid_at n by A3,A4,CKB35:def 1;
end;
hence thesis by CKB36:def 1;
end;
