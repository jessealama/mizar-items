requirements NUMERALS,REAL,BOOLE,SUBSET,ARITHM;
begin
theorem
(for n being Ordinal holds (for T being  connected (TermOrder of n) holds (for b1,b2 being (bag of n) holds (( min (b1,b2,T) ) = b1 iff ( max (b1,b2,T) ) = b2))))
proof
let n being Ordinal;
let T being  connected (TermOrder of n);
let b1 being (bag of n),b2 being (bag of n);
A1:now
assume A2: ( max (b1,b2,T) ) = b2;
now
per cases  by A2,CKB16:def 1;
case (not b2 <= b1,T);
then b1 <= b2,T by CKB21:1;
hence ( min (b1,b2,T) ) = b1 by CKB15:def 1;
end;
case b1 = b2;
hence ( min (b1,b2,T) ) = b1 by CKB27:1;
end;
end;
hence ( min (b1,b2,T) ) = b1;
end;
now
assume A3: ( min (b1,b2,T) ) = b1;
now
per cases  by A3,CKB15:def 1;
case b1 <= b2,T;
then ( max (b2,b1,T) ) = b2 by CKB16:def 1;
hence ( max (b1,b2,T) ) = b2 by CKB32:1;
end;
case b1 = b2;
hence ( max (b1,b2,T) ) = b2 by CKB28:1;
end;
end;
hence ( max (b1,b2,T) ) = b2;
end;
hence thesis by A1;
end;
