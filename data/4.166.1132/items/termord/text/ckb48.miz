begin
theorem
(for n being Ordinal holds (for T being  connected (TermOrder of n) holds (for L being non  trivial ZeroStr holds (for p being (Polynomial of n,L) holds ( Support ( HM (p,T) ) ) c= ( Support p )))))
proof
let n being Ordinal;
let O being  connected (TermOrder of n);
let L being non  trivial ZeroStr;
let p being (Polynomial of n,L);
now
let u being set;
assume A1: u in ( Support ( HM (p,O) ) );
now
per cases  by A1,CKB47:1;
case u in ( {} );
hence u in ( Support p );
end;
case u in { ( HT (p,O) ) };
then A2: u = ( HT (p,O) ) by TARSKI:def 1;
now
per cases ;
case ( HC (p,O) ) = ( 0. L );
then ( ( HM (p,O) ) . ( HT (p,O) ) ) = ( 0. L ) by CKB38:1;
hence contradiction by A1,A2,POLYNOM1:def 3;
end;
case ( HC (p,O) ) <> ( 0. L );
hence u in ( Support p ) by A2,POLYNOM1:def 3;
end;
end;
hence u in ( Support p );
end;
end;
hence u in ( Support p );
end;
hence thesis by TARSKI:def 3;
end;
