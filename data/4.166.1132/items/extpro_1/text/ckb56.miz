begin
reserve N for  with_non-empty_elements non empty set;
reserve n for (Element of ( NAT ));
theorem
(for S being  halting  IC-Ins-separated non empty AMI-Struct over N holds (for P being (Instruction-Sequence of S) holds (for s being (State of S) holds (s = ( Following (P,s) ) implies (for n holds ( Comput (P,s,n) ) = s)))))
proof
let S being  halting  IC-Ins-separated non empty AMI-Struct over N;
let P being (Instruction-Sequence of S);
let s being (State of S);
defpred X[ (Element of ( NAT )) ]
 means
( Comput (P,s,$1) ) = s;
assume s = ( Following (P,s) );
then A1: (for n holds (X[ n ] implies X[ ( n + 1 ) ])) by CKB21:1;
A2: X[ ( 0 ) ] by CKB20:1;
thus (for n holds X[ n ]) from NAT_1:sch 1(A2,A1);
end;
