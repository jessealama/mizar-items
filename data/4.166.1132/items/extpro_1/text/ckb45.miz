begin
reserve N for  with_non-empty_elements non empty set;
theorem
Th18: (for S being  halting  IC-Ins-separated non empty AMI-Struct over N holds (for p being ( NAT ) -defined (the Instructions of S) -valued Function holds (for s being (State of S) holds (for k being Nat holds (p halts_on s implies (( Result (p,s) ) = ( Comput (p,s,k) ) iff p halts_at ( IC ( Comput (p,s,k) ) )))))))
proof
let S being  halting  IC-Ins-separated non empty AMI-Struct over N;
let p being ( NAT ) -defined (the Instructions of S) -valued Function;
let s being (State of S);
let k being Nat;
assume A1: p halts_on s;
then consider n being Nat such that A2: ( IC ( Comput (p,s,n) ) ) in ( dom p ) and A3: ( CurInstr (p,( Comput (p,s,n) )) ) = ( halt S ) by CKB19:def 1;
hereby
assume A4: ( Result (p,s) ) = ( Comput (p,s,k) );
consider i being (Element of ( NAT )) such that A5: ( Result (p,s) ) = ( Comput (p,s,i) ) and A6: ( CurInstr (p,( Result (p,s) )) ) = ( halt S ) by A1,CKB24:def 1;
reconsider i,n as (Element of ( NAT )) by ORDINAL1:def 12;
A7:now
per cases ;
suppose i <= n;

hence ( Comput (p,s,i) ) = ( Comput (p,s,n) ) by A5,A6,CKB23:1;
end;
suppose n <= i;

hence ( Comput (p,s,i) ) = ( Comput (p,s,n) ) by A3,CKB23:1;
end;
end;
( p . ( IC ( Comput (p,s,k) ) ) ) = ( halt S ) by A7,A6,A4,A2,A5,PARTFUN1:def 6;
hence p halts_at ( IC ( Comput (p,s,k) ) ) by A7,A2,A5,A4,COMPOS_1:def 6;
end;
assume that
A8: ( IC ( Comput (p,s,k) ) ) in ( dom p )
and
A9: ( p . ( IC ( Comput (p,s,k) ) ) ) = ( halt S );
A10: ( CurInstr (p,( Comput (p,s,k) )) ) = ( halt S ) by A8,A9,PARTFUN1:def 6;
reconsider k,n as (Element of ( NAT )) by ORDINAL1:def 12;
now
per cases ;
suppose n <= k;

hence ( Comput (p,s,k) ) = ( Comput (p,s,n) ) by A3,CKB23:1;
end;
suppose k <= n;

hence ( Comput (p,s,k) ) = ( Comput (p,s,n) ) by A10,CKB23:1;
end;
end;
hence thesis by A3,CKB24:def 1,A1;
end;
