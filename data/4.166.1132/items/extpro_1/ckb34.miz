theorem
Th13: (for S being  halting  IC-Ins-separated non empty AMI-Struct over N holds (for l being (Element of ( NAT )) holds (for P being ( NAT ) -defined (the Instructions of S) -valued Function holds (( l .--> ( halt S ) ) c= P implies (for p being l -started (PartState of S) holds p is P -autonomic)))))
proof
let S being  halting  IC-Ins-separated non empty AMI-Struct over N;
let l being (Element of ( NAT ));
set h = ( halt S );
set p = ( Start-At (l,S) );
let P being ( NAT ) -defined (the Instructions of S) -valued Function;
assume that
A1: ( l .--> ( halt S ) ) c= P;
let p being l -started (PartState of S);
set I = ( p +* P );
let Q1 being (Instruction-Sequence of S),Q2 being (Instruction-Sequence of S);
assume that
A2: (P c= Q1 & P c= Q2);
let s1 being (State of S),s2 being (State of S);
assume that
A3: p c= s1
and
A4: p c= s2;
let i;
A10: (( l .--> ( halt S ) ) c= Q1 & ( l .--> ( halt S ) ) c= Q2) by A1,A2,XBOOLE_1:1;
hence ( ( Comput (Q1,s1,i) ) | ( dom p ) ) = ( s1 | ( dom p ) ) by A3,CKB33:1
.= p by A3,GRFUNC_1:23
.= ( s2 | ( dom p ) ) by A4,GRFUNC_1:23
.= ( ( Comput (Q2,s2,i) ) | ( dom p ) ) by A4,CKB33:1,A10;
end;
