begin
reserve N for  with_non-empty_elements non empty set;
reserve S for  halting  IC-Ins-separated non empty AMI-Struct over N;
reserve P for (Instruction-Sequence of S);
theorem
(for s being (State of S) holds (P halts_on s implies (for k being (Element of ( NAT )) holds (( LifeSpan (P,s) ) <= k implies ( CurInstr (P,( Comput (P,s,k) )) ) = ( halt S )))))
proof
let s being (State of S);
assume P halts_on s;
then A1: ( CurInstr (P,( Comput (P,s,( LifeSpan (P,s) )) )) ) = ( halt S ) by CKB50:def 1;
let k being (Element of ( NAT ));
assume ( LifeSpan (P,s) ) <= k;
then ( Comput (P,s,( LifeSpan (P,s) )) ) = ( Comput (P,s,k) ) by A1,CKB23:1;
hence thesis by A1;
end;
