reserve SC for (SimplicialComplex of X);
theorem
(for A,B being (Subset of SC) holds (A c= B implies ( SC | A ) is (SubSimplicialComplex of ( SC | B ))))
proof
let A being (Subset of SC);
let B being (Subset of SC);
A1: (( ( bool A ) /\ (the topology of SC) ) = (the topology of ( SC | A )) & ( ( bool B ) /\ (the topology of SC) ) = (the topology of ( SC | B ))) by CKB123:1;
assume A2: A c= B;
then ( bool A ) c= ( bool B ) by ZFMISC_1:67;
then A3: ( ( bool A ) /\ (the topology of SC) ) c= ( ( bool B ) /\ (the topology of SC) ) by XBOOLE_1:27;
(( [#] ( SC | A ) ) = A & ( [#] ( SC | B ) ) = B) by CKB122:def 1;
hence thesis by A2,A3,A1,CKB103:def 1;
end;
