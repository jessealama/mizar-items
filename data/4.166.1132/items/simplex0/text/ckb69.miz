registration
cluster  finite-vertices ->  locally-finite  finite-degree for SimplicialComplexStr;
coherence
proof
let K;
assume A6: K is  finite-vertices;
then reconsider V = ( Vertices K ) as  finite (Subset of K) by CKB51:def 1;
thus K is  locally-finite
proof
let v being (Vertex of K);
A7: { S where S is (Subset of K): (S is  simplex-like & v in S) } c= (the topology of K)
proof
let x;
assume x in { S where S is (Subset of K): (S is  simplex-like & v in S) };
then (ex S being (Subset of K) st ((x = S & S is  simplex-like) & v in S));
hence thesis by PRE_TOPC:def 2;
end;
(the topology of K) is  finite by A6,CKB63:1;
hence thesis by A7;
end;

thus K is  finite-membered
proof
let x;
assume A8: x in ( the_family_of K );
then reconsider X = x as (Subset of K);
X is  simplex-like by A8,PRE_TOPC:def 2;
then X c= V by CKB61:1;
hence thesis;
end;

take ( card V );
let A being  finite (Subset of K);
assume A is  open;
hence thesis by CKB61:1,NAT_1:43;
end;
end;
