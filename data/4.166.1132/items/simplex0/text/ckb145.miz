begin
reserve D for non  empty set;
reserve i for Integer;
reserve K for non  void  subset-closed SimplicialComplexStr;
theorem
(for K being non  void  subset-closed (SimplicialComplexStr of D) holds (for S being non  void (SubSimplicialComplex of K) holds (for i being Integer holds (for A being (Simplex of i,S) holds (((A is non  empty or i <= ( degree S )) or ( degree S ) = ( degree K )) implies A is (Simplex of i,K))))))
proof
let K being non  void  subset-closed (SimplicialComplexStr of D);
let S being non  void (SubSimplicialComplex of K);
let i being Integer;
let A being (Simplex of i,S);
assume that
A1: ((A is non  empty or i <= ( degree S )) or ( degree S ) = ( degree K ));
(A in (the topology of S) & (the topology of S) c= (the topology of K)) by CKB103:def 1,PRE_TOPC:def 2;
then A in (the topology of K);
then reconsider B = A as (Simplex of K) by PRE_TOPC:def 2;
A2: ( degree S ) <= ( degree K ) by CKB114:1;
per cases  by A1;
suppose A3: A is non  empty;

then A4: ( - 1 ) <= i by CKB141:def 1;
A5: i <= ( degree S ) by A3,CKB141:def 1;
( - 1 ) <= i by A3,CKB141:def 1;
then A6: ( card B ) = ( i + 1 ) by A5,CKB141:def 1;
i <= ( degree K ) by A2,A5,XXREAL_0:2;
hence thesis by A6,A4,CKB141:def 1;
end;
suppose A7: i <= ( degree S );

then A8: i <= ( degree K ) by A2,XXREAL_0:2;
per cases ;
suppose A9: ( - 1 ) <= i;

then ( card B ) = ( i + 1 ) by A7,CKB141:def 1;
hence thesis by A8,A9,CKB141:def 1;
end;
suppose A10: ( - 1 ) > i;

then B is  empty by CKB141:def 1;
hence thesis by A10,CKB141:def 1;
end;
end;
suppose A11: ( degree S ) = ( degree K );

per cases ;
suppose A12: (( - 1 ) <= i & i <= ( degree S ));

then ( card B ) = ( i + 1 ) by CKB141:def 1;
hence thesis by A11,A12,CKB141:def 1;
end;
suppose A13: (( - 1 ) > i or i > ( degree S ));

then B is  empty by CKB141:def 1;
hence thesis by A11,A13,CKB141:def 1;
end;
end;
end;
