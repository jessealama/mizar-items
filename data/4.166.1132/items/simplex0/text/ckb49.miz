begin
reserve x for set;
reserve K for SimplicialComplexStr;
definition
let K;
func Vertices K -> (Subset of K)
means
:Def4: (for v being (Element of K) holds (v in it iff v is  vertex-like));
existence
proof
set B = { v where v is (Element of K): v is  vertex-like };
B c= ( [#] K )
proof
let x;
assume x in B;
then consider v being (Element of K) such that A1: x = v and A2: v is  vertex-like;
(ex S being (Subset of K) st (S is  simplex-like & v in S)) by A2,CKB48:def 1;
hence thesis by A1;
end;
then reconsider B as (Subset of K);
take B;
let v being (Element of K);
hereby
assume v in B;
then (ex w being (Element of K) st (v = w & w is  vertex-like));
hence v is  vertex-like;
end;
assume v is  vertex-like;
hence thesis;
end;
uniqueness
proof
let V1 being (Subset of K),V2 being (Subset of K);
assume that
A3: (for v being (Element of K) holds (v in V1 iff v is  vertex-like))
and
A4: (for v being (Element of K) holds (v in V2 iff v is  vertex-like));
hereby
let x;
assume A5: x in V1;
then reconsider v = x as (Element of K);
v is  vertex-like by A3,A5;
hence x in V2 by A4;
end;
let x;
assume A6: x in V2;
then reconsider v = x as (Element of K);
v is  vertex-like by A4,A6;
hence thesis by A3;
end;
end;
