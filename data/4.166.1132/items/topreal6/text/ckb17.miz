end;
then A23: X is  bounded_above by XXREAL_2:def 10;
A24: (for r being  real number holds (r in X implies b >= r))
proof
let r being  real number;
assume r in X;
then r in { l where l is Real: (a < l & l < b) } by RCOMP_1:def 2;
then (ex r1 being Real st ((r1 = r & a < r1) & r1 < b));
hence thesis;
end;
( ( a + b ) / 2 ) in X by A4,RCOMP_1:def 2;
hence thesis by A21,A23,A22,A5,A24,A13,SEQ_4:def 1,SEQ_4:def 2;
