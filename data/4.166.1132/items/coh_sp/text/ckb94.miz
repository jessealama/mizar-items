begin
reserve X for set;
theorem
Th49: CatStr (# ( TOL X ),( MapsT X ),( fDom X ),( fCod X ),( fComp X ),( fId X ) #) is Category
proof
set M = ( MapsT X );
set d = ( fDom X );
set c = ( fCod X );
set p = ( fComp X );
set i = ( fId X );
now
thus (for f,g being (Element of M) holds ([ g,f ] in ( dom p ) iff ( d . g ) = ( c . f )))
proof
let f being (Element of M),g being (Element of M);
(( d . g ) = ( dom g ) & ( c . f ) = ( cod f )) by CKB90:def 1,CKB91:def 1;
hence thesis by CKB92:def 1;
end;

thus (for f,g being (Element of M) holds (( d . g ) = ( c . f ) implies (( d . ( p . (g,f) ) ) = ( d . f ) & ( c . ( p . (g,f) ) ) = ( c . g ))))
proof
let f being (Element of M),g being (Element of M);
assume that
A1: ( d . g ) = ( c . f );
A2: (( d . g ) = ( dom g ) & ( c . f ) = ( cod f )) by CKB90:def 1,CKB91:def 1;
then A3: ( p . [ g,f ] ) = ( g * f ) by A1,CKB92:def 1;
A4: (( d . f ) = ( dom f ) & ( c . g ) = ( cod g )) by CKB90:def 1,CKB91:def 1;
(( dom ( g * f ) ) = ( dom f ) & ( cod ( g * f ) ) = ( cod g )) by A1,A2,CKB86:1;
hence thesis by A3,A4,CKB90:def 1,CKB91:def 1;
end;

thus (for f,g,h being (Element of M) holds ((( d . h ) = ( c . g ) & ( d . g ) = ( c . f )) implies ( p . (h,( p . (g,f) )) ) = ( p . (( p . (h,g) ),f) )))
proof
let f being (Element of M),g being (Element of M),h being (Element of M);
assume that
A5: ( d . h ) = ( c . g )
and
A6: ( d . g ) = ( c . f );
A7: (( dom h ) = ( d . h ) & ( cod g ) = ( c . g )) by CKB90:def 1,CKB91:def 1;
then A8: ( dom ( h * g ) ) = ( dom g ) by A5,CKB86:1;
A9: (( dom g ) = ( d . g ) & ( cod f ) = ( c . f )) by CKB90:def 1,CKB91:def 1;
then A10: ( cod ( g * f ) ) = ( dom h ) by A5,A6,A7,CKB86:1;
thus ( p . (h,( p . (g,f) )) ) = ( p . [ h,( g * f ) ] ) by A6,A9,CKB92:def 1
.= ( h * ( g * f ) ) by A10,CKB92:def 1
.= ( ( h * g ) * f ) by A5,A6,A7,A9,CKB87:1
.= ( p . [ ( h * g ),f ] ) by A6,A9,A8,CKB92:def 1
.= ( p . (( p . (h,g) ),f) ) by A5,A7,CKB92:def 1;
end;

let b being (Element of ( TOL X ));
A11: ( i . b ) = ( id$ b ) by CKB93:def 1;
A12: ( cod ( id$ b ) ) = b by CKB88:1;
A13: ( dom ( id$ b ) ) = b by CKB88:1;
hence (( d . ( i . b ) ) = b & ( c . ( i . b ) ) = b) by A11,A12,CKB90:def 1,CKB91:def 1;
thus (for f being (Element of M) holds (( c . f ) = b implies ( p . (( i . b ),f) ) = f))
proof
let f being (Element of M);
assume that
A14: ( c . f ) = b;
A15: ( cod f ) = ( c . f ) by CKB91:def 1;
then ( ( id$ b ) * f ) = f by A14,CKB89:1;
hence thesis by A11,A13,A14,A15,CKB92:def 1;
end;

let g being (Element of M);
assume that
A16: ( d . g ) = b;
A17: ( dom g ) = ( d . g ) by CKB90:def 1;
then ( g * ( id$ b ) ) = g by A16,CKB89:1;
hence ( p . (g,( i . b )) ) = g by A11,A12,A16,A17,CKB92:def 1;
end;
hence thesis by CAT_1:def 5;
end;
