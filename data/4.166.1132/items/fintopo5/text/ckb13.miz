begin
definition
let n being (Element of ( NAT ));
let m being (Element of ( NAT ));
func Nbdl2 (n,m) -> (Relation of [: ( Seg n ),( Seg m ) :]) means 
:Def2: (for x being set holds (x in [: ( Seg n ),( Seg m ) :] implies (for i,j being (Element of ( NAT )) holds (x = [ i,j ] implies ( Im (it,x) ) = [: ( Im (( Nbdl1 n ),i) ),( Im (( Nbdl1 m ),j) ) :]))));
existence
proof
defpred P[ set,set ]
 means
(for i,j being (Element of ( NAT )) holds ($1 = [ i,j ] implies $2 = [: ( Im (( Nbdl1 n ),i) ),( Im (( Nbdl1 m ),j) ) :]));
A1: (for x being set holds (x in [: ( Seg n ),( Seg m ) :] implies (ex y being set st (y in ( bool [: ( Seg n ),( Seg m ) :] ) & P[ x,y ]))))
proof
let x being set;
assume x in [: ( Seg n ),( Seg m ) :];
then consider u,y being set such that A2: (u in ( Seg n ) & y in ( Seg m )) and A3: x = [ u,y ] by ZFMISC_1:def 2;
reconsider i = u,j = y as (Element of ( NAT )) by A2;
set y3 = [: ( Im (( Nbdl1 n ),i) ),( Im (( Nbdl1 m ),j) ) :];
A4: y3 c= [: ( Seg n ),( Seg m ) :]
proof
let z being set;
assume z in y3;
then (ex x4,y4 being set st ((x4 in ( Im (( Nbdl1 n ),i) ) & y4 in ( Im (( Nbdl1 m ),j) )) & z = [ x4,y4 ])) by ZFMISC_1:def 2;
hence thesis by ZFMISC_1:def 2;
end;
(for i4,j4 being (Element of ( NAT )) holds (x = [ i4,j4 ] implies y3 = [: ( Im (( Nbdl1 n ),i4) ),( Im (( Nbdl1 m ),j4) ) :]))
proof
let i4 being (Element of ( NAT ));
let j4 being (Element of ( NAT ));
assume A5: x = [ i4,j4 ];
then i4 = u by A3,ZFMISC_1:27;
hence thesis by A3,A5,ZFMISC_1:27;
end;
hence thesis by A4;
end;
consider f being (Function of [: ( Seg n ),( Seg m ) :],( bool [: ( Seg n ),( Seg m ) :] )) such that A6: (for x being set holds (x in [: ( Seg n ),( Seg m ) :] implies P[ x,( f . x ) ])) from FUNCT_2:sch 1(A1);
consider R being (Relation of [: ( Seg n ),( Seg m ) :]) such that A7: (for i being set holds (i in [: ( Seg n ),( Seg m ) :] implies ( Im (R,i) ) = ( f . i ))) by FUNCT_2:93;
take R;
let x being set;
assume that
A8: x in [: ( Seg n ),( Seg m ) :];
let i being (Element of ( NAT ));
let j being (Element of ( NAT ));
assume that
A9: x = [ i,j ];
thus ( Im (R,x) ) = ( f . x ) by A7,A8
.= [: ( Im (( Nbdl1 n ),i) ),( Im (( Nbdl1 m ),j) ) :] by A6,A8,A9;
end;
uniqueness
proof
let f1 being (Relation of [: ( Seg n ),( Seg m ) :]);
let f2 being (Relation of [: ( Seg n ),( Seg m ) :]);
assume that
A10: (for x being set holds (x in [: ( Seg n ),( Seg m ) :] implies (for i,j being (Element of ( NAT )) holds (x = [ i,j ] implies ( Im (f1,x) ) = [: ( Im (( Nbdl1 n ),i) ),( Im (( Nbdl1 m ),j) ) :]))))
and
A11: (for x being set holds (x in [: ( Seg n ),( Seg m ) :] implies (for i,j being (Element of ( NAT )) holds (x = [ i,j ] implies ( Im (f2,x) ) = [: ( Im (( Nbdl1 n ),i) ),( Im (( Nbdl1 m ),j) ) :]))));
(for x being set holds (x in [: ( Seg n ),( Seg m ) :] implies ( Im (f1,x) ) = ( Im (f2,x) )))
proof
let x being set;
assume A12: x in [: ( Seg n ),( Seg m ) :];
then consider u,y being set such that A13: (u in ( Seg n ) & y in ( Seg m )) and A14: x = [ u,y ] by ZFMISC_1:def 2;
reconsider i = u,j = y as (Element of ( NAT )) by A13;
( Im (f1,x) ) = [: ( Im (( Nbdl1 n ),i) ),( Im (( Nbdl1 m ),j) ) :] by A10,A12,A14;
hence thesis by A11,A12,A14;
end;
hence f1 = f2 by RELSET_1:31;
end;
