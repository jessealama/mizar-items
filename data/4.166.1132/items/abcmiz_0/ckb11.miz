begin
theorem
Th1: (for T being  Noetherian sup-Semilattice holds (for I being (Ideal of T) holds ( ex_sup_of I,T & ( sup I ) in I)))
proof
let T being  Noetherian sup-Semilattice;
let I being (Ideal of T);
consider a being (Element of T) such that A1: a in I and A2: (for b being (Element of T) holds (b in I implies (not a < b))) by CKB5:def 1;
A3: I is_<=_than a
proof
let d being (Element of T);
assume d in I;
then ( a "\/" d ) in I by A1,WAYBEL_0:40;
then A4: (not a < ( a "\/" d )) by A2;
a <= ( a "\/" d ) by YELLOW_0:22;
then a = ( a "\/" d ) by A4,ORDERS_2:def 6;
hence thesis by YELLOW_0:22;
end;
(for c being (Element of T) holds (I is_<=_than c implies a <= c)) by A1,LATTICE3:def 9;
hence thesis by A1,A3,YELLOW_0:30;
end;
