begin
theorem
(for T being non  empty TA-structure holds (for a being (adjective of T) holds ( types a ) = { t where t is (type of T): a in ( adjs t ) }))
proof
let T being non  empty TA-structure;
let a being (adjective of T);
set X = { t where t is (type of T): a in ( adjs t ) };
X c= (the carrier of T)
proof
let x being set;
assume x in X;
then (ex t being (type of T) st (x = t & a in ( adjs t )));
hence thesis;
end;
then reconsider X as (Subset of T);
(for x being set holds (x in X iff (ex t being (type of T) st (x = t & a in ( adjs t )))));
hence thesis by CKB37:def 1;
end;
