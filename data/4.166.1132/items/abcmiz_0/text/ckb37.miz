begin
definition
let T being TA-structure;
let a being (Element of (the adjectives of T));
func types a -> (Subset of T) means 
:Def12: (for x being set holds (x in it iff (ex t being (type of T) st (x = t & a in ( adjs t )))));
existence
proof
defpred P[ set ]
 means
(ex t being (type of T) st ($1 = t & a in ( adjs t )));
consider tt being set such that A1: (for x being set holds (x in tt iff (x in (the carrier of T) & P[ x ]))) from XBOOLE_0:sch 1;
tt c= (the carrier of T)
proof
let x being set;
thus thesis by A1;
end;
then reconsider tt as (Subset of T);
take tt;
let x being set;
thus (x in tt implies (ex t being (type of T) st (x = t & a in ( adjs t )))) by A1;
given t being (type of T) such that
A2: x = t
and
A3: a in ( adjs t );

now
A4: ( dom (the adj-map of T) ) = (the carrier of T) by FUNCT_2:def 1;
assume (not x in (the carrier of T));
hence contradiction by A2,A3,A4,FUNCT_1:def 2;
end;
hence thesis by A1,A2,A3;
end;
uniqueness
proof
defpred P[ set ]
 means
(ex t being (type of T) st ($1 = t & a in ( adjs t )));
let X1 being (Subset of T);
let X2 being (Subset of T);
assume that
A5: (for x being set holds (x in X1 iff P[ x ]))
and
A6: (for x being set holds (x in X2 iff P[ x ]));
thus thesis from XBOOLE_0:sch 2(A5,A6);
end;
end;
