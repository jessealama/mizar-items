begin
theorem
Th11: (for m,k being Nat holds (k >= 2 implies (for r being (Tuple of ( m + 2 ),( k -SD )) holds ( ( SDDec ( Mmin r ) ) + ( SDDec ( DecSD (( 0 ),( m + 2 ),k) ) ) ) = ( ( SDDec ( M0 r ) ) + ( SDDec ( SDMin (( m + 2 ),m,k) ) ) ))))
proof
let m being Nat,k being Nat;
assume that
A1: k >= 2;
let r being (Tuple of ( m + 2 ),( k -SD ));
A2: (for i being Nat holds (i in ( Seg ( m + 2 ) ) implies ((( DigA (( M0 r ),i) ) = ( DigA (( Mmin r ),i) ) & ( DigA (( SDMin (( m + 2 ),m,k) ),i) ) = ( 0 )) or (( DigA (( SDMin (( m + 2 ),m,k) ),i) ) = ( DigA (( Mmin r ),i) ) & ( DigA (( M0 r ),i) ) = ( 0 )))))
proof
let i being Nat;
assume A3: i in ( Seg ( m + 2 ) );
then A4: ( DigA (( Mmin r ),i) ) = ( MminDigit (r,i) ) by CKB9:def 1;
A5: i >= 1 by A3,FINSEQ_1:1;
now
per cases ;
suppose A6: i < m;

A7: ( DigA (( M0 r ),i) ) = ( M0Digit (r,i) ) by A3,CKB5:def 1
.= ( 0 ) by A3,A6,CKB4:def 1;
( DigA (( Mmin r ),i) ) = ( ( - ( Radix k ) ) + 1 ) by A1,A3,A4,A6,CKB8:def 1
.= ( SDMinDigit (m,k,i) ) by A1,A5,A6,RADIX_5:def 1
.= ( DigA (( SDMin (( m + 2 ),m,k) ),i) ) by A3,RADIX_5:def 2;
hence thesis by A7;
end;
suppose A8: i >= m;

A9: ( DigA (( SDMin (( m + 2 ),m,k) ),i) ) = ( SDMinDigit (m,k,i) ) by A3,RADIX_5:def 2
.= ( 0 ) by A1,A8,RADIX_5:def 1;
( DigA (( Mmin r ),i) ) = ( r . i ) by A1,A3,A4,A8,CKB8:def 1
.= ( M0Digit (r,i) ) by A3,A8,CKB4:def 1
.= ( DigA (( M0 r ),i) ) by A3,CKB5:def 1;
hence thesis by A9;
end;
end;
hence thesis;
end;
( m + 2 ) >= 1 by NAT_1:12;
hence thesis by A1,A2,RADIX_5:15;
end;
