begin
theorem
(for m being Nat holds (m >= 1 implies (for n,k being Nat holds (for r being (Tuple of ( m + 2 ),( k -SD )) holds ((((k >= 2 & n in ( Seg ( m + 2 ) )) & ( Mmask r ) is_Zero_over n) & ( DigA (( Mmask r ),n) ) > ( 0 )) implies ( SDDec ( Mmask r ) ) > ( 0 ))))))
proof
let m being Nat;
assume m >= 1;
then A1: ( m + 2 ) >= 1 by CKB1:1;
let n being Nat;
let k being Nat;
let r being (Tuple of ( m + 2 ),( k -SD ));
assume that
A2: k >= 2
and
A3: n in ( Seg ( m + 2 ) )
and
A4: ( Mmask r ) is_Zero_over n
and
A5: ( DigA (( Mmask r ),n) ) > ( 0 );
(for i being Nat holds (i in ( Seg ( m + 2 ) ) implies ( DigA (( Mmask r ),i) ) >= ( DigA (( FSDMin (( m + 2 ),n,k) ),i) )))
proof
let i being Nat;
assume A6: i in ( Seg ( m + 2 ) );
now
per cases ;
suppose A7: i > n;

( DigA (( FSDMin (( m + 2 ),n,k) ),i) ) = ( FSDMinDigit (n,k,i) ) by A6,CKB30:def 1
.= ( 0 ) by A2,A7,CKB29:def 1;
hence thesis by A4,A7,CKB32:def 1;
end;
suppose A8: i <= n;

now
per cases  by A8,XXREAL_0:1;
suppose A9: i = n;

then A10: ( DigA (( Mmask r ),i) ) >= ( ( 0 ) + 1 ) by A5,INT_1:7;
( DigA (( FSDMin (( m + 2 ),n,k) ),i) ) = ( FSDMinDigit (n,k,i) ) by A6,CKB30:def 1
.= 1 by A2,A9,CKB29:def 1;
hence thesis by A10;
end;
suppose A11: i < n;

A12: ( DigA (( Mmask r ),i) ) = ( ( Mmask r ) . i ) by A6,RADIX_1:def 3;
A13: ( ( Mmask r ) . i ) is (Element of ( k -SD )) by A6,RADIX_1:15;
( DigA (( FSDMin (( m + 2 ),n,k) ),i) ) = ( FSDMinDigit (n,k,i) ) by A6,CKB30:def 1
.= ( ( - ( Radix k ) ) + 1 ) by A2,A11,CKB29:def 1;
hence thesis by A12,A13,RADIX_1:13;
end;
end;
hence thesis;
end;
end;
hence thesis;
end;
then ( SDDec ( Mmask r ) ) >= ( SDDec ( FSDMin (( m + 2 ),n,k) ) ) by A1,RADIX_5:13;
hence thesis by A2,A3,A1,CKB31:1;
end;
