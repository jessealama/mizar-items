begin
theorem
Th4: (for m,k being Nat holds (k >= 2 implies (for r being (Tuple of ( m + 2 ),( k -SD )) holds ( SDDec r ) >= ( SDDec ( Mmin r ) ))))
proof
let m being Nat;
let k being Nat;
assume that
A1: k >= 2;
let r being (Tuple of ( m + 2 ),( k -SD ));
(for i being Nat holds (i in ( Seg ( m + 2 ) ) implies ( DigA (r,i) ) >= ( DigA (( Mmin r ),i) )))
proof
let i being Nat;
assume A2: i in ( Seg ( m + 2 ) );
then A3: ( DigA (( Mmin r ),i) ) = ( MminDigit (r,i) ) by CKB9:def 1;
now
per cases ;
suppose i >= m;

then ( DigA (( Mmin r ),i) ) = ( r . i ) by A1,A2,A3,CKB8:def 1
.= ( DigA (r,i) ) by A2,RADIX_1:def 3;
hence thesis;
end;
suppose A4: i < m;

A5: ( DigA (r,i) ) = ( r . i ) by A2,RADIX_1:def 3;
A6: ( r . i ) is (Element of ( k -SD )) by A2,RADIX_1:15;
( DigA (( Mmin r ),i) ) = ( ( - ( Radix k ) ) + 1 ) by A1,A2,A3,A4,CKB8:def 1;
hence thesis by A5,A6,RADIX_1:13;
end;
end;
hence thesis;
end;
hence thesis by NAT_1:12,RADIX_5:13;
end;
