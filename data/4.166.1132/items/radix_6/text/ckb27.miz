constructors REAL_1,NAT_D,NEWTON,GR_CY_1,RADIX_5,SEQ_1,RELSET_1,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB12,CKB25,CKB26;
requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
theorem
Th17: (for m,k being Nat holds (for r being (Tuple of ( m + 2 ),( k -SD )) holds (k >= 2 implies ( ( SDDec ( M0 r ) ) + ( SDDec ( Mmask r ) ) ) = ( ( SDDec r ) + ( SDDec ( DecSD (( 0 ),( m + 2 ),k) ) ) ))))
proof
let m being Nat;
let k being Nat;
let r being (Tuple of ( m + 2 ),( k -SD ));
A1: ( m + 2 ) >= 1 by NAT_1:12;
A2: (for i being Nat holds (i in ( Seg ( m + 2 ) ) implies ((( DigA (( M0 r ),i) ) = ( DigA (r,i) ) & ( DigA (( Mmask r ),i) ) = ( 0 )) or (( DigA (( Mmask r ),i) ) = ( DigA (r,i) ) & ( DigA (( M0 r ),i) ) = ( 0 )))))
proof
let i being Nat;
assume A3: i in ( Seg ( m + 2 ) );
now
per cases ;
suppose A4: i < m;

A5: ( DigA (( M0 r ),i) ) = ( M0Digit (r,i) ) by A3,CKB5:def 1
.= ( 0 ) by A3,A4,CKB4:def 1;
( DigA (( Mmask r ),i) ) = ( MmaskDigit (r,i) ) by A3,CKB26:def 1
.= ( r . i ) by A3,A4,CKB25:def 1
.= ( DigA (r,i) ) by A3,RADIX_1:def 3;
hence thesis by A5;
end;
suppose A6: i >= m;

A7: ( DigA (( Mmask r ),i) ) = ( MmaskDigit (r,i) ) by A3,CKB26:def 1
.= ( 0 ) by A3,A6,CKB25:def 1;
( DigA (( M0 r ),i) ) = ( M0Digit (r,i) ) by A3,CKB5:def 1
.= ( r . i ) by A3,A6,CKB4:def 1
.= ( DigA (r,i) ) by A3,RADIX_1:def 3;
hence thesis by A7;
end;
end;
hence thesis;
end;
assume k >= 2;
hence thesis by A1,A2,RADIX_5:15;
end;
