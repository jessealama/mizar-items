environ
vocabularies NUMBERS,NAT_1,XXREAL_0,ARYTM_3,RADIX_1,RADIX_5,FINSEQ_1,CARD_1,FUNCT_1,NEWTON,RELAT_1,PARTFUN1,ARYTM_1,CARD_3,FINSEQ_2,SUBSET_1,INT_1,RADIX_6;
notations SUBSET_1,NUMBERS,XCMPLX_0,XXREAL_0,INT_1,NAT_1,NAT_D,NEWTON,RADIX_1,FINSEQ_1,FINSEQ_2,FUNCT_1,RELSET_1,PARTFUN1,GR_CY_1,RADIX_5,CKB4,CKB5,CKB6;
definitions CKB4,CKB5,CKB6;
theorems RADIX_1,RADIX_2,RADIX_4,RADIX_5,INT_1,NAT_1,NAT_2,FINSEQ_1,FINSEQ_2,RVSUM_1,NEWTON,PREPOWER,FUNCT_1,FINSEQ_3,XREAL_1,XXREAL_0,NAT_D,CARD_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6;
schemes FINSEQ_2,NAT_1;
registrations RELSET_1,XXREAL_0,XREAL_0,NAT_1,INT_1,ORDINAL1,NEWTON,XBOOLE_0,FINSEQ_2,CARD_1;
constructors REAL_1,NAT_D,NEWTON,GR_CY_1,RADIX_5,SEQ_1,RELSET_1,CKB4,CKB5,CKB6;
requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
definition
let m being Nat;
let k being Nat;
let r being (Tuple of ( m + 2 ),( k -SD ));
func Mmax r -> (Tuple of ( m + 2 ),( k -SD )) means 
:Def4: (for i being Nat holds (i in ( Seg ( m + 2 ) ) implies ( DigA (it,i) ) = ( MmaxDigit (r,i) )));
existence
proof
deffunc F(Nat) = ( MmaxDigit (r,$1) );
consider z being (FinSequence of ( k -SD )) such that A1: ( len z ) = ( m + 2 ) and A2: (for j being Nat holds (j in ( dom z ) implies ( z . j ) = F(j))) from FINSEQ_2:sch 1;
A3: ( dom z ) = ( Seg ( m + 2 ) ) by A1,FINSEQ_1:def 3;
z is (Element of ( ( m + 2 ) -tuples_on ( k -SD ) )) by A1,FINSEQ_2:92;
then reconsider z as (Tuple of ( m + 2 ),( k -SD ));
take z;
let i being Nat;
assume A4: i in ( Seg ( m + 2 ) );
hence ( DigA (z,i) ) = ( z . i ) by RADIX_1:def 3
.= ( MmaxDigit (r,i) ) by A2,A3,A4;
end;
uniqueness
proof
let k1 being (Tuple of ( m + 2 ),( k -SD ));
let k2 being (Tuple of ( m + 2 ),( k -SD ));
assume that
A5: (for i being Nat holds (i in ( Seg ( m + 2 ) ) implies ( DigA (k1,i) ) = ( MmaxDigit (r,i) )))
and
A6: (for i being Nat holds (i in ( Seg ( m + 2 ) ) implies ( DigA (k2,i) ) = ( MmaxDigit (r,i) )));
A7: ( len k1 ) = ( m + 2 ) by CARD_1:def 7;
then A8: ( dom k1 ) = ( Seg ( m + 2 ) ) by FINSEQ_1:def 3;
A9:now
let j being Nat;
assume A10: j in ( dom k1 );
then ( k1 . j ) = ( DigA (k1,j) ) by A8,RADIX_1:def 3
.= ( MmaxDigit (r,j) ) by A5,A8,A10
.= ( DigA (k2,j) ) by A6,A8,A10
.= ( k2 . j ) by A8,A10,RADIX_1:def 3;
hence ( k1 . j ) = ( k2 . j );
end;
( len k2 ) = ( m + 2 ) by CARD_1:def 7;
hence k1 = k2 by A7,A9,FINSEQ_2:9;
end;
end;
