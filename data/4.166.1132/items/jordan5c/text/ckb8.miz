begin
theorem
Th6: (for P,Q being (Subset of ( TOP-REAL 2 )) holds (for p1,p2 being (Point of ( TOP-REAL 2 )) holds (((p2 in Q & ( P /\ Q ) is  closed) & P is_an_arc_of p1,p2) implies ( Last_Point (P,p1,p2,Q) ) = p2)))
proof
let P being (Subset of ( TOP-REAL 2 ));
let Q being (Subset of ( TOP-REAL 2 ));
let p1 being (Point of ( TOP-REAL 2 ));
let p2 being (Point of ( TOP-REAL 2 ));
assume that
A1: p2 in Q
and
A2: ( P /\ Q ) is  closed
and
A3: P is_an_arc_of p1,p2;
A4: (for g being (Function of ( I[01] ),( ( TOP-REAL 2 ) | P )) holds (for s2 being Real holds ((((((g is  being_homeomorphism & ( g . ( 0 ) ) = p1) & ( g . 1 ) = p2) & ( g . s2 ) = p2) & ( 0 ) <= s2) & s2 <= 1) implies (for t being Real holds ((1 >= t & t > s2) implies (not ( g . t ) in Q))))))
proof
let g being (Function of ( I[01] ),( ( TOP-REAL 2 ) | P ));
let s2 being Real;
assume that
A5: g is  being_homeomorphism
and
( g . ( 0 ) ) = p1
and
A6: (( g . 1 ) = p2 & ( g . s2 ) = p2)
and
A7: (( 0 ) <= s2 & s2 <= 1);
A8: g is  one-to-one by A5,TOPS_2:def 5;
let t being Real;
assume A9: (1 >= t & t > s2);
A10: ( dom g ) = ( [#] ( I[01] ) ) by A5,TOPS_2:def 5
.= (the carrier of ( I[01] ));
then A11: 1 in ( dom g ) by BORSUK_1:43;
s2 in ( dom g ) by A7,A10,BORSUK_1:43;
hence thesis by A6,A11,A8,A9,FUNCT_1:def 4;
end;
p2 in P by A3,TOPREAL1:1;
