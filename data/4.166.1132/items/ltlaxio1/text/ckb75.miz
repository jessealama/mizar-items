registrations SUBSET_1,ORDINAL1,FUNCT_1,XXREAL_0,NAT_1,XBOOLEAN,RELSET_1,MARGREL1,XBOOLE_0,XREAL_0,FUNCT_2,HILBERT1,CKB57,CKB63;
constructors XXREAL_0,NAT_D,RELSET_1,AOFA_I00,HILBERT2,DOMAIN_1,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB20,CKB38,CKB39,CKB40,CKB50,CKB53,CKB55,CKB56,CKB60,CKB61,CKB62,CKB64,CKB65,CKB66,CKB67,CKB68,CKB69,CKB70,CKB73,CKB74;
requirements NUMERALS,BOOLE,SUBSET,ARITHM,REAL;
begin
reserve X for (Subset of ( LTLB_WFF ));
reserve f for (FinSequence of ( LTLB_WFF ));
reserve f2 for (FinSequence of ( LTLB_WFF ));
theorem
Th40: (for i,n being Nat holds ((((( n + ( len f ) ) <= ( len f2 ) & (for k being Nat holds ((1 <= k & k <= ( len f )) implies ( f . k ) = ( f2 . ( k + n ) )))) & 1 <= i) & i <= ( len f )) implies ( prc f,X,i implies  prc f2,X,( i + n ))))
proof
let i being Nat,n being Nat;
assume that
A1: ( n + ( len f ) ) <= ( len f2 )
and
A2: (for k being Nat holds ((1 <= k & k <= ( len f )) implies ( f . k ) = ( f2 . ( k + n ) )))
and
A3: 1 <= i
and
A4: i <= ( len f );
( i + n ) <= ( ( len f ) + n ) by A4,XREAL_1:6;
then A5: ( i + n ) <= ( len f2 ) by A1,XXREAL_0:2;
A6: ( f /. i ) = ( f . i ) by A3,A4,CKB1:1
.= ( f2 . ( i + n ) ) by A2,A3,A4
.= ( f2 /. ( i + n ) ) by A3,A5,CKB1:1,NAT_1:12;
assume A7:  prc f,X,i;
per cases  by A7,CKB73:def 1;
suppose ( f . i ) in ( LTL_axioms );

then ( f2 . ( i + n ) ) in ( LTL_axioms ) by A2,A3,A4;
hence  prc f2,X,( i + n ) by CKB73:def 1;
end;
suppose ( f . i ) in X;

then ( f2 . ( i + n ) ) in X by A2,A3,A4;
hence  prc f2,X,( i + n ) by CKB73:def 1;
end;
suppose (ex j,k being Nat st ((((1 <= j & j < i) & 1 <= k) & k < i) & (( f /. j ),( f /. k ) MP_rule ( f /. i ) or ( f /. j ),( f /. k ) IND_rule ( f /. i ))));

then consider j being Nat,k being Nat such that A8: 1 <= j and A9: j < i and A10: 1 <= k and A11: k < i and A12: (( f /. j ),( f /. k ) MP_rule ( f /. i ) or ( f /. j ),( f /. k ) IND_rule ( f /. i ));
A13: (1 <= ( j + n ) & ( j + n ) < ( i + n )) by A8,A9,NAT_1:12,XREAL_1:8;
A14: k <= ( len f ) by A4,A11,XXREAL_0:2;
then ( k + n ) <= ( ( len f ) + n ) by XREAL_1:6;
then A15: ( k + n ) <= ( len f2 ) by A1,XXREAL_0:2;
A16: j <= ( len f ) by A4,A9,XXREAL_0:2;
then ( j + n ) <= ( ( len f ) + n ) by XREAL_1:6;
then A17: ( j + n ) <= ( len f2 ) by A1,XXREAL_0:2;
A18: ( f /. k ) = ( f . k ) by A10,A14,CKB1:1
.= ( f2 . ( k + n ) ) by A2,A10,A14
.= ( f2 /. ( k + n ) ) by A10,A15,CKB1:1,NAT_1:12;
A19: (1 <= ( k + n ) & ( k + n ) < ( i + n )) by A10,A11,NAT_1:12,XREAL_1:8;
( f /. j ) = ( f . j ) by A8,A16,CKB1:1
.= ( f2 . ( j + n ) ) by A2,A8,A16
.= ( f2 /. ( j + n ) ) by A8,A17,CKB1:1,NAT_1:12;
hence  prc f2,X,( i + n ) by A6,A12,A13,A19,A18,CKB73:def 1;
end;
suppose (ex j being Nat st ((1 <= j & j < i) & ( f /. j ) NEX_rule ( f /. i )));

then consider j being Nat such that A20: 1 <= j and A21: j < i and A22: ( f /. j ) NEX_rule ( f /. i );
A23: (1 <= ( j + n ) & ( j + n ) < ( i + n )) by A20,A21,NAT_1:12,XREAL_1:8;
A24: j <= ( len f ) by A4,A21,XXREAL_0:2;
then ( j + n ) <= ( ( len f ) + n ) by XREAL_1:6;
then A25: ( j + n ) <= ( len f2 ) by A1,XXREAL_0:2;
( f /. j ) = ( f . j ) by A20,A24,CKB1:1
.= ( f2 . ( j + n ) ) by A2,A20,A24
.= ( f2 /. ( j + n ) ) by A20,A25,CKB1:1,NAT_1:12;
hence  prc f2,X,( i + n ) by A6,A22,A23,CKB73:def 1;
end;
end;
