assume A5:  prc f,X,( len f );
A6: ( ( 0 ) + ( len f1 ) ) <= ( len f ) by A3,NAT_1:12;
A7:now
let i being Nat;
assume that
A8: 1 <= i
and
A9: i <= ( len f );
A10: (i < ( ( len f1 ) + 1 ) or i = ( ( len f1 ) + 1 )) by A3,A9,XXREAL_0:1;
A11: (for k being Nat holds ((1 <= k & k <= ( len f1 )) implies ( f1 . k ) = ( f . ( k + ( 0 ) ) ))) by A1,FINSEQ_1:64;
per cases  by A3,A10,NAT_1:13;
suppose i <= ( len f1 );

then  prc f,X,( i + ( 0 ) ) by A2,A6,A8,A11,CKB75:1;
hence  prc f,X,i;
end;
suppose i = ( len f );

hence  prc f,X,i by A5;
end;
end;
hence (for i being Nat holds ((1 <= i & i <= ( len f )) implies  prc f,X,i));
( f . ( len f ) ) = ( f . ( ( len f1 ) + ( len <* p *> ) ) ) by A1,FINSEQ_1:22
.= ( f . ( ( len f1 ) + 1 ) ) by FINSEQ_1:39
.= p by A1,FINSEQ_1:42;
hence X |- p by A4,A7,CKB74:def 1;
end;
