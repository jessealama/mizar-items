constructors XXREAL_0,NAT_D,RELSET_1,AOFA_I00,HILBERT2,DOMAIN_1,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB20,CKB38,CKB39,CKB40;
requirements NUMERALS,BOOLE,SUBSET,ARITHM,REAL;
begin
reserve A for (Element of ( LTLB_WFF ));
reserve B for (Element of ( LTLB_WFF ));
reserve F for (Subset of ( LTLB_WFF ));
reserve i for (Element of ( NAT ));
reserve j for (Element of ( NAT ));
reserve n for (Element of ( NAT ));
reserve M for LTLModel;
theorem
(( F \/ { A } ) |= B iff F |= ( ( 'G' A ) => B ))
proof
hereby
assume A1: ( F \/ { A } ) |= B;
thus F |= ( ( 'G' A ) => B )
proof
let M;
assume A2: M |= F;
thus M |= ( ( 'G' A ) => B )
proof
let n;
per cases  by XBOOLEAN:def 3;
suppose A3: ( ( SAT M ) . [ n,( 'G' A ) ] ) = ( 0 );

thus ( ( SAT M ) . [ n,( ( 'G' A ) => B ) ] ) = ( ( ( SAT M ) . [ n,( 'G' A ) ] ) => ( ( SAT M ) . [ n,B ] ) ) by CKB20:def 1
.= 1 by A3;
end;
suppose A4: ( ( SAT M ) . [ n,( 'G' A ) ] ) = 1;

now
let j;
( ( SAT M ) . [ ( n + j ),A ] ) = 1 by A4,CKB26:1;
hence ( ( SAT ( M ^\ n ) ) . [ j,A ] ) = 1 by CKB47:1;
end;
then ( M ^\ n ) |= A by CKB38:def 1;
then A5: ( M ^\ n ) |= { A } by CKB42:1;
( M ^\ n ) |= F by A2,CKB48:1;
then ( M ^\ n ) |= ( F \/ { A } ) by A5,CKB41:1;
then ( M ^\ n ) |= B by A1,CKB40:def 1;
then ( ( SAT ( M ^\ n ) ) . [ ( 0 ),B ] ) = 1 by CKB38:def 1;
then A6: ( ( SAT M ) . [ ( n + ( 0 ) ),B ] ) = 1 by CKB47:1;
thus ( ( SAT M ) . [ n,( ( 'G' A ) => B ) ] ) = ( ( ( SAT M ) . [ n,( 'G' A ) ] ) => ( ( SAT M ) . [ n,B ] ) ) by CKB20:def 1
.= 1 by A6;
end;
end;

end;

end;
assume A7: F |= ( ( 'G' A ) => B );
let M;
assume A8: M |= ( F \/ { A } );
let i;
M |= { A } by A8,CKB41:1;
then M |= A by CKB42:1;
then (for j holds ( ( SAT M ) . [ ( i + j ),A ] ) = 1) by CKB38:def 1;
then A9: ( ( SAT M ) . [ i,( 'G' A ) ] ) = 1 by CKB26:1;
M |= F by A8,CKB41:1;
then M |= ( ( 'G' A ) => B ) by A7,CKB40:def 1;
then ( ( SAT M ) . [ i,( ( 'G' A ) => B ) ] ) = 1 by CKB38:def 1;
then ( ( ( SAT M ) . [ i,( 'G' A ) ] ) => ( ( SAT M ) . [ i,B ] ) ) = 1 by CKB20:def 1;
hence ( ( SAT M ) . [ i,B ] ) = 1 by A9;
end;
