begin
reserve A for (Element of ( LTLB_WFF ));
reserve B for (Element of ( LTLB_WFF ));
reserve F for (Subset of ( LTLB_WFF ));
reserve M for LTLModel;
theorem
Th25: ((F |= A & F |= ( A => B )) implies F |= B)
proof
assume that
A1: F |= A
and
A2: F |= ( A => B );
let M;
assume A3: M |= F;
let n being (Element of ( NAT ));
M |= ( A => B ) by A2,A3,CKB40:def 1;
then ( ( SAT M ) . [ n,( A => B ) ] ) = 1 by CKB38:def 1;
then A4: ( ( ( SAT M ) . [ n,A ] ) => ( ( SAT M ) . [ n,B ] ) ) = 1 by CKB20:def 1;
M |= A by A1,A3,CKB40:def 1;
then ( ( SAT M ) . [ n,A ] ) = 1 by CKB38:def 1;
hence ( ( SAT M ) . [ n,B ] ) = 1 by A4;
end;
