theorem
Th33: (for f being (Function of ( LTLB_WFF ),( BOOLEAN )) holds ((for B being set holds (B in ( LTLB_WFF ) implies ( f . B ) = ( ( SAT M ) . [ n,B ] ))) implies ( ( VAL f ) . A ) = ( ( SAT M ) . [ n,A ] )))
proof
let f being (Function of ( LTLB_WFF ),( BOOLEAN ));
defpred P1[ (Element of ( LTLB_WFF )) ]
 means
( ( VAL f ) . $1 ) = ( ( SAT M ) . [ n,$1 ] );
assume A1: (for B being set holds (B in ( LTLB_WFF ) implies ( f . B ) = ( ( SAT M ) . [ n,B ] )));
A2: (for k holds P1[ ( prop k ) ])
proof
let k;
( ( SAT M ) . [ n,( prop k ) ] ) = ( f . ( prop k ) ) by A1
.= ( ( VAL f ) . ( prop k ) ) by CKB50:def 1;
hence thesis;
end;
A3: (for r holds (for s holds ((P1[ r ] & P1[ s ]) implies (P1[ ( r 'Us' s ) ] & P1[ ( r => s ) ]))))
proof
let r;
let s;
assume A4: (P1[ r ] & P1[ s ]);
( ( VAL f ) . ( r 'Us' s ) ) = ( f . ( r 'Us' s ) ) by CKB50:def 1
.= ( ( SAT M ) . [ n,( r 'Us' s ) ] ) by A1;
hence P1[ ( r 'Us' s ) ];
( ( SAT M ) . [ n,( r => s ) ] ) = ( ( ( SAT M ) . [ n,r ] ) => ( ( SAT M ) . [ n,s ] ) ) by CKB20:def 1
.= ( ( VAL f ) . ( r => s ) ) by A4,CKB50:def 1;
hence P1[ ( r => s ) ];
end;
( ( SAT M ) . [ n,( TFALSUM ) ] ) = ( 0 ) by CKB20:def 1
.= ( ( VAL f ) . ( TFALSUM ) ) by CKB50:def 1;
then A5: P1[ ( TFALSUM ) ];
(for r holds P1[ r ]) from HILBERT2:sch 2(A5,A2,A3);
hence ( ( VAL f ) . A ) = ( ( SAT M ) . [ n,A ] );
end;
