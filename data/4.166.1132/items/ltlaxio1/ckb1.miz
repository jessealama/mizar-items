begin
theorem
Th1: (for X being set holds (for f being (FinSequence of X) holds (for i being Nat holds ((1 <= i & i <= ( len f )) implies ( f . i ) = ( f /. i )))))
proof
let X being set;
let f being (FinSequence of X);
let i being Nat;
assume (1 <= i & i <= ( len f ));
then i in ( Seg ( len f ) ) by FINSEQ_1:1;
then i in ( dom f ) by FINSEQ_1:def 3;
hence thesis by PARTFUN1:def 6;
end;
