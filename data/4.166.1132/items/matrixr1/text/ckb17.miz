begin
reserve i for Nat;
theorem
Th17: (for r being Real holds (for fr being (Element of ( F_Real )) holds (for p being (FinSequence of ( REAL )) holds (for fp being (FinSequence of ( F_Real )) holds ((r = fr & p = fp) implies ( r * p ) = ( fr * fp ))))))
proof
let r being Real;
let fr being (Element of ( F_Real ));
let p being (FinSequence of ( REAL ));
let fp being (FinSequence of ( F_Real ));
assume that
A1: r = fr
and
A2: p = fp;
A3: ( len ( r * p ) ) = ( len fp ) by A2,RVSUM_1:117;
then A4: ( len ( r * p ) ) = ( len ( fr * fp ) ) by CKB16:1;
(for i being Nat holds ((1 <= i & i <= ( len ( r * p ) )) implies ( ( r * p ) . i ) = ( ( fr * fp ) . i )))
proof
let i being Nat;
assume (1 <= i & i <= ( len ( r * p ) ));
then i in ( Seg ( len fp ) ) by A3,FINSEQ_1:1;
then A6: i in ( dom ( fr * fp ) ) by A3,A4,FINSEQ_1:def 3;
reconsider s = ( fp . i ) as (Element of ( F_Real ));
thus ( ( r * p ) . i ) = ( fr * s ) by A1,A2,RVSUM_1:44
.= ( ( fr * fp ) . i ) by A6,FVSUM_1:50;
end;
hence thesis by A4,FINSEQ_1:14;
end;
