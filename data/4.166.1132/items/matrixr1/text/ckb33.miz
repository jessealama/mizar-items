begin
definition
let a being  real number;
let A being (Matrix of ( REAL ));
func a * A -> (Matrix of ( REAL )) means 
:Def7: (for ea being (Element of ( F_Real )) holds (ea = a implies it = ( MXF2MXR ( ea * ( MXR2MXF A ) ) )));
existence
proof
reconsider aa = a as (Element of ( F_Real )) by XREAL_0:def 1;
set C = ( MXF2MXR ( aa * ( MXR2MXF A ) ) );
(for ea being (Element of ( F_Real )) holds (ea = a implies C = ( MXF2MXR ( ea * ( MXR2MXF A ) ) )));
hence thesis;
end;
uniqueness
proof
reconsider aa = a as (Element of ( F_Real )) by XREAL_0:def 1;
let C1 being (Matrix of ( REAL ));
let C2 being (Matrix of ( REAL ));
assume that
A1: (for ea being (Element of ( F_Real )) holds (ea = a implies C1 = ( MXF2MXR ( ea * ( MXR2MXF A ) ) )))
and
A2: (for ea being (Element of ( F_Real )) holds (ea = a implies C2 = ( MXF2MXR ( ea * ( MXR2MXF A ) ) )));
C2 = ( MXF2MXR ( aa * ( MXR2MXF A ) ) ) by A2;
hence thesis by A1;
end;
end;
