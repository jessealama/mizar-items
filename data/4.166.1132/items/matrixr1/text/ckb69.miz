let x1 being (FinSequence of ( REAL ));
let x2 being (FinSequence of ( REAL ));
let A being (Matrix of ( REAL ));
assume that
A1: ( len x1 ) = ( len x2 )
and
A2: ( width A ) = ( len x1 )
