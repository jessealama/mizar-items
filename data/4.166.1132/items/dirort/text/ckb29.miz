environ
vocabularies RLVECT_1,XBOOLE_0,ANALOAF,SUBSET_1,ANALMETR,ANALORT,STRUCT_0,SYMSP_1,DIRORT;
notations STRUCT_0,RLVECT_1,ANALOAF,ANALMETR,ANALORT,CKB1,CKB4,CKB6,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17;
definitions CKB4,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17;
theorems ANALMETR,ANALORT,CKB2,CKB3,CKB4,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB26,CKB28;
registrations ANALOAF,ANALORT,CKB5,CKB25,CKB27;
constructors ANALMETR,ANALORT,CKB4,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17;
begin
reserve AS for Oriented_Orthogonality_Space;
reserve u for (Element of AS);
reserve u1 for (Element of AS);
reserve u2 for (Element of AS);
reserve v for (Element of AS);
reserve v1 for (Element of AS);
reserve v2 for (Element of AS);
reserve w for (Element of AS);
reserve w1 for (Element of AS);
theorem
(AS is  left_transitive implies AS is  bach_transitive)
proof
((for u,u1,u2,v,v1,v2,w,w1 being (Element of AS) holds (((u,u1 '//' v,v1 & v,v1 '//' w,w1) & u,u1 '//' u2,v2) implies ((u = u1 or v = v1) or u2,v2 '//' w,w1))) implies (for u,u1,u2,v,v1,v2,w,w1 being (Element of AS) holds (((u,u1 '//' v,v1 & w,w1 '//' v,v1) & w,w1 '//' u2,v2) implies ((w = w1 or v = v1) or u,u1 '//' u2,v2))))
proof
assume A1: (for u,u1,u2,v,v1,v2,w,w1 being (Element of AS) holds (((u,u1 '//' v,v1 & v,v1 '//' w,w1) & u,u1 '//' u2,v2) implies ((u = u1 or v = v1) or u2,v2 '//' w,w1)));
let u;
let u1;
let u2;
let v;
let v1;
let v2;
let w;
let w1;
A2: (AS is  left_transitive implies AS is  right_transitive) by CKB28:1;
then A3: (((u,u1 '//' v,v1 & v,v1 '//' w1,w) & u2,v2 '//' w1,w) implies thesis) by A1,CKB14:def 1,CKB15:def 1;
assume that
A4: u,u1 '//' v,v1
and
A5: w,w1 '//' v,v1
and
A6: w,w1 '//' u2,v2;
A7: ((((v = v1 or w = w1) or u,u1 '//' u2,v2) or u,u1 '//' v2,u2) implies ((w = w1 or v = v1) or u,u1 '//' u2,v2))
proof
A8:now
assume that
A9: u,u1 '//' v2,u2
and
A10: u <> u1
and
A11: v <> v1
and
w <> w1;
A12:now
assume A13: u2,v2 '//' u,u1;
then A14: u2,v2 '//' w,w1 by A2,A1,A4,A5,A10,A11,CKB14:def 1,CKB15:def 1;
A15:now
assume u2,v2 '//' w1,w;
then (w = w1 or u2 = v2) by A14,CKB4:def 1;
hence thesis;
end;
w1,w '//' v2,u2 by A6,CKB4:def 1;
then (u2,v2 '//' w1,w or u2 = v2) by A2,A1,A9,A10,A13,CKB14:def 1,CKB15:def 1;
hence thesis by A15;
end;
A16:now
A17: w1,w '//' v1,v by A5,CKB4:def 1;
assume A18: u2,v2 '//' u1,u;
u1,u '//' v1,v by A4,CKB4:def 1;
then A19: u2,v2 '//' w1,w by A2,A1,A10,A11,A18,A17,CKB14:def 1,CKB15:def 1;
A20:now
assume u2,v2 '//' w,w1;
then (w = w1 or u2 = v2) by A19,CKB4:def 1;
hence thesis;
end;
u1,u '//' u2,v2 by A9,CKB4:def 1;
then (u2,v2 '//' w,w1 or u2 = v2) by A2,A1,A6,A10,A18,CKB14:def 1,CKB15:def 1;
hence thesis by A20;
end;
(v2,u2 '//' u,u1 or v2,u2 '//' u1,u) by A9,CKB4:def 1;
hence thesis by A16,A12,CKB4:def 1;
end;
assume (((v = v1 or w = w1) or u,u1 '//' u2,v2) or u,u1 '//' v2,u2);
hence thesis by A8,CKB4:def 1;
end;
A21:now
assume that
A22: u,u1 '//' v,v1
and
A23: v,v1 '//' w,w1
and
A24: u2,v2 '//' w1,w;
v2,u2 '//' w,w1 by A24,CKB4:def 1;
hence thesis by A2,A1,A7,A22,A23,CKB14:def 1,CKB15:def 1;
end;
A25:now
assume that
A26: u,u1 '//' v,v1
and
A27: v,v1 '//' w1,w
and
A28: u2,v2 '//' w,w1;
v2,u2 '//' w1,w by A28,CKB4:def 1;
hence thesis by A2,A1,A7,A26,A27,CKB14:def 1,CKB15:def 1;
end;
(((u,u1 '//' v,v1 & v,v1 '//' w,w1) & u2,v2 '//' w,w1) implies thesis) by A2,A1,CKB14:def 1,CKB15:def 1;
hence thesis by A4,A5,A6,A21,A3,A25,CKB4:def 1;
end;
hence thesis by CKB13:def 1,CKB15:def 1;
end;
