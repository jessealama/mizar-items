environ
vocabularies RLVECT_1,XBOOLE_0,ANALOAF,SUBSET_1,ANALMETR,ANALORT,STRUCT_0,SYMSP_1,DIRORT;
notations STRUCT_0,RLVECT_1,ANALOAF,ANALMETR,ANALORT,CKB1,CKB4,CKB6,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17;
definitions CKB4,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17;
theorems ANALMETR,ANALORT,CKB2,CKB3,CKB4,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB26,CKB28,CKB29;
registrations ANALOAF,ANALORT,CKB5,CKB25,CKB27;
constructors ANALMETR,ANALORT,CKB4,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17;
begin
reserve AS for Oriented_Orthogonality_Space;
reserve u for (Element of AS);
reserve u1 for (Element of AS);
reserve u2 for (Element of AS);
reserve u3 for (Element of AS);
reserve v for (Element of AS);
reserve v1 for (Element of AS);
reserve v2 for (Element of AS);
reserve v3 for (Element of AS);
reserve w for (Element of AS);
reserve w1 for (Element of AS);
theorem
(AS is  bach_transitive implies (AS is  right_transitive iff (for u holds (for u1 holds (for v holds (for v1 holds (for u2 holds (for v2 holds (((u,u1 '//' u2,v2 & v,v1 '//' u2,v2) & u2 <> v2) implies u,u1 // v,v1)))))))))
proof
A1: ((for u,u1,u2,v,v1,v2,w,w1 being (Element of AS) holds (((u,u1 '//' v,v1 & v,v1 '//' w,w1) & u2,v2 '//' w,w1) implies ((w = w1 or v = v1) or u,u1 '//' u2,v2))) implies (for u holds (for u1 holds (for v holds (for v1 holds (for u2 holds (for v2 holds (((u,u1 '//' u2,v2 & v,v1 '//' u2,v2) & u2 <> v2) implies u,u1 // v,v1))))))))
proof
set w = the (Element of AS);
assume A2: (for u,u1,u2,v,v1,v2,w,w1 being (Element of AS) holds (((u,u1 '//' v,v1 & v,v1 '//' w,w1) & u2,v2 '//' w,w1) implies ((w = w1 or v = v1) or u,u1 '//' u2,v2)));
let u;
let u1;
let v;
let v1;
let u2;
let v2;
assume that
A3: u,u1 '//' u2,v2
and
A4: v,v1 '//' u2,v2
and
A5: u2 <> v2;
consider w1 such that A6: w <> w1 and A7: w,w1 '//' u,u1 by CKB4:def 1;
A8:now
set w = the (Element of AS);
assume A9: u = u1;
consider w1 such that A10: w <> w1 and A11: w,w1 '//' v,v1 by CKB4:def 1;
w,w1 '//' u,u by CKB4:def 1;
hence thesis by A9,A10,A11,CKB12:def 1;
end;
now
assume u <> u1;
then w,w1 '//' v,v1 by A2,A3,A4,A5,A7;
hence thesis by A6,A7,CKB12:def 1;
end;
hence thesis by A8;
end;
assume A12: AS is  bach_transitive;
((for u holds (for u1 holds (for v holds (for v1 holds (for u2 holds (for v2 holds (((u,u1 '//' u2,v2 & v,v1 '//' u2,v2) & u2 <> v2) implies u,u1 // v,v1))))))) implies (for u,u1,u2,v,v1,v2,w,w1 being (Element of AS) holds (((u,u1 '//' v,v1 & v,v1 '//' w,w1) & u2,v2 '//' w,w1) implies ((w = w1 or v = v1) or u,u1 '//' u2,v2))))
proof
assume A13: (for u holds (for u1 holds (for v holds (for v1 holds (for u2 holds (for v2 holds (((u,u1 '//' u2,v2 & v,v1 '//' u2,v2) & u2 <> v2) implies u,u1 // v,v1)))))));
let u;
let u1;
let u2;
let v;
let v1;
let v2;
let w;
let w1;
assume that
A14: u,u1 '//' v,v1
and
A15: v,v1 '//' w,w1
and
A16: u2,v2 '//' w,w1;
now
assume that
A17: w <> w1
and
v <> v1;
v,v1 // u2,v2 by A13,A15,A16,A17;
then (ex u3 st (ex v3 st ((u3 <> v3 & u3,v3 '//' v,v1) & u3,v3 '//' u2,v2))) by CKB12:def 1;
hence thesis by A12,A14,CKB13:def 1;
end;
hence thesis;
end;
hence thesis by A1,CKB14:def 1;
end;
