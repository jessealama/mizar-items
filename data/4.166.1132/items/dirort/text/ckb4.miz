begin
definition
let IT being non empty AffinStruct;
attr IT is  Oriented_Orthogonality_Space-like
means
:Def1: (((for u,u1,v,v1,w being (Element of IT) holds ((((((u,u '//' v,w & u,v '//' w,w) & ((u,v '//' u1,v1 & u,v '//' v1,u1) implies (u = v or u1 = v1))) & ((u,v '//' u1,v1 & u,v '//' u1,w) implies (u,v '//' v1,w or u,v '//' w,v1))) & (u,v '//' u1,v1 implies v,u '//' v1,u1)) & ((u,v '//' u1,v1 & u,v '//' v1,w) implies u,v '//' u1,w)) & (u,u1 '//' v,v1 implies (v,v1 '//' u,u1 or v,v1 '//' u1,u)))) & (for u,v,w being (Element of IT) holds (ex u1 being (Element of IT) st (w <> u1 & w,u1 '//' u,v)))) & (for u,v,w being (Element of IT) holds (ex u1 being (Element of IT) st (w <> u1 & u,v '//' w,u1))));
end;
