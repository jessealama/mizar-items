environ
vocabularies RLVECT_1,XBOOLE_0,ANALOAF,SUBSET_1,ANALMETR,ANALORT,STRUCT_0,SYMSP_1,DIRORT;
notations STRUCT_0,RLVECT_1,ANALOAF,ANALMETR,ANALORT,CKB1,CKB4,CKB6;
definitions CKB4;
theorems ANALMETR,ANALORT,CKB2,CKB3,CKB4,CKB7,CKB8,CKB9;
registrations ANALOAF,ANALORT,CKB5;
constructors ANALMETR,ANALORT,CKB4;
begin
reserve AS for Oriented_Orthogonality_Space;
reserve u for (Element of AS);
reserve u1 for (Element of AS);
reserve v for (Element of AS);
reserve w for (Element of AS);
theorem
(for u,v,w being (Element of AS) holds (ex u1 being (Element of AS) st (u <> u1 & (v,w '//' u,u1 or v,w '//' u1,u))))
proof
let u;
let v;
let w;
consider u1 such that A1: u <> u1 and A2: u,u1 '//' v,w by CKB4:def 1;
(v,w '//' u,u1 or v,w '//' u1,u) by A2,CKB4:def 1;
hence thesis by A1;
end;
