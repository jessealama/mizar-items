begin
theorem
(for P being  mutually-disjoint  with_non-empty_elements non  empty set holds (for f being (Choice_Function of P) holds f is  one-to-one))
proof
let P being  mutually-disjoint  with_non-empty_elements non  empty set;
let f being (Choice_Function of P);
let x1 being set;
let x2 being set;
assume that
A1: x1 in ( dom f )
and
A2: x2 in ( dom f )
and
A3: ( f . x1 ) = ( f . x2 );
A4: (not ( {} ) in P);
then A5: ( f . x1 ) in x1 by A1,ORDERS_1:def 1;
( f . x1 ) in x2 by A2,A3,A4,ORDERS_1:def 1;
then x1 meets x2 by A5,XBOOLE_0:3;
hence thesis by A1,A2,TAXONOM2:def 5;
end;
