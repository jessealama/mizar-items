theorem
Th69: (for A being TopSpace holds (for B being non empty TopSpace holds (for f being (Function of A,B) holds (for C being TopSpace holds (for X being (Subset of A) holds ((f is  continuous & C is (SubSpace of B)) implies (for h being (Function of ( A | X ),C) holds (h = ( f | X ) implies h is  continuous))))))))
proof
let A being TopSpace;
let B being non empty TopSpace;
let f being (Function of A,B);
let C being TopSpace;
let X being (Subset of A);
assume that
A1: f is  continuous
and
A2: C is (SubSpace of B);
(the carrier of ( A | X )) = X by PRE_TOPC:8;
then reconsider g = ( f | X ) as (Function of ( A | X ),B) by FUNCT_2:32;
let h being (Function of ( A | X ),C);
assume A3: h = ( f | X );
g is  continuous by A1,TOPMETR:7;
hence thesis by A2,A3,PRE_TOPC:27;
end;
