begin
reserve I for (Element of ( Segm 9 ));
reserve J for (Element of ( Segm 9 ));
reserve K for (Element of ( Segm 9 ));
reserve a for (Element of ( NAT ));
reserve a1 for (Element of ( NAT ));
reserve b for (Element of ( Data-Locations ( SCM ) ));
reserve b1 for (Element of ( Data-Locations ( SCM ) ));
reserve c for (Element of ( Data-Locations ( SCM ) ));
theorem
(for ins being (Instruction of ( SCM )) holds (( InsCode ins ) = ( 0 ) implies ins = ( halt ( SCM ) )))
proof
let ins being (Instruction of ( SCM ));
assume that
A1: ( InsCode ins ) = ( 0 );
A2:now
assume ins in { [ J,<* a *>,( {} ) ]: J = 6 };
then (ex J st (ex a st (ins = [ J,<* a *>,( {} ) ] & J = 6)));
hence contradiction by A1,RECDEF_2:def 1;
end;
now
assume ins in { [ I,( {} ),<* b,c *> ]: I in { 1,2,3,4,5 } };
then (ex I st (ex b st (ex c st (ins = [ I,( {} ),<* b,c *> ] & I in { 1,2,3,4,5 }))));
hence contradiction by A1,RECDEF_2:def 1;
end;
then A3: ins in ( ( { [ ( SCM-Halt ),( {} ),( {} ) ] } \/ { [ J,<* a *>,( {} ) ]: J = 6 } ) \/ { [ K,<* a1 *>,<* b1 *> ]: K in { 7,8 } } ) by AMI_3:27,XBOOLE_0:def 3;
now
assume ins in { [ K,<* a1 *>,<* b1 *> ]: K in { 7,8 } };
then (ex K st (ex a1 st (ex b1 st (ins = [ K,<* a1 *>,<* b1 *> ] & K in { 7,8 }))));
hence contradiction by A1,RECDEF_2:def 1;
end;
then ins in ( { [ ( SCM-Halt ),( {} ),( {} ) ] } \/ { [ J,<* a *>,( {} ) ]: J = 6 } ) by A3,XBOOLE_0:def 3;
then ins in { [ ( SCM-Halt ),( {} ),( {} ) ] } by A2,XBOOLE_0:def 3;
hence thesis by AMI_3:26,TARSKI:def 1;
end;
