begin
reserve n for Nat;
theorem
(for K being  add-associative  right_zeroed  right_complementable  Abelian  associative  well-unital  distributive non empty doubleLoopStr holds (for V1 being  Abelian  add-associative  right_zeroed  right_complementable  vector-distributive  scalar-distributive  scalar-associative  scalar-unital non empty VectSpStr over K holds (for W being (Subspace of V1) holds (for f being (Function of V1,V1) holds (for fW being (Function of W,W) holds (fW = ( f | W ) implies ( ( f |^ n ) | W ) = ( fW |^ n )))))))
proof
let K being  add-associative  right_zeroed  right_complementable  Abelian  associative  well-unital  distributive non empty doubleLoopStr;
let V1 being  Abelian  add-associative  right_zeroed  right_complementable  vector-distributive  scalar-distributive  scalar-associative  scalar-unital non empty VectSpStr over K;
let W being (Subspace of V1);
let f being (Function of V1,V1);
let fW being (Function of W,W);
assume that
A1: fW = ( f | W );
defpred P[ Nat ]
 means
( ( f |^ $1 ) | W ) = ( fW |^ $1 );
A2: (for n holds (P[ n ] implies P[ ( n + 1 ) ]))
proof
let n;
assume that
A3: P[ n ];
A4: ( rng ( fW |^ n ) ) c= ( [#] W ) by RELAT_1:def 19;
thus ( ( f |^ ( n + 1 ) ) | W ) = ( ( ( f |^ 1 ) * ( f |^ n ) ) | W ) by CKB28:1
.= ( ( f |^ 1 ) * ( ( f |^ n ) | W ) ) by RELAT_1:83
.= ( ( f |^ 1 ) * ( ( id W ) * ( fW |^ n ) ) ) by A3,A4,RELAT_1:53
.= ( ( ( f |^ 1 ) * ( id W ) ) * ( fW |^ n ) ) by RELAT_1:36
.= ( ( f * ( id W ) ) * ( fW |^ n ) ) by CKB27:1
.= ( fW * ( fW |^ n ) ) by A1,RELAT_1:65
.= ( ( fW |^ 1 ) * ( fW |^ n ) ) by CKB27:1
.= ( fW |^ ( n + 1 ) ) by CKB28:1;
end;
( [#] W ) c= ( [#] V1 ) by VECTSP_4:def 2;
then A5: ( [#] W ) = ( ( [#] V1 ) /\ ( [#] W ) ) by XBOOLE_1:28;
( ( f |^ ( 0 ) ) | W ) = ( ( id V1 ) | W ) by CKB26:1
.= ( ( id V1 ) * ( id W ) ) by RELAT_1:65
.= ( id W ) by A5,FUNCT_1:22
.= ( fW |^ ( 0 ) ) by CKB26:1;
then A6: P[ ( 0 ) ];
(for n holds P[ n ]) from NAT_1:sch 2(A6,A2);
hence thesis;
end;
