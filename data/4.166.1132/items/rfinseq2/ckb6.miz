begin
theorem
Th2: (for f being (FinSequence of ( REAL )) holds (for i being (Element of ( NAT )) holds ((1 <= i & i <= ( len f )) implies (( f . i ) >= ( f . ( min_p f ) ) & ( f . i ) >= ( min f )))))
proof
let f being (FinSequence of ( REAL ));
let i being (Element of ( NAT ));
assume A1: (1 <= i & i <= ( len f ));
then A2: i in ( dom f ) by FINSEQ_3:25;
hence ( f . i ) >= ( f . ( min_p f ) ) by A1,CKB2:def 1;
thus thesis by A1,A2,CKB2:def 1;
end;
