begin
reserve n for (Element of ( NAT ));
theorem
Th16: (for R being (FinSequence of ( REAL )) holds ((( len R ) = ( 0 ) or ( len R ) = 1) implies R is  non-decreasing))
proof
let R being (FinSequence of ( REAL ));
assume A1: (( len R ) = ( 0 ) or ( len R ) = 1);
per cases  by A1;
suppose ( len R ) = ( 0 );

then R = ( <*> ( REAL ) );
then (for n holds ((n in ( dom R ) & ( n + 1 ) in ( dom R )) implies ( R . n ) <= ( R . ( n + 1 ) )));
hence thesis by INTEGRA2:def 1;
end;
suppose ( len R ) = 1;

then A2: ( dom R ) = { 1 } by FINSEQ_1:2,FINSEQ_1:def 3;
now
let n;
assume that
A3: n in ( dom R )
and
A4: ( n + 1 ) in ( dom R );
n = 1 by A2,A3,TARSKI:def 1;
hence ( R . n ) <= ( R . ( n + 1 ) ) by A2,A4,TARSKI:def 1;
end;
hence thesis by INTEGRA2:def 1;
end;
end;
