theorems TARSKI,FINSEQ_1,FINSEQ_2,FINSEQ_3,NAT_1,FUNCT_1,FUNCT_2,RVSUM_1,RELAT_1,FUNCOP_1,RFINSEQ,INTEGRA2,XREAL_1,XXREAL_0,VALUED_1,CLASSES1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23;
schemes NAT_1;
registrations XBOOLE_0,FUNCT_1,RELSET_1,FUNCT_2,NUMBERS,XXREAL_0,NAT_1,MEMBERED,FINSEQ_1,RFINSEQ,INTEGRA2,VALUED_0,FINSET_1,CARD_1;
constructors FUNCOP_1,REAL_1,NAT_1,BINOP_2,SEQM_3,RFINSEQ,INTEGRA2,SEQ_1,RVSUM_1,CLASSES1,RELSET_1,CKB1,CKB2,CKB3,CKB4,CKB22;
requirements REAL,NUMERALS,BOOLE,SUBSET,ARITHM;
begin
reserve n for (Element of ( NAT ));
reserve m for (Element of ( NAT ));
theorem
Th17: (for R being (FinSequence of ( REAL )) holds (R is  non-decreasing iff (for n,m being (Element of ( NAT )) holds (((n in ( dom R ) & m in ( dom R )) & n < m) implies ( R . n ) <= ( R . m )))))
proof
let R being (FinSequence of ( REAL ));
thus (R is  non-decreasing implies (for n holds (for m holds (((n in ( dom R ) & m in ( dom R )) & n < m) implies ( R . n ) <= ( R . m )))))
proof
defpred P[ (Element of ( NAT )) ]
 means
($1 in ( dom R ) implies (for n holds ((n in ( dom R ) & n < $1) implies ( R . n ) <= ( R . $1 ))));
assume A1: R is  non-decreasing;
A2: (for m holds (P[ m ] implies P[ ( m + 1 ) ]))
proof
let m;
assume A3: P[ m ];
assume A4: ( m + 1 ) in ( dom R );
then ( m + 1 ) <= ( len R ) by FINSEQ_3:25;
then A5: m <= ( len R ) by NAT_1:13;
let n;
assume that
A6: n in ( dom R )
and
A7: n < ( m + 1 );
set t = ( R . m );
set r = ( R . n );
set s = ( R . ( m + 1 ) );
A8: n <= m by A7,NAT_1:13;
1 <= n by A6,FINSEQ_3:25;
then A9: 1 <= m by A8,XXREAL_0:2;
then A10: m in ( dom R ) by A5,FINSEQ_3:25;
now
per cases ;
case n = m;
hence thesis by A1,A4,A6,INTEGRA2:def 1;
end;
case n <> m;
then n < m by A8,XXREAL_0:1;
then A11: r <= t by A3,A6,A9,A5,FINSEQ_3:25;
t <= s by A1,A4,A10,INTEGRA2:def 1;
hence thesis by A11,XXREAL_0:2;
end;
end;
hence thesis;
end;
A12: P[ ( 0 ) ];
(for m holds P[ m ]) from NAT_1:sch 1(A12,A2);
hence thesis;
end;

assume A13: (for n holds (for m holds (((n in ( dom R ) & m in ( dom R )) & n < m) implies ( R . n ) <= ( R . m ))));
let n;
A14: n < ( n + 1 ) by NAT_1:13;
assume (n in ( dom R ) & ( n + 1 ) in ( dom R ));
hence thesis by A13,A14;
end;
