constructors FUNCOP_1,REAL_1,NAT_1,BINOP_2,SEQM_3,RFINSEQ,INTEGRA2,SEQ_1,RVSUM_1,CLASSES1,RELSET_1,CKB1,CKB2,CKB3,CKB4;
