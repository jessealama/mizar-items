theorem
(for f being (FinSequence of ( REAL )) holds (( len f ) >= 1 implies (((( max_p ( sort_d f ) ) = 1 & ( min_p ( sort_a f ) ) = 1) & ( ( sort_d f ) . 1 ) = ( max f )) & ( ( sort_a f ) . 1 ) = ( min f ))))
proof
let f being (FinSequence of ( REAL ));
assume A1: ( len f ) >= 1;
A2: ( len ( sort_d f ) ) = ( len f ) by CKB40:1;
then 1 in ( Seg ( len ( sort_d f ) ) ) by A1,FINSEQ_1:1;
then A3: 1 in ( dom ( sort_d f ) ) by FINSEQ_1:def 3;
A4: (for i being (Element of ( NAT )) holds (for r1,r2 being Real holds (((i in ( dom ( sort_d f ) ) & r1 = ( ( sort_d f ) . i )) & r2 = ( ( sort_d f ) . 1 )) implies r1 <= r2)))
proof
let i being (Element of ( NAT ));
let r1 being Real;
let r2 being Real;
assume that
A5: i in ( dom ( sort_d f ) )
and
A6: (r1 = ( ( sort_d f ) . i ) & r2 = ( ( sort_d f ) . 1 ));
i in ( Seg ( len ( sort_d f ) ) ) by A5,FINSEQ_1:def 3;
then A7: 1 <= i by FINSEQ_1:1;
now
per cases ;
case 1 = i;
hence thesis by A6;
end;
case 1 <> i;
then 1 < i by A7,XXREAL_0:1;
hence thesis by A3,A5,A6,RFINSEQ:19;
end;
end;
hence thesis;
end;
A8: ( len ( sort_a f ) ) = ( len f ) by CKB41:1;
then A9: 1 in ( dom ( sort_a f ) ) by A1,FINSEQ_3:25;
A10: (for i being (Element of ( NAT )) holds (for r1,r2 being Real holds (((i in ( dom ( sort_a f ) ) & r1 = ( ( sort_a f ) . i )) & r2 = ( ( sort_a f ) . 1 )) implies r1 >= r2)))
proof
let i being (Element of ( NAT ));
let r1 being Real;
let r2 being Real;
assume that
A11: i in ( dom ( sort_a f ) )
and
A12: (r1 = ( ( sort_a f ) . i ) & r2 = ( ( sort_a f ) . 1 ));
A13: 1 <= i by A11,FINSEQ_3:25;
per cases ;
suppose 1 = i;

hence thesis by A12;
end;
suppose 1 <> i;

then 1 < i by A13,XXREAL_0:1;
hence thesis by A9,A11,A12,CKB24:1;
end;
end;
A14: f,( sort_a f ) are_fiberwise_equipotent  by CKB29:def 1;
A15: f,( sort_d f ) are_fiberwise_equipotent  by CKB22:def 1;
A16: (for j being (Element of ( NAT )) holds ((j in ( dom ( sort_a f ) ) & ( ( sort_a f ) . j ) = ( ( sort_a f ) . 1 )) implies 1 <= j)) by FINSEQ_3:25;
