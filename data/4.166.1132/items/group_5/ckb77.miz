theorem
Th77: (for N1,N2 being  strict  normal (Subgroup of G) holds [. N1,N2 .] is  normal  normal (Subgroup of G))
proof
let N1 being  strict  normal (Subgroup of G),N2 being  strict  normal (Subgroup of G);
now
let a;
now
let b;
assume b in ( [. N1,N2 .] |^ a );
