then A24: ( [. c,b .] |^ d ) in [. N1,N3 .] by A1,GROUP_3:def 13;
(x = ( ( [. c,b .] |^ d ) * [. d,b .] ) & [. d,b .] in [. N2,N3 .]) by A18,A20,A21,A23,CKB27:1,CKB74:1;
