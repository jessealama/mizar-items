environ
vocabularies XBOOLE_0,SUBSET_1,TARSKI,NUMBERS,INT_1,GROUP_1,GROUP_2,PRE_TOPC,FINSEQ_1,STRUCT_0,NEWTON,RELAT_1,EQREL_1,BINOP_1,NAT_1,FUNCT_1,PARTFUN1,ORDINAL4,ARYTM_3,CARD_1,CARD_3,QC_LANG1,XXREAL_1,ARYTM_1,GROUP_4,RLSUB_1,FINSET_1,CQC_SIM1,GROUP_3,GROUP_5;
notations TARSKI,XBOOLE_0,SUBSET_1,NUMBERS,XCMPLX_0,NAT_1,FUNCT_1,PARTFUN1,CARD_1,FINSEQ_1,FINSET_1,FINSEQ_4,INT_1,DOMAIN_1,STRUCT_0,ALGSTR_0,GROUP_1,GROUP_2,GROUP_3,GROUP_4,CKB9;
definitions TARSKI,GROUP_1,XBOOLE_0,GROUP_2,GROUP_3,GROUP_4,CKB9;
theorems FINSEQ_1,FINSEQ_2,FINSEQ_3,FINSEQ_4,FUNCT_1,GROUP_1,GROUP_2,GROUP_3,GROUP_4,TARSKI,ZFMISC_1,XBOOLE_0,XBOOLE_1,CARD_2,FINSOP_1,STRUCT_0,PARTFUN1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9;
schemes FINSEQ_2,DOMAIN_1;
registrations XBOOLE_0,SUBSET_1,RELSET_1,NUMBERS,XREAL_0,INT_1,STRUCT_0,GROUP_1,GROUP_2,GROUP_3,ORDINAL1,FINSEQ_1,CARD_1;
constructors PARTFUN1,SETFAM_1,NAT_1,FINSEQ_4,FINSOP_1,REALSET1,GROUP_4,RELSET_1,BINOP_2,CKB9;
requirements NUMERALS,BOOLE,SUBSET;
begin
reserve k for (Element of ( NAT ));
reserve G for Group;
reserve a for (Element of G);
reserve F1 for (FinSequence of (the carrier of G));
reserve F2 for (FinSequence of (the carrier of G));
theorem
Th12: ( ( F1 |^ a ) ^ ( F2 |^ a ) ) = ( ( F1 ^ F2 ) |^ a )
proof
A1:now
let k;
assume k in ( dom ( F1 |^ a ) );
then k in ( Seg ( len ( F1 |^ a ) ) ) by FINSEQ_1:def 3;
then k in ( Seg ( len F1 ) ) by CKB9:def 1;
then A2: k in ( dom F1 ) by FINSEQ_1:def 3;
then A3: (( F1 /. k ) = ( ( F1 ^ F2 ) /. k ) & k in ( dom ( F1 ^ F2 ) )) by FINSEQ_3:22,FINSEQ_4:68;
thus ( ( F1 |^ a ) . k ) = ( ( F1 /. k ) |^ a ) by A2,CKB9:def 1
.= ( ( ( F1 ^ F2 ) |^ a ) . k ) by A3,CKB9:def 1;
end;
A4:now
let k;
assume A5: k in ( dom ( F2 |^ a ) );
( len F2 ) = ( len ( F2 |^ a ) ) by CKB9:def 1;
then A6: k in ( dom F2 ) by A5,FINSEQ_3:29;
then ( ( len F1 ) + k ) in ( dom ( F1 ^ F2 ) ) by FINSEQ_1:28;
then ( ( len ( F1 |^ a ) ) + k ) in ( dom ( F1 ^ F2 ) ) by CKB9:def 1;
hence ( ( ( F1 ^ F2 ) |^ a ) . ( ( len ( F1 |^ a ) ) + k ) ) = ( ( ( F1 ^ F2 ) /. ( ( len ( F1 |^ a ) ) + k ) ) |^ a ) by CKB9:def 1
.= ( ( ( F1 ^ F2 ) /. ( ( len F1 ) + k ) ) |^ a ) by CKB9:def 1
.= ( ( F2 /. k ) |^ a ) by A6,FINSEQ_4:69
.= ( ( F2 |^ a ) . k ) by A6,CKB9:def 1;
end;
( ( len ( F1 |^ a ) ) + ( len ( F2 |^ a ) ) ) = ( ( len F1 ) + ( len ( F2 |^ a ) ) ) by CKB9:def 1
.= ( ( len F1 ) + ( len F2 ) ) by CKB9:def 1
.= ( len ( F1 ^ F2 ) ) by FINSEQ_1:22
.= ( len ( ( F1 ^ F2 ) |^ a ) ) by CKB9:def 1;
hence thesis by A1,A4,FINSEQ_3:38;
end;
