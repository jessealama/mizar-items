begin
reserve G for Group;
reserve a for (Element of G);
reserve b for (Element of G);
theorem
Th3: (for N being  strict  normal (Subgroup of G) holds (a in N implies ( a |^ b ) in N))
proof
let N being  strict  normal (Subgroup of G);
assume a in N;
then ( a |^ b ) in ( N |^ b ) by GROUP_3:58;
hence thesis by GROUP_3:def 13;
end;
