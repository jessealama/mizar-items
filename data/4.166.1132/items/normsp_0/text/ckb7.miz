environ
vocabularies SUBSET_1,STRUCT_0,NORMSP_1,FUNCT_1,NUMBERS,REAL_1,XBOOLE_0,FUNCT_5,FUNCOP_1,CARD_1,METRIC_1,RELAT_2,SUPINF_2,XCMPLX_0,NAT_1,SEQ_1,RELAT_1,TARSKI,PARTFUN1,NORMSP_0;
notations TARSKI,XBOOLE_0,SUBSET_1,RELAT_1,FUNCT_1,PARTFUN1,FUNCT_2,FUNCOP_1,FUNCT_5,NUMBERS,XCMPLX_0,REAL_1,NAT_1,SEQ_1,STRUCT_0,CKB1,CKB4,CKB5;
definitions TARSKI,FUNCT_5,FUNCOP_1,RELAT_1,FUNCT_1,FUNCT_2,STRUCT_0,CKB4,CKB5;
theorems CARD_1,FUNCOP_1,TARSKI,FUNCT_1,RELAT_1,RELSET_1,PARTFUN1,FUNCT_2,CKB2,CKB4,CKB5;
schemes FUNCT_1;
registrations STRUCT_0,RELSET_1,XBOOLE_0,REAL_1,NUMBERS,FUNCOP_1,XREAL_0,NAT_1,ORDINAL1,XCMPLX_0,FUNCT_1,FUNCT_2,CKB1,CKB3,CKB6;
constructors FUNCT_2,NUMBERS,STRUCT_0,FUNCT_5,FUNCOP_1,XCMPLX_0,RELSET_1,CKB1,CKB4,CKB5;
requirements BOOLE,SUBSET;
begin
reconsider f = ( op1 ) as (Function of 1,( REAL )) by CKB2:1,FUNCOP_1:46;
reserve X for non  empty N-Str;
definition
let C being non  empty set;
let X;
let f being (PartFunc of C,(the carrier of X));
redefine func ||.f .|| -> (PartFunc of C,( REAL )) means 
(( dom it ) = ( dom f ) & (for c being (Element of C) holds (c in ( dom it ) implies ( it . c ) = ||. ( f /. c ) .||)));
coherence
proof
( dom ||. f .|| ) = ( dom f ) by CKB5:def 1;
then A1: ( dom ||. f .|| ) c= C by RELAT_1:def 18;
( rng ||. f .|| ) c= ( REAL )
proof
let e being set;
assume e in ( rng ||. f .|| );
then consider u being set such that A2: u in ( dom ||. f .|| ) and A3: e = ( ||. f .|| . u ) by FUNCT_1:def 3;
e = ||. ( f /. u ) .|| by A2,A3,CKB5:def 1;
hence e in ( REAL );
end;
hence ||. f .|| is (PartFunc of C,( REAL )) by A1,RELSET_1:4;
end;
compatibility
proof
let F being (PartFunc of C,( REAL ));
thus (F = ||. f .|| implies (( dom F ) = ( dom f ) & (for c being (Element of C) holds (c in ( dom F ) implies ( F . c ) = ||. ( f /. c ) .||)))) by CKB5:def 1;
assume that
A4: ( dom F ) = ( dom f )
and
A5: (for c being (Element of C) holds (c in ( dom F ) implies ( F . c ) = ||. ( f /. c ) .||));
(for e being set holds (e in ( dom F ) implies ( F . e ) = ||. ( f /. e ) .||))
proof
let e being set;
assume A6: e in ( dom F );
( dom F ) c= C by RELAT_1:def 18;
then reconsider c = e as (Element of C) by A6;
thus ( F . e ) = ||. ( f /. c ) .|| by A6,A5
.= ||. ( f /. e ) .||;
end;
hence F = ||. f .|| by A4,CKB5:def 1;
end;
end;
