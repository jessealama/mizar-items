environ
vocabularies SUBSET_1,STRUCT_0,NORMSP_1,FUNCT_1,NUMBERS,REAL_1,XBOOLE_0,FUNCT_5,FUNCOP_1,CARD_1,METRIC_1,RELAT_2,SUPINF_2,XCMPLX_0,NAT_1,SEQ_1,RELAT_1,TARSKI,PARTFUN1,NORMSP_0;
notations TARSKI,XBOOLE_0,SUBSET_1,RELAT_1,FUNCT_1,PARTFUN1,FUNCT_2,FUNCOP_1,FUNCT_5,NUMBERS,XCMPLX_0,REAL_1,NAT_1,SEQ_1,STRUCT_0,CKB1,CKB4;
definitions TARSKI,FUNCT_5,FUNCOP_1,RELAT_1,FUNCT_1,FUNCT_2,STRUCT_0,CKB4;
theorems CARD_1,FUNCOP_1,TARSKI,FUNCT_1,RELAT_1,RELSET_1,PARTFUN1,FUNCT_2,CKB2,CKB4;
schemes FUNCT_1;
registrations STRUCT_0,RELSET_1,XBOOLE_0,REAL_1,NUMBERS,FUNCOP_1,XREAL_0,NAT_1,ORDINAL1,XCMPLX_0,FUNCT_1,FUNCT_2,CKB1,CKB3;
constructors FUNCT_2,NUMBERS,STRUCT_0,FUNCT_5,FUNCOP_1,XCMPLX_0,RELSET_1,CKB1,CKB4;
requirements BOOLE,SUBSET;
begin
reconsider f = ( op1 ) as (Function of 1,( REAL )) by CKB2:1,FUNCOP_1:46;
reserve X for non  empty N-Str;
definition
let X;
let f being (the carrier of X) -valued Function;
func ||.f .|| -> Function means 
:Def2: (( dom it ) = ( dom f ) & (for e being set holds (e in ( dom it ) implies ( it . e ) = ||. ( f /. e ) .||)));
existence
proof
deffunc F(set) = ||. ( f /. $1 ) .||;
consider g being Function such that A1: ( dom g ) = ( dom f ) and A2: (for x being set holds (x in ( dom f ) implies ( g . x ) = F(x))) from FUNCT_1:sch 3;
take g;
thus thesis by A1,A2;
end;
uniqueness
proof
let f1 being Function;
let f2 being Function;
assume that
A3: ( dom f1 ) = ( dom f )
and
A4: (for e being set holds (e in ( dom f1 ) implies ( f1 . e ) = ||. ( f /. e ) .||))
and
A5: ( dom f2 ) = ( dom f )
and
A6: (for e being set holds (e in ( dom f2 ) implies ( f2 . e ) = ||. ( f /. e ) .||));
thus ( dom f1 ) = ( dom f2 ) by A3,A5;
let e being set;
assume A7: e in ( dom f1 );
hence ( f1 . e ) = ||. ( f /. e ) .|| by A4
.= ( f2 . e ) by A3,A5,A6,A7;
end;
end;
