registrations XBOOLE_0,FUNCT_1,FUNCT_2,NUMBERS,XCMPLX_0,XXREAL_0,NAT_1,BINOP_2,MEMBERED,FINSEQ_2,COMPLFLD,MATRIX_1,VALUED_0,RELSET_1,CARD_1,FINSEQ_1;
constructors PARTFUN1,SETWISEO,XXREAL_0,NAT_1,FINSOP_1,BINARITH,MATRIX_3,MATRIX_4,MATRIX_5,SEQ_4,SEQ_1,BINOP_2,BINOP_1,RVSUM_1,NAT_D,RELSET_1,COMPLSP2,CKB1;
requirements NUMERALS,BOOLE,SUBSET,REAL,ARITHM;
begin
reserve i for Nat;
reserve j for Nat;
theorem
(for M1,M2 being (Matrix of ( COMPLEX )) holds ((( len M1 ) = ( len M2 ) & ( width M1 ) = ( width M2 )) implies ( ( M1 + M2 ) *' ) = ( ( M1 *' ) + ( M2 *' ) )))
proof
let M1 being (Matrix of ( COMPLEX ));
let M2 being (Matrix of ( COMPLEX ));
assume that
A1: ( len M1 ) = ( len M2 )
and
A2: ( width M1 ) = ( width M2 );
A3: ( len ( M1 + M2 ) ) = ( len M1 ) by CKB7:1;
A4: ( width ( ( M1 + M2 ) *' ) ) = ( width ( M1 + M2 ) ) by CKB1:def 1;
A5: ( width ( M1 + M2 ) ) = ( width M1 ) by CKB7:1;
A6: ( len ( ( M1 + M2 ) *' ) ) = ( len ( M1 + M2 ) ) by CKB1:def 1;
A7:now
let i;
let j;
assume A8: [ i,j ] in ( Indices ( ( M1 + M2 ) *' ) );
then A9: 1 <= j by CKB2:1;
A10: 1 <= i by A8,CKB2:1;
A11: j <= ( width ( M1 + M2 ) ) by A4,A8,CKB2:1;
then A12: j <= ( width ( M1 *' ) ) by A5,CKB1:def 1;
A13: i <= ( len ( M1 + M2 ) ) by A6,A8,CKB2:1;
then i <= ( len ( M1 *' ) ) by A3,CKB1:def 1;
then A14: [ i,j ] in ( Indices ( M1 *' ) ) by A9,A10,A12,CKB2:1;
A15: 1 <= i by A8,CKB2:1;
then A16: [ i,j ] in ( Indices M1 ) by A3,A5,A13,A9,A11,CKB2:1;
A17: [ i,j ] in ( Indices M2 ) by A1,A2,A3,A5,A15,A13,A9,A11,CKB2:1;
[ i,j ] in ( Indices ( M1 + M2 ) ) by A15,A13,A9,A11,CKB2:1;
then ( ( ( M1 + M2 ) *' ) * (i,j) ) = ( ( ( M1 + M2 ) * (i,j) ) *' ) by CKB1:def 1;
hence ( ( ( M1 + M2 ) *' ) * (i,j) ) = ( ( ( M1 * (i,j) ) + ( M2 * (i,j) ) ) *' ) by A16,CKB8:1
.= ( ( ( M1 * (i,j) ) *' ) + ( ( M2 * (i,j) ) *' ) ) by COMPLEX1:32
.= ( ( ( M1 *' ) * (i,j) ) + ( ( M2 * (i,j) ) *' ) ) by A16,CKB1:def 1
.= ( ( ( M1 *' ) * (i,j) ) + ( ( M2 *' ) * (i,j) ) ) by A17,CKB1:def 1
.= ( ( ( M1 *' ) + ( M2 *' ) ) * (i,j) ) by A14,CKB8:1;
end;
A18: ( width ( M1 *' ) ) = ( width M1 ) by CKB1:def 1;
A19: ( width ( ( M1 *' ) + ( M2 *' ) ) ) = ( width ( M1 *' ) ) by CKB7:1;
A20: ( len ( ( M1 *' ) + ( M2 *' ) ) ) = ( len ( M1 *' ) ) by CKB7:1;
( len ( M1 *' ) ) = ( len M1 ) by CKB1:def 1;
hence thesis by A6,A3,A20,A4,A5,A19,A18,A7,MATRIX_1:21;
end;
