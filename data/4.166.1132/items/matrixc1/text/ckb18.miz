begin
reserve j for Nat;
reserve k for Nat;
definition
let x being (FinSequence of ( COMPLEX ));
assume A1: ( len x ) > ( 0 );
func FinSeq2Matrix x -> (Matrix of ( COMPLEX ))
means
((( len it ) = ( len x ) & ( width it ) = 1) & (for j holds (j in ( Seg ( len x ) ) implies ( it . j ) = <* ( x . j ) *>)));
existence
proof
reconsider n = ( len x ) as (Element of ( NAT ));
deffunc F(Nat) = <* ( x . $1 ) *>;
consider M3 being FinSequence such that A2: (( len M3 ) = n & (for j being Nat holds (j in ( dom M3 ) implies ( M3 . j ) = F(j)))) from FINSEQ_1:sch 2;
(for y being set holds (y in ( rng M3 ) implies (ex p being (FinSequence of ( COMPLEX )) st (y = p & ( len p ) = 1))))
proof
let y being set;
assume y in ( rng M3 );
then consider z being set such that A3: z in ( dom M3 ) and A4: y = ( M3 . z ) by FUNCT_1:def 3;
reconsider z as (Element of ( NAT )) by A3;
( len <* ( x . z ) *> ) = 1 by FINSEQ_1:39;
hence thesis by A2,A3,A4;
end;
then reconsider M2 = M3 as (Matrix of ( COMPLEX )) by MATRIX_1:9;
(for p being (FinSequence of ( COMPLEX )) holds (p in ( rng M2 ) implies ( len p ) = 1))
proof
let p being (FinSequence of ( COMPLEX ));
assume p in ( rng M2 );
then consider i being Nat such that A5: i in ( dom M2 ) and A6: ( M2 . i ) = p by FINSEQ_2:10;
( len <* ( x . i ) *> ) = 1 by FINSEQ_1:39;
hence thesis by A2,A5,A6;
end;
then reconsider M1 = M2 as (Matrix of ( len M2 ),1,( COMPLEX )) by MATRIX_1:def 2;
take M2;
A7: ( dom M3 ) = ( Seg n ) by A2,FINSEQ_1:def 3;
( width M1 ) = 1 by A1,A2,MATRIX_1:23;
hence thesis by A2,A7;
end;
uniqueness
proof
let M1 being (Matrix of ( COMPLEX )),M2 being (Matrix of ( COMPLEX ));
assume that
A8: ( len M1 ) = ( len x )
and
( width M1 ) = 1
and
A9: (for k holds (k in ( Seg ( len x ) ) implies ( M1 . k ) = <* ( x . k ) *>))
and
A10: ( len M2 ) = ( len x )
and
( width M2 ) = 1
and
A11: (for k holds (k in ( Seg ( len x ) ) implies ( M2 . k ) = <* ( x . k ) *>));
A12: ( dom M1 ) = ( Seg ( len x ) ) by A8,FINSEQ_1:def 3;
now
let k being Nat;
assume A13: k in ( dom M1 );
hence ( M1 . k ) = <* ( x . k ) *> by A9,A12
.= ( M2 . k ) by A11,A12,A13;
end;
hence thesis by A8,A10,FINSEQ_2:9;
end;
end;
