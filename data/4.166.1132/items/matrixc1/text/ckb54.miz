begin
reserve i for Nat;
theorem
Th45: (for F being (FinSequence of ( COMPLEX )) holds (for M being (Matrix of ( COMPLEX )) holds ((( len F ) = ( width M ) & ( len F ) >= 1) implies ( ( M * F ) *' ) = ( ( M *' ) * ( F *' ) ))))
proof
let F being (FinSequence of ( COMPLEX ));
let M being (Matrix of ( COMPLEX ));
assume that
A1: ( len F ) = ( width M )
and
A2: ( len F ) >= 1;
A3: ( len ( F *' ) ) = ( len F ) by COMPLSP2:def 1;
A4: ( width ( M *' ) ) = ( width M ) by CKB1:def 1;
A5: ( len ( ( M * F ) *' ) ) = ( len ( M * F ) ) by COMPLSP2:def 1
.= ( len M ) by CKB21:def 1;
A6: ( len ( M *' ) ) = ( len M ) by CKB1:def 1;
A7:now
let i being Nat;
assume that
A8: 1 <= i
and
A9: i <= ( len ( ( M * F ) *' ) );
A10: i in ( Seg ( len M ) ) by A5,A8,A9,FINSEQ_1:1;
( len ( Line (( M *' ),i) ) ) = ( len ( F *' ) ) by A1,A3,A4,MATRIX_1:def 7;
then A11: ( len ( mlt (( Line (( M *' ),i) ),( F *' )) ) ) >= 1 by A2,A3,FINSEQ_2:72;
A12: i in ( Seg ( len ( M *' ) ) ) by A5,A6,A8,A9,FINSEQ_1:1;
i <= ( len ( M * F ) ) by A9,COMPLSP2:def 1;
hence ( ( ( M * F ) *' ) . i ) = ( ( ( M * F ) . i ) *' ) by A8,COMPLSP2:def 1
.= ( ( Sum ( mlt (F,( Line (M,i) )) ) ) *' ) by A10,CKB21:def 1
.= ( ( Sum ( mlt (F,( ( Line (( M *' ),i) ) *' )) ) ) *' ) by A10,CKB52:1
.= ( ( Sum ( ( mlt (( Line (( M *' ),i) ),( F *' )) ) *' ) ) *' ) by A1,CKB53:1
.= ( ( ( Sum ( mlt (( Line (( M *' ),i) ),( F *' )) ) ) *' ) *' ) by A11,CKB32:1
.= ( ( ( M *' ) * ( F *' ) ) . i ) by A12,CKB21:def 1;
end;
( len ( ( M *' ) * ( F *' ) ) ) = ( len ( M *' ) ) by CKB21:def 1
.= ( len M ) by CKB1:def 1;
hence thesis by A5,A7,FINSEQ_1:14;
end;
