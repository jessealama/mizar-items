begin
reserve i for Nat;
definition
let M being (Matrix of ( COMPLEX ));
let F being (FinSequence of ( COMPLEX ));
func M * F -> (FinSequence of ( COMPLEX )) means 
:Def7: (( len it ) = ( len M ) & (for i holds (i in ( Seg ( len M ) ) implies ( it . i ) = ( Sum ( mlt (( Line (M,i) ),F) ) ))));
existence
proof
deffunc V(Nat) = ( Sum ( mlt (( Line (M,$1) ),F) ) );
consider z being (FinSequence of ( COMPLEX )) such that A1: (( len z ) = ( len M ) & (for i being Nat holds (i in ( dom z ) implies ( z . i ) = V(i)))) from FINSEQ_2:sch 1;
take z;
( dom z ) = ( Seg ( len M ) ) by A1,FINSEQ_1:def 3;
hence thesis by A1;
end;
uniqueness
proof
let p1 being (FinSequence of ( COMPLEX ));
let p2 being (FinSequence of ( COMPLEX ));
assume that
A2: ( len p1 ) = ( len M )
and
A3: (for i holds (i in ( Seg ( len M ) ) implies ( p1 . i ) = ( Sum ( mlt (( Line (M,i) ),F) ) )))
and
A4: ( len p2 ) = ( len M )
and
A5: (for i holds (i in ( Seg ( len M ) ) implies ( p2 . i ) = ( Sum ( mlt (( Line (M,i) ),F) ) )));
A6: ( dom p1 ) = ( Seg ( len M ) ) by A2,FINSEQ_1:def 3;
now
let i being Nat;
assume A7: i in ( dom p1 );
then ( p1 . i ) = ( Sum ( mlt (( Line (M,i) ),F) ) ) by A3,A6;
hence ( p1 . i ) = ( p2 . i ) by A5,A6,A7;
end;
hence thesis by A2,A4,FINSEQ_2:9;
end;
end;
