begin
reserve K for Field;
reserve A for (Matrix of K);
reserve B for (Matrix of K);
theorem
(for A,B being (Matrix of K) holds (( width A ) = ( len B ) implies (( the_rank_of ( A * B ) ) <= ( the_rank_of A ) & ( the_rank_of ( A * B ) ) <= ( the_rank_of B ))))
proof
let A being (Matrix of K);
let B being (Matrix of K);
assume that
A1: ( width A ) = ( len B );
set AB = ( A * B );
A2: ( width AB ) = ( width B ) by A1,MATRIX_3:def 4;
per cases ;
suppose ( the_rank_of AB ) = ( 0 );

hence thesis;
end;
suppose A3: ( the_rank_of AB ) > ( 0 );

set AT = ( A @ );
A4: ( width AB ) > ( 0 ) by A3,MATRIX13:74;
then A5: ( width A ) > ( 0 ) by A1,A2,MATRIX_1:def 3;
then A6: ( len AT ) = ( width A ) by MATRIX_2:10;
set BT = ( B @ );
set BA = ( BT * AT );
( width AT ) = ( len A ) by A5,MATRIX_2:10;
then A7: (( width AT ) = ( 0 ) implies ( len AT ) = ( 0 )) by A5,MATRIX_1:def 3;
then A8: ( dim ( Space_of_Solutions_of AT ) ) = ( ( width AT ) - ( the_rank_of AT ) ) by CKB86:1;
A9: ( width BT ) = ( len B ) by A2,A4,MATRIX_2:10;
then (( width BT ) = ( 0 ) implies ( len BT ) = ( 0 )) by A2,A4,MATRIX_1:def 3;
then A10: ( Space_of_Solutions_of AT ) is (Subspace of ( Space_of_Solutions_of BA )) by A1,A6,A9,A7,CKB90:1;
A11: ( width BA ) = ( width AT ) by A1,A6,A9,MATRIX_3:def 4;
then ( dim ( Space_of_Solutions_of BA ) ) = ( ( width BA ) - ( the_rank_of BA ) ) by A5,A7,CKB86:1,MATRIX_2:10;
then ( ( width AT ) - ( the_rank_of AT ) ) <= ( ( width AT ) - ( the_rank_of BA ) ) by A11,A10,A8,VECTSP_9:25;
then ( the_rank_of AT ) >= ( the_rank_of BA ) by XREAL_1:10;
then A12: ( the_rank_of A ) >= ( the_rank_of BA ) by MATRIX13:84;
(( width A ) = ( 0 ) implies ( len A ) = ( 0 )) by A1,A2,A4,MATRIX_1:def 3;
then A13: ( Space_of_Solutions_of B ) is (Subspace of ( Space_of_Solutions_of AB )) by A1,A2,A4,CKB90:1;
(( dim ( Space_of_Solutions_of B ) ) = ( ( width B ) - ( the_rank_of B ) ) & ( dim ( Space_of_Solutions_of AB ) ) = ( ( width AB ) - ( the_rank_of AB ) )) by A2,A4,CKB86:1;
then A14: ( ( width B ) - ( the_rank_of B ) ) <= ( ( width B ) - ( the_rank_of AB ) ) by A2,A13,VECTSP_9:25;
BA = ( AB @ ) by A1,A2,A4,MATRIX_3:22;
hence thesis by A14,A12,MATRIX13:84,XREAL_1:10;
