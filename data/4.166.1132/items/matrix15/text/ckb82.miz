theorem
Th66: (for N holds ((((N c= ( dom A ) & N is non  empty) & ( width A ) > ( 0 )) & (for i holds (i in ( ( dom A ) \ N ) implies ( Line (A,i) ) = ( ( width A ) |-> ( 0. K ) )))) implies ( Space_of_Solutions_of A ) = ( Space_of_Solutions_of ( Segm (A,N,( Seg ( width A ) )) ) )))
proof
let N;
assume that
A1: N c= ( dom A )
and
A2: N is non  empty
and
A3: ( width A ) > ( 0 )
and
A4: (for i holds (i in ( ( dom A ) \ N ) implies ( Line (A,i) ) = ( ( width A ) |-> ( 0. K ) )));
set L = ( ( len A ) |-> ( 0. K ) );
set C = ( ColVec2Mx L );
A5: ( len L ) = ( len A ) by CARD_1:def 7;
set S = ( Segm (A,N,( Seg ( width A ) )) );
A6: ( width S ) = ( card ( Seg ( width A ) ) ) by A2,MATRIX_1:23;
then A7: ( width A ) = ( width S ) by FINSEQ_1:57;
set SS = ( Space_of_Solutions_of S );
( len S ) = ( card N ) by MATRIX_1:def 2;
then A8: (the carrier of SS) = ( Solutions_of (S,( ( card N ) |-> ( 0. K ) )) ) by A3,A6,CKB77:def 1;
set SA = ( Space_of_Solutions_of A );
A9: (the carrier of SA) = ( Solutions_of (A,L) ) by A3,CKB77:def 1;
A10: C = ( 0. (K,( len A ),1) ) by CKB37:1;
( len C ) = ( len L ) by MATRIX_1:def 2;
then A11: ( dom C ) = ( dom A ) by A5,FINSEQ_3:29;
A12: ( dom A ) = ( Seg ( len A ) ) by FINSEQ_1:def 3;
then A13: ( Seg ( len A ) ) <> ( {} ) by A1,A2,XBOOLE_1:3;
then A14: ( width C ) = 1 by CKB31:1;
then A15: ( card ( Seg ( width C ) ) ) = 1 by FINSEQ_1:57;
now
A16: ( rng ( Sgm ( Seg 1 ) ) ) = ( Seg 1 ) by FINSEQ_1:def 13;
A17: ( Indices ( Segm (C,N,( Seg ( width C ) )) ) ) = ( Indices ( 0. (K,( card N ),1) ) ) by A15,MATRIX_1:26;
let k;
let l;
assume that
A18: [ k,l ] in ( Indices ( Segm (C,N,( Seg ( width C ) )) ) );
reconsider kk = k,ll = l as (Element of ( NAT )) by ORDINAL1:def 12;
([: N,( Seg ( width C ) ) :] c= ( Indices C ) & ( rng ( Sgm N ) ) = N) by A1,A12,A11,FINSEQ_1:def 13,ZFMISC_1:95;
then A19: [ ( ( Sgm N ) . kk ),( ( Sgm ( Seg ( width C ) ) ) . ll ) ] in ( Indices C ) by A14,A18,A16,MATRIX13:17;
thus ( ( Segm (C,N,( Seg ( width C ) )) ) * (k,l) ) = ( C * (( ( Sgm N ) . kk ),( ( Sgm ( Seg ( width C ) ) ) . ll )) ) by A18,MATRIX13:def 1
.= ( 0. K ) by A10,A19,MATRIX_3:1
.= ( ( 0. (K,( card N ),1) ) * (k,l) ) by A18,A17,MATRIX_3:1;
end;
then A20: ( Segm (C,N,( Seg ( width C ) )) ) = ( 0. (K,( card N ),1) ) by A15,MATRIX_1:27
.= ( ColVec2Mx ( ( card N ) |-> ( 0. K ) ) ) by CKB37:1;
now
let i;
assume that
A21: i in ( ( dom A ) \ N );
A22: i in ( dom A ) by A21,XBOOLE_0:def 5;
then ( Line (C,i) ) = ( C . i ) by A5,A12,MATRIX_2:8
.= ( ( ( len A ) |-> ( ( width C ) |-> ( 0. K ) ) ) . i ) by A10,A13,CKB31:1
.= ( ( width C ) |-> ( 0. K ) ) by A12,A22,FINSEQ_2:57;
hence (( Line (A,i) ) = ( ( width A ) |-> ( 0. K ) ) & ( Line (C,i) ) = ( ( width C ) |-> ( 0. K ) )) by A4,A21;
end;
then ( Solutions_of (A,C) ) = ( Solutions_of (( Segm (A,N,( Seg ( width A ) )) ),( Segm (C,N,( Seg ( width C ) )) )) ) by A1,A2,A11,CKB55:1;
hence thesis by A20,A7,A9,A8,VECTSP_4:29;
