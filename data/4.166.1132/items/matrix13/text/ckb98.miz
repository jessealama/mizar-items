reserve x for set;
reserve y for set;
theorem
Th81: (for f,g being Function holds (( rng f ) c= ( rng g ) implies (ex h being Function st ((( dom h ) = ( dom f ) & ( rng h ) c= ( dom g )) & f = ( g * h )))))
proof
let f being Function;
let g being Function;
assume that
A1: ( rng f ) c= ( rng g );
defpred P[ set,set ]
 means
( f . $1 ) = ( g . $2 );
A2: (for x holds (x in ( dom f ) implies (ex y st (y in ( dom g ) & P[ x,y ])))) by A1,GRAPH_5:1;
consider h being (Function of ( dom f ),( dom g )) such that A3: (for x holds (x in ( dom f ) implies P[ x,( h . x ) ])) from FUNCT_2:sch 1(A2);
per cases ;
suppose ( dom g ) = ( {} );

then ( rng g ) = ( {} ) by RELAT_1:42;
then A4: f = ( g * ( {} ) ) by A1;
( rng ( {} ) ) c= ( dom g ) by XBOOLE_1:2;
hence thesis by A4;
end;
suppose A5: ( dom g ) <> ( {} );

A6: ( rng h ) c= ( dom g ) by RELAT_1:def 19;
A7: ( dom h ) = ( dom f ) by A5,FUNCT_2:def 1;
then A8: ( dom ( g * h ) ) = ( dom f ) by A6,RELAT_1:27;
now
let x;
assume that
A9: x in ( dom f );
thus ( f . x ) = ( g . ( h . x ) ) by A3,A9
.= ( ( g * h ) . x ) by A8,A9,FUNCT_1:12;
end;
hence thesis by A7,A6,A8,FUNCT_1:2;
end;
end;
