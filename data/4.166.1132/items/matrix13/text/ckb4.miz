begin
reserve x for set;
reserve y for set;
reserve i for Nat;
reserve j for Nat;
reserve n for Nat;
reserve K for Field;
reserve p for (FinSequence of K);
theorem
Th4: (for perm being (Element of ( Permutations n )) holds (perm <> ( idseq n ) implies ((ex i st (i in ( Seg n ) & ( perm . i ) > i)) & (ex j st (j in ( Seg n ) & ( perm . j ) < j)))))
proof
let p being (Element of ( Permutations n ));
assume that
A1: p <> ( idseq n );
reconsider p9 = p as (Permutation of ( Seg n )) by MATRIX_2:def 9;
( dom p9 ) = ( Seg n ) by FUNCT_2:52;
then consider x being set such that A2: x in ( Seg n ) and A3: ( p . x ) <> x by A1,FUNCT_1:17;
consider i being (Element of ( NAT )) such that A4: i = x and A5: 1 <= i and A6: i <= n by A2;
now
per cases  by A3,A4,XXREAL_0:1;
suppose ( p . i ) > i;

hence (ex j st (j in ( Seg n ) & ( p . j ) > j)) by A2,A4;
end;
suppose A7: ( p . i ) < i;

then reconsider i1 = ( i - 1 ) as Nat by NAT_1:20;
thus (ex j st (j in ( Seg n ) & ( p . j ) > j))
proof
reconsider pS = ( p9 .: ( Seg i ) ) as  finite set;
A8: ( dom p9 ) = ( Seg n ) by FUNCT_2:52;
( Seg i ) c= ( Seg n ) by A6,FINSEQ_1:5;
then ( Seg i ),pS are_equipotent  by A8,CARD_1:33;
then A9: ( card ( Seg i ) ) = ( card pS ) by CARD_1:5;
assume A10: (for j holds (j in ( Seg n ) implies ( p . j ) <= j));
( p .: ( Seg i ) ) c= ( Seg i1 )
proof
let x;
assume x in ( p .: ( Seg i ) );
then consider y being set such that A11: y in ( dom p9 ) and A12: y in ( Seg i ) and A13: ( p . y ) = x by FUNCT_1:def 6;
consider j being (Element of ( NAT )) such that A14: j = y and 1 <= j and A15: j <= i by A12;
per cases  by A15,XXREAL_0:1;
suppose j = i;

then ( p . j ) < ( i1 + 1 ) by A7;
then A16: ( p . j ) <= i1 by NAT_1:13;
A17: ( rng p9 ) = ( Seg n ) by FUNCT_2:def 3;
( p . j ) in ( rng p ) by A11,A14,FUNCT_1:def 3;
then ( p . j ) >= 1 by A17,FINSEQ_1:1;
hence thesis by A13,A14,A16,FINSEQ_1:1;
end;
suppose A18: j < i;

( p . j ) <= j by A10,A11,A14;
then ( p . j ) < ( i1 + 1 ) by A18,XXREAL_0:2;
then A19: ( p . j ) <= i1 by NAT_1:13;
A20: ( rng p9 ) = ( Seg n ) by FUNCT_2:def 3;
( p . j ) in ( rng p ) by A11,A14,FUNCT_1:def 3;
then ( p . j ) >= 1 by A20,FINSEQ_1:1;
hence thesis by A13,A14,A19,FINSEQ_1:1;
end;
end;
then A21: ( card pS ) <= ( card ( Seg i1 ) ) by NAT_1:43;
( card ( Seg i ) ) = i by FINSEQ_1:57;
then ( i1 + 1 ) <= i1 by A9,A21,FINSEQ_1:57;
hence thesis by NAT_1:13;
end;

end;
end;
hence (ex j st (j in ( Seg n ) & ( p . j ) > j));
A22: n in ( NAT ) by ORDINAL1:def 12;
per cases  by A3,A4,XXREAL_0:1;
suppose ( p . i ) < i;

hence thesis by A2,A4;
end;
suppose A23: ( p . i ) > i;

thus (ex j st (j in ( Seg n ) & ( p . j ) < j))
proof
set NI = ( nat_interval (i,n) );
reconsider pN = ( p9 .: NI ) as  finite set;
A24: i in NI by A22,A6,SGRAPH1:2;
assume A25: (for j holds (j in ( Seg n ) implies ( p . j ) >= j));
A26: pN c= NI
proof
let x;
A27: ( rng p9 ) = ( Seg n ) by FUNCT_2:def 3;
assume x in pN;
then consider j being set such that A28: j in ( dom p9 ) and A29: j in NI and A30: ( p . j ) = x by FUNCT_1:def 6;
reconsider j as Nat by A29;
reconsider pj = ( p . j ) as (Element of ( NAT )) by ORDINAL1:def 12;
A31: j <= pj by A25,A28;
i <= j by A22,A29,SGRAPH1:2;
then A32: i <= pj by A31,XXREAL_0:2;
pj in ( rng p9 ) by A28,FUNCT_1:def 3;
then pj <= n by A27,FINSEQ_1:1;
hence thesis by A22,A30,A32,SGRAPH1:1;
end;
( dom p9 ) = ( Seg n ) by FUNCT_2:52;
then NI,pN are_equipotent  by A22,A5,CARD_1:33,SGRAPH1:4;
then ( card NI ) = ( card pN ) by CARD_1:5;
then NI = pN by A26,CARD_FIN:1;
then consider j being set such that A33: j in ( dom p9 ) and A34: j in NI and A35: ( p . j ) = i by A24,FUNCT_1:def 6;
reconsider j as Nat by A34;
A36: i <= j by A22,A34,SGRAPH1:2;
j <= i by A25,A33,A35;
hence thesis by A23,A35,A36,XXREAL_0:1;
end;

end;
end;
