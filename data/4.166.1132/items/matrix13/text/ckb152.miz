(for RANK being (Element of ( NAT )) holds (( the_rank_of M ) = RANK iff ((ex U being  finite (Subset of ( n -VectSp_over K )) st ((U is  linearly-independent & U c= ( lines M )) & ( card U ) = RANK)) & (for W being  finite (Subset of ( n -VectSp_over K )) holds ((W is  linearly-independent & W c= ( lines M )) implies ( card W ) <= RANK)))))
