begin
reserve D for non  empty set;
reserve i for Nat;
reserve k for Nat;
reserve m for Nat;
reserve n for Nat;
reserve m9 for Nat;
reserve n9 for Nat;
reserve A9 for (Matrix of n9,m9,D);
reserve nt for (Element of ( n -tuples_on ( NAT ) ));
reserve mt for (Element of ( m -tuples_on ( NAT ) ));
theorem
Th38: (for F being (FinSequence of D) holds (for i holds (for nt holds (((not i in ( rng nt )) & [: ( rng nt ),( rng mt ) :] c= ( Indices A9 )) implies ( Segm (A9,nt,mt) ) = ( Segm (( RLine (A9,i,F) ),nt,mt) )))))
proof
let F being (FinSequence of D);
let i;
let nt;
assume that
A1: (not i in ( rng nt ))
and
A2: [: ( rng nt ),( rng mt ) :] c= ( Indices A9 );
set S = ( Segm (A9,nt,mt) );
set R = ( RLine (A9,i,F) );
set SR = ( Segm (R,nt,mt) );
per cases ;
suppose ( len F ) <> ( width A9 );

hence thesis by MATRIX11:def 3;
end;
suppose A3: ( len F ) = ( width A9 );

A4: ( Indices SR ) = ( Indices S ) by MATRIX_1:26;
now
A5: ( dom nt ) = ( Seg n ) by FINSEQ_2:124;
let k;
let m;
assume that
A6: [ k,m ] in ( Indices SR );
( Indices SR ) = [: ( Seg n ),( Seg ( width SR ) ) :] by MATRIX_1:25;
then k in ( Seg n ) by A6,ZFMISC_1:87;
then A7: i <> ( nt . k ) by A1,A5,FUNCT_1:def 3;
reconsider K = k,M = m as (Element of ( NAT )) by ORDINAL1:def 12;
[ ( nt . K ),( mt . M ) ] in ( Indices A9 ) by A2,A4,A6,CKB18:1;
then A8: ( A9 * (( nt . K ),( mt . M )) ) = ( R * (( nt . K ),( mt . M )) ) by A3,A7,MATRIX11:def 3;
( S * (K,M) ) = ( A9 * (( nt . K ),( mt . M )) ) by A4,A6,CKB17:def 1;
hence ( SR * (k,m) ) = ( S * (k,m) ) by A6,A8,CKB17:def 1;
end;
hence thesis by MATRIX_1:27;
end;
end;
