reserve M for (Matrix of K);
theorem
(( the_rank_of M ) = 1 iff (ex i st ((i in ( Seg ( len M ) ) & (ex j st (j in ( Seg ( width M ) ) & ( M * (i,j) ) <> ( 0. K )))) & (for k holds (k in ( Seg ( len M ) ) implies (ex a st ( Line (M,k) ) = ( a * ( Line (M,i) ) )))))))
proof
A1: ( Indices M ) = [: ( Seg ( len M ) ),( Seg ( width M ) ) :] by FINSEQ_1:def 3;
thus (( the_rank_of M ) = 1 implies (ex i st ((i in ( Seg ( len M ) ) & (ex j st (j in ( Seg ( width M ) ) & ( M * (i,j) ) <> ( 0. K )))) & (for k holds (k in ( Seg ( len M ) ) implies (ex a st ( Line (M,k) ) = ( a * ( Line (M,i) ) )))))))
proof
assume A2: ( the_rank_of M ) = 1;
then consider i,j such that A3: [ i,j ] in ( Indices M ) and A4: ( M * (i,j) ) <> ( 0. K ) by CKB115:1;
take i;
A5: j in ( Seg ( width M ) ) by A3,ZFMISC_1:87;
hence (i in ( Seg ( len M ) ) & (ex j st (j in ( Seg ( width M ) ) & ( M * (i,j) ) <> ( 0. K )))) by A1,A3,A4,ZFMISC_1:87;
set Li = ( Line (M,i) );
set ij = ( M * (i,j) );
let k;
assume that
A6: k in ( Seg ( len M ) );
set Lk = ( Line (M,k) );
set kj = ( M * (k,j) );
take a = ( kj * ( ij " ) );
A7: i in ( Seg ( len M ) ) by A1,A3,ZFMISC_1:87;
A8:now
let n;
assume that
A9: 1 <= n
and
A10: n <= ( width M );
n in ( NAT ) by ORDINAL1:def 12;
then A11: n in ( Seg ( width M ) ) by A9,A10;
then A12: { j,n } c= ( Seg ( width M ) ) by A5,ZFMISC_1:32;
( Li . n ) = ( M * (i,n) ) by A11,MATRIX_1:def 7;
then A13: ( ( a * Li ) . n ) = ( a * ( M * (i,n) ) ) by A11,FVSUM_1:51;
A14: kj = ( kj * ( 1_ K ) ) by VECTSP_1:def 4
.= ( kj * ( ij * ( ij " ) ) ) by A4,VECTSP_1:def 10
.= ( a * ij ) by GROUP_1:def 3;
{ i,k } c= ( Seg ( len M ) ) by A7,A6,ZFMISC_1:32;
then [: { i,k },{ j,n } :] c= ( Indices M ) by A1,A12,ZFMISC_1:96;
then A15: ( ij * ( M * (k,n) ) ) = ( ( a * ij ) * ( M * (i,n) ) ) by A2,A14,CKB115:1
.= ( ij * ( a * ( M * (i,n) ) ) ) by GROUP_1:def 3;
( Lk . n ) = ( M * (k,n) ) by A11,MATRIX_1:def 7;
hence ( Lk . n ) = ( ( a * Li ) . n ) by A4,A15,A13,VECTSP_2:8;
end;
A16: ( len ( a * Li ) ) = ( width M ) by CARD_1:def 7;
( len Lk ) = ( width M ) by CARD_1:def 7;
hence thesis by A16,A8,FINSEQ_1:14;
end;

given i such that
A17: i in ( Seg ( len M ) )
and
A18: (ex j st (j in ( Seg ( width M ) ) & ( M * (i,j) ) <> ( 0. K )))
and
A19: (for k holds (k in ( Seg ( len M ) ) implies (ex a st ( Line (M,k) ) = ( a * ( Line (M,i) ) ))));

A20:now
set Li = ( Line (M,i) );
let I being Nat;
let J being Nat;
let n being Nat;
let m being Nat;
assume that
A21: [: { I,J },{ n,m } :] c= ( Indices M );
A22: { n,m } c= ( Seg ( width M ) ) by A21,ZFMISC_1:114;
then A23: n in ( Seg ( width M ) ) by ZFMISC_1:32;
then A24: ( Li . n ) = ( M * (i,n) ) by MATRIX_1:def 7;
set LJ = ( Line (M,J) );
set LI = ( Line (M,I) );
A25: { I,J } c= ( Seg ( len M ) ) by A1,A21,ZFMISC_1:114;
then I in ( Seg ( len M ) ) by ZFMISC_1:32;
then consider a such that A26: LI = ( a * Li ) by A19;
J in ( Seg ( len M ) ) by A25,ZFMISC_1:32;
then consider b such that A27: LJ = ( b * Li ) by A19;
( LJ . n ) = ( M * (J,n) ) by A23,MATRIX_1:def 7;
then A28: ( M * (J,n) ) = ( b * ( M * (i,n) ) ) by A27,A23,A24,FVSUM_1:51;
A29: m in ( Seg ( width M ) ) by A22,ZFMISC_1:32;
then A30: ( Li . m ) = ( M * (i,m) ) by MATRIX_1:def 7;
( LJ . m ) = ( M * (J,m) ) by A29,MATRIX_1:def 7;
then A31: ( M * (J,m) ) = ( b * ( M * (i,m) ) ) by A27,A29,A30,FVSUM_1:51;
( LI . m ) = ( M * (I,m) ) by A29,MATRIX_1:def 7;
then A32: ( M * (I,m) ) = ( a * ( M * (i,m) ) ) by A26,A29,A30,FVSUM_1:51;
( LI . n ) = ( M * (I,n) ) by A23,MATRIX_1:def 7;
then ( M * (I,n) ) = ( a * ( M * (i,n) ) ) by A26,A23,A24,FVSUM_1:51;
hence ( ( M * (I,n) ) * ( M * (J,m) ) ) = ( ( ( a * ( M * (i,n) ) ) * b ) * ( M * (i,m) ) ) by A31,GROUP_1:def 3
.= ( ( ( b * ( M * (i,n) ) ) * a ) * ( M * (i,m) ) ) by GROUP_1:def 3
.= ( ( M * (I,m) ) * ( M * (J,n) ) ) by A28,A32,GROUP_1:def 3;
end;
consider j such that A33: j in ( Seg ( width M ) ) and A34: ( M * (i,j) ) <> ( 0. K ) by A18;
[ i,j ] in ( Indices M ) by A1,A17,A33,ZFMISC_1:87;
hence thesis by A34,A20,CKB115:1;
end;
