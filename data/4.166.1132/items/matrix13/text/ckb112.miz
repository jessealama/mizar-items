take y;
y in Q1 by A9,TARSKI:def 1;
then [ x,y ] in [: P,Q :] by A5,A6,ZFMISC_1:87;
hence [ x,y ] in ( Indices M ) by A1;
A10: ( card { x } ) = 1 by CARD_1:30;
