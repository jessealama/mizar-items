environ
vocabularies PRE_TOPC,SUBSET_1,XBOOLE_0,TARSKI,RCOMP_1,RELAT_2,FUNCT_1,ORDINAL2,RELAT_1,STRUCT_0,TOPS_1,SETFAM_1,CONNSP_1;
notations TARSKI,XBOOLE_0,SUBSET_1,SETFAM_1,RELSET_1,DOMAIN_1,STRUCT_0,PRE_TOPC,TOPS_1,CKB1,CKB11,CKB17;
definitions XBOOLE_0,SUBSET_1,STRUCT_0,PRE_TOPC,CKB1,CKB11,CKB17;
theorems TARSKI,FUNCT_1,FUNCT_2,ZFMISC_1,SETFAM_1,PRE_TOPC,TOPS_1,SUBSET_1,RELAT_1,XBOOLE_0,XBOOLE_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24;
schemes SUBSET_1;
registrations XBOOLE_0,SUBSET_1,RELSET_1,STRUCT_0,PRE_TOPC;
constructors SETFAM_1,DOMAIN_1,TOPS_1,RELSET_1,CKB1,CKB11,CKB17;
requirements SUBSET,BOOLE;
begin
reserve GX for TopSpace;
reserve A for (Subset of GX);
reserve C for (Subset of GX);
theorem
(((C is  connected & C meets A) & ( C \ A ) <> ( {} GX )) implies C meets ( Fr A ))
proof
assume that
A1: C is  connected
and
A2: C meets A
and
A3: ( C \ A ) <> ( {} GX );
A4: ( A ` ) c= ( Cl ( A ` ) ) by PRE_TOPC:18;
( Cl ( C /\ A ) ) c= ( Cl A ) by PRE_TOPC:19,XBOOLE_1:17;
then A5: ( ( Cl ( C /\ A ) ) /\ ( A ` ) ) c= ( ( Cl A ) /\ ( Cl ( A ` ) ) ) by A4,XBOOLE_1:27;
A6: A c= ( Cl A ) by PRE_TOPC:18;
A7: ( C \ A ) = ( C /\ ( A ` ) ) by SUBSET_1:13;
then ( Cl ( C \ A ) ) c= ( Cl ( A ` ) ) by PRE_TOPC:19,XBOOLE_1:17;
then ( A /\ ( Cl ( C /\ ( A ` ) ) ) ) c= ( ( Cl A ) /\ ( Cl ( A ` ) ) ) by A7,A6,XBOOLE_1:27;
then ( ( ( Cl ( C /\ A ) ) /\ ( A ` ) ) \/ ( A /\ ( Cl ( C /\ ( A ` ) ) ) ) ) c= ( ( Cl A ) /\ ( Cl ( A ` ) ) ) by A5,XBOOLE_1:8;
then A8: ( C /\ ( ( ( Cl ( C /\ A ) ) /\ ( A ` ) ) \/ ( A /\ ( Cl ( C /\ ( A ` ) ) ) ) ) ) c= ( C /\ ( ( Cl A ) /\ ( Cl ( A ` ) ) ) ) by XBOOLE_1:27;
A9: C = ( C /\ ( [#] GX ) ) by XBOOLE_1:28
.= ( C /\ ( A \/ ( A ` ) ) ) by PRE_TOPC:2
.= ( ( C /\ A ) \/ ( C \ A ) ) by A7,XBOOLE_1:23;
( C /\ A ) <> ( {} ) by A2,XBOOLE_0:def 7;
then (not ( C /\ A ),( C \ A ) are_separated ) by A1,A3,A9,CKB18:1;
then (( Cl ( C /\ A ) ) meets ( C \ A ) or ( C /\ A ) meets ( Cl ( C \ A ) )) by CKB1:def 1;
then A10: (( ( Cl ( C /\ A ) ) /\ ( C \ A ) ) <> ( {} ) or ( ( C /\ A ) /\ ( Cl ( C \ A ) ) ) <> ( {} )) by XBOOLE_0:def 7;
( ( ( Cl ( C /\ A ) ) /\ ( C \ A ) ) \/ ( ( C /\ A ) /\ ( Cl ( C \ A ) ) ) ) = ( ( ( ( Cl ( C /\ A ) ) /\ C ) /\ ( A ` ) ) \/ ( ( C /\ A ) /\ ( Cl ( C /\ ( A ` ) ) ) ) ) by A7,XBOOLE_1:16
.= ( ( ( C /\ ( Cl ( C /\ A ) ) ) /\ ( A ` ) ) \/ ( C /\ ( A /\ ( Cl ( C /\ ( A ` ) ) ) ) ) ) by XBOOLE_1:16
.= ( ( C /\ ( ( Cl ( C /\ A ) ) /\ ( A ` ) ) ) \/ ( C /\ ( A /\ ( Cl ( C /\ ( A ` ) ) ) ) ) ) by XBOOLE_1:16
.= ( C /\ ( ( ( Cl ( C /\ A ) ) /\ ( A ` ) ) \/ ( A /\ ( Cl ( C /\ ( A ` ) ) ) ) ) ) by XBOOLE_1:23;
then ( ( ( Cl ( C /\ A ) ) /\ ( C \ A ) ) \/ ( ( C /\ A ) /\ ( Cl ( C \ A ) ) ) ) c= ( C /\ ( Fr A ) ) by A8,TOPS_1:def 2;
hence ( C /\ ( Fr A ) ) <> ( {} ) by A10;
end;
