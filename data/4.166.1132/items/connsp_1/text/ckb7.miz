environ
vocabularies PRE_TOPC,SUBSET_1,XBOOLE_0,TARSKI,RCOMP_1,RELAT_2,FUNCT_1,ORDINAL2,RELAT_1,STRUCT_0,TOPS_1,SETFAM_1,CONNSP_1;
notations TARSKI,XBOOLE_0,SUBSET_1,SETFAM_1,RELSET_1,DOMAIN_1,STRUCT_0,PRE_TOPC,TOPS_1,CKB1;
definitions XBOOLE_0,SUBSET_1,STRUCT_0,PRE_TOPC,CKB1;
theorems TARSKI,FUNCT_1,FUNCT_2,ZFMISC_1,SETFAM_1,PRE_TOPC,TOPS_1,SUBSET_1,RELAT_1,XBOOLE_0,XBOOLE_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6;
schemes SUBSET_1;
registrations XBOOLE_0,SUBSET_1,RELSET_1,STRUCT_0,PRE_TOPC;
constructors SETFAM_1,DOMAIN_1,TOPS_1,RELSET_1,CKB1;
requirements SUBSET,BOOLE;
begin
reserve GX for TopSpace;
theorem
Th7: (for X9 being (SubSpace of GX) holds (for P,Q being (Subset of GX) holds (for P1,Q1 being (Subset of X9) holds (((P = P1 & Q = Q1) & ( P \/ Q ) c= ( [#] X9 )) implies (P,Q are_separated  implies P1,Q1 are_separated )))))
proof
let X9 being (SubSpace of GX);
let P being (Subset of GX),Q being (Subset of GX);
let P1 being (Subset of X9),Q1 being (Subset of X9);
assume that
A1: P = P1
and
A2: Q = Q1
and
A3: ( P \/ Q ) c= ( [#] X9 );
A4: Q c= ( P \/ Q ) by XBOOLE_1:7;
P c= ( P \/ Q ) by XBOOLE_1:7;
then reconsider P2 = P,Q2 = Q as (Subset of X9) by A3,A4,XBOOLE_1:1;
assume that
A5: ( ( Cl P ) /\ Q ) = ( {} )
and
A6: ( P /\ ( Cl Q ) ) = ( {} );
( P2 /\ ( Cl Q2 ) ) = ( P2 /\ ( ( [#] X9 ) /\ ( Cl Q ) ) ) by PRE_TOPC:17
.= ( ( P2 /\ ( [#] X9 ) ) /\ ( Cl Q ) ) by XBOOLE_1:16
.= ( P /\ ( Cl Q ) ) by XBOOLE_1:28;
then A7: P2 misses ( Cl Q2 ) by A6,XBOOLE_0:def 7;
( Cl P2 ) = ( ( Cl P ) /\ ( [#] X9 ) ) by PRE_TOPC:17;
then ( ( Cl P2 ) /\ Q2 ) = ( ( Cl P ) /\ ( Q2 /\ ( [#] X9 ) ) ) by XBOOLE_1:16
.= ( ( Cl P ) /\ Q ) by XBOOLE_1:28;
then ( Cl P2 ) misses Q2 by A5,XBOOLE_0:def 7;
hence thesis by A1,A2,A7,CKB1:def 1;
end;
