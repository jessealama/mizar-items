environ
vocabularies PRE_TOPC,SUBSET_1,XBOOLE_0,TARSKI,RCOMP_1,RELAT_2,FUNCT_1,ORDINAL2,RELAT_1,STRUCT_0,TOPS_1,SETFAM_1,CONNSP_1;
notations TARSKI,XBOOLE_0,SUBSET_1,SETFAM_1,RELSET_1,DOMAIN_1,STRUCT_0,PRE_TOPC,TOPS_1,CKB1;
definitions XBOOLE_0,SUBSET_1,STRUCT_0,PRE_TOPC,CKB1;
theorems TARSKI,FUNCT_1,FUNCT_2,ZFMISC_1,SETFAM_1,PRE_TOPC,TOPS_1,SUBSET_1,RELAT_1,XBOOLE_0,XBOOLE_1,CKB1,CKB2;
schemes SUBSET_1;
registrations XBOOLE_0,SUBSET_1,RELSET_1,STRUCT_0,PRE_TOPC;
constructors SETFAM_1,DOMAIN_1,TOPS_1,RELSET_1,CKB1;
requirements SUBSET,BOOLE;
begin
reserve TS for TopStruct;
reserve K for (Subset of TS);
reserve L for (Subset of TS);
theorem
Th3: (((K is  closed & L is  closed) & K misses L) implies K,L are_separated )
proof
assume that
A1: K is  closed
and
A2: L is  closed
and
A3: K misses L;
A4: ( K /\ L ) = ( {} ) by A3,XBOOLE_0:def 7;
( K /\ ( Cl L ) ) = ( K /\ L ) by A2,PRE_TOPC:22;
then A5: K misses ( Cl L ) by A4,XBOOLE_0:def 7;
( ( Cl K ) /\ L ) = ( K /\ L ) by A1,PRE_TOPC:22;
then ( Cl K ) misses L by A4,XBOOLE_0:def 7;
hence thesis by A5,CKB1:def 1;
end;
