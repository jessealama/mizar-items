begin
theorem
(for T being TopSpace holds (for X being (Subset of T) holds (for Y being (Subset of ( the TopStruct of T )) holds (X = Y implies (X is  a_component iff Y is  a_component)))))
proof
let T being TopSpace;
let X being (Subset of T);
let Y being (Subset of ( the TopStruct of T ));
assume that
A1: X = Y;
thus (X is  a_component implies Y is  a_component)
proof
assume A2: X is  a_component;
hence Y is  connected by A1,CKB58:1;
let B being (Subset of ( the TopStruct of T ));
assume that
A3: B is  connected
and
A4: Y c= B;
reconsider C = B as (Subset of T);
C is  connected by A3,CKB58:1;
hence Y = B by A1,A2,A4,CKB37:def 1;
end;

assume A5: Y is  a_component;
hence X is  connected by A1,CKB58:1;
let B being (Subset of T);
assume that
A6: B is  connected
and
A7: X c= B;
reconsider C = B as (Subset of ( the TopStruct of T ));
C is  connected by A6,CKB58:1;
hence X = B by A1,A5,A7,CKB37:def 1;
end;
