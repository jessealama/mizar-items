environ
vocabularies PRE_TOPC,SUBSET_1,XBOOLE_0,TARSKI,RCOMP_1,RELAT_2,FUNCT_1,ORDINAL2,RELAT_1,STRUCT_0,TOPS_1,SETFAM_1,CONNSP_1;
notations TARSKI,XBOOLE_0,SUBSET_1,SETFAM_1,RELSET_1,DOMAIN_1,STRUCT_0,PRE_TOPC,TOPS_1,CKB1,CKB11,CKB17;
definitions XBOOLE_0,SUBSET_1,STRUCT_0,PRE_TOPC,CKB1,CKB11,CKB17;
theorems TARSKI,FUNCT_1,FUNCT_2,ZFMISC_1,SETFAM_1,PRE_TOPC,TOPS_1,SUBSET_1,RELAT_1,XBOOLE_0,XBOOLE_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20;
schemes SUBSET_1;
registrations XBOOLE_0,SUBSET_1,RELSET_1,STRUCT_0,PRE_TOPC;
constructors SETFAM_1,DOMAIN_1,TOPS_1,RELSET_1,CKB1,CKB11,CKB17;
requirements SUBSET,BOOLE;
begin
reserve GX for TopSpace;
reserve A for (Subset of GX);
reserve C for (Subset of GX);
theorem
Th19: (((C is  connected & C c= A) & A c= ( Cl C )) implies A is  connected)
proof
assume that
A1: C is  connected
and
A2: C c= A
and
A3: A c= ( Cl C );
assume (not A is  connected);
then consider P being (Subset of GX),Q being (Subset of GX) such that A4: A = ( P \/ Q ) and A5: P,Q are_separated  and A6: P <> ( {} GX ) and A7: Q <> ( {} GX ) by CKB18:1;
P misses ( Cl Q ) by A5,CKB1:def 1;
then A8: ( P /\ ( Cl Q ) ) = ( {} ) by XBOOLE_0:def 7;
A9:now
assume C c= Q;
then ( Cl C ) c= ( Cl Q ) by PRE_TOPC:19;
then ( P /\ ( Cl C ) ) = ( {} ) by A8,XBOOLE_1:3,XBOOLE_1:27;
then ( P /\ A ) = ( {} ) by A3,XBOOLE_1:3,XBOOLE_1:27;
hence contradiction by A4,A6,XBOOLE_1:21;
end;
( Cl P ) misses Q by A5,CKB1:def 1;
then A10: ( ( Cl P ) /\ Q ) = ( {} ) by XBOOLE_0:def 7;
now
assume C c= P;
then ( Cl C ) c= ( Cl P ) by PRE_TOPC:19;
then ( ( Cl C ) /\ Q ) = ( {} ) by A10,XBOOLE_1:3,XBOOLE_1:27;
then ( A /\ Q ) = ( {} ) by A3,XBOOLE_1:3,XBOOLE_1:27;
hence contradiction by A4,A7,XBOOLE_1:21;
end;
hence contradiction by A1,A2,A4,A5,A9,CKB19:1;
end;
