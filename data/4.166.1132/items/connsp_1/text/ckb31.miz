environ
vocabularies PRE_TOPC,SUBSET_1,XBOOLE_0,TARSKI,RCOMP_1,RELAT_2,FUNCT_1,ORDINAL2,RELAT_1,STRUCT_0,TOPS_1,SETFAM_1,CONNSP_1;
notations TARSKI,XBOOLE_0,SUBSET_1,SETFAM_1,RELSET_1,DOMAIN_1,STRUCT_0,PRE_TOPC,TOPS_1,CKB1,CKB11,CKB17;
definitions XBOOLE_0,SUBSET_1,STRUCT_0,PRE_TOPC,CKB1,CKB11,CKB17;
theorems TARSKI,FUNCT_1,FUNCT_2,ZFMISC_1,SETFAM_1,PRE_TOPC,TOPS_1,SUBSET_1,RELAT_1,XBOOLE_0,XBOOLE_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30;
schemes SUBSET_1;
registrations XBOOLE_0,SUBSET_1,RELSET_1,STRUCT_0,PRE_TOPC;
constructors SETFAM_1,DOMAIN_1,TOPS_1,RELSET_1,CKB1,CKB11,CKB17;
requirements SUBSET,BOOLE;
begin
registration
let GX being non empty TopSpace;
let x being (Point of GX);
cluster { x } ->  connected for (Subset of GX);
coherence
proof
now
assume (not { x } is  connected);
then consider P being (Subset of GX),Q being (Subset of GX) such that A1: { x } = ( P \/ Q ) and A2: P,Q are_separated  and A3: P <> ( {} GX ) and A4: Q <> ( {} GX ) by CKB18:1;
P <> Q
proof
assume (not thesis);
then A5: ( P /\ Q ) = P;
P misses Q by A2,CKB2:1;
hence contradiction by A3,A5,XBOOLE_0:def 7;
end;
hence contradiction by A1,A3,A4,ZFMISC_1:38;
end;
hence thesis;
end;
end;
