begin
theorem
Th33: (for GX being non empty TopSpace holds (for x being (Point of GX) holds (for F being (Subset-Family of GX) holds ((for A being (Subset of GX) holds (A in F iff (A is  connected & x in A))) implies F <> ( {} )))))
proof
let GX being non empty TopSpace;
let x being (Point of GX);
let F being (Subset-Family of GX);
assume that
A1: (for A being (Subset of GX) holds (A in F iff (A is  connected & x in A)));
x in { x } by TARSKI:def 1;
hence thesis by A1;
end;
