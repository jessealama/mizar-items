constructors PARTFUN1,BINOP_1,REAL_1,NAT_1,ALGSTR_1,CAT_3,RELSET_1,CKB2,CKB4,CKB6,CKB7,CKB8,CKB9,CKB10,CKB23,CKB30,CKB36,CKB37,CKB42;
requirements NUMERALS,SUBSET,BOOLE;
begin
reserve C for Cartesian_category;
reserve a for (Object of C);
reserve b for (Object of C);
reserve c for (Object of C);
reserve d for (Object of C);
theorem
(for f being (Morphism of c,a) holds (for g being (Morphism of c,b) holds (((( Hom (c,a) ) <> ( {} ) & ( Hom (c,b) ) <> ( {} )) & (f is  monic or g is  monic)) implies <: f,g :> is  monic)))
proof
let f being (Morphism of c,a);
let g being (Morphism of c,b);
assume that
A1: ( Hom (c,a) ) <> ( {} )
and
A2: ( Hom (c,b) ) <> ( {} )
and
A3: (f is  monic or g is  monic);
A4:now
assume A5: g is  monic;
let d being (Object of C);
let f1 being (Morphism of d,c),f2 being (Morphism of d,c);
assume that
A6: ( Hom (d,c) ) <> ( {} )
and
A7: ( <: f,g :> * f1 ) = ( <: f,g :> * f2 );
A8: (( Hom (d,a) ) <> ( {} ) & ( Hom (d,b) ) <> ( {} )) by A1,A2,A6,CAT_1:24;
(<: ( f * f1 ),( g * f1 ) :> = ( <: f,g :> * f1 ) & <: ( f * f2 ),( g * f2 ) :> = ( <: f,g :> * f2 )) by A1,A2,A6,CKB45:1;
then ( g * f1 ) = ( g * f2 ) by A7,A8,CKB46:1;
hence f1 = f2 by A2,A5,A6,CAT_1:31;
end;
A9:now
assume A10: f is  monic;
let d;
let f1 being (Morphism of d,c),f2 being (Morphism of d,c);
assume that
A11: ( Hom (d,c) ) <> ( {} )
and
A12: ( <: f,g :> * f1 ) = ( <: f,g :> * f2 );
A13: (( Hom (d,a) ) <> ( {} ) & ( Hom (d,b) ) <> ( {} )) by A1,A2,A11,CAT_1:24;
(<: ( f * f1 ),( g * f1 ) :> = ( <: f,g :> * f1 ) & <: ( f * f2 ),( g * f2 ) :> = ( <: f,g :> * f2 )) by A1,A2,A11,CKB45:1;
then ( f * f1 ) = ( f * f2 ) by A12,A13,CKB46:1;
hence f1 = f2 by A1,A10,A11,CAT_1:31;
end;
( Hom (c,( a [x] b )) ) <> ( {} ) by A1,A2,CKB43:1;
hence thesis by A3,A9,A4,CAT_1:31;
end;
