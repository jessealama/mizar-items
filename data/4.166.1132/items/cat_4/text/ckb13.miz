registrations XBOOLE_0,RELAT_1,FUNCT_1,FINSET_1,XREAL_0,NAT_1,ORDINAL1,CARD_1,STRUCT_0,PARTFUN1,FUNCT_2,RELSET_1,CKB4,CKB5,CKB11;
constructors PARTFUN1,BINOP_1,REAL_1,NAT_1,ALGSTR_1,CAT_3,RELSET_1,CKB2,CKB4,CKB6,CKB7,CKB8,CKB9,CKB10;
requirements NUMERALS,SUBSET,BOOLE;
begin
reserve o for set;
reserve m for set;
theorem
Lm1: ( c1Cat (o,m) ) is  Category-like
proof
set C = ( c1Cat (o,m) );
set CP = (the Comp of C);
set CD = (the Source of C);
set CC = (the Target of C);
set CI = (the Id of C);
thus (for f,g being (Morphism of C) holds ([ g,f ] in ( dom CP ) iff ( CD . g ) = ( CC . f )))
proof
let f being (Morphism of C);
let g being (Morphism of C);
A1: ( dom CP ) = { [ m,m ] } by FUNCOP_1:13;
(f = m & g = m) by TARSKI:def 1;
hence thesis by A1,TARSKI:def 1;
end;

thus (for f,g being (Morphism of C) holds (( CD . g ) = ( CC . f ) implies (( CD . ( CP . (g,f) ) ) = ( CD . f ) & ( CC . ( CP . (g,f) ) ) = ( CC . g ))))
proof
let f being (Morphism of C);
let g being (Morphism of C);
( CP . (g,f) ) = m by FUNCOP_1:77;
then reconsider gf = ( CP . (g,f) ) as (Morphism of C) by TARSKI:def 1;
( CD . gf ) = o by FUNCT_2:50;
hence thesis by FUNCT_2:50;
end;

thus (for f,g,h being (Morphism of C) holds ((( CD . h ) = ( CC . g ) & ( CD . g ) = ( CC . f )) implies ( CP . (h,( CP . (g,f) )) ) = ( CP . (( CP . (h,g) ),f) )))
proof
let f being (Morphism of C);
let g being (Morphism of C);
let h being (Morphism of C);
(( CP . (g,f) ) = m & ( CP . (h,g) ) = m) by FUNCOP_1:77;
then reconsider gf = ( CP . (g,f) ),hg = ( CP . (h,g) ) as (Morphism of C) by TARSKI:def 1;
(( CP . (h,gf) ) = m & ( CP . (hg,f) ) = m) by FUNCOP_1:77;
hence thesis;
end;

let b being (Object of C);
b = o by TARSKI:def 1;
hence (( CD . ( CI . b ) ) = b & ( CC . ( CI . b ) ) = b) by FUNCT_2:50;
thus (for f being (Morphism of C) holds (( CC . f ) = b implies ( CP . (( CI . b ),f) ) = f))
proof
let f being (Morphism of C);
f = m by TARSKI:def 1;
hence thesis by FUNCOP_1:77;
end;

let g being (Morphism of C);
assume ( CD . g ) = b;
g = m by TARSKI:def 1;
hence ( CP . (g,( CI . b )) ) = g by FUNCOP_1:77;
end;
