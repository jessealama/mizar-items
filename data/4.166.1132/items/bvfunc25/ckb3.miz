environ
vocabularies XBOOLE_0,SUBSET_1,FUNCT_2,MARGREL1,XBOOLEAN,BVFUNC_1,PARTIT1;
notations XBOOLE_0,MARGREL1,FUNCT_2,BVFUNC_1;
theorems BVFUNC_1,BVFUNC_4,BVFUNC_5,BVFUNC_6,BVFUNC_7,BVFUNC_8,BVFUNC_9,CKB1,CKB2;
registrations XBOOLE_0,MARGREL1;
constructors BVFUNC_1;
begin
reserve Y for non empty set;
reserve a for (Element of ( Funcs (Y,( BOOLEAN )) ));
reserve b for (Element of ( Funcs (Y,( BOOLEAN )) ));
theorem
Th3: ( a 'imp' b ) = ( ( 'not' b ) 'imp' ( 'not' a ) )
proof
( ( a 'imp' b ) 'imp' ( ( 'not' b ) 'imp' ( 'not' a ) ) ) = ( I_el Y ) by BVFUNC_5:32;
then A1: ( a 'imp' b ) '<' ( ( 'not' b ) 'imp' ( 'not' a ) ) by BVFUNC_1:16;
( ( ( 'not' b ) 'imp' ( 'not' a ) ) 'imp' ( a 'imp' b ) ) = ( I_el Y ) by CKB2:1;
then ( ( 'not' b ) 'imp' ( 'not' a ) ) '<' ( a 'imp' b ) by BVFUNC_1:16;
hence thesis by A1,BVFUNC_1:15;
end;
