environ
vocabularies XBOOLE_0,SUBSET_1,FUNCT_2,MARGREL1,XBOOLEAN,BVFUNC_1,PARTIT1;
notations XBOOLE_0,MARGREL1,FUNCT_2,BVFUNC_1;
theorems BVFUNC_1,BVFUNC_4,BVFUNC_5,BVFUNC_6,BVFUNC_7,BVFUNC_8,BVFUNC_9,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13;
registrations XBOOLE_0,MARGREL1;
constructors BVFUNC_1;
begin
reserve Y for non  empty set;
reserve a for (Element of ( Funcs (Y,( BOOLEAN )) ));
theorem
( a 'xor' ( 'not' a ) ) = ( I_el Y )
proof
thus ( a 'xor' ( 'not' a ) ) = ( ( ( 'not' a ) '&' ( 'not' a ) ) 'or' ( a '&' ( 'not' ( 'not' a ) ) ) ) by BVFUNC_4:9
.= ( I_el Y ) by BVFUNC_4:6;
end;
