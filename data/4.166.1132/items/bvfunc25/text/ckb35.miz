environ
vocabularies XBOOLE_0,SUBSET_1,FUNCT_2,MARGREL1,XBOOLEAN,BVFUNC_1,PARTIT1;
notations XBOOLE_0,MARGREL1,FUNCT_2,BVFUNC_1;
theorems BVFUNC_1,BVFUNC_4,BVFUNC_5,BVFUNC_6,BVFUNC_7,BVFUNC_8,BVFUNC_9,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34;
registrations XBOOLE_0,MARGREL1;
constructors BVFUNC_1;
begin
reserve Y for non empty set;
reserve a for (Element of ( Funcs (Y,( BOOLEAN )) ));
reserve b for (Element of ( Funcs (Y,( BOOLEAN )) ));
theorem
a = ( ( b 'imp' a ) '&' ( ( 'not' b ) 'imp' a ) )
proof
a = ( ( a 'or' b ) '&' ( a 'or' ( 'not' b ) ) ) by BVFUNC_8:7
.= ( ( a 'or' ( 'not' ( 'not' b ) ) ) '&' ( b 'imp' a ) ) by BVFUNC_4:8
.= ( ( ( 'not' b ) 'imp' a ) '&' ( b 'imp' a ) ) by BVFUNC_4:8;
hence thesis;
end;
