func (a,b) In_Power n -> (FinSequence of (the carrier of R)) means 
:Def10: (( len it ) = ( n + 1 ) & (for i,l,m being (Element of ( NAT )) holds (((i in ( dom it ) & m = ( i - 1 )) & l = ( n - m )) implies ( it /. i ) = ( ( ( n choose m ) * ( a |^ l ) ) * ( b |^ m ) ))));
existence
proof
defpred P[ (Element of ( NAT )),(Element of R) ]
 means
(for l,m being (Element of ( NAT )) holds ((m = ( $1 - 1 ) & l = ( n - m )) implies $2 = ( ( ( n choose m ) * ( a |^ l ) ) * ( b |^ m ) )));
A1: (for k being (Element of ( NAT )) holds (k in ( Seg ( n + 1 ) ) implies (ex y being (Element of R) st P[ k,y ])))
proof
let k being (Element of ( NAT ));
assume A2: k in ( Seg ( n + 1 ) );
then k >= 1 by FINSEQ_1:1;
then reconsider m1 = ( k - 1 ) as (Element of ( NAT )) by INT_1:5;
k <= ( n + 1 ) by A2,FINSEQ_1:1;
then ( k - 1 ) <= ( ( n + 1 ) - 1 ) by XREAL_1:9;
then reconsider l1 = ( n - m1 ) as (Element of ( NAT )) by INT_1:5;
reconsider z = ( ( ( n choose m1 ) * ( a |^ l1 ) ) * ( b |^ m1 ) ) as (Element of R);
take z;
thus thesis;
end;
consider p being (FinSequence of (the carrier of R)) such that A3: (( dom p ) = ( Seg ( n + 1 ) ) & (for k being (Element of ( NAT )) holds (k in ( Seg ( n + 1 ) ) implies P[ k,( p /. k ) ]))) from RECDEF_1:sch 17(A1);
take p;
thus thesis by A3,FINSEQ_1:def 3;
end;
uniqueness
proof
let f being (FinSequence of (the carrier of R));
let g being (FinSequence of (the carrier of R));
assume that
A4: ( len f ) = ( n + 1 )
and
A5: (for i,l,m being (Element of ( NAT )) holds (((i in ( dom f ) & m = ( i - 1 )) & l = ( n - m )) implies ( f /. i ) = ( ( ( n choose m ) * ( a |^ l ) ) * ( b |^ m ) )));
assume that
A6: ( len g ) = ( n + 1 )
and
A7: (for i,l,m being (Element of ( NAT )) holds (((i in ( dom g ) & m = ( i - 1 )) & l = ( n - m )) implies ( g /. i ) = ( ( ( n choose m ) * ( a |^ l ) ) * ( b |^ m ) )));
(for i being Nat holds ((1 <= i & i <= ( len f )) implies ( f . i ) = ( g . i )))
proof
let i being Nat;
assume that
A8: 1 <= i
and
A9: i <= ( len f );
reconsider m = ( i - 1 ) as (Element of ( NAT )) by A8,INT_1:5;
( i - 1 ) <= ( ( n + 1 ) - 1 ) by A4,A9,XREAL_1:9;
then reconsider l = ( n - m ) as (Element of ( NAT )) by INT_1:5;
A10: i in ( Seg ( n + 1 ) ) by A4,A8,A9,FINSEQ_1:1;
then A11: i in ( dom f ) by A4,FINSEQ_1:def 3;
A12: i in ( dom g ) by A6,A10,FINSEQ_1:def 3;
hence ( g . i ) = ( g /. i ) by PARTFUN1:def 6
.= ( ( ( n choose m ) * ( a |^ l ) ) * ( b |^ m ) ) by A7,A12
.= ( f /. i ) by A5,A11
.= ( f . i ) by A11,PARTFUN1:def 6;
end;
hence f = g by A4,A6,FINSEQ_1:14;
