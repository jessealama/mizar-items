Th50: (for o1,o2 being (object of ( AllMono C )) holds (for m being (Morphism of o1,o2) holds (<^ o1,o2 ^> <> ( {} ) implies m is  mono)))
proof
let o1 being (object of ( AllMono C ));
let o2 being (object of ( AllMono C ));
let m being (Morphism of o1,o2);
