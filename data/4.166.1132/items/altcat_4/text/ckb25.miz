begin
reserve C for category;
reserve o1 for (object of C);
reserve o2 for (object of C);
theorem
Th22: (for A,B being  transitive  with_units non  empty AltCatStr holds (for F being  contravariant (Functor of A,B) holds (for o1,o2 being (object of A) holds (for a being (Morphism of o1,o2) holds (((<^ o1,o2 ^> <> ( {} ) & <^ o2,o1 ^> <> ( {} )) & a is  retraction) implies ( F . a ) is  coretraction)))))
proof
let A being  transitive  with_units non  empty AltCatStr;
let B being  transitive  with_units non  empty AltCatStr;
let F being  contravariant (Functor of A,B);
let o1 being (object of A);
let o2 being (object of A);
let a being (Morphism of o1,o2);
assume that
A1: (<^ o1,o2 ^> <> ( {} ) & <^ o2,o1 ^> <> ( {} ));
assume a is  retraction;
then consider b being (Morphism of o2,o1) such that A2: b is_right_inverse_of a by ALTCAT_3:def 2;
take ( F . b );
( a * b ) = ( idm o2 ) by A2,ALTCAT_3:def 1;
hence ( ( F . b ) * ( F . a ) ) = ( F . ( idm o2 ) ) by A1,FUNCTOR0:def 24
.= ( idm ( F . o2 ) ) by CKB16:1;
end;
