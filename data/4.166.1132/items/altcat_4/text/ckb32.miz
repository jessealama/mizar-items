begin
reserve C for category;
reserve o1 for (object of C);
reserve o2 for (object of C);
theorem
(for A,B being category holds (for F being  covariant (Functor of A,B) holds (for o1,o2 being (object of A) holds ((((F is  full  faithful & <^ o1,o2 ^> <> ( {} )) & <^ o2,o1 ^> <> ( {} )) & ( F . o1 ),( F . o2 ) are_iso ) implies o1,o2 are_iso ))))
proof
let A being category;
let B being category;
let F being  covariant (Functor of A,B);
let o1 being (object of A);
let o2 being (object of A);
assume that
A1: F is  full  faithful
and
A2: <^ o1,o2 ^> <> ( {} )
and
A3: <^ o2,o1 ^> <> ( {} )
and
A4: ( F . o1 ),( F . o2 ) are_iso ;
consider Fa being (Morphism of ( F . o1 ),( F . o2 )) such that A5: Fa is  iso by A4,ALTCAT_3:def 6;
consider a being (Morphism of o1,o2) such that A6: Fa = ( F . a ) by A1,A2,CKB19:1;
thus (<^ o1,o2 ^> <> ( {} ) & <^ o2,o1 ^> <> ( {} )) by A2,A3;
take a;
thus thesis by A1,A2,A3,A5,A6,CKB31:1;
end;
