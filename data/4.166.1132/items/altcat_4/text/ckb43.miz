registrations SUBSET_1,RELSET_1,FUNCOP_1,STRUCT_0,FUNCT_1,RELAT_1,ALTCAT_1,ALTCAT_2,FUNCTOR0,FUNCTOR2,PBOOLE,CKB1,CKB6,CKB7,CKB39;
constructors REALSET1,MSUALG_3,FUNCTOR0,ALTCAT_3,RELSET_1;
requirements SUBSET,BOOLE;
begin
reserve C for category;
reserve o1 for (object of C);
reserve o2 for (object of C);
theorem
(for D being  full non  empty (subcategory of C) holds (for o1,o2 being (object of C) holds (for p1,p2 being (object of D) holds (for m being (Morphism of o1,o2) holds (for n being (Morphism of p1,p2) holds (((((p1 = o1 & p2 = o2) & m = n) & <^ p1,p2 ^> <> ( {} )) & <^ p2,p1 ^> <> ( {} )) implies (((m is  retraction implies n is  retraction) & (m is  coretraction implies n is  coretraction)) & (m is  iso implies n is  iso))))))))
proof
let D being  full non  empty (subcategory of C);
let o1 being (object of C);
let o2 being (object of C);
let p1 being (object of D);
let p2 being (object of D);
let m being (Morphism of o1,o2);
let n being (Morphism of p1,p2);
assume that
A1: (p1 = o1 & p2 = o2)
and
A2: m = n
and
A3: (<^ p1,p2 ^> <> ( {} ) & <^ p2,p1 ^> <> ( {} ));
thus A4: (m is  retraction implies n is  retraction)
proof
assume m is  retraction;
then consider B being (Morphism of o2,o1) such that A5: B is_right_inverse_of m by ALTCAT_3:def 2;
reconsider B1 = B as (Morphism of p2,p1) by A1,ALTCAT_2:28;
take B1;
thus thesis by A1,A2,A3,A5,CKB42:1;
end;

thus A6: (m is  coretraction implies n is  coretraction)
proof
assume m is  coretraction;
then consider B being (Morphism of o2,o1) such that A7: B is_left_inverse_of m by ALTCAT_3:def 3;
reconsider B1 = B as (Morphism of p2,p1) by A1,ALTCAT_2:28;
take B1;
thus thesis by A1,A2,A3,A7,CKB42:1;
end;

assume m is  iso;
hence thesis by A3,A4,A6,ALTCAT_3:5,ALTCAT_3:6;
end;
