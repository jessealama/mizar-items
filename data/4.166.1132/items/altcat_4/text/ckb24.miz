begin
reserve C for category;
reserve o1 for (object of C);
reserve o2 for (object of C);
theorem
(for A,B being category holds (for F being  covariant (Functor of A,B) holds (for o1,o2 being (object of A) holds (o1,o2 are_iso  implies ( F . o1 ),( F . o2 ) are_iso ))))
proof
let A being category;
let B being category;
let F being  covariant (Functor of A,B);
let o1 being (object of A);
let o2 being (object of A);
assume A1: o1,o2 are_iso ;
then consider a being (Morphism of o1,o2) such that A2: a is  iso by ALTCAT_3:def 6;
A3: (<^ o1,o2 ^> <> ( {} ) & <^ o2,o1 ^> <> ( {} )) by A1,ALTCAT_3:def 6;
hence (<^ ( F . o1 ),( F . o2 ) ^> <> ( {} ) & <^ ( F . o2 ),( F . o1 ) ^> <> ( {} )) by FUNCTOR0:def 18;
take ( F . a );
thus thesis by A3,A2,CKB23:1;
end;
