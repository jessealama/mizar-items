begin
reserve l for (Element of ( NAT ));
reserve P for QC-pred_symbol;
definition
let P being QC-pred_symbol;
let l being (FinSequence of ( QC-variables ));
assume A1: ( the_arity_of P ) = ( len l );
func P ! l -> (Element of ( QC-WFF )) equals 
:Def11: ( <* P *> ^ l );
coherence
proof
set k = ( len l );
set QCP = { Q where Q is QC-pred_symbol: ( the_arity_of Q ) = k };
P in QCP by A1;
then reconsider P as (QC-pred_symbol of k);
reconsider l as (QC-variable_list of k) by CARD_1:def 7;
reconsider X = ( <* P *> ^ l ) as (FinSequence of [: ( NAT ),( NAT ) :]) by CKB28:1;
X is QC-formula by CKB26:def 1,CKB31:1;
hence thesis;
end;
