begin
reserve k for (Element of ( NAT ));
reserve l for (Element of ( NAT ));
definition
let P being (Element of ( QC-pred_symbols ));
func the_arity_of P -> (Element of ( NAT )) means 
:Def6: ( P `1 ) = ( 7 + it );
existence
proof
P in { [ k,l ]: 7 <= k };
then consider k,l such that A1: P = [ k,l ] and A2: 7 <= k;
consider w being Nat such that A3: k = ( 7 + w ) by A2,NAT_1:10;
w in ( NAT ) by ORDINAL1:def 12;
hence thesis by A1,A3,MCART_1:7;
end;
