environ
vocabularies NUMBERS,ORDINAL1,SUBSET_1,XXREAL_0,ARYTM_3,XBOOLE_0,TARSKI,FINSET_1,CARD_1,MCART_1,ZFMISC_1,CQC_LANG,QC_LANG1,XBOOLEAN,BVFUNC_2,FUNCT_1,FINSEQ_1,NAT_1,RELAT_1,ORDINAL4,ARYTM_1,CQC_THE1;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,ORDINAL1,XCMPLX_0,FUNCT_1,NUMBERS,NAT_1,FINSET_1,FINSEQ_1,MCART_1,QC_LANG1,CQC_LANG,XXREAL_0,CKB5;
definitions TARSKI,XBOOLE_0,CKB5;
theorems TARSKI,ZFMISC_1,FINSET_1,FINSEQ_1,MCART_1,FUNCT_1,NAT_1,XBOOLE_0,XBOOLE_1,XREAL_1,XXREAL_0,ORDINAL1,RELAT_1,CKB1,CKB2,CKB3,CKB4,CKB5;
schemes NAT_1,FRAENKEL,XBOOLE_0;
registrations SUBSET_1,RELSET_1,FINSET_1,XXREAL_0,XREAL_0,FINSEQ_1,CQC_LANG,ORDINAL1,CARD_1;
constructors XXREAL_0,XREAL_0,NAT_1,CQC_LANG,CKB5;
requirements NUMERALS,REAL,BOOLE,SUBSET,ARITHM;
begin
reserve T for (Subset of ( CQC-WFF ));
reserve S for (Subset of ( CQC-WFF ));
reserve p for (Element of ( CQC-WFF ));
reserve q for (Element of ( CQC-WFF ));
reserve r for (Element of ( CQC-WFF ));
reserve s for QC-formula;
reserve x for bound_QC-variable;
reserve y for bound_QC-variable;
theorem
((T is  being_a_theory & S is  being_a_theory) implies ( T /\ S ) is  being_a_theory)
proof
assume that
A1: T is  being_a_theory
and
A2: S is  being_a_theory;
(( VERUM ) in T & ( VERUM ) in S) by A1,A2,CKB5:def 1;
hence ( VERUM ) in ( T /\ S ) by XBOOLE_0:def 4;
let p;
let q;
let r;
let s;
let x;
let y;
(( ( ( 'not' p ) => p ) => p ) in T & ( ( ( 'not' p ) => p ) => p ) in S) by A1,A2,CKB5:def 1;
hence ( ( ( 'not' p ) => p ) => p ) in ( T /\ S ) by XBOOLE_0:def 4;
(( p => ( ( 'not' p ) => q ) ) in T & ( p => ( ( 'not' p ) => q ) ) in S) by A1,A2,CKB5:def 1;
hence ( p => ( ( 'not' p ) => q ) ) in ( T /\ S ) by XBOOLE_0:def 4;
(( ( p => q ) => ( ( 'not' ( q '&' r ) ) => ( 'not' ( p '&' r ) ) ) ) in T & ( ( p => q ) => ( ( 'not' ( q '&' r ) ) => ( 'not' ( p '&' r ) ) ) ) in S) by A1,A2,CKB5:def 1;
hence ( ( p => q ) => ( ( 'not' ( q '&' r ) ) => ( 'not' ( p '&' r ) ) ) ) in ( T /\ S ) by XBOOLE_0:def 4;
(( ( p '&' q ) => ( q '&' p ) ) in T & ( ( p '&' q ) => ( q '&' p ) ) in S) by A1,A2,CKB5:def 1;
hence ( ( p '&' q ) => ( q '&' p ) ) in ( T /\ S ) by XBOOLE_0:def 4;
A3: ((p in T & ( p => q ) in T) implies q in T) by A1,CKB5:def 1;
((p in S & ( p => q ) in S) implies q in S) by A2,CKB5:def 1;
hence ((p in ( T /\ S ) & ( p => q ) in ( T /\ S )) implies q in ( T /\ S )) by A3,XBOOLE_0:def 4;
(( ( All (x,p) ) => p ) in T & ( ( All (x,p) ) => p ) in S) by A1,A2,CKB5:def 1;
hence ( ( All (x,p) ) => p ) in ( T /\ S ) by XBOOLE_0:def 4;
A4: ((( p => q ) in T & (not x in ( still_not-bound_in p ))) implies ( p => ( All (x,q) ) ) in T) by A1,CKB5:def 1;
((( p => q ) in S & (not x in ( still_not-bound_in p ))) implies ( p => ( All (x,q) ) ) in S) by A2,CKB5:def 1;
hence ((( p => q ) in ( T /\ S ) & (not x in ( still_not-bound_in p ))) implies ( p => ( All (x,q) ) ) in ( T /\ S )) by A4,XBOOLE_0:def 4;
A5: ((((( s . x ) in ( CQC-WFF ) & ( s . y ) in ( CQC-WFF )) & (not x in ( still_not-bound_in s ))) & ( s . x ) in T) implies ( s . y ) in T) by A1,CKB5:def 1;
((((( s . x ) in ( CQC-WFF ) & ( s . y ) in ( CQC-WFF )) & (not x in ( still_not-bound_in s ))) & ( s . x ) in S) implies ( s . y ) in S) by A2,CKB5:def 1;
hence thesis by A5,XBOOLE_0:def 4;
end;
