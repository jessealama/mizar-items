begin
reserve n for (Element of ( NAT ));
reserve S for RealNormSpace;
theorem
Th4: (for h being (PartFunc of ( REAL ),(the carrier of S)) holds (for seq being Real_Sequence holds (for r being Real holds (( rng seq ) c= ( dom h ) implies ( ( r (#) h ) /* seq ) = ( r * ( h /* seq ) )))))
proof
let h being (PartFunc of ( REAL ),(the carrier of S));
let seq being Real_Sequence;
let r being Real;
assume A1: ( rng seq ) c= ( dom h );
then A2: ( rng seq ) c= ( dom ( r (#) h ) ) by VFUNCT_1:def 4;
now
let n;
A3: ( seq . n ) in ( dom ( r (#) h ) ) by A2,CKB1:1;
thus ( ( ( r (#) h ) /* seq ) . n ) = ( ( r (#) h ) /. ( seq . n ) ) by A2,FUNCT_2:109
.= ( r * ( h /. ( seq . n ) ) ) by A3,VFUNCT_1:def 4
.= ( r * ( ( h /* seq ) . n ) ) by A1,FUNCT_2:109;
end;
hence thesis by NORMSP_1:def 5;
end;
