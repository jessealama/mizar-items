begin
reserve G for Group;
theorem
(for G being Group holds (G is  finite implies (for C being (Subgroup of G) holds (for A,B being (Subgroup of C) holds (for D being (Subgroup of A) holds (D = ( A /\ B ) implies (for E being (Subgroup of B) holds (E = ( A /\ B ) implies (for F being (Subgroup of C) holds (F = ( A /\ B ) implies (( index (C,A) ),( index (C,B) ) are_relative_prime  implies (( index (C,B) ) = ( index (A,D) ) & ( index (C,A) ) = ( index (B,E) )))))))))))))
proof
let G;
assume that
A1: G is  finite;
let C being (Subgroup of G);
let A being (Subgroup of C),B being (Subgroup of C);
let D being (Subgroup of A);
assume that
A2: D = ( A /\ B );
let E being (Subgroup of B);
assume that
A3: E = ( A /\ B );
let F being (Subgroup of C);
assume that
A4: F = ( A /\ B );
assume that
A5: ( index A ),( index B ) are_relative_prime ;
( index F ) = ( ( index E ) * ( index B ) ) by A1,A3,A4,GROUP_2:149;
then A6: ( ( index E ) * ( index B ) ) = ( ( index D ) * ( index A ) ) by A1,A2,A4,GROUP_2:149;
then (( index B ) qua Integer) divides ( (( index A ) qua Integer) * (( index D ) qua Integer) ) by INT_1:def 3;
then A7: (( index B ) qua Integer) divides (( index D ) qua Integer) by A5,INT_2:25;
(ex n being (Element of ( NAT )) st ( index D ) = ( ( index B ) * n ))
proof
consider i being Integer such that A8: ( index D ) = ( (( index B ) qua Integer) * i ) by A7,INT_1:def 3;
( 0 ) <= i by A1,A8,CKB23:1;
then reconsider i as (Element of ( NAT )) by INT_1:3;
take i;
thus thesis by A8;
end;
then A9: ( index B ) divides ( index D ) by NAT_D:def 3;
A10: ( index (C,B) ) >= ( index (A,D) ) by A1,A2,CKB22:1;
A11: ( index B ) = ( index D )
proof
assume A12: ( index B ) <> ( index D );
( index D ) > ( 0 ) by A1,CKB23:1;
then ( index B ) <= ( index D ) by A9,NAT_D:7;
hence contradiction by A10,A12,XXREAL_0:1;
end;
(( index A ) qua Integer) divides ( (( index B ) qua Integer) * (( index E ) qua Integer) ) by A6,INT_1:def 3;
then A13: (( index A ) qua Integer) divides ( index E ) by A5,INT_2:25;
(ex n being (Element of ( NAT )) st ( index E ) = ( ( index A ) * n ))
proof
consider i being Integer such that A14: ( index E ) = ( (( index A ) qua Integer) * i ) by A13,INT_1:def 3;
( 0 ) <= i by A1,A14,CKB23:1;
then reconsider i as (Element of ( NAT )) by INT_1:3;
take i;
thus thesis by A14;
end;
then A15: ( index A ) divides ( index E ) by NAT_D:def 3;
A16: ( index A ) >= ( index E ) by A1,A3,CKB22:1;
( index A ) = ( index E )
proof
assume A17: ( index A ) <> ( index E );
( index E ) > ( 0 ) by A1,CKB23:1;
then ( index A ) <= ( index E ) by A15,NAT_D:7;
hence contradiction by A16,A17,XXREAL_0:1;
end;
hence thesis by A11;
end;
