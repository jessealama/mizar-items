theorem
(for G being Group holds (for H being (Subgroup of G) holds (for h being (Element of G) holds (h in H implies ( H * h ) c= (the carrier of H)))))
proof
let G being Group;
let H being (Subgroup of G);
let h being (Element of G);
assume that
A1: h in H;
(for a being set holds (a in ( H * h ) implies a in (the carrier of H)))
proof
let a being set;
assume a in ( H * h );
then consider g being (Element of G) such that A2: a = ( g * h ) and A3: g in H by GROUP_2:104;
( g * h ) in H by A1,A3,GROUP_2:50;
hence thesis by A2,STRUCT_0:def 5;
end;
hence thesis by TARSKI:def 3;
