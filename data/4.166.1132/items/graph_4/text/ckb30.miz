begin
theorem
Lm2: (for p being FinSequence holds (for m,n being (Element of ( NAT )) holds (((1 <= m & m <= ( n + 1 )) & n <= ( len p )) implies (( ( len ( (m,n) -cut p ) ) + m ) = ( n + 1 ) & (for i being (Element of ( NAT )) holds (i < ( len ( (m,n) -cut p ) ) implies ( ( (m,n) -cut p ) . ( i + 1 ) ) = ( p . ( m + i ) )))))))
proof
let p being FinSequence;
let m being (Element of ( NAT )),n being (Element of ( NAT ));
assume that
A1: 1 <= m
and
A2: m <= ( n + 1 )
and
A3: n <= ( len p );
A4: (m = ( n + 1 ) or m < ( n + 1 )) by A2,XXREAL_0:1;
per cases  by A4,NAT_1:13;
suppose A5: m = ( n + 1 );

then A6: n < m by XREAL_1:29;
A7: (not ((1 <= m & m <= n) & n <= ( len p ))) by A5,XREAL_1:29;
( (m,n) -cut p ) = ( {} ) by A6,GRAPH_2:def 1;
hence ( ( len ( (m,n) -cut p ) ) + m ) = ( n + 1 ) by A5,CARD_1:27;
thus thesis by A7,CARD_1:27,GRAPH_2:def 1;
end;
suppose m <= n;

hence thesis by A1,A3,GRAPH_2:def 1;
end;
end;
