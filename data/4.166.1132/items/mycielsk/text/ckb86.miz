begin
registration
let n being Nat;
cluster ( Mycielskian n ) ->  symmetric;
coherence
proof
defpred P[ Nat ]
 means
( Mycielskian $1 ) is  symmetric;
A1: P[ ( 0 ) ] by CKB84:1;
A2: (for n being Nat holds (P[ n ] implies P[ ( n + 1 ) ]))
proof
let n being Nat;
set cMRn = (the carrier of ( Mycielskian n ));
set iMRn = (the InternalRel of ( Mycielskian n ));
set cMRn1 = (the carrier of ( Mycielskian ( n + 1 ) ));
set iMRn1 = (the InternalRel of ( Mycielskian ( n + 1 ) ));
assume A3: P[ n ];
A4: ( Mycielskian ( n + 1 ) ) = ( Mycielskian ( Mycielskian n ) ) by CKB84:1;
set N = ( ( 3 * ( 2 |^ n ) ) -' 1 );
A5: cMRn1 = ( ( 2 * N ) + 1 ) by A4,CKB57:def 1;
A6: cMRn = N by CKB57:def 1;
let x being set;
let y being set;
assume that
A7: x in cMRn1
and
A8: y in cMRn1
and
A9: [ x,y ] in iMRn1;
reconsider xp1 = x,yp1 = y as Nat by A5,A7,A8,NECKLACE:3;
A10: [ xp1,yp1 ] in iMRn1 by A9;
per cases  by A10,A4,CKB72:1;
suppose (xp1 < N & yp1 < N);

then (xp1 in N & yp1 in N) by NAT_1:44;
then A11: [ yp1,xp1 ] in iMRn by A3,A6,CKB5:1,A9,A4,CKB74:1;
iMRn c= iMRn1 by A4,CKB73:1;
hence [ y,x ] in iMRn1 by A11;
end;
suppose that A12: xp1 < N
and
A13: N <= yp1
and
A14: yp1 < ( 2 * N );
consider yy being Nat such that A15: yp1 = ( N + yy ) by A13,NAT_1:10;
A16: xp1 in N by A12,NAT_1:44;
( yy + N ) < ( N + N ) by A14,A15;
then yy < N by XREAL_1:6;
then A17: yy in N by NAT_1:44;
[ x,yy ] in iMRn by A9,A16,A15,A4,CKB76:1;
then [ yy,x ] in iMRn by A17,A16,A6,A3,CKB5:1;
hence [ y,x ] in iMRn1 by A4,A10,A15,CKB75:1;
end;
suppose that A18: N <= xp1
and
A19: xp1 < ( 2 * N )
and
A20: yp1 < N;
consider xx being Nat such that A21: xp1 = ( N + xx ) by A18,NAT_1:10;
A22: yp1 in N by A20,NAT_1:44;
( xx + N ) < ( N + N ) by A21,A19;
then xx < N by XREAL_1:6;
then A23: xx in N by NAT_1:44;
[ xx,y ] in iMRn by A22,A9,A4,A21,CKB77:1;
then [ y,xx ] in iMRn by A23,A22,A6,A3,CKB5:1;
hence [ y,x ] in iMRn1 by A4,A10,A21,CKB75:1;
end;
suppose ((xp1 = ( 2 * N ) & N <= yp1) & yp1 < ( 2 * N ));

hence [ y,x ] in iMRn1 by A4,CKB78:1;
end;
suppose ((N <= xp1 & xp1 < ( 2 * N )) & yp1 = ( 2 * N ));

hence [ y,x ] in iMRn1 by A4,CKB78:1;
end;
end;
(for n being Nat holds P[ n ]) from NAT_1:sch 2(A1,A2);
hence thesis;
end;
end;
