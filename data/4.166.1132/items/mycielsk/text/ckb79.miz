begin
theorem
Th45: (for n being Nat holds (for R being (NatRelStr of n) holds (for S being (Subset of ( Mycielskian R )) holds (S = n implies R = ( subrelstr S )))))
proof
let n being Nat;
let R being (NatRelStr of n);
let S being (Subset of ( Mycielskian R ));
assume that
A1: S = n;
set cR = (the carrier of R);
set iR = (the InternalRel of R);
set sS = ( subrelstr S );
set csS = (the carrier of sS);
set isS = (the InternalRel of sS);
set MR = ( Mycielskian R );
set cMR = (the carrier of MR);
set iMR = (the InternalRel of MR);
A2: cR = n by CKB57:def 1;
A3: csS = n by A1,YELLOW_0:def 15;
A4: iR = isS
proof
thus iR c= isS
proof
let z being set;
assume A5: z in iR;
then consider x,y being set such that A6: x in cR and A7: y in cR and A8: z = [ x,y ] by ZFMISC_1:def 2;
cR c= cMR by CKB71:1;
then reconsider xMR = x,yMR = y as (Element of MR) by A6,A7;
reconsider xsS = x,ysS = y as (Element of sS) by A6,A7,CKB57:def 1,A3;
iR c= iMR by CKB73:1;
then xMR <= yMR by A5,A8,ORDERS_2:def 5;
then xsS <= ysS by A3,A2,A7,YELLOW_0:60;
hence z in isS by A8,ORDERS_2:def 5;
end;

thus isS c= iR
proof
let z being set;
assume A9: z in isS;
then consider x,y being set such that A10: x in csS and A11: y in csS and A12: z = [ x,y ] by ZFMISC_1:def 2;
cR c= cMR by CKB71:1;
then reconsider xMR = x,yMR = y as (Element of MR) by A10,A11,A2,A3;
reconsider xsS = x,ysS = y as (Element of sS) by A10,A11;
xsS <= ysS by A9,A12,ORDERS_2:def 5;
then xMR <= yMR by YELLOW_0:59;
then z in iMR by A12,ORDERS_2:def 5;
hence z in iR by A10,A11,A3,A12,CKB74:1;
end;

end;
thus R = ( subrelstr S ) by CKB57:def 1,A3,A4;
end;
