begin
theorem
Th43: (for n being Nat holds (for R being (NatRelStr of n) holds (for x,y being Nat holds ((y in n & [ ( x + n ),y ] in (the InternalRel of ( Mycielskian R ))) implies [ x,y ] in (the InternalRel of R)))))
proof
let n being Nat;
let R being (NatRelStr of n);
let a being Nat;
let b being Nat;
set cR = (the carrier of R);
set iR = (the InternalRel of R);
set MR = ( Mycielskian R );
set iMR = (the InternalRel of MR);
assume that
A1: b in n
and
A2: [ ( a + n ),b ] in iMR;
A3: iMR = ( ( ( ( iR \/ { [ x,( y + n ) ] where x is (Element of ( NAT )),y is (Element of ( NAT )): [ x,y ] in iR } ) \/ { [ ( x + n ),y ] where x is (Element of ( NAT )),y is (Element of ( NAT )): [ x,y ] in iR } ) \/ [: { ( 2 * n ) },( ( 2 * n ) \ n ) :] ) \/ [: ( ( 2 * n ) \ n ),{ ( 2 * n ) } :] ) by CKB70:def 1;
per cases  by A2,A3,CKB4:1;
suppose [ ( a + n ),b ] in iR;

then ( a + n ) in cR by ZFMISC_1:87;
then ( a + n ) in n by CKB57:def 1;
then ( a + n ) < n by NAT_1:44;
then a < ( n - n ) by XREAL_1:20;
then a < ( 0 );
hence [ a,b ] in iR;
end;
suppose [ ( a + n ),b ] in { [ x,( y + n ) ] where x is (Element of ( NAT )),y is (Element of ( NAT )): [ x,y ] in iR };

then consider x,y being (Element of ( NAT )) such that A4: [ ( a + n ),b ] = [ x,( y + n ) ] and A5: [ x,y ] in iR;
( a + n ) = x by A4,ZFMISC_1:27;
then ( a + n ) in cR by A5,ZFMISC_1:87;
then ( a + n ) in n by CKB57:def 1;
then ( a + n ) < n by NAT_1:44;
then a < ( n - n ) by XREAL_1:20;
then a < ( 0 );
hence [ a,b ] in iR;
end;
suppose [ ( a + n ),b ] in { [ ( x + n ),y ] where x is (Element of ( NAT )),y is (Element of ( NAT )): [ x,y ] in iR };

then consider x,y being (Element of ( NAT )) such that A6: [ ( a + n ),b ] = [ ( x + n ),y ] and A7: [ x,y ] in iR;
( a + n ) = ( x + n ) by A6,ZFMISC_1:27;
hence [ a,b ] in iR by A7,A6,ZFMISC_1:27;
end;
suppose [ ( a + n ),b ] in [: { ( 2 * n ) },( ( 2 * n ) \ n ) :];

then consider c,d being set such that c in { ( 2 * n ) } and A8: d in ( ( 2 * n ) \ n ) and A9: [ ( a + n ),b ] = [ c,d ] by ZFMISC_1:def 2;
b = d by A9,ZFMISC_1:27;
then n <= b by A8,CKB2:1;
hence [ a,b ] in iR by A1,NAT_1:44;
end;
suppose [ ( a + n ),b ] in [: ( ( 2 * n ) \ n ),{ ( 2 * n ) } :];

then consider c,d being set such that c in ( ( 2 * n ) \ n ) and A10: d in { ( 2 * n ) } and A11: [ ( a + n ),b ] = [ c,d ] by ZFMISC_1:def 2;
A12: d = ( 2 * n ) by A10,TARSKI:def 1;
d = b by A11,ZFMISC_1:27;
then ( n + n ) < n by A1,A12,NAT_1:44;
then n < ( n - n ) by XREAL_1:20;
then n < ( 0 );
hence [ a,b ] in iR;
end;
end;
