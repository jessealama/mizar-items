theorem
Th2: (for x,y,z being Nat holds (x in ( y \ z ) iff (z <= x & x < y)))
proof
let x being Nat;
let y being Nat;
let z being Nat;
hereby
assume A1: x in ( y \ z );
(not x in z) by A1,XBOOLE_0:def 5;
hence z <= x by NAT_1:44;
thus x < y by A1,NAT_1:44;
end;
assume (z <= x & x < y);
then (x in y & (not x in z)) by NAT_1:44;
hence x in ( y \ z ) by XBOOLE_0:def 5;
end;
