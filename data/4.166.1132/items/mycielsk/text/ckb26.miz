registrations SUBSET_1,XBOOLE_0,STRUCT_0,XCMPLX_0,XXREAL_0,XREAL_0,NAT_1,CARD_1,RELSET_1,PARTFUN1,ZFMISC_1,ORDINAL1,EQREL_1,NECKLA_3,RELAT_1,FUNCT_1,FUNCT_2,FINSET_1,DILWORTH,NUMBERS,NEWTON,NECKLACE,YELLOW_0,YELLOW14,ORDERS_2,CKB9,CKB10,CKB15,CKB16,CKB17,CKB18,CKB20,CKB21,CKB23,CKB24,CKB25;
constructors STRUCT_0,NECKLACE,NECKLA_3,RELSET_1,SETFAM_1,XXREAL_0,NAT_1,NAT_D,FUNCT_2,NEWTON,YELLOW_0,DILWORTH,DOMAIN_1,CKB8,CKB14,CKB19,CKB22;
requirements SUBSET,BOOLE,NUMERALS,ARITHM,REAL;
begin
registration
let R being  with_finite_cliquecover# RelStr;
let S being (Subset of R);
cluster ( subrelstr S ) ->  with_finite_cliquecover#;
coherence
proof
set sS = ( subrelstr S );
consider C being (Clique-partition of R) such that A1: C is  finite by CKB22:def 1;
A2: (the carrier of sS) = S by YELLOW_0:def 15;
reconsider CS = ( C | S ) as (a_partition of S);
(for x being set holds (x in CS implies x is (Clique of sS)))
proof
let x being set;
assume x in CS;
then consider X being (Element of C) such that A3: x = ( X /\ S ) and A4: X meets S;
(ex y being set st (y in X & y in S)) by A4,XBOOLE_0:3;
then X is (Clique of R) by DILWORTH:def 11;
hence x is (Clique of sS) by A3,DILWORTH:29;
end;
then CS is (Clique-partition of sS) by A2,DILWORTH:def 11;
hence thesis by A1,CKB22:def 1;
end;
end;
