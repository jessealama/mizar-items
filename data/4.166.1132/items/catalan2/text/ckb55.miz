let Fr2;
assume that
A1: ( dom Fr1 ) = ( dom Fr2 )
and
A2: (for n holds (n in ( len Fr1 ) implies ( Fr1 . n ) = ( Fr2 . ( ( len Fr1 ) -' ( 1 + n ) ) )));
