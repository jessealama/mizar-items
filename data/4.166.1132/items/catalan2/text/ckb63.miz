theorem
Th54: (for seq1 holds (for seq2 holds (for n holds (seq2 is  summable implies (ex Fr st ((( ( Partial_Sums ( seq1 (##) seq2 ) ) . n ) = ( ( ( Sum seq2 ) * ( ( Partial_Sums seq1 ) . n ) ) - ( Sum Fr ) ) & ( dom Fr ) = ( n + 1 )) & (for i holds (i in ( n + 1 ) implies ( Fr . i ) = ( ( seq1 . i ) * ( Sum ( seq2 ^\ ( ( n -' i ) + 1 ) ) ) )))))))))
proof
let seq1;
let seq2;
let n;
assume that
A1: seq2 is  summable;
defpred Q[ set,set ]
 means
(for i holds (i = $1 implies $2 = ( ( seq1 . i ) * ( Sum ( seq2 ^\ ( ( n -' i ) + 1 ) ) ) )));
set P2 = ( Partial_Sums seq2 );
set P1 = ( Partial_Sums seq1 );
set S = ( seq1 (##) seq2 );
A2: (for i holds (i in ( n + 1 ) implies (ex x being (Element of ( REAL )) st Q[ i,x ])))
proof
let i;
assume that
i in ( n + 1 );
take ( ( seq1 . i ) * ( Sum ( seq2 ^\ ( ( n -' i ) + 1 ) ) ) );
thus thesis;
end;
consider Fr such that A3: ( dom Fr ) = ( n + 1 ) and A4: (for i holds (i in ( n + 1 ) implies Q[ i,( Fr . i ) ])) from STIRL2_1:sch 5(A2);
consider Fr1 such that A5: ( ( Partial_Sums S ) . n ) = ( Sum Fr1 ) and A6: ( dom Fr1 ) = ( n + 1 ) and A7: (for i holds (i in ( n + 1 ) implies ( Fr1 . i ) = ( ( seq1 . i ) * ( P2 . ( n -' i ) ) ))) by CKB62:1;
A8: ( 0 ) in ( n + 1 ) by NAT_1:44;
then A9: (( Fr1 . ( 0 ) ) = ( ( seq1 . ( 0 ) ) * ( P2 . ( n -' ( 0 ) ) ) ) & ( Sum ( Fr1 | ( ( 0 ) + 1 ) ) ) = ( ( Fr1 . ( 0 ) ) + ( Sum ( Fr1 | (( 0 ) qua Ordinal) ) ) )) by A6,A7,AFINSQ_2:65;
defpred P[ Nat ]
 means
(( $1 + 1 ) <= ( n + 1 ) implies ( ( Sum ( Fr1 | ( $1 + 1 ) ) ) + ( Sum ( Fr | ( $1 + 1 ) ) ) ) = ( ( Sum seq2 ) * ( P1 . $1 ) ));
A10: (for k holds (P[ k ] implies P[ ( k + 1 ) ]))
proof
let k;
assume that
A11: P[ k ];
reconsider k1 = ( k + 1 ) as (Element of ( NAT ));
assume A12: ( ( k + 1 ) + 1 ) <= ( n + 1 );
then k1 < ( n + 1 ) by NAT_1:13;
then A13: k1 in ( n + 1 ) by NAT_1:44;
then A14: (( Fr . k1 ) = ( ( seq1 . k1 ) * ( Sum ( seq2 ^\ ( ( n -' k1 ) + 1 ) ) ) ) & ( Sum ( Fr1 | ( k1 + 1 ) ) ) = ( ( Fr1 . k1 ) + ( Sum ( Fr1 | k1 ) ) )) by A4,A6,AFINSQ_2:65;
A15: ( ( Sum ( Fr1 | k1 ) ) + ( Sum ( Fr | k1 ) ) ) = ( ( Sum seq2 ) * ( P1 . k ) ) by A12,A11,NAT_1:13;
A16: k in ( NAT ) by ORDINAL1:def 12;
(( Sum ( Fr | ( k1 + 1 ) ) ) = ( ( Fr . k1 ) + ( Sum ( Fr | k1 ) ) ) & ( Fr1 . k1 ) = ( ( seq1 . k1 ) * ( P2 . ( n -' k1 ) ) )) by A3,A7,A13,AFINSQ_2:65;
then ( ( Sum ( Fr | ( k1 + 1 ) ) ) + ( Sum ( Fr1 | ( k1 + 1 ) ) ) ) = ( ( ( seq1 . k1 ) * ( ( Sum ( seq2 ^\ ( ( n -' k1 ) + 1 ) ) ) + ( P2 . ( n -' k1 ) ) ) ) + ( ( Sum seq2 ) * ( P1 . k ) ) ) by A15,A14
.= ( ( ( seq1 . k1 ) * ( Sum seq2 ) ) + ( ( Sum seq2 ) * ( P1 . k ) ) ) by A1,SERIES_1:15
.= ( ( Sum seq2 ) * ( ( P1 . k ) + ( seq1 . k1 ) ) )
.= ( ( P1 . k1 ) * ( Sum seq2 ) ) by A16,SERIES_1:def 1;
hence thesis;
end;
(( Sum ( Fr | ( ( 0 ) + 1 ) ) ) = ( ( Fr . ( 0 ) ) + ( Sum ( Fr | (( 0 ) qua Ordinal) ) ) ) & ( Fr . ( 0 ) ) = ( ( seq1 . ( 0 ) ) * ( Sum ( seq2 ^\ ( ( n -' ( 0 ) ) + 1 ) ) ) )) by A3,A4,A8,AFINSQ_2:65;
then ( ( Sum ( Fr | ( ( 0 ) + 1 ) ) ) + ( Sum ( Fr1 | ( ( 0 ) + 1 ) ) ) ) = ( ( seq1 . ( 0 ) ) * ( ( Sum ( seq2 ^\ ( ( n -' ( 0 ) ) + 1 ) ) ) + ( P2 . ( n -' ( 0 ) ) ) ) ) by A9
.= ( ( seq1 . ( 0 ) ) * ( Sum seq2 ) ) by A1,SERIES_1:15;
then A17: P[ ( 0 ) ] by SERIES_1:def 1;
A18: (for k holds P[ k ]) from NAT_1:sch 2(A17,A10);
take Fr;
A19: ( Fr1 | ( n + 1 ) ) = Fr1 by A6,RELAT_1:69;
( Fr | ( n + 1 ) ) = Fr by A3,RELAT_1:69;
