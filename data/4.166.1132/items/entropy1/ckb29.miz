begin
reserve i for (Element of ( NAT ));
reserve j for (Element of ( NAT ));
theorem
Th22: (for p being non empty (FinSequence of ( REAL )) holds (for q being (FinSequence of ( REAL )) holds (for M being (Matrix of ( REAL )) holds (M = ( ( ColVec2Mx p ) * ( LineVec2Mx q ) ) iff ((( len M ) = ( len p ) & ( width M ) = ( len q )) & (for i holds (i in ( dom M ) implies ( Line (M,i) ) = ( ( p . i ) * q ))))))))
proof
let p being non empty (FinSequence of ( REAL ));
let q being (FinSequence of ( REAL ));
let M being (Matrix of ( REAL ));
set M1 = ( ColVec2Mx p );
set M2 = ( LineVec2Mx q );
hereby
assume A1: M = ( M1 * M2 );
hence (( len M ) = ( len p ) & ( width M ) = ( len q )) by CKB28:1;
thus (for i holds (i in ( dom M ) implies ( Line (M,i) ) = ( ( p . i ) * q )))
proof
let i;
assume i in ( dom M );
then A2: i in ( Seg ( len M ) ) by FINSEQ_1:def 3;
A3: (for j being Nat holds (j in ( dom ( Line (M,i) ) ) implies ( ( Line (M,i) ) . j ) = ( ( ( p . i ) * q ) . j )))
proof
let j being Nat;
assume j in ( dom ( Line (M,i) ) );
then j in ( Seg ( len ( Line (M,i) ) ) ) by FINSEQ_1:def 3;
then A4: j in ( Seg ( width M ) ) by MATRIX_1:def 7;
then A5: [ i,j ] in ( Indices M ) by A2,MATRPROB:12;
A6: j in ( NAT ) by ORDINAL1:def 12;
thus ( ( Line (M,i) ) . j ) = ( M * (i,j) ) by A4,MATRIX_1:def 7
.= ( ( p . i ) * ( q . j ) ) by A1,A6,A5,CKB28:1
.= ( ( ( p . i ) * q ) . j ) by RVSUM_1:44;
end;
( dom ( Line (M,i) ) ) = ( Seg ( len ( Line (M,i) ) ) ) by FINSEQ_1:def 3
.= ( Seg ( width M ) ) by MATRIX_1:def 7
.= ( Seg ( len q ) ) by A1,CKB28:1
.= ( dom q ) by FINSEQ_1:def 3
.= ( dom ( ( p . i ) * q ) ) by VALUED_1:def 5;
hence thesis by A3,FINSEQ_1:13;
end;

end;
assume that
A7: ( len M ) = ( len p )
and
A8: ( width M ) = ( len q )
and
A9: (for i holds (i in ( dom M ) implies ( Line (M,i) ) = ( ( p . i ) * q )));
(for i holds (for j holds ([ i,j ] in ( Indices M ) implies ( M * (i,j) ) = ( ( p . i ) * ( q . j ) ))))
proof
let i;
let j;
assume that
A10: [ i,j ] in ( Indices M );
A11: i in ( dom M ) by A10,MATRPROB:13;
j in ( Seg ( width M ) ) by A10,MATRPROB:12;
hence ( M * (i,j) ) = ( ( Line (M,i) ) . j ) by MATRIX_1:def 7
.= ( ( ( p . i ) * q ) . j ) by A9,A11
.= ( ( p . i ) * ( q . j ) ) by RVSUM_1:44;
end;
hence thesis by A7,A8,CKB28:1;
end;
