begin
reserve k for (Element of ( NAT ));
reserve p for (FinSequence of ( REAL ));
theorem
Th62: (for M being non  empty-yielding  Conditional_Probability (Matrix of ( REAL )) holds (for p being (FinSequence of ( REAL )) holds (p = ( Entropy_of_Cond_Prob M ) iff (( len p ) = ( len M ) & (for k holds (k in ( dom p ) implies ( p . k ) = ( - ( Sum ( ( Infor_FinSeq_of M ) . k ) ) )))))))
proof
let M being non  empty-yielding  Conditional_Probability (Matrix of ( REAL ));
let p being (FinSequence of ( REAL ));
A1: ( len ( Infor_FinSeq_of M ) ) = ( len M ) by CKB66:def 1;
hereby
assume A2: p = ( Entropy_of_Cond_Prob M );
then A3: ( len p ) = ( len M ) by CKB78:def 1;
then A4: ( dom p ) = ( dom M ) by FINSEQ_3:29;
now
let k;
assume that
A5: k in ( dom p );
A6: k in ( dom ( Infor_FinSeq_of M ) ) by A1,A3,A5,FINSEQ_3:29;
thus ( p . k ) = ( Entropy ( Line (M,k) ) ) by A2,A5,CKB78:def 1
.= ( - ( Sum ( Line (( Infor_FinSeq_of M ),k) ) ) ) by A4,A5,CKB67:1
.= ( - ( Sum ( ( Infor_FinSeq_of M ) . k ) ) ) by A6,MATRIX_2:16;
end;
hence (( len p ) = ( len M ) & (for k holds (k in ( dom p ) implies ( p . k ) = ( - ( Sum ( ( Infor_FinSeq_of M ) . k ) ) )))) by A2,CKB78:def 1;
end;
assume that
A7: ( len p ) = ( len M )
and
A8: (for k holds (k in ( dom p ) implies ( p . k ) = ( - ( Sum ( ( Infor_FinSeq_of M ) . k ) ) )));
A9: ( dom p ) = ( dom M ) by A7,FINSEQ_3:29;
A10: ( dom ( Infor_FinSeq_of M ) ) = ( dom M ) by A1,FINSEQ_3:29;
now
let k;
assume that
A11: k in ( dom p );
thus ( p . k ) = ( - ( Sum ( ( Infor_FinSeq_of M ) . k ) ) ) by A8,A11
.= ( - ( Sum ( Line (( Infor_FinSeq_of M ),k) ) ) ) by A10,A9,A11,MATRIX_2:16
.= ( Entropy ( Line (M,k) ) ) by A9,A11,CKB67:1;
end;
hence thesis by A7,CKB78:def 1;
end;
