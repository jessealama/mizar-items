begin
reserve D for non  empty set;
reserve k for (Element of ( NAT ));
reserve n for Nat;
reserve p for (FinSequence of ( REAL ));
theorem
Th29: (for M being (Matrix of D) holds (for p being (FinSequence of ( D * )) holds (((( len p ) = ( len M ) & ( p . 1 ) = ( M . 1 )) & (for k holds ((k >= 1 & k < ( len M )) implies ( p . ( k + 1 ) ) = ( ( p . k ) ^ ( M . ( k + 1 ) ) )))) implies (for k holds (k in ( dom p ) implies ( len ( p . k ) ) = ( k * ( width M ) ))))))
proof
let M being (Matrix of D);
let p being (FinSequence of ( D * ));
assume that
A1: ( len p ) = ( len M )
and
A2: ( p . 1 ) = ( M . 1 )
and
A3: (for k holds ((k >= 1 & k < ( len M )) implies ( p . ( k + 1 ) ) = ( ( p . k ) ^ ( M . ( k + 1 ) ) )));
defpred P[ (Element of ( NAT )) ]
 means
(($1 >= 1 & $1 <= ( len M )) implies ( len ( p . $1 ) ) = ( $1 * ( width M ) ));
A4: (for n being (Element of ( NAT )) holds (P[ n ] implies P[ ( n + 1 ) ]))
proof
let n being (Element of ( NAT ));
assume that
A5: ((n >= 1 & n <= ( len M )) implies ( len ( p . n ) ) = ( n * ( width M ) ));
assume that
A6: ( n + 1 ) >= 1
and
A7: ( n + 1 ) <= ( len M );
now
per cases ;
suppose A8: n = ( 0 );

then 1 in ( dom M ) by A7,FINSEQ_3:25;
hence thesis by A2,A8,GOBRD13:3;
end;
suppose A9: n <> ( 0 );

A10: ( n + 1 ) in ( dom M ) by A6,A7,FINSEQ_3:25;
n < ( len M ) by A7,NAT_1:13;
then ( p . ( n + 1 ) ) = ( ( p . n ) ^ ( M . ( n + 1 ) ) ) by A3,A9,NAT_1:14;
then ( len ( p . ( n + 1 ) ) ) = ( ( len ( p . n ) ) + ( len ( M . ( n + 1 ) ) ) ) by FINSEQ_1:22
.= ( ( n * ( width M ) ) + ( width M ) ) by A5,A7,A9,A10,GOBRD13:3,NAT_1:13,NAT_1:14
.= ( ( n + 1 ) * ( width M ) );
hence thesis;
end;
end;
hence thesis;
end;
A11: P[ ( 0 ) ];
A12: (for n being (Element of ( NAT )) holds P[ n ]) from NAT_1:sch 1(A11,A4);
let k;
assume k in ( dom p );
then A13: k in ( Seg ( len p ) ) by FINSEQ_1:def 3;
then A14: k <= ( len p ) by FINSEQ_1:1;
k >= 1 by A13,FINSEQ_1:1;
hence thesis by A1,A12,A14;
end;
