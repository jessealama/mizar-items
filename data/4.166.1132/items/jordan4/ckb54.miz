begin
theorem
(for f being non constant  standard special_circular_sequence holds (for g being (FinSequence of ( TOP-REAL 2 )) holds (for i1,i2 being (Element of ( NAT )) holds ((g is_a_part_of f,i1,i2 & ( g . 1 ) <> ( g . ( len g ) )) implies ( L~ g ) is_S-P_arc_joining ( f /. i1 ),( f /. i2 )))))
proof
let f being non constant  standard special_circular_sequence;
let g being (FinSequence of ( TOP-REAL 2 ));
let i1 being (Element of ( NAT )),i2 being (Element of ( NAT ));
assume that
A1: g is_a_part_of f,i1,i2
and
A2: ( g . 1 ) <> ( g . ( len g ) );
now
per cases  by A1,CKB27:def 1;
case A3: g is_a_part>_of f,i1,i2;
then ( i1 + 1 ) <= ( len f ) by CKB25:def 1;
then A4: ( ( i1 + 1 ) - 1 ) <= ( ( len f ) - 1 ) by XREAL_1:9;
A5: 1 <= i1 by A3,CKB25:def 1;
A6: 1 <= ( len g ) by A3,CKB25:def 1;
( len g ) < ( len f ) by A3,CKB25:def 1;
then A7: i1 <= ( ( len f ) -' 1 ) by A6,A4,XREAL_1:233,XXREAL_0:2;
now
assume A8: i1 = i2;
( g . 1 ) = ( f . ( S_Drop (( ( i1 + 1 ) -' 1 ),f) ) ) by A3,A6,CKB25:def 1
.= ( f . ( S_Drop (i1,f) ) ) by NAT_D:34
.= ( f . i1 ) by A5,A7,CKB23:1;
hence contradiction by A2,A3,A8,CKB25:def 1;
end;
hence thesis by A1,CKB53:1;
end;
case A9: g is_a_part<_of f,i1,i2;
then A10: 1 <= i1 by CKB26:def 1;
A11: 1 <= ( len g ) by A9,CKB26:def 1;
A12: ( len g ) < ( len f ) by A9,CKB26:def 1;
( i1 + 1 ) <= ( len f ) by A9,CKB26:def 1;
then ( ( i1 + 1 ) - 1 ) <= ( ( len f ) - 1 ) by XREAL_1:9;
then A13: i1 <= ( ( len f ) -' 1 ) by A11,A12,XREAL_1:233,XXREAL_0:2;
now
assume A14: i1 = i2;
( g . 1 ) = ( f . ( S_Drop (( ( ( len f ) + i1 ) -' 1 ),f) ) ) by A9,A11,CKB26:def 1
.= ( f . ( S_Drop (( ( ( ( ( len f ) -' 1 ) + 1 ) + i1 ) -' 1 ),f) ) ) by A11,A12,XREAL_1:235,XXREAL_0:2
.= ( f . ( S_Drop (( ( ( ( ( len f ) -' 1 ) + i1 ) + 1 ) -' 1 ),f) ) )
.= ( f . ( S_Drop (( ( ( len f ) -' 1 ) + i1 ),f) ) ) by NAT_D:34
.= ( f . ( S_Drop (i1,f) ) ) by CKB24:1
.= ( f . i1 ) by A10,A13,CKB23:1;
hence contradiction by A2,A9,A14,CKB26:def 1;
end;
hence thesis by A1,CKB53:1;
end;
end;
hence thesis;
end;
