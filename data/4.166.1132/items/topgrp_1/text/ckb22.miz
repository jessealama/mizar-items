requirements SUBSET,BOOLE,NUMERALS;
begin
reserve G for Group;
reserve a for (Element of G);
registration
let G being Group;
let a being (Element of G);
cluster ( a * ) ->  one-to-one  onto;
coherence
proof
set f = ( a * );
A1: ( dom f ) = (the carrier of G) by FUNCT_2:def 1;
hereby
let x1 being set;
let x2 being set;
assume that
A2: (x1 in ( dom f ) & x2 in ( dom f ))
and
A3: ( f . x1 ) = ( f . x2 );
reconsider y1 = x1,y2 = x2 as (Element of G) by A2;
A4: (( f . y1 ) = ( a * y1 ) & ( f . y2 ) = ( a * y2 )) by CKB20:def 1;
thus x1 = ( ( 1_ G ) * y1 ) by GROUP_1:def 4
.= ( ( ( a " ) * a ) * y1 ) by GROUP_1:def 5
.= ( ( a " ) * ( a * y1 ) ) by GROUP_1:def 3
.= ( ( ( a " ) * a ) * y2 ) by A3,A4,GROUP_1:def 3
.= ( ( 1_ G ) * y2 ) by GROUP_1:def 5
.= x2 by GROUP_1:def 4;
end;
thus ( rng f ) c= (the carrier of G);
let y being set;
assume y in (the carrier of G);
then reconsider y1 = y as (Element of G);
( f . ( ( a " ) * y1 ) ) = ( a * ( ( a " ) * y1 ) ) by CKB20:def 1
.= ( ( a * ( a " ) ) * y1 ) by GROUP_1:def 3
.= ( ( 1_ G ) * y1 ) by GROUP_1:def 5
.= y by GROUP_1:def 4;
hence thesis by A1,FUNCT_1:def 3;
end;
end;
