begin
theorem
Th5: (for L being  antisymmetric RelStr holds (for R being  auxiliary(i) (Relation of L) holds (for C being (strict_chain of R) holds (for x,y being (Element of L) holds (((x in C & y in C) & x < y) implies [ x,y ] in R)))))
proof
let L being  antisymmetric RelStr;
let R being  auxiliary(i) (Relation of L);
let C being (strict_chain of R);
let x being (Element of L);
let y being (Element of L);
assume that
A1: x in C
and
A2: y in C
and
A3: x < y;
([ x,y ] in R or [ y,x ] in R) by A1,A2,A3,CKB14:def 1;
then ([ x,y ] in R or y <= x) by WAYBEL_4:def 3;
hence thesis by A3,ORDERS_2:6;
end;
