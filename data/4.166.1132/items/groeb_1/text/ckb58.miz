begin
theorem
(for n being (Element of ( NAT )) holds (for T being  connected  admissible (TermOrder of n) holds (for L being  Abelian  add-associative  right_complementable  right_zeroed  commutative  associative  well-unital  distributive  almost_left_invertible non  degenerated non  empty doubleLoopStr holds (for I being  add-closed  left-ideal non  empty (Subset of ( Polynom-Ring (n,L) )) holds (for G being (Subset of ( Polynom-Ring (n,L) )) holds (for p being (Polynomial of n,L) holds (for q being  non-zero (Polynomial of n,L) holds ((((p in G & q in G) & p <> q) & ( HT (q,T) ) divides ( HT (p,T) )) implies (G is_Groebner_basis_of I,T implies ( G \ { p } ) is_Groebner_basis_of I,T)))))))))
proof
let n being (Element of ( NAT ));
let T being  connected  admissible (TermOrder of n);
let L being  Abelian  add-associative  right_complementable  right_zeroed  commutative  associative  well-unital  distributive  almost_left_invertible non  degenerated non  empty doubleLoopStr;
let I being  add-closed  left-ideal non  empty (Subset of ( Polynom-Ring (n,L) ));
let G being (Subset of ( Polynom-Ring (n,L) ));
let p being (Polynomial of n,L);
let q being  non-zero (Polynomial of n,L);
assume that
A1: p in G
and
A2: q in G
and
A3: p <> q
and
A4: ( HT (q,T) ) divides ( HT (p,T) );
reconsider GG = G as non  empty (Subset of ( Polynom-Ring (n,L) )) by A1;
assume A5: G is_Groebner_basis_of I,T;
set G9 = ( G \ { p } );
A6: (not q in { p }) by A3,TARSKI:def 1;
then (q <> ( 0_ (n,L) ) & q in G9) by A2,POLYNOM7:def 1,XBOOLE_0:def 5;
then A7: ( HT (q,T) ) in { ( HT (u,T) ) where u is (Polynomial of n,L): (u in G9 & u <> ( 0_ (n,L) )) };
GG c= ( GG -Ideal ) by IDEAL_1:def 14;
then A8: G c= I by A5,CKB30:def 1;
(for f being (Polynomial of n,L) holds (f in I implies ( PolyRedRel (G,T) ) reduces f,( 0_ (n,L) ))) by A1,A5,CKB36:1;
then (for f being  non-zero (Polynomial of n,L) holds (f in I implies f is_reducible_wrt G,T)) by CKB37:1;
then A9: (for f being  non-zero (Polynomial of n,L) holds (f in I implies f is_top_reducible_wrt G,T)) by A8,CKB38:1;
(for b being (bag of n) holds (b in ( HT (I,T) ) implies (ex b9 being (bag of n) st (b9 in ( HT (G9,T) ) & b9 divides b))))
proof
let b being (bag of n);
assume b in ( HT (I,T) );
then consider bb being (bag of n) such that A10: bb in ( HT (G,T) ) and A11: bb divides b by A9,CKB39:1;
consider r being (Polynomial of n,L) such that A12: bb = ( HT (r,T) ) and A13: r in G and A14: r <> ( 0_ (n,L) ) by A10;
now
per cases ;
case r = p;
hence thesis by A4,A7,A11,A12,CKB46:1;
end;
case r <> p;
then (not r in { p }) by TARSKI:def 1;
then r in G9 by A13,XBOOLE_0:def 5;
then bb in { ( HT (u,T) ) where u is (Polynomial of n,L): (u in G9 & u <> ( 0_ (n,L) )) } by A12,A14;
hence thesis by A11;
end;
end;
hence thesis;
end;
then A15: ( HT (I,T) ) c= ( multiples ( HT (G9,T) ) ) by CKB40:1;
G9 c= G by XBOOLE_1:36;
then A16: G9 c= I by A8,XBOOLE_1:1;
G9 <> ( {} ) by A2,A6,XBOOLE_0:def 5;
hence thesis by A16,A15,CKB41:1;
end;
