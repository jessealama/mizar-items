theorem
Lm9: (for L being  add-associative  left_zeroed  right_zeroed  right_complementable  associative  distributive  well-unital non  empty doubleLoopStr holds (for A,B being non  empty (Subset of L) holds (B = ( A \ { ( 0. L ) } ) implies (for f being (LinearCombination of A) holds (for u being set holds (u = ( Sum f ) implies (ex g being (LinearCombination of B) st u = ( Sum g ))))))))
proof
let L being  add-associative  left_zeroed  right_zeroed  right_complementable  associative  distributive  well-unital non  empty doubleLoopStr;
let A being non  empty (Subset of L);
let B being non  empty (Subset of L);
defpred P[ (Element of ( NAT )) ]
