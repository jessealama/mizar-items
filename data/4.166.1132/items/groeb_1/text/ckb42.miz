A1: ( 0_ (n,L) ) = ( 0. ( Polynom-Ring (n,L) ) ) by POLYNOM1:def 10;
now
let a being set;
let b being set;
let c being set;
assume that
A2: [ a,b ] in R
and
[ a,c ] in R;
