begin
reserve T for  Reflexive  symmetric  triangle non  empty MetrStruct;
reserve P for (Subset of T);
reserve s for Real;
reserve x for set;
reserve y for set;
theorem
(for P being (Subset of T) holds (P = { x } implies ( diameter P ) = ( 0 )))
proof
let P being (Subset of T);
assume A1: P = { x };
then A2: x in P by TARSKI:def 1;
then reconsider t1 = x as (Element of T);
((for x,y being (Point of T) holds ((x in P & y in P) implies ( dist (x,y) ) <= ( 0 ))) & (for s holds ((for x,y being (Point of T) holds ((x in P & y in P) implies ( dist (x,y) ) <= s)) implies ( 0 ) <= s)))
proof
thus (for x,y being (Point of T) holds ((x in P & y in P) implies ( dist (x,y) ) <= ( 0 )))
proof
let x being (Point of T);
let y being (Point of T);
assume that
A3: x in P
and
A4: y in P;
x = t1 by A1,A3,TARSKI:def 1;
then ( dist (x,y) ) = ( dist (t1,t1) ) by A1,A4,TARSKI:def 1
.= ( 0 ) by METRIC_1:1;
hence thesis;
end;

let s;
assume (for x,y being (Point of T) holds ((x in P & y in P) implies ( dist (x,y) ) <= s));
then ( dist (t1,t1) ) <= s by A2;
hence thesis by METRIC_1:1;
end;
hence thesis by A1,CKB39:def 1;
end;
