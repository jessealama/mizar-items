theorem
Th19: (for X,Y being non empty set holds (for T being non empty Poset holds (for S1 being  full non empty (SubRelStr of ( ( T |^ X ) |^ Y )) holds (for S2 being  full non empty (SubRelStr of ( ( T |^ Y ) |^ X )) holds (for F being (Function of S1,S2) holds (F is  commuting implies F is  monotone))))))
proof
let X being non empty set,Y being non empty set;
let T being non empty Poset;
let S1 being  full non empty (SubRelStr of ( ( T |^ X ) |^ Y ));
let S2 being  full non empty (SubRelStr of ( ( T |^ Y ) |^ X ));
let F being (Function of S1,S2);
assume that
(for x being set holds (x in ( dom F ) implies x is  Function-yielding  Function-yielding Function))
and
A1: (for f being Function holds (f in ( dom F ) implies ( F . f ) = ( commute f )));
let f being (Element of S1),g being (Element of S1);
A2: ( dom F ) = (the carrier of S1) by FUNCT_2:def 1;
then A3: ( F . g ) = ( commute g ) by A1;
reconsider Fa = ( F . f ),Fb = ( F . g ) as (Element of ( ( T |^ Y ) |^ X )) by YELLOW_0:58;
reconsider a = f,b = g as (Element of ( ( T |^ X ) |^ Y )) by YELLOW_0:58;
A4: (the carrier of ( ( T |^ X ) |^ Y )) = ( Funcs (Y,(the carrier of ( T |^ X ))) ) by YELLOW_1:28
.= ( Funcs (Y,( Funcs (X,(the carrier of T)) )) ) by YELLOW_1:28;
assume f <= g;
then A5: a <= b by YELLOW_0:59;
A6: ( F . f ) = ( commute a ) by A2,A1;
now
let x being (Element of X);
now
let y being (Element of Y);
A7: ( ( Fa . x ) . y ) = ( ( a . y ) . x ) by A4,A6,FUNCT_6:56;
A8: ( ( Fb . x ) . y ) = ( ( b . y ) . x ) by A4,A3,FUNCT_6:56;
( a . y ) <= ( b . y ) by A5,CKB28:1;
hence ( ( Fa . x ) . y ) <= ( ( Fb . x ) . y ) by A7,A8,CKB28:1;
end;
hence ( Fa . x ) <= ( Fb . x ) by CKB28:1;
end;
then Fa <= Fb by CKB28:1;
hence thesis by YELLOW_0:60;
end;
