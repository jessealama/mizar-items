begin
reserve Y for non empty set;
theorem
(for a,b being (Element of ( Funcs (Y,( BOOLEAN )) )) holds (for G being (Subset of ( PARTITIONS Y )) holds (for P being (a_partition of Y) holds (a '<' b implies ( Ex (a,P,G) ) '<' ( Ex (b,P,G) )))))
proof
let a being (Element of ( Funcs (Y,( BOOLEAN )) )),b being (Element of ( Funcs (Y,( BOOLEAN )) ));
let G being (Subset of ( PARTITIONS Y ));
let P being (a_partition of Y);
A1: ( Ex (b,P,G) ) = ( Ex (( 'not' ( 'not' b ) ),P,G) )
.= ( 'not' ( All (( 'not' b ),P,G) ) ) by BVFUNC_2:18;
assume a '<' b;
then ( 'not' b ) '<' ( 'not' a ) by CKB12:1;
then A2: ( All (( 'not' b ),P,G) ) '<' ( All (( 'not' a ),P,G) ) by CKB13:1;
( Ex (a,P,G) ) = ( Ex (( 'not' ( 'not' a ) ),P,G) )
.= ( 'not' ( All (( 'not' a ),P,G) ) ) by BVFUNC_2:18;
hence thesis by A1,A2,CKB12:1;
end;
