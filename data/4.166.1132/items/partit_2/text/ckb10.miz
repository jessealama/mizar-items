begin
reserve Y for non  empty set;
theorem
Th9: (for Y being set holds (for R being (Relation of Y) holds (R is_reflexive_in Y implies Y = ( field R ))))
proof
let Y being set;
let R being (Relation of Y);
assume R is_reflexive_in Y;
hence Y c= ( field R ) by CKB9:1;
( field R ) c= ( Y \/ Y ) by RELSET_1:8;
hence thesis;
end;
