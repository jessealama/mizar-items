begin
theorem
Th20: (for X being non empty set holds (for Y being (Subset-Family of X) holds Y is (prebasis of TopStruct (# X,( UniCl ( FinMeetCl Y ) ) #))))
proof
let X being non empty set;
let A being (Subset-Family of X);
set T = TopStruct (# X,( UniCl ( FinMeetCl A ) ) #);
reconsider A9 = A as (Subset-Family of T);
now
(A9 c= ( FinMeetCl A ) & ( FinMeetCl A ) c= (the topology of T)) by CKB6:1,CKB10:1;
then A9 c= (the topology of T) by XBOOLE_1:1;
hence A9 is  open by TOPS_2:64;
thus A9 is  quasi_prebasis
proof
reconsider B = ( FinMeetCl A9 ) as (Basis of T) by CKB28:1;
take B;
thus thesis;
end;

end;
hence thesis;
end;
