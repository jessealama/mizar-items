environ
vocabularies XREAL_0,ORDINAL1,SIN_COS,REAL_1,ARYTM_3,ARYTM_1,RELAT_1,CARD_1,SIN_COS4;
notations ORDINAL1,NUMBERS,XCMPLX_0,XREAL_0,REAL_1,SIN_COS,CKB1,CKB2,CKB3,CKB4;
definitions CKB1,CKB2,CKB3,CKB4;
theorems XCMPLX_1,SIN_COS,XREAL_0,CKB1,CKB2,CKB3,CKB4,CKB5;
registrations XCMPLX_0,XREAL_0,INT_1,SIN_COS,ORDINAL1;
constructors SQUARE_1,NEWTON,SIN_COS,CKB1,CKB2,CKB3,CKB4;
requirements NUMERALS,SUBSET,ARITHM;
begin
reserve th for  real number;
theorem
( cosec ( - th ) ) = ( - ( 1 / ( sin th ) ) )
proof
( cosec ( - th ) ) = ( 1 / ( - ( sin th ) ) ) by SIN_COS:31
.= ( - ( 1 / ( sin th ) ) ) by XCMPLX_1:188;
hence thesis;
end;
