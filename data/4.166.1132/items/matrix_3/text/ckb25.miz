begin
reserve i for Nat;
reserve k for Nat;
reserve K for Field;
theorem
Th19: (for p,q being (FinSequence of K) holds (for i holds ((((i in ( dom p ) & i in ( dom q )) & ( p . i ) = ( 1. K )) & (for k holds ((k in ( dom p ) & k <> i) implies ( p . k ) = ( 0. K )))) implies ( Sum ( mlt (p,q) ) ) = ( q . i ))))
proof
let p being (FinSequence of K);
let q being (FinSequence of K);
let i;
assume that
A1: (i in ( dom p ) & i in ( dom q ))
and
A2: (( p . i ) = ( 1. K ) & (for k holds ((k in ( dom p ) & k <> i) implies ( p . k ) = ( 0. K ))));
reconsider r = ( mlt (p,q) ) as (FinSequence of K);
A3: (for k holds ((k in ( dom r ) & k <> i) implies ( r . k ) = ( 0. K ))) by A2,CKB22:1;
A4: (( dom p ) = ( Seg ( len p ) ) & ( dom q ) = ( Seg ( len q ) )) by FINSEQ_1:def 3;
(( dom ( mlt (p,q) ) ) = ( Seg ( len ( mlt (p,q) ) ) ) & ( len ( mlt (p,q) ) ) = ( min (( len p ),( len q )) )) by CKB21:1,FINSEQ_1:def 3;
then ( ( dom p ) /\ ( dom q ) ) = ( dom ( mlt (p,q) ) ) by A4,FINSEQ_2:2;
then A5: i in ( dom r ) by A1,XBOOLE_0:def 4;
then ( r . i ) = ( q . i ) by A2,CKB22:1;
hence thesis by A5,A3,CKB20:1;
end;
