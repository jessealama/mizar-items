begin
reserve i for Nat;
reserve j for Nat;
reserve n for Nat;
reserve K for Field;
definition
let n;
let K;
let M being (Matrix of n,K);
let p being (Element of ( Permutations n ));
func Path_matrix (p,M) -> (FinSequence of K)
means
:Def7: (( len it ) = n & (for i holds (for j holds ((i in ( dom it ) & j = ( p . i )) implies ( it . i ) = ( M * (i,j) )))));
existence
proof
defpred P[ Nat,set ]
 means
(for j holds (j = ( p . $1 ) implies $2 = ( M * ($1,j) )));
reconsider n1 = n as (Element of ( NAT )) by ORDINAL1:def 12;
A1: (for k being Nat holds (k in ( Seg n1 ) implies (ex x being (Element of K) st P[ k,x ])))
proof
reconsider p as (Function of ( Seg n ),( Seg n )) by MATRIX_2:def 9;
let k being Nat;
assume k in ( Seg n1 );
then ( p . k ) in ( Seg n ) by FUNCT_2:5;
then reconsider j = ( p . k ) as (Element of ( NAT ));
reconsider x = ( M * (k,j) ) as (Element of K);
take x;
thus thesis;
end;
consider t being (FinSequence of K) such that A2: ( dom t ) = ( Seg n1 ) and A3: (for k being Nat holds (k in ( Seg n1 ) implies P[ k,( t . k ) ])) from FINSEQ_1:sch 5(A1);
take t;
thus ( len t ) = n by A2,FINSEQ_1:def 3;
let i;
let j;
assume (i in ( dom t ) & j = ( p . i ));
hence thesis by A2,A3;
end;
uniqueness
proof
(for p1,p2 being (FinSequence of K) holds (((( len p1 ) = n & (for i holds (for j holds ((i in ( dom p1 ) & j = ( p . i )) implies ( p1 . i ) = ( M * (i,j) ))))) & (( len p2 ) = n & (for i holds (for j holds ((i in ( dom p2 ) & j = ( p . i )) implies ( p2 . i ) = ( M * (i,j) )))))) implies p1 = p2))
proof
let p1 being (FinSequence of K),p2 being (FinSequence of K);
assume that
A4: ( len p1 ) = n
and
A5: (for i holds (for j holds ((i in ( dom p1 ) & j = ( p . i )) implies ( p1 . i ) = ( M * (i,j) ))))
and
A6: ( len p2 ) = n
and
A7: (for i holds (for j holds ((i in ( dom p2 ) & j = ( p . i )) implies ( p2 . i ) = ( M * (i,j) ))));
A8: ( dom p2 ) = ( Seg n ) by A6,FINSEQ_1:def 3;
A9: ( dom p1 ) = ( Seg n ) by A4,FINSEQ_1:def 3;
(for i being Nat holds (i in ( Seg n ) implies ( p1 . i ) = ( p2 . i )))
proof
reconsider p as (Function of ( Seg n ),( Seg n )) by MATRIX_2:def 9;
let i being Nat;
assume A10: i in ( Seg n );
then ( p . i ) in ( Seg n ) by FUNCT_2:5;
then reconsider j = ( p . i ) as (Element of ( NAT ));
( p1 . i ) = ( M * (i,j) ) by A5,A9,A10;
hence thesis by A7,A8,A10;
end;
hence thesis by A9,A8,FINSEQ_1:13;
end;
hence thesis;
end;
