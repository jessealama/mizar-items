constructors RELAT_2,PARTFUN1,SETWISEO,SQUARE_1,NAT_1,FINSEQOP,FINSOP_1,SETWOP_2,CAT_2,ALGSTR_1,MATRIX_2,FVSUM_1,RELSET_1,FINSEQ_4,CKB1,CKB2,CKB3,CKB9,CKB10,CKB11,CKB12;
requirements NUMERALS,REAL,BOOLE,SUBSET;
begin
reserve i for Nat;
reserve j for Nat;
reserve k for Nat;
reserve n for Nat;
reserve K for Field;
theorem
(for A being (Matrix of n,K) holds ( ( 1. (K,n) ) * A ) = A)
proof
let A being (Matrix of n,K);
set B = ( 1. (K,n) );
A1: ( len B ) = n by MATRIX_1:24;
A2: ( len A ) = n by MATRIX_1:24;
A3: ( width B ) = n by MATRIX_1:24;
then A4: ( len ( B * A ) ) = ( len B ) by A2,CKB9:def 1;
A5:now
A6: ( dom A ) = ( Seg ( len A ) ) by FINSEQ_1:def 3;
let i;
let j;
assume A7: [ i,j ] in ( Indices ( B * A ) );
A8: (( dom ( B * A ) ) = ( Seg ( len ( B * A ) ) ) & ( Indices ( B * A ) ) = [: ( dom ( B * A ) ),( Seg ( width ( B * A ) ) ) :]) by FINSEQ_1:def 3;
then A9: i in ( Seg ( width B ) ) by A1,A3,A4,A7,ZFMISC_1:87;
then i in ( Seg ( len ( Line (B,i) ) ) ) by MATRIX_1:def 7;
then A10: i in ( dom ( Line (B,i) ) ) by FINSEQ_1:def 3;
A11: ( dom B ) = ( Seg ( len B ) ) by FINSEQ_1:def 3;
then A12: i in ( dom B ) by A4,A7,A8,ZFMISC_1:87;
then [ i,i ] in [: ( dom B ),( Seg ( width B ) ) :] by A9,ZFMISC_1:87;
then [ i,i ] in ( Indices B );
then A13: ( ( Line (B,i) ) . i ) = ( 1. K ) by CKB23:1;
A14:now
let k;
assume that
A15: k in ( dom ( Line (B,i) ) )
and
A16: k <> i;
k in ( Seg ( len ( Line (B,i) ) ) ) by A15,FINSEQ_1:def 3;
then k in ( Seg ( width B ) ) by MATRIX_1:def 7;
then [ i,k ] in [: ( dom B ),( Seg ( width B ) ) :] by A12,ZFMISC_1:87;
then [ i,k ] in ( Indices B );
hence ( ( Line (B,i) ) . k ) = ( 0. K ) by A16,CKB23:1;
end;
i in ( Seg ( len ( Col (A,j) ) ) ) by A3,A2,A9,MATRIX_1:def 8;
then A17: i in ( dom ( Col (A,j) ) ) by FINSEQ_1:def 3;
thus ( ( B * A ) * (i,j) ) = ( ( Line (B,i) ) "*" ( Col (A,j) ) ) by A3,A2,A7,CKB9:def 1
.= ( Sum ( mlt (( Line (B,i) ),( Col (A,j) )) ) ) by FVSUM_1:def 9
.= ( ( Col (A,j) ) . i ) by A10,A17,A14,A13,CKB25:1
.= ( A * (i,j) ) by A1,A2,A6,A11,A12,MATRIX_1:def 8;
end;
( width ( B * A ) ) = ( width A ) by A3,A2,CKB9:def 1;
hence thesis by A1,A2,A4,A5,MATRIX_1:21;
end;
