constructors RELAT_2,PARTFUN1,SETWISEO,SQUARE_1,NAT_1,FINSEQOP,FINSOP_1,SETWOP_2,CAT_2,ALGSTR_1,MATRIX_2,FVSUM_1,RELSET_1,FINSEQ_4,CKB1,CKB2,CKB3;
requirements NUMERALS,REAL,BOOLE,SUBSET;
begin
reserve i for Nat;
reserve j for Nat;
reserve n for Nat;
reserve m for Nat;
reserve K for Field;
theorem
(for A being (Matrix of n,m,K) holds ( A + ( 0. (K,n,m) ) ) = A)
proof
let A being (Matrix of n,m,K);
per cases ;
suppose A1: n > ( 0 );

A2: ( width A ) = ( width ( A + ( 0. (K,n,m) ) ) ) by CKB3:def 1;
A3: ( Indices A ) = [: ( Seg n ),( Seg m ) :] by A1,MATRIX_1:23;
A4: ( len A ) = ( len ( A + ( 0. (K,n,m) ) ) ) by CKB3:def 1;
then ( dom A ) = ( dom ( A + ( 0. (K,n,m) ) ) ) by FINSEQ_3:29;
then A5: ( Indices A ) = ( Indices ( A + ( 0. (K,n,m) ) ) ) by A2;
A6: ( Indices ( 0. (K,n,m) ) ) = [: ( Seg n ),( Seg m ) :] by A1,MATRIX_1:23;
now
let i;
let j;
assume A7: [ i,j ] in ( Indices ( A + ( 0. (K,n,m) ) ) );
hence ( ( A + ( 0. (K,n,m) ) ) * (i,j) ) = ( ( A * (i,j) ) + ( ( 0. (K,n,m) ) * (i,j) ) ) by A5,CKB3:def 1
.= ( ( A * (i,j) ) + ( 0. K ) ) by A3,A6,A5,A7,CKB4:1
.= ( A * (i,j) ) by RLVECT_1:4;
end;
hence thesis by A4,A2,MATRIX_1:21;
end;
suppose A8: n = ( 0 );

A9: ( width A ) = ( width ( A + ( 0. (K,n,m) ) ) ) by CKB3:def 1;
A10: ( len A ) = ( len ( A + ( 0. (K,n,m) ) ) ) by CKB3:def 1;
then ( dom A ) = ( dom ( A + ( 0. (K,n,m) ) ) ) by FINSEQ_3:29;
then (( Indices A ) = [: ( dom A ),( Seg ( width A ) ) :] & ( Indices ( A + ( 0. (K,n,m) ) ) ) = [: ( dom A ),( Seg ( width A ) ) :]) by A9;
then (for i holds (for j holds ([ i,j ] in ( Indices ( A + ( 0. (K,n,m) ) ) ) implies ( ( A + ( 0. (K,n,m) ) ) * (i,j) ) = ( A * (i,j) )))) by A8,MATRIX_1:22;
hence thesis by A10,A9,MATRIX_1:21;
end;
end;
