environ
vocabularies XBOOLE_0,SUBSET_1,FUNCT_2,TARSKI,FUNCT_1,RELAT_1,ZFMISC_1,MCART_1,GRAPH_1,PARTFUN1,CAT_1,STRUCT_0,QC_LANG1,CLASSES2,FUNCOP_1,FUNCT_4,CAT_2,OPPCAT_1,FUNCT_5,ARYTM_0,ENS_1;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,MCART_1,DOMAIN_1,RELAT_1,FUNCT_1,FUNCT_2,BINOP_1,PARTFUN1,FUNCT_4,FUNCT_5,CLASSES2,FUNCOP_1,STRUCT_0,GRAPH_1,CAT_1,CAT_2,OPPCAT_1,CKB1,CKB6,CKB14,CKB15,CKB21,CKB23,CKB27,CKB36,CKB37,CKB38,CKB39,CKB40,CKB41;
definitions TARSKI,FUNCT_1,CAT_1,CAT_2,BINOP_1,RELAT_1,GRAPH_1,CKB1,CKB6,CKB14,CKB15,CKB21,CKB23,CKB27,CKB36,CKB37,CKB38,CKB39,CKB40,CKB41;
theorems TARSKI,ENUMSET1,ZFMISC_1,MCART_1,DOMAIN_1,FUNCT_1,FUNCT_2,PARTFUN1,FUNCOP_1,FUNCT_4,CLASSES2,CAT_1,CAT_2,OPPCAT_1,GRFUNC_1,RELAT_1,RELSET_1,XBOOLE_0,XBOOLE_1,BINOP_1,CKB1,CKB3,CKB4,CKB5,CKB6,CKB8,CKB9,CKB10,CKB11,CKB12,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB37,CKB38,CKB39,CKB40,CKB41;
schemes FUNCT_2,BINOP_1,FUNCT_1;
registrations XBOOLE_0,SUBSET_1,FUNCT_1,FUNCT_2,RELSET_1,STRUCT_0,PARTFUN1,CKB2,CKB7,CKB13;
constructors PARTFUN1,DOMAIN_1,CLASSES2,CAT_2,OPPCAT_1,FUNCOP_1,RELSET_1,CKB1,CKB6,CKB14,CKB15,CKB21,CKB23,CKB27,CKB36,CKB37,CKB38,CKB39,CKB40,CKB41;
requirements SUBSET,BOOLE;
begin
reserve V for non  empty set;
reserve f for (Element of ( Funcs V ));
theorem
Th21: CatStr (# V,( Maps V ),( fDom V ),( fCod V ),( fComp V ),( fId V ) #) is Category
proof
set M = ( Maps V );
set d = ( fDom V );
set c = ( fCod V );
set p = ( fComp V );
set i = ( fId V );
now
thus (for f,g being (Element of M) holds ([ g,f ] in ( dom p ) iff ( d . g ) = ( c . f )))
proof
let f being (Element of M);
let g being (Element of M);
(( d . g ) = ( dom g ) & ( c . f ) = ( cod f )) by CKB37:def 1,CKB38:def 1;
hence thesis by CKB39:def 1;
end;

thus (for f,g being (Element of M) holds (( d . g ) = ( c . f ) implies (( d . ( p . (g,f) ) ) = ( d . f ) & ( c . ( p . (g,f) ) ) = ( c . g ))))
proof
let f being (Element of M);
let g being (Element of M);
assume that
A1: ( d . g ) = ( c . f );
A2: (( d . g ) = ( dom g ) & ( c . f ) = ( cod f )) by CKB37:def 1,CKB38:def 1;
then A3: ( p . [ g,f ] ) = ( g * f ) by A1,CKB39:def 1;
A4: (( d . f ) = ( dom f ) & ( c . g ) = ( cod g )) by CKB37:def 1,CKB38:def 1;
(( dom ( g * f ) ) = ( dom f ) & ( cod ( g * f ) ) = ( cod g )) by A1,A2,CKB24:1;
hence thesis by A3,A4,CKB37:def 1,CKB38:def 1;
end;

thus (for f,g,h being (Element of M) holds ((( d . h ) = ( c . g ) & ( d . g ) = ( c . f )) implies ( p . (h,( p . (g,f) )) ) = ( p . (( p . (h,g) ),f) )))
proof
let f being (Element of M);
let g being (Element of M);
let h being (Element of M);
assume that
A5: ( d . h ) = ( c . g )
and
A6: ( d . g ) = ( c . f );
A7: (( dom h ) = ( d . h ) & ( cod g ) = ( c . g )) by CKB37:def 1,CKB38:def 1;
then A8: ( dom ( h * g ) ) = ( dom g ) by A5,CKB24:1;
A9: (( dom g ) = ( d . g ) & ( cod f ) = ( c . f )) by CKB37:def 1,CKB38:def 1;
then A10: ( cod ( g * f ) ) = ( dom h ) by A5,A6,A7,CKB24:1;
thus ( p . (h,( p . (g,f) )) ) = ( p . [ h,( g * f ) ] ) by A6,A9,CKB39:def 1
.= ( h * ( g * f ) ) by A10,CKB39:def 1
.= ( ( h * g ) * f ) by A5,A6,A7,A9,CKB25:1
.= ( p . [ ( h * g ),f ] ) by A6,A9,A8,CKB39:def 1
.= ( p . (( p . (h,g) ),f) ) by A5,A7,CKB39:def 1;
end;

let b being (Element of V);
A11: ( i . b ) = ( id$ b ) by CKB40:def 1;
A12: ( cod ( id$ b ) ) = b by CKB22:1;
A13: ( dom ( id$ b ) ) = b by CKB22:1;
hence (( d . ( i . b ) ) = b & ( c . ( i . b ) ) = b) by A11,A12,CKB37:def 1,CKB38:def 1;
thus (for f being (Element of M) holds (( c . f ) = b implies ( p . (( i . b ),f) ) = f))
proof
let f being (Element of M);
assume that
A14: ( c . f ) = b;
A15: ( cod f ) = ( c . f ) by CKB38:def 1;
then ( ( id$ b ) * f ) = f by A14,CKB26:1;
hence thesis by A11,A13,A14,A15,CKB39:def 1;
end;

let g being (Element of M);
assume that
A16: ( d . g ) = b;
A17: ( dom g ) = ( d . g ) by CKB37:def 1;
then ( g * ( id$ b ) ) = g by A16,CKB26:1;
hence ( p . (g,( i . b )) ) = g by A11,A12,A16,A17,CKB39:def 1;
end;
hence thesis by CAT_1:def 5;
end;
