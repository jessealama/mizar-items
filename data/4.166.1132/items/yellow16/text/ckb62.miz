begin
theorem
(for L being non  empty Poset holds (for S being  directed-sups-inheriting non  empty  full (SubRelStr of L) holds (for X being non  empty set holds ( S |^ X ) is  directed-sups-inheriting  directed-sups-inheriting (SubRelStr of ( L |^ X )))))
proof
let L being non  empty Poset;
let S being  directed-sups-inheriting non  empty  full (SubRelStr of L);
let X being non  empty set;
reconsider SX = ( S |^ X ) as  full non  empty (SubRelStr of ( L |^ X )) by CKB52:1;
defpred P[ set,non  empty  reflexive RelStr ]
 means
$1 is  directed non  empty  directed non  empty (Subset of $2);
A1:now
let A being (Subset of ( S |^ X ));
assume P[ A,( S |^ X ) ];
then reconsider B = A as  directed non  empty (Subset of ( S |^ X ));
let i being (Element of X);
( ( X --> S ) . i ) = S by FUNCOP_1:7;
then ( pi (B,i) ) is  directed non  empty  directed non  empty (Subset of S) by CKB48:1;
hence P[ ( pi (A,i) ),S ];
end;
A2:now
let X being (Subset of S);
assume P[ X,S ];
then ( ex_sup_of X,L implies ( ex_sup_of X,S & ( sup X ) = ( "\/" (X,L) ))) by WAYBEL_0:7;
hence S inherits_sup_of X,L by CKB55:1;
end;
SX is  directed-sups-inheriting
proof
let A being  directed (Subset of SX);
(for A being (Subset of ( S |^ X )) holds (P[ A,( S |^ X ) ] implies ( S |^ X ) inherits_sup_of A,( L |^ X ))) from CKB58:sch 1(A1,A2);
then (A <> ( {} ) implies ( S |^ X ) inherits_sup_of A,( L |^ X ));
hence thesis by CKB53:def 1;
end;
hence thesis;
end;
