(for L being  up-complete non  empty Poset holds (for S1,S2 being  directed-sups-inheriting  full non  empty (SubRelStr of L) holds (S1 is (SubRelStr of S2) implies S1 is  directed-sups-inheriting  full  directed-sups-inheriting  full (SubRelStr of S2))))
proof
let L being  up-complete non  empty Poset;
let S1 being  directed-sups-inheriting  full non  empty (SubRelStr of L);
let S2 being  directed-sups-inheriting  full non  empty (SubRelStr of L);
