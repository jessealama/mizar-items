environ
vocabularies NUMBERS,XBOOLE_0,RLVECT_1,RUSUB_4,SUBSET_1,ARYTM_3,SUPINF_2,TARSKI,ALGSTR_0,ARYTM_1,REAL_1,RELAT_1,BHSP_1,RLSUB_1,STRUCT_0,RVSUM_1,PROB_2,CARD_1,NORMSP_1,SQUARE_1,XXREAL_0,RLVECT_3,PCOMPS_1,SETFAM_1,PRE_TOPC,METRIC_1,RCOMP_1,COMPLEX1,XREAL_0,ORDINAL1,RUSUB_5;
notations TARSKI,XBOOLE_0,SUBSET_1,SETFAM_1,ORDINAL1,NUMBERS,XXREAL_0,XREAL_0,COMPLEX1,REAL_1,PRE_TOPC,DOMAIN_1,STRUCT_0,ALGSTR_0,RLVECT_1,RLSUB_1,SQUARE_1,BHSP_1,BHSP_2,RUSUB_1,RUSUB_3,RUSUB_4,CKB1,CKB5;
definitions XBOOLE_0,TARSKI,PRE_TOPC,RLVECT_1,SQUARE_1,SUBSET_1,STRUCT_0,CKB1,CKB5;
theorems RLVECT_1,TARSKI,XBOOLE_0,XBOOLE_1,RUSUB_1,SUBSET_1,RLSUB_1,PRE_TOPC,RUSUB_4,BHSP_1,BHSP_2,SQUARE_1,ZFMISC_1,SETFAM_1,ABSVALUE,XREAL_0,TOPS_1,XCMPLX_1,XREAL_1,COMPLEX1,XXREAL_0,STRUCT_0,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10;
schemes DOMAIN_1,SUBSET_1;
registrations XBOOLE_0,SUBSET_1,XXREAL_0,XREAL_0,MEMBERED,STRUCT_0,PRE_TOPC,TOPS_1,RLVECT_1,RUSUB_4;
constructors SETFAM_1,REAL_1,SQUARE_1,MEMBERED,COMPLEX1,REALSET1,PRE_TOPC,RLVECT_3,BHSP_2,RUSUB_3,RUSUB_4,CKB1,CKB5;
requirements REAL,NUMERALS,BOOLE,SUBSET,ARITHM;
begin
theorem
Th9: (for V being  Abelian  add-associative  right_zeroed  right_complementable non empty addLoopStr holds (for M,N being (Subset of V) holds (for v being (Element of V) holds (M = ( N + { v } ) iff ( M - { v } ) = N))))
proof
let V being  Abelian  add-associative  right_zeroed  right_complementable non empty addLoopStr;
let M being (Subset of V),N being (Subset of V);
let v being (Element of V);
A1: (( M - { v } ) = N implies M = ( N + { v } ))
proof
assume A2: ( M - { v } ) = N;
(for x being set holds (x in ( N + { v } ) implies x in M))
proof
let x being set;
assume A3: x in ( N + { v } );
then reconsider x as (Element of V);
x in { ( u1 + v1 ) where u1 is (Element of V),v1 is (Element of V): (u1 in N & v1 in { v }) } by A3,RUSUB_4:def 9;
then consider u1 being (Element of V),v1 being (Element of V) such that A4: x = ( u1 + v1 ) and A5: u1 in N and A6: v1 in { v };
A7: ( x - v1 ) = ( u1 + ( v1 - v1 ) ) by A4,RLVECT_1:def 3
.= ( u1 + ( 0. V ) ) by RLVECT_1:15
.= u1 by RLVECT_1:4;
v1 = v by A6,TARSKI:def 1;
then consider u2 being (Element of V),v2 being (Element of V) such that A8: ( x - v ) = ( u2 - v2 ) and A9: u2 in M and A10: v2 in { v } by A2,A5,A7;
v2 = v by A10,TARSKI:def 1;
then ( ( x - v ) + v ) = ( u2 - ( v - v ) ) by A8,RLVECT_1:29
.= ( u2 - ( 0. V ) ) by RLVECT_1:15
.= u2 by RLVECT_1:13;
then u2 = ( x - ( v - v ) ) by RLVECT_1:29
.= ( x - ( 0. V ) ) by RLVECT_1:15;
hence thesis by A9,RLVECT_1:13;
end;
then A11: ( N + { v } ) c= M by TARSKI:def 3;
(for x being set holds (x in M implies x in ( N + { v } )))
proof
let x being set;
assume A12: x in M;
then reconsider x as (Element of V);
A13: v in { v } by TARSKI:def 1;
then ( x - v ) in { ( u2 - v2 ) where u2 is (Element of V),v2 is (Element of V): (u2 in M & v2 in { v }) } by A12;
then consider u2 being (Element of V) such that A14: ( x - v ) = u2 and A15: u2 in N by A2;
( u2 + v ) = ( x - ( v - v ) ) by A14,RLVECT_1:29
.= ( x - ( 0. V ) ) by RLVECT_1:15
.= x by RLVECT_1:13;
then x in { ( u1 + v1 ) where u1 is (Element of V),v1 is (Element of V): (u1 in N & v1 in { v }) } by A13,A15;
hence thesis by RUSUB_4:def 9;
end;
then M c= ( N + { v } ) by TARSKI:def 3;
hence thesis by A11,XBOOLE_0:def 10;
end;
(M = ( N + { v } ) implies ( M - { v } ) = N)
proof
assume A16: M = ( N + { v } );
(for x being set holds (x in ( M - { v } ) implies x in N))
proof
let x being set;
assume A17: x in ( M - { v } );
then reconsider x as (Element of V);
consider u1 being (Element of V),v1 being (Element of V) such that A18: x = ( u1 - v1 ) and A19: u1 in M and A20: v1 in { v } by A17;
A21: ( x + v1 ) = ( u1 - ( v1 - v1 ) ) by A18,RLVECT_1:29
.= ( u1 - ( 0. V ) ) by RLVECT_1:15
.= u1 by RLVECT_1:13;
v1 = v by A20,TARSKI:def 1;
then ( x + v ) in { ( u2 + v2 ) where u2 is (Element of V),v2 is (Element of V): (u2 in N & v2 in { v }) } by A16,A19,A21,RUSUB_4:def 9;
then consider u2 being (Element of V),v2 being (Element of V) such that A22: (( x + v ) = ( u2 + v2 ) & u2 in N) and A23: v2 in { v };
v2 = v by A23,TARSKI:def 1;
hence thesis by A22,RLVECT_1:8;
end;
then A24: ( M - { v } ) c= N by TARSKI:def 3;
(for x being set holds (x in N implies x in ( M - { v } )))
proof
let x being set;
assume A25: x in N;
then reconsider x as (Element of V);
A26: v in { v } by TARSKI:def 1;
then ( x + v ) in { ( u2 + v2 ) where u2 is (Element of V),v2 is (Element of V): (u2 in N & v2 in { v }) } by A25;
then ( x + v ) in M by A16,RUSUB_4:def 9;
then consider u2 being (Element of V) such that A27: ( x + v ) = u2 and A28: u2 in M;
( u2 - v ) = ( x + ( v - v ) ) by A27,RLVECT_1:def 3
.= ( x + ( 0. V ) ) by RLVECT_1:15
.= x by RLVECT_1:4;
hence thesis by A26,A28;
end;
then N c= ( M - { v } ) by TARSKI:def 3;
hence thesis by A24,XBOOLE_0:def 10;
end;
hence thesis by A1;
end;
