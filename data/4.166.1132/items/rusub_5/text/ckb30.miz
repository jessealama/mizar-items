theorem
Th26: (for V being RealUnitarySpace holds (for M,N being non empty (Subset of V) holds (M c= N implies (the carrier of ( Ort_Comp N )) c= (the carrier of ( Ort_Comp M )))))
proof
let V being RealUnitarySpace;
let M being non empty (Subset of V),N being non empty (Subset of V);
assume A1: M c= N;
(for x being set holds (x in (the carrier of ( Ort_Comp N )) implies x in (the carrier of ( Ort_Comp M ))))
proof
let x being set;
assume x in (the carrier of ( Ort_Comp N ));
then x in { v where v is (VECTOR of V): (for w being (VECTOR of V) holds (w in N implies w,v are_orthogonal )) } by CKB24:def 1;
then A2: (ex v1 being (VECTOR of V) st (x = v1 & (for w being (VECTOR of V) holds (w in N implies w,v1 are_orthogonal ))));
then reconsider x as (VECTOR of V);
(for y being (VECTOR of V) holds (y in M implies y,x are_orthogonal )) by A1,A2;
then x in { v where v is (VECTOR of V): (for w being (VECTOR of V) holds (w in M implies w,v are_orthogonal )) };
hence thesis by CKB24:def 1;
end;
hence thesis by TARSKI:def 3;
