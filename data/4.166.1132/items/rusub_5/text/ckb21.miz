begin
theorem
Th19: (for V being RealLinearSpace holds (for M being non empty  Affine (Subset of V) holds (for v being (VECTOR of V) holds (v in M implies ( M - { v } ) = ( union { ( M - { u } ) where u is (VECTOR of V): u in M } )))))
proof
let V being RealLinearSpace;
let M being non empty  Affine (Subset of V);
let v being (VECTOR of V);
assume A1: v in M;
(for x being set holds (x in ( union { ( M - { u } ) where u is (VECTOR of V): u in M } ) implies x in ( M - { v } )))
proof
let x being set;
assume x in ( union { ( M - { u } ) where u is (VECTOR of V): u in M } );
then consider N being set such that A2: x in N and A3: N in { ( M - { u } ) where u is (VECTOR of V): u in M } by TARSKI:def 4;
(ex v1 being (VECTOR of V) st (N = ( M - { v1 } ) & v1 in M)) by A3;
hence thesis by A1,A2,CKB19:1;
end;
then A4: ( union { ( M - { u } ) where u is (VECTOR of V): u in M } ) c= ( M - { v } ) by TARSKI:def 3;
(for x being set holds (x in ( M - { v } ) implies x in ( union { ( M - { u } ) where u is (VECTOR of V): u in M } )))
proof
let x being set;
assume A5: x in ( M - { v } );
( M - { v } ) in { ( M - { u } ) where u is (VECTOR of V): u in M } by A1;
hence thesis by A5,TARSKI:def 4;
end;
then ( M - { v } ) c= ( union { ( M - { u } ) where u is (VECTOR of V): u in M } ) by TARSKI:def 3;
hence thesis by A4,XBOOLE_0:def 10;
end;
