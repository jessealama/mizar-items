requirements REAL,NUMERALS,BOOLE,SUBSET,ARITHM;
begin
theorem
(for V being RealUnitarySpace holds (for u,v,w being (Point of V) holds (for r,p being Real holds (v in ( ( Ball (u,r) ) /\ ( Ball (w,p) ) ) implies (ex q being Real st (( Ball (v,q) ) c= ( Ball (u,r) ) & ( Ball (v,q) ) c= ( Ball (w,p) )))))))
proof
let V being RealUnitarySpace;
let u being (Point of V);
let v being (Point of V);
let w being (Point of V);
let r being Real;
let p being Real;
assume A1: v in ( ( Ball (u,r) ) /\ ( Ball (w,p) ) );
then v in ( Ball (u,r) ) by XBOOLE_0:def 4;
then consider s being Real such that s > ( 0 ) and A2: ( Ball (v,s) ) c= ( Ball (u,r) ) by CKB40:1;
v in ( Ball (w,p) ) by A1,XBOOLE_0:def 4;
then consider t being Real such that t > ( 0 ) and A3: ( Ball (v,t) ) c= ( Ball (w,p) ) by CKB40:1;
take q = ( min (s,t) );
( Ball (v,q) ) c= ( Ball (v,s) ) by CKB38:1,XXREAL_0:17;
hence ( Ball (v,q) ) c= ( Ball (u,r) ) by A2,XBOOLE_1:1;
( Ball (v,q) ) c= ( Ball (v,t) ) by CKB38:1,XXREAL_0:17;
hence thesis by A3,XBOOLE_1:1;
end;
