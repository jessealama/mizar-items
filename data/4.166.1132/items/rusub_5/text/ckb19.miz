begin
theorem
Th17: (for V being RealLinearSpace holds (for M being non  empty  Affine (Subset of V) holds (for u,v being (VECTOR of V) holds ((u in M & v in M) implies ( M - { v } ) = ( M - { u } )))))
proof
let V being RealLinearSpace;
let M being non  empty  Affine (Subset of V);
let u being (VECTOR of V);
let v being (VECTOR of V);
assume (u in M & v in M);
then ((ex N1 being non  empty  Affine (Subset of V) st ((N1 = ( M - { u } ) & M is_parallel_to N1) & N1 is  Subspace-like)) & (ex N2 being non  empty  Affine (Subset of V) st ((N2 = ( M - { v } ) & M is_parallel_to N2) & N2 is  Subspace-like))) by CKB18:1;
hence thesis by CKB16:1;
end;
