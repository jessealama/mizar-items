theorem
Th33: (for V being RealUnitarySpace holds (for v being (Point of V) holds (for r,p being Real holds (r <= p implies ( Ball (v,r) ) c= ( Ball (v,p) )))))
proof
let V being RealUnitarySpace;
let v being (Point of V);
let r being Real,p being Real;
assume A1: r <= p;
(for u being (Point of V) holds (u in ( Ball (v,r) ) implies u in ( Ball (v,p) )))
proof
let u being (Point of V);
assume u in ( Ball (v,r) );
then ( dist (v,u) ) < r by BHSP_2:41;
then ( ( dist (v,u) ) + r ) < ( r + p ) by A1,XREAL_1:8;
then ( dist (v,u) ) < ( ( r + p ) - r ) by XREAL_1:20;
hence thesis by BHSP_2:41;
end;
hence thesis by SUBSET_1:2;
end;
