begin
theorem
Th12: (for X being RealNormSpace holds (for seq,seq1 being (sequence of X) holds ((seq1 is (subsequence of seq) & seq is  convergent) implies ( lim seq1 ) = ( lim seq ))))
proof
let X being RealNormSpace;
let seq being (sequence of X),seq1 being (sequence of X);
assume that
A1: seq1 is (subsequence of seq)
and
A2: seq is  convergent;
consider Nseq being  increasing (sequence of ( NAT )) such that A3: seq1 = ( seq * Nseq ) by A1,VALUED_0:def 17;
A4:now
let p being Real;
assume ( 0 ) < p;
then consider n1 being (Element of ( NAT )) such that A5: (for m being (Element of ( NAT )) holds (n1 <= m implies ||. ( ( seq . m ) - ( lim seq ) ) .|| < p)) by A2,NORMSP_1:def 7;
take n = n1;
let m being (Element of ( NAT ));
assume that
A6: n <= m;
m <= ( Nseq . m ) by SEQM_3:14;
then A7: n <= ( Nseq . m ) by A6,XXREAL_0:2;
( seq1 . m ) = ( seq . ( Nseq . m ) ) by A3,FUNCT_2:15;
hence ||. ( ( seq1 . m ) - ( lim seq ) ) .|| < p by A5,A7;
end;
seq1 is  convergent by A1,A2,CKB11:1;
hence thesis by A4,NORMSP_1:def 7;
end;
