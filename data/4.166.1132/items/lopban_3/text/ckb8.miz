proof
let X being RealNormSpace;
let seq being (sequence of X);
assume seq is  convergent;
then consider g1 being (Element of X) such that A1: (for s being Real holds (( 0 ) < s implies (ex n being (Element of ( NAT )) st (for m being (Element of ( NAT )) holds (n <= m implies ||. ( ( seq . m ) - g1 ) .|| < s))))) by NORMSP_1:def 6;
let s being Real;
assume ( 0 ) < s;
then consider n being (Element of ( NAT )) such that A2: (for m being (Element of ( NAT )) holds (n <= m implies ||. ( ( seq . m ) - g1 ) .|| < ( s / 2 ))) by A1,XREAL_1:215;
now
let m being (Element of ( NAT ));
assume n <= m;
then A3: ||. ( ( seq . m ) - g1 ) .|| < ( s / 2 ) by A2;
A4: (||. ( ( ( seq . m ) - g1 ) + ( g1 - ( seq . n ) ) ) .|| <= ( ||. ( ( seq . m ) - g1 ) .|| + ||. ( g1 - ( seq . n ) ) .|| ) & ||. ( ( ( seq . m ) - g1 ) + ( g1 - ( seq . n ) ) ) .|| = ||. ( ( seq . m ) - ( seq . n ) ) .||) by CKB7:1,NORMSP_1:def 1;
||. ( ( seq . n ) - g1 ) .|| < ( s / 2 ) by A2;
then ||. ( g1 - ( seq . n ) ) .|| < ( s / 2 ) by NORMSP_1:7;
then ( ||. ( ( seq . m ) - g1 ) .|| + ||. ( g1 - ( seq . n ) ) .|| ) < ( ( s / 2 ) + ( s / 2 ) ) by A3,XREAL_1:8;
hence ||. ( ( seq . m ) - ( seq . n ) ) .|| < s by A4,XXREAL_0:2;
end;
hence thesis;
