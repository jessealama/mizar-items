Th17: (for X being RealNormSpace holds (for seq being (sequence of X) holds (seq is  constant implies seq is  convergent)))
proof
let X being RealNormSpace;
let seq being (sequence of X);
assume seq is  constant;
then consider r being (Element of X) such that A1: (for n being Nat holds ( seq . n ) = r) by VALUED_0:def 18;
take g = r;
let p being Real;
assume that
A2: ( 0 ) < p;
take n = ( 0 );
let m being (Element of ( NAT ));
assume that
n <= m;
||. ( ( seq . m ) - g ) .|| = ||. ( r - g ) .|| by A1
.= ||. ( 0. X ) .|| by RLVECT_1:15
.= ( 0 );
hence thesis by A2;
