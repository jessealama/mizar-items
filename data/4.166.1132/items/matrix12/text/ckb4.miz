environ
vocabularies NUMBERS,NAT_1,VECTSP_1,SUBSET_1,MATRIX_1,FINSEQ_1,TREES_1,INCSP_1,MATRIX11,RELAT_1,FUNCT_1,XXREAL_0,CARD_1,ARYTM_3,FINSEQ_2,STRUCT_0,ALGSTR_0,MESFUNC1,ZFMISC_1,GROUP_1,SUPINF_2,FVSUM_1,CARD_3,RVSUM_1,MATRIX_6,ARYTM_1,QC_LANG1,MATRIX12;
notations MATRIX_1,XXREAL_0,MATRIX11,TARSKI,NUMBERS,NAT_1,FUNCT_1,FINSEQ_1,ZFMISC_1,BINOP_1,STRUCT_0,ALGSTR_0,GROUP_1,VECTSP_1,FVSUM_1,FINSEQ_2,MATRIX_3,MATRIX_6,CKB2;
definitions FINSEQ_1,MATRIX_1,FVSUM_1,ALGSTR_0,CKB2;
theorems MATRIX_6,MATRIX11,FINSEQ_1,FINSEQ_2,FVSUM_1,MATRIX_1,FINSEQOP,ZFMISC_1,MATRIX_2,FUNCOP_1,RLVECT_1,GROUP_1,MATRIX_3,MATRIX_4,MATRIXR1,VECTSP_2,ORDINAL1,CARD_1,CKB1,CKB2,CKB3;
registrations RELSET_1,STRUCT_0,FVSUM_1,VECTSP_1,ORDINAL1,XXREAL_0,NAT_1,FINSEQ_2,XBOOLE_0,CARD_1;
constructors MATRIX_6,MATRIX11,CAT_2,ALGSTR_1,FVSUM_1,XXREAL_0,RELSET_1,CKB2;
requirements NUMERALS,REAL,BOOLE,SUBSET;
begin
reserve j for Nat;
reserve k for Nat;
reserve l for Nat;
reserve n for Nat;
reserve m for Nat;
reserve i for Nat;
reserve K for Field;
reserve M for (Matrix of n,m,K);
reserve M1 for (Matrix of n,m,K);
theorem
Th2: (for M holds (for M1 holds (for i holds ((((l in ( dom M ) & k in ( dom M )) & i in ( dom M )) & M1 = ( InterchangeLine (M,l,k) )) implies (((i = l implies ( Line (M1,i) ) = ( Line (M,k) )) & (i = k implies ( Line (M1,i) ) = ( Line (M,l) ))) & ((i <> l & i <> k) implies ( Line (M1,i) ) = ( Line (M,i) )))))))
proof
let M;
let M1;
let i;
assume that
A1: l in ( dom M )
and
A2: k in ( dom M )
and
A3: i in ( dom M )
and
A4: M1 = ( InterchangeLine (M,l,k) );
thus (i = l implies ( Line (M1,i) ) = ( Line (M,k) ))
proof
A5: ( width M1 ) = ( width M ) by CKB3:1;
A6: ( len ( Line (M1,i) ) ) = ( width M1 ) by MATRIX_1:def 7;
assume A7: i = l;
A8:now
let j being Nat;
assume that
A9: (1 <= j & j <= ( len ( Line (M1,i) ) ));
j in ( NAT ) by ORDINAL1:def 12;
then A10: j in ( Seg ( width M1 ) ) by A6,A9;
hence ( ( Line (M1,i) ) . j ) = ( M1 * (i,j) ) by MATRIX_1:def 7
.= ( M * (k,j) ) by A1,A4,A7,A5,A10,CKB2:def 1
.= ( ( Line (M,k) ) . j ) by A5,A10,MATRIX_1:def 7;
end;
( len ( Line (M,k) ) ) = ( width M ) by MATRIX_1:def 7;
hence thesis by A6,A8,CKB3:1,FINSEQ_1:14;
end;

thus (i = k implies ( Line (M1,i) ) = ( Line (M,l) ))
proof
A11: ( width M1 ) = ( width M ) by CKB3:1;
A12: ( len ( Line (M1,i) ) ) = ( width M1 ) by MATRIX_1:def 7;
assume A13: i = k;
A14:now
let j being Nat;
assume that
A15: (1 <= j & j <= ( len ( Line (M1,i) ) ));
j in ( NAT ) by ORDINAL1:def 12;
then A16: j in ( Seg ( width M1 ) ) by A12,A15;
hence ( ( Line (M1,i) ) . j ) = ( M1 * (i,j) ) by MATRIX_1:def 7
.= ( M * (l,j) ) by A2,A4,A13,A11,A16,CKB2:def 1
.= ( ( Line (M,l) ) . j ) by A11,A16,MATRIX_1:def 7;
end;
( len ( Line (M,l) ) ) = ( width M ) by MATRIX_1:def 7;
hence thesis by A12,A14,CKB3:1,FINSEQ_1:14;
end;

thus ((i <> l & i <> k) implies ( Line (M1,i) ) = ( Line (M,i) ))
proof
A17: ( width M1 ) = ( width M ) by CKB3:1;
A18: ( len ( Line (M1,i) ) ) = ( width M1 ) by MATRIX_1:def 7;
assume A19: (i <> l & i <> k);
A20:now
let j being Nat;
assume that
A21: (1 <= j & j <= ( len ( Line (M1,i) ) ));
j in ( NAT ) by ORDINAL1:def 12;
then A22: j in ( Seg ( width M1 ) ) by A18,A21;
hence ( ( Line (M1,i) ) . j ) = ( M1 * (i,j) ) by MATRIX_1:def 7
.= ( M * (i,j) ) by A3,A4,A19,A17,A22,CKB2:def 1
.= ( ( Line (M,i) ) . j ) by A17,A22,MATRIX_1:def 7;
end;
( len ( Line (M,i) ) ) = ( width M ) by MATRIX_1:def 7;
hence thesis by A18,A20,CKB3:1,FINSEQ_1:14;
end;

end;
