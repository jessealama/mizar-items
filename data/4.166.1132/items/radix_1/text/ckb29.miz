environ
vocabularies NUMBERS,NAT_1,INT_1,ARYTM_1,ARYTM_3,CARD_1,XXREAL_0,SUBSET_1,RELAT_1,NEWTON,POWER,TARSKI,XBOOLE_0,FINSEQ_2,FINSEQ_1,FUNCT_1,PARTFUN1,CARD_3,ORDINAL4,RADIX_1;
notations TARSKI,XBOOLE_0,SUBSET_1,NUMBERS,XCMPLX_0,INT_1,NAT_1,NAT_D,FUNCT_1,PARTFUN1,NEWTON,POWER,FINSEQ_1,FINSEQ_2,TREES_4,GR_CY_1,XXREAL_0,MESFUNC1,CKB8,CKB9,CKB19,CKB21,CKB22,CKB25,CKB26,CKB27,CKB28;
definitions TARSKI,RVSUM_1,CKB8,CKB9,CKB21,CKB22,CKB25,CKB26,CKB27,CKB28;
theorems NAT_1,NAT_2,INT_1,FINSEQ_1,EULER_1,PREPOWER,POWER,RVSUM_1,FINSEQ_2,FINSEQ_4,TARSKI,FUNCT_1,PEPIN,FINSEQ_5,NEWTON,XCMPLX_1,XREAL_1,XXREAL_0,FINSOP_1,ORDINAL1,NAT_D,PARTFUN1,CARD_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28;
schemes NAT_1,FINSEQ_2;
registrations RELSET_1,FINSET_1,NUMBERS,XXREAL_0,XREAL_0,NAT_1,INT_1,MEMBERED,NEWTON,XBOOLE_0,VALUED_0,POWER,CARD_1,FINSEQ_1,CKB18;
constructors REAL_1,NAT_D,FINSOP_1,NEWTON,POWER,MESFUNC1,WSIERP_1,GR_CY_1,BINOP_2,CKB8,CKB9,CKB19,CKB21,CKB22,CKB25,CKB26,CKB27,CKB28;
requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve i for Nat;
reserve k for Nat;
reserve n for Nat;
reserve x for Nat;
definition
let k being Nat;
let n being Nat;
let x being Nat;
func DecSD (x,n,k) -> (Tuple of n,( k -SD )) means 
:Def9: (for i being Nat holds (i in ( Seg n ) implies ( DigA (it,i) ) = ( DigitDC (x,i,k) )));
existence
proof
deffunc F(Nat) = ( DigitDC (x,$1,k) );
consider z being (FinSequence of ( k -SD )) such that A1: ( len z ) = n and A2: (for j being Nat holds (j in ( dom z ) implies ( z . j ) = F(j))) from FINSEQ_2:sch 1;
A3: ( dom z ) = ( Seg n ) by A1,FINSEQ_1:def 3;
reconsider z as (Tuple of n,( k -SD )) by A1,CARD_1:def 7;
take z;
let i;
assume A4: i in ( Seg n );
hence ( DigA (z,i) ) = ( z . i ) by CKB21:def 1
.= ( DigitDC (x,i,k) ) by A2,A3,A4;
end;
uniqueness
proof
let k1 being (Tuple of n,( k -SD ));
let k2 being (Tuple of n,( k -SD ));
assume that
A5: (for i being Nat holds (i in ( Seg n ) implies ( DigA (k1,i) ) = ( DigitDC (x,i,k) )))
and
A6: (for i being Nat holds (i in ( Seg n ) implies ( DigA (k2,i) ) = ( DigitDC (x,i,k) )));
A7: ( len k1 ) = n by CARD_1:def 7;
then A8: ( dom k1 ) = ( Seg n ) by FINSEQ_1:def 3;
A9:now
let j being Nat;
assume A10: j in ( dom k1 );
then ( k1 . j ) = ( DigA (k1,j) ) by A8,CKB21:def 1
.= ( DigitDC (x,j,k) ) by A5,A8,A10
.= ( DigA (k2,j) ) by A6,A8,A10
.= ( k2 . j ) by A8,A10,CKB21:def 1;
hence ( k1 . j ) = ( k2 . j );
end;
( len k2 ) = n by CARD_1:def 7;
hence thesis by A7,A9,FINSEQ_2:9;
end;
end;
