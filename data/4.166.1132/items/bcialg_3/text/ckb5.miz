environ
vocabularies XBOOLE_0,BCIALG_1,BINOP_1,SUBSET_1,XXREAL_0,SUPINF_2,XXREAL_2,CARD_FIL,BCIALG_3;
notations XBOOLE_0,SUBSET_1,STRUCT_0,BCIALG_1,CKB1;
definitions BCIALG_1,CKB1;
theorems BCIALG_1,STRUCT_0,CKB1,CKB4;
registrations STRUCT_0,BCIALG_1,CKB2,CKB3;
constructors BCIALG_1,CKB1;
requirements SUBSET;
begin
reserve X for BCK-algebra;
reserve x for (Element of X);
reserve y for (Element of X);
theorem
Th2: (for X being BCK-algebra holds (for x,y being (Element of X) holds (( x \ ( x \ y ) ) <= y & ( x \ ( x \ y ) ) <= x)))
proof
let X being BCK-algebra;
let x being (Element of X);
let y being (Element of X);
A1: ( ( 0. X ) \ ( x \ y ) ) = ( ( x \ y ) ` )
.= ( 0. X ) by BCIALG_1:def 8;
( ( ( x \ ( x \ y ) ) \ ( ( 0. X ) \ ( x \ y ) ) ) \ ( x \ ( 0. X ) ) ) = ( 0. X ) by BCIALG_1:def 3;
then ( ( ( x \ ( x \ y ) ) \ ( 0. X ) ) \ x ) = ( 0. X ) by A1,BCIALG_1:2;
then (( ( x \ ( x \ y ) ) \ y ) = ( 0. X ) & ( ( x \ ( x \ y ) ) \ x ) = ( 0. X )) by BCIALG_1:1,BCIALG_1:2;
hence thesis by BCIALG_1:def 11;
end;
