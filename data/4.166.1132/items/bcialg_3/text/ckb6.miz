environ
vocabularies XBOOLE_0,BCIALG_1,BINOP_1,SUBSET_1,XXREAL_0,SUPINF_2,XXREAL_2,CARD_FIL,BCIALG_3;
notations XBOOLE_0,SUBSET_1,STRUCT_0,BCIALG_1,CKB1;
definitions BCIALG_1,CKB1;
theorems BCIALG_1,STRUCT_0,CKB1,CKB4,CKB5;
registrations STRUCT_0,BCIALG_1,CKB2,CKB3;
constructors BCIALG_1,CKB1;
requirements SUBSET;
begin
reserve X for BCK-algebra;
reserve x for (Element of X);
reserve y for (Element of X);
theorem
Th3: (X is  commutative  commutative BCK-algebra iff (for x,y being (Element of X) holds ( x \ y ) = ( x \ ( y \ ( y \ x ) ) )))
proof
thus (X is  commutative  commutative BCK-algebra implies (for x,y being (Element of X) holds ( x \ y ) = ( x \ ( y \ ( y \ x ) ) )))
proof
assume A1: X is  commutative  commutative BCK-algebra;
let x being (Element of X);
let y being (Element of X);
( x \ y ) = ( x \ ( x \ ( x \ y ) ) ) by BCIALG_1:8
.= ( x \ ( y \ ( y \ x ) ) ) by A1,CKB1:def 1;
hence thesis;
end;

assume A2: (for x,y being (Element of X) holds ( x \ y ) = ( x \ ( y \ ( y \ x ) ) ));
(for x,y being (Element of X) holds ( x \ ( x \ y ) ) <= ( y \ ( y \ x ) ))
proof
let x;
let y;
( x \ ( x \ ( y \ ( y \ x ) ) ) ) <= ( y \ ( y \ x ) ) by CKB5:1;
hence thesis by A2;
end;
hence thesis by CKB4:1;
end;
