definition
let T being TopSpace;
func density T ->  cardinal number means 
:Def12: ((ex A being (Subset of T) st (A is  dense & it = ( card A ))) & (for B being (Subset of T) holds (B is  dense implies it c= ( card B ))));
existence
proof
set B0 = ( [#] T );
defpred P[  ordinal number ]
 means
(ex A being (Subset of T) st (A is  dense & $1 = ( card A )));
( card B0 ) is  ordinal;
then A1: (ex A being  ordinal number st P[ A ]);
consider A being  ordinal number such that A2: P[ A ] and A3: (for B being  ordinal number holds (P[ B ] implies A c= B)) from ORDINAL1:sch 1(A1);
consider B1 being (Subset of T) such that A4: (B1 is  dense & A = ( card B1 )) by A2;
take ( card B1 );
thus thesis by A3,A4;
end;
uniqueness
proof
let M1 being  cardinal number;
let M2 being  cardinal number;
assume (((ex B being (Subset of T) st (B is  dense & M1 = ( card B ))) & (for B being (Subset of T) holds (B is  dense implies M1 c= ( card B )))) & ((ex B being (Subset of T) st (B is  dense & M2 = ( card B ))) & (for B being (Subset of T) holds (B is  dense implies M2 c= ( card B )))));
hence (M1 c= M2 & M2 c= M1);
