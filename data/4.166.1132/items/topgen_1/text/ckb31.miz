constructors SETFAM_1,CARD_3,TOPS_1,TDLAT_3,TOPMETR,YELLOW_8,BORSUK_5,ORDERS_4,TOPS_2,CKB11,CKB23,CKB25,CKB30;
requirements BOOLE,SUBSET;
begin
theorem
(for T being non empty TopSpace holds (for A being (Subset of T) holds (for p being set holds (p in ( A \ ( Der A ) ) iff p is_isolated_in A))))
proof
let T being non empty TopSpace;
let A being (Subset of T);
let p being set;
hereby
assume A1: p in ( A \ ( Der A ) );
then (not p in ( Der A )) by XBOOLE_0:def 5;
then A2: (not p is_an_accumulation_point_of A) by CKB26:1;
p in A by A1,XBOOLE_0:def 5;
hence p is_isolated_in A by A2,CKB30:def 1;
end;
assume A3: p is_isolated_in A;
then (not p is_an_accumulation_point_of A) by CKB30:def 1;
then A4: (not p in ( Der A )) by CKB26:1;
p in A by A3,CKB30:def 1;
hence thesis by A4,XBOOLE_0:def 5;
end;
