begin
theorem
Th1: (for T being 1-sorted holds (for A,B being (Subset of T) holds (A meets ( B ` ) iff ( A \ B ) <> ( {} ))))
proof
let T being 1-sorted;
let A being (Subset of T);
let B being (Subset of T);
hereby
assume A meets ( B ` );
then ( A /\ ( B ` ) ) <> ( {} ) by XBOOLE_0:def 7;
hence ( A \ B ) <> ( {} ) by SUBSET_1:13;
end;
assume ( A \ B ) <> ( {} );
then ( A /\ ( B ` ) ) <> ( {} ) by SUBSET_1:13;
hence thesis by XBOOLE_0:def 7;
end;
