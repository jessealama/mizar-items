begin
theorem
Th18: (for T being non  empty TopSpace holds (for A being (Subset of T) holds (for x being set holds (x in ( Der A ) iff x is_an_accumulation_point_of A))))
proof
let T being non  empty TopSpace;
let A being (Subset of T);
let x being set;
thus (x in ( Der A ) implies x is_an_accumulation_point_of A) by CKB25:def 1;
assume A1: x is_an_accumulation_point_of A;
then x is (Point of T) by CKB24:1;
hence thesis by A1,CKB25:def 1;
end;
