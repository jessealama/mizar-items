begin
theorem
(for T being non empty TopSpace holds (for p being (Point of T) holds (p is  isolated iff { p } is  open)))
proof
let T being non empty TopSpace;
let p being (Point of T);
A1: ( { p } /\ ( [#] T ) ) = { p } by XBOOLE_1:28;
hereby
assume p is  isolated;
then p is_isolated_in ( [#] T ) by CKB34:def 1;
then (ex G being  open (Subset of T) st ( G /\ ( [#] T ) ) = { p }) by CKB33:1;
hence { p } is  open;
end;
assume { p } is  open;
then p is_isolated_in ( [#] T ) by A1,CKB33:1;
hence thesis by CKB34:def 1;
end;
