constructors SETFAM_1,DOMAIN_1,REAL_1,SQUARE_1,NAT_1,MEMBERED,FINSEQ_1,TOPS_2,COMPTS_1,METRIC_1,RELSET_1,CKB8,CKB12,CKB25;
requirements NUMERALS,REAL,BOOLE,SUBSET,ARITHM;
begin
reserve T for non  empty TopSpace;
reserve FX for (Subset-Family of T);
reserve GX for (Subset-Family of T);
theorem
(T is  compact implies T is  paracompact)
proof
assume A1: T is  compact;
(for FX holds ((FX is (Cover of T) & FX is  open) implies (ex GX st (((GX is  open & GX is (Cover of T)) & GX is_finer_than FX) & GX is  locally_finite))))
proof
let FX;
assume that
A2: FX is (Cover of T)
and
A3: FX is  open;
consider GX such that A4: GX c= FX and A5: GX is (Cover of T) and A6: GX is  finite by A1,A2,A3,COMPTS_1:def 1;
take GX;
(for W being (Subset of T) holds (W in GX implies W is  open)) by A3,A4,TOPS_2:def 1;
hence GX is  open by TOPS_2:def 1;
thus GX is (Cover of T) by A5;
thus GX is_finer_than FX by A4,SETFAM_1:12;
thus thesis by A6,CKB11:1;
end;
hence thesis by CKB25:def 1;
end;
