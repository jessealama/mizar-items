environ
vocabularies NUMBERS,METRIC_1,SUBSET_1,REAL_1,XREAL_0,ORDINAL1,XXREAL_0,TARSKI,ARYTM_3,XBOOLE_0,PRE_TOPC,SETFAM_1,ZFMISC_1,COMPTS_1,STRUCT_0,RCOMP_1,FINSET_1,FINSEQ_1,RELAT_1,NAT_1,CARD_1,FUNCT_1,CARD_5,ARYTM_1,PCOMPS_1;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,ORDINAL1,NUMBERS,XXREAL_0,XREAL_0,REAL_1,TOPS_2,SETFAM_1,DOMAIN_1,STRUCT_0,FINSET_1,COMPTS_1,PRE_TOPC,SQUARE_1,RELAT_1,FUNCT_1,FUNCT_2,FINSEQ_1,NAT_1,METRIC_1,CKB8,CKB12,CKB25;
definitions STRUCT_0,COMPTS_1,SUBSET_1,RELAT_1,CKB8,CKB12,CKB25;
theorems COMPTS_1,PRE_TOPC,TOPS_1,TOPS_2,NAT_1,FINSET_1,SUBSET_1,SETFAM_1,TARSKI,FINSEQ_1,ZFMISC_1,FUNCT_1,FUNCT_2,METRIC_1,RELAT_1,XBOOLE_0,XBOOLE_1,XREAL_0,XREAL_1,XXREAL_0,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB21,CKB22,CKB23,CKB24,CKB25,CKB27,CKB28,CKB29;
schemes SUBSET_1,FUNCT_2,NAT_1,CKB20;
registrations XBOOLE_0,SUBSET_1,RELSET_1,FINSET_1,XXREAL_0,XREAL_0,NAT_1,MEMBERED,STRUCT_0,TOPS_1,COMPTS_1,METRIC_1,FINSEQ_1,PRE_TOPC,CKB26;
constructors SETFAM_1,DOMAIN_1,REAL_1,SQUARE_1,NAT_1,MEMBERED,FINSEQ_1,TOPS_2,COMPTS_1,METRIC_1,RELSET_1,CKB8,CKB12,CKB25;
requirements NUMERALS,REAL,BOOLE,SUBSET,ARITHM;
begin
reserve T for non  empty TopSpace;
reserve Z for (Subset of T);
reserve V for (Subset of T);
reserve W for (Subset of T);
reserve Y for (Subset of T);
reserve x for (Point of T);
reserve A for (Subset of T);
reserve B for (Subset of T);
theorem
((T is  T_2 & T is  paracompact) implies T is  normal)
proof
assume that
A1: T is  T_2
and
A2: T is  paracompact;
(for A,B being (Subset of T) holds (((((A <> ( {} ) & B <> ( {} )) & A is  closed) & B is  closed) & A misses B) implies (ex W,V being (Subset of T) st ((((W is  open & V is  open) & A c= W) & B c= V) & W misses V))))
proof
let A being (Subset of T);
let B being (Subset of T);
assume that
A3: A <> ( {} )
and
B <> ( {} )
and
A4: A is  closed
and
A5: B is  closed
and
A6: A misses B;
(for x holds (x in B implies (ex W,V being (Subset of T) st ((((W is  open & V is  open) & A c= W) & x in V) & W misses V))))
proof
let x;
assume x in B;
then (not x in A) by A6,XBOOLE_0:3;
then A7: x in ( A ` ) by SUBSET_1:29;
T is  regular by A1,A2,CKB29:1;
then consider V,W being (Subset of T) such that A8: ((((V is  open & W is  open) & x in V) & A c= W) & V misses W) by A3,A4,A7,COMPTS_1:def 2;
take W;
take V;
thus thesis by A8;
end;
then consider Y,Z being (Subset of T) such that A9: ((((Y is  open & Z is  open) & A c= Y) & B c= Z) & Y misses Z) by A2,A5,CKB28:1;
take Y;
take Z;
thus thesis by A9;
end;
hence thesis by COMPTS_1:def 3;
end;
