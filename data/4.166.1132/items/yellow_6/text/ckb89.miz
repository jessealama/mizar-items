theorem
Th41: (for T being non  empty TopSpace holds (for N being (net of T) holds (for Y being (subnet of N) holds ( Lim N ) c= ( Lim Y ))))
proof
let T being non  empty TopSpace;
let N being (net of T);
let Y being (subnet of N);
let x;
consider f being (Function of Y,N) such that A1: (the mapping of Y) = ( (the mapping of N) * f ) and A2: (for m being (Element of N) holds (ex n being (Element of Y) st (for p being (Element of Y) holds (n <= p implies m <= ( f . p ))))) by CKB52:def 1;
assume A3: x in ( Lim N );
then reconsider p = x as (Point of T);
(for V being (a_neighborhood of p) holds Y is_eventually_in V)
proof
let V being (a_neighborhood of p);
N is_eventually_in V by A3,CKB88:def 1;
then consider ii being (Element of N) such that A4: (for j being (Element of N) holds (ii <= j implies ( N . j ) in V)) by WAYBEL_0:def 11;
consider n being (Element of Y) such that A5: (for p being (Element of Y) holds (n <= p implies ii <= ( f . p ))) by A2;
take n;
let j being (Element of Y);
assume A6: n <= j;
( N . ( f . j ) ) = ( Y . j ) by A1,FUNCT_2:15;
hence thesis by A4,A5,A6;
end;
hence thesis by CKB88:def 1;
end;
