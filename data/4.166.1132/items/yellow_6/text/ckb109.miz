begin
reserve X for set;
reserve T for Universe;
registration
let T being non  empty TopSpace;
cluster ( Convergence T ) ->  (CONSTANTS)  (SUBNETS)  (DIVERGENCE)  (ITERATED_LIMITS);
coherence
proof
set C = ( Convergence T );
thus C is  (CONSTANTS)
proof
let N being  constant (net of T);
assume that
A1: N in ( NetUniv T );
( the_value_of N ) in ( Lim N ) by CKB90:1;
hence thesis by A1,CKB104:def 1;
end;

thus C is  (SUBNETS)
proof
let N being (net of T);
let Y being (subnet of N);
assume that
A2: Y in ( NetUniv T );
let p being (Element of T);
assume [ N,p ] in C;
then A3: p in ( Lim N ) by CKB104:def 1;
( Lim N ) c= ( Lim Y ) by CKB89:1;
hence thesis by A2,A3,CKB104:def 1;
end;

thus C is  (DIVERGENCE)
proof
let X being (net of T);
let p being (Element of T);
assume that
A4: X in ( NetUniv T );
assume (not [ X,p ] in C);
then (not p in ( Lim X )) by A4,CKB104:def 1;
then consider Y being (subnet of X) such that A5: Y in ( NetUniv T ) and A6: (not (ex Z being (subnet of Y) st p in ( Lim Z ))) by A4,CKB100:1;
take Y;
thus Y in ( NetUniv T ) by A5;
let Z being (subnet of Y);
(not p in ( Lim Z )) by A6;
hence thesis by CKB104:def 1;
end;

let X being (net of T);
let p being (Element of T);
assume A7: [ X,p ] in C;
let J being (net_set of (the carrier of X),T);
assume that
A8: (for i being (Element of X) holds [ ( J . i ),( X . i ) ] in C);
A9:now
let i being (Element of X);
[ ( J . i ),( X . i ) ] in C by A8;
hence (( X . i ) in ( Lim ( J . i ) ) & ( J . i ) in ( NetUniv T )) by CKB104:def 1;
end;
X in ( NetUniv T ) by A7,CKB104:def 1;
then A10: ( Iterated J ) in ( NetUniv T ) by A9,CKB78:1;
p in ( Lim X ) by A7,CKB104:def 1;
then p in ( Lim ( Iterated J ) ) by A9,CKB101:1;
hence thesis by A10,CKB104:def 1;
end;
end;
