begin
reserve X for set;
registration
let S being non empty 1-sorted;
let C being (Convergence-Class of S);
cluster ( ConvergenceSpace C ) ->  TopSpace-like;
coherence
proof
set IT = ( ConvergenceSpace C );
A1: (the topology of IT) = { V where V is (Subset of S): (for p being (Element of S) holds (p in V implies (for N being (net of S) holds ([ N,p ] in C implies N is_eventually_in V)))) } by CKB110:def 1;
reconsider V = ( [#] IT ) as (Subset of S) by CKB110:def 1;
V = (the carrier of S) by CKB110:def 1;
then (for p being (Element of S) holds (p in V implies (for N being (net of S) holds ([ N,p ] in C implies N is_eventually_in V)))) by CKB59:1;
hence (the carrier of IT) in (the topology of IT) by A1;
hereby
let a being (Subset-Family of IT);
assume that
A2: a c= (the topology of IT);
reconsider V = ( union a ) as (Subset of S) by CKB110:def 1;
now
let p being (Element of S);
assume p in V;
then consider X such that A3: p in X and A4: X in a by TARSKI:def 4;
A5: X c= V by A4,ZFMISC_1:74;
X in (the topology of IT) by A2,A4;
then A6: (ex W being (Subset of S) st (X = W & (for p being (Element of S) holds (p in W implies (for N being (net of S) holds ([ N,p ] in C implies N is_eventually_in W)))))) by A1;
let N being (net of S);
assume [ N,p ] in C;
hence N is_eventually_in V by A3,A6,A5,WAYBEL_0:8;
end;
hence ( union a ) in (the topology of IT) by A1;
end;
let a being (Subset of IT),b being (Subset of IT);
assume a in (the topology of IT);
then consider Va being (Subset of S) such that A7: a = Va and A8: (for p being (Element of S) holds (p in Va implies (for N being (net of S) holds ([ N,p ] in C implies N is_eventually_in Va)))) by A1;
reconsider V = ( a /\ b ) as (Subset of S) by CKB110:def 1;
assume b in (the topology of IT);
then consider Vb being (Subset of S) such that A9: b = Vb and A10: (for p being (Element of S) holds (p in Vb implies (for N being (net of S) holds ([ N,p ] in C implies N is_eventually_in Vb)))) by A1;
now
let p being (Element of S);
assume that
A11: p in V;
let N being (net of S);
assume A12: [ N,p ] in C;
p in b by A11,XBOOLE_0:def 4;
then N is_eventually_in Vb by A9,A10,A12;
then consider i2 being (Element of N) such that A13: (for j being (Element of N) holds (i2 <= j implies ( N . j ) in Vb)) by WAYBEL_0:def 11;
p in a by A11,XBOOLE_0:def 4;
then N is_eventually_in Va by A7,A8,A12;
then consider i1 being (Element of N) such that A14: (for j being (Element of N) holds (i1 <= j implies ( N . j ) in Va)) by WAYBEL_0:def 11;
consider i being (Element of N) such that A15: i1 <= i and A16: i2 <= i by CKB15:def 1;
thus N is_eventually_in V
proof
take i;
let j being (Element of N);
assume A17: i <= j;
then i2 <= j by A16,YELLOW_0:def 2;
then A18: ( N . j ) in Vb by A13;
i1 <= j by A15,A17,YELLOW_0:def 2;
then ( N . j ) in Va by A14;
hence thesis by A7,A9,A18,XBOOLE_0:def 4;
end;

end;
hence thesis by A1;
end;
end;
