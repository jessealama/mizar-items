begin
reserve x for set;
reserve y for set;
reserve z for set;
reserve T for Universe;
registration
let T being non  empty 1-sorted;
let Y being (net of T);
let J being (net_set of (the carrier of Y),T);
cluster ( product J ) ->  directed  transitive;
coherence
proof
A1: (the carrier of ( product J )) = ( product ( Carrier J ) ) by YELLOW_1:def 4;
thus ( product J ) is  directed
proof
let x being (Element of ( product J ));
let y being (Element of ( product J ));
defpred P[ (Element of Y),set ]
 means
([ ( x . $1 ),$2 ] in (the InternalRel of ( J . $1 )) & [ ( y . $1 ),$2 ] in (the InternalRel of ( J . $1 )));
A2:now
let i being (Element of Y);
consider zi being (Element of ( J . i )) such that A3: (( x . i ) <= zi & ( y . i ) <= zi) by CKB15:def 1;
reconsider z = zi as set;
take z;
thus P[ i,z ] by A3,ORDERS_2:def 5;
end;
consider z being (ManySortedSet of (the carrier of Y)) such that A4: (for i being (Element of Y) holds P[ i,( z . i ) ]) from PBOOLE:sch 6(A2);
A5:now
let i being set;
assume i in ( dom ( Carrier J ) );
then reconsider j = i as (Element of Y) by PARTFUN1:def 2;
[ ( x . j ),( z . j ) ] in (the InternalRel of ( J . j )) by A4;
then ( z . j ) in (the carrier of ( J . j )) by ZFMISC_1:87;
hence ( z . i ) in ( ( Carrier J ) . i ) by CKB5:1;
end;
( dom z ) = (the carrier of Y) by PARTFUN1:def 2
.= ( dom ( Carrier J ) ) by PARTFUN1:def 2;
then reconsider z as (Element of ( product J )) by A1,A5,CARD_3:9;
take z;
(for i being set holds (i in (the carrier of Y) implies (ex R being RelStr st (ex xi,yi being (Element of R) st (((R = ( J . i ) & xi = ( x . i )) & yi = ( z . i )) & xi <= yi)))))
proof
let i being set;
assume i in (the carrier of Y);
then reconsider j = i as (Element of Y);
reconsider xi = ( x . j ),zi = ( z . j ) as (Element of ( J . j ));
take ( J . j );
take xi;
take zi;
thus ((( J . j ) = ( J . i ) & xi = ( x . i )) & zi = ( z . i ));
[ xi,zi ] in (the InternalRel of ( J . j )) by A4;
hence thesis by ORDERS_2:def 5;
end;
hence x <= z by A1,YELLOW_1:def 4;
(for i being set holds (i in (the carrier of Y) implies (ex R being RelStr st (ex yi,zi being (Element of R) st (((R = ( J . i ) & yi = ( y . i )) & zi = ( z . i )) & yi <= zi)))))
proof
let i being set;
assume i in (the carrier of Y);
then reconsider j = i as (Element of Y);
reconsider yi = ( y . j ),zi = ( z . j ) as (Element of ( J . j ));
take ( J . j );
take yi;
take zi;
thus ((( J . j ) = ( J . i ) & yi = ( y . i )) & zi = ( z . i ));
[ yi,zi ] in (the InternalRel of ( J . j )) by A4;
hence thesis by ORDERS_2:def 5;
end;
hence thesis by A1,YELLOW_1:def 4;
end;

let x being (Element of ( product J ));
let y being (Element of ( product J ));
let z being (Element of ( product J ));
assume that
A6: x <= y
and
A7: y <= z;
A8: (ex fy,gz being Function st ((fy = y & gz = z) & (for i being set holds (i in (the carrier of Y) implies (ex R being RelStr st (ex xi,yi being (Element of R) st (((R = ( J . i ) & xi = ( fy . i )) & yi = ( gz . i )) & xi <= yi))))))) by A1,A7,YELLOW_1:def 4;
A9: (ex fx,gy being Function st ((fx = x & gy = y) & (for i being set holds (i in (the carrier of Y) implies (ex R being RelStr st (ex xi,yi being (Element of R) st (((R = ( J . i ) & xi = ( fx . i )) & yi = ( gy . i )) & xi <= yi))))))) by A1,A6,YELLOW_1:def 4;
(for i being set holds (i in (the carrier of Y) implies (ex R being RelStr st (ex xi,yi being (Element of R) st (((R = ( J . i ) & xi = ( x . i )) & yi = ( z . i )) & xi <= yi)))))
proof
let i being set;
assume A10: i in (the carrier of Y);
then reconsider j = i as (Element of Y);
consider R1 being RelStr,xi,yi being (Element of R1) such that A11: R1 = ( J . i ) and A12: xi = ( x . i ) and A13: (yi = ( y . i ) & xi <= yi) by A9,A10;
consider R2 being RelStr,yi9,zi being (Element of R2) such that A14: R2 = ( J . i ) and A15: yi9 = ( y . i ) and A16: zi = ( z . i ) and A17: yi9 <= zi by A8,A10;
reconsider xi,zi as (Element of ( J . j )) by A11,A14;
take ( J . j );
take xi;
take zi;
thus ((( J . j ) = ( J . i ) & xi = ( x . i )) & zi = ( z . i )) by A12,A16;
thus thesis by A11,A13,A14,A15,A17,YELLOW_0:def 2;
end;
hence thesis by A1,YELLOW_1:def 4;
end;
end;
