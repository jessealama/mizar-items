reserve x for set;
reserve y for set;
theorem
Th49: (for S being non empty 1-sorted holds (for C being (Convergence-Class of S) holds C c= ( Convergence ( ConvergenceSpace C ) )))
proof
let S being non empty 1-sorted;
let C being (Convergence-Class of S);
set T = ( ConvergenceSpace C );
let x;
let y;
assume A1: [ x,y ] in C;
C c= [: ( NetUniv S ),(the carrier of S) :] by CKB102:def 1;
then consider M being (Element of ( NetUniv S )),p being (Element of S) such that A2: [ x,y ] = [ M,p ] by A1,DOMAIN_1:1;
reconsider q = p as (Point of T) by CKB110:def 1;
A3: ((the carrier of S) = (the carrier of T) & M in ( NetUniv S )) by CKB110:def 1;
(ex N being  strict (net of S) st (N = M & (the carrier of N) in ( the_universe_of (the carrier of S) ))) by CKB65:def 1;
then reconsider M as (net of S);
reconsider N = M as (net of T) by CKB110:def 1;
A4: (the topology of T) = { V where V is (Subset of S): (for p being (Element of S) holds (p in V implies (for N being (net of S) holds ([ N,p ] in C implies N is_eventually_in V)))) } by CKB110:def 1;
now
let V being (a_neighborhood of q);
( Int V ) in (the topology of T) by PRE_TOPC:def 2;
then (p in ( Int V ) & (ex W being (Subset of S) st (W = ( Int V ) & (for p being (Element of S) holds (p in W implies (for N being (net of S) holds ([ N,p ] in C implies N is_eventually_in W))))))) by A4,CONNSP_2:def 1;
then M is_eventually_in ( Int V ) by A1,A2;
then consider ii being (Element of M) such that A5: (for j being (Element of M) holds (ii <= j implies ( M . j ) in ( Int V ))) by WAYBEL_0:def 11;
reconsider i = ii as (Element of N);
now
let j being (Element of N);
assume that
A6: i <= j;
reconsider jj = j as (Element of M);
( M . jj ) = ( N . j );
hence ( N . j ) in ( Int V ) by A5,A6;
end;
then (( Int V ) c= V & N is_eventually_in ( Int V )) by TOPS_1:16,WAYBEL_0:def 11;
hence N is_eventually_in V by WAYBEL_0:8;
end;
then A7: p in ( Lim N ) by CKB88:def 1;
N in ( NetUniv T ) by A3,CKB68:1;
hence thesis by A2,A7,CKB104:def 1;
end;
