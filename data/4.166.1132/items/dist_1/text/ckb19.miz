begin
definition
let S being non empty  finite set;
func distribution_family S -> non empty (Subset-Family of ( S * ))
means
:Def6: (for A being (Subset of ( S * )) holds (A in it iff (ex s being (FinSequence of S) st A = ( Finseq-EQclass s ))));
existence
proof
defpred P[ set ]
 means
(ex u being (FinSequence of S) st $1 = ( Finseq-EQclass u ));
set s = the (Element of ( S * ));
consider T being (Subset-Family of ( S * )) such that A1: (for A being (Subset of ( S * )) holds (A in T iff P[ A ])) from SUBSET_1:sch 3;
( Finseq-EQclass s ) in T by A1;
hence thesis by A1;
end;
uniqueness
proof
defpred P[ set ]
 means
(ex u being (FinSequence of S) st $1 = ( Finseq-EQclass u ));
let F1 being non empty (Subset-Family of ( S * )),F2 being non empty (Subset-Family of ( S * ));
assume that
A2: (for A being (Subset of ( S * )) holds (A in F1 iff P[ A ]))
and
A3: (for A being (Subset of ( S * )) holds (A in F2 iff P[ A ]));
thus thesis from SUBSET_1:sch 4(A2,A3);
end;
