begin
definition
let S being non  empty  finite set;
func uniform_distribution S -> (Element of ( distribution_family S )) means 
:Def12: (for s being (FinSequence of S) holds (s in it iff s is_uniformly_distributed ));
existence
proof
consider s being (FinSequence of S) such that A1: s = ( canFS S );
consider CS being non  empty (Subset of ( S * )) such that A2: CS = ( Finseq-EQclass s );
reconsider CS as (Element of ( distribution_family S )) by A2,CKB19:def 1;
take CS;
A3: s is_uniformly_distributed  by A1,CKB43:1;
(for t being (FinSequence of S) holds (t in CS implies s,t -are_prob_equivalent )) by A2,CKB16:1;
then (for t being (FinSequence of S) holds (t in CS implies t is_uniformly_distributed )) by A3,CKB41:1;
hence thesis by A3,A2,CKB45:1;
end;
uniqueness
proof
let A being (Element of ( distribution_family S ));
let B being (Element of ( distribution_family S ));
assume A4: (for s being (FinSequence of S) holds (s in A iff s is_uniformly_distributed ));
assume A5: (for s being (FinSequence of S) holds (s in B iff s is_uniformly_distributed ));
A6: (for s being set holds (s in B implies s in A))
proof
let s being set;
assume A7: s in B;
then reconsider s as (Element of ( S * ));
s is_uniformly_distributed  by A5,A7;
hence thesis by A4;
end;
(for s being set holds (s in A implies s in B))
proof
let s being set;
assume A8: s in A;
then reconsider s as (Element of ( S * ));
s is_uniformly_distributed  by A4,A8;
hence thesis by A5;
end;
hence thesis by A6,TARSKI:1;
end;
end;
