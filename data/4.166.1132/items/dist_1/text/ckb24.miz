begin
theorem
Lm1: (for S being non  empty  finite set holds ( GenProbSEQ S ) is  one-to-one)
proof
let S being non  empty  finite set;
now
let x1 being set;
let x2 being set;
assume that
A1: x1 in ( distribution_family S )
and
A2: x2 in ( distribution_family S )
and
A3: ( ( GenProbSEQ S ) . x1 ) = ( ( GenProbSEQ S ) . x2 );
reconsider A1 = x1 as (Subset of ( S * )) by A1;
consider u1 being (FinSequence of S) such that A4: A1 = ( Finseq-EQclass u1 ) by A1,CKB19:def 1;
reconsider A2 = x2 as (Subset of ( S * )) by A2;
consider u2 being (FinSequence of S) such that A5: A2 = ( Finseq-EQclass u2 ) by A2,CKB19:def 1;
consider v being (FinSequence of S) such that A6: v in x2 and A7: ( ( GenProbSEQ S ) . x2 ) = ( FDprobSEQ v ) by A2,CKB22:def 1;
consider u being (FinSequence of S) such that A8: u in x1 and A9: ( ( GenProbSEQ S ) . x1 ) = ( FDprobSEQ u ) by A1,CKB22:def 1;
A10: u,v -are_prob_equivalent  by A3,A9,A7,CKB20:1;
u1,u -are_prob_equivalent  by A8,A4,CKB16:1;
then A11: u1,v -are_prob_equivalent  by A10,CKB14:1;
v,u2 -are_prob_equivalent  by A6,A5,CKB16:1;
then u1,u2 -are_prob_equivalent  by A11,CKB14:1;
hence x1 = x2 by A4,A5,CKB18:1;
end;
hence thesis by FUNCT_2:19;
end;
