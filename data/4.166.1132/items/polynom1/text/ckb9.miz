begin
definition
let L being non  empty multMagma;
let p being (FinSequence of (the carrier of L));
let a being (Element of L);
func p * a -> (FinSequence of (the carrier of L)) means 
:Def3: (( dom it ) = ( dom p ) & (for i being set holds (i in ( dom p ) implies ( it /. i ) = ( ( p /. i ) * a ))));
existence
proof
deffunc F(set) = ( ( p /. $1 ) * a );
consider f being (FinSequence of (the carrier of L)) such that A1: ( len f ) = ( len p ) and A2: (for j being Nat holds (j in ( dom f ) implies ( f /. j ) = F(j))) from FINSEQ_4:sch 2;
take f;
thus A3: ( dom f ) = ( dom p ) by A1,FINSEQ_3:29;
let j being set;
assume j in ( dom p );
hence thesis by A2,A3;
end;
uniqueness
proof
let it1 being (FinSequence of (the carrier of L));
let it2 being (FinSequence of (the carrier of L));
assume that
A4: ( dom it1 ) = ( dom p )
and
A5: (for i being set holds (i in ( dom p ) implies ( it1 /. i ) = ( ( p /. i ) * a )))
and
A6: ( dom it2 ) = ( dom p )
and
A7: (for i being set holds (i in ( dom p ) implies ( it2 /. i ) = ( ( p /. i ) * a )));
now
let j being Nat;
assume A8: j in ( dom p );
hence ( it1 /. j ) = ( ( p /. j ) * a ) by A5
.= ( it2 /. j ) by A7,A8;
end;
hence thesis by A4,A6,FINSEQ_5:12;
end;
end;
