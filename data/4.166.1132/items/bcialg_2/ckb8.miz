theorem
Th7: ( ( (x,y) to_power n ) \ z ) = ( (( x \ z ),y) to_power n )
proof
defpred P[ set ]
 means
(for m holds ((m = $1 & m <= n) implies ( ( (x,y) to_power m ) \ z ) = ( (( x \ z ),y) to_power m )));
now
let k;
assume A1: (for m holds ((m = k & m <= n) implies ( ( (x,y) to_power m ) \ z ) = ( (( x \ z ),y) to_power m )));
let m;
assume that
A2: m = ( k + 1 )
and
A3: m <= n;
( ( (x,y) to_power m ) \ z ) = ( ( ( (x,y) to_power k ) \ y ) \ z ) by A2,CKB5:1;
