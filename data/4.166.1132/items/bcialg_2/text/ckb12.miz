begin
reserve X for BCI-algebra;
reserve x for (Element of X);
reserve y for (Element of X);
reserve z for (Element of X);
reserve k for (Element of ( NAT ));
reserve n for (Element of ( NAT ));
reserve m for (Element of ( NAT ));
theorem
( (( (x,y) to_power n ),z) to_power m ) = ( (( (x,z) to_power m ),y) to_power n )
proof
defpred P[ set ]
 means
(for m1 being (Element of ( NAT )) holds ((m1 = $1 & m1 <= n) implies ( (( (x,y) to_power m1 ),z) to_power m ) = ( (( (x,z) to_power m ),y) to_power m1 )));
now
let k;
assume A1: (for m1 being (Element of ( NAT )) holds ((m1 = k & m1 <= n) implies ( (( (x,y) to_power m1 ),z) to_power m ) = ( (( (x,z) to_power m ),y) to_power m1 )));
let m1 being (Element of ( NAT ));
assume (m1 = ( k + 1 ) & m1 <= n);
then k <= n by NAT_1:13;
then ( ( (( (x,y) to_power k ),z) to_power m ) \ y ) = ( ( (( (x,z) to_power m ),y) to_power k ) \ y ) by A1
.= ( (( (x,z) to_power m ),y) to_power ( k + 1 ) ) by CKB5:1;
then ( (( (x,z) to_power m ),y) to_power ( k + 1 ) ) = ( (( ( (x,y) to_power k ) \ y ),z) to_power m ) by CKB8:1;
hence ( (( (x,y) to_power ( k + 1 ) ),z) to_power m ) = ( (( (x,z) to_power m ),y) to_power ( k + 1 ) ) by CKB5:1;
end;
then A2: (for k holds (P[ k ] implies P[ ( k + 1 ) ]));
( (( (x,y) to_power ( 0 ) ),z) to_power m ) = ( (x,z) to_power m ) by CKB2:1;
then A3: P[ ( 0 ) ] by CKB2:1;
(for n holds P[ n ]) from NAT_1:sch 1(A3,A2);
hence thesis;
end;
