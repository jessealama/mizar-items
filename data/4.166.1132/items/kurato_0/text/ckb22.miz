begin
theorem
(for X being set holds (for A,B being (SetSequence of X) holds (for C being (Subset of X) holds ((for n being (Element of ( NAT )) holds ( B . n ) = ( C \+\ ( A . n ) )) implies ( C \+\ ( lim_inf A ) ) c= ( lim_sup B )))))
proof
let X being set;
let A being (SetSequence of X);
let B being (SetSequence of X);
let C being (Subset of X);
assume A1: (for n being (Element of ( NAT )) holds ( B . n ) = ( C \+\ ( A . n ) ));
let x being set;
assume A2: x in ( C \+\ ( lim_inf A ) );
per cases  by A2,XBOOLE_0:1;
suppose A3: (x in C & (not x in ( lim_inf A )));

(for n being (Element of ( NAT )) holds (ex k being (Element of ( NAT )) st x in ( B . ( n + k ) )))
proof
let n being (Element of ( NAT ));
consider k being (Element of ( NAT )) such that A4: (not x in ( A . ( n + k ) )) by A3,CKB10:1;
take k;
x in ( C \+\ ( A . ( n + k ) ) ) by A3,A4,XBOOLE_0:1;
hence thesis by A1;
end;
hence thesis by CKB11:1;
end;
suppose A5: ((not x in C) & x in ( lim_inf A ));

then consider n being (Element of ( NAT )) such that A6: (for k being (Element of ( NAT )) holds x in ( A . ( n + k ) )) by CKB10:1;
(for m being (Element of ( NAT )) holds (ex k being (Element of ( NAT )) st x in ( B . ( m + k ) )))
proof
let m being (Element of ( NAT ));
take k = n;
x in ( A . ( m + k ) ) by A6;
then x in ( C \+\ ( A . ( m + k ) ) ) by A5,XBOOLE_0:1;
hence thesis by A1;
end;
hence thesis by CKB11:1;
end;
end;
