theorem
(for X being set holds (for A,B,C being (SetSequence of X) holds ((for n being (Element of ( NAT )) holds ( C . n ) = ( ( A . n ) /\ ( B . n ) )) implies ( lim_inf C ) = ( ( lim_inf A ) /\ ( lim_inf B ) ))))
proof
let X being set;
let A being (SetSequence of X);
let B being (SetSequence of X);
let C being (SetSequence of X);
assume A1: (for n being (Element of ( NAT )) holds ( C . n ) = ( ( A . n ) /\ ( B . n ) ));
thus ( lim_inf C ) c= ( ( lim_inf A ) /\ ( lim_inf B ) )
proof
let x being set;
assume x in ( lim_inf C );
then consider n being (Element of ( NAT )) such that A2: (for k being (Element of ( NAT )) holds x in ( C . ( n + k ) )) by CKB10:1;
(for k being (Element of ( NAT )) holds x in ( B . ( n + k ) ))
proof
let k being (Element of ( NAT ));
(( C . ( n + k ) ) = ( ( A . ( n + k ) ) /\ ( B . ( n + k ) ) ) & x in ( C . ( n + k ) )) by A1,A2;
hence thesis by XBOOLE_0:def 4;
end;
then A3: x in ( lim_inf B ) by CKB10:1;
(for k being (Element of ( NAT )) holds x in ( A . ( n + k ) ))
proof
let k being (Element of ( NAT ));
(( C . ( n + k ) ) = ( ( A . ( n + k ) ) /\ ( B . ( n + k ) ) ) & x in ( C . ( n + k ) )) by A1,A2;
hence thesis by XBOOLE_0:def 4;
end;
then x in ( lim_inf A ) by CKB10:1;
hence thesis by A3,XBOOLE_0:def 4;
end;

thus ( ( lim_inf A ) /\ ( lim_inf B ) ) c= ( lim_inf C )
proof
let x being set;
assume A4: x in ( ( lim_inf A ) /\ ( lim_inf B ) );
