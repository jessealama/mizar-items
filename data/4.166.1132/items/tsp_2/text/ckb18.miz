begin
theorem
Th11: (for Y being non  empty TopStruct holds (for Y0 being (SubSpace of Y) holds (for A being (Subset of Y) holds (A = (the carrier of Y0) implies (A is  maximal_T_0 iff Y0 is  maximal_T_0)))))
proof
let Y being non  empty TopStruct;
let Y0 being (SubSpace of Y);
let A being (Subset of Y);
assume A1: A = (the carrier of Y0);
hereby
assume A is  maximal_T_0;
then (for A being (Subset of Y) holds (A = (the carrier of Y0) implies A is  maximal_T_0)) by A1;
hence Y0 is  maximal_T_0 by CKB17:def 1;
end;
thus thesis by A1,CKB17:def 1;
end;
