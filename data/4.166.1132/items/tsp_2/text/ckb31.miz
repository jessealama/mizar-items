environ
vocabularies XBOOLE_0,PRE_TOPC,SUBSET_1,TEX_4,RCOMP_1,TARSKI,STRUCT_0,TOPS_1,ZFMISC_1,SETFAM_1,FUNCT_1,RELAT_1,NATTRA_1,ORDINAL2,BORSUK_1,TSP_2;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,FUNCT_1,RELAT_1,RELSET_1,FUNCT_2,DOMAIN_1,STRUCT_0,PRE_TOPC,TOPS_1,BORSUK_1,TSEP_1,TEX_2,TEX_3,TEX_4,TSP_1,CKB1,CKB2,CKB3,CKB4,CKB6,CKB14,CKB17,CKB21;
definitions SUBSET_1,STRUCT_0,RELAT_1,CKB1,CKB2,CKB3,CKB4,CKB6,CKB14,CKB17,CKB21;
theorems TARSKI,ZFMISC_1,FUNCT_1,FUNCT_2,PRE_TOPC,TOPS_1,BORSUK_1,TSEP_1,TOPS_3,TEX_2,TEX_3,TEX_4,TSP_1,RELAT_1,XBOOLE_0,XBOOLE_1,SUBSET_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB21,CKB22;
schemes TEX_2,FUNCT_2;
registrations XBOOLE_0,SUBSET_1,RELSET_1,STRUCT_0,PRE_TOPC,TOPS_1,TSP_1,CKB19,CKB20,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30;
constructors TOPS_1,BORSUK_1,TSEP_1,TEX_2,TEX_4,TSP_1,TEX_3,CKB4,CKB17;
requirements BOOLE,SUBSET;
begin
Lm1:now
let Z being non  empty TopStruct;
let Z0 being (SubSpace of Z);
( [#] Z0 ) c= ( [#] Z ) by PRE_TOPC:def 4;
hence (the carrier of Z0) is (Subset of Z);
end;
reserve X for non  empty TopSpace;
theorem
(for Y0 being  T_0 non  empty (SubSpace of X) holds (ex X0 being  strict (SubSpace of X) st (Y0 is (SubSpace of X0) & X0 is  maximal_T_0)))
proof
let Y0 being  T_0 non  empty (SubSpace of X);
reconsider A = (the carrier of Y0) as (Subset of X) by Lm1;
A is  T_0 by TSP_1:13;
then consider M being (Subset of X) such that A1: A c= M and A2: M is  maximal_T_0 by CKB15:1;
M is non  empty by A2,CKB9:1;
then consider X0 being  strict non  empty (SubSpace of X) such that A3: X0 is  maximal_T_0 and A4: M = (the carrier of X0) by A2,CKB22:1;
take X0;
thus thesis by A1,A3,A4,TSEP_1:4;
end;
