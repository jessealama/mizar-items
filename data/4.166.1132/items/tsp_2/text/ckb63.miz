begin
reserve X for non  empty TopSpace;
reserve X0 for non  empty (maximal_Kolmogorov_subspace of X);
theorem
(for A being (Subset of X) holds (A is  open implies ( ( Stone-retraction (X,X0) ) .: A ) is  open))
proof
let A being (Subset of X);
reconsider M = (the carrier of X0) as (Subset of X) by TSEP_1:1;
set B = ( ( Stone-retraction (X,X0) ) .: A );
assume A1: A is  open;
then A = ( MaxADSet A ) by TEX_4:56;
then ( A /\ M ) = B by CKB57:def 1;
hence thesis by A1,TSP_1:def 1;
end;
