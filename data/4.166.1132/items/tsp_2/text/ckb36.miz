begin
reserve X for non  empty TopSpace;
theorem
Th16: (for X0 being (maximal_Kolmogorov_subspace of X) holds (for F being (Subset of X) holds (for F0 being (Subset of X0) holds (F0 = F implies (F0 is  closed iff (( MaxADSet F ) is  closed & F0 = ( ( MaxADSet F ) /\ (the carrier of X0) )))))))
proof
let X0 being (maximal_Kolmogorov_subspace of X);
reconsider M = (the carrier of X0) as (Subset of X) by TSEP_1:1;
let F being (Subset of X);
let F0 being (Subset of X0);
assume A1: F0 = F;
A2: M is  maximal_T_0 by CKB18:1;
thus (F0 is  closed implies (( MaxADSet F ) is  closed & F0 = ( ( MaxADSet F ) /\ (the carrier of X0) )))
proof
assume F0 is  closed;
then A3: (ex H being (Subset of X) st (H is  closed & F0 = ( H /\ M ))) by TSP_1:def 2;
hence ( MaxADSet F ) is  closed by A2,A1,CKB10:1;
thus thesis by A2,A1,A3,CKB10:1;
end;

assume A4: ( MaxADSet F ) is  closed;
assume F0 = ( ( MaxADSet F ) /\ (the carrier of X0) );
hence thesis by A4,TSP_1:def 2;
end;
