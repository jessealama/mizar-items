begin
reserve X for non  empty TopSpace;
theorem
Th5: (for M being (Subset of X) holds (M is  maximal_T_0 implies (for A being (Subset of X) holds (A is  closed implies A = ( MaxADSet ( M /\ A ) )))))
proof
let M being (Subset of X);
assume A1: M is  maximal_T_0;
let A being (Subset of X);
assume A2: A is  closed;
then ( MaxADSet ( M /\ A ) ) = ( ( MaxADSet M ) /\ ( MaxADSet A ) ) by TEX_4:64;
then A3: ( MaxADSet ( M /\ A ) ) = ( ( MaxADSet M ) /\ A ) by A2,TEX_4:60;
A = ( (the carrier of X) /\ A ) by XBOOLE_1:28;
hence thesis by A1,A3,CKB6:def 1;
end;
