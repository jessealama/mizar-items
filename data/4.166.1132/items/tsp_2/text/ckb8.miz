constructors TOPS_1,BORSUK_1,TSEP_1,TEX_2,TEX_4,TSP_1,TEX_3,CKB4;
requirements BOOLE,SUBSET;
begin
reserve X for non  empty TopSpace;
theorem
Th3: (for A being (Subset of X) holds ((A is  open or A is  closed) implies (A is  maximal_T_0 implies (not A is  proper))))
proof
let A being (Subset of X);
assume (A is  open or A is  closed);
then A1: A = ( MaxADSet A ) by TEX_4:56,TEX_4:60;
assume A is  maximal_T_0;
then A = (the carrier of X) by A1,CKB6:def 1;
hence thesis by SUBSET_1:def 6;
end;
