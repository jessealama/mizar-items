begin
Lm1:now
let Z being non  empty TopStruct;
let Z0 being (SubSpace of Z);
( [#] Z0 ) c= ( [#] Z ) by PRE_TOPC:def 4;
hence (the carrier of Z0) is (Subset of Z);
end;
registration
let Y being non  empty TopStruct;
cluster  maximal_T_0 ->  T_0 for non  empty non  empty (SubSpace of Y);
coherence
proof
let Y0 being non  empty (SubSpace of Y);
reconsider A = (the carrier of Y0) as (Subset of Y) by Lm1;
assume Y0 is  maximal_T_0;
then A is  maximal_T_0 by CKB18:1;
then A is  T_0 by CKB4:def 1;
hence thesis by TSP_1:13;
end;
