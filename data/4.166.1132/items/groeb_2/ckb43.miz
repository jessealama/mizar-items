theorem
(for L being non empty multLoopStr holds (for P,Q being non empty (Subset of L) holds (for A being (LeftLinearCombination of P) holds (P c= Q implies A is (LeftLinearCombination of Q)))))
proof
let L being non empty multLoopStr;
let P being non empty (Subset of L),Q being non empty (Subset of L);
let A being (LeftLinearCombination of P);
assume A1: P c= Q;
now
let i being set;
assume i in ( dom A );
then consider u being (Element of L),a being (Element of P) such that A2: ( A /. i ) = ( u * a ) by IDEAL_1:def 9;
a in P;
hence (ex u being (Element of L) st (ex a being (Element of Q) st ( A /. i ) = ( u * a ))) by A1,A2;
end;
hence thesis by IDEAL_1:def 9;
end;
