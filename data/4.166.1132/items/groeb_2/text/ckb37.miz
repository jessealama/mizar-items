theorem
Th30: (for n being (Element of ( NAT )) holds (for T being  admissible  connected (TermOrder of n) holds (for L being  add-associative  right_complementable  right_zeroed  commutative  associative  well-unital  distributive  Abelian  almost_left_invertible non degenerated non empty doubleLoopStr holds (for G being (Subset of ( Polynom-Ring (n,L) )) holds ((not ( 0_ (n,L) ) in G) implies ((for g1,g2 being (Polynomial of n,L) holds ((g1 in G & g2 in G) implies ( PolyRedRel (G,T) ) reduces ( S-Poly (g1,g2,T) ),( 0_ (n,L) ))) implies G is_Groebner_basis_wrt T))))))
proof
let n being (Element of ( NAT ));
let T being  admissible  connected (TermOrder of n);
let L being  add-associative  right_complementable  right_zeroed  commutative  associative  well-unital  distributive  Abelian  almost_left_invertible non degenerated non empty doubleLoopStr;
let G being (Subset of ( Polynom-Ring (n,L) ));
assume A1: (not ( 0_ (n,L) ) in G);
assume A2: (for g1,g2 being (Polynomial of n,L) holds ((g1 in G & g2 in G) implies ( PolyRedRel (G,T) ) reduces ( S-Poly (g1,g2,T) ),( 0_ (n,L) )));
now
let g1 being (Polynomial of n,L),g2 being (Polynomial of n,L);
assume that
g1 <> g2
and
A3: g1 in G
and
A4: g2 in G;
thus (for m1,m2 being (Monomial of n,L) holds (( HM (( m1 *' g1 ),T) ) = ( HM (( m2 *' g2 ),T) ) implies ( PolyRedRel (G,T) ) reduces ( ( m1 *' g1 ) - ( m2 *' g2 ) ),( 0_ (n,L) )))
