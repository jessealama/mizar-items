requirements NUMERALS,REAL,SUBSET,BOOLE,ARITHM;
begin
registration
let n being Ordinal;
let T being  connected (TermOrder of n);
let L being  add-associative  right_complementable  right_zeroed  commutative  associative  well-unital  distributive  almost_left_invertible non  trivial doubleLoopStr;
let P being  finite (Subset of ( Polynom-Ring (n,L) ));
cluster ( S-Poly (P,T) ) ->  finite;
coherence
proof
defpred P[ set,set ]
 means
(ex p1,p2 being (Polynomial of n,L) st ((((p1 = ( $1 `1 ) & ( $1 `2 ) = p2) & p1 in P) & p2 in P) & $2 = ( S-Poly (p1,p2,T) )));
A1: (for x being set holds (x in [: P,P :] implies (ex y being set st P[ x,y ])))
proof
let x being set;
assume x in [: P,P :];
then consider p1,p2 being set such that A2: (p1 in P & p2 in P) and A3: [ p1,p2 ] = x by ZFMISC_1:def 2;
reconsider p1,p2 as (Polynomial of n,L) by A2,POLYNOM1:def 10;
take ( S-Poly (p1,p2,T) );
take p1;
take p2;
thus ( x `1 ) = p1 by A3,MCART_1:def 1;
thus ( x `2 ) = p2 by A3,MCART_1:def 2;
thus thesis by A2;
end;
consider f being Function such that A4: (( dom f ) = [: P,P :] & (for x being set holds (x in [: P,P :] implies P[ x,( f . x ) ]))) from CLASSES1:sch 1(A1);
A5:now
let v being set;
assume v in ( S-Poly (P,T) );
then consider p1,p2 being (Polynomial of n,L) such that A6: v = ( S-Poly (p1,p2,T) ) and A7: (p1 in P & p2 in P);
A8: [ p1,p2 ] in ( dom f ) by A4,A7,ZFMISC_1:def 2;
then consider p19,p29 being (Polynomial of n,L) such that A9: (( [ p1,p2 ] `1 ) = p19 & ( [ p1,p2 ] `2 ) = p29) and p19 in P and p29 in P and A10: ( f . [ p1,p2 ] ) = ( S-Poly (p19,p29,T) ) by A4;
(p1 = p19 & p2 = p29) by A9,MCART_1:def 1,MCART_1:def 2;
hence v in ( rng f ) by A6,A8,A10,FUNCT_1:def 3;
end;
now
let v being set;
assume v in ( rng f );
then consider u being set such that A11: u in ( dom f ) and A12: v = ( f . u ) by FUNCT_1:def 3;
(ex p1,p2 being (Polynomial of n,L) st ((((p1 = ( u `1 ) & ( u `2 ) = p2) & p1 in P) & p2 in P) & ( f . u ) = ( S-Poly (p1,p2,T) ))) by A4,A11;
hence v in ( S-Poly (P,T) ) by A12;
end;
then ( rng f ) = ( S-Poly (P,T) ) by A5,TARSKI:1;
hence thesis by A4,FINSET_1:8;
end;
end;
