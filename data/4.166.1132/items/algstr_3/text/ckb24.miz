environ
vocabularies STRUCT_0,SUBSET_1,MULTOP_1,XBOOLE_0,VECTSP_1,FUNCT_1,NUMBERS,REAL_1,RELAT_1,ARYTM_3,CARD_1,ARYTM_1,MESFUNC1,SUPINF_2,ALGSTR_3;
notations XBOOLE_0,SUBSET_1,NUMBERS,STRUCT_0,REAL_1,MULTOP_1,CKB1,CKB3,CKB4,CKB5,CKB6,CKB8,CKB21,CKB23;
definitions STRUCT_0,CKB4,CKB5,CKB6,CKB8,CKB21;
theorems MULTOP_1,XCMPLX_1,CKB4,CKB5,CKB6,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21;
schemes MULTOP_1;
registrations NUMBERS,MEMBERED,STRUCT_0,CKB1,CKB2,CKB7,CKB22;
constructors REAL_1,MEMBERED,MULTOP_1,RLVECT_1,CKB1,CKB4,CKB5,CKB6,CKB8,CKB21;
requirements NUMERALS,SUBSET,ARITHM,BOOLE;
begin
reserve F for Ternary-Field;
reserve a for (Scalar of F);
reserve a9 for (Scalar of F);
reserve u for (Scalar of F);
reserve u9 for (Scalar of F);
reserve v for (Scalar of F);
reserve v9 for (Scalar of F);
theorem
(((a <> a9 & ( Tern (u,a,v) ) = ( Tern (u9,a,v9) )) & ( Tern (u,a9,v) ) = ( Tern (u9,a9,v9) )) implies (u = u9 & v = v9))
proof
assume that
A1: a <> a9
and
A2: ( Tern (u,a,v) ) = ( Tern (u9,a,v9) )
and
A3: ( Tern (u,a9,v) ) = ( Tern (u9,a9,v9) );
u = u9 by A1,A2,A3,CKB21:def 1;
hence thesis by A2,CKB21:def 1;
end;
