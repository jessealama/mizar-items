begin
definition
let n being (Element of ( NAT ));
let p being (Element of ( n -tuples_on ( NAT ) ));
let q being (Element of ( n -tuples_on ( NAT ) ));
pred p < q
means
:Def1: (ex i being (Element of ( NAT )) st ((i in ( Seg n ) & ( p . i ) < ( q . i )) & (for k being (Element of ( NAT )) holds ((1 <= k & k < i) implies ( p . k ) = ( q . k )))))
;asymmetry
proof
let p being (Element of ( n -tuples_on ( NAT ) ));
let q being (Element of ( n -tuples_on ( NAT ) ));
given i being (Element of ( NAT )) such that
A1: i in ( Seg n )
and
A2: ( p . i ) < ( q . i )
and
A3: (for k being (Element of ( NAT )) holds ((1 <= k & k < i) implies ( p . k ) = ( q . k )));

A4: 1 <= i by A1,FINSEQ_1:1;
given j being (Element of ( NAT )) such that
A5: j in ( Seg n )
and
A6: ( q . j ) < ( p . j )
and
A7: (for k being (Element of ( NAT )) holds ((1 <= k & k < j) implies ( q . k ) = ( p . k )));

A8: 1 <= j by A5,FINSEQ_1:1;
per cases  by XXREAL_0:1;
suppose i < j;

hence contradiction by A2,A7,A4;
end;
suppose j < i;

hence contradiction by A3,A6,A8;
end;
suppose i = j;

hence contradiction by A2,A6;
end;
end;
end;
