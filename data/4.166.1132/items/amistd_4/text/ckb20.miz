begin
reserve N for  with_non-empty_elements non  empty set;
theorem
(for A being  with_non_trivial_ObjectKinds  IC-Ins-separated non  empty AMI-Struct over N holds (for I being (Instruction of A) holds ( Out_U_Inp I ) = ( ( Output I ) \/ ( Input I ) )))
proof
let A being  with_non_trivial_ObjectKinds  IC-Ins-separated non  empty AMI-Struct over N;
let I being (Instruction of A);
(for o being (Object of A) holds (o in ( Out_U_Inp I ) implies o in ( ( Output I ) \/ ( Input I ) )))
proof
let o being (Object of A);
assume that
A1: o in ( Out_U_Inp I );
(o in ( Input I ) or o in ( Output I ))
proof
assume A2: (not o in ( Input I ));
per cases  by A2,XBOOLE_0:def 5;
suppose (not o in ( Out_U_Inp I ));

hence thesis by A1;
end;
suppose A3: o in ( Out_\_Inp I );

( Out_\_Inp I ) c= ( Output I ) by CKB17:1;
hence thesis by A3;
end;
end;
hence thesis by XBOOLE_0:def 3;
end;
hence ( Out_U_Inp I ) c= ( ( Output I ) \/ ( Input I ) ) by SUBSET_1:2;
(( Output I ) c= ( Out_U_Inp I ) & ( Input I ) c= ( Out_U_Inp I )) by CKB18:1,XBOOLE_1:36;
hence thesis by XBOOLE_1:8;
end;
