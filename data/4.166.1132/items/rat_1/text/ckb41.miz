begin
reserve k for (Element of ( NAT ));
reserve k1 for (Element of ( NAT ));
reserve l for (Element of ( NAT ));
reserve l1 for (Element of ( NAT ));
reserve m for Integer;
reserve m1 for Integer;
reserve n for Integer;
reserve n1 for Integer;
reserve p for Rational;
definition
let p;
func denominator p -> (Element of ( NAT )) means 
:Def3: ((it <> ( 0 ) & (ex m st p = ( m / it ))) & (for n holds (for k holds ((k <> ( 0 ) & p = ( n / k )) implies it <= k))));
existence
proof
consider m,k such that A1: k <> ( 0 ) and A2: p = ( m / k ) and A3: (for n holds (for l holds ((l <> ( 0 ) & p = ( n / l )) implies k <= l))) by CKB40:1;
take k;
thus thesis by A1,A2,A3;
end;
uniqueness
proof
let k;
let l;
assume that
A4: k <> ( 0 )
and
A5: (ex m st p = ( m / k ))
and
A6: (for m1 holds (for k1 holds ((k1 <> ( 0 ) & p = ( m1 / k1 )) implies k <= k1)))
and
A7: l <> ( 0 )
and
A8: (ex n st p = ( n / l ))
and
A9: (for n1 holds (for l1 holds ((l1 <> ( 0 ) & p = ( n1 / l1 )) implies l <= l1)));
A10: l <= k by A4,A5,A9;
k <= l by A6,A7,A8;
hence thesis by A10,XXREAL_0:1;
end;
end;
