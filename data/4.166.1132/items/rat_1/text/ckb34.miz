begin
reserve m1 for Integer;
reserve n1 for Integer;
reserve p for Rational;
reserve q for Rational;
theorem
Th16: (for p holds (for q holds ( p / q ) is Rational))
proof
let p;
let q;
consider m1,n1 being Integer such that n1 <> ( 0 ) and A1: p = ( m1 / n1 ) by CKB13:1;
consider m2,n2 being Integer such that n2 <> ( 0 ) and A2: q = ( m2 / n2 ) by CKB13:1;
( p / q ) = ( p * ( 1 / ( m2 / n2 ) ) ) by A2
.= ( p * ( ( 1 * n2 ) / m2 ) ) by XCMPLX_1:77
.= ( ( m1 * n2 ) / ( n1 * m2 ) ) by A1,XCMPLX_1:76;
hence thesis by CKB15:1;
end;
