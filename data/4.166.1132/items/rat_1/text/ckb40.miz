begin
reserve k for (Element of ( NAT ));
reserve k1 for (Element of ( NAT ));
reserve l for (Element of ( NAT ));
reserve m for Integer;
reserve m1 for Integer;
reserve n for Integer;
reserve n1 for Integer;
reserve p for Rational;
theorem
Th25: (ex m st (ex k st ((k <> ( 0 ) & p = ( m / k )) & (for n holds (for l holds ((l <> ( 0 ) & p = ( n / l )) implies k <= l))))))
proof
defpred P[ Nat ]
 means
($1 <> ( 0 ) & (ex m1 st p = ( m1 / $1 )));
(ex m st (ex k st (k <> ( 0 ) & p = ( m / k )))) by CKB39:1;
then A1: (ex l being Nat st P[ l ]);
(ex k1 being Nat st (P[ k1 ] & (for l1 being Nat holds (P[ l1 ] implies k1 <= l1)))) from NAT_1:sch 5(A1);
then consider k1 being Nat such that A2: k1 <> ( 0 ) and A3: (ex m1 st p = ( m1 / k1 )) and A4: (for l1 being Nat holds ((l1 <> ( 0 ) & (ex n1 st p = ( n1 / l1 ))) implies k1 <= l1));
reconsider k1 as (Element of ( NAT )) by ORDINAL1:def 12;
consider m1 such that A5: p = ( m1 / k1 ) and A6: (for l1 being Nat holds ((l1 <> ( 0 ) & (ex n1 st p = ( n1 / l1 ))) implies k1 <= l1)) by A3,A4;
take m1;
take k1;
thus k1 <> ( 0 ) by A2;
thus ( m1 / k1 ) = p by A5;
thus thesis by A6;
end;
