end;
hence thesis by A1,RELAT_1:68;
end;
hence ( ( diff (( sin ),Z) ) . i ) is_differentiable_on Z by A1,FDIFF_1:def 6;
end;
hence ( sin ) is_differentiable_on n,Z by TAYLOR_1:def 6;
