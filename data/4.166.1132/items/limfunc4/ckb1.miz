begin
reserve r for Real;
reserve g for Real;
theorem
Lm1: (( 0 ) < g implies (( r - g ) < r & r < ( r + g )))
proof
assume A1: ( 0 ) < g;
then ( r - g ) < ( r - ( 0 ) ) by XREAL_1:15;
hence ( r - g ) < r;
( r + ( 0 ) ) < ( r + g ) by A1,XREAL_1:8;
hence thesis;
end;
