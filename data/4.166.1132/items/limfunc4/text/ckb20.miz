environ
vocabularies REAL_1,PARTFUN1,NUMBERS,CARD_1,ARYTM_3,ARYTM_1,SEQ_1,RELAT_1,TARSKI,FUNCT_2,SUBSET_1,FUNCT_1,XBOOLE_0,LIMFUNC1,LIMFUNC2,SEQ_2,ORDINAL2,XXREAL_1,XXREAL_0,NAT_1,LIMFUNC3;
notations TARSKI,SUBSET_1,NUMBERS,XXREAL_0,REAL_1,NAT_1,SEQ_1,SEQ_2,VALUED_0,RCOMP_1,RELSET_1,PARTFUN1,FUNCT_2,LIMFUNC1,LIMFUNC2,LIMFUNC3;
definitions TARSKI,PROB_1,LIMFUNC1;
theorems TARSKI,NAT_1,FUNCT_1,SEQ_4,RCOMP_1,LIMFUNC1,LIMFUNC2,LIMFUNC3,RELAT_1,XBOOLE_0,XBOOLE_1,XREAL_1,XXREAL_1,FUNCT_2,VALUED_0,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19;
registrations RELSET_1,MEMBERED,VALUED_0,ORDINAL1,NUMBERS,XBOOLE_0,SEQ_4,FUNCT_2;
constructors PARTFUN1,REAL_1,NAT_1,SEQ_2,SEQM_3,PROB_1,RCOMP_1,RFUNCT_2,LIMFUNC1,LIMFUNC2,LIMFUNC3,SEQ_1,MEMBERED,RELSET_1;
requirements SUBSET,ARITHM,BOOLE;
begin
reserve r for Real;
reserve g for Real;
reserve x0 for Real;
reserve f1 for (PartFunc of ( REAL ),( REAL ));
reserve f2 for (PartFunc of ( REAL ),( REAL ));
theorem
(((f1 is_right_divergent_to-infty_in x0 & f2 is  divergent_in-infty_to-infty) & (for r holds (x0 < r implies (ex g st ((g < r & x0 < g) & g in ( dom ( f2 * f1 ) )))))) implies ( f2 * f1 ) is_right_divergent_to-infty_in x0)
proof
assume that
A1: f1 is_right_divergent_to-infty_in x0
and
A2: f2 is  divergent_in-infty_to-infty
and
A3: (for r holds (x0 < r implies (ex g st ((g < r & x0 < g) & g in ( dom ( f2 * f1 ) )))));
now
let s being Real_Sequence;
assume that
A4: (s is  convergent & ( lim s ) = x0)
and
A5: ( rng s ) c= ( ( dom ( f2 * f1 ) ) /\ ( right_open_halfline x0 ) );
A6: ( rng s ) c= ( dom ( f2 * f1 ) ) by A5,CKB3:1;
( rng s ) c= ( ( dom f1 ) /\ ( right_open_halfline x0 ) ) by A5,CKB3:1;
then A7: ( f1 /* s ) is  divergent_to-infty by A1,A4,LIMFUNC2:def 6;
( rng ( f1 /* s ) ) c= ( dom f2 ) by A5,CKB3:1;
then ( f2 /* ( f1 /* s ) ) is  divergent_to-infty by A2,A7,LIMFUNC1:def 11;
hence ( ( f2 * f1 ) /* s ) is  divergent_to-infty by A6,VALUED_0:31;
end;
hence thesis by A3,LIMFUNC2:def 6;
end;
