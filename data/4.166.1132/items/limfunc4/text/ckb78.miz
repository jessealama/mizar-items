begin
reserve r for Real;
reserve g for Real;
reserve g1 for Real;
reserve g2 for Real;
reserve x0 for Real;
reserve f1 for (PartFunc of ( REAL ),( REAL ));
reserve f2 for (PartFunc of ( REAL ),( REAL ));
theorem
((((f1 is_right_convergent_in x0 & f2 is_convergent_in ( lim_right (f1,x0) )) & (for r holds (x0 < r implies (ex g st ((g < r & x0 < g) & g in ( dom ( f2 * f1 ) )))))) & (ex g st (( 0 ) < g & (for r holds (r in ( ( dom f1 ) /\ ]. x0,( x0 + g ) .[ ) implies ( f1 . r ) <> ( lim_right (f1,x0) )))))) implies (( f2 * f1 ) is_right_convergent_in x0 & ( lim_right (( f2 * f1 ),x0) ) = ( lim (f2,( lim_right (f1,x0) )) )))
proof
assume that
A1: f1 is_right_convergent_in x0
and
A2: f2 is_convergent_in ( lim_right (f1,x0) )
and
A3: (for r holds (x0 < r implies (ex g st ((g < r & x0 < g) & g in ( dom ( f2 * f1 ) )))));
given g such that
A4: ( 0 ) < g
and
A5: (for r holds (r in ( ( dom f1 ) /\ ]. x0,( x0 + g ) .[ ) implies ( f1 . r ) <> ( lim_right (f1,x0) )));

A6:now
let s being Real_Sequence;
assume that
A7: (s is  convergent & ( lim s ) = x0)
and
A8: ( rng s ) c= ( ( dom ( f2 * f1 ) ) /\ ( right_open_halfline x0 ) );
consider k being (Element of ( NAT )) such that A9: (for n being (Element of ( NAT )) holds (k <= n implies ( s . n ) < ( x0 + g ))) by A4,A7,CKB1:1,LIMFUNC2:2;
A10: ( rng s ) c= ( dom f1 ) by A8,CKB3:1;
set q = ( ( f1 /* s ) ^\ k );
A11: ( rng s ) c= ( dom ( f2 * f1 ) ) by A8,CKB3:1;
A12: ( rng s ) c= ( right_open_halfline x0 ) by A8,CKB3:1;
now
let x being set;
assume x in ( rng q );
then consider n being (Element of ( NAT )) such that A13: ( q . n ) = x by FUNCT_2:113;
A14: ( f1 . ( s . ( n + k ) ) ) = ( ( f1 /* s ) . ( n + k ) ) by A10,FUNCT_2:108
.= x by A13,NAT_1:def 3;
A15: ( s . ( n + k ) ) < ( x0 + g ) by A9,NAT_1:12;
A16: ( s . ( n + k ) ) in ( rng s ) by VALUED_0:28;
then ( s . ( n + k ) ) in ( right_open_halfline x0 ) by A12;
then ( s . ( n + k ) ) in { g1: x0 < g1 } by XXREAL_1:230;
then (ex g1 st (g1 = ( s . ( n + k ) ) & x0 < g1));
then ( s . ( n + k ) ) in { g2: (x0 < g2 & g2 < ( x0 + g )) } by A15;
then ( s . ( n + k ) ) in ]. x0,( x0 + g ) .[ by RCOMP_1:def 2;
then ( s . ( n + k ) ) in ( ( dom f1 ) /\ ]. x0,( x0 + g ) .[ ) by A10,A16,XBOOLE_0:def 4;
then ( f1 . ( s . ( n + k ) ) ) <> ( lim_right (f1,x0) ) by A5;
then A17: (not ( f1 . ( s . ( n + k ) ) ) in { ( lim_right (f1,x0) ) }) by TARSKI:def 1;
( f1 . ( s . ( n + k ) ) ) in ( dom f2 ) by A11,A16,FUNCT_1:11;
hence x in ( ( dom f2 ) \ { ( lim_right (f1,x0) ) } ) by A17,A14,XBOOLE_0:def 5;
end;
then A18: ( rng q ) c= ( ( dom f2 ) \ { ( lim_right (f1,x0) ) } ) by TARSKI:def 3;
( rng ( f1 /* s ) ) c= ( dom f2 ) by A8,CKB3:1;
then A19: ( f2 /* q ) = ( ( f2 /* ( f1 /* s ) ) ^\ k ) by VALUED_0:27
.= ( ( ( f2 * f1 ) /* s ) ^\ k ) by A11,VALUED_0:31;
A20: ( rng s ) c= ( ( dom f1 ) /\ ( right_open_halfline x0 ) ) by A8,CKB3:1;
then A21: ( f1 /* s ) is  convergent by A1,A2,A7,LIMFUNC2:def 8;
( lim ( f1 /* s ) ) = ( lim_right (f1,x0) ) by A1,A7,A20,LIMFUNC2:def 8;
then A22: ( lim q ) = ( lim_right (f1,x0) ) by A21,SEQ_4:20;
( lim (f2,( lim_right (f1,x0) )) ) = ( lim (f2,( lim_right (f1,x0) )) );
then A23: ( f2 /* q ) is  convergent by A2,A21,A22,A18,LIMFUNC3:def 4;
hence ( ( f2 * f1 ) /* s ) is  convergent by A19,SEQ_4:21;
( lim ( f2 /* q ) ) = ( lim (f2,( lim_right (f1,x0) )) ) by A2,A21,A22,A18,LIMFUNC3:def 4;
hence ( lim ( ( f2 * f1 ) /* s ) ) = ( lim (f2,( lim_right (f1,x0) )) ) by A23,A19,SEQ_4:22;
end;
hence ( f2 * f1 ) is_right_convergent_in x0 by A3,LIMFUNC2:def 4;
hence thesis by A6,LIMFUNC2:def 8;
end;
