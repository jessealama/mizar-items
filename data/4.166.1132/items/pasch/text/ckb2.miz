environ
vocabularies ANALOAF,SUBSET_1,DIRAF,PARSP_1,VECTSP_1,PASCH;
notations STRUCT_0,ANALOAF,DIRAF,CKB1;
definitions CKB1;
theorems DIRAF,ANALOAF,CKB1;
registrations STRUCT_0;
constructors DIRAF,CKB1;
begin
reserve OAS for OAffinSpace;
reserve a for (Element of OAS);
reserve b for (Element of OAS);
reserve c for (Element of OAS);
reserve d for (Element of OAS);
reserve p for (Element of OAS);
definition
let OAS;
attr OAS is  satisfying_Ext_Par_Pasch
means
(for a holds (for b holds (for c holds (for d holds (for p holds (((( Mid p,b,c &  LIN p,a,d) & a,b '||' c,d) & (not  LIN p,a,b)) implies  Mid p,a,d))))));
end;
