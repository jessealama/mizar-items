environ
vocabularies ANALOAF,SUBSET_1,DIRAF,PARSP_1,VECTSP_1,PASCH;
notations STRUCT_0,ANALOAF,DIRAF;
theorems DIRAF,ANALOAF;
registrations STRUCT_0;
constructors DIRAF;
begin
reserve OAS for OAffinSpace;
reserve a for (Element of OAS);
reserve b for (Element of OAS);
reserve c for (Element of OAS);
reserve d for (Element of OAS);
reserve p for (Element of OAS);
definition
let OAS;
attr OAS is  satisfying_Int_Par_Pasch
means
(for a holds (for b holds (for c holds (for d holds (for p holds (((((not  LIN p,b,c) &  Mid b,p,a) &  LIN p,c,d) & b,c '||' d,a) implies  Mid c,p,d))))));
end;
