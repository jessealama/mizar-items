environ
vocabularies ANALOAF,SUBSET_1,DIRAF,PARSP_1,VECTSP_1,PASCH;
notations STRUCT_0,ANALOAF,DIRAF,CKB1,CKB2,CKB3,CKB4,CKB5;
definitions CKB1,CKB2,CKB3,CKB4,CKB5;
theorems DIRAF,ANALOAF,CKB1,CKB2,CKB3,CKB4,CKB5;
registrations STRUCT_0;
constructors DIRAF,CKB1,CKB2,CKB3,CKB4,CKB5;
begin
reserve OAS for OAffinSpace;
reserve a for (Element of OAS);
reserve b for (Element of OAS);
reserve c for (Element of OAS);
reserve d for (Element of OAS);
reserve x for (Element of OAS);
definition
let OAS;
attr OAS is  Fanoian
means
(for a holds (for b holds (for c holds (for d holds (((a,b // c,d & a,c // b,d) & (not  LIN a,b,c)) implies (ex x st ( Mid a,x,d &  Mid b,x,c)))))));
end;
