environ
vocabularies NUMBERS,XREAL_0,ORDINAL1,XXREAL_0,SUBSET_1,TARSKI,ARYTM_3,XCMPLX_0,RELAT_1,ARYTM_1,CARD_1,SQUARE_1;
notations TARSKI,SUBSET_1,ORDINAL1,NUMBERS,XCMPLX_0,XREAL_0,XXREAL_0,CKB2,CKB3;
definitions TARSKI,XCMPLX_0;
theorems AXIOMS,XREAL_0,XCMPLX_0,XCMPLX_1,XREAL_1,XXREAL_0,CKB4;
schemes CKB1;
registrations NUMBERS,XCMPLX_0,XXREAL_0,XREAL_0,REAL_1,ORDINAL1,XBOOLE_0;
constructors NUMBERS,XCMPLX_0,XXREAL_0,XREAL_0,CKB2,CKB3;
requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve x for  real number;
reserve y for  real number;
theorem
(for x,y being  real number holds ((( 0 ) <= x & ( 0 ) <= y) implies ( max (x,y) ) <= ( x + y )))
proof
let x being  real number;
let y being  real number;
assume that
A1: ( 0 ) <= x
and
A2: ( 0 ) <= y;
now
per cases  by XXREAL_0:16;
suppose A3: ( max (x,y) ) = x;

( x + ( 0 ) ) <= ( x + y ) by A2,XREAL_1:7;
hence thesis by A3;
end;
suppose A4: ( max (x,y) ) = y;

( y + ( 0 ) ) <= ( y + x ) by A1,XREAL_1:7;
hence thesis by A4;
end;
end;
hence thesis;
end;
