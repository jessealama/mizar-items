environ
vocabularies AFVECT0,SUBSET_1,XBOOLE_0,RELAT_1,ZFMISC_1,ANALOAF,PARSP_1,DIRAF,STRUCT_0,AFVECT01;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,STRUCT_0,ANALOAF,DIRAF,AFVECT0,RELSET_1;
theorems ZFMISC_1,AFVECT0,STRUCT_0,ANALOAF,DIRAF,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10;
schemes RELSET_1;
registrations XBOOLE_0,STRUCT_0,AFVECT0,CKB1;
constructors DOMAIN_1,DIRAF,AFVECT0;
requirements SUBSET,BOOLE;
begin
reserve AFV for WeakAffVect;
reserve a for (Element of AFV);
reserve b for (Element of AFV);
reserve b9 for (Element of AFV);
reserve c for (Element of AFV);
reserve p for (Element of AFV);
reserve p9 for (Element of AFV);
theorem
Lm10: (for a holds (for b holds (for b9 holds (for p holds (for p9 holds (for c holds ((((a,b '||' b,c & b,b9 '||' p,p9) & b,p '||' p,b9) & b,p9 '||' p9,b9) implies a,b9 '||' b9,c)))))))
proof
let a;
let b;
let b9;
let p;
let p9;
let c;
assume that
A1: a,b '||' b,c
and
A2: ((b,b9 '||' p,p9 & b,p '||' p,b9) & b,p9 '||' p9,b9);
A3: b,b9 // b9,b by A2,CKB3:1;
A4:now
assume A5: a,b // b,c;
then A6:  Mid a,b,c by AFVECT0:def 3;
A7:now
assume  MDist b,b9;
then  Mid a,b9,c by A6,AFVECT0:23;
then a,b9 // b9,c by AFVECT0:def 3;
hence thesis by DIRAF:def 4;
end;
(b = b9 implies thesis) by A5,DIRAF:def 4;
hence thesis by A3,A7,AFVECT0:def 2;
end;
now
assume a,b // c,b;
then a = c by AFVECT0:4,AFVECT0:7;
then a,b9 // c,b9 by AFVECT0:1;
hence thesis by DIRAF:def 4;
end;
hence thesis by A1,A4,DIRAF:def 4;
end;
