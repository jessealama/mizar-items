environ
vocabularies AFVECT0,SUBSET_1,XBOOLE_0,RELAT_1,ZFMISC_1,ANALOAF,PARSP_1,DIRAF,STRUCT_0,AFVECT01;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,STRUCT_0,ANALOAF,DIRAF,AFVECT0,RELSET_1;
theorems ZFMISC_1,AFVECT0,STRUCT_0,ANALOAF,DIRAF,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8;
schemes RELSET_1;
registrations XBOOLE_0,STRUCT_0,AFVECT0,CKB1;
constructors DOMAIN_1,DIRAF,AFVECT0;
requirements SUBSET,BOOLE;
begin
reserve AFV for WeakAffVect;
reserve a for (Element of AFV);
reserve a9 for (Element of AFV);
reserve b for (Element of AFV);
reserve b9 for (Element of AFV);
reserve p for (Element of AFV);
theorem
Lm8: (for a holds (for a9 holds (for b holds (for b9 holds (for p holds ((((a <> a9 & b <> b9) & p,a '||' p,a9) & p,b '||' p,b9) implies a,b '||' a9,b9))))))
proof
let a;
let a9;
let b;
let b9;
let p;
assume that
A1: a <> a9
and
A2: b <> b9
and
A3: p,a '||' p,a9
and
A4: p,b '||' p,b9;
b,p // p,b9 by A2,A4,CKB2:1,CKB4:1;
then A5:  Mid b,p,b9 by AFVECT0:def 3;
a,p // p,a9 by A1,A3,CKB2:1,CKB4:1;
then  Mid a,p,a9 by AFVECT0:def 3;
then a,b // b9,a9 by A5,AFVECT0:25;
hence thesis by DIRAF:def 4;
end;
