environ
vocabularies AFVECT0,SUBSET_1,XBOOLE_0,RELAT_1,ZFMISC_1,ANALOAF,PARSP_1,DIRAF,STRUCT_0,AFVECT01;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,STRUCT_0,ANALOAF,DIRAF,AFVECT0,RELSET_1;
theorems ZFMISC_1,AFVECT0,STRUCT_0,ANALOAF,DIRAF,CKB2;
schemes RELSET_1;
registrations XBOOLE_0,STRUCT_0,AFVECT0,CKB1;
constructors DOMAIN_1,DIRAF,AFVECT0;
requirements SUBSET,BOOLE;
begin
reserve AFV for WeakAffVect;
reserve a for (Element of AFV);
reserve b for (Element of AFV);
reserve p for (Element of AFV);
reserve q for (Element of AFV);
theorem
Lm2: (a,b // b,a iff (ex p st (ex q st ((a,b '||' p,q & a,p '||' p,b) & a,q '||' q,b))))
proof
A1:now
given p,q such that
A2: a,b '||' p,q
and
A3: a,p '||' p,b
and
A4: a,q '||' q,b;

now
A5:now
assume A6:  MDist p,q;
(a,b // p,q or a,b // q,p) by A2,DIRAF:def 4;
then  MDist a,b by A6,AFVECT0:3,AFVECT0:15;
hence a,b // b,a by AFVECT0:def 2;
end;
assume A7: a <> b;
then a,q // q,b by A4,CKB2:1;
then A8:  Mid a,q,b by AFVECT0:def 3;
A9:now
assume p = q;
then a,b // p,p by A2,DIRAF:def 4;
hence contradiction by A7,AFVECT0:def 1;
end;
a,p // p,b by A3,A7,CKB2:1;
then  Mid a,p,b by AFVECT0:def 3;
hence a,b // b,a by A8,A9,A5,AFVECT0:20;
end;
hence a,b // b,a by AFVECT0:2;
end;
now
assume A10: a,b // b,a;
A11:now
assume a <> b;
then A12:  MDist a,b by A10,AFVECT0:def 2;
consider p such that A13:  Mid a,p,b by AFVECT0:19;
A14: a,p // p,b by A13,AFVECT0:def 3;
consider q such that A15: a,b // p,q by AFVECT0:def 1;
take p;
take q;
 Mid a,q,b by A13,A15,A12,AFVECT0:15,AFVECT0:23;
then a,q // q,b by AFVECT0:def 3;
hence ((a,b '||' p,q & a,p '||' p,b) & a,q '||' q,b) by A15,A14,DIRAF:def 4;
end;
now
assume A16: a = b;
take p = a;
take q = a;
a,b // p,q by A16,AFVECT0:2;
hence ((a,b '||' p,q & a,p '||' p,b) & a,q '||' q,b) by A16,DIRAF:def 4;
end;
hence (ex p st (ex q st ((a,b '||' p,q & a,p '||' p,b) & a,q '||' q,b))) by A11;
end;
hence thesis by A1;
end;
