environ
vocabularies AFVECT0,SUBSET_1,XBOOLE_0,RELAT_1,ZFMISC_1,ANALOAF,PARSP_1,DIRAF,STRUCT_0,AFVECT01;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,STRUCT_0,ANALOAF,DIRAF,AFVECT0,RELSET_1;
theorems ZFMISC_1,AFVECT0,STRUCT_0,ANALOAF,DIRAF,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9;
schemes RELSET_1;
registrations XBOOLE_0,STRUCT_0,AFVECT0,CKB1;
constructors DOMAIN_1,DIRAF,AFVECT0;
requirements SUBSET,BOOLE;
begin
reserve AFV for WeakAffVect;
reserve a for (Element of AFV);
reserve b for (Element of AFV);
reserve c for (Element of AFV);
reserve p for (Element of AFV);
reserve p9 for (Element of AFV);
theorem
Lm9: (a = b or (ex c st ((a <> c & a,b '||' b,c) or (ex p st (ex p9 st (((p <> p9 & a,b '||' p,p9) & a,p '||' p,b) & a,p9 '||' p9,b))))))
proof
consider c such that A1: a,b // b,c by AFVECT0:def 1;
A2:now
assume a = c;
then consider p,p9 such that A3: a,b '||' p,p9 and A4: (a,p '||' p,b & a,p9 '||' p9,b) by A1,CKB3:1;
(p = p9 implies a = b) by A3,CKB6:1;
hence (a = b or (ex p st (ex p9 st (((p <> p9 & a,b '||' p,p9) & a,p '||' p,b) & a,p9 '||' p9,b)))) by A3,A4;
end;
now
assume A5: a <> c;
a,b '||' b,c by A1,DIRAF:def 4;
hence (ex c st (a <> c & a,b '||' b,c)) by A5;
end;
hence thesis by A2;
end;
