environ
vocabularies AFVECT0,SUBSET_1,XBOOLE_0,RELAT_1,ZFMISC_1,ANALOAF,PARSP_1,DIRAF,STRUCT_0,AFVECT01;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,STRUCT_0,ANALOAF,DIRAF,AFVECT0,RELSET_1,CKB14;
definitions CKB14;
theorems ZFMISC_1,AFVECT0,STRUCT_0,ANALOAF,DIRAF,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14;
schemes RELSET_1;
registrations XBOOLE_0,STRUCT_0,AFVECT0,CKB1;
constructors DOMAIN_1,DIRAF,AFVECT0,CKB14;
requirements SUBSET,BOOLE;
begin
reserve AFV for WeakAffVect;
reserve a for (Element of AFV);
reserve a9 for (Element of AFV);
reserve b for (Element of AFV);
reserve b9 for (Element of AFV);
reserve c for (Element of AFV);
reserve d for (Element of AFV);
reserve p for (Element of AFV);
reserve p9 for (Element of AFV);
reserve q for (Element of AFV);
reserve q9 for (Element of AFV);
reserve r for (Element of AFV);
reserve r9 for (Element of AFV);
set AFV0 = the WeakAffVect;
set X = (the carrier of AFV0);
set XX = [: X,X :];
defpred P[ set,set ]
 means
(ex a,b,c,d being (Element of X) st (($1 = [ a,b ] & $2 = [ c,d ]) & a,b '||' c,d));
consider P being (Relation of XX,XX) such that Lm13: (for x,y being set holds ([ x,y ] in P iff ((x in XX & y in XX) & P[ x,y ]))) from RELSET_1:sch 1;
Lm14: (for a,b,c,d being (Element of X) holds ([ [ a,b ],[ c,d ] ] in P iff a,b '||' c,d))
proof
let a being (Element of X);
let b being (Element of X);
let c being (Element of X);
let d being (Element of X);
A1: ([ [ a,b ],[ c,d ] ] in P implies a,b '||' c,d)
proof
assume [ [ a,b ],[ c,d ] ] in P;
then consider a9,b9,c9,d9 being (Element of X) such that A2: [ a,b ] = [ a9,b9 ] and A3: [ c,d ] = [ c9,d9 ] and A4: a9,b9 '||' c9,d9 by Lm13;
A5: c = c9 by A3,ZFMISC_1:27;
(a = a9 & b = b9) by A2,ZFMISC_1:27;
hence thesis by A3,A4,A5,ZFMISC_1:27;
end;
([ a,b ] in XX & [ c,d ] in XX) by ZFMISC_1:def 2;
hence thesis by A1,Lm13;
end;
set WAS = AffinStruct (# (the carrier of AFV0),P #);
Lm15: (for a,b,c,d being (Element of AFV0) holds (for a9,b9,c9,d9 being (Element of WAS) holds ((((a = a9 & b = b9) & c = c9) & d = d9) implies (a,b '||' c,d iff a9,b9 // c9,d9))))
proof
let a being (Element of AFV0);
let b being (Element of AFV0);
let c being (Element of AFV0);
let d being (Element of AFV0);
let a9 being (Element of WAS);
let b9 being (Element of WAS);
let c9 being (Element of WAS);
let d9 being (Element of WAS);
assume that
A1: (((a = a9 & b = b9) & c = c9) & d = d9);
A2:now
assume a9,b9 // c9,d9;
then [ [ a9,b9 ],[ c9,d9 ] ] in P by ANALOAF:def 2;
hence a,b '||' c,d by A1,Lm14;
end;
now
assume a,b '||' c,d;
then [ [ a,b ],[ c,d ] ] in (the CONGR of WAS) by Lm14;
hence a9,b9 // c9,d9 by A1,ANALOAF:def 2;
end;
hence thesis by A2;
end;
Lm16:now
thus (ex a9,b9 being (Element of WAS) st a9 <> b9) by STRUCT_0:def 10;
thus (for a9,b9 being (Element of WAS) holds a9,b9 // b9,a9)
proof
let a9 being (Element of WAS);
let b9 being (Element of WAS);
reconsider a = a9,b = b9 as (Element of AFV0);
a,b '||' b,a by CKB5:1;
hence thesis by Lm15;
end;

thus (for a9,b9 being (Element of WAS) holds (a9,b9 // a9,a9 implies a9 = b9))
proof
let a9 being (Element of WAS);
let b9 being (Element of WAS);
assume that
A1: a9,b9 // a9,a9;
reconsider a = a9,b = b9 as (Element of AFV0);
a,b '||' a,a by A1,Lm15;
hence thesis by CKB6:1;
end;

thus (for a,b,c,d,p,q being (Element of WAS) holds ((a,b // p,q & c,d // p,q) implies a,b // c,d))
proof
let a being (Element of WAS);
let b being (Element of WAS);
let c being (Element of WAS);
let d being (Element of WAS);
let p being (Element of WAS);
let q being (Element of WAS);
assume that
A2: (a,b // p,q & c,d // p,q);
reconsider a1 = a,b1 = b,c1 = c,d1 = d,p1 = p,q1 = q as (Element of AFV0);
(a1,b1 '||' p1,q1 & c1,d1 '||' p1,q1) by A2,Lm15;
then a1,b1 '||' c1,d1 by CKB7:1;
hence thesis by Lm15;
end;

thus (for a,c being (Element of WAS) holds (ex b being (Element of WAS) st a,b // b,c))
proof
let a being (Element of WAS);
let c being (Element of WAS);
reconsider a1 = a,c1 = c as (Element of AFV0);
consider b1 being (Element of AFV0) such that A3: a1,b1 '||' b1,c1 by CKB8:1;
reconsider b = b1 as (Element of WAS);
a,b // b,c by A3,Lm15;
hence thesis;
end;

thus (for a,a9,b,b9,p being (Element of WAS) holds ((((a <> a9 & b <> b9) & p,a // p,a9) & p,b // p,b9) implies a,b // a9,b9))
proof
let a being (Element of WAS);
let a9 being (Element of WAS);
let b being (Element of WAS);
let b9 being (Element of WAS);
let p being (Element of WAS);
assume that
A4: (a <> a9 & b <> b9)
and
A5: (p,a // p,a9 & p,b // p,b9);
reconsider a1 = a,a19 = a9,b1 = b,b19 = b9,p1 = p as (Element of AFV0);
(p1,a1 '||' p1,a19 & p1,b1 '||' p1,b19) by A5,Lm15;
then a1,b1 '||' a19,b19 by A4,CKB9:1;
hence thesis by Lm15;
end;

thus (for a,b being (Element of WAS) holds (a = b or (ex c being (Element of WAS) st ((a <> c & a,b // b,c) or (ex p,p9 being (Element of WAS) st (((p <> p9 & a,b // p,p9) & a,p // p,b) & a,p9 // p9,b))))))
proof
let a being (Element of WAS);
let b being (Element of WAS);
assume that
A6: (not a = b);
reconsider a1 = a,b1 = b as (Element of AFV0);
A7:now
given p1 being (Element of AFV0),p19 being (Element of AFV0) such that
A8: p1 <> p19
and
A9: (a1,b1 '||' p1,p19 & a1,p1 '||' p1,b1)
and
A10: a1,p19 '||' p19,b1;

reconsider p = p1,p9 = p19 as (Element of WAS);
A11: a,p9 // p9,b by A10,Lm15;
(a,b // p,p9 & a,p // p,b) by A9,Lm15;
hence (ex p,p9 being (Element of WAS) st (((p <> p9 & a,b // p,p9) & a,p // p,b) & a,p9 // p9,b)) by A8,A11;
end;
now
given c1 being (Element of AFV0) such that
A12: a1 <> c1
and
A13: a1,b1 '||' b1,c1;

reconsider c = c1 as (Element of WAS);
a,b // b,c by A13,Lm15;
hence (ex c being (Element of WAS) st (a <> c & a,b // b,c)) by A12;
end;
hence thesis by A6,A7,CKB10:1;
end;

thus (for a,b,b9,p,p9,c being (Element of WAS) holds ((((a,b // b,c & b,b9 // p,p9) & b,p // p,b9) & b,p9 // p9,b9) implies a,b9 // b9,c))
proof
let a being (Element of WAS);
let b being (Element of WAS);
let b9 being (Element of WAS);
let p being (Element of WAS);
let p9 being (Element of WAS);
let c being (Element of WAS);
assume that
A14: (a,b // b,c & b,b9 // p,p9)
and
A15: (b,p // p,b9 & b,p9 // p9,b9);
reconsider a1 = a,b1 = b,b19 = b9,p1 = p,p19 = p9,c1 = c as (Element of AFV0);
A16: (b1,p1 '||' p1,b19 & b1,p19 '||' p19,b19) by A15,Lm15;
(a1,b1 '||' b1,c1 & b1,b19 '||' p1,p19) by A14,Lm15;
then a1,b19 '||' b19,c1 by A16,CKB11:1;
hence thesis by Lm15;
end;

thus (for a,b,b9,c being (Element of WAS) holds ((((a <> c & b <> b9) & a,b // b,c) & a,b9 // b9,c) implies (ex p,p9 being (Element of WAS) st (((p <> p9 & b,b9 // p,p9) & b,p // p,b9) & b,p9 // p9,b9))))
proof
let a being (Element of WAS);
let b being (Element of WAS);
let b9 being (Element of WAS);
let c being (Element of WAS);
assume that
A17: (a <> c & b <> b9)
and
A18: (a,b // b,c & a,b9 // b9,c);
reconsider a1 = a,b1 = b,b19 = b9,c1 = c as (Element of AFV0);
(a1,b1 '||' b1,c1 & a1,b19 '||' b19,c1) by A18,Lm15;
then consider p1,p19 being (Element of AFV0) such that A19: p1 <> p19 and A20: (b1,b19 '||' p1,p19 & b1,p1 '||' p1,b19) and A21: b1,p19 '||' p19,b19 by A17,CKB12:1;
reconsider p = p1,p9 = p19 as (Element of WAS);
A22: b,p9 // p9,b9 by A21,Lm15;
(b,b9 // p,p9 & b,p // p,b9) by A20,Lm15;
hence thesis by A19,A22;
end;

thus (for a,b,c,p,p9,q,q9 being (Element of WAS) holds ((((((a,b // p,p9 & a,c // q,q9) & a,p // p,b) & a,q // q,c) & a,p9 // p9,b) & a,q9 // q9,c) implies (ex r,r9 being (Element of WAS) st ((b,c // r,r9 & b,r // r,c) & b,r9 // r9,c))))
proof
let a being (Element of WAS);
let b being (Element of WAS);
let c being (Element of WAS);
let p being (Element of WAS);
let p9 being (Element of WAS);
let q being (Element of WAS);
let q9 being (Element of WAS);
assume that
A23: (a,b // p,p9 & a,c // q,q9)
and
A24: (a,p // p,b & a,q // q,c)
and
A25: (a,p9 // p9,b & a,q9 // q9,c);
reconsider a1 = a,b1 = b,c1 = c,p1 = p,p19 = p9,q1 = q,q19 = q9 as (Element of AFV0);
A26: (a1,p1 '||' p1,b1 & a1,q1 '||' q1,c1) by A24,Lm15;
A27: (a1,p19 '||' p19,b1 & a1,q19 '||' q19,c1) by A25,Lm15;
(a1,b1 '||' p1,p19 & a1,c1 '||' q1,q19) by A23,Lm15;
then consider r1,r19 being (Element of AFV0) such that A28: (b1,c1 '||' r1,r19 & b1,r1 '||' r1,c1) and A29: b1,r19 '||' r19,c1 by A26,A27,CKB13:1;
reconsider r = r1,r9 = r19 as (Element of WAS);
A30: b,r9 // r9,c by A29,Lm15;
(b,c // r,r9 & b,r // r,c) by A28,Lm15;
hence thesis by A30;
end;

end;
registration
cluster  strict  WeakAffSegm-like for non  trivial non  trivial AffinStruct;
existence
proof
WAS is  WeakAffSegm-like non  trivial by CKB14:def 1,Lm16;
hence thesis;
end;
end;
