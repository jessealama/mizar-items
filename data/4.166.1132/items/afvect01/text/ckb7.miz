environ
vocabularies AFVECT0,SUBSET_1,XBOOLE_0,RELAT_1,ZFMISC_1,ANALOAF,PARSP_1,DIRAF,STRUCT_0,AFVECT01;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,STRUCT_0,ANALOAF,DIRAF,AFVECT0,RELSET_1;
theorems ZFMISC_1,AFVECT0,STRUCT_0,ANALOAF,DIRAF,CKB2,CKB3,CKB4,CKB5,CKB6;
schemes RELSET_1;
registrations XBOOLE_0,STRUCT_0,AFVECT0,CKB1;
constructors DOMAIN_1,DIRAF,AFVECT0;
requirements SUBSET,BOOLE;
begin
reserve AFV for WeakAffVect;
reserve a for (Element of AFV);
reserve b for (Element of AFV);
reserve c for (Element of AFV);
reserve d for (Element of AFV);
reserve p for (Element of AFV);
reserve q for (Element of AFV);
theorem
Lm6: (for a holds (for b holds (for c holds (for d holds (for p holds (for q holds ((a,b '||' p,q & c,d '||' p,q) implies a,b '||' c,d)))))))
proof
let a;
let b;
let c;
let d;
let p;
let q;
assume (a,b '||' p,q & c,d '||' p,q);
then ((((a,b // p,q & c,d // p,q) or (a,b // p,q & c,d // q,p)) or (a,b // q,p & c,d // p,q)) or (a,b // q,p & c,d // q,p)) by DIRAF:def 4;
then ((a,b // c,d or (a,b // p,q & d,c // p,q)) or (a,b // q,p & d,c // q,p)) by AFVECT0:7,AFVECT0:def 1;
then (a,b // c,d or a,b // d,c) by AFVECT0:def 1;
hence thesis by DIRAF:def 4;
end;
