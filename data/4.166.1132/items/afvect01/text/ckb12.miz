environ
vocabularies AFVECT0,SUBSET_1,XBOOLE_0,RELAT_1,ZFMISC_1,ANALOAF,PARSP_1,DIRAF,STRUCT_0,AFVECT01;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,STRUCT_0,ANALOAF,DIRAF,AFVECT0,RELSET_1;
theorems ZFMISC_1,AFVECT0,STRUCT_0,ANALOAF,DIRAF,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11;
schemes RELSET_1;
registrations XBOOLE_0,STRUCT_0,AFVECT0,CKB1;
constructors DOMAIN_1,DIRAF,AFVECT0;
requirements SUBSET,BOOLE;
begin
reserve AFV for WeakAffVect;
reserve a for (Element of AFV);
reserve b for (Element of AFV);
reserve b9 for (Element of AFV);
reserve c for (Element of AFV);
reserve p for (Element of AFV);
reserve p9 for (Element of AFV);
theorem
Lm11: (for a holds (for b holds (for b9 holds (for c holds ((((a <> c & b <> b9) & a,b '||' b,c) & a,b9 '||' b9,c) implies (ex p st (ex p9 st (((p <> p9 & b,b9 '||' p,p9) & b,p '||' p,b9) & b,p9 '||' p9,b9))))))))
proof
let a;
let b;
let b9;
let c;
assume that
A1: a <> c
and
A2: b <> b9
and
A3: a,b '||' b,c
and
A4: a,b9 '||' b9,c;
a,b9 // b9,c by A1,A4,CKB2:1;
then A5:  Mid a,b9,c by AFVECT0:def 3;
a,b // b,c by A1,A3,CKB2:1;
then  Mid a,b,c by AFVECT0:def 3;
then  MDist b,b9 by A2,A5,AFVECT0:20;
then b,b9 // b9,b by AFVECT0:def 2;
then consider p,p9 such that A6: b,b9 '||' p,p9 and A7: (b,p '||' p,b9 & b,p9 '||' p9,b9) by CKB3:1;
(p <> p9 implies thesis) by A6,A7;
hence thesis by A2,A6,CKB6:1;
end;
