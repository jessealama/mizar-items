environ
vocabularies NUMBERS,SUBSET_1,COMPLEX1,ARYTM_3,ARYTM_1,TARSKI,XBOOLE_0,CARD_1,RCOMP_1,SETFAM_1,METRIC_1,PRE_TOPC,STRUCT_0,CARD_5,COMPLSP1;
notations TARSKI,XBOOLE_0,SUBSET_1,ORDINAL1,NUMBERS,XCMPLX_0,XXREAL_0,XREAL_0,NAT_1,SETFAM_1,REAL_1,COMPLEX1,FUNCT_1,RELSET_1,FUNCT_2,BINOP_2,STRUCT_0,PRE_TOPC,FINSEQ_1,VALUED_1,RVSUM_1,FINSEQ_2,SEQ_4,CKB1;
definitions TARSKI,PRE_TOPC,COMPTS_1,FINSEQOP,SEQ_4,STRUCT_0,XBOOLE_0,COMPLEX1,FINSEQ_1,FINSEQ_2,SQUARE_1,SUBSET_1,VALUED_1,CKB1;
theorems SUBSET_1,PRE_TOPC,ZFMISC_1,XBOOLE_0,XREAL_1,SEQ_4,CKB1,CKB3,CKB4,CKB5,CKB6,CKB7;
registrations XBOOLE_0,SUBSET_1,FUNCT_1,RELSET_1,FUNCT_2,NUMBERS,XXREAL_0,XREAL_0,BINOP_2,MEMBERED,FINSEQ_2,PRE_TOPC,VALUED_0,VALUED_1,SEQ_4,CKB2;
constructors SETFAM_1,PARTFUN1,BINOP_1,SETWISEO,REAL_1,SQUARE_1,BINOP_2,COMPLEX1,SEQ_4,FINSEQOP,RVSUM_1,COMPTS_1,SEQ_1,XXREAL_2,RELSET_1,CKB1;
requirements NUMERALS,REAL,SUBSET,BOOLE,ARITHM;
begin
reserve n for (Element of ( NAT ));
reserve x for (Element of ( COMPLEX n ));
reserve p for (Point of ( the_Complex_Space n ));
reserve q for (Point of ( the_Complex_Space n ));
theorem
( the_Complex_Space n ) is  T_2
proof
let p;
let q;
assume A1: p <> q;
reconsider z1 = p,z2 = q as (Element of ( COMPLEX n ));
set d = ( |. ( z1 - z2 ) .| / 2 );
reconsider K1 = ( Ball (z1,d) ),K2 = ( Ball (z2,d) ) as (Subset of ( the_Complex_Space n ));
take K1;
take K2;
(( Ball (z1,d) ) is  open & ( Ball (z2,d) ) is  open) by SEQ_4:112;
hence (K1 is  open & K2 is  open) by CKB6:1;
( 0 ) < |. ( z1 - z2 ) .| by A1,SEQ_4:103;
hence (p in K1 & q in K2) by SEQ_4:111,XREAL_1:215;
assume ( K1 /\ K2 ) <> ( {} );
then consider x such that A2: x in ( ( Ball (z1,d) ) /\ ( Ball (z2,d) ) ) by SUBSET_1:4;
x in K2 by A2,XBOOLE_0:def 4;
then A3: |. ( z2 - x ) .| < d by SEQ_4:110;
x in K1 by A2,XBOOLE_0:def 4;
then |. ( z1 - x ) .| < d by SEQ_4:110;
then ( |. ( z1 - x ) .| + |. ( z2 - x ) .| ) < ( d + d ) by A3,XREAL_1:8;
then ( |. ( z1 - x ) .| + |. ( x - z2 ) .| ) < |. ( z1 - z2 ) .| by SEQ_4:104;
hence contradiction by SEQ_4:105;
end;
