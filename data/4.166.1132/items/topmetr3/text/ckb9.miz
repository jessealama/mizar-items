requirements NUMERALS,REAL,BOOLE,SUBSET,ARITHM;
begin
theorem
(for a,b being  real number holds (for s being Real_Sequence holds (for S being (sequence of ( Closed-Interval-MSpace (a,b) )) holds (((S = s & a <= b) & s is  non-decreasing) implies S is  convergent))))
proof
let a being  real number;
let b being  real number;
let s being Real_Sequence;
let S being (sequence of ( Closed-Interval-MSpace (a,b) ));
assume that
A1: S = s
and
A2: a <= b
and
A3: s is  non-decreasing;
(for n being (Element of ( NAT )) holds ( s . n ) < ( b + 1 ))
proof
let n being (Element of ( NAT ));
A4: b < ( b + 1 ) by XREAL_1:29;
( dom S ) = ( NAT ) by FUNCT_2:def 1;
then A5: ( s . n ) in ( rng S ) by A1,FUNCT_1:def 3;
(the carrier of ( Closed-Interval-MSpace (a,b) )) = [. a,b .] by A2,TOPMETR:10;
then ( s . n ) <= b by A5,XXREAL_1:1;
hence thesis by A4,XXREAL_0:2;
end;
then s is  bounded_above by SEQ_2:def 3;
hence thesis by A1,A2,A3,CKB8:1;
end;
