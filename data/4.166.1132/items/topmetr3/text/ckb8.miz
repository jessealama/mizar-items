begin
theorem
Th10: (for a,b being  real number holds (for s being Real_Sequence holds (for S being (sequence of ( Closed-Interval-MSpace (a,b) )) holds (((S = s & a <= b) & s is  convergent) implies (S is  convergent & ( lim s ) = ( lim S ))))))
proof
let a being  real number,b being  real number;
let s being Real_Sequence;
let S being (sequence of ( Closed-Interval-MSpace (a,b) ));
assume that
A1: S = s
and
A2: a <= b
and
A3: s is  convergent;
reconsider S0 = S as (sequence of ( RealSpace )) by A2,CKB6:1;
A4: S0 is  convergent by A1,A3,CKB4:1;
hence S is  convergent by A2,CKB7:1;
( lim S0 ) = ( lim S ) by A2,A4,CKB7:1;
hence thesis by A1,A3,CKB4:1;
end;
