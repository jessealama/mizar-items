constructors ENUMSET1,WELLORD2,XXREAL_0,XREAL_0,NAT_1,FINSEQ_1,FINSEQ_2,FUNCOP_1,FUNCT_2,RELSET_1,CKB15,CKB24,CKB29;
requirements NUMERALS,REAL,BOOLE,SUBSET,ARITHM;
begin
reserve k for (Element of ( NAT ));
reserve n for (Element of ( NAT ));
reserve p for (FinSequence of ( NAT ));
reserve T for Tree;
reserve T1 for Tree;
registration
let T;
let T1;
cluster ( T \/ T1 ) ->  Tree-like;
coherence
proof
set D = ( T \/ T1 );
(T c= ( ( NAT ) * ) & T1 c= ( ( NAT ) * )) by CKB24:def 1;
hence D c= ( ( NAT ) * ) by XBOOLE_1:8;
thus (for p holds (p in D implies ( ProperPrefixes p ) c= D))
proof
let p;
assume p in D;
then (p in T or p in T1) by XBOOLE_0:def 3;
then A1: (( ProperPrefixes p ) c= T or ( ProperPrefixes p ) c= T1) by CKB24:def 1;
(T c= D & T1 c= D) by XBOOLE_1:7;
hence thesis by A1,XBOOLE_1:1;
end;

let p;
let k;
let n;
assume that
A2: ( p ^ <* k *> ) in D
and
A3: n <= k;
(( p ^ <* k *> ) in T or ( p ^ <* k *> ) in T1) by A2,XBOOLE_0:def 3;
then (( p ^ <* n *> ) in T or ( p ^ <* n *> ) in T1) by A3,CKB24:def 1;
hence thesis by XBOOLE_0:def 3;
end;
end;
