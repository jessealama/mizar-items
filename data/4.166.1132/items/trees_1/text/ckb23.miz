begin
reserve k for (Element of ( NAT ));
reserve n for (Element of ( NAT ));
reserve p for (FinSequence of ( NAT ));
reserve q for (FinSequence of ( NAT ));
reserve r for (FinSequence of ( NAT ));
theorem
Th42: (for p,q,r being FinSequence holds ((q in ( ProperPrefixes p ) & r in ( ProperPrefixes p )) implies q,r are_c=-comparable ))
proof
let p being FinSequence;
let q being FinSequence;
let r being FinSequence;
assume q in ( ProperPrefixes p );
then A1: (ex q1 being FinSequence st (q = q1 & q1 is_a_proper_prefix_of p)) by CKB15:def 1;
assume r in ( ProperPrefixes p );
then A2: (ex r1 being FinSequence st (r = r1 & r1 is_a_proper_prefix_of p)) by CKB15:def 1;
q is_a_prefix_of p by A1,XBOOLE_0:def 8;
then consider n such that A3: q = ( p | ( Seg n ) ) by CKB2:def 1;
r is_a_prefix_of p by A2,XBOOLE_0:def 8;
then consider k such that A4: r = ( p | ( Seg k ) ) by CKB2:def 1;
A5: (n <= k implies thesis)
proof
assume n <= k;
then ( Seg n ) c= ( Seg k ) by FINSEQ_1:5;
then q = ( r | ( Seg n ) ) by A3,A4,FUNCT_1:51;
then q is_a_prefix_of r by CKB2:def 1;
hence thesis by XBOOLE_0:def 9;
end;
(k <= n implies thesis)
proof
assume k <= n;
then ( Seg k ) c= ( Seg n ) by FINSEQ_1:5;
then r = ( q | ( Seg k ) ) by A3,A4,FUNCT_1:51;
then r is_a_prefix_of q by CKB2:def 1;
hence thesis by XBOOLE_0:def 9;
end;
hence thesis by A5;
end;
