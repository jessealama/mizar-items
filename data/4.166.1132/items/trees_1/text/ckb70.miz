reserve T for Tree;
definition
let T being  finite Tree;
func width T -> (Element of ( NAT )) means 
:Def16: (ex X being (AntiChain_of_Prefixes of T) st (it = ( card X ) & (for Y being (AntiChain_of_Prefixes of T) holds ( card Y ) <= ( card X ))));
existence
proof
defpred X[ Nat ]
 means
(ex X being  finite set st (($1 = ( card X ) & X c= T) & (for p holds (for q holds (((p in X & q in X) & p <> q) implies (not p,q are_c=-comparable ))))));
(( 0 ) = ( card ( {} ) ) & (for p holds (for q holds (((p in ( {} ) & q in ( {} )) & p <> q) implies (not p,q are_c=-comparable )))));
then A21: (ex n being Nat st X[ n ]) by XBOOLE_1:2;
A22: (for n being Nat holds (X[ n ] implies n <= ( card T )))
proof
let n being Nat;
given X being  finite set such that
A23: (n = ( card X ) & X c= T)
and
(for p holds (for q holds (((p in X & q in X) & p <> q) implies (not p,q are_c=-comparable ))));

A24: (( card X ) c= ( card T ) & ( card X ) = ( card n )) by A23,CARD_1:11;
( card T ) = ( card ( card T ) );
hence thesis by A24,NAT_1:40;
end;
consider n being Nat such that A25: X[ n ] and A26: (for m being Nat holds (X[ m ] implies m <= n)) from NAT_1:sch 6(A22,A21);
consider X being  finite set such that A27: n = ( card X ) and A28: X c= T and A29: (for p holds (for q holds (((p in X & q in X) & p <> q) implies (not p,q are_c=-comparable )))) by A25;
X is  AntiChain_of_Prefixes-like
proof
thus (for x holds (x in X implies x is FinSequence))
proof
let x;
assume A30: x in X;
T c= ( ( NAT ) * ) by CKB24:def 1;
hence thesis by A30,A28;
end;

let p1;
let p2;
assume A31: (p1 in X & p2 in X);
then reconsider q1 = p1,q2 = p2 as (Element of T) by A28;
(p1 = q1 & p2 = q2);
hence thesis by A29,A31;
end;
then reconsider X as AntiChain_of_Prefixes;
reconsider X as (AntiChain_of_Prefixes of T) by A28,CKB64:def 1;
reconsider n as (Element of ( NAT )) by ORDINAL1:def 12;
take n;
take X;
thus n = ( card X ) by A27;
let Y being (AntiChain_of_Prefixes of T);
(Y c= T & (for p holds (for q holds (((p in Y & q in Y) & p <> q) implies (not p,q are_c=-comparable ))))) by CKB59:def 1,CKB64:def 1;
hence thesis by A26,A27;
end;
uniqueness
proof
let n1 being (Element of ( NAT ));
let n2 being (Element of ( NAT ));
given X1 being (AntiChain_of_Prefixes of T) such that
A32: n1 = ( card X1 )
and
A33: (for Y being (AntiChain_of_Prefixes of T) holds ( card Y ) <= ( card X1 ));

given X2 being (AntiChain_of_Prefixes of T) such that
A34: n2 = ( card X2 )
and
A35: (for Y being (AntiChain_of_Prefixes of T) holds ( card Y ) <= ( card X2 ));

A36: ( card X1 ) <= ( card X2 ) by A35;
( card X2 ) <= ( card X1 ) by A33;
hence thesis by A32,A34,A36,XXREAL_0:1;
end;
