scheme ILambdaD { F( integer number) ->  integer number } : (ex f being (Function of ( INT ),( INT )) st (for x being  integer number holds ( f . x ) = F(x)))
proof
defpred P[ (Element of ( INT )),set ]
 means
$2 = F($1);
A1: (for x being (Element of ( INT )) holds (ex y being (Element of ( INT )) st P[ x,y ]))
proof
let x being (Element of ( INT ));
reconsider y = F(x) as (Element of ( INT )) by INT_1:def 2;
take y;
thus P[ x,y ];
end;
consider f being (Function of ( INT ),( INT )) such that A2: (for x being (Element of ( INT )) holds P[ x,( f . x ) ]) from FUNCT_2:sch 3(A1);
take f;
let c being  integer number;
reconsider c as (Element of ( INT )) by INT_1:def 2;
P[ c,( f . c ) ] by A2;
hence thesis;
end;
