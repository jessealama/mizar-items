begin
reserve V for RealLinearSpace;
theorem
Th16: (for W1,W2 being (Subspace of V) holds (V is_the_direct_sum_of W1,W2 implies (for v,v1,v2 being (VECTOR of V) holds (( v |-- (W1,W2) ) = [ v1,v2 ] implies v = ( v1 + v2 )))))
proof
let W1 being (Subspace of V),W2 being (Subspace of V);
assume that
A1: V is_the_direct_sum_of W1,W2;
let v being (VECTOR of V),v1 being (VECTOR of V),v2 being (VECTOR of V);
assume ( v |-- (W1,W2) ) = [ v1,v2 ];
then (( ( v |-- (W1,W2) ) `1 ) = v1 & ( ( v |-- (W1,W2) ) `2 ) = v2) by MCART_1:7;
hence thesis by A1,RLSUB_2:def 6;
end;
