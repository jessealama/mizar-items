begin
reserve V for RealLinearSpace;
theorem
Th20: (for W1,W2 being (Subspace of V) holds (V is_the_direct_sum_of W1,W2 implies (for v being (VECTOR of V) holds (v in W2 implies ( v |-- (W1,W2) ) = [ ( 0. V ),v ]))))
proof
let W1 being (Subspace of V);
let W2 being (Subspace of V);
assume A1: V is_the_direct_sum_of W1,W2;
let v being (VECTOR of V);
assume v in W2;
then ( v |-- (W2,W1) ) = [ v,( 0. V ) ] by A1,CKB6:1,RLSUB_2:38;
hence thesis by A1,CKB5:1,RLSUB_2:38;
end;
