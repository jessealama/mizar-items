begin
reserve V for RealLinearSpace;
theorem
(for V being RealNormSpace holds (for X being (Subspace of V) holds (for fi being (linear-Functional of X) holds ((for x being (VECTOR of X) holds (for v being (VECTOR of V) holds (x = v implies ( fi . x ) <= ||. v .||))) implies (ex psi being (linear-Functional of V) st (( psi | (the carrier of X) ) = fi & (for x being (VECTOR of V) holds ( psi . x ) <= ||. x .||)))))))
proof
let V being RealNormSpace;
let X being (Subspace of V);
let fi being (linear-Functional of X);
assume that
A1: (for x being (VECTOR of X) holds (for v being (VECTOR of V) holds (x = v implies ( fi . x ) <= ||. v .||)));
reconsider q = (the normF of V) as (Banach-Functional of V) by CKB45:1;
now
let x being (VECTOR of X);
let v being (VECTOR of V);
assume that
A2: x = v;
( q . v ) = ||. v .|| by NORMSP_0:def 1;
hence ( fi . x ) <= ( q . v ) by A1,A2;
end;
then consider psi being (linear-Functional of V) such that A3: ( psi | (the carrier of X) ) = fi and A4: (for x being (VECTOR of V) holds ( psi . x ) <= ( q . x )) by CKB44:1;
take psi;
thus ( psi | (the carrier of X) ) = fi by A3;
let x being (VECTOR of V);
( q . x ) = ||. x .|| by NORMSP_0:def 1;
hence thesis by A4;
end;
