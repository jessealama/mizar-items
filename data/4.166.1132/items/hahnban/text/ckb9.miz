registrations XBOOLE_0,SUBSET_1,RELSET_1,PARTFUN1,NUMBERS,XXREAL_0,XREAL_0,MEMBERED,STRUCT_0,RLVECT_1,RLSUB_1,ALGSTR_0,FUNCT_2,FUNCT_1,RELAT_1,DOMAIN_1;
constructors BINOP_1,REAL_1,COMPLEX1,SUPINF_1,REALSET1,RFUNCT_3,RLSUB_2,RLVECT_2,RLVECT_3,NORMSP_1,XXREAL_2,RELSET_1,XXREAL_1;
requirements NUMERALS,REAL,BOOLE,SUBSET,ARITHM;
begin
reserve V for RealLinearSpace;
theorem
Th22: (for V1,V2,W being (Subspace of V) holds (for W1,W2 being (Subspace of W) holds ((W1 = V1 & W2 = V2) implies ( W1 + W2 ) = ( V1 + V2 ))))
proof
let V1 being (Subspace of V),V2 being (Subspace of V),W being (Subspace of V);
let W1 being (Subspace of W),W2 being (Subspace of W);
assume that
A1: (W1 = V1 & W2 = V2);
reconsider W3 = ( W1 + W2 ) as (Subspace of V) by RLSUB_1:27;
now
let v being (VECTOR of V);
A2: ((the carrier of W1) c= (the carrier of W) & (the carrier of W2) c= (the carrier of W)) by RLSUB_1:def 2;
hereby
assume A3: v in W3;
then reconsider w = v as (VECTOR of W) by CKB8:1;
consider w1 being (VECTOR of W),w2 being (VECTOR of W) such that A4: (w1 in W1 & w2 in W2) and A5: w = ( w1 + w2 ) by A3,RLSUB_2:1;
reconsider v1 = w1,v2 = w2 as (VECTOR of V) by RLSUB_1:10;
v = ( v1 + v2 ) by A5,RLSUB_1:13;
hence v in ( V1 + V2 ) by A1,A4,RLSUB_2:1;
end;
assume v in ( V1 + V2 );
then consider v1 being (VECTOR of V),v2 being (VECTOR of V) such that A6: (v1 in V1 & v2 in V2) and A7: v = ( v1 + v2 ) by RLSUB_2:1;
(v1 in (the carrier of W1) & v2 in (the carrier of W2)) by A1,A6,STRUCT_0:def 5;
then reconsider w1 = v1,w2 = v2 as (VECTOR of W) by A2;
v = ( w1 + w2 ) by A7,RLSUB_1:13;
hence v in W3 by A1,A6,RLSUB_2:1;
end;
hence thesis by RLSUB_1:31;
end;
