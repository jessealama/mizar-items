begin
theorem
Th14: (for C being Category holds (for I being (Indexing of C) holds (for D being  Categorial Category holds (( rng I ) is (Subcategory of D) iff D is (TargetCat of I)))))
proof
let C being Category;
let I being (Indexing of C);
let D being  Categorial Category;
hereby
assume A1: ( rng I ) is (Subcategory of D);
thus D is (TargetCat of I)
proof
hereby
let a being (Object of C);
( ( I `1 ) . a ) is (Object of ( rng I )) by CKB44:def 1;
hence ( ( I `1 ) . a ) is (Object of D) by A1,CAT_2:6;
end;
let b being (Morphism of C);
[ [ ( ( I `1 ) . ( (the Source of C) . b ) ),( ( I `1 ) . ( (the Target of C) . b ) ) ],( ( I `2 ) . b ) ] is (Morphism of ( rng I )) by CKB44:def 1;
hence thesis by A1,CAT_2:8;
end;

end;
assume D is (TargetCat of I);
then reconsider T = D as (TargetCat of I);
( rng I ) = ( Image ( I -functor (C,T) ) ) by CKB59:def 1;
hence thesis;
end;
