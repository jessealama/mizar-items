begin
reserve i for Nat;
reserve j for Nat;
reserve k for Nat;
reserve m for Nat;
reserve n for Nat;
reserve K for Field;
reserve M for (Matrix of n,K);
theorem
Th13: (for i holds (for j holds ((i in ( Seg n ) & j in ( Seg n )) implies (for k holds (for m holds ((k in ( Seg ( n -' 1 ) ) & m in ( Seg ( n -' 1 ) )) implies (((((k < i & m < j) implies ( ( Delete (M,i,j) ) * (k,m) ) = ( M * (k,m) )) & ((k < i & m >= j) implies ( ( Delete (M,i,j) ) * (k,m) ) = ( M * (k,( m + 1 )) ))) & ((k >= i & m < j) implies ( ( Delete (M,i,j) ) * (k,m) ) = ( M * (( k + 1 ),m) ))) & ((k >= i & m >= j) implies ( ( Delete (M,i,j) ) * (k,m) ) = ( M * (( k + 1 ),( m + 1 )) )))))))))
proof
let i;
let j;
assume that
A1: i in ( Seg n )
and
A2: j in ( Seg n );
set DM = ( Delete (M,i,j) );
A3: ( Deleting (M,i,j) ) = DM by A1,A2,CKB14:def 1;
n > ( 0 ) by A1;
then reconsider n9 = ( n - 1 ) as (Element of ( NAT )) by NAT_1:20;
set DL = ( DelLine (M,i) );
let k;
let m;
assume that
A4: k in ( Seg ( n -' 1 ) )
and
A5: m in ( Seg ( n -' 1 ) );
A6: ( n -' 1 ) = n9 by XREAL_0:def 2;
then A7: ( k + 1 ) in ( Seg ( n9 + 1 ) ) by A4,FINSEQ_1:60;
reconsider I = i,J = j,K = k,U = m as (Element of ( NAT )) by ORDINAL1:def 12;
n9 <= ( n9 + 1 ) by NAT_1:11;
then A8: ( Seg n9 ) c= ( Seg n ) by FINSEQ_1:5;
A9: ( len M ) = n by MATRIX_1:24;
then A10: ( dom M ) = ( Seg n ) by FINSEQ_1:def 3;
then ( len DL ) = n9 by A1,A6,A9,CKB1:1;
then A11: ( dom DL ) = ( Seg n9 ) by FINSEQ_1:def 3;
then A12: ( ( Deleting (M,i,j) ) . k ) = ( Del (( Line (DL,k) ),j) ) by A4,A6,MATRIX_2:def 5;
( len DM ) = n9 by A6,MATRIX_1:24;
then ( dom DM ) = ( Seg n9 ) by FINSEQ_1:def 3;
then A13: ( DM . k ) = ( Line (DM,k) ) by A4,A6,MATRIX_2:16;
( width DM ) = n9 by A6,MATRIX_1:24;
then A14: ( ( Line (DM,k) ) . m ) = ( DM * (k,m) ) by A5,A6,MATRIX_1:def 7;
A15: ( Line (DL,k) ) = ( DL . k ) by A4,A6,A11,MATRIX_2:16;
A16: ( m + 1 ) in ( Seg ( n9 + 1 ) ) by A5,A6,FINSEQ_1:60;
A17: (K >= I implies ((U < J implies ( DM * (K,U) ) = ( M * (( K + 1 ),U) )) & (U >= J implies ( DM * (K,U) ) = ( M * (( K + 1 ),( U + 1 )) ))))
proof
assume A18: K >= I;
K <= n9 by A4,A6,FINSEQ_1:1;
then A19: ( DL . K ) = ( M . ( K + 1 ) ) by A1,A9,A10,A7,A18,FINSEQ_3:111;
A20: ( M . ( K + 1 ) ) = ( Line (M,( K + 1 )) ) by A10,A7,MATRIX_2:16;
thus (U < J implies ( DM * (K,U) ) = ( M * (( K + 1 ),U) ))
proof
A21: ( width M ) = n by MATRIX_1:24;
assume U < J;
then ( DM * (K,U) ) = ( ( Line (M,( K + 1 )) ) . U ) by A12,A3,A13,A14,A15,A19,A20,FINSEQ_3:110;
hence thesis by A5,A6,A8,A21,MATRIX_1:def 7;
end;

assume A22: U >= J;
A23: U <= n9 by A5,A6,FINSEQ_1:1;
A24: ( width M ) = n by MATRIX_1:24;
A25: ( len ( Line (DL,K) ) ) = ( width M ) by A15,A19,A20,MATRIX_1:def 7;
then J in ( dom ( Line (DL,K) ) ) by A2,A24,FINSEQ_1:def 3;
then ( DM * (K,U) ) = ( ( Line (M,( K + 1 )) ) . ( U + 1 ) ) by A12,A3,A13,A14,A15,A7,A19,A20,A22,A25,A23,FINSEQ_3:111,MATRIX_1:24;
hence thesis by A16,A24,MATRIX_1:def 7;
end;
(K < I implies ((U < J implies ( DM * (K,U) ) = ( M * (K,U) )) & (U >= J implies ( DM * (K,U) ) = ( M * (K,( U + 1 )) ))))
proof
assume K < I;
then A26: ( DL . K ) = ( M . K ) by FINSEQ_3:110;
A27: ( M . K ) = ( Line (M,K) ) by A4,A6,A10,A8,MATRIX_2:16;
thus (U < J implies ( DM * (K,U) ) = ( M * (K,U) ))
proof
assume A28: U < J;
A29: ( width M ) = ( n9 + 1 ) by MATRIX_1:24;
( DM * (K,U) ) = ( ( Line (M,K) ) . U ) by A12,A3,A13,A14,A15,A26,A27,A28,FINSEQ_3:110;
hence thesis by A5,A6,A8,A29,MATRIX_1:def 7;
end;

assume A30: U >= J;
A31: U <= n9 by A5,A6,FINSEQ_1:1;
A32: ( width M ) = n by MATRIX_1:24;
A33: ( len ( Line (DL,K) ) ) = ( width M ) by A15,A26,A27,MATRIX_1:def 7;
then J in ( dom ( Line (DL,K) ) ) by A2,A32,FINSEQ_1:def 3;
then ( DM * (K,U) ) = ( ( Line (M,K) ) . ( U + 1 ) ) by A12,A3,A13,A14,A15,A7,A26,A27,A30,A33,A31,FINSEQ_3:111,MATRIX_1:24;
hence thesis by A16,A32,MATRIX_1:def 7;
end;
hence thesis by A17;
end;
