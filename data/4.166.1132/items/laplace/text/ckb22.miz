begin
reserve c for Nat;
reserve i for Nat;
reserve j for Nat;
reserve m for Nat;
reserve n for Nat;
reserve D for non  empty set;
reserve AD for (Matrix of n,m,D);
theorem
( RCol (AD,c,( Col (AD,c) )) ) = AD
proof
set C = ( Col (AD,c) );
set R = ( RCol (AD,c,C) );
now
reconsider c as (Element of ( NAT )) by ORDINAL1:def 12;
let i being Nat;
let j being Nat;
assume that
A1: [ i,j ] in ( Indices AD );
A2: ( len C ) = ( len AD ) by MATRIX_1:def 8;
reconsider I = i,J = j as (Element of ( NAT )) by ORDINAL1:def 12;
A3: i in ( dom AD ) by A1,ZFMISC_1:87;
now
per cases ;
suppose A4: c = j;

hence ( R * (i,j) ) = ( C . I ) by A1,A2,CKB18:def 1
.= ( AD * (i,j) ) by A3,A4,MATRIX_1:def 8;
end;
suppose c <> J;

hence ( R * (i,j) ) = ( AD * (i,j) ) by A1,A2,CKB18:def 1;
end;
end;
hence ( R * (i,j) ) = ( AD * (i,j) );
end;
hence thesis by MATRIX_1:27;
end;
