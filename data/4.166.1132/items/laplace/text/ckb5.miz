begin
reserve i for Nat;
reserve j for Nat;
reserve n for Nat;
reserve K for Field;
reserve M for (Matrix of n,K);
theorem
Th5: (for i being Nat holds (j in ( Seg ( width M ) ) implies ( width ( Deleting (M,i,j) ) ) = ( n -' 1 )))
proof
let i being Nat;
assume A1: j in ( Seg ( width M ) );
per cases ;
suppose A2: (( len M ) <= 1 & i in ( dom M ));

( Seg ( width M ) ) <> ( {} ) by A1;
then ( width M ) <> ( {} );
then ( len M ) > ( 0 ) by MATRIX_1:def 3;
then A3: ( len M ) = 1 by A2,NAT_1:25;
A4: ( len ( Deleting (M,i,j) ) ) = ( n -' 1 ) by A2,CKB2:1;
( len M ) = n by MATRIX_1:24;
then ( len ( Deleting (M,i,j) ) ) = ( 0 ) by A3,A4,XREAL_1:232;
hence thesis by A4,MATRIX_1:def 3;
end;
suppose A5: ( len M ) > 1;

A6: ( width M ) = n by MATRIX_1:24;
( width M ) = ( width ( DelLine (M,i) ) ) by A5,CKB4:1;
hence thesis by A1,A6,CKB3:1;
end;
suppose A7: (not i in ( dom M ));

A8: ( width M ) = n by MATRIX_1:24;
( DelLine (M,i) ) = M by A7,FINSEQ_3:104;
hence thesis by A1,A8,CKB3:1;
end;
end;
