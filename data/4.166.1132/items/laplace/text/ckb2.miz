begin
reserve i for Nat;
reserve j for Nat;
reserve n for Nat;
reserve K for Field;
reserve M for (Matrix of n,K);
theorem
Th2: (for i,j,n being Nat holds (for M being (Matrix of n,K) holds (i in ( dom M ) implies ( len ( Deleting (M,i,j) ) ) = ( n -' 1 ))))
proof
let i being Nat;
let j being Nat;
let n being Nat;
let M being (Matrix of n,K);
assume A1: i in ( dom M );
A2: ( len M ) = n by MATRIX_1:def 2;
thus ( len ( Deleting (M,i,j) ) ) = ( len ( DelLine (M,i) ) ) by MATRIX_2:def 5
.= ( n -' 1 ) by A1,A2,CKB1:1;
end;
