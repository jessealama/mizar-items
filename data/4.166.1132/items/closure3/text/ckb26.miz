environ
vocabularies XBOOLE_0,STRUCT_0,PBOOLE,FUNCT_4,RELAT_1,CLOSURE2,SETFAM_1,MSUALG_1,TARSKI,MSUALG_2,UNIALG_2,MSSUBFAM,FUNCT_1,MARGREL1,SUBSET_1,FINSET_1,FUNCOP_1,WAYBEL_8,RELAT_2,MSAFREE2,BINOP_1,ZFMISC_1,CARD_3,FUNCT_2,CLOSURE3;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,SETFAM_1,RELAT_1,FUNCT_1,STRUCT_0,PARTFUN1,FUNCT_2,FINSET_1,FUNCT_4,FUNCOP_1,PBOOLE,FINSEQ_2,MSUALG_1,MSUALG_2,CARD_3,CLOSURE2,MSSUBFAM,MBOOLEAN,CKB6,CKB11,CKB14,CKB17,CKB20,CKB22,CKB23;
definitions XBOOLE_0,TARSKI,PBOOLE,CLOSURE2,MSUALG_2,SETFAM_1,CKB6,CKB11,CKB20,CKB22,CKB23;
theorems FUNCT_4,FUNCT_1,TARSKI,PBOOLE,MSUALG_1,MBOOLEAN,FUNCOP_1,MSUALG_2,CLOSURE2,ZFMISC_1,RELAT_1,CARD_3,MSSUBFAM,FUNCT_2,XBOOLE_0,XBOOLE_1,SETFAM_1,PARTFUN1,FINSET_1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB13,CKB15,CKB16,CKB18,CKB19,CKB20,CKB22,CKB23,CKB24;
schemes CLASSES1;
registrations XBOOLE_0,SUBSET_1,FUNCT_1,RELSET_1,FUNCT_2,FINSET_1,PBOOLE,STRUCT_0,MSUALG_1,CLOSURE2,CKB1,CKB12,CKB21,CKB25;
constructors SETFAM_1,FUNCT_4,MSSUBFAM,MSUALG_2,PRALG_2,CLOSURE2,RELSET_1,FINSEQ_2,CKB6,CKB11,CKB14,CKB17,CKB20,CKB22,CKB23;
requirements SUBSET,BOOLE;
begin
registration
let S being non  void non  empty ManySortedSign;
let MA being  strict  non-empty MSAlgebra over S;
cluster ( SubAlgCl MA ) ->  algebraic;
coherence
proof
reconsider I = (the carrier of S) as non  empty set;
set SS = ( ClSys->ClOp ( SubAlgCl MA ) );
set M = (the Sorts of ( SubAlgCl MA ));
let x being (Element of ( Bool M ));
assume that
A1: x = ( SS . x );
set A = { ( SS . a ) where a is (Element of ( Bool M )): ((a is  finite-yielding & ( supp a ) is  finite) & a c= x) };
A c= ( Bool M )
proof
let v being set;
assume v in A;
then (ex a being (Element of ( Bool M )) st (((v = ( SS . a ) & a is  finite-yielding) & ( supp a ) is  finite) & a c= x));
then reconsider vv = v as (Element of ( Bool M ));
vv in ( Bool M );
hence thesis;
end;
then reconsider A as (SubsetFamily of M);
take A;
thus A = { ( SS . b ) where b is (Element of ( Bool M )): ((b is  finite-yielding & ( supp b ) is  finite) & b c= x) };
(for i being set holds (i in I implies ( x . i ) c= ( ( MSUnion A ) . i )))
proof
reconsider SS9 = SS as  reflexive (SetOp of M);
let i being set;
assume that
A2: i in I;
let v being set;
assume v in ( x . i );
then consider b being (Element of ( Bool M )) such that A3: v in ( b . i ) and A4: ((b is  finite-yielding & ( supp b ) is  finite) & b c= x) by A2,CKB10:1;
b c=' ( SS9 . b ) by CLOSURE2:def 10;
then A5: ( b . i ) c= ( ( SS9 . b ) . i ) by A2,PBOOLE:def 2;
( SS . b ) in A by A4;
then ( ( SS9 . b ) . i ) in { ( f . i ) where f is (Element of ( Bool M )): f in A };
then v in ( union { ( f . i ) where f is (Element of ( Bool M )): f in A } ) by A3,A5,TARSKI:def 4;
hence thesis by A2,CKB11:def 1;
end;
hence x c= ( MSUnion A ) by PBOOLE:def 2;
(for i being set holds (i in I implies ( ( MSUnion A ) . i ) c= ( x . i )))
proof
let i being set;
assume that
A6: i in I;
(for v being set holds (v in ( ( MSUnion A ) . i ) implies v in ( x . i )))
proof
reconsider SS9 = SS as  monotonic (SetOp of M);
let v being set;
assume v in ( ( MSUnion A ) . i );
then v in ( union { ( ff . i ) where ff is (Element of ( Bool M )): ff in A } ) by A6,CKB11:def 1;
then consider Y being set such that A7: v in Y and A8: Y in { ( ff . i ) where ff is (Element of ( Bool M )): ff in A } by TARSKI:def 4;
consider ff being (Element of ( Bool M )) such that A9: Y = ( ff . i ) and A10: ff in A by A8;
consider a being (Element of ( Bool M )) such that A11: ff = ( SS . a ) and a is  finite-yielding and ( supp a ) is  finite and A12: a c=' x by A10;
( SS9 . a ) c=' ( SS9 . x ) by A12,CLOSURE2:def 11;
then ( ff . i ) c= ( x . i ) by A1,A6,A11,PBOOLE:def 2;
hence thesis by A7,A9;
end;
hence thesis by TARSKI:def 3;
end;
hence ( MSUnion A ) c= x by PBOOLE:def 2;
end;
end;
