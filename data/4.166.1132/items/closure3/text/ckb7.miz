theorem
(for I being non  empty set holds (for M being  non-empty (ManySortedSet of I) holds M = ( ( [[0]] I ) +* ( M | ( supp M ) ) )))
proof
let I being non  empty set;
let M being  non-empty (ManySortedSet of I);
set MM = ( M | ( supp M ) );
A1: I c= ( supp M )
proof
let v being set;
assume A2: v in I;
then ( M . v ) <> ( {} );
then v in { x where x is (Element of I): ( M . x ) <> ( {} ) } by A2;
hence thesis by CKB6:def 1;
end;
( dom M ) = I by PARTFUN1:def 2;
then MM = M by A1,RELAT_1:68;
hence thesis by CKB2:1;
end;
