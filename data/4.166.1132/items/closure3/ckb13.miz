begin
theorem
(for I being set holds (for M being (ManySortedSet of I) holds (for A being (SubsetFamily of M) holds ( MSUnion A ) = ( union |: A :| ))))
proof
let I being set;
let M being (ManySortedSet of I);
let A being (SubsetFamily of M);
set AA = |: A :|;
reconsider UA = ( union AA ) as (ManySortedSet of I);
per cases ;
suppose A1: A is non empty;

(for i being set holds (i in I implies ( ( MSUnion A ) . i ) = ( UA . i )))
proof
let i being set;
assume A2: i in I;
then (( AA . i ) = { ( g . i ) where g is (Element of ( Bool M )): g in A } & ( UA . i ) = ( union ( AA . i ) )) by A1,CLOSURE2:14,MBOOLEAN:def 2;
hence thesis by A2,CKB11:def 1;
end;
hence thesis by PBOOLE:3;
end;
suppose A3: A is  empty  empty (SubsetFamily of M);

(for i being set holds (i in I implies ( ( MSUnion A ) . i ) = ( UA . i )))
proof
let i being set;
assume A4: i in I;
AA = ( [[0]] I ) by A3,CLOSURE2:def 3;
then UA = ( [[0]] I ) by MBOOLEAN:21;
then ( UA . i ) is  empty by A4,PBOOLE:def 12;
hence thesis by A3,A4,PBOOLE:def 12;
end;
hence thesis by PBOOLE:3;
end;
end;
