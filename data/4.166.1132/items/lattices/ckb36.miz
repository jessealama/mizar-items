environ
vocabularies STRUCT_0,BINOP_1,XBOOLE_0,ZFMISC_1,SUBSET_1,EQREL_1,FUNCT_1,PBOOLE,XXREAL_2,CAT_1,LATTICES,CARD_1;
notations TARSKI,XBOOLE_0,ZFMISC_1,CARD_1,STRUCT_0,BINOP_1,CKB1,CKB2,CKB3,CKB10,CKB11,CKB12,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34;
definitions STRUCT_0,CKB10,CKB11,CKB12,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB31,CKB32,CKB33,CKB34;
theorems ZFMISC_1,SUBSET_1,STRUCT_0,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB31,CKB32,CKB33,CKB34,CKB35;
registrations XBOOLE_0,SUBSET_1,CARD_1,ORDINAL1,STRUCT_0,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB23,CKB24,CKB25,CKB26,CKB27;
constructors BINOP_1,STRUCT_0,CKB1,CKB2,CKB3,CKB10,CKB11,CKB12,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34;
requirements SUBSET,BOOLE,NUMERALS;
begin
registration
cluster  strict  distributive  lower-bounded  upper-bounded  modular for Lattice;
existence
proof
set uu = the (BinOp of ( bool ( {} ) ));
set GG = LattStr (# ( bool ( {} ) ),uu,uu #);
reconsider GG as Lattice by CKB35:1;
reconsider 0GG = ( {} ),D = ( {} ) as (Element of GG) by ZFMISC_1:def 1;
(for x being (Element of GG) holds (( 0GG "/\" x ) = 0GG & ( x "/\" 0GG ) = 0GG));
then A1: GG is  lower-bounded by CKB33:def 1;
(for x being (Element of GG) holds (( D "\/" x ) = D & ( x "\/" D ) = D));
then A2: GG is  upper-bounded by CKB34:def 1;
(for x,y,z being (Element of GG) holds ( x "/\" ( y "\/" z ) ) = ( ( x "/\" y ) "\/" ( x "/\" z ) )) by CKB13:1;
then A3: GG is  distributive by CKB31:def 1;
(for x,y,z being (Element of GG) holds (x [= z implies ( x "\/" ( y "/\" z ) ) = ( ( x "\/" y ) "/\" z ))) by CKB13:1;
then GG is  modular by CKB32:def 1;
hence thesis by A1,A3,A2;
end;
end;
