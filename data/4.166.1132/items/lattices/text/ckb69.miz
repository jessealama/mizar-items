begin
theorem
(for L being Lattice holds ((for a,b,c being (Element of L) holds ( ( ( a "/\" b ) "\/" ( b "/\" c ) ) "\/" ( c "/\" a ) ) = ( ( ( a "\/" b ) "/\" ( b "\/" c ) ) "/\" ( c "\/" a ) )) implies L is  distributive))
proof
let L being Lattice;
assume A1: (for a,b,c being (Element of L) holds ( ( ( a "/\" b ) "\/" ( b "/\" c ) ) "\/" ( c "/\" a ) ) = ( ( ( a "\/" b ) "/\" ( b "\/" c ) ) "/\" ( c "\/" a ) ));
A2:now
let a being (Element of L),b being (Element of L),c being (Element of L);
assume A3: c [= a;
thus ( a "/\" ( b "\/" c ) ) = ( ( b "\/" c ) "/\" ( a "/\" ( a "\/" b ) ) ) by CKB21:def 1
.= ( ( a "\/" b ) "/\" ( ( b "\/" c ) "/\" a ) ) by CKB19:def 1
.= ( ( a "\/" b ) "/\" ( ( b "\/" c ) "/\" ( c "\/" a ) ) ) by A3,CKB12:def 1
.= ( ( ( a "\/" b ) "/\" ( b "\/" c ) ) "/\" ( c "\/" a ) ) by CKB19:def 1
.= ( ( ( a "/\" b ) "\/" ( b "/\" c ) ) "\/" ( c "/\" a ) ) by A1
.= ( ( a "/\" b ) "\/" ( ( b "/\" c ) "\/" ( c "/\" a ) ) ) by CKB17:def 1
.= ( ( a "/\" b ) "\/" ( ( b "/\" c ) "\/" c ) ) by A3,CKB62:1
.= ( ( a "/\" b ) "\/" c ) by CKB20:def 1;
end;
let a being (Element of L),b being (Element of L),c being (Element of L);
( ( ( a "/\" b ) "\/" ( c "/\" a ) ) "\/" a ) = ( ( a "/\" b ) "\/" ( ( c "/\" a ) "\/" a ) ) by CKB17:def 1
.= ( ( a "/\" b ) "\/" a ) by CKB20:def 1
.= a by CKB20:def 1;
then A4: ( ( a "/\" b ) "\/" ( c "/\" a ) ) [= a by CKB12:def 1;
thus ( a "/\" ( b "\/" c ) ) = ( ( a "/\" ( c "\/" a ) ) "/\" ( b "\/" c ) ) by CKB21:def 1
.= ( a "/\" ( ( c "\/" a ) "/\" ( b "\/" c ) ) ) by CKB19:def 1
.= ( ( a "/\" ( a "\/" b ) ) "/\" ( ( c "\/" a ) "/\" ( b "\/" c ) ) ) by CKB21:def 1
.= ( a "/\" ( ( a "\/" b ) "/\" ( ( b "\/" c ) "/\" ( c "\/" a ) ) ) ) by CKB19:def 1
.= ( a "/\" ( ( ( a "\/" b ) "/\" ( b "\/" c ) ) "/\" ( c "\/" a ) ) ) by CKB19:def 1
.= ( a "/\" ( ( ( b "/\" c ) "\/" ( a "/\" b ) ) "\/" ( c "/\" a ) ) ) by A1
.= ( a "/\" ( ( b "/\" c ) "\/" ( ( a "/\" b ) "\/" ( c "/\" a ) ) ) ) by CKB17:def 1
.= ( ( a "/\" ( b "/\" c ) ) "\/" ( ( a "/\" b ) "\/" ( c "/\" a ) ) ) by A2,A4
.= ( ( ( a "/\" b ) "/\" c ) "\/" ( ( a "/\" b ) "\/" ( c "/\" a ) ) ) by CKB19:def 1
.= ( ( ( ( a "/\" b ) "/\" c ) "\/" ( a "/\" b ) ) "\/" ( c "/\" a ) ) by CKB17:def 1
.= ( ( a "/\" b ) "\/" ( a "/\" c ) ) by CKB20:def 1;
end;
