theorem
Th27: (for L being  meet-absorbing  join-absorbing  meet-associative non empty LattStr holds (for a,b,c being (Element of L) holds (a [= b implies ( a "/\" c ) [= ( b "/\" c ))))
proof
let L being  meet-absorbing  join-absorbing  meet-associative non empty LattStr;
let a being (Element of L),b being (Element of L),c being (Element of L);
assume a [= b;
hence ( ( a "/\" c ) "\/" ( b "/\" c ) ) = ( ( ( a "/\" b ) "/\" c ) "\/" ( b "/\" c ) ) by CKB62:1
.= ( ( a "/\" ( b "/\" c ) ) "\/" ( b "/\" c ) ) by CKB19:def 1
.= ( b "/\" c ) by CKB20:def 1;
end;
