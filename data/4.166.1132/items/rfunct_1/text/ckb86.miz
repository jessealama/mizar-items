theorem
(( f | Y ) is  bounded iff (ex r st (for c being set holds (c in ( Y /\ ( dom f ) ) implies ( abs ( f . c ) ) <= r))))
proof
thus (( f | Y ) is  bounded implies (ex r st (for c being set holds (c in ( Y /\ ( dom f ) ) implies ( abs ( f . c ) ) <= r))))
proof
assume A1: ( f | Y ) is  bounded;
then consider r1 such that A2: (for c being set holds (c in ( Y /\ ( dom f ) ) implies ( f . c ) <= r1)) by CKB83:1;
consider r2 such that A3: (for c being set holds (c in ( Y /\ ( dom f ) ) implies r2 <= ( f . c ))) by A1,CKB84:1;
take g = ( ( abs r1 ) + ( abs r2 ) );
A4: r1 <= ( abs r1 ) by ABSVALUE:4;
let c being set;
assume that
A5: c in ( Y /\ ( dom f ) );
( f . c ) <= r1 by A2,A5;
then A6: ( f . c ) <= ( abs r1 ) by A4,XXREAL_0:2;
A7: ( - ( abs r2 ) ) <= r2 by ABSVALUE:4;
r2 <= ( f . c ) by A3,A5;
then A8: ( - ( abs r2 ) ) <= ( f . c ) by A7,XXREAL_0:2;
( 0 ) <= ( abs r1 ) by COMPLEX1:46;
then A9: ( ( - ( abs r1 ) ) + ( - ( abs r2 ) ) ) <= ( ( 0 ) + ( f . c ) ) by A8,XREAL_1:7;
( 0 ) <= ( abs r2 ) by COMPLEX1:46;
then A10: ( ( f . c ) + ( 0 ) ) <= ( ( abs r1 ) + ( abs r2 ) ) by A6,XREAL_1:7;
( ( - ( abs r1 ) ) + ( - ( abs r2 ) ) ) = ( - g );
hence thesis by A10,A9,ABSVALUE:5;
end;

given r such that
A11: (for c being set holds (c in ( Y /\ ( dom f ) ) implies ( abs ( f . c ) ) <= r));

now
let c being set;
assume c in ( Y /\ ( dom f ) );
then ( abs ( f . c ) ) <= r by A11;
then A12: ( - r ) <= ( - ( abs ( f . c ) ) ) by XREAL_1:24;
( - ( abs ( f . c ) ) ) <= ( f . c ) by ABSVALUE:4;
hence ( - r ) <= ( f . c ) by A12,XXREAL_0:2;
end;
then A13: ( f | Y ) is  bounded_below by CKB84:1;
now
let c being set;
assume c in ( Y /\ ( dom f ) );
then A14: ( abs ( f . c ) ) <= r by A11;
( f . c ) <= ( abs ( f . c ) ) by ABSVALUE:4;
hence ( f . c ) <= r by A14,XXREAL_0:2;
end;
then ( f | Y ) is  bounded_above by CKB83:1;
hence thesis by A13;
end;
