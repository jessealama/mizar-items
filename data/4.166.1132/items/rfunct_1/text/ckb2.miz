begin
reserve x for set;
definition
let f1 being  complex-valued Function;
let f2 being  complex-valued Function;
func f1 / f2 -> Function
means
:Def4: (( dom it ) = ( ( dom f1 ) /\ ( ( dom f2 ) \ ( f2 " { ( 0 ) } ) ) ) & (for c being set holds (c in ( dom it ) implies ( it . c ) = ( ( f1 . c ) * ( ( f2 . c ) " ) ))));
existence
proof
deffunc F(set) = ( ( f1 . $1 ) * ( ( f2 . $1 ) " ) );
(ex f being Function st (( dom f ) = ( ( dom f1 ) /\ ( ( dom f2 ) \ ( f2 " { ( 0 ) } ) ) ) & (for x holds (x in ( ( dom f1 ) /\ ( ( dom f2 ) \ ( f2 " { ( 0 ) } ) ) ) implies ( f . x ) = F(x))))) from FUNCT_1:sch 3;
hence thesis;
end;
uniqueness
proof
let f being Function,g being Function;
assume that
A1: ( dom f ) = ( ( dom f1 ) /\ ( ( dom f2 ) \ ( f2 " { ( 0 ) } ) ) )
and
A2: (for c being set holds (c in ( dom f ) implies ( f . c ) = ( ( f1 . c ) * ( ( f2 . c ) " ) )))
and
A3: ( dom g ) = ( ( dom f1 ) /\ ( ( dom f2 ) \ ( f2 " { ( 0 ) } ) ) )
and
A4: (for c being set holds (c in ( dom g ) implies ( g . c ) = ( ( f1 . c ) * ( ( f2 . c ) " ) )));
now
let x;
assume A5: x in ( dom f );
hence ( f . x ) = ( ( f1 . x ) * ( ( f2 . x ) " ) ) by A2
.= ( g . x ) by A1,A3,A4,A5;
end;
hence thesis by A1,A3,FUNCT_1:2;
end;
end;
