environ
vocabularies NUMBERS,XREAL_0,ORDINAL1,SUBSET_1,INT_1,RAT_1,RELAT_1,ARYTM_1,NEWTON,ARYTM_3,CARD_1,PREPOWER,FUNCT_1,XXREAL_0,NAT_1,REAL_1,SEQ_1,SEQ_2,ORDINAL2,SQUARE_1,XXREAL_2,SEQ_4,COMPLEX1,VALUED_0,POWER,ABIAN;
notations SUBSET_1,NEWTON,ORDINAL1,XCMPLX_0,XREAL_0,XXREAL_0,COMPLEX1,INT_1,RAT_1,REAL_1,NUMBERS,NAT_1,SEQ_1,SEQ_2,SEQM_3,SQUARE_1,SEQ_4,PREPOWER,ABIAN,XXREAL_2,CKB4,CKB5;
definitions SQUARE_1,XCMPLX_0,INT_1,ABIAN,XXREAL_2,CKB4;
theorems ABSVALUE,NAT_1,SEQ_2,SEQM_3,INT_1,SEQ_4,NEWTON,PREPOWER,TARSKI,FUNCT_2,XREAL_0,XCMPLX_1,XREAL_1,XXREAL_0,ABIAN,CKB1,CKB2,CKB3,CKB4,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15;
schemes SEQ_1;
registrations NUMBERS,XXREAL_0,XREAL_0,SQUARE_1,NAT_1,INT_1,RAT_1,MEMBERED,NEWTON,PREPOWER,XBOOLE_0,SEQ_4,ABIAN;
constructors REAL_1,SQUARE_1,NAT_1,SEQ_2,SEQM_3,SEQ_4,NEWTON,PREPOWER,SEQ_1,XXREAL_2,RELSET_1,ABIAN,BINOP_2,CKB4,CKB5;
requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve a for  real number;
reserve m for (Element of ( NAT ));
reserve n for (Element of ( NAT ));
reserve m1 for (Element of ( NAT ));
reserve m2 for (Element of ( NAT ));
theorem
((((a >= ( 0 ) & n >= 1) & m >= 1) or (n is  odd & m is  odd)) implies ( n -root ( m -root a ) ) = ( ( n * m ) -root a ))
proof
assume A1: (((a >= ( 0 ) & n >= 1) & m >= 1) or (n is  odd & m is  odd));
A2:now
let a;
let n;
let m;
assume that
A3: a >= ( 0 )
and
A4: n >= 1
and
A5: m >= 1;
A6: ( n * m ) >= 1 by A4,A5,XREAL_1:159;
( m -root a ) >= ( 0 ) by A3,A5,CKB9:1;
then A7: ( m -Root a ) >= ( 0 ) by A3,A5,CKB4:def 1;
thus ( n -root ( m -root a ) ) = ( n -root ( m -Root a ) ) by A3,A5,CKB4:def 1
.= ( n -Root ( m -Root a ) ) by A4,A7,CKB4:def 1
.= ( ( n * m ) -Root a ) by A3,A4,A5,PREPOWER:25
.= ( ( n * m ) -root a ) by A3,A6,CKB4:def 1;
end;
now
assume n is  odd;
then consider m1 such that A8: n = ( ( 2 * m1 ) + 1 ) by ABIAN:9;
assume m is  odd;
then consider m2 such that A9: m = ( ( 2 * m2 ) + 1 ) by ABIAN:9;
A10: n >= ( ( 0 ) + 1 ) by A8,XREAL_1:6;
A11: m >= ( ( 0 ) + 1 ) by A9,XREAL_1:6;
then A12: ( n * m ) >= 1 by A10,XREAL_1:159;
A13: ( n * m ) = ( ( 2 * ( ( ( m1 * ( 2 * m2 ) ) + m1 ) + m2 ) ) + 1 ) by A8,A9;
now
per cases ;
suppose a >= ( 0 );

hence thesis by A2,A10,A11;
end;
suppose A14: a < ( 0 );

then ( m -root ( - a ) ) >= ( 0 ) by A11,CKB9:1;
then A15: ( m -Root ( - a ) ) >= ( 0 ) by A11,A14,CKB4:def 1;
thus ( n -root ( m -root a ) ) = ( n -root ( - ( m -Root ( - a ) ) ) ) by A9,A14,CKB4:def 1
.= ( - ( n -root ( - ( - ( m -Root ( - a ) ) ) ) ) ) by A8,CKB12:1
.= ( - ( n -Root ( m -Root ( - a ) ) ) ) by A10,A15,CKB4:def 1
.= ( - ( ( n * m ) -Root ( - a ) ) ) by A10,A11,A14,PREPOWER:25
.= ( - ( ( n * m ) -root ( - a ) ) ) by A12,A14,CKB4:def 1
.= ( ( n * m ) -root a ) by A13,CKB12:1;
end;
end;
hence thesis;
end;
hence thesis by A1,A2;
end;
