scheme NonUniqPiFinRecExD { D() -> non  empty set,A() -> (Element of D()),N() -> Nat,P[set,set,set] } : (ex p being (FinSequence of D()) st ((( len p ) = N() & (( p /. 1 ) = A() or N() = ( 0 ))) & (for n being Nat holds ((1 <= n & n < N()) implies P[ n,( p /. n ),( p /. ( n + 1 ) ) ]))))
provided
A1: (for n being Nat holds ((1 <= n & n < N()) implies (for x being (Element of D()) holds (ex y being (Element of D()) st P[ n,x,y ]))))
proof
A2: (for n being (Element of ( NAT )) holds ((1 <= n & n < N()) implies (for x being (Element of D()) holds (ex y being (Element of D()) st P[ n,x,y ])))) by A1;
consider p being (FinSequence of D()) such that A3: ( len p ) = N() and A4: (( p . 1 ) = A() or N() = ( 0 )) and A5: (for n being (Element of ( NAT )) holds ((1 <= n & n < N()) implies P[ n,( p . n ),( p . ( n + 1 ) ) ])) from RECDEF_1:sch 4(A2);
take p;
thus ( len p ) = N() by A3;
now
assume N() <> ( 0 );
then N() >= ( ( 0 ) + 1 ) by NAT_1:13;
hence ( p /. 1 ) = A() by A3,A4,FINSEQ_4:15;
end;
hence (( p /. 1 ) = A() or N() = ( 0 ));
let n being Nat;
