requirements NUMERALS,SUBSET,REAL,BOOLE,ARITHM;
begin
reserve x0 for Real;
reserve f for (PartFunc of ( REAL ),( REAL ));
reserve X for set;
theorem
(for X holds (for f holds (X c= ( dom f ) implies ((f is_upper_semicontinuous_on X & f is_lower_semicontinuous_on X) iff ( f | X ) is  continuous))))
proof
let X;
let f;
assume that
A1: X c= ( dom f );
A2: (( f | X ) is  continuous implies (f is_upper_semicontinuous_on X & f is_lower_semicontinuous_on X))
proof
assume A3: ( f | X ) is  continuous;
A4: (for x0 holds (x0 in X implies ( f | X ) is_lower_semicontinuous_in x0))
proof
let x0;
assume x0 in X;
then A5: x0 in ( dom ( f | X ) ) by A1,RELAT_1:57;
then ( f | X ) is_continuous_in x0 by A3,FCONT_1:def 2;
hence thesis by A5,CKB30:1;
end;
(for x0 holds (x0 in X implies ( f | X ) is_upper_semicontinuous_in x0))
proof
let x0;
assume x0 in X;
then A6: x0 in ( dom ( f | X ) ) by A1,RELAT_1:57;
then ( f | X ) is_continuous_in x0 by A3,FCONT_1:def 2;
hence thesis by A6,CKB30:1;
end;
hence thesis by A1,A4,CKB27:def 1,CKB29:def 1;
end;
((f is_upper_semicontinuous_on X & f is_lower_semicontinuous_on X) implies ( f | X ) is  continuous)
proof
assume that
A7: f is_upper_semicontinuous_on X
and
A8: f is_lower_semicontinuous_on X;
(X c= ( dom f ) & (for x0 being  real number holds (x0 in ( dom ( f | X ) ) implies ( f | X ) is_continuous_in x0)))
proof
thus X c= ( dom f ) by A7,CKB27:def 1;
let x0 being  real number;
assume that
A9: x0 in ( dom ( f | X ) );
A10: x0 is Real by XREAL_0:def 1;
x0 in X by A9,RELAT_1:57;
then (( f | X ) is_upper_semicontinuous_in x0 & ( f | X ) is_lower_semicontinuous_in x0) by A7,A8,A10,CKB27:def 1,CKB29:def 1;
hence thesis by A9,CKB30:1;
end;
hence thesis by FCONT_1:def 2;
end;
hence thesis by A2;
end;
