environ
vocabularies NUMBERS,REAL_1,PARTFUN1,SUBSET_1,XREAL_0,ORDINAL1,XXREAL_0,FINSEQ_2,RVSUM_1,ORDINAL4,FINSEQ_1,RELAT_1,ARYTM_3,NAT_1,FUNCT_1,CARD_3,CARD_1,TARSKI,ARYTM_1,RFUNCT_3,XXREAL_1,VALUED_1,XBOOLE_0,BINOP_2,MEASURE5,FCONT_1,COMPLEX1,ORDINAL2,RFUNCT_4;
notations TARSKI,XBOOLE_0,SUBSET_1,ORDINAL1,NUMBERS,XCMPLX_0,XREAL_0,RELAT_1,COMPLEX1,NAT_1,FUNCT_1,FINSEQ_1,REAL_1,FINSEQ_2,PARTFUN1,FINSEQOP,BINOP_2,VALUED_1,SEQ_1,RVSUM_1,RCOMP_1,RFUNCT_3,MEASURE5,FCONT_1,XXREAL_0;
definitions FINSEQ_2,RVSUM_1;
theorems RCOMP_1,RFUNCT_3,TARSKI,PARTFUN1,FINSEQ_1,FINSEQ_2,RVSUM_1,NAT_1,FCONT_1,ABSVALUE,FUNCT_1,XREAL_0,XBOOLE_1,XCMPLX_0,XCMPLX_1,XREAL_1,COMPLEX1,XXREAL_0,FINSEQOP,FUNCOP_1,VALUED_1,XXREAL_2,RELAT_1,CARD_1,CKB1;
schemes NAT_1;
registrations RELSET_1,NUMBERS,XXREAL_0,XREAL_0,BINOP_2,MEMBERED,FINSEQ_2,XBOOLE_0,VALUED_0,CARD_1,FINSEQ_1;
constructors BINOP_1,REAL_1,NAT_1,BINOP_2,COMPLEX1,FINSEQOP,RCOMP_1,RVSUM_1,FCONT_1,RFUNCT_3,MEASURE5,SEQ_1,XXREAL_2,RELSET_1;
requirements NUMERALS,SUBSET,REAL,BOOLE,ARITHM;
begin
theorem
Th2: (for n being (Element of ( NAT )) holds (for R1,R2 being (Element of ( n -tuples_on ( REAL ) )) holds (for r1,r2 being Real holds ( mlt (( R1 ^ <* r1 *> ),( R2 ^ <* r2 *> )) ) = ( ( mlt (R1,R2) ) ^ <* ( r1 * r2 ) *> ))))
proof
let n being (Element of ( NAT ));
let R1 being (Element of ( n -tuples_on ( REAL ) ));
let R2 being (Element of ( n -tuples_on ( REAL ) ));
let r1 being Real;
let r2 being Real;
( len ( mlt (R1,R2) ) ) = n by CARD_1:def 7;
then ( len ( ( mlt (R1,R2) ) ^ <* ( r1 * r2 ) *> ) ) = ( n + 1 ) by FINSEQ_2:16;
then A1: ( ( mlt (R1,R2) ) ^ <* ( r1 * r2 ) *> ) is (Element of ( ( n + 1 ) -tuples_on ( REAL ) )) by FINSEQ_2:92;
( len ( R1 ^ <* r1 *> ) ) = ( ( len R1 ) + 1 ) by FINSEQ_2:16
.= ( n + 1 ) by CARD_1:def 7
.= ( ( len R2 ) + 1 ) by CARD_1:def 7
.= ( len ( R2 ^ <* r2 *> ) ) by FINSEQ_2:16;
then A2: ( len ( mlt (( R1 ^ <* r1 *> ),( R2 ^ <* r2 *> )) ) ) = ( len ( R1 ^ <* r1 *> ) ) by FINSEQ_2:72
.= ( ( len R1 ) + 1 ) by FINSEQ_2:16
.= ( n + 1 ) by CARD_1:def 7;
A3: (for k being Nat holds (k in ( Seg ( n + 1 ) ) implies ( ( mlt (( R1 ^ <* r1 *> ),( R2 ^ <* r2 *> )) ) . k ) = ( ( ( mlt (R1,R2) ) ^ <* ( r1 * r2 ) *> ) . k )))
proof
let k being Nat;
assume that
A4: k in ( Seg ( n + 1 ) );
A5: k <= ( n + 1 ) by A4,FINSEQ_1:1;
now
per cases  by A5,XXREAL_0:1;
suppose k < ( n + 1 );

then A6: k <= n by NAT_1:13;
1 <= k by A4,FINSEQ_1:1;
then A7: k in ( Seg n ) by A6,FINSEQ_1:1;
then k in ( Seg ( len R1 ) ) by CARD_1:def 7;
then k in ( dom R1 ) by FINSEQ_1:def 3;
then A8: ( ( R1 ^ <* r1 *> ) . k ) = ( R1 . k ) by FINSEQ_1:def 7;
k in ( Seg ( len R2 ) ) by A7,CARD_1:def 7;
then k in ( dom R2 ) by FINSEQ_1:def 3;
then ( ( R2 ^ <* r2 *> ) . k ) = ( R2 . k ) by FINSEQ_1:def 7;
then A9: ( ( mlt (( R1 ^ <* r1 *> ),( R2 ^ <* r2 *> )) ) . k ) = ( ( R1 . k ) * ( R2 . k ) ) by A8,RVSUM_1:59;
( len ( mlt (R1,R2) ) ) = n by CARD_1:def 7;
then k in ( dom ( mlt (R1,R2) ) ) by A7,FINSEQ_1:def 3;
then ( ( ( mlt (R1,R2) ) ^ <* ( r1 * r2 ) *> ) . k ) = ( ( mlt (R1,R2) ) . k ) by FINSEQ_1:def 7;
hence thesis by A9,RVSUM_1:59;
end;
suppose A10: k = ( n + 1 );

then k = ( ( len R1 ) + 1 ) by CARD_1:def 7;
then A11: ( ( R1 ^ <* r1 *> ) . k ) = r1 by FINSEQ_1:42;
A12: ( n + 1 ) = ( ( len ( mlt (R1,R2) ) ) + 1 ) by CARD_1:def 7;
k = ( ( len R2 ) + 1 ) by A10,CARD_1:def 7;
then ( ( R2 ^ <* r2 *> ) . k ) = r2 by FINSEQ_1:42;
then ( ( mlt (( R1 ^ <* r1 *> ),( R2 ^ <* r2 *> )) ) . k ) = ( r1 * r2 ) by A11,RVSUM_1:59;
hence thesis by A10,A12,FINSEQ_1:42;
end;
end;
hence thesis;
end;
( mlt (( R1 ^ <* r1 *> ),( R2 ^ <* r2 *> )) ) is (Element of ( ( n + 1 ) -tuples_on ( REAL ) )) by A2,FINSEQ_2:92;
hence thesis by A1,A3,FINSEQ_2:119;
end;
