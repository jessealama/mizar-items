begin
reserve U0 for Universal_Algebra;
definition
let U0;
func Operations U0 -> (PFuncsDomHQN of U0)
equals
( rng (the charact of U0) );
coherence
proof
reconsider A = ( rng (the charact of U0) ) as non empty set by RELAT_1:41;
now
let x being (Element of A);
consider i being Nat such that A1: i in ( dom (the charact of U0) ) and A2: ( (the charact of U0) . i ) = x by FINSEQ_2:10;
reconsider p = ( (the charact of U0) . i ) as (PartFunc of U0) by A1,UNIALG_1:1;
p is  homogeneous  quasi_total non empty by A1,FUNCT_1:def 9,MARGREL1:def 24;
hence x is  homogeneous  quasi_total non empty  homogeneous  quasi_total non empty (PartFunc of U0) by A2;
end;
hence thesis by MARGREL1:def 26;
end;
