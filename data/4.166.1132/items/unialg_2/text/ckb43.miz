requirements BOOLE,SUBSET;
begin
reserve x for set;
reserve y for set;
definition
let U0 being Universal_Algebra;
func Sub U0 -> set
means
:Def15: (for x holds (x in it iff x is  strict  strict (SubAlgebra of U0)));
existence
proof
reconsider X = { ( GenUnivAlg A ) where A is (Subset of U0): A is non empty } as set;
take X;
let x;
thus (x in X implies x is  strict  strict (SubAlgebra of U0))
proof
assume x in X;
then (ex A being (Subset of U0) st (x = ( GenUnivAlg A ) & A is non empty));
hence thesis;
end;

assume x is  strict  strict (SubAlgebra of U0);
then reconsider a = x as  strict (SubAlgebra of U0);
reconsider A = (the carrier of a) as non empty (Subset of U0) by CKB15:def 1;
a = ( GenUnivAlg A ) by CKB37:1;
hence thesis;
end;
uniqueness
proof
let A being set,B being set;
assume that
A1: (for x holds (x in A iff x is  strict  strict (SubAlgebra of U0)))
and
A2: (for y holds (y in B iff y is  strict  strict (SubAlgebra of U0)));
now
let x;
assume x in A;
then x is  strict  strict (SubAlgebra of U0) by A1;
hence x in B by A2;
end;
hence A c= B by TARSKI:def 3;
let y;
assume y in B;
then y is  strict  strict (SubAlgebra of U0) by A2;
hence thesis by A1;
end;
end;
