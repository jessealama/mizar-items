constructors SETFAM_1,BINOP_1,DOMAIN_1,FUNCOP_1,LATTICES,UNIALG_1,MARGREL1,FINSEQ_2,RELSET_1,CKB3,CKB7,CKB9,CKB10,CKB11,CKB12;
requirements BOOLE,SUBSET;
begin
reserve U0 for Universal_Algebra;
reserve o for (operation of U0);
theorem
Th7: (for B being non empty (Subset of U0) holds (B = (the carrier of U0) implies (B is  opers_closed & (for o holds ( o /. B ) = o))))
proof
let B being non empty (Subset of U0);
assume A1: B = (the carrier of U0);
A2: (for o holds B is_closed_on o)
proof
let o;
let s being (FinSequence of B);
assume A3: ( len s ) = ( arity o );
(( dom o ) = ( ( arity o ) -tuples_on B ) & s is (Element of ( ( len s ) -tuples_on B ))) by A1,FINSEQ_2:92,MARGREL1:22;
then A4: ( o . s ) in ( rng o ) by A3,FUNCT_1:def 3;
( rng o ) c= B by A1,RELAT_1:def 19;
hence thesis by A4;
end;
(for o holds ( o /. B ) = o)
proof
let o;
(( dom o ) = ( ( arity o ) -tuples_on B ) & ( o /. B ) = ( o | ( ( arity o ) -tuples_on B ) )) by A1,A2,CKB11:def 1,MARGREL1:22;
hence thesis by RELAT_1:68;
end;
hence thesis by A2,CKB10:def 1;
end;
