requirements BOOLE,SUBSET;
begin
definition
let U0 being Universal_Algebra;
let U1 being (SubAlgebra of U0);
let U2 being (SubAlgebra of U0);
func U1 "\/" U2 ->  strict (SubAlgebra of U0)
means
:Def14: (for A being non empty (Subset of U0) holds (A = ( (the carrier of U1) \/ (the carrier of U2) ) implies it = ( GenUnivAlg A )));
existence
proof
reconsider B = ( (the carrier of U1) \/ (the carrier of U2) ) as non empty set;
((the carrier of U1) is (Subset of U0) & (the carrier of U2) is (Subset of U0)) by CKB15:def 1;
then reconsider B as non empty (Subset of U0) by XBOOLE_1:8;
take ( GenUnivAlg B );
thus thesis;
end;
uniqueness
proof
reconsider B = ( (the carrier of U1) \/ (the carrier of U2) ) as non empty set;
let W1 being  strict (SubAlgebra of U0),W2 being  strict (SubAlgebra of U0);
assume that
A1: (for A being non empty (Subset of U0) holds (A = ( (the carrier of U1) \/ (the carrier of U2) ) implies W1 = ( GenUnivAlg A )))
and
A2: (for A being non empty (Subset of U0) holds (A = ( (the carrier of U1) \/ (the carrier of U2) ) implies W2 = ( GenUnivAlg A )));
((the carrier of U1) is (Subset of U0) & (the carrier of U2) is (Subset of U0)) by CKB15:def 1;
then reconsider B as non empty (Subset of U0) by XBOOLE_1:8;
W1 = ( GenUnivAlg B ) by A1;
hence thesis by A2;
end;
end;
