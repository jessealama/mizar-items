theorem
Th24: (for U0 being Universal_Algebra holds (for U1,U2 being (SubAlgebra of U0) holds ( U1 "\/" U2 ) = ( U2 "\/" U1 )))
proof
let U0 being Universal_Algebra;
let U1 being (SubAlgebra of U0);
let U2 being (SubAlgebra of U0);
reconsider u1 = (the carrier of U1),u2 = (the carrier of U2) as non  empty (Subset of U0) by CKB15:def 1;
reconsider A = ( u1 \/ u2 ) as non  empty (Subset of U0);
( U1 "\/" U2 ) = ( GenUnivAlg A ) by CKB38:def 1;
hence thesis by CKB38:def 1;
end;
