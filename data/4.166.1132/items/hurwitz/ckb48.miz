assume A4: m is_at_least_length_of p;
now
assume k > m;
then ( k9 + 1 ) > m;
then k9 >= m by NAT_1:13;
hence contradiction by A2,A4,ALGSEQ_1:def 2;
end;
hence k <= m;
end;
(for i being Nat holds (i >= k implies ( p . i ) = ( 0. L ))) by CKB47:def 1;
