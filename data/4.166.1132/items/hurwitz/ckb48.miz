proof
let L being  well-unital non degenerated non empty doubleLoopStr;
let z being (Element of L);
let k being (Element of ( NAT ));
set p = ( qpoly (k,z) );
A1: ( k - 1 ) < ( k - ( 0 ) ) by XREAL_1:10;
assume k >= 1;
then ( k - 1 ) >= ( 1 - 1 ) by XREAL_1:9;
