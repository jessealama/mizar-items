begin
reserve S for non  trivial RealNormSpace;
reserve T for non  trivial RealNormSpace;
reserve R for (REST of S,T);
theorem
Th7: (for R being (REST of S,T) holds (( R /. ( 0. S ) ) = ( 0. T ) implies (for e being Real holds (e > ( 0 ) implies (ex d being Real st (d > ( 0 ) & (for h being (Point of S) holds (||. h .|| < d implies ||. ( R /. h ) .|| <= ( e * ||. h .|| )))))))))
proof
let R being (REST of S,T);
assume that
A1: ( R /. ( 0. S ) ) = ( 0. T );
let e being Real;
assume that
A2: e > ( 0 );
R is  total by NDIFF_1:def 5;
then consider d being Real such that A3: d > ( 0 ) and A4: (for z being (Point of S) holds ((z <> ( 0. S ) & ||. z .|| < d) implies ( ( ||. z .|| " ) * ||. ( R /. z ) .|| ) < e)) by A2,NDIFF_1:23;
take d;
now
let h being (Point of S);
assume that
A5: ||. h .|| < d;
now
per cases ;
case A6: h <> ( 0. S );
then (( 0 ) <= ||. h .|| & ( ( ||. h .|| " ) * ||. ( R /. h ) .|| ) <= e) by A4,A5,NORMSP_1:4;
then ( ||. h .|| * ( ( ||. h .|| " ) * ||. ( R /. h ) .|| ) ) <= ( ||. h .|| * e ) by XREAL_1:64;
then A7: ( ( ||. h .|| * ( ||. h .|| " ) ) * ||. ( R /. h ) .|| ) <= ( e * ||. h .|| );
||. h .|| <> ( 0 ) by A6,NORMSP_0:def 5;
then ( 1 * ||. ( R /. h ) .|| ) <= ( e * ||. h .|| ) by A7,XCMPLX_0:def 7;
hence ||. ( R /. h ) .|| <= ( e * ||. h .|| );
end;
case A8: h = ( 0. S );
( 0 ) <= ||. h .|| by NORMSP_1:4;
then ( ( 0 ) * ||. h .|| ) <= ( e * ||. h .|| ) by A2;
hence ||. ( R /. h ) .|| <= ( e * ||. h .|| ) by A1,A8,NORMSP_0:def 6;
end;
end;
hence ||. ( R /. h ) .|| <= ( e * ||. h .|| );
end;
hence thesis by A3;
end;
