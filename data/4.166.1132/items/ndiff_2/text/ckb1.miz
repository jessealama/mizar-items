requirements SUBSET,REAL,BOOLE,NUMERALS,ARITHM;
begin
reserve S for non  trivial RealNormSpace;
reserve T for non  trivial RealNormSpace;
reserve f for (PartFunc of S,T);
definition
let X being RealNormSpace;
let Y being RealNormSpace;
let Z being RealNormSpace;
let f being (Element of ( BoundedLinearOperators (X,Y) ));
let g being (Element of ( BoundedLinearOperators (Y,Z) ));
func g * f -> (Element of ( BoundedLinearOperators (X,Z) )) equals 
( ( modetrans (g,Y,Z) ) * ( modetrans (f,X,Y) ) );
correctness
proof
( ( modetrans (g,Y,Z) ) * ( modetrans (f,X,Y) ) ) is  bounded  bounded (LinearOperator of X,Z) by LOPBAN_2:2;
hence thesis by LOPBAN_1:def 9;
end;
end;
