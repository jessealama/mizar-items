environ
vocabularies NUMBERS,XBOOLE_0,MSUALG_1,SUBSET_1,FUNCT_1,PBOOLE,MEMBER_1,RELAT_1,STRUCT_0,CARD_3,MARGREL1,PARTFUN1,MOD_4,MSUALG_3,NAT_1,FUNCT_4,RLTOPSP1,TARSKI,REWRITE1,FUNCOP_1,FINSEQ_1,ARYTM_3,FUNCT_7,CARD_1,XXREAL_0,ORDINAL4,MSUALG_4,CIRCUIT2,MCART_1,ZFMISC_1,EQREL_1,RELAT_2,MSUALG_5,MSUALG_6;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,NUMBERS,NAT_1,MCART_1,STRUCT_0,RELAT_1,RELAT_2,RELSET_1,EQREL_1,REWRITE1,FUNCT_1,PBOOLE,PARTFUN1,FUNCT_2,FINSEQ_1,CARD_3,FUNCOP_1,MSUALG_1,MSUALG_3,MSUALG_4,MSUALG_5,FUNCT_7,XXREAL_0,CKB1,CKB2,CKB5,CKB9,CKB13,CKB14;
definitions TARSKI,RELAT_1,REWRITE1,FUNCT_1,PBOOLE,MSUALG_3,MSUALG_4,FUNCT_7,FUNCOP_1,CKB5,CKB9,CKB14;
theorems TARSKI,NAT_1,MCART_1,ZFMISC_1,RELAT_1,RELSET_1,EQREL_1,FUNCT_1,FINSEQ_1,FINSEQ_3,FUNCT_2,CARD_3,FUNCT_7,REWRITE1,PBOOLE,PRALG_2,MSUALG_3,MSUALG_4,MSUALG_5,FINSEQ_5,XBOOLE_0,PARTFUN1,RELAT_2,ORDERS_1,XREAL_1,XXREAL_0,ORDINAL1,CKB3,CKB4,CKB5,CKB6,CKB9,CKB10,CKB11,CKB12,CKB14,CKB15,CKB16;
schemes NAT_1,RECDEF_1,RELSET_1,FUNCT_1,FINSEQ_1,CARD_3,CLASSES1,PBOOLE,PUA2MSS1;
registrations XBOOLE_0,RELAT_1,FUNCT_1,PARTFUN1,FUNCT_2,FUNCOP_1,XXREAL_0,XREAL_0,NAT_1,FINSEQ_1,PBOOLE,REWRITE1,FUNCT_7,STRUCT_0,MSUALG_1,MSUALG_3,MSUALG_4,MSUALG_5,ORDINAL1,CARD_1,RELSET_1,CKB7,CKB8;
constructors XXREAL_0,NAT_1,NAT_D,REWRITE1,FUNCT_7,MSUALG_3,MSUALG_5,RELSET_1,CKB2,CKB5,CKB9,CKB13,CKB14;
requirements NUMERALS,REAL,BOOLE,SUBSET,ARITHM;
begin
reserve S for non  empty non  void ManySortedSign;
theorem
Th9: (for S being non  empty non  void ManySortedSign holds (for o being (OperSymbol of S) holds (for i being (Element of ( NAT )) holds (i in ( dom ( the_arity_of o ) ) implies (for A1,A2 being MSAlgebra over S holds (for h being (ManySortedFunction of A1,A2) holds (for a,b being (Element of ( Args (o,A1) )) holds ((a in ( Args (o,A1) ) & ( h # a ) in ( Args (o,A2) )) implies (for f,g1,g2 being Function holds (((f = a & g1 = ( h # a )) & g2 = ( h # b )) implies (for x being (Element of A1,( ( the_arity_of o ) /. i )) holds (b = ( f +* (i,x) ) implies (( g2 . i ) = ( ( h . ( ( the_arity_of o ) /. i ) ) . x ) & ( h # b ) = ( g1 +* (i,( g2 . i )) ))))))))))))))
proof
let S being non  empty non  void ManySortedSign;
let o being (OperSymbol of S);
let i being (Element of ( NAT ));
assume that
A1: i in ( dom ( the_arity_of o ) );
let A1 being MSAlgebra over S;
let A2 being MSAlgebra over S;
let h being (ManySortedFunction of A1,A2);
let a being (Element of ( Args (o,A1) ));
let b being (Element of ( Args (o,A1) ));
assume that
A2: a in ( Args (o,A1) )
and
A3: ( h # a ) in ( Args (o,A2) );
reconsider f2 = b as Function by A2,CKB3:1;
A4: ( dom f2 ) = ( dom ( the_arity_of o ) ) by A2,CKB4:1;
let f being Function;
let g1 being Function;
let g2 being Function;
assume that
A5: f = a
and
A6: g1 = ( h # a )
and
A7: g2 = ( h # b );
reconsider g3 = ( g1 +* (i,( g2 . i )) ) as Function;
A8: ( dom f ) = ( dom ( the_arity_of o ) ) by A2,A5,CKB4:1;
let x being (Element of A1,( ( the_arity_of o ) /. i ));
assume that
A9: b = ( f +* (i,x) );
( f2 . i ) = x by A1,A9,A8,FUNCT_7:31;
hence ( g2 . i ) = ( ( h . ( ( the_arity_of o ) /. i ) ) . x ) by A1,A2,A3,A7,A4,MSUALG_3:24;
then ( g2 . i ) in ( (the Sorts of A2) . ( ( the_arity_of o ) /. i ) ) by A1,A2,A3,CKB16:1;
then ( g1 +* (i,( g2 . i )) ) in ( Args (o,A2) ) by A3,A6,CKB15:1;
then A10: ( dom g3 ) = ( dom ( the_arity_of o ) ) by CKB4:1;
A11:now
let z being set;
assume that
A12: z in ( dom ( the_arity_of o ) )
and
A13: z <> i;
reconsider j = z as (Element of ( NAT )) by A12;
A14: ( f2 . j ) = ( f . j ) by A9,A13,FUNCT_7:32;
( g2 . j ) = ( ( h . ( ( the_arity_of o ) /. j ) ) . ( f2 . j ) ) by A2,A3,A7,A4,A12,MSUALG_3:24;
hence ( g2 . z ) = ( g1 . z ) by A2,A3,A5,A6,A8,A12,A14,MSUALG_3:24;
end;
A15: ( dom g1 ) = ( dom ( the_arity_of o ) ) by A3,A6,CKB4:1;
A16:now
let z being set;
assume A17: z in ( dom ( the_arity_of o ) );
per cases ;
suppose z = i;

hence ( g2 . z ) = ( ( g1 +* (i,( g2 . i )) ) . z ) by A1,A15,FUNCT_7:31;
end;
suppose A18: z <> i;

then ( g2 . z ) = ( g1 . z ) by A11,A17;
hence ( g2 . z ) = ( ( g1 +* (i,( g2 . i )) ) . z ) by A18,FUNCT_7:32;
end;
end;
( dom g2 ) = ( dom ( the_arity_of o ) ) by A3,A7,CKB4:1;
hence thesis by A7,A10,A16,FUNCT_1:2;
end;
