begin
reserve S for non  empty non  void ManySortedSign;
reserve A for MSAlgebra over S;
theorem
(for S being non  empty non  void ManySortedSign holds (for A being  non-empty MSAlgebra over S holds (for s1,s2 being (SortSymbol of S) holds (( TranslationRel S ) reduces s1,s2 implies (for q being (RedSequence of ( TranslationRel S )) holds (for p being  Function-yielding FinSequence holds ((((( len q ) = ( ( len p ) + 1 ) & s1 = ( q . 1 )) & s2 = ( q . ( len q ) )) & (for i being (Element of ( NAT )) holds (for f being Function holds (for s1,s2 being (SortSymbol of S) holds ((((i in ( dom p ) & f = ( p . i )) & s1 = ( q . i )) & s2 = ( q . ( i + 1 ) )) implies f is_e.translation_of A,s1,s2))))) implies ( compose (p,( (the Sorts of A) . s1 )) ) is (Translation of A,s1,s2))))))))
proof
let S being non  empty non  void ManySortedSign;
let A being  non-empty MSAlgebra over S;
let s1 being (SortSymbol of S);
let s2 being (SortSymbol of S);
assume that
A1: ( TranslationRel S ) reduces s1,s2;
let q being (RedSequence of ( TranslationRel S ));
let p being  Function-yielding FinSequence;
assume that
A2: ( len q ) = ( ( len p ) + 1 )
and
A3: s1 = ( q . 1 )
and
A4: s2 = ( q . ( len q ) )
and
A5: (for i being (Element of ( NAT )) holds (for f being Function holds (for s1,s2 being (SortSymbol of S) holds ((((i in ( dom p ) & f = ( p . i )) & s1 = ( q . i )) & s2 = ( q . ( i + 1 ) )) implies f is_e.translation_of A,s1,s2))));
( compose (p,( (the Sorts of A) . s1 )) ) is (Function of ( (the Sorts of A) . s1 ),( (the Sorts of A) . s2 )) by A2,A3,A4,A5,CKB24:1;
hence thesis by A1,A2,A3,A4,A5,CKB25:def 1;
end;
