theorem
Th42: (for S being non  empty set holds (for A being  non-empty (ManySortedSet of S) holds (for R being (ManySortedRelation of A) holds (for s being (Element of S) holds (for a,b being (Element of ( A . s )) holds ([ a,b ] in ( ( EqCl R ) . s ) iff a,b are_convertible_wrt ( R . s )))))))
proof
let S being non  empty set;
let A being  non-empty (ManySortedSet of S);
let R being (ManySortedRelation of A);
let s being (Element of S);
( ( EqCl R ) . s ) = ( EqCl ( R . s ) ) by MSUALG_5:def 3;
hence thesis by CKB71:1;
end;
