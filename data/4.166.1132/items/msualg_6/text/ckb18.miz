begin
reserve S for non  empty non  void ManySortedSign;
reserve A for MSAlgebra over S;
definition
let S being non  empty non  void ManySortedSign;
let o being (OperSymbol of S);
let i being (Element of ( NAT ));
let A being MSAlgebra over S;
let a being Function;
func transl (o,i,a,A) -> Function means 
:Def4: (( dom it ) = ( (the Sorts of A) . ( ( the_arity_of o ) /. i ) ) & (for x being set holds (x in ( (the Sorts of A) . ( ( the_arity_of o ) /. i ) ) implies ( it . x ) = ( ( Den (o,A) ) . ( a +* (i,x) ) ))));
existence
proof
deffunc F(set) = ( ( Den (o,A) ) . ( a +* (i,$1) ) );
(ex f being Function st (( dom f ) = ( (the Sorts of A) . ( ( the_arity_of o ) /. i ) ) & (for x being set holds (x in ( (the Sorts of A) . ( ( the_arity_of o ) /. i ) ) implies ( f . x ) = F(x))))) from FUNCT_1:sch 3;
hence thesis;
end;
uniqueness
proof
let f1 being Function;
let f2 being Function;
assume that
A1: ( dom f1 ) = ( (the Sorts of A) . ( ( the_arity_of o ) /. i ) )
and
A2: (for x being set holds (x in ( (the Sorts of A) . ( ( the_arity_of o ) /. i ) ) implies ( f1 . x ) = ( ( Den (o,A) ) . ( a +* (i,x) ) )))
and
A3: ( dom f2 ) = ( (the Sorts of A) . ( ( the_arity_of o ) /. i ) )
and
A4: (for x being set holds (x in ( (the Sorts of A) . ( ( the_arity_of o ) /. i ) ) implies ( f2 . x ) = ( ( Den (o,A) ) . ( a +* (i,x) ) )));
now
let x being set;
assume A5: x in ( (the Sorts of A) . ( ( the_arity_of o ) /. i ) );
then ( f1 . x ) = ( ( Den (o,A) ) . ( a +* (i,x) ) ) by A2;
hence ( f1 . x ) = ( f2 . x ) by A4,A5;
end;
hence thesis by A1,A3,FUNCT_1:2;
end;
end;
