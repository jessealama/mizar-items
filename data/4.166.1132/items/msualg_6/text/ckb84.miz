begin
reserve S for non  empty non  void ManySortedSign;
reserve A for  non-empty MSAlgebra over S;
reserve R for (ManySortedRelation of (the Sorts of A));
theorem
Th50: (for R,E being (ManySortedRelation of A) holds ((for s being (SortSymbol of S) holds (for a,b being (Element of A,s) holds ([ a,b ] in ( E . s ) iff a,b are_convertible_wrt ( ( TRS R ) . s )))) implies E is (EquationalTheory of A)))
proof
let R being (ManySortedRelation of A);
let E being (ManySortedRelation of A);
assume A1: (for s being (SortSymbol of S) holds (for a,b being (Element of A,s) holds ([ a,b ] in ( E . s ) iff a,b are_convertible_wrt ( ( TRS R ) . s ))));
A2: E is  stable
proof
let h being (Endomorphism of A);
let s being (SortSymbol of S);
let a being set;
let b being set;
assume A3: [ a,b ] in ( E . s );
then reconsider x = a,y = b as (Element of A,s) by ZFMISC_1:87;
reconsider a9 = ( ( h . s ) . x ),b9 = ( ( h . s ) . y ) as (Element of A,s);
x,y are_convertible_wrt ( ( TRS R ) . s ) by A1,A3;
then a9,b9 are_convertible_wrt ( ( TRS R ) . s ) by CKB77:1;
hence thesis by A1;
end;
A4: E is  invariant
proof
let s1 being (SortSymbol of S);
let s2 being (SortSymbol of S);
let t being Function;
assume A5: t is_e.translation_of A,s1,s2;
then reconsider f = t as (Function of ( (the Sorts of A) . s1 ),( (the Sorts of A) . s2 )) by CKB21:1;
let a being set;
let b being set;
assume A6: [ a,b ] in ( E . s1 );
then reconsider x = a,y = b as (Element of A,s1) by ZFMISC_1:87;
x,y are_convertible_wrt ( ( TRS R ) . s1 ) by A1,A6;
then A7: ( t . x ),( t . y ) are_convertible_wrt ( ( TRS R ) . s2 ) by A5,CKB80:1;
A8: ( t . y ) = ( f . y );
( t . x ) = ( f . x );
hence thesis by A1,A8,A7;
end;
E is  MSEquivalence_Relation-like by A1,CKB83:1;
hence thesis by A2,A4,MSUALG_4:def 3;
end;
