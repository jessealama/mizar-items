theorem
(for L being non empty RelStr holds ( lim_inf-Convergence L ) is  (SUBNETS))
proof
let L being non empty RelStr;
let N being (net of L);
let M being (subnet of N);
assume A1: M in ( NetUniv L );
let x being (Element of L);
assume A2: [ N,x ] in ( lim_inf-Convergence L );
( lim_inf-Convergence L ) c= [: ( NetUniv L ),(the carrier of L) :] by YELLOW_6:def 18;
then A3: N in ( NetUniv L ) by A2,ZFMISC_1:87;
(for M1 being (subnet of M) holds x = ( lim_inf M1 ))
proof
let M1 being (subnet of M);
reconsider M19 = M1 as (subnet of N) by YELLOW_6:15;
x = ( lim_inf M19 ) by A2,A3,CKB22:def 1;
hence thesis;
end;
hence thesis by A1,CKB22:def 1;
end;
