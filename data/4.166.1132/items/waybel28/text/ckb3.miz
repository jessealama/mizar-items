begin
theorem
Th3: (for L being  complete LATTICE holds (for N being (net of L) holds (for x being (Element of L) holds (N in ( NetUniv L ) implies ((for M being (subnet of N) holds (M in ( NetUniv L ) implies x = ( lim_inf M ))) implies (x = ( lim_inf N ) & (for M being (subnet of N) holds (M in ( NetUniv L ) implies x >= ( inf M )))))))))
proof
let L being  complete LATTICE;
let N being (net of L);
let x being (Element of L);
assume A1: N in ( NetUniv L );
assume A2: (for M being (subnet of N) holds (M in ( NetUniv L ) implies x = ( lim_inf M )));
N is (subnet of N) by YELLOW_6:14;
hence x = ( lim_inf N ) by A1,A2;
let M being (subnet of N);
assume M in ( NetUniv L );
then x = ( lim_inf M ) by A2;
hence thesis by CKB1:1;
end;
