begin
theorem
(for L being  complete LATTICE holds (for N being (net of L) holds (for x being (Element of L) holds (N in ( NetUniv L ) implies ([ N,x ] in ( lim_inf-Convergence L ) iff (for M being (subnet of N) holds (M in ( NetUniv L ) implies x = ( lim_inf M ))))))))
proof
let L being  complete LATTICE;
let N being (net of L);
let x being (Element of L);
assume A1: N in ( NetUniv L );
hence ([ N,x ] in ( lim_inf-Convergence L ) implies (for M being (subnet of N) holds (M in ( NetUniv L ) implies x = ( lim_inf M )))) by CKB22:def 1;
assume A2: (for M being (subnet of N) holds (M in ( NetUniv L ) implies x = ( lim_inf M )));
then (for M being (subnet of N) holds (M in ( NetUniv L ) implies x >= ( inf M ))) by A1,CKB3:1;
then A3: (for p being  greater_or_equal_to_id (Function of N,N) holds x >= ( inf ( N * p ) )) by A1,CKB16:1;
x = ( lim_inf N ) by A1,A2,CKB3:1;
then (for M being (subnet of N) holds x = ( lim_inf M )) by A3,CKB21:1;
hence thesis by A1,CKB22:def 1;
end;
