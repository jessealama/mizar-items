requirements NUMERALS,REAL,SUBSET,BOOLE,ARITHM;
begin
reserve R for  good Ring;
reserve r for (Element of R);
reserve a for (Data-Location of R);
reserve c for (Data-Location of R);
reserve s for (State of ( SCM R ));
theorem
Th19: ((( ( Exec (( a := r ),s) ) . ( IC ( SCM R ) ) ) = ( succ ( IC s ) ) & ( ( Exec (( a := r ),s) ) . a ) = r) & (for c holds (c <> a implies ( ( Exec (( a := r ),s) ) . c ) = ( s . c ))))
proof
A1: a is (Element of ( Data-Locations ( SCM ) )) by CKB5:1;
A2: (the Object-Kind of ( SCM R )) = ( SCM-OK R ) by CKB2:def 1;
reconsider S = s as (SCM-State of R) by A2,CARD_3:107;
reconsider I = ( a := r ) as (Element of ( SCM-Instr R )) by CKB2:def 1;
set S1 = ( SCM-Chg (S,( I const_address ),( I const_value )) );
reconsider i = 5 as (Element of ( Segm 8 )) by NAT_1:44;
A3: ( IC s ) = ( IC S ) by CKB2:def 1;
A4: I = [ i,( {} ),<* a,r *> ];
then A5: ( I const_address ) = a by A1,AMI_3:27,SCMRING1:16;
A6: ( I const_value ) = r by A4,A1,AMI_3:27,SCMRING1:16;
A7: ( Exec (( a := r ),s) ) = ( SCM-Exec-Res (I,S) ) by CKB23:1
.= ( SCM-Chg (S1,( succ ( IC S ) )) ) by A1,AMI_3:27,SCMRING1:def 14;
thus ( ( Exec (( a := r ),s) ) . ( IC ( SCM R ) ) ) = ( ( Exec (( a := r ),s) ) . ( NAT ) ) by CKB2:def 1
.= ( succ ( IC s ) ) by A3,A7,SCMRING1:7;
thus ( ( Exec (( a := r ),s) ) . a ) = ( S1 . a ) by A1,A7,AMI_3:27,SCMRING1:8
.= r by A5,A6,SCMRING1:11;
let c;
assume A8: c <> a;
A9: c is (Element of ( Data-Locations ( SCM ) )) by CKB5:1;
hence ( ( Exec (( a := r ),s) ) . c ) = ( S1 . c ) by A7,AMI_3:27,SCMRING1:8
.= ( s . c ) by A5,A8,A9,AMI_3:27,SCMRING1:12;
end;
