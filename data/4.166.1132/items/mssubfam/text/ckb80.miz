constructors SETFAM_1,FUNCT_4,FUNCT_6,MBOOLEAN,PZFMISC1,PARTFUN1,RELSET_1,ORDINAL1,PBOOLE,FINSET_1,CKB52,CKB56,CKB66,CKB67;
requirements SUBSET,BOOLE;
begin
reserve I for set;
reserve i for set;
theorem
(for A,M being (ManySortedSet of I) holds (for SF being  non-empty (MSSubsetFamily of M) holds ((A in M & (for B being (ManySortedSet of I) holds (B in SF implies A in B))) implies A in ( meet SF ))))
proof
let A being (ManySortedSet of I),M being (ManySortedSet of I);
let SF being  non-empty (MSSubsetFamily of M);
assume that
A1: A in M
and
A2: (for B being (ManySortedSet of I) holds (B in SF implies A in B));
let i;
consider T being (ManySortedSet of I) such that A3: T in SF by PBOOLE:134;
assume A4: i in I;
then consider Q being (Subset-Family of ( M . i )) such that A5: Q = ( SF . i ) and A6: ( ( meet SF ) . i ) = ( Intersect Q ) by CKB66:def 1;
A7: (for B9 being set holds (B9 in Q implies ( A . i ) in B9))
proof
let B9 being set;
assume that
A8: B9 in Q;
( dom ( T +* ( i .--> B9 ) ) ) = I by A4,PZFMISC1:1;
then reconsider K = ( T +* ( i .--> B9 ) ) as (ManySortedSet of I) by PARTFUN1:def 2,RELAT_1:def 18;
A9: ( dom ( i .--> B9 ) ) = { i } by FUNCOP_1:13;
i in { i } by TARSKI:def 1;
then A10: ( K . i ) = ( ( i .--> B9 ) . i ) by A9,FUNCT_4:13
.= B9 by FUNCOP_1:72;
K in SF
proof
let q being set;
assume that
A11: q in I;
per cases ;
suppose q = i;

hence thesis by A5,A8,A10;
end;
suppose q <> i;

then (not q in ( dom ( i .--> B9 ) )) by A9,TARSKI:def 1;
then ( K . q ) = ( T . q ) by FUNCT_4:11;
hence thesis by A3,A11,PBOOLE:def 1;
end;
end;
then A in K by A2;
hence thesis by A4,A10,PBOOLE:def 1;
end;
( A . i ) in ( M . i ) by A1,A4,PBOOLE:def 1;
hence thesis by A6,A7,SETFAM_1:43;
end;
