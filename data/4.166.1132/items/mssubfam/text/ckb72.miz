environ
vocabularies PBOOLE,FUNCT_6,RELAT_1,PARTFUN1,FUNCT_1,SETFAM_1,SUBSET_1,FINSET_1,XBOOLE_0,TARSKI,MEMBER_1,PZFMISC1,ZFMISC_1,FUNCT_4,FUNCOP_1,FUNCT_2,ORDINAL1,MSSUBFAM;
