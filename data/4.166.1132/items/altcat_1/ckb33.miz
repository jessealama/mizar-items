constructors PARTFUN1,BINOP_1,MULTOP_1,PBOOLE,REALSET2,RELSET_1,CKB15,CKB17,CKB19,CKB20,CKB21,CKB23,CKB24,CKB25,CKB26,CKB27,CKB29,CKB30;
requirements BOOLE,SUBSET,NUMERALS;
begin
reserve i for set;
reserve j for set;
definition
let A being  functional set;
let B being  functional set;
func FuncComp (A,B) ->  compositional (ManySortedFunction of [: B,A :])
means
:Def12: (not contradiction);
uniqueness
proof
let M1 being  compositional (ManySortedFunction of [: B,A :]),M2 being  compositional (ManySortedFunction of [: B,A :]);
now
let i;
let j;
assume (i in B & j in A);
then A1: [ i,j ] in [: B,A :] by ZFMISC_1:87;
then [ i,j ] in ( dom M1 ) by PARTFUN1:def 2;
then consider f1 being Function,g1 being Function such that A2: [ i,j ] = [ g1,f1 ] and A3: ( M1 . [ i,j ] ) = ( g1 * f1 ) by CKB30:def 1;
[ i,j ] in ( dom M2 ) by A1,PARTFUN1:def 2;
then consider f2 being Function,g2 being Function such that A4: [ i,j ] = [ g2,f2 ] and A5: ( M2 . [ i,j ] ) = ( g2 * f2 ) by CKB30:def 1;
g1 = g2 by A2,A4,ZFMISC_1:27;
hence ( M1 . (i,j) ) = ( M2 . (i,j) ) by A2,A3,A4,A5,ZFMISC_1:27;
end;
hence M1 = M2 by CKB10:1;
end;
correctness;
end;
