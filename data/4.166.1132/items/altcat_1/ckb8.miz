A3: (( [ [ i,j ],k ] `2 ) = k & [ i,j,k ] = [ [ i,j ],k ]) by MCART_1:7,MCART_1:def 3;
( [ i,j ] `2 ) = j by MCART_1:7;
then A4: ( ( [ [ i,j ],k ] `1 ) `2 ) = j by MCART_1:7;
( [ i,j ] `1 ) = i by MCART_1:7;
then A5: ( ( [ [ i,j ],k ] `1 ) `1 ) = i by MCART_1:7;
assume ((i in A() & j in B()) & k in C());
