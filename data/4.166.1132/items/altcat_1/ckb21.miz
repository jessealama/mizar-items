begin
reserve i for set;
reserve j for set;
reserve k for set;
definition
let I being set;
let G being (ManySortedSet of [: I,I :]);
let H being (ManySortedSet of [: I,I :]);
func {|G,H |} -> (ManySortedSet of [: I,I,I :])
means
:Def6: (for i holds (for j holds (for k holds (((i in I & j in I) & k in I) implies ( it . (i,j,k) ) = [: ( H . (j,k) ),( G . (i,j) ) :]))));
existence
proof
deffunc F(set,set,set) = [: ( H . ($2,$3) ),( G . ($1,$2) ) :];
(ex M being (ManySortedSet of [: I,I,I :]) st (for i holds (for j holds (for k holds (((i in I & j in I) & k in I) implies ( M . (i,j,k) ) = F(i,j,k)))))) from CKB8:sch 1;
hence thesis;
end;
uniqueness
proof
let M1 being (ManySortedSet of [: I,I,I :]),M2 being (ManySortedSet of [: I,I,I :]);
assume that
A4: (for i holds (for j holds (for k holds (((i in I & j in I) & k in I) implies ( M1 . (i,j,k) ) = [: ( H . (j,k) ),( G . (i,j) ) :]))))
and
A5: (for i holds (for j holds (for k holds (((i in I & j in I) & k in I) implies ( M2 . (i,j,k) ) = [: ( H . (j,k) ),( G . (i,j) ) :]))));
now
let i;
let j;
let k;
assume A6: ((i in I & j in I) & k in I);
hence ( M1 . (i,j,k) ) = [: ( H . (j,k) ),( G . (i,j) ) :] by A4
.= ( M2 . (i,j,k) ) by A5,A6;
end;
hence M1 = M2 by CKB12:1;
end;
