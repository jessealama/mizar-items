begin
theorem
Th13: (for A,B being  functional set holds (for F being  compositional (ManySortedSet of [: A,B :]) holds (for g,f being Function holds ((g in A & f in B) implies ( F . (g,f) ) = ( g * f )))))
proof
let A being  functional set,B being  functional set;
let F being  compositional (ManySortedSet of [: A,B :]);
let g being Function,f being Function;
assume that
A1: (g in A & f in B);
( dom F ) = [: A,B :] by PARTFUN1:def 2;
then [ g,f ] in ( dom F ) by A1,ZFMISC_1:87;
then consider ff being Function,gg being Function such that A2: [ g,f ] = [ gg,ff ] and A3: ( F . [ g,f ] ) = ( gg * ff ) by CKB30:def 1;
g = gg by A2,ZFMISC_1:27;
hence thesis by A2,A3,ZFMISC_1:27;
end;
