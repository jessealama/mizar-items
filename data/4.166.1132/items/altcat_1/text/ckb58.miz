begin
theorem
(for C being  associative  transitive non  empty AltCatStr holds (for o1,o2,o3,o4 being (object of C) holds (((<^ o1,o2 ^> <> ( {} ) & <^ o2,o3 ^> <> ( {} )) & <^ o3,o4 ^> <> ( {} )) implies (for a being (Morphism of o1,o2) holds (for b being (Morphism of o2,o3) holds (for c being (Morphism of o3,o4) holds ( c * ( b * a ) ) = ( ( c * b ) * a )))))))
proof
let C being  associative  transitive non  empty AltCatStr;
let o1 being (object of C);
let o2 being (object of C);
let o3 being (object of C);
let o4 being (object of C);
assume that
A1: <^ o1,o2 ^> <> ( {} )
and
A2: <^ o2,o3 ^> <> ( {} )
and
A3: <^ o3,o4 ^> <> ( {} );
let a being (Morphism of o1,o2);
let b being (Morphism of o2,o3);
let c being (Morphism of o3,o4);
A4: (<^ o2,o4 ^> <> ( {} ) & ( c * b ) = ( ( (the Comp of C) . (o2,o3,o4) ) . (c,b) )) by A2,A3,CKB19:def 1,CKB29:def 1;
A5: (the Comp of C) is  associative by CKB45:def 1;
(<^ o1,o3 ^> <> ( {} ) & ( b * a ) = ( ( (the Comp of C) . (o1,o2,o3) ) . (b,a) )) by A1,A2,CKB19:def 1,CKB29:def 1;
hence ( c * ( b * a ) ) = ( ( (the Comp of C) . (o1,o3,o4) ) . (c,( ( (the Comp of C) . (o1,o2,o3) ) . (b,a) )) ) by A3,CKB29:def 1
.= ( ( (the Comp of C) . (o1,o2,o4) ) . (( ( (the Comp of C) . (o2,o3,o4) ) . (c,b) ),a) ) by A1,A2,A3,A5,CKB24:def 1
.= ( ( c * b ) * a ) by A1,A4,CKB29:def 1;
end;
