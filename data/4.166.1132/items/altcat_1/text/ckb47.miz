notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,MCART_1,DOMAIN_1,RELAT_1,CARD_1,NUMBERS,STRUCT_0,FUNCT_1,FUNCT_2,BINOP_1,MULTOP_1,FUNCOP_1,PBOOLE,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB29,CKB30,CKB33,CKB37,CKB38,CKB39,CKB44,CKB45,CKB46;
definitions TARSKI,STRUCT_0,XBOOLE_0,FUNCOP_1,BINOP_1,CKB17,CKB19,CKB20,CKB21,CKB24,CKB25,CKB26,CKB29,CKB30,CKB33,CKB37,CKB38,CKB39,CKB44,CKB45,CKB46;
theorems FUNCT_1,ZFMISC_1,PBOOLE,DOMAIN_1,MULTOP_1,MCART_1,FUNCT_2,TARSKI,FUNCOP_1,RELAT_1,REALSET1,STRUCT_0,RELSET_1,XBOOLE_0,XBOOLE_1,CARD_1,PARTFUN1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB10,CKB11,CKB12,CKB13,CKB14,CKB17,CKB19,CKB20,CKB21,CKB24,CKB25,CKB26,CKB29,CKB30,CKB32,CKB33,CKB34,CKB35,CKB36,CKB37,CKB38,CKB39,CKB42,CKB43,CKB44,CKB45,CKB46;
schemes FUNCT_1,CKB6,CKB7,CKB8,CKB9;
registrations XBOOLE_0,SUBSET_1,RELAT_1,FUNCT_1,FUNCT_2,FUNCOP_1,STRUCT_0,RELSET_1,ZFMISC_1,CARD_1,ORDINAL1,CKB15,CKB27,CKB28,CKB31,CKB40,CKB41;
constructors PARTFUN1,BINOP_1,MULTOP_1,PBOOLE,REALSET2,RELSET_1,CKB15,CKB17,CKB19,CKB20,CKB21,CKB23,CKB24,CKB25,CKB26,CKB27,CKB29,CKB30,CKB33,CKB37,CKB38,CKB39,CKB44,CKB45,CKB46;
requirements BOOLE,SUBSET,NUMERALS;
begin
reserve i for set;
reserve j for set;
reserve k for set;
theorem
Lm1: (for A being non  empty set holds ( EnsCat A ) is  transitive  associative  with_units)
proof
let A being non  empty set;
set G = (the Arrows of ( EnsCat A ));
set C = (the Comp of ( EnsCat A ));
thus A1: ( EnsCat A ) is  transitive
proof
let o1 being (object of ( EnsCat A ));
let o2 being (object of ( EnsCat A ));
let o3 being (object of ( EnsCat A ));
assume (<^ o1,o2 ^> <> ( {} ) & <^ o2,o3 ^> <> ( {} ));
then (( Funcs (o1,o2) ) <> ( {} ) & ( Funcs (o2,o3) ) <> ( {} )) by CKB44:def 1;
then ( Funcs (o1,o3) ) <> ( {} ) by CKB4:1;
hence thesis by CKB44:def 1;
end;

thus ( EnsCat A ) is  associative
proof
let i being (Element of ( EnsCat A ));
let j being (Element of ( EnsCat A ));
let k being (Element of ( EnsCat A ));
let l being (Element of ( EnsCat A ));
reconsider i9 = i,j9 = j,k9 = k,l9 = l as (object of ( EnsCat A ));
let f being set;
let g being set;
let h being set;
assume that
A2: f in ( G . (i,j) )
and
A3: g in ( G . (j,k) )
and
A4: h in ( G . (k,l) );
reconsider h99 = h as (Morphism of k9,l9) by A4;
reconsider g99 = g as (Morphism of j9,k9) by A3;
A5: <^ k9,l9 ^> = ( Funcs (k,l) ) by CKB44:def 1;
(<^ i9,j9 ^> = ( Funcs (i,j) ) & <^ j9,k9 ^> = ( Funcs (j,k) )) by CKB44:def 1;
then reconsider f9 = f,g9 = g,h9 = h as Function by A2,A3,A4,A5;
A6: ( G . (k,l) ) = <^ k9,l9 ^>;
A7: <^ j9,k9 ^> <> ( {} ) by A3;
then A8: <^ j9,l9 ^> <> ( {} ) by A1,A4,A6,CKB19:def 1;
then A9: ( h99 * g99 ) = ( h9 * g9 ) by A3,A4,CKB43:1;
reconsider f99 = f as (Morphism of i9,j9) by A2;
( G . (i,j) ) = <^ i9,j9 ^>;
then A10: <^ i9,k9 ^> <> ( {} ) by A1,A2,A7,CKB19:def 1;
then A11: ( g99 * f99 ) = ( g9 * f9 ) by A2,A3,CKB43:1;
A12: <^ i9,l9 ^> <> ( {} ) by A1,A4,A6,A10,CKB19:def 1;
A13: ( ( C . (j,k,l) ) . (h,g) ) = ( h99 * g99 ) by A3,A4,CKB29:def 1;
( ( C . (i,j,k) ) . (g,f) ) = ( g99 * f99 ) by A2,A3,CKB29:def 1;
hence ( ( C . (i,k,l) ) . (h,( ( C . (i,j,k) ) . (g,f) )) ) = ( h99 * ( g99 * f99 ) ) by A4,A10,CKB29:def 1
.= ( h9 * ( g9 * f9 ) ) by A4,A10,A12,A11,CKB43:1
.= ( ( h9 * g9 ) * f9 ) by RELAT_1:36
.= ( ( h99 * g99 ) * f99 ) by A2,A8,A12,A9,CKB43:1
.= ( ( C . (i,j,l) ) . (( ( C . (j,k,l) ) . (h,g) ),f) ) by A2,A8,A13,CKB29:def 1;
end;

thus (the Comp of ( EnsCat A )) is  with_left_units
proof
let i being (Element of ( EnsCat A ));
reconsider i9 = i as (object of ( EnsCat A ));
take ( id i );
A14: <^ i9,i9 ^> = ( Funcs (i,i) ) by CKB44:def 1;
hence ( id i ) in ( G . (i,i) ) by CKB1:1;
reconsider I = ( id i ) as (Morphism of i9,i9) by A14,CKB1:1;
let j being (Element of ( EnsCat A ));
let f being set;
reconsider j9 = j as (object of ( EnsCat A ));
assume A15: f in ( G . (j,i) );
then reconsider f99 = f as (Morphism of j9,i9);
<^ j9,i9 ^> = ( Funcs (j,i) ) by CKB44:def 1;
then reconsider f9 = f as (Function of j,i) by A15,FUNCT_2:66;
thus ( ( C . (j,i,i) ) . (( id i ),f) ) = ( I * f99 ) by A14,A15,CKB29:def 1
.= ( ( id i ) * f9 ) by A14,A15,CKB43:1
.= f by FUNCT_2:17;
end;

let i being (Element of ( EnsCat A ));
reconsider i9 = i as (object of ( EnsCat A ));
take ( id i );
A16: <^ i9,i9 ^> = ( Funcs (i,i) ) by CKB44:def 1;
hence ( id i ) in ( G . (i,i) ) by CKB1:1;
reconsider I = ( id i ) as (Morphism of i9,i9) by A16,CKB1:1;
let j being (Element of ( EnsCat A ));
let f being set;
reconsider j9 = j as (object of ( EnsCat A ));
assume A17: f in ( G . (i,j) );
then reconsider f99 = f as (Morphism of i9,j9);
<^ i9,j9 ^> = ( Funcs (i,j) ) by CKB44:def 1;
then reconsider f9 = f as (Function of i,j) by A17,FUNCT_2:66;
thus ( ( C . (i,i,j) ) . (f,( id i )) ) = ( f99 * I ) by A16,A17,CKB29:def 1
.= ( f9 * ( id i ) ) by A16,A17,CKB43:1
.= f by FUNCT_2:17;
end;
