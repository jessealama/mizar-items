requirements BOOLE,SUBSET,NUMERALS;
begin
reserve i for set;
reserve j for set;
reserve k for set;
reserve x for set;
theorem
Th10: (for A being set holds (for N,M being (ManySortedSet of [: A,A,A :]) holds ((for i holds (for j holds (for k holds (((i in A & j in A) & k in A) implies ( N . (i,j,k) ) = ( M . (i,j,k) ))))) implies M = N)))
proof
let A being set;
let N being (ManySortedSet of [: A,A,A :]),M being (ManySortedSet of [: A,A,A :]);
assume A1: (for i holds (for j holds (for k holds (((i in A & j in A) & k in A) implies ( N . (i,j,k) ) = ( M . (i,j,k) )))));
A2:now
let x;
assume A3: x in [: A,A,A :];
then reconsider A as non empty set by MCART_1:31;
consider i being (Element of A),j being (Element of A),k being (Element of A) such that A4: x = [ i,j,k ] by A3,DOMAIN_1:3;
thus ( M . x ) = ( M . (i,j,k) ) by A4,MULTOP_1:def 1
.= ( N . (i,j,k) ) by A1
.= ( N . x ) by A4,MULTOP_1:def 1;
end;
(( dom M ) = [: A,A,A :] & ( dom N ) = [: A,A,A :]) by PARTFUN1:def 2;
hence thesis by A2,FUNCT_1:2;
end;
