begin
reserve i for set;
theorem
Th16: (for A,B being  functional set holds (for A1 being (Subset of A) holds (for B1 being (Subset of B) holds ( FuncComp (A1,B1) ) = ( ( FuncComp (A,B) ) | ([: B1,A1 :] qua set) ))))
proof
let A being  functional set,B being  functional set;
let A1 being (Subset of A);
let B1 being (Subset of B);
set f = ( ( FuncComp (A,B) ) | ([: B1,A1 :] qua set) );
A1: ( dom ( FuncComp (A,B) ) ) = [: B,A :] by PARTFUN1:def 2;
then A2: ( dom f ) = [: B1,A1 :] by RELAT_1:62;
then reconsider f as (ManySortedFunction of [: B1,A1 :]) by PARTFUN1:def 2;
f is  compositional
proof
let i;
assume A3: i in ( dom f );
then ( f . i ) = ( ( FuncComp (A,B) ) . i ) by A2,FUNCT_1:49;
hence thesis by A1,A2,A3,CKB30:def 1;
end;
hence thesis by CKB33:def 1;
end;
