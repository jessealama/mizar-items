begin
reserve i for set;
reserve j for set;
theorem
(for C being  with_units non  empty AltCatStr holds (C is  pseudo-discrete iff (for o being (object of C) holds <^ o,o ^> = { ( idm o ) })))
proof
let C being  with_units non  empty AltCatStr;
hereby
assume A1: C is  pseudo-discrete;
let o being (object of C);
now
let j;
hereby
(( idm o ) in <^ o,o ^> & <^ o,o ^> is  trivial) by A1,CKB60:def 1,CKB56:1;
then consider i such that A2: <^ o,o ^> = { i } by ZFMISC_1:131;
assume j in <^ o,o ^>;
then j = i by A2,TARSKI:def 1;
hence j = ( idm o ) by A2,TARSKI:def 1;
end;
thus (j = ( idm o ) implies j in <^ o,o ^>) by CKB56:1;
end;
hence <^ o,o ^> = { ( idm o ) } by TARSKI:def 1;
end;
assume A3: (for o being (object of C) holds <^ o,o ^> = { ( idm o ) });
let o being (object of C);
<^ o,o ^> = { ( idm o ) } by A3;
hence thesis;
end;
