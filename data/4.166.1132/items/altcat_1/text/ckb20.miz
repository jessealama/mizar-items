environ
vocabularies FUNCT_1,SUBSET_1,RELAT_1,FUNCT_2,XBOOLE_0,TARSKI,PBOOLE,ZFMISC_1,MCART_1,FUNCOP_1,STRUCT_0,CAT_1,RELAT_2,BINOP_1,CARD_1,ALTCAT_1;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,MCART_1,DOMAIN_1,RELAT_1,CARD_1,NUMBERS,STRUCT_0,FUNCT_1,FUNCT_2,BINOP_1,MULTOP_1,FUNCOP_1,PBOOLE,CKB15,CKB16,CKB17,CKB18,CKB19;
definitions TARSKI,STRUCT_0,XBOOLE_0,FUNCOP_1,BINOP_1,CKB17,CKB19;
theorems FUNCT_1,ZFMISC_1,PBOOLE,DOMAIN_1,MULTOP_1,MCART_1,FUNCT_2,TARSKI,FUNCOP_1,RELAT_1,REALSET1,STRUCT_0,RELSET_1,XBOOLE_0,XBOOLE_1,CARD_1,PARTFUN1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB10,CKB11,CKB12,CKB13,CKB14,CKB17,CKB19;
schemes FUNCT_1,CKB6,CKB7,CKB8,CKB9;
registrations XBOOLE_0,SUBSET_1,RELAT_1,FUNCT_1,FUNCT_2,FUNCOP_1,STRUCT_0,RELSET_1,ZFMISC_1,CARD_1,ORDINAL1,CKB15;
constructors PARTFUN1,BINOP_1,MULTOP_1,PBOOLE,REALSET2,RELSET_1,CKB15,CKB17,CKB19;
requirements BOOLE,SUBSET,NUMERALS;
begin
reserve i for set;
reserve j for set;
reserve k for set;
definition
let I being set;
let G being (ManySortedSet of [: I,I :]);
func {|G |} -> (ManySortedSet of [: I,I,I :]) means 
:Def5: (for i holds (for j holds (for k holds (((i in I & j in I) & k in I) implies ( it . (i,j,k) ) = ( G . (i,k) )))));
existence
proof
deffunc F(set,set,set) = ( G . ($1,$3) );
(ex M being (ManySortedSet of [: I,I,I :]) st (for i holds (for j holds (for k holds (((i in I & j in I) & k in I) implies ( M . (i,j,k) ) = F(i,j,k)))))) from CKB8:sch 1;
hence thesis;
end;
uniqueness
proof
let M1 being (ManySortedSet of [: I,I,I :]);
let M2 being (ManySortedSet of [: I,I,I :]);
assume that
A1: (for i holds (for j holds (for k holds (((i in I & j in I) & k in I) implies ( M1 . (i,j,k) ) = ( G . (i,k) )))))
and
A2: (for i holds (for j holds (for k holds (((i in I & j in I) & k in I) implies ( M2 . (i,j,k) ) = ( G . (i,k) )))));
now
let i;
let j;
let k;
assume A3: ((i in I & j in I) & k in I);
hence ( M1 . (i,j,k) ) = ( G . (i,k) ) by A1
.= ( M2 . (i,j,k) ) by A2,A3;
end;
hence M1 = M2 by CKB12:1;
end;
end;
