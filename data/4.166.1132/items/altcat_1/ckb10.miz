begin
reserve i for set;
reserve j for set;
reserve x for set;
theorem
Th8: (for A,B being set holds (for N,M being (ManySortedSet of [: A,B :]) holds ((for i holds (for j holds ((i in A & j in B) implies ( N . (i,j) ) = ( M . (i,j) )))) implies M = N)))
proof
let A being set,B being set;
let N being (ManySortedSet of [: A,B :]),M being (ManySortedSet of [: A,B :]);
assume A1: (for i holds (for j holds ((i in A & j in B) implies ( N . (i,j) ) = ( M . (i,j) ))));
A2:now
let x;
assume A3: x in [: A,B :];
then reconsider A1 = A,B1 = B as non empty set;
consider i being (Element of A1),j being (Element of B1) such that A4: x = [ i,j ] by A3,DOMAIN_1:1;
thus ( N . x ) = ( N . (i,j) ) by A4
.= ( M . (i,j) ) by A1
.= ( M . x ) by A4;
end;
(( dom M ) = [: A,B :] & ( dom N ) = [: A,B :]) by PARTFUN1:def 2;
hence thesis by A2,FUNCT_1:2;
end;
