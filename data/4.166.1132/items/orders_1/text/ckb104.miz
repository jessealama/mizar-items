reserve R for Relation;
reserve P for Relation;
reserve X for set;
reserve Y for set;
reserve x for set;
reserve y for set;
reserve z for set;
reserve A for Ordinal;
scheme ZornMax { A() -> non  empty set,P[set,set] } : (ex x being (Element of A()) st (for y being (Element of A()) holds (x <> y implies (not P[ x,y ]))))
provided
A1: (for x being (Element of A()) holds P[ x,x ])
and
A2: (for x,y being (Element of A()) holds ((P[ x,y ] & P[ y,x ]) implies x = y))
and
A3: (for x,y,z being (Element of A()) holds ((P[ x,y ] & P[ y,z ]) implies P[ x,z ]))
and
A4: (for X holds ((X c= A() & (for x,y being (Element of A()) holds ((x in X & y in X) implies (P[ x,y ] or P[ y,x ])))) implies (ex y being (Element of A()) st (for x being (Element of A()) holds (x in X implies P[ x,y ])))))
proof
consider R being (Relation of A()) such that A5: (for x,y being (Element of A()) holds ([ x,y ] in R iff P[ x,y ])) from RELSET_1:sch 2;
A6: R is_transitive_in A()
proof
let x;
let y;
let z;
assume that
A7: x in A()
and
A8: y in A()
and
A9: z in A();
reconsider x9 = x,y9 = y,z9 = z as (Element of A()) by A7,A8,A9;
assume that
A10: [ x,y ] in R
and
A11: [ y,z ] in R;
A12: P[ y9,z9 ] by A5,A11;
P[ x9,y9 ] by A5,A10;
then P[ x9,z9 ] by A3,A12;
hence thesis by A5;
end;
A() c= ( dom R )
proof
let x;
assume x in A();
then reconsider x9 = x as (Element of A());
P[ x9,x9 ] by A1;
then [ x,x ] in R by A5;
hence thesis by RELAT_1:def 4;
end;
then ( dom R ) = A() by XBOOLE_0:def 10;
then A13: A() c= ( field R ) by XBOOLE_1:7;
( field R ) = ( ( dom R ) \/ ( rng R ) );
then A14: ( field R ) = A() by A13,XBOOLE_0:def 10;
A15: R is_reflexive_in A()
proof
let x;
assume x in A();
then reconsider x9 = x as (Element of A());
P[ x9,x9 ] by A1;
hence thesis by A5;
end;
A16: A() has_upper_Zorn_property_wrt R
proof
let Y;
assume that
A17: Y c= A()
and
A18: ( R |_2 Y ) is  being_linear-order;
(for x,y being (Element of A()) holds ((x in Y & y in Y) implies (P[ x,y ] or P[ y,x ])))
proof
let x being (Element of A());
let y being (Element of A());
assume that
A19: x in Y
and
A20: y in Y;
A21: ( R |_2 Y ) is  reflexive  connected by A18,CKB24:def 1;
Y c= ( field ( R |_2 Y ) )
proof
let x;
assume A22: x in Y;
then A23: [ x,x ] in [: Y,Y :] by ZFMISC_1:87;
[ x,x ] in R by A15,A17,A22,RELAT_2:def 1;
then [ x,x ] in ( R |_2 Y ) by A23,XBOOLE_0:def 4;
hence thesis by RELAT_1:15;
end;
then A24: Y = ( field ( ( R |_2 Y ) |_2 Y ) ) by A21,CKB36:1
.= ( field ( R |_2 Y ) ) by WELLORD1:21;
then A25: ( R |_2 Y ) is_connected_in Y by A21,RELAT_2:def 14;
A26: ( R |_2 Y ) is_reflexive_in Y by A21,A24,RELAT_2:def 9;
(x = y or x <> y);
then ([ x,y ] in ( R |_2 Y ) or [ y,x ] in ( R |_2 Y )) by A19,A20,A25,A26,RELAT_2:def 1,RELAT_2:def 6;
then ([ x,y ] in R or [ y,x ] in R) by XBOOLE_0:def 4;
hence thesis by A5;
end;
then consider y being (Element of A()) such that A27: (for x being (Element of A()) holds (x in Y implies P[ x,y ])) by A4,A17;
take y;
thus y in A();
let x;
assume A28: x in Y;
then P[ x,y ] by A17,A27;
hence thesis by A5,A17,A28;
end;
R is_antisymmetric_in A()
proof
let x;
let y;
assume that
A29: x in A()
and
A30: y in A();
reconsider x9 = x,y9 = y as (Element of A()) by A29,A30;
assume that
A31: [ x,y ] in R
and
A32: [ y,x ] in R;
A33: P[ y9,x9 ] by A5,A32;
P[ x9,y9 ] by A5,A31;
hence thesis by A2,A33;
end;
then R partially_orders A() by A15,A6,CKB44:def 1;
then consider x such that A34: x is_maximal_in R by A14,A16,CKB98:1;
take x;
thus x is (Element of A()) by A14,A34,CKB83:def 1;
let y being (Element of A());
assume x <> y;
then A35: (not [ x,y ] in R) by A14,A34,CKB83:def 1;
x in A() by A14,A34,CKB83:def 1;
hence thesis by A5,A35;
end;
