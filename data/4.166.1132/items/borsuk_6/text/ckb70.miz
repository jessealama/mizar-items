schemes RECDEF_2,DOMAIN_1,XBOOLE_0,JGRAPH_2,PARTFUN1,BINOP_1,CKB1;
registrations XBOOLE_0,RELSET_1,FUNCT_2,NUMBERS,XXREAL_0,XREAL_0,MEMBERED,STRUCT_0,PRE_TOPC,BORSUK_1,MONOID_0,EUCLID,TOPMETR,BORSUK_2,BORSUK_3,JGRAPH_2,BORSUK_4,TOPS_1,COMPTS_1,SUBSET_1,ZFMISC_1,RELAT_1,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB30,CKB40,CKB64,CKB67;
constructors REAL_1,RCOMP_1,FINSEQ_4,CONNSP_1,TOPS_2,COMPTS_1,GRCAT_1,T_0TOPSP,MONOID_0,TREAL_1,BORSUK_2,JGRAPH_2,BORSUK_4,SEQ_1,FUNCOP_1,COMPLEX1,PCOMPS_1,TOPREAL2,CKB44,CKB54,CKB60,CKB63,CKB66,CKB69;
requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
registration
cluster ( 3RP ) ->  continuous;
coherence
proof
A1: ( 1 / 2 ) is (Point of ( I[01] )) by BORSUK_1:43;
A2: ( 3 / 4 ) is (Point of ( I[01] )) by BORSUK_1:43;
then reconsider B = [. ( 1 / 2 ),( 3 / 4 ) .] as non empty  compact (Subset of ( I[01] )) by A1,BORSUK_4:24;
A3: ( 0 ) is (Point of ( I[01] )) by BORSUK_1:43;
then reconsider A = [. ( 0 ),( 1 / 2 ) .] as non empty  compact (Subset of ( I[01] )) by A1,BORSUK_4:24;
reconsider G = [. ( 0 ),( 3 / 4 ) .] as non empty  compact (Subset of ( I[01] )) by A3,A2,BORSUK_4:24;
A4: ( 1 / 4 ) is (Point of ( I[01] )) by BORSUK_1:43;
then reconsider E = [. ( 1 / 4 ),( 1 / 2 ) .] as non empty  compact (Subset of ( I[01] )) by A1,BORSUK_4:24;
reconsider F = [. ( 0 ),( 1 / 4 ) .] as non empty  compact (Subset of ( I[01] )) by A3,A4,BORSUK_4:24;
A5: 1 is (Point of ( I[01] )) by BORSUK_1:43;
then reconsider C = [. ( 3 / 4 ),1 .] as non empty  compact (Subset of ( I[01] )) by A2,BORSUK_4:24;
reconsider D = [. ( 1 / 2 ),1 .] as non empty  compact (Subset of ( I[01] )) by A1,A5,BORSUK_4:24;
set T = ( I[01] );
set T1 = ( T | A );
set T2 = ( T | B );
set T3 = ( T | C );
set T4 = ( T | D );
set T5 = ( T | E );
set T6 = ( T | F );
set f = ( L[01] (( 0 ),( 1 / 2 ),( 0 ),( 1 / 4 )) );
set g = ( L[01] (( 1 / 2 ),( 3 / 4 ),( 1 / 4 ),( 1 / 2 )) );
set h = ( L[01] (( 3 / 4 ),1,( 1 / 2 ),1) );
reconsider TT1 = T1,TT2 = T2 as (SubSpace of ( T | G )) by TOPMETR:22,XXREAL_1:34;
(( Closed-Interval-TSpace (( 3 / 4 ),1) ) = T3 & ( Closed-Interval-TSpace (( 1 / 2 ),1) ) = T4) by TOPMETR:24;
then reconsider h as  continuous (Function of T3,T4) by CKB46:1;
reconsider h as  continuous (Function of T3,T) by JORDAN6:3;
A6: (for x being (Point of T3) holds ( h . x ) = ( ( 2 * x ) - 1 ))
proof
let x being (Point of T3);
x in (the carrier of T3);
then x in C by PRE_TOPC:8;
then (( 3 / 4 ) <= x & x <= 1) by XXREAL_1:1;
then ( h . x ) = ( ( ( ( 1 - ( 1 / 2 ) ) / ( 1 - ( 3 / 4 ) ) ) * ( x - ( 3 / 4 ) ) ) + ( 1 / 2 ) ) by CKB47:1
.= ( ( 2 * x ) - 1 );
hence thesis;
end;
(( Closed-Interval-TSpace (( 0 ),( 1 / 4 )) ) = T6 & ( Closed-Interval-TSpace (( 0 ),( 1 / 2 )) ) = T1) by TOPMETR:24;
then reconsider f as  continuous (Function of T1,T6) by CKB46:1;
(( Closed-Interval-TSpace (( 1 / 4 ),( 1 / 2 )) ) = T5 & ( Closed-Interval-TSpace (( 1 / 2 ),( 3 / 4 )) ) = T2) by TOPMETR:24;
then reconsider g as  continuous (Function of T2,T5) by CKB46:1;
reconsider g as  continuous (Function of T2,T) by JORDAN6:3;
reconsider f as  continuous (Function of T1,T) by JORDAN6:3;
set f1 = f;
set g1 = g;
A7: (for x being (Point of T2) holds ( g . x ) = ( x - ( 1 / 4 ) ))
proof
let x being (Point of T2);
x in (the carrier of T2);
then x in B by PRE_TOPC:8;
then (( 1 / 2 ) <= x & x <= ( 3 / 4 )) by XXREAL_1:1;
then ( g . x ) = ( ( ( ( ( 1 / 2 ) - ( 1 / 4 ) ) / ( ( 3 / 4 ) - ( 1 / 2 ) ) ) * ( x - ( 1 / 2 ) ) ) + ( 1 / 4 ) ) by CKB47:1
.= ( x - ( 1 / 4 ) );
hence thesis;
end;
A8: ( ( [#] TT1 ) /\ ( [#] TT2 ) ) = ( A /\ ( [#] TT2 ) ) by PRE_TOPC:def 5
.= ( A /\ B ) by PRE_TOPC:def 5
.= { ( 1 / 2 ) } by XXREAL_1:418;
A9: (for p being set holds (p in ( ( [#] TT1 ) /\ ( [#] TT2 ) ) implies ( f1 . p ) = ( g1 . p )))
proof
let p being set;
assume p in ( ( [#] TT1 ) /\ ( [#] TT2 ) );
then A10: p = ( 1 / 2 ) by A8,TARSKI:def 1;
then reconsider p as (Point of ( I[01] )) by BORSUK_1:40,XXREAL_1:1;
( f1 . p ) = ( ( ( ( ( 1 / 4 ) - ( 0 ) ) / ( ( 1 / 2 ) - ( 0 ) ) ) * ( p - ( 0 ) ) ) + ( 0 ) ) by A10,CKB47:1
.= ( ( ( ( ( 1 / 2 ) - ( 1 / 4 ) ) / ( ( 3 / 4 ) - ( 1 / 2 ) ) ) * ( p - ( 1 / 2 ) ) ) + ( 1 / 4 ) ) by A10
.= ( g1 . p ) by A10,CKB47:1;
hence thesis;
end;
( ( [#] TT1 ) \/ ( [#] TT2 ) ) = ( A \/ ( [#] TT2 ) ) by PRE_TOPC:def 5
.= ( A \/ B ) by PRE_TOPC:def 5
.= [. ( 0 ),( 3 / 4 ) .] by XXREAL_1:174
.= ( [#] ( T | G ) ) by PRE_TOPC:def 5;
then consider FF being (Function of ( T | G ),T) such that A11: FF = ( f1 +* g1 ) and A12: FF is  continuous by A9,BORSUK_2:1;
A13: ( ( [#] ( T | G ) ) /\ ( [#] T3 ) ) = ( G /\ ( [#] T3 ) ) by PRE_TOPC:def 5
.= ( G /\ C ) by PRE_TOPC:def 5
.= { ( 3 / 4 ) } by XXREAL_1:418;
A14: (for p being set holds (p in ( ( [#] ( T | G ) ) /\ ( [#] T3 ) ) implies ( FF . p ) = ( h . p )))
proof
let p being set;
assume p in ( ( [#] ( T | G ) ) /\ ( [#] T3 ) );
then A15: p = ( 3 / 4 ) by A13,TARSKI:def 1;
then reconsider p as (Point of T) by BORSUK_1:43;
p in [. ( 1 / 2 ),( 3 / 4 ) .] by A15,XXREAL_1:1;
then p in (the carrier of T2) by PRE_TOPC:8;
then p in ( dom g ) by FUNCT_2:def 1;
then ( FF . p ) = ( g . p ) by A11,FUNCT_4:13
.= ( 1 / 2 ) by A15,CKB45:1
.= ( h . p ) by A15,CKB45:1;
hence thesis;
end;
( ( [#] ( T | G ) ) \/ ( [#] T3 ) ) = ( G \/ ( [#] T3 ) ) by PRE_TOPC:def 5
.= ( G \/ C ) by PRE_TOPC:def 5
.= ( [#] T ) by BORSUK_1:40,XXREAL_1:174;
then consider HH being (Function of T,T) such that A16: HH = ( FF +* h ) and A17: HH is  continuous by A12,A14,BORSUK_2:1;
A18: (for x being (Point of T1) holds ( f . x ) = ( ( 1 / 2 ) * x ))
proof
let x being (Point of T1);
x in (the carrier of T1);
then x in A by PRE_TOPC:8;
then (( 0 ) <= x & x <= ( 1 / 2 )) by XXREAL_1:1;
then ( f . x ) = ( ( ( ( ( 1 / 4 ) - ( 0 ) ) / ( ( 1 / 2 ) - ( 0 ) ) ) * ( x - ( 0 ) ) ) + ( 0 ) ) by CKB47:1
.= ( ( 1 / 2 ) * x );
hence thesis;
end;
(for x being (Element of T) holds ( HH . x ) = ( ( 3RP ) . x ))
proof
let x being (Element of T);
A19: ( 0 ) <= x by BORSUK_1:43;
A20: x <= 1 by BORSUK_1:43;
per cases  by XXREAL_0:1;
suppose A21: x < ( 1 / 2 );

then (not x in [. ( 1 / 2 ),( 3 / 4 ) .]) by XXREAL_1:1;
then (not x in (the carrier of T2)) by PRE_TOPC:8;
then A22: (not x in ( dom g ));
x in [. ( 0 ),( 1 / 2 ) .] by A19,A21,XXREAL_1:1;
then A23: x is (Point of T1) by PRE_TOPC:8;
x < ( 3 / 4 ) by A21,XXREAL_0:2;
then (not x in [. ( 3 / 4 ),1 .]) by XXREAL_1:1;
then (not x in (the carrier of T3)) by PRE_TOPC:8;
then (not x in ( dom h ));
then ( HH . x ) = ( FF . x ) by A16,FUNCT_4:11
.= ( f . x ) by A11,A22,FUNCT_4:11
.= ( ( 1 / 2 ) * x ) by A18,A23
.= ( ( 3RP ) . x ) by A21,CKB69:def 1;
hence thesis;
end;
suppose A24: x = ( 1 / 2 );

then x in [. ( 1 / 2 ),( 3 / 4 ) .] by XXREAL_1:1;
then x in (the carrier of T2) by PRE_TOPC:8;
then A25: x in ( dom g ) by FUNCT_2:def 1;
(not x in [. ( 3 / 4 ),1 .]) by A24,XXREAL_1:1;
then (not x in (the carrier of T3)) by PRE_TOPC:8;
then (not x in ( dom h ));
then ( HH . x ) = ( FF . x ) by A16,FUNCT_4:11
.= ( g . x ) by A11,A25,FUNCT_4:13
.= ( ( 1 / 2 ) * x ) by A24,CKB45:1
.= ( ( 3RP ) . x ) by A24,CKB69:def 1;
hence thesis;
end;
suppose A26: (x > ( 1 / 2 ) & x < ( 3 / 4 ));

then x in [. ( 1 / 2 ),( 3 / 4 ) .] by XXREAL_1:1;
then A27: x in (the carrier of T2) by PRE_TOPC:8;
then A28: x in ( dom g ) by FUNCT_2:def 1;
(not x in [. ( 3 / 4 ),1 .]) by A26,XXREAL_1:1;
then (not x in (the carrier of T3)) by PRE_TOPC:8;
then (not x in ( dom h ));
then ( HH . x ) = ( FF . x ) by A16,FUNCT_4:11
.= ( g . x ) by A11,A28,FUNCT_4:13
.= ( x - ( 1 / 4 ) ) by A7,A27
.= ( ( 3RP ) . x ) by A26,CKB69:def 1;
hence thesis;
end;
suppose A29: x = ( 3 / 4 );

then x in [. ( 3 / 4 ),1 .] by XXREAL_1:1;
then x in (the carrier of T3) by PRE_TOPC:8;
then x in ( dom h ) by FUNCT_2:def 1;
then ( HH . x ) = ( h . x ) by A16,FUNCT_4:13
.= ( x - ( 1 / 4 ) ) by A29,CKB45:1
.= ( ( 3RP ) . x ) by A29,CKB69:def 1;
hence thesis;
end;
suppose A30: x > ( 3 / 4 );

then x in [. ( 3 / 4 ),1 .] by A20,XXREAL_1:1;
then A31: x in (the carrier of T3) by PRE_TOPC:8;
then x in ( dom h ) by FUNCT_2:def 1;
then ( HH . x ) = ( h . x ) by A16,FUNCT_4:13
.= ( ( 2 * x ) - 1 ) by A6,A31
.= ( ( 3RP ) . x ) by A30,CKB69:def 1;
hence thesis;
end;
end;
hence thesis by A17,FUNCT_2:63;
end;
end;
