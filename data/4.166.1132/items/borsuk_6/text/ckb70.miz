definitions XBOOLE_0,BORSUK_2,MEMBERED,BINOP_1,EUCLID,STRUCT_0,PCOMPS_1,TARSKI,SUBSET_1,CKB44,CKB58,CKB59,CKB60,CKB63,CKB66,CKB69;
