existence
proof
reconsider z9 = ( 0 ) as (Element of ( REAL+ )) by ARYTM_2:20;
A1: x in ( ( REAL+ ) \/ [: { ( 0 ) },( REAL+ ) :] ) by NUMBERS:def 1,XBOOLE_0:def 5;
per cases  by A1,XBOOLE_0:def 3;
suppose A2: x = ( 0 );

then reconsider x9 = x as (Element of ( REAL+ )) by ARYTM_2:20;
take x;
( x9 + x9 ) = ( 0 ) by A2,ARYTM_2:def 8;
hence thesis by CKB10:def 1;
end;
suppose that A3: x in ( REAL+ )
and
A4: x <> ( 0 );
A5: [ ( 0 ),x ] <> [ ( 0 ),( 0 ) ] by A4,ZFMISC_1:27;
( 0 ) in { ( 0 ) } by TARSKI:def 1;
then A6: [ ( 0 ),x ] in [: { ( 0 ) },( REAL+ ) :] by A3,ZFMISC_1:87;
then [ ( 0 ),x ] in ( ( REAL+ ) \/ [: { ( 0 ) },( REAL+ ) :] ) by XBOOLE_0:def 3;
then reconsider y = [ ( 0 ),x ] as (Element of ( REAL )) by A5,NUMBERS:def 1,ZFMISC_1:56;
reconsider x9 = x as (Element of ( REAL+ )) by A3;
A7: x9 <=' x9;
take y;
( z9 + x9 ) = x9 by ARYTM_2:def 8;
then z9 = ( x9 -' x9 ) by A7,ARYTM_1:def 1;
then ( 0 ) = ( x9 - x9 ) by A7,ARYTM_1:def 2;
hence thesis by A6,CKB10:def 1;
end;
suppose A8: x in [: { ( 0 ) },( REAL+ ) :];

then consider a,b being set such that A9: a in { ( 0 ) } and A10: b in ( REAL+ ) and A11: x = [ a,b ] by ZFMISC_1:84;
reconsider y = b as (Element of ( REAL )) by A10,CKB2:1;
take y;
now
per cases ;
case (x in ( REAL+ ) & y in ( REAL+ ));
hence (ex x9,y9 being (Element of ( REAL+ )) st ((x = x9 & y = y9) & ( 0 ) = ( x9 + y9 ))) by A8,CKB6:1,XBOOLE_0:3;
end;
case (x in ( REAL+ ) & y in [: { ( 0 ) },( REAL+ ) :]);
hence (ex x9,y9 being (Element of ( REAL+ )) st ((x = x9 & y = [ ( 0 ),y9 ]) & ( 0 ) = ( x9 - y9 ))) by A10,CKB6:1,XBOOLE_0:3;
end;
case (y in ( REAL+ ) & x in [: { ( 0 ) },( REAL+ ) :]);
reconsider x9 = b,y9 = y as (Element of ( REAL+ )) by A10;
take x9;
take y9;
thus x = [ ( 0 ),x9 ] by A9,A11,TARSKI:def 1;
thus y = y9;
thus ( 0 ) = ( y9 - x9 ) by ARYTM_1:18;
end;
case (((not (x in ( REAL+ ) & y in ( REAL+ ))) & (not (x in ( REAL+ ) & y in [: { ( 0 ) },( REAL+ ) :]))) & (not (y in ( REAL+ ) & x in [: { ( 0 ) },( REAL+ ) :])));
hence (ex x9,y9 being (Element of ( REAL+ )) st ((x = [ ( 0 ),x9 ] & y = [ ( 0 ),y9 ]) & ( 0 ) = [ ( 0 ),( y9 + x9 ) ])) by A8,A10;
end;
end;
hence thesis by CKB10:def 1;
end;
end;
uniqueness
proof
let y being (Element of ( REAL ));
let z being (Element of ( REAL ));
assume that
A12: ( + (x,y) ) = ( 0 )
and
A13: ( + (x,z) ) = ( 0 );
per cases ;
suppose ((x in ( REAL+ ) & y in ( REAL+ )) & z in ( REAL+ ));

then ((ex x9,y9 being (Element of ( REAL+ )) st ((x = x9 & y = y9) & ( 0 ) = ( x9 + y9 ))) & (ex x9,y9 being (Element of ( REAL+ )) st ((x = x9 & z = y9) & ( 0 ) = ( x9 + y9 )))) by A12,A13,CKB10:def 1;
hence thesis by ARYTM_2:11;
end;
suppose that A14: x in ( REAL+ )
and
A15: y in ( REAL+ )
and
A16: z in [: { ( 0 ) },( REAL+ ) :];
consider x99,y99 being (Element of ( REAL+ )) such that A17: x = x99 and A18: (z = [ ( 0 ),y99 ] & ( 0 ) = ( x99 - y99 )) by A13,A14,A16,CKB10:def 1;
(ex x9,y9 being (Element of ( REAL+ )) st ((x = x9 & y = y9) & ( 0 ) = ( x9 + y9 ))) by A12,A14,A15,CKB10:def 1;
then A19: x99 = ( 0 ) by A17,ARYTM_2:5;
[ ( 0 ),( 0 ) ] in { [ ( 0 ),( 0 ) ] } by TARSKI:def 1;
then A20: (not [ ( 0 ),( 0 ) ] in ( REAL )) by NUMBERS:def 1,XBOOLE_0:def 5;
z in ( REAL );
hence thesis by A18,A19,A20,ARYTM_1:19;
end;
suppose that A21: x in ( REAL+ )
and
A22: z in ( REAL+ )
and
A23: y in [: { ( 0 ) },( REAL+ ) :];
consider x99,y9 being (Element of ( REAL+ )) such that A24: x = x99 and A25: (y = [ ( 0 ),y9 ] & ( 0 ) = ( x99 - y9 )) by A12,A21,A23,CKB10:def 1;
(ex x9,z9 being (Element of ( REAL+ )) st ((x = x9 & z = z9) & ( 0 ) = ( x9 + z9 ))) by A13,A21,A22,CKB10:def 1;
then A26: x99 = ( 0 ) by A24,ARYTM_2:5;
[ ( 0 ),( 0 ) ] in { [ ( 0 ),( 0 ) ] } by TARSKI:def 1;
then A27: (not [ ( 0 ),( 0 ) ] in ( REAL )) by NUMBERS:def 1,XBOOLE_0:def 5;
y in ( REAL );
hence thesis by A25,A26,A27,ARYTM_1:19;
end;
suppose that A28: x in ( REAL+ )
and
A29: y in [: { ( 0 ) },( REAL+ ) :]
and
A30: z in [: { ( 0 ) },( REAL+ ) :];
consider x99,z9 being (Element of ( REAL+ )) such that A31: x = x99 and A32: z = [ ( 0 ),z9 ] and A33: ( 0 ) = ( x99 - z9 ) by A13,A28,A30,CKB10:def 1;
consider x9,y9 being (Element of ( REAL+ )) such that A34: x = x9 and A35: y = [ ( 0 ),y9 ] and A36: ( 0 ) = ( x9 - y9 ) by A12,A28,A29,CKB10:def 1;
y9 = x9 by A36,CKB7:1
.= z9 by A34,A31,A33,CKB7:1;
hence thesis by A35,A32;
end;
suppose that A37: z in ( REAL+ )
and
A38: y in ( REAL+ )
and
A39: x in [: { ( 0 ) },( REAL+ ) :];
consider x99,z9 being (Element of ( REAL+ )) such that A40: x = [ ( 0 ),x99 ] and A41: z = z9 and A42: ( 0 ) = ( z9 - x99 ) by A13,A37,A39,CKB10:def 1;
consider x9,y9 being (Element of ( REAL+ )) such that A43: x = [ ( 0 ),x9 ] and A44: y = y9 and A45: ( 0 ) = ( y9 - x9 ) by A12,A38,A39,CKB10:def 1;
x9 = x99 by A43,A40,ZFMISC_1:27;
then z9 = x9 by A42,CKB7:1
.= y9 by A45,CKB7:1;
hence thesis by A44,A41;
end;
suppose (((not (x in ( REAL+ ) & y in ( REAL+ ))) & (not (x in ( REAL+ ) & y in [: { ( 0 ) },( REAL+ ) :]))) & (not (y in ( REAL+ ) & x in [: { ( 0 ) },( REAL+ ) :])));

then (ex x9,y9 being (Element of ( REAL+ )) st ((x = [ ( 0 ),x9 ] & y = [ ( 0 ),y9 ]) & ( 0 ) = [ ( 0 ),( x9 + y9 ) ])) by A12,CKB10:def 1;
hence thesis;
end;
suppose (((not (x in ( REAL+ ) & z in ( REAL+ ))) & (not (x in ( REAL+ ) & z in [: { ( 0 ) },( REAL+ ) :]))) & (not (z in ( REAL+ ) & x in [: { ( 0 ) },( REAL+ ) :])));

then (ex x9,z9 being (Element of ( REAL+ )) st ((x = [ ( 0 ),x9 ] & z = [ ( 0 ),z9 ]) & ( 0 ) = [ ( 0 ),( x9 + z9 ) ])) by A13,CKB10:def 1;
hence thesis;
end;
