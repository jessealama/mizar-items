begin
reserve x for (Element of ( REAL ));
reserve y for (Element of ( REAL ));
theorem
Th21: (for x holds (for y holds (y = 1 implies ( * (x,y) ) = x)))
proof
let x;
let y;
assume that
A1: y = 1;
per cases ;
suppose x = ( 0 );

hence thesis by CKB20:1;
end;
suppose A2: x <> ( 0 );

A3:now
assume A4: ( inv x ) = ( 0 );
thus 1 = ( * (x,( inv x )) ) by A2,CKB13:def 1
.= ( 0 ) by A4,CKB20:1;
end;
A5: (ex x9,y9 being (Element of ( REAL+ )) st ((y = x9 & y = y9) & ( * (y,y) ) = ( x9 *' y9 ))) by A1,CKB11:def 1,ARYTM_2:20;
A6: ( * (x,( inv x )) ) = 1 by A2,CKB13:def 1;
( * (( * (x,y) ),( inv x )) ) = ( * (( * (x,( inv x )) ),y) ) by CKB21:1
.= ( * (y,y) ) by A1,A2,CKB13:def 1
.= 1 by A1,A5,ARYTM_2:15;
hence thesis by A3,A6,CKB26:1;
end;
end;
