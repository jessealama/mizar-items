begin
reserve n for (Element of ( NAT ));
reserve m for (Element of ( NAT ));
theorem
Th13: (for f being (Function of ( REAL m ),( REAL n )) holds (for g being (Function of ( REAL-NS m ),( REAL-NS n )) holds (f = g implies (f is  homogeneous iff g is  homogeneous))))
proof
let f being (Function of ( REAL m ),( REAL n ));
let g being (Function of ( REAL-NS m ),( REAL-NS n ));
assume A1: f = g;
hereby
assume A2: f is  homogeneous;
now
let x being (Point of ( REAL-NS m ));
let r being Real;
reconsider x1 = x as (Element of ( REAL m )) by REAL_NS1:def 4;
( g . ( r * x ) ) = ( f . ( r * x1 ) ) by A1,REAL_NS1:3
.= ( r * ( f . x1 ) ) by A2,CKB12:def 1;
hence ( g . ( r * x ) ) = ( r * ( g . x ) ) by A1,REAL_NS1:3;
end;
hence g is  homogeneous by LOPBAN_1:def 5;
end;
assume A3: g is  homogeneous;
now
let x being (Element of ( REAL m ));
let r being Real;
reconsider x1 = x as (Point of ( REAL-NS m )) by REAL_NS1:def 4;
( f . ( r * x ) ) = ( g . ( r * x1 ) ) by A1,REAL_NS1:3
.= ( r * ( g . x1 ) ) by A3,LOPBAN_1:def 5;
hence ( f . ( r * x ) ) = ( r * ( f . x ) ) by A1,REAL_NS1:3;
end;
hence f is  homogeneous by CKB12:def 1;
end;
