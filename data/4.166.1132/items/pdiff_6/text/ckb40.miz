begin
reserve n for (Element of ( NAT ));
reserve m for (Element of ( NAT ));
theorem
Th30: (for X being set holds (for m,n being non  empty (Element of ( NAT )) holds (for f being (PartFunc of ( REAL m ),( REAL n )) holds (for g being (PartFunc of ( REAL-NS m ),( REAL-NS n )) holds (f = g implies (f is_differentiable_on X iff g is_differentiable_on X))))))
proof
let X being set;
let m being non  empty (Element of ( NAT ));
let n being non  empty (Element of ( NAT ));
let f being (PartFunc of ( REAL m ),( REAL n ));
let g being (PartFunc of ( REAL-NS m ),( REAL-NS n ));
assume A1: f = g;
A2:now
assume A3: f is_differentiable_on X;
then A4: (X c= ( dom f ) & (for x being (Element of ( REAL m )) holds (x in X implies ( f | X ) is_differentiable_in x))) by CKB39:def 1;
now
let y being (Point of ( REAL-NS m ));
reconsider x = y as (Element of ( REAL m )) by REAL_NS1:def 4;
assume y in X;
then ( f | X ) is_differentiable_in x by A3,CKB39:def 1;
then (ex g being (PartFunc of ( REAL-NS m ),( REAL-NS n )) st (ex y being (Point of ( REAL-NS m )) st ((( f | X ) = g & x = y) & g is_differentiable_in y))) by PDIFF_1:def 7;
hence ( g | X ) is_differentiable_in y by A1;
end;
hence g is_differentiable_on X by A1,A4,NDIFF_1:def 8;
end;
now
assume A5: g is_differentiable_on X;
hence X c= ( dom f ) by A1,NDIFF_1:def 8;
A6: (X c= ( dom g ) & (for x being (Point of ( REAL-NS m )) holds (x in X implies ( g | X ) is_differentiable_in x))) by A5,NDIFF_1:def 8;
now
let y being (Element of ( REAL m ));
reconsider x = y as (Point of ( REAL-NS m )) by REAL_NS1:def 4;
assume y in X;
then ( g | X ) is_differentiable_in x by A5,NDIFF_1:def 8;
hence ( f | X ) is_differentiable_in y by A1,PDIFF_1:def 7;
end;
hence f is_differentiable_on X by A1,A6,CKB39:def 1;
end;
hence thesis by A2;
end;
