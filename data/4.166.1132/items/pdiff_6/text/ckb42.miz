begin
reserve n for (Element of ( NAT ));
reserve m for (Element of ( NAT ));
theorem
(for m,n being non  empty (Element of ( NAT )) holds (for f being (PartFunc of ( REAL m ),( REAL n )) holds (for Z being (Subset of ( REAL m )) holds ((ex Z0 being (Subset of ( REAL-NS m )) st (Z = Z0 & Z0 is  open)) implies (f is_differentiable_on Z iff (Z c= ( dom f ) & (for x being (Element of ( REAL m )) holds (x in Z implies f is_differentiable_in x))))))))
proof
let m being non  empty (Element of ( NAT ));
let n being non  empty (Element of ( NAT ));
let f being (PartFunc of ( REAL m ),( REAL n ));
let Z being (Subset of ( REAL m ));
assume (ex Z0 being (Subset of ( REAL-NS m )) st (Z = Z0 & Z0 is  open));
then consider Z0 being (Subset of ( REAL-NS m )) such that A1: (Z = Z0 & Z0 is  open);
((the carrier of ( REAL-NS m )) = ( REAL m ) & (the carrier of ( REAL-NS n )) = ( REAL n )) by REAL_NS1:def 4;
then reconsider g = f as (PartFunc of ( REAL-NS m ),( REAL-NS n ));
A2: (g is_differentiable_on Z0 iff (Z0 c= ( dom g ) & (for x being (Point of ( REAL-NS m )) holds (x in Z0 implies g is_differentiable_in x)))) by A1,NDIFF_1:31;
thus (f is_differentiable_on Z implies (Z c= ( dom f ) & (for x being (Element of ( REAL m )) holds (x in Z implies f is_differentiable_in x))))
proof
assume A3: f is_differentiable_on Z;
hence Z c= ( dom f ) by CKB39:def 1;
let x being (Element of ( REAL m ));
reconsider y = x as (Point of ( REAL-NS m )) by REAL_NS1:def 4;
assume x in Z;
then g is_differentiable_in y by A1,A2,A3,CKB40:1;
hence thesis by PDIFF_1:def 7;
end;

assume A4: (Z c= ( dom f ) & (for x being (Element of ( REAL m )) holds (x in Z implies f is_differentiable_in x)));
now
let y being (Point of ( REAL-NS m ));
reconsider x = y as (Element of ( REAL m )) by REAL_NS1:def 4;
assume y in Z0;
then f is_differentiable_in x by A1,A4;
then (ex g being (PartFunc of ( REAL-NS m ),( REAL-NS n )) st (ex y being (Point of ( REAL-NS m )) st ((f = g & x = y) & g is_differentiable_in y))) by PDIFF_1:def 7;
hence g is_differentiable_in y;
end;
hence f is_differentiable_on Z by A1,A2,A4,CKB40:1;
end;
