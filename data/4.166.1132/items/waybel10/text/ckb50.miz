requirements SUBSET,BOOLE;
begin
registration
let L being  complete LATTICE;
cluster ( ClImageMap L ) ->  isomorphic;
correctness
proof
set f = ( ClImageMap L );
set S = ( ClOpers L );
set T = ( ( ClosureSystems L ) opp );
reconsider g = ( f " ) as (Function of T,S) by CKB48:1;
per cases ;
case (S is non  empty & T is non  empty);
thus f is  one-to-one;
thus f is  monotone
proof
let x being (Element of S);
let y being (Element of S);
reconsider c = x,d = y as  closure (Function of L,L) by CKB24:1;
A1: ( f . y ) = ( Image d ) by CKB42:def 1;
assume x <= y;
then c <= d by CKB26:1;
then A2: ( Image d ) is (SubRelStr of ( Image c )) by CKB28:1;
( f . x ) = ( Image c ) by CKB42:def 1;
then ( ~ ( f . x ) ) >= ( ~ ( f . y ) ) by A2,A1,CKB40:1;
hence ( f . x ) <= ( f . y ) by YELLOW_7:1;
end;

take g;
thus g = ( f " );
thus g is  monotone
proof
let x being (Element of T);
let y being (Element of T);
reconsider A = ( ~ x ),B = ( ~ y ) as  infs-inheriting  full  strict (SubRelStr of L) by CKB39:1;
A3: B = ( Image ( closure_op B ) ) by CKB45:1;
A4: ( g . A ) = ( closure_op A ) by CKB49:1;
assume x <= y;
then ( ~ y ) <= ( ~ x ) by YELLOW_7:1;
then A5: B is (SubRelStr of A) by CKB40:1;
A6: ( g . B ) = ( closure_op B ) by CKB49:1;
A = ( Image ( closure_op A ) ) by CKB45:1;
then ( closure_op A ) <= ( closure_op B ) by A5,A3,CKB28:1;
hence ( g . x ) <= ( g . y ) by A4,A6,CKB26:1;
end;

end;
case (S is  empty or T is  empty);
hence thesis;
end;
end;
end;
