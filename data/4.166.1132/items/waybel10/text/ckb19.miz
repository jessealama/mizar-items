begin
registration
let S being non empty  reflexive RelStr;
let T being non empty  reflexive RelStr;
let f being  monotone (Function of S,T);
let R being (SubRelStr of S);
cluster ( f | R ) ->  monotone;
coherence
proof
let x being (Element of R),y being (Element of R);
A1: (the carrier of R) c= (the carrier of S) by YELLOW_0:def 13;
assume A2: x <= y;
then A3: [ x,y ] in (the InternalRel of R) by ORDERS_2:def 5;
then A4: x in (the carrier of R) by ZFMISC_1:87;
y in (the carrier of R) by A3,ZFMISC_1:87;
then reconsider a = x,b = y as (Element of S) by A4,A1;
A5: a <= b by A2,YELLOW_0:59;
A6: ( f . b ) = ( ( f | R ) . y ) by A4,CKB17:1;
( f . a ) = ( ( f | R ) . x ) by A4,CKB17:1;
hence thesis by A5,A6,WAYBEL_1:def 2;
end;
end;
