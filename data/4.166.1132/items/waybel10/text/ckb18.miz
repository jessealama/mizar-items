begin
theorem
Th8: (for S,T being RelStr holds (for f being (Function of S,T) holds (f is  one-to-one implies (for R being (SubRelStr of S) holds ( f | R ) is  one-to-one))))
proof
let S being RelStr;
let T being RelStr;
let f being (Function of S,T);
assume that
A1: f is  one-to-one;
let R being (SubRelStr of S);
( f | R ) = ( f | (the carrier of R) ) by CKB17:1;
hence thesis by A1,FUNCT_1:52;
end;
