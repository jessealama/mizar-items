begin
reserve X for non  empty TopSpace;
theorem
Th39: (for X being non  empty TopSpace holds (for A,B being (Subset of X) holds (for U being (a_neighborhood of B) holds (A c= B implies U is (a_neighborhood of A)))))
proof
let X being non  empty TopSpace;
let A being (Subset of X);
let B being (Subset of X);
let U being (a_neighborhood of B);
assume that
A1: A c= B;
B c= ( Int U ) by CONNSP_2:def 2;
hence A c= ( Int U ) by A1,XBOOLE_1:1;
end;
