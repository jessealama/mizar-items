begin
reserve e for set;
reserve X for non empty TopSpace;
reserve Y for non empty TopSpace;
theorem
Th56: (for A being (Subset of [: X,Y :]) holds (for H being (Subset-Family of [: X,Y :]) holds ((for e holds (e in H implies (e c= A & (ex X1 being (Subset of X) st (ex Y1 being (Subset of Y) st e = [: X1,Y1 :]))))) implies [: ( union ( ( Pr1 (X,Y) ) .: H ) ),( meet ( ( Pr2 (X,Y) ) .: H ) ) :] c= A)))
proof
let A being (Subset of [: X,Y :]);
let H being (Subset-Family of [: X,Y :]);
assume A1: (for e holds (e in H implies (e c= A & (ex X1 being (Subset of X) st (ex Y1 being (Subset of Y) st e = [: X1,Y1 :])))));
(the carrier of [: X,Y :]) = [: (the carrier of X),(the carrier of Y) :] by CKB9:def 1;
hence thesis by A1,EQREL_1:51;
end;
