environ
vocabularies NUMBERS,SUBSET_1,FUNCT_1,TARSKI,RELAT_1,ZFMISC_1,MCART_1,XBOOLE_0,SETFAM_1,EQREL_1,CARD_3,PRE_TOPC,STRUCT_0,ORDINAL2,CONNSP_2,TOPS_1,RCOMP_1,FINSET_1,PCOMPS_1,METRIC_1,XXREAL_1,CARD_1,XREAL_0,ORDINAL1,XXREAL_0,REAL_1,BORSUK_1;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,ORDINAL1,NUMBERS,XXREAL_0,XREAL_0,REAL_1,FINSET_1,RELAT_1,FUNCT_1,MCART_1,DOMAIN_1,RCOMP_1,SETFAM_1,EQREL_1,FUNCT_3,RELSET_1,PARTFUN1,FUNCT_2,BINOP_1,STRUCT_0,METRIC_1,PCOMPS_1,PRE_TOPC,TOPS_1,TOPS_2,COMPTS_1,CONNSP_2,CKB2,CKB6,CKB9,CKB12,CKB13,CKB18,CKB20,CKB21,CKB27,CKB28;
definitions TARSKI,PRE_TOPC,TOPS_2,CONNSP_2,COMPTS_1,EQREL_1,STRUCT_0,XBOOLE_0,SUBSET_1,BINOP_1,RELAT_1,SETFAM_1,CKB2,CKB9,CKB21,CKB27,CKB28;
theorems TARSKI,ZFMISC_1,DOMAIN_1,PRE_TOPC,FUNCT_1,FUNCT_2,TOPS_1,EQREL_1,FUNCT_3,SUBSET_1,TOPS_2,CONNSP_2,COMPTS_1,SETFAM_1,PCOMPS_1,METRIC_1,RCOMP_1,MCART_1,RELAT_1,XBOOLE_0,XBOOLE_1,XREAL_0,XXREAL_1,CKB1,CKB2,CKB4,CKB5,CKB7,CKB9,CKB11,CKB14,CKB15,CKB16,CKB17,CKB19,CKB21,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB37,CKB38;
schemes CLASSES1,DOMAIN_1;
registrations XBOOLE_0,SUBSET_1,RELAT_1,FUNCT_1,PARTFUN1,FUNCT_2,FINSET_1,XREAL_0,MEMBERED,EQREL_1,STRUCT_0,PRE_TOPC,TOPS_1,METRIC_1,PCOMPS_1,RELSET_1,CKB3,CKB8,CKB10,CKB22;
constructors SETFAM_1,DOMAIN_1,FUNCT_3,FINSET_1,MEMBERED,EQREL_1,RCOMP_1,TOPS_1,TOPS_2,COMPTS_1,CONNSP_2,PCOMPS_1,RELSET_1,CKB6,CKB9,CKB12,CKB13,CKB18,CKB20,CKB21,CKB27,CKB28;
requirements NUMERALS,REAL,BOOLE,SUBSET;
begin
reserve e for set;
reserve X for non  empty TopSpace;
reserve Y for non  empty TopSpace;
theorem
Th67: (for t being (Point of Y) holds (for A being (Subset of X) holds (A is  compact implies (for G being (a_neighborhood of [: A,{ t } :]) holds (ex V being (a_neighborhood of A) st (ex W being (a_neighborhood of t) st [: V,W :] c= G))))))
proof
let t being (Point of Y);
let A being (Subset of X);
assume that
A1: A is  compact;
let G being (a_neighborhood of [: A,{ t } :]);
A2: (the carrier of [: X,Y :]) = [: (the carrier of X),(the carrier of Y) :] by CKB9:def 1;
now
per cases ;
suppose A3: A <> ( {} X );

[: A,{ t } :] c= ( Int G ) by CONNSP_2:def 2;
then [: A,{ t } :] c= ( union ( Base-Appr G ) ) by CKB26:1;
then ( Base-Appr G ) is (Cover of [: A,{ t } :]) by SETFAM_1:def 11;
then consider K being (Subset-Family of [: X,Y :]) such that A4: K c= ( Base-Appr G ) and A5: K is (Cover of [: A,{ t } :]) and A6: (for c being set holds (c in K implies c meets [: A,{ t } :])) by CKB36:1;
reconsider PK = ( ( Pr1 (X,Y) ) .: K ) as (Subset-Family of X);
K is  open by A4,TOPS_2:11;
then A7: ( ( Pr1 (X,Y) ) .: K ) is  open by CKB33:1;
PK is (Cover of A) by A5,CKB35:1;
then consider M being (Subset-Family of X) such that A8: M c= ( ( Pr1 (X,Y) ) .: K ) and A9: M is (Cover of A) and A10: M is  finite by A1,A7,COMPTS_1:def 4;
consider N being (Subset-Family of [: X,Y :]) such that A11: N c= K and A12: N is  finite and A13: ( ( Pr1 (X,Y) ) .: N ) = M by A8,A10,CKB37:1;
reconsider F = ( ( Pr1 (X,Y) ) .: N ) as (Subset-Family of X);
A14: N is (Cover of [: A,{ t } :])
proof
let e1 being set;
let e2 being set;
A15: A c= ( union M ) by A9,SETFAM_1:def 11;
assume A16: [ e1,e2 ] in [: A,{ t } :];
then ( [ e1,e2 ] `2 ) in { t } by MCART_1:10;
then ( [ e1,e2 ] `2 ) = t by TARSKI:def 1;
then A17: [ e1,e2 ] = [ ( [ e1,e2 ] `1 ),t ] by A16,MCART_1:21;
( [ e1,e2 ] `1 ) in A by A16,MCART_1:10;
then consider X1 being set such that A18: ( [ e1,e2 ] `1 ) in X1 and A19: X1 in M by A15,TARSKI:def 4;
consider XY being (Subset of [: X,Y :]) such that A20: XY in N and A21: ( ( Pr1 (X,Y) ) . XY ) = X1 by A13,A19,FUNCT_2:65;
XY in K by A11,A20;
then XY in ( Base-Appr G ) by A4;
then consider X2 being (Subset of X),Y2 being (Subset of Y) such that A22: XY = [: X2,Y2 :] and [: X2,Y2 :] c= G and X2 is  open and Y2 is  open;
XY meets [: A,{ t } :] by A6,A11,A20;
then consider xy being set such that A23: xy in XY and A24: xy in [: A,{ t } :] by XBOOLE_0:3;
( xy `2 ) in { t } by A24,MCART_1:10;
then ( xy `2 ) = t by TARSKI:def 1;
then A25: t in Y2 by A22,A23,MCART_1:10;
XY <> ( {} ) by A18,A21,FUNCT_3:8;
then ( [ e1,e2 ] `1 ) in X2 by A18,A21,A22,EQREL_1:50;
then [ e1,e2 ] in [: X2,Y2 :] by A25,A17,ZFMISC_1:87;
hence [ e1,e2 ] in ( union N ) by A20,A22,TARSKI:def 4;
end;
then F is (Cover of A) by CKB35:1;
then A26: A c= ( union F ) by SETFAM_1:def 11;
reconsider H = ( ( Pr2 (X,Y) ) .: N ) as (Subset-Family of Y);
A27:now
let C being set;
assume C in H;
then consider D being (Subset of [: X,Y :]) such that A28: D in N and A29: C = ( ( pr2 ((the carrier of X),(the carrier of Y)) ) .: D ) by CKB31:1;
D meets [: A,{ t } :] by A6,A11,A28;
then ( D /\ [: A,{ t } :] ) <> ( {} ) by XBOOLE_0:def 7;
then consider x being (Point of [: X,Y :]) such that A30: x in ( D /\ [: A,{ t } :] ) by SUBSET_1:4;
A31: x in [: A,{ t } :] by A30,XBOOLE_0:def 4;
then ( x `1 ) in A by MCART_1:10;
then A32: ( ( pr2 ((the carrier of X),(the carrier of Y)) ) . (( x `1 ),t) ) = t by FUNCT_3:def 5;
( x `2 ) in { t } by A31,MCART_1:10;
then ( x `2 ) = t by TARSKI:def 1;
then A33: x = [ ( x `1 ),t ] by A2,MCART_1:21;
x in D by A30,XBOOLE_0:def 4;
hence t in C by A2,A29,A33,A32,FUNCT_2:35;
end;
[: A,{ t } :] c= ( union N ) by A14,SETFAM_1:def 11;
then N <> ( {} ) by A3,ZFMISC_1:2;
then H <> ( {} ) by CKB34:1;
then A34: t in ( meet H ) by A27,SETFAM_1:def 1;
A35: N c= ( Base-Appr G ) by A4,A11,XBOOLE_1:1;
then A36: N is  open by TOPS_2:11;
then ( meet H ) is  open by A12,CKB33:1,TOPS_2:20;
then t in ( Int ( meet H ) ) by A34,TOPS_1:23;
then reconsider W = ( meet H ) as (a_neighborhood of t) by CONNSP_2:def 1;
( union F ) is  open by A36,CKB33:1,TOPS_2:19;
then A c= ( Int ( union F ) ) by A26,TOPS_1:23;
then reconsider V = ( union F ) as (a_neighborhood of A) by CONNSP_2:def 2;
take V;
take W;
now
let e;
assume e in N;
then e in ( Base-Appr G ) by A35;
then (ex X1 being (Subset of X) st (ex Y1 being (Subset of Y) st (((e = [: X1,Y1 :] & [: X1,Y1 :] c= G) & X1 is  open) & Y1 is  open)));
hence (e c= G & (ex X1 being (Subset of X) st (ex Y1 being (Subset of Y) st e = [: X1,Y1 :])));
end;
hence [: V,W :] c= G by CKB29:1;
end;
suppose A = ( {} X );

then A c= ( Int ( {} X ) );
then reconsider V = ( {} X ) as (a_neighborhood of A) by CONNSP_2:def 2;
set W = the (a_neighborhood of t);
take V;
take W;
[: V,W :] = ( {} ) by ZFMISC_1:90;
hence [: V,W :] c= G by XBOOLE_1:2;
end;
end;
hence thesis;
end;
