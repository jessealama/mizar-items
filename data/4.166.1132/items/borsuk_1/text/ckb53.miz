begin
reserve X for non  empty TopSpace;
theorem
Th75: (for XX being non  empty TopSpace holds (for X being non  empty (SubSpace of XX) holds (for D being non  empty (a_partition of (the carrier of X)) holds (for x being (Point of XX) holds ((not x in (the carrier of X)) implies { x } in ( TrivExt D ))))))
proof
let XX being non  empty TopSpace;
let X being non  empty (SubSpace of XX);
let D being non  empty (a_partition of (the carrier of X));
let x being (Point of XX);
( union ( TrivExt D ) ) = (the carrier of XX) by EQREL_1:def 4;
then consider A being set such that A1: x in A and A2: A in ( TrivExt D ) by TARSKI:def 4;
assume (not x in (the carrier of X));
then (not A in D) by A1;
then A in { { p } where p is (Point of XX): (not p in (the carrier of X)) } by A2,XBOOLE_0:def 3;
then (ex p being (Point of XX) st (A = { p } & (not p in (the carrier of X))));
hence thesis by A1,A2,TARSKI:def 1;
end;
