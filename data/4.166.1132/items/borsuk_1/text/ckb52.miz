begin
theorem
Th74: (for XX being non empty TopSpace holds (for X being non empty (SubSpace of XX) holds (for D being non empty (a_partition of (the carrier of X)) holds (for A being (Subset of XX) holds (A in ( TrivExt D ) implies (A in D or (ex x being (Point of XX) st ((not x in ( [#] X )) & A = { x }))))))))
proof
let XX being non empty TopSpace;
let X being non empty (SubSpace of XX);
let D being non empty (a_partition of (the carrier of X));
let A being (Subset of XX);
assume A1: A in ( TrivExt D );
now
per cases  by A1,XBOOLE_0:def 3;
case A in D;
hence A in D;
end;
case A in { { p } where p is (Point of XX): (not p in (the carrier of X)) };
then consider x being (Point of XX) such that A2: A = { x } and A3: (not x in (the carrier of X));
take x;
thus (not x in ( [#] X )) by A3;
thus A = { x } by A2;
end;
end;
hence thesis;
end;
