begin
reserve X for non empty TopSpace;
reserve Y for non empty TopSpace;
theorem
Th38: (for W being (Point of Y) holds (for A being  continuous (Function of X,Y) holds (for G being (a_neighborhood of W) holds ( A " G ) is (a_neighborhood of ( A " { W } )))))
proof
let W being (Point of Y);
let A being  continuous (Function of X,Y);
let G being (a_neighborhood of W);
W in ( Int G ) by CONNSP_2:def 1;
then { W } c= ( Int G ) by ZFMISC_1:31;
then A1: ( A " { W } ) c= ( A " ( Int G ) ) by RELAT_1:143;
( A " ( Int G ) ) c= ( Int ( A " G ) ) by CKB4:1;
hence ( A " { W } ) c= ( Int ( A " G ) ) by A1,XBOOLE_1:1;
end;
