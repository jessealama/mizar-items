begin
reserve e for set;
reserve X for non  empty TopSpace;
reserve Y for non  empty TopSpace;
theorem
Th46: (for X,Y being TopSpace holds (for V being (Subset of X) holds (for W being (Subset of Y) holds ((V is  open & W is  open) implies [: V,W :] is  open))))
proof
let X being TopSpace;
let Y being TopSpace;
let V being (Subset of X);
let W being (Subset of Y);
assume that
A1: (V is  open & W is  open);
reconsider PP = { [: V,W :] } as (Subset-Family of [: X,Y :]);
reconsider PP as (Subset-Family of [: X,Y :]);
A2:now
let e;
assume A3: e in PP;
take V;
take W;
thus ((e = [: V,W :] & V is  open) & W is  open) by A1,A3,TARSKI:def 1;
end;
[: V,W :] = ( union { [: V,W :] } ) by ZFMISC_1:25;
hence thesis by A2,CKB11:1;
end;
