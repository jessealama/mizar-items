constructors SETFAM_1,DOMAIN_1,FUNCT_3,FINSET_1,MEMBERED,EQREL_1,RCOMP_1,TOPS_1,TOPS_2,COMPTS_1,CONNSP_2,PCOMPS_1,RELSET_1,CKB6,CKB9,CKB12,CKB13;
requirements NUMERALS,REAL,BOOLE,SUBSET;
begin
reserve X for non empty TopSpace;
reserve Y for non empty TopSpace;
theorem
Th49: (for A being (Subset of X) holds (for B being (Subset of Y) holds (for V being (a_neighborhood of A) holds (for W being (a_neighborhood of B) holds [: V,W :] is (a_neighborhood of [: A,B :])))))
proof
let A being (Subset of X);
let B being (Subset of Y);
let V being (a_neighborhood of A);
let W being (a_neighborhood of B);
(A c= ( Int V ) & B c= ( Int W )) by CONNSP_2:def 2;
then [: A,B :] c= [: ( Int V ),( Int W ) :] by ZFMISC_1:96;
hence [: A,B :] c= ( Int [: V,W :] ) by CKB15:1;
end;
