requirements NUMERALS,REAL,BOOLE,SUBSET;
begin
reserve e for set;
reserve u for set;
reserve X for non  empty TopSpace;
reserve Y for non  empty TopSpace;
theorem
Th64: (for F being (Subset-Family of X) holds (for H being (Subset-Family of [: X,Y :]) holds ((F is  finite & F c= ( ( Pr1 (X,Y) ) .: H )) implies (ex G being (Subset-Family of [: X,Y :]) st ((G c= H & G is  finite) & F = ( ( Pr1 (X,Y) ) .: G ))))))
proof
let F being (Subset-Family of X);
let H being (Subset-Family of [: X,Y :]);
assume that
A1: F is  finite
and
A2: F c= ( ( Pr1 (X,Y) ) .: H );
defpred P[ set,set ]
 means
(($2 in ( dom ( Pr1 (X,Y) ) ) & $2 in H) & $1 = ( ( Pr1 (X,Y) ) . $2 ));
A3: (for e holds (e in F implies (ex u st P[ e,u ]))) by A2,FUNCT_1:def 6;
consider f being Function such that A4: ( dom f ) = F and A5: (for e holds (e in F implies P[ e,( f . e ) ])) from CLASSES1:sch 1(A3);
A6: ( f .: F ) c= H
proof
let e;
assume e in ( f .: F );
then (ex u st ((u in ( dom f ) & u in F) & e = ( f . u ))) by FUNCT_1:def 6;
hence thesis by A5;
end;
then reconsider G = ( f .: F ) as (Subset-Family of [: X,Y :]) by XBOOLE_1:1;
take G;
thus G c= H by A6;
thus G is  finite by A1;
now
let e;
thus (e in F iff (ex u st ((u in ( dom ( Pr1 (X,Y) ) ) & u in G) & e = ( ( Pr1 (X,Y) ) . u ))))
proof
thus (e in F implies (ex u st ((u in ( dom ( Pr1 (X,Y) ) ) & u in G) & e = ( ( Pr1 (X,Y) ) . u ))))
proof
assume A7: e in F;
take ( f . e );
thus ( f . e ) in ( dom ( Pr1 (X,Y) ) ) by A5,A7;
thus ( f . e ) in G by A4,A7,FUNCT_1:def 6;
thus thesis by A5,A7;
end;

given u such that
u in ( dom ( Pr1 (X,Y) ) )
and
A8: u in G
and
A9: e = ( ( Pr1 (X,Y) ) . u );

(ex v being set st ((v in ( dom f ) & v in F) & u = ( f . v ))) by A8,FUNCT_1:def 6;
hence thesis by A5,A9;
end;

end;
hence thesis by FUNCT_1:def 6;
end;
