begin
reserve n for (Element of ( NAT ));
reserve k for (Element of ( NAT ));
reserve x for set;
reserve X for set;
reserve A1 for (SetSequence of X);
reserve A2 for (SetSequence of X);
reserve A3 for (SetSequence of X);
theorem
((for n holds ( A3 . n ) = ( ( A1 . n ) \/ ( A2 . n ) )) implies (for n holds ( ( ( inferior_setsequence A1 ) . n ) \/ ( ( inferior_setsequence A2 ) . n ) ) c= ( ( inferior_setsequence A3 ) . n )))
proof
assume A1: (for n holds ( A3 . n ) = ( ( A1 . n ) \/ ( A2 . n ) ));
let n;
set X1 = ( inferior_setsequence A1 );
set X2 = ( inferior_setsequence A2 );
set X3 = ( inferior_setsequence A3 );
now
let x;
assume A2: x in ( ( X1 . n ) \/ ( X2 . n ) );
per cases  by A2,XBOOLE_0:def 3;
suppose A3: x in ( X1 . n );

now
let k;
(x in ( A1 . ( n + k ) ) & ( A3 . ( n + k ) ) = ( ( A1 . ( n + k ) ) \/ ( A2 . ( n + k ) ) )) by A1,A3,CKB26:1;
hence x in ( A3 . ( n + k ) ) by XBOOLE_0:def 3;
end;
hence x in ( X3 . n ) by CKB26:1;
end;
suppose A4: x in ( X2 . n );

now
let k;
(x in ( A2 . ( n + k ) ) & ( A3 . ( n + k ) ) = ( ( A1 . ( n + k ) ) \/ ( A2 . ( n + k ) ) )) by A1,A4,CKB26:1;
hence x in ( A3 . ( n + k ) ) by XBOOLE_0:def 3;
end;
hence x in ( X3 . n ) by CKB26:1;
end;
end;
hence thesis by TARSKI:def 3;
end;
