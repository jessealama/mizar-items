reserve k for (Element of ( NAT ));
definition
let X being set;
let B being (SetSequence of X);
redefine func lim_sup B
equals
( Intersection ( superior_setsequence B ) );
compatibility
proof
let F being (Subset of X);
hereby
assume A1: F = ( lim_sup B );
(for x being set holds (x in F iff x in ( Intersection ( superior_setsequence B ) )))
proof
let x being set;
hereby
assume A2: x in F;
(for m holds x in ( ( superior_setsequence B ) . m ))
proof
let m;
(ex k st x in ( B . ( m + k ) )) by A1,A2,KURATO_0:5;
hence thesis by CKB27:1;
end;
hence x in ( Intersection ( superior_setsequence B ) ) by PROB_1:13;
end;
assume A3: x in ( Intersection ( superior_setsequence B ) );
now
let m;
x in ( ( superior_setsequence B ) . m ) by A3,PROB_1:13;
hence (ex k st x in ( B . ( m + k ) )) by CKB27:1;
end;
hence thesis by A1,KURATO_0:5;
end;
hence F = ( Intersection ( superior_setsequence B ) ) by TARSKI:1;
end;
