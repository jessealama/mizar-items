begin
reserve D for non empty set;
reserve n for (Element of ( NAT ));
theorem
Th34: (for p1,p2,p3 being (FinSequence of D) holds (((( len p1 ) = n & ( len p2 ) = n) & ( len p3 ) = n) implies <* p1,p2,p3 *> is (Matrix of 3,n,D)))
proof
let p1 being (FinSequence of D),p2 being (FinSequence of D),p3 being (FinSequence of D);
reconsider q1 = p1,q2 = p2,q3 = p3 as (Element of ( D * )) by FINSEQ_1:def 11;
reconsider M = <* q1,q2,q3 *> as (FinSequence of ( D * ));
assume A1: ((( len p1 ) = n & ( len p2 ) = n) & ( len p3 ) = n);
then reconsider M as (Matrix of D) by CKB39:1;
M is (Matrix of 3,n,D)
proof
thus ( len M ) = 3 by FINSEQ_1:45;
let r being (FinSequence of D);
assume r in ( rng M );
then r in { p1,p2,p3 } by FINSEQ_2:128;
hence thesis by A1,ENUMSET1:def 1;
end;
hence thesis;
end;
