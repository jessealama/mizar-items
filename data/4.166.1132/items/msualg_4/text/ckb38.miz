registrations RELAT_1,FUNCT_1,PARTFUN1,FUNCOP_1,PBOOLE,STRUCT_0,MSUALG_1,MSUALG_3,CARD_3,RELSET_1,FINSEQ_2,FINSEQ_1,CKB1,CKB3,CKB9,CKB12,CKB18,CKB19,CKB29;
constructors EQREL_1,PRALG_2,MSUALG_3,RELSET_1,CKB2,CKB5,CKB6,CKB8,CKB11,CKB14,CKB15,CKB16,CKB17,CKB20,CKB21,CKB22,CKB23,CKB24,CKB26,CKB27,CKB28,CKB30,CKB31,CKB33,CKB34,CKB35,CKB36;
requirements SUBSET,BOOLE;
begin
reserve S for non  void non  empty ManySortedSign;
reserve U1 for MSAlgebra over S;
theorem
Th5: (for U1,U2 being  non-empty MSAlgebra over S holds (for F being (ManySortedFunction of U1,U2) holds (F is_epimorphism U1,U2 implies ( MSHomQuot F ) is_isomorphism ( QuotMSAlg (U1,( MSCng F )) ),U2)))
proof
let U1 being  non-empty MSAlgebra over S;
let U2 being  non-empty MSAlgebra over S;
let F being (ManySortedFunction of U1,U2);
set mc = ( MSCng F );
set qa = ( QuotMSAlg (U1,mc) );
set qh = ( MSHomQuot F );
set Sq = (the Sorts of qa);
set S1 = (the Sorts of U1);
set S2 = (the Sorts of U2);
assume A1: F is_epimorphism U1,U2;
then A2: F is_homomorphism U1,U2 by MSUALG_3:def 8;
A3: F is  "onto" by A1,MSUALG_3:def 8;
(for i being set holds (i in (the carrier of S) implies ( rng ( qh . i ) ) = ( S2 . i )))
proof
let i being set;
set f = ( qh . i );
assume i in (the carrier of S);
then reconsider s = i as (SortSymbol of S);
A4: ( rng ( F . s ) ) = ( S2 . s ) by A3,MSUALG_3:def 3;
A5: ( qh . i ) = ( MSHomQuot (F,s) ) by CKB36:def 1;
hence ( rng f ) c= ( S2 . i ) by RELAT_1:def 19;
let x being set;
assume x in ( S2 . i );
then consider a being set such that A6: a in ( dom ( F . s ) ) and A7: ( ( F . s ) . a ) = x by A4,FUNCT_1:def 3;
A8: (( MSCng (F,s) ) = ( ( MSCng F ) . s ) & ( Sq . s ) = ( Class ( ( MSCng F ) . s ) )) by A2,CKB16:def 1,CKB34:def 1;
reconsider a as (Element of ( S1 . s )) by A6;
( dom f ) = ( Sq . s ) by A5,FUNCT_2:def 1;
then A9: ( Class (( MSCng (F,s) ),a) ) in ( dom f ) by A8,EQREL_1:def 3;
( f . ( Class (( MSCng (F,s) ),a) ) ) = x by A2,A5,A7,CKB35:def 1;
hence thesis by A9,FUNCT_1:def 3;
end;
then A10: qh is  "onto" by MSUALG_3:def 3;
qh is_monomorphism qa,U2 by A2,CKB37:1;
then (qh is_homomorphism qa,U2 & qh is  "1-1") by MSUALG_3:def 9;
hence thesis by A10,MSUALG_3:13;
end;
