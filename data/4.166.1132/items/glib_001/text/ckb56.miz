begin
theorem
Lm17: (for G being _Graph holds (for W being (Walk of G) holds (for m,n,o being  odd (Element of ( NAT )) holds (((m <= n & n <= o) & o <= ( len W )) implies ( ( W .cut (m,n) ) .append ( W .cut (n,o) ) ) = ( W .cut (m,o) )))))
proof
let G being _Graph;
let W being (Walk of G);
let m being  odd (Element of ( NAT ));
let n being  odd (Element of ( NAT ));
let o being  odd (Element of ( NAT ));
assume that
A1: m <= n
and
A2: n <= o
and
A3: o <= ( len W );
set W1 = ( W .cut (m,n) );
set W2 = ( W .cut (n,o) );
set W3 = ( W .cut (m,o) );
set W4 = ( W1 .append W2 );
A4: n <= ( len W ) by A2,A3,XXREAL_0:2;
A5: m <= o by A1,A2,XXREAL_0:2;
now
A6: ( ( len W3 ) + m ) = ( o + 1 ) by A3,A5,CKB54:1;
A7: ( W1 .last() ) = ( W . n ) by A1,A4,CKB55:1
.= ( W2 .first() ) by A2,A3,CKB55:1;
A8: ( ( len W1 ) + m ) = ( n + 1 ) by A1,A4,CKB54:1;
A9: ( ( len W2 ) + n ) = ( o + 1 ) by A2,A3,CKB54:1;
then ( ( ( len W1 ) + ( len W2 ) ) + m ) = ( ( 1 + ( len W3 ) ) + m ) by A8,A6;
then A10: ( ( len W4 ) + 1 ) = ( ( len W3 ) + 1 ) by A7,CKB48:1;
hence (( len W4 ) = ( len W4 ) & ( len W3 ) = ( len W4 ));
let x being Nat;
assume A11: x in ( dom W4 );
then A12: 1 <= x by FINSEQ_3:25;
then reconsider xaa1 = ( x - 1 ) as (Element of ( NAT )) by INT_1:5;
A13: x <= ( len W4 ) by A11,FINSEQ_3:25;
then xaa1 < ( ( len W4 ) - ( 0 ) ) by XREAL_1:15;
then A14: ( W3 . ( xaa1 + 1 ) ) = ( W . ( m + xaa1 ) ) by A3,A5,A10,CKB54:1;
now
per cases ;
suppose A15: x <= ( len W1 );

then A16: xaa1 < ( ( len W1 ) - ( 0 ) ) by XREAL_1:15;
x in ( dom W1 ) by A12,A15,FINSEQ_3:25;
hence ( W4 . x ) = ( W1 . ( xaa1 + 1 ) ) by CKB51:1
.= ( W3 . x ) by A1,A4,A14,A16,CKB54:1;
end;
suppose x > ( len W1 );

then consider k being Nat such that A17: ( ( len W1 ) + k ) = x by NAT_1:10;
reconsider k as (Element of ( NAT )) by ORDINAL1:def 12;
( ( ( len W1 ) + k ) + 1 ) <= ( ( len W3 ) + 1 ) by A10,A13,A17,XREAL_1:7;
then ( ( ( k + 1 ) + ( len W1 ) ) - ( len W1 ) ) <= ( ( ( len W2 ) + ( len W1 ) ) - ( len W1 ) ) by A8,A9,A6,XREAL_1:13;
then A18: ( ( k + 1 ) - 1 ) < ( ( ( len W2 ) + 1 ) - 1 ) by NAT_1:13;
then ( W4 . x ) = ( W2 . ( k + 1 ) ) by A7,A17,CKB52:1
.= ( W . ( n + k ) ) by A2,A3,A18,CKB54:1;
hence ( W4 . x ) = ( W3 . x ) by A8,A14,A17;
end;
end;
hence ( W4 . x ) = ( W3 . x );
end;
hence thesis by FINSEQ_2:9;
end;
