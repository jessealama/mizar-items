theorem
Lm38: (for G being _Graph holds (for W being (Walk of G) holds (for e,x being set holds (e Joins ( W .last() ),x,G implies ((( ( W .addEdge e ) . ( ( len W ) + 1 ) ) = e & ( ( W .addEdge e ) . ( ( len W ) + 2 ) ) = x) & (for n being (Element of ( NAT )) holds (n in ( dom W ) implies ( ( W .addEdge e ) . n ) = ( W . n ))))))))
proof
let G being _Graph;
let W being (Walk of G);
let e being set;
let x being set;
set W2 = ( W .addEdge e );
A1: ( <* e,x *> . 1 ) = e by FINSEQ_1:44;
assume e Joins ( W .last() ),x,G;
then A2: W2 = ( W ^ <* e,x *> ) by CKB74:1;
A3: ( dom <* e,x *> ) = ( Seg 2 ) by FINSEQ_1:89;
then 1 in ( dom <* e,x *> ) by FINSEQ_1:1;
hence ( W2 . ( ( len W ) + 1 ) ) = e by A2,A1,FINSEQ_1:def 7;
A4: ( <* e,x *> . 2 ) = x by FINSEQ_1:44;
2 in ( dom <* e,x *> ) by A3,FINSEQ_1:1;
hence ( W2 . ( ( len W ) + 2 ) ) = x by A2,A4,FINSEQ_1:def 7;
let n being (Element of ( NAT ));
assume n in ( dom W );
hence thesis by A2,FINSEQ_1:def 7;
end;
