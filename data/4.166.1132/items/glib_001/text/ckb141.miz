begin
registration
let G being _Graph;
let W being (Path of G);
let m being (Element of ( NAT ));
let n being (Element of ( NAT ));
cluster ( W .remove (m,n) ) ->  Path-like;
coherence
proof
set W2 = ( W .remove (m,n) );
A1: ( len W2 ) <= ( len W ) by CKB65:1;
now
per cases ;
suppose A2: ((((m is  odd & n is  odd) & m <= n) & n <= ( len W )) & ( W . m ) = ( W . n ));

then reconsider m9 = m,n9 = n as  odd (Element of ( NAT ));
now
let x being  odd (Element of ( NAT ));
let y being  odd (Element of ( NAT ));
assume that
A3: x < y
and
A4: y <= ( len W2 )
and
A5: ( W2 . x ) = ( W2 . y );
A6: 1 <= x by ABIAN:12;
x <= ( len W2 ) by A3,A4,XXREAL_0:2;
then A7: x in ( dom W2 ) by A6,FINSEQ_3:25;
1 <= y by ABIAN:12;
then A8: y in ( dom W2 ) by A4,FINSEQ_3:25;
A9: y <= ( len W ) by A1,A4,XXREAL_0:2;
now
per cases  by A2,A7,CKB73:1;
suppose x in ( Seg m );

then A10: ( W2 . x ) = ( W . x ) by A2,CKB68:1;
now
per cases  by A2,A8,CKB73:1;
suppose y in ( Seg m );

then A11: ( W2 . y ) = ( W . y ) by A2,CKB68:1;
then y = ( len W ) by A3,A5,A9,A10,CKB36:def 1;
hence (x = 1 & y = ( len W2 )) by A1,A3,A4,A5,A10,A11,CKB36:def 1,XXREAL_0:1;
end;
suppose A12: (m <= y & y <= ( len W2 ));

then A13: ( ( y - m ) + n ) <= ( len W ) by A2,CKB69:1;
A14: ( W2 . y ) = ( W . ( ( y - m ) + n ) ) by A2,A12,CKB69:1;
reconsider y2 = ( ( y - m9 ) + n9 ) as  odd (Element of ( NAT )) by A2,A12,CKB69:1;
( ( y - m ) + n ) >= ( ( y - m ) + m ) by A2,XREAL_1:7;
then A15: x < y2 by A3,XXREAL_0:2;
y2 <= ( len W ) by A2,A12,CKB69:1;
then y2 = ( len W ) by A5,A10,A14,A15,CKB36:def 1;
then ( ( len W2 ) + n ) = ( ( ( y - m ) + n ) + m ) by A2,CKB63:1
.= ( y + n );
hence (x = 1 & y = ( len W2 )) by A5,A10,A14,A13,A15,CKB36:def 1;
end;
end;
hence (x = 1 & y = ( len W2 ));
end;
suppose A16: (m <= x & x <= ( len W2 ));

then reconsider x2 = ( ( x - m9 ) + n9 ) as  odd (Element of ( NAT )) by A2,CKB69:1;
A17: ( W2 . x ) = ( W . ( ( x - m ) + n ) ) by A2,A16,CKB69:1;
now
per cases  by A2,A8,CKB73:1;
suppose y in ( Seg m );

then y <= m by FINSEQ_1:1;
hence (x = 1 & y = ( len W2 )) by A3,A16,XXREAL_0:2;
end;
suppose A18: (m <= y & y <= ( len W2 ));

then reconsider y2 = ( ( y - m9 ) + n9 ) as  odd (Element of ( NAT )) by A2,CKB69:1;
( x + ( n - m ) ) < ( y + ( n - m ) ) by A3,XREAL_1:8;
then A19: x2 < y2;
reconsider xm4 = ( x - m ) as (Element of ( NAT )) by A16,INT_1:5;
A20: 1 <= n9 by ABIAN:12;
A21: 1 <= m9 by ABIAN:12;
A22: ( ( y - m ) + n ) <= ( len W ) by A2,A18,CKB69:1;
A23: ( W2 . y ) = ( W . ( ( y - m ) + n ) ) by A2,A18,CKB69:1;
then y2 = ( len W ) by A5,A17,A22,A19,CKB36:def 1;
then A24: ( ( len W2 ) + n ) = ( ( ( y - m ) + n ) + m ) by A2,CKB63:1
.= ( y + n );
x2 = 1 by A5,A17,A23,A22,A19,CKB36:def 1;
then ( x2 - n9 ) <= ( 1 - 1 ) by A20,XREAL_1:13;
then A25: xm4 = ( 0 );
then m <= 1 by A2,A5,A17,A23,A22,A19,CKB36:def 1;
hence (x = 1 & y = ( len W2 )) by A24,A25,A21,XXREAL_0:1;
end;
end;
hence (x = 1 & y = ( len W2 ));
end;
end;
hence (x = 1 & y = ( len W2 ));
end;
hence thesis by CKB36:def 1;
end;
suppose (not ((((m is  odd & n is  odd) & m <= n) & n <= ( len W )) & ( W . m ) = ( W . n )));

hence thesis by CKB19:def 1;
end;
end;
hence thesis;
end;
