begin
theorem
Th15: (for L being non  empty multMagma holds (for B being non  empty AlgebraStr over L holds (for A being non  empty (Subalgebra of B) holds (for x,y being (Element of B) holds (for x9,y9 being (Element of A) holds ((x = x9 & y = y9) implies ( x + y ) = ( x9 + y9 )))))))
proof
let L being non  empty multMagma;
let B being non  empty AlgebraStr over L;
let A being non  empty (Subalgebra of B);
let x being (Element of B);
let y being (Element of B);
let x9 being (Element of A);
let y9 being (Element of A);
assume that
A1: (x = x9 & y = y9);
[ x9,y9 ] in [: (the carrier of A),(the carrier of A) :] by ZFMISC_1:87;
hence ( x + y ) = ( ( (the addF of B) || (the carrier of A) ) . [ x9,y9 ] ) by A1,FUNCT_1:49
.= ( x9 + y9 ) by CKB29:def 1;
end;
