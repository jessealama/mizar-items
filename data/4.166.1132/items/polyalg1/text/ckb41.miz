begin
theorem
(for L being non  empty multMagma holds (for B being non  empty AlgebraStr over L holds (for A being non  empty (Subalgebra of B) holds (ex C being (Subset of B) st ((the carrier of A) = C & C is  opers_closed)))))
proof
let L being non  empty multMagma;
let B being non  empty AlgebraStr over L;
let A being non  empty (Subalgebra of B);
take C = (the carrier of A);
A1: (( 1. B ) = ( 1. A ) & ( 0. B ) = ( 0. A )) by CKB29:def 1;
reconsider C as (Subset of B) by CKB29:def 1;
A2: (for a being (Element of L) holds (for v being (Element of B) holds (v in C implies ( a * v ) in C)))
proof
let a being (Element of L);
let v being (Element of B);
assume v in C;
then reconsider x = v as (Element of A);
( a * v ) = ( a * x ) by CKB40:1;
hence thesis;
end;
A3: (for x,y being (Element of B) holds ((x in C & y in C) implies ( x * y ) in C))
proof
let x being (Element of B);
let y being (Element of B);
assume that
A4: (x in C & y in C);
reconsider x9 = x,y9 = y as (Element of B);
reconsider x1 = x9,y1 = y9 as (Element of A) by A4;
( x * y ) = ( x1 * y1 ) by CKB39:1;
hence thesis;
end;
(for v,u being (Element of B) holds ((v in C & u in C) implies ( v + u ) in C))
proof
let v being (Element of B);
let u being (Element of B);
assume (v in C & u in C);
then reconsider x = u,y = v as (Element of A);
( v + u ) = ( y + x ) by CKB38:1;
hence thesis;
end;
then C is  linearly-closed by A2,VECTSP_4:def 1;
hence thesis by A1,A3,CKB37:def 1;
end;
