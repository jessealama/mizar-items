theorem
(for K being  add-associative  right_zeroed  right_complementable  left-distributive non  empty doubleLoopStr holds (for a,b,c being (Element of K) holds ( ( a - b ) * c ) = ( ( a * c ) - ( b * c ) )))
proof
let K being  add-associative  right_zeroed  right_complementable  left-distributive non  empty doubleLoopStr;
let y being (Element of K);
let z being (Element of K);
let x being (Element of K);
thus ( ( y - z ) * x ) = ( ( y * x ) + ( ( - z ) * x ) ) by CKB12:def 1
.= ( ( y * x ) - ( z * x ) ) by CKB49:1;
end;
