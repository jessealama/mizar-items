environ
vocabularies NUMBERS,BHSP_1,PRE_TOPC,SUBSET_1,FINSEQ_1,FUNCT_1,RELAT_1,FUNCT_2,CARD_1,TARSKI,XBOOLE_0,BINOP_1,SETWISEO,FINSET_1,MEMBER_1,FINSOP_1,ALGSTR_0,SUPINF_2,NAT_1,DECOMP_1,REAL_1,STRUCT_0,BINOP_2,PROB_2,NORMSP_1,ARYTM_3,SQUARE_1,ARYTM_1,XXREAL_0,RVSUM_1,RLVECT_1,ORDINAL4,BHSP_5;
notations TARSKI,SUBSET_1,XBOOLE_0,CARD_1,NUMBERS,XXREAL_0,NAT_1,STRUCT_0,ALGSTR_0,REAL_1,FUNCT_2,FINSET_1,RELAT_1,BINOP_2,PRE_TOPC,RLVECT_1,BHSP_1,SQUARE_1,BINOP_1,SETWISEO,FUNCT_1,FINSEQ_1,FINSOP_1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB12;
definitions TARSKI,SQUARE_1,BINOP_1,RLVECT_1,ALGSTR_0,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB12;
theorems XBOOLE_0,SUBSET_1,BHSP_1,SQUARE_1,TARSKI,INT_1,FINSEQ_1,FINSEQ_2,FINSEQ_3,FINSEQ_4,FUNCT_1,NAT_1,FUNCT_2,FINSOP_1,HILBASIS,XBOOLE_1,BINOP_2,XREAL_1,XXREAL_0,RELAT_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB12,CKB13,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24;
schemes NAT_1,FINSEQ_1,FUNCT_2;
registrations XBOOLE_0,FUNCT_1,ORDINAL1,RELSET_1,FUNCT_2,FINSET_1,NUMBERS,BINOP_2,MEMBERED,STRUCT_0,CARD_1,CKB11,CKB14;
constructors BINOP_1,DOMAIN_1,SETWISEO,REAL_1,SQUARE_1,NAT_1,BINOP_2,MEMBERED,FINSOP_1,BHSP_1,RELSET_1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB12;
requirements SUBSET,REAL,BOOLE,NUMERALS,ARITHM;
begin
theorem
(for DK,DX being non empty set holds (for f being (BinOp of DK) holds ((f is  commutative  associative & f is  having_a_unity) implies (for Y1,Y2 being  finite (Subset of DX) holds (Y1 misses Y2 implies (for F being (Function of DX,DK) holds ((Y1 c= ( dom F ) & Y2 c= ( dom F )) implies (for Z being  finite (Subset of DX) holds (Z = ( Y1 \/ Y2 ) implies ( setopfunc (Z,DX,DK,F,f) ) = ( f . (( setopfunc (Y1,DX,DK,F,f) ),( setopfunc (Y2,DX,DK,F,f) )) ))))))))))
proof
let DK being non empty set,DX being non empty set;
let f being (BinOp of DK);
assume that
A1: f is  commutative  associative
and
A2: f is  having_a_unity;
let Y1 being  finite (Subset of DX),Y2 being  finite (Subset of DX);
assume that
A3: Y1 misses Y2;
let F being (Function of DX,DK);
assume that
A4: Y1 c= ( dom F )
and
A5: Y2 c= ( dom F );
let Z being  finite (Subset of DX);
assume A6: Z = ( Y1 \/ Y2 );
then A7: Z c= ( dom F ) by A4,A5,XBOOLE_1:8;
consider p1 being (FinSequence of DX) such that A8: p1 is  one-to-one and A9: ( rng p1 ) = Y1 and A10: ( setopfunc (Y1,DX,DK,F,f) ) = ( f "**" ( Func_Seq (F,p1) ) ) by A1,A2,A4,CKB6:def 1;
consider p2 being (FinSequence of DX) such that A11: p2 is  one-to-one and A12: ( rng p2 ) = Y2 and A13: ( setopfunc (Y2,DX,DK,F,f) ) = ( f "**" ( Func_Seq (F,p2) ) ) by A1,A2,A5,CKB6:def 1;
set q = ( p1 ^ p2 );
A14: q is  one-to-one by A3,A8,A9,A11,A12,FINSEQ_3:91;
( rng q ) = Z by A6,A9,A12,FINSEQ_1:31;
then A15: ( setopfunc (Z,DX,DK,F,f) ) = ( f "**" ( Func_Seq (F,q) ) ) by A1,A2,A7,A14,CKB6:def 1;
(ex fp1,fp2 being FinSequence st ((fp1 = ( F * p1 ) & fp2 = ( F * p2 )) & ( F * ( p1 ^ p2 ) ) = ( fp1 ^ fp2 ))) by A4,A5,A6,A9,A12,HILBASIS:1,XBOOLE_1:8;
hence thesis by A1,A2,A10,A13,A15,FINSOP_1:5;
end;
