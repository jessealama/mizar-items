begin
reserve E for set;
reserve x for set;
reserve A for (Subset of ( E ^omega ));
reserve n for Nat;
theorem
(( A * ) = { ( <%> E ) } iff (A = ( {} ) or A = { ( <%> E ) }))
proof
thus (( A * ) = { ( <%> E ) } implies (A = ( {} ) or A = { ( <%> E ) }))
proof
A1: A c= ( A * ) by CKB52:1;
assume that
A2: ( A * ) = { ( <%> E ) }
and
A3: (A <> ( {} ) & A <> { ( <%> E ) });
(ex x st (x in A & x <> ( <%> E ))) by A3,ZFMISC_1:35;
hence contradiction by A2,A1,TARSKI:def 1;
end;

A4:now
assume A5: A = ( {} );
A6:now
let x;
assume x in ( A * );
then consider n such that A7: x in ( A |^ n ) by CKB50:1;
(n = ( 0 ) implies x in { ( <%> E ) }) by A7,CKB32:1;
hence x in { ( <%> E ) } by A5,A7,CKB35:1;
end;
now
let x;
assume x in { ( <%> E ) };
then x in ( A |^ ( 0 ) ) by CKB32:1;
hence x in ( A * ) by CKB50:1;
end;
hence ( A * ) = { ( <%> E ) } by A6,TARSKI:1;
end;
now
assume A8: A = { ( <%> E ) };
A9: ( A * ) c= { ( <%> E ) }
proof
let x;
assume x in ( A * );
then (ex n st x in ( A |^ n )) by CKB50:1;
hence thesis by A8,CKB36:1;
end;
{ ( <%> E ) } c= ( A * )
proof
let x;
assume x in { ( <%> E ) };
then x in ( A |^ ( 0 ) ) by CKB32:1;
hence thesis by CKB50:1;
end;
hence ( A * ) = { ( <%> E ) } by A9,XBOOLE_0:def 10;
end;
hence thesis by A4;
end;
