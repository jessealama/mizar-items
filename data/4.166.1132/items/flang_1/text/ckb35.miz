begin
reserve E for set;
reserve A for (Subset of ( E ^omega ));
reserve a1 for (Element of ( E ^omega ));
reserve a2 for (Element of ( E ^omega ));
reserve n for Nat;
reserve m for Nat;
theorem
Th28: (( A |^ n ) = ( {} ) iff (n > ( 0 ) & A = ( {} )))
proof
defpred P[ Nat ]
 means
( A |^ $1 ) = ( {} );
thus (( A |^ n ) = ( {} ) implies (n > ( 0 ) & A = ( {} )))
proof
assume that
A1: ( A |^ n ) = ( {} )
and
A2: (n <= ( 0 ) or A <> ( {} ));
A3:now
defpred P[ Nat ]
 means
( A |^ $1 ) <> ( {} );
assume A4: A <> ( {} );
A5:now
let n;
assume P[ n ];
then consider a1 such that A6: a1 in ( A |^ n ) by SUBSET_1:4;
consider a2 such that A7: a2 in A by A4,SUBSET_1:4;
( a1 ^ a2 ) in ( ( A |^ n ) ^^ A ) by A6,A7,CKB18:def 1;
hence P[ ( n + 1 ) ] by CKB31:1;
end;
( A |^ ( 0 ) ) = { ( <%> E ) } by CKB32:1;
then A8: P[ ( 0 ) ];
(for n holds P[ n ]) from NAT_1:sch 2(A8,A5);
hence contradiction by A1;
end;
now
assume n <= ( 0 );
then n = ( 0 );
then ( A |^ n ) = { ( <%> E ) } by CKB32:1;
hence contradiction by A1;
end;
hence thesis by A2,A3;
end;

assume that
A9: n > ( 0 )
and
A10: A = ( {} );
A11:now
let m being Nat;
assume that
1 <= m
and
P[ m ];
( ( {} ( E ^omega ) ) |^ ( m + 1 ) ) = ( ( ( {} ( E ^omega ) ) |^ m ) ^^ ( {} ( E ^omega ) ) ) by CKB31:1
.= ( {} ) by CKB19:1;
hence P[ ( m + 1 ) ] by A10;
end;
A12: P[ 1 ] by A10,CKB33:1;
A13: (for m being Nat holds (m >= 1 implies P[ m ])) from NAT_1:sch 8(A12,A11);
n >= ( ( 0 ) + 1 ) by A9,NAT_1:13;
hence thesis by A13;
end;
