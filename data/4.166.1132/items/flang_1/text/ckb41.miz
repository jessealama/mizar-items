theorem
Th34: ( A |^ ( m + n ) ) = ( ( A |^ m ) ^^ ( A |^ n ) )
proof
defpred P[ Nat ]
 means
(for m holds (for n holds (( m + n ) <= $1 implies ( A |^ ( m + n ) ) = ( ( A |^ m ) ^^ ( A |^ n ) ))));
A1:now
let l;
assume A2: P[ l ];
now
let m being Nat;
let n being Nat;
A3:now
assume ( m + n ) < ( l + 1 );
then ( m + n ) <= l by NAT_1:13;
hence ( A |^ ( m + n ) ) = ( ( A |^ m ) ^^ ( A |^ n ) ) by A2;
end;
A4:now
assume A5: ( m + n ) = ( l + 1 );
A6:now
assume A7: n = ( 0 );
thus ( A |^ ( m + n ) ) = ( ( A |^ ( l + 1 ) ) ^^ { ( <%> E ) } ) by A5,CKB20:1
.= ( ( A |^ m ) ^^ ( A |^ n ) ) by A5,A7,CKB32:1;
end;
A8:now
assume that
A9: m > ( 0 )
and
A10: n > ( 0 );
consider k such that A11: ( k + 1 ) = m by A9,NAT_1:6;
( A |^ ( m + n ) ) = ( A |^ ( ( k + n ) + 1 ) ) by A11
.= ( ( A |^ ( k + n ) ) ^^ A ) by CKB31:1
.= ( A ^^ ( A |^ ( k + n ) ) ) by CKB40:1
.= ( A ^^ ( ( A |^ k ) ^^ ( A |^ n ) ) ) by A2,A5,A11
.= ( ( A ^^ ( A |^ k ) ) ^^ ( A |^ n ) ) by CKB25:1
.= ( ( ( A |^ k ) ^^ A ) ^^ ( A |^ n ) ) by CKB40:1
.= ( ( ( A |^ k ) ^^ ( A |^ 1 ) ) ^^ ( A |^ n ) ) by CKB33:1
.= ( ( A |^ m ) ^^ ( A |^ n ) ) by A2,A5,A10,A11,CKB7:1;
hence ( A |^ ( m + n ) ) = ( ( A |^ m ) ^^ ( A |^ n ) );
end;
now
assume A12: m = ( 0 );
thus ( A |^ ( m + n ) ) = ( { ( <%> E ) } ^^ ( A |^ ( l + 1 ) ) ) by A5,CKB20:1
.= ( ( A |^ m ) ^^ ( A |^ n ) ) by A5,A12,CKB32:1;
end;
hence ( A |^ ( m + n ) ) = ( ( A |^ m ) ^^ ( A |^ n ) ) by A6,A8;
end;
assume ( m + n ) <= ( l + 1 );
hence ( A |^ ( m + n ) ) = ( ( A |^ m ) ^^ ( A |^ n ) ) by A4,A3,XXREAL_0:1;
end;
hence P[ ( l + 1 ) ];
end;
A13: P[ ( 0 ) ]
proof
let m;
let n;
assume A14: ( m + n ) <= ( 0 );
then A15: m = ( 0 );
A16: ( m + n ) = ( 0 ) by A14;
hence ( A |^ ( m + n ) ) = ( ( A |^ ( 0 ) ) ^^ { ( <%> E ) } ) by CKB20:1
.= ( ( A |^ m ) ^^ ( A |^ n ) ) by A16,A15,CKB32:1;
end;
(for l holds P[ l ]) from NAT_1:sch 2(A13,A1);
hence thesis;
end;
