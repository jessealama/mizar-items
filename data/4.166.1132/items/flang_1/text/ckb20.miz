begin
reserve E for set;
reserve x for set;
reserve A for (Subset of ( E ^omega ));
reserve a for (Element of ( E ^omega ));
reserve d for (Element of ( E ^omega ));
theorem
Th14: (( A ^^ { ( <%> E ) } ) = A & ( { ( <%> E ) } ^^ A ) = A)
proof
A1: (x in ( A ^^ { ( <%> E ) } ) implies x in A)
proof
assume x in ( A ^^ { ( <%> E ) } );
then consider a,d such that A2: a in A and A3: (d in { ( <%> E ) } & x = ( a ^ d )) by CKB18:def 1;
x = ( a ^ ( <%> E ) ) by A3,TARSKI:def 1;
hence thesis by A2,AFINSQ_1:29;
end;
A4: (x in A implies x in ( { ( <%> E ) } ^^ A ))
proof
assume A5: x in A;
(ex d st (ex a st ((d in { ( <%> E ) } & a in A) & x = ( d ^ a ))))
proof
reconsider a = x as (Element of ( E ^omega )) by A5;
consider d such that A6: d = ( <%> E );
take d;
take a;
thus thesis by A5,A6,AFINSQ_1:29,TARSKI:def 1;
end;
hence thesis by CKB18:def 1;
end;
A7: (x in A implies x in ( A ^^ { ( <%> E ) } ))
proof
assume A8: x in A;
(ex a st (ex d st ((a in A & d in { ( <%> E ) }) & x = ( a ^ d ))))
proof
reconsider a = x as (Element of ( E ^omega )) by A8;
set d = ( <%> E );
take a;
take d;
thus thesis by A8,AFINSQ_1:29,TARSKI:def 1;
end;
hence thesis by CKB18:def 1;
end;
(x in ( { ( <%> E ) } ^^ A ) implies x in A)
proof
assume x in ( { ( <%> E ) } ^^ A );
then consider d,a such that A9: d in { ( <%> E ) } and A10: a in A and A11: x = ( d ^ a ) by CKB18:def 1;
x = ( ( <%> E ) ^ a ) by A9,A11,TARSKI:def 1;
hence thesis by A10,AFINSQ_1:29;
end;
hence thesis by A1,A4,A7,TARSKI:1;
end;
