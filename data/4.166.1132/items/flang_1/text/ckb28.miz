begin
reserve E for set;
reserve x for set;
reserve A for (Subset of ( E ^omega ));
reserve B for (Subset of ( E ^omega ));
reserve C for (Subset of ( E ^omega ));
reserve a for (Element of ( E ^omega ));
reserve b for (Element of ( E ^omega ));
theorem
Th22: (( ( A ^^ B ) \ ( A ^^ C ) ) c= ( A ^^ ( B \ C ) ) & ( ( B ^^ A ) \ ( C ^^ A ) ) c= ( ( B \ C ) ^^ A ))
proof
thus ( ( A ^^ B ) \ ( A ^^ C ) ) c= ( A ^^ ( B \ C ) )
proof
let x;
assume A1: x in ( ( A ^^ B ) \ ( A ^^ C ) );
then x in ( A ^^ B ) by XBOOLE_0:def 5;
then consider a,b such that A2: a in A and A3: b in B and A4: x = ( a ^ b ) by CKB18:def 1;
A5:now
assume (not b in C);
then b in ( B \ C ) by A3,XBOOLE_0:def 5;
hence thesis by A2,A4,CKB18:def 1;
end;
(not x in ( A ^^ C )) by A1,XBOOLE_0:def 5;
hence thesis by A2,A4,A5,CKB18:def 1;
end;

thus ( ( B ^^ A ) \ ( C ^^ A ) ) c= ( ( B \ C ) ^^ A )
proof
let x;
assume A6: x in ( ( B ^^ A ) \ ( C ^^ A ) );
then x in ( B ^^ A ) by XBOOLE_0:def 5;
then consider b,a such that A7: b in B and A8: (a in A & x = ( b ^ a )) by CKB18:def 1;
A9:now
assume (not b in C);
then b in ( B \ C ) by A7,XBOOLE_0:def 5;
hence thesis by A8,CKB18:def 1;
end;
(not x in ( C ^^ A )) by A6,XBOOLE_0:def 5;
hence thesis by A8,A9,CKB18:def 1;
end;

end;
