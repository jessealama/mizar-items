reserve a for (Element of ( E ^omega ));
reserve b for (Element of ( E ^omega ));
reserve c for (Element of ( E ^omega ));
reserve e for (Element of E);
reserve n for Nat;
theorem
Th74: a in ( ( Lex E ) |^ ( len a ) )
proof
defpred P[ Nat ]
 means
(for a holds (( len a ) = $1 implies a in ( ( Lex E ) |^ $1 )));
A1:now
let n;
assume A2: P[ n ];
now
let b;
assume ( len b ) = ( n + 1 );
then consider c,e such that A3: ( len c ) = n and A4: b = ( c ^ <% e %> ) by CKB12:1;
<% e %> is (Element of ( E ^omega )) by A4,CKB10:1;
then e in E by CKB11:1;
then <% e %> in ( Lex E ) by CKB82:def 1;
then A5: <% e %> in ( ( Lex E ) |^ 1 ) by CKB33:1;
c in ( ( Lex E ) |^ n ) by A2,A3;
hence b in ( ( Lex E ) |^ ( n + 1 ) ) by A4,A5,CKB48:1;
end;
hence P[ ( n + 1 ) ];
end;
A6: P[ ( 0 ) ]
proof
let a;
assume ( len a ) = ( 0 );
then a = ( <%> E );
then a in { ( <%> E ) } by TARSKI:def 1;
hence thesis by CKB32:1;
end;
(for n holds P[ n ]) from NAT_1:sch 2(A6,A1);
hence thesis;
end;
