begin
theorem
Th18: (for T being non empty TopStruct holds (for S being (sequence of T) holds (for S1 being (subsequence of S) holds (for x being (Point of T) holds (S is_convergent_to x implies S1 is_convergent_to x)))))
proof
let T being non empty TopStruct;
let S being (sequence of T);
let S1 being (subsequence of S);
let x being (Point of T);
assume A1: S is_convergent_to x;
let U1 being (Subset of T);
assume (U1 is  open & x in U1);
then consider n being (Element of ( NAT )) such that A2: (for m being (Element of ( NAT )) holds (n <= m implies ( S . m ) in U1)) by A1,FRECHET:def 3;
take n;
let m being (Element of ( NAT ));
assume A3: n <= m;
m in ( NAT );
then A4: m in ( dom S1 ) by NORMSP_1:12;
consider NS being  increasing (sequence of ( NAT )) such that A5: S1 = ( S * NS ) by VALUED_0:def 17;
m <= ( NS . m ) by SEQM_3:14;
then ( S . ( NS . m ) ) in U1 by A2,A3,XXREAL_0:2;
hence ( S1 . m ) in U1 by A5,A4,FUNCT_1:12;
end;
