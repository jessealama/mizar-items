theorem
Th14: (for T1,T2 being non  empty TopSpace holds (for f being (Function of T1,T2) holds (f is  continuous implies (for S1 being (sequence of T1) holds (for S2 being (sequence of T2) holds (S2 = ( f * S1 ) implies ( f .: ( Lim S1 ) ) c= ( Lim S2 )))))))
proof
let T1 being non  empty TopSpace;
let T2 being non  empty TopSpace;
let f being (Function of T1,T2);
assume A1: f is  continuous;
let S1 being (sequence of T1);
let S2 being (sequence of T2);
assume A2: S2 = ( f * S1 );
let y being set;
assume A3: y in ( f .: ( Lim S1 ) );
then reconsider y9 = y as (Point of T2);
S2 is_convergent_to y9
proof
let U2 being (Subset of T2);
assume that
A4: U2 is  open
and
A5: y9 in U2;
consider x being set such that A6: x in ( dom f ) and A7: x in ( Lim S1 ) and A8: y = ( f . x ) by A3,FUNCT_1:def 6;
A9: x in ( f " U2 ) by A5,A6,A8,FUNCT_1:def 7;
reconsider U1 = ( f " U2 ) as (Subset of T1);
( [#] T2 ) <> ( {} );
then A10: U1 is  open by A1,A4,TOPS_2:43;
reconsider x as (Point of T1) by A6;
S1 is_convergent_to x by A7,FRECHET:def 5;
then consider n being (Element of ( NAT )) such that A11: (for m being (Element of ( NAT )) holds (n <= m implies ( S1 . m ) in ( f " U2 ))) by A10,A9,FRECHET:def 3;
take n;
let m being (Element of ( NAT ));
assume n <= m;
then ( S1 . m ) in ( f " U2 ) by A11;
then A12: ( f . ( S1 . m ) ) in U2 by FUNCT_1:def 7;
( dom S1 ) = ( NAT ) by FUNCT_2:def 1;
hence ( S2 . m ) in U2 by A2,A12,FUNCT_1:13;
end;
hence thesis by FRECHET:def 5;
end;
