begin
theorem
(for T being non  empty TopStruct holds (for S being (sequence of T) holds (for x being (Point of T) holds ((not S is_convergent_to x) implies (ex S1 being (subsequence of S) st (for S2 being (subsequence of S1) holds (not S2 is_convergent_to x)))))))
proof
let T being non  empty TopStruct;
let S being (sequence of T);
let x being (Point of T);
assume (not S is_convergent_to x);
then consider A being (Subset of T) such that A1: (A is  open & x in A) and A2: (for n being (Element of ( NAT )) holds (ex m being (Element of ( NAT )) st (n <= m & (not ( S . m ) in A)))) by FRECHET:def 3;
defpred P[ set ]
 means
(not $1 in A);
A3: (for n being (Element of ( NAT )) holds (ex m being (Element of ( NAT )) st (n <= m & P[ ( S . m ) ]))) by A2;
consider S1 being (subsequence of S) such that A4: (for n being (Element of ( NAT )) holds P[ ( S1 . n ) ]) from VALUED_1:sch 1(A3);
take S1;
let S2 being (subsequence of S1);
(ex U1 being (Subset of T) st ((U1 is  open & x in U1) & (for n being (Element of ( NAT )) holds (ex m being (Element of ( NAT )) st (n <= m & (not ( S2 . m ) in U1))))))
proof
take A;
consider NS being  increasing (sequence of ( NAT )) such that A5: S2 = ( S1 * NS ) by VALUED_0:def 17;
thus (A is  open & x in A) by A1;
let n being (Element of ( NAT ));
take n;
thus n <= n;
n in ( NAT );
then n in ( dom S2 ) by NORMSP_1:12;
then ( S2 . n ) = ( S1 . ( NS . n ) ) by A5,FUNCT_1:12;
hence thesis by A4;
end;
hence thesis by FRECHET:def 3;
end;
