begin
theorem
Th40: (for T being non  empty TopStruct holds (for S being (sequence of T) holds (for P being (Permutation of ( NAT )) holds (for x being (Point of T) holds (S is_convergent_to x implies ( S * P ) is_convergent_to x)))))
proof
let T being non  empty TopStruct;
let S being (sequence of T);
let P being (Permutation of ( NAT ));
let x being (Point of T);
assume A1: S is_convergent_to x;
(for U1 being (Subset of T) holds ((U1 is  open & x in U1) implies (ex n being (Element of ( NAT )) st (for m being (Element of ( NAT )) holds (n <= m implies ( ( S * P ) . m ) in U1)))))
proof
let U1 being (Subset of T);
defpred P[ set ]
 means
$1 in U1;
assume A2: (U1 is  open & x in U1);
A3: (ex n being (Element of ( NAT )) st (for m being (Element of ( NAT )) holds (for x being (Point of T) holds ((n <= m & x = ( S . m )) implies P[ x ]))))
proof
consider n being (Element of ( NAT )) such that A4: (for m being (Element of ( NAT )) holds (n <= m implies ( S . m ) in U1)) by A1,A2,FRECHET:def 3;
take n;
let m being (Element of ( NAT ));
let x being (Point of T);
assume (n <= m & x = ( S . m ));
hence thesis by A4;
end;
thus (ex n being (Element of ( NAT )) st (for m being (Element of ( NAT )) holds (n <= m implies P[ ( ( S * P ) . m ) ]))) from CKB46:sch 1(A3);
end;
hence thesis by FRECHET:def 3;
end;
