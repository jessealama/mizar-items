begin
theorem
Th36: (for T being non empty TopStruct holds (for S being (sequence of T) holds (for x being (Point of T) holds (for Y being (Subset of T) holds ((Y = { y where y is (Point of T): x in ( Cl { y } ) } & ( rng S ) c= Y) implies S is_convergent_to x)))))
proof
let T being non empty TopStruct;
let S being (sequence of T);
let x being (Point of T);
let Y being (Subset of T);
assume that
A1: Y = { y where y is (Point of T): x in ( Cl { y } ) }
and
A2: ( rng S ) c= Y;
let U1 being (Subset of T);
assume A3: (U1 is  open & x in U1);
take ( 0 );
let m being (Element of ( NAT ));
m in ( NAT );
then m in ( dom S ) by NORMSP_1:12;
then ( S . m ) in ( rng S ) by FUNCT_1:def 3;
then ( S . m ) in Y by A2;
then consider y being (Point of T) such that A4: y = ( S . m ) and A5: x in ( Cl { y } ) by A1;
assume ( 0 ) <= m;
{ y } meets U1 by A3,A5,PRE_TOPC:def 7;
hence ( S . m ) in U1 by A4,ZFMISC_1:50;
end;
