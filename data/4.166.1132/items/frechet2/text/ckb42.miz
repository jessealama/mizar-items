theorem
Th37: (for T being non empty TopStruct holds (for S being (sequence of T) holds (for x,y being (Point of T) holds ((for n being (Element of ( NAT )) holds (( S . n ) = y & S is_convergent_to x)) implies x in ( Cl { y } )))))
proof
let T being non empty TopStruct;
let S being (sequence of T);
let x being (Point of T),y being (Point of T);
assume A1: (for n being (Element of ( NAT )) holds (( S . n ) = y & S is_convergent_to x));
(for G being (Subset of T) holds (G is  open implies (x in G implies { y } meets G)))
proof
let G being (Subset of T);
assume A2: G is  open;
assume x in G;
then consider n being (Element of ( NAT )) such that A3: (for m being (Element of ( NAT )) holds (n <= m implies ( S . m ) in G)) by A1,A2,FRECHET:def 3;
( S . n ) in G by A3;
then A4: y in G by A1;
y in { y } by TARSKI:def 1;
then y in ( { y } /\ G ) by A4,XBOOLE_0:def 4;
hence thesis by XBOOLE_0:def 7;
end;
hence thesis by PRE_TOPC:def 7;
end;
