begin
registration
let X being  compact non  empty TopSpace;
cluster ( R_Normed_Algebra_of_ContinuousFunctions X ) ->  Banach_Algebra-like;
coherence
proof
set B = ( R_Normed_Algebra_of_ContinuousFunctions X );
A1:now
let f being (Element of B);
let g being (Element of B);
reconsider f1 = f,g1 = g as (Point of ( R_Normed_Algebra_of_BoundedFunctions (the carrier of X) )) by CKB17:1;
A2: ((||. f .|| = ||. f1 .|| & ||. g .|| = ||. g1 .||) & ||. ( f * g ) .|| = ||. ( f1 * g1 ) .||) by CKB27:1,CKB24:1;
( R_Normed_Algebra_of_BoundedFunctions (the carrier of X) ) is  Banach_Algebra-like_1 by C0SP1:37;
hence ||. ( f * g ) .|| <= ( ||. f .|| * ||. g .|| ) by A2,LOPBAN_2:def 9;
end;
A3: ( R_Normed_Algebra_of_BoundedFunctions (the carrier of X) ) is  Banach_Algebra-like_2 by C0SP1:37;
A4: ||. ( 1. B ) .|| = ||. ( 1. ( R_Normed_Algebra_of_BoundedFunctions (the carrier of X) ) ) .|| by CKB33:1,CKB24:1
.= 1 by A3,LOPBAN_2:def 10;
A5:now
let a being Real;
let f being (Element of B);
let g being (Element of B);
reconsider f1 = f,g1 = g as (Point of ( R_Normed_Algebra_of_BoundedFunctions (the carrier of X) )) by CKB17:1;
A6: ( a * g1 ) = ( a * g ) by CKB26:1;
A7: ( f * g ) = ( f1 * g1 ) by CKB27:1;
A8: ( R_Normed_Algebra_of_BoundedFunctions (the carrier of X) ) is  Banach_Algebra-like_3 by C0SP1:37;
( a * ( f * g ) ) = ( a * ( f1 * g1 ) ) by A7,CKB26:1;
then ( a * ( f * g ) ) = ( f1 * ( a * g1 ) ) by A8,LOPBAN_2:def 11;
hence ( a * ( f * g ) ) = ( f * ( a * g ) ) by A6,CKB27:1;
end;
now
let f being (Element of B);
let g being (Element of B);
let h being (Element of B);
reconsider f1 = f,g1 = g,h1 = h as (Point of ( R_Normed_Algebra_of_BoundedFunctions (the carrier of X) )) by CKB17:1;
A9: ( g + h ) = ( g1 + h1 ) by CKB25:1;
A10: (( g * f ) = ( g1 * f1 ) & ( h * f ) = ( h1 * f1 )) by CKB27:1;
A11: ( R_Normed_Algebra_of_BoundedFunctions (the carrier of X) ) is  left-distributive by C0SP1:37;
thus ( ( g + h ) * f ) = ( ( g1 + h1 ) * f1 ) by CKB27:1,A9
.= ( ( g1 * f1 ) + ( h1 * f1 ) ) by A11,VECTSP_1:def 3
.= ( ( g * f ) + ( h * f ) ) by CKB25:1,A10;
end;
then B is  Banach_Algebra-like_1  Banach_Algebra-like_2  Banach_Algebra-like_3  left-distributive  left_unital  complete  Banach_Algebra-like_1  Banach_Algebra-like_2  Banach_Algebra-like_3  left-distributive  left_unital  complete Normed_Algebra by A1,A4,A5,LOPBAN_2:def 9,LOPBAN_2:def 10,LOPBAN_2:def 11,VECTSP_1:def 3;
hence thesis;
end;
end;
