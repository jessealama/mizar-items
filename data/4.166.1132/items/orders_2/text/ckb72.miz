environ
vocabularies STRUCT_0,RELAT_1,XBOOLE_0,PARTFUN1,RELAT_2,ORDERS_1,SUBSET_1,XXREAL_0,ARYTM_3,TREES_2,TARSKI,WELLORD1,FUNCT_1,ZFMISC_1,ORDERS_2,CARD_1;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,RELAT_1,RELAT_2,FUNCT_1,RELSET_1,PARTFUN1,CARD_1,WELLORD1,DOMAIN_1,STRUCT_0,ORDERS_1,CKB1,CKB3,CKB4,CKB5,CKB6,CKB9,CKB18,CKB19,CKB20,CKB21,CKB23,CKB30,CKB33,CKB40,CKB41,CKB52,CKB53,CKB66;
definitions RELAT_1,RELAT_2,TARSKI,WELLORD1,STRUCT_0,XBOOLE_0,ORDERS_1,CKB3,CKB4,CKB5,CKB6,CKB18,CKB20,CKB30,CKB40,CKB41,CKB52,CKB53,CKB66;
theorems RELAT_1,RELAT_2,TARSKI,WELLORD1,ZFMISC_1,XBOOLE_0,XBOOLE_1,PARTFUN1,ORDERS_1,CKB3,CKB4,CKB5,CKB6,CKB17,CKB18,CKB20,CKB22,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB34,CKB35,CKB36,CKB37,CKB38,CKB39,CKB40,CKB41,CKB42,CKB43,CKB44,CKB45,CKB46,CKB47,CKB48,CKB49,CKB50,CKB51,CKB52,CKB53,CKB54,CKB55,CKB56,CKB57,CKB58,CKB59,CKB60,CKB61,CKB62,CKB63,CKB64,CKB65,CKB66,CKB67,CKB68,CKB69,CKB70,CKB71;
schemes XBOOLE_0;
registrations XBOOLE_0,SUBSET_1,RELSET_1,ORDERS_1,STRUCT_0,PARTFUN1,CARD_1,ORDINAL1,CKB1,CKB2,CKB7,CKB8,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB31,CKB32;
constructors RELAT_2,WELLORD1,PARTFUN1,DOMAIN_1,ORDERS_1,PRE_TOPC,RELSET_1,SETFAM_1,CKB1,CKB3,CKB4,CKB5,CKB6,CKB18,CKB20,CKB23,CKB30,CKB40,CKB41,CKB52,CKB53,CKB66;
requirements BOOLE,SUBSET,NUMERALS;
begin
reserve x for set;
reserve A for non empty Poset;
reserve a for (Element of A);
reserve a1 for (Element of A);
reserve a2 for (Element of A);
reserve b for (Element of A);
reserve f for (Choice_Function of ( BOOL (the carrier of A) ));
reserve fC1 for (Chain of f);
reserve fC2 for (Chain of f);
theorem
Th83: (fC1 <> fC2 implies (fC1 is (Initial_Segm of fC2) iff (not fC2 is (Initial_Segm of fC1))))
proof
assume A1: fC1 <> fC2;
set N = { a: (a in ( fC1 /\ fC2 ) & ( InitSegm (fC1,a) ) = ( InitSegm (fC2,a) )) };
A2: N c= fC1
proof
let x;
assume x in N;
then (ex a st ((a = x & a in ( fC1 /\ fC2 )) & ( InitSegm (fC1,a) ) = ( InitSegm (fC2,a) )));
hence thesis by XBOOLE_0:def 4;
end;
then reconsider N as (Subset of A) by XBOOLE_1:1;
A3: N c= fC2
proof
let x;
assume x in N;
then (ex a st ((a = x & a in ( fC1 /\ fC2 )) & ( InitSegm (fC1,a) ) = ( InitSegm (fC2,a) )));
hence thesis by XBOOLE_0:def 4;
end;
A4:now
let a1;
let a2;
assume that
A5: a2 in N
and
A6: a1 in fC1
and
A7: a1 < a2;
A8: (ex a st ((a = a2 & a in ( fC1 /\ fC2 )) & ( InitSegm (fC1,a) ) = ( InitSegm (fC2,a) ))) by A5;
A9: ( InitSegm (fC1,a1) ) = ( InitSegm (fC2,a1) )
proof
thus ( InitSegm (fC1,a1) ) c= ( InitSegm (fC2,a1) )
proof
let x;
assume A10: x in ( InitSegm (fC1,a1) );
then reconsider b = x as (Element of A);
A11: b in fC1 by A10,CKB54:1;
A12: b < a1 by A10,CKB54:1;
then b < a2 by A7,CKB27:1;
then b in ( InitSegm (fC1,a2) ) by A11,CKB54:1;
then b in fC2 by A8,CKB54:1;
hence thesis by A12,CKB54:1;
end;

let x;
assume A13: x in ( InitSegm (fC2,a1) );
then reconsider b = x as (Element of A);
A14: b in fC2 by A13,CKB54:1;
A15: b < a1 by A13,CKB54:1;
then b < a2 by A7,CKB27:1;
then b in ( InitSegm (fC2,a2) ) by A14,CKB54:1;
then b in fC1 by A8,CKB54:1;
hence thesis by A15,CKB54:1;
end;
a1 in ( InitSegm (fC2,a2) ) by A6,A7,A8,CKB54:1;
then a1 in fC2 by XBOOLE_0:def 4;
then a1 in ( fC1 /\ fC2 ) by A6,XBOOLE_0:def 4;
hence a1 in N by A9;
end;
A16:now
let a1;
let a2;
assume that
A17: a2 in N
and
A18: a1 in fC2
and
A19: a1 < a2;
A20: (ex a st ((a = a2 & a in ( fC1 /\ fC2 )) & ( InitSegm (fC1,a) ) = ( InitSegm (fC2,a) ))) by A17;
A21: ( InitSegm (fC1,a1) ) = ( InitSegm (fC2,a1) )
proof
thus ( InitSegm (fC1,a1) ) c= ( InitSegm (fC2,a1) )
proof
let x;
assume A22: x in ( InitSegm (fC1,a1) );
then reconsider b = x as (Element of A);
A23: b in fC1 by A22,CKB54:1;
A24: b < a1 by A22,CKB54:1;
then b < a2 by A19,CKB27:1;
then b in ( InitSegm (fC1,a2) ) by A23,CKB54:1;
then b in fC2 by A20,CKB54:1;
hence thesis by A24,CKB54:1;
end;

let x;
assume A25: x in ( InitSegm (fC2,a1) );
then reconsider b = x as (Element of A);
A26: b in fC2 by A25,CKB54:1;
A27: b < a1 by A25,CKB54:1;
then b < a2 by A19,CKB27:1;
then b in ( InitSegm (fC2,a2) ) by A26,CKB54:1;
then b in fC1 by A20,CKB54:1;
hence thesis by A27,CKB54:1;
end;
a1 in ( InitSegm (fC1,a2) ) by A18,A19,A20,CKB54:1;
then a1 in fC1 by XBOOLE_0:def 4;
then a1 in ( fC1 /\ fC2 ) by A18,XBOOLE_0:def 4;
hence a1 in N by A21;
end;
A28: ((the InternalRel of A) well_orders fC1 & (the InternalRel of A) well_orders fC2) by CKB66:def 1;
now
per cases  by A2,A4,A28,A3,A16,CKB65:1;
suppose A29: (N is (Initial_Segm of fC1) & N = fC2);

fC1 <> ( {} ) by CKB66:def 1;
hence thesis by A29,CKB61:1;
end;
suppose A30: (N is (Initial_Segm of fC2) & N = fC1);

fC2 <> ( {} ) by CKB66:def 1;
hence thesis by A30,CKB61:1;
end;
suppose (N = fC1 & N = fC2);

hence thesis by A1;
end;
suppose A31: (N is (Initial_Segm of fC1) & N is (Initial_Segm of fC2));

fC2 <> ( {} ) by CKB66:def 1;
then consider b such that A32: b in fC2 and A33: N = ( InitSegm (fC2,b) ) by A31,CKB53:def 1;
fC1 <> ( {} ) by CKB66:def 1;
then consider a such that A34: a in fC1 and A35: N = ( InitSegm (fC1,a) ) by A31,CKB53:def 1;
A36: a = ( f . ( UpperCone ( InitSegm (fC2,b) ) ) ) by A34,A35,A33,CKB66:def 1
.= b by A32,CKB66:def 1;
then a in ( fC1 /\ fC2 ) by A34,A32,XBOOLE_0:def 4;
then a in N by A35,A33,A36;
hence thesis by A35,CKB56:1;
end;
end;
hence thesis;
end;
