begin
theorem
Th26: (for A being  transitive RelStr holds (for a1,a2,a3 being (Element of A) holds ((a1 <= a2 & a2 <= a3) implies a1 <= a3)))
proof
let A being  transitive RelStr;
let a1 being (Element of A),a2 being (Element of A),a3 being (Element of A);
assume that
A1: [ a1,a2 ] in (the InternalRel of A)
and
A2: [ a2,a3 ] in (the InternalRel of A);
A3: (the InternalRel of A) is_transitive_in (the carrier of A) by CKB5:def 1;
a1 in (the carrier of A) by A1,ZFMISC_1:87;
hence [ a1,a3 ] in (the InternalRel of A) by A1,A2,A3,RELAT_2:def 8;
end;
