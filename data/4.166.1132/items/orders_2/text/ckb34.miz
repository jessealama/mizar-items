theorem
Th35: (for A being non empty  reflexive RelStr holds (for a being (Element of A) holds { a } is (Chain of A)))
proof
let A being non empty  reflexive RelStr;
let a being (Element of A);
A1: (the InternalRel of A) is_reflexive_in (the carrier of A) by CKB4:def 1;
{ a } is  strongly_connected
proof
let x1 being set,x2 being set;
assume (x1 in { a } & x2 in { a });
then (x1 = a & x2 = a) by TARSKI:def 1;
hence thesis by A1,RELAT_2:def 1;
end;
hence thesis;
end;
