environ
vocabularies STRUCT_0,RELAT_1,XBOOLE_0,PARTFUN1,RELAT_2,ORDERS_1,SUBSET_1,XXREAL_0,ARYTM_3,TREES_2,TARSKI,WELLORD1,FUNCT_1,ZFMISC_1,ORDERS_2,CARD_1;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,RELAT_1,RELAT_2,FUNCT_1,RELSET_1,PARTFUN1,CARD_1,WELLORD1,DOMAIN_1,STRUCT_0,ORDERS_1,CKB1,CKB3,CKB4,CKB5,CKB6,CKB9,CKB18,CKB19,CKB20,CKB21,CKB23,CKB30,CKB33,CKB40,CKB41,CKB52,CKB53;
definitions RELAT_1,RELAT_2,TARSKI,WELLORD1,STRUCT_0,XBOOLE_0,ORDERS_1,CKB3,CKB4,CKB5,CKB6,CKB18,CKB20,CKB30,CKB40,CKB41,CKB52,CKB53;
theorems RELAT_1,RELAT_2,TARSKI,WELLORD1,ZFMISC_1,XBOOLE_0,XBOOLE_1,PARTFUN1,ORDERS_1,CKB3,CKB4,CKB5,CKB6,CKB17,CKB18,CKB20,CKB22,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB34,CKB35,CKB36,CKB37,CKB38,CKB39,CKB40,CKB41,CKB42,CKB43,CKB44,CKB45,CKB46,CKB47,CKB48,CKB49,CKB50,CKB51,CKB52,CKB53,CKB54,CKB55,CKB56,CKB57,CKB58,CKB59,CKB60,CKB61,CKB62,CKB63;
schemes XBOOLE_0;
registrations XBOOLE_0,SUBSET_1,RELSET_1,ORDERS_1,STRUCT_0,PARTFUN1,CARD_1,ORDINAL1,CKB1,CKB2,CKB7,CKB8,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB31,CKB32;
constructors RELAT_2,WELLORD1,PARTFUN1,DOMAIN_1,ORDERS_1,PRE_TOPC,RELSET_1,SETFAM_1,CKB1,CKB3,CKB4,CKB5,CKB6,CKB18,CKB20,CKB23,CKB30,CKB40,CKB41,CKB52,CKB53;
requirements BOOLE,SUBSET,NUMERALS;
begin
reserve x for set;
reserve y for set;
reserve A for non empty Poset;
reserve a for (Element of A);
reserve a1 for (Element of A);
reserve a2 for (Element of A);
reserve S for (Subset of A);
reserve T for (Subset of A);
theorem
(((S c= T & (the InternalRel of A) well_orders T) & (for a1 holds (for a2 holds ((a2 in S & a1 < a2) implies a1 in S)))) implies (S = T or S is (Initial_Segm of T)))
proof
assume that
A1: S c= T
and
A2: (the InternalRel of A) well_orders T
and
A3: (for a1 holds (for a2 holds ((a2 in S & a1 < a2) implies a1 in S)))
and
A4: S <> T;
per cases ;
case T <> ( {} );
set Y = ( T \ S );
(not T c= S) by A1,A4,XBOOLE_0:def 10;
then Y <> ( {} ) by XBOOLE_1:37;
then consider x such that A5: x in Y and A6: (for y holds (y in Y implies [ x,y ] in (the InternalRel of A))) by A2,WELLORD1:5,XBOOLE_1:36;
reconsider x as (Element of A) by A5;
take x;
thus A7: x in T by A5,XBOOLE_0:def 5;
S = ( ( LowerCone { x } ) /\ T )
proof
thus S c= ( ( LowerCone { x } ) /\ T )
proof
let y;
assume A8: y in S;
then reconsider a = y as (Element of A);
now
let a1;
assume that
A9: a1 in { x }
and
A10: (not a < a1);
A11: a1 = x by A9,TARSKI:def 1;
then A12: a1 <> a by A5,A8,XBOOLE_0:def 5;
T is (Chain of A) by A2,CKB39:1;
then a1 <= a by A1,A7,A8,A10,A11,CKB38:1;
then a1 < a by A12,CKB20:def 1;
then a1 in S by A3,A8;
hence contradiction by A5,A11,XBOOLE_0:def 5;
end;
then y in { a1: (for a2 holds (a2 in { x } implies a1 < a2)) };
hence thesis by A1,A8,XBOOLE_0:def 4;
end;

let y;
assume A13: y in ( ( LowerCone { x } ) /\ T );
then y in ( LowerCone { x } ) by XBOOLE_0:def 4;
then consider a such that A14: a = y and A15: (for a2 holds (a2 in { x } implies a < a2));
A16:now
assume y in Y;
then [ x,y ] in (the InternalRel of A) by A6;
then A17: x <= a by A14,CKB18:def 1;
x in { x } by TARSKI:def 1;
hence contradiction by A15,A17,CKB28:1;
end;
y in T by A13,XBOOLE_0:def 4;
hence thesis by A16,XBOOLE_0:def 5;
end;
hence thesis;
end;
case T = ( {} );
hence thesis by A1;
end;
end;
