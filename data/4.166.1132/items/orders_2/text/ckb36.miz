begin
theorem
(for A being RelStr holds (for C being (Chain of A) holds (for S being (Subset of A) holds (S c= C implies S is (Chain of A)))))
proof
let A being RelStr;
let C being (Chain of A);
let S being (Subset of A);
assume A1: S c= C;
(the InternalRel of A) is_strongly_connected_in C by CKB30:def 1;
then (the InternalRel of A) is_strongly_connected_in S by A1,ORDERS_1:11;
hence thesis by CKB30:def 1;
end;
