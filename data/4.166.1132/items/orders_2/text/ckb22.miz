theorem
Th24: (for A being  reflexive non empty RelStr holds (for a being (Element of A) holds a <= a))
proof
let A being  reflexive non empty RelStr;
let a being (Element of A);
(the InternalRel of A) is_reflexive_in (the carrier of A) by CKB4:def 1;
then [ a,a ] in (the InternalRel of A) by RELAT_2:def 1;
hence thesis by CKB18:def 1;
end;
