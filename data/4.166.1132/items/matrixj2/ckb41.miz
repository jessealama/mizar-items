.= ( Sum ( lmlt (( Line (( ( LineVec2Mx ( Line (( 1. (K,( len b1 )) ),BI) ) ) * M ),1) ),B2) ) ) by A23,MATRLIN2:19
.= ( Sum ( lmlt (( Line (( LineVec2Mx ( Line (( ( 1. (K,( len b1 )) ) * M ),BI) ) ),1) ),B2) ) ) by A1,A12,A8,A15,A10,A22,FINSEQ_1:28,MATRLIN2:35
