.= ( Sum ( lmlt (( Line (( LineVec2Mx ( Line (( ( 1. (K,( len bw1 )) ) * M1 ),i) ) ),1) ),Bu1) ) ) by A16,MATRIXR2:68
.= ( Sum ( lmlt (( Line (( ( LineVec2Mx ( Line (( 1. (K,( len bw1 )) ),i) ) ) * M1 ),1) ),Bu1) ) ) by A14,A9,A16,A19,MATRIX_1:24,MATRLIN2:35
