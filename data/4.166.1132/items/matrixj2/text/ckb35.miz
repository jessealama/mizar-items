begin
reserve i for Nat;
reserve j for Nat;
reserve n for Nat;
reserve k for Nat;
reserve K for Field;
definition
let K being doubleLoopStr;
let V being non  empty VectSpStr over K;
let f being  nilpotent (Function of V,V);
func degree_of_nilpotent f -> Nat means 
:Def5: (( f |^ it ) = ( ZeroMap (V,V) ) & (for k holds (( f |^ k ) = ( ZeroMap (V,V) ) implies it <= k)));
existence
proof
defpred P[ Nat ]
 means
( f |^ $1 ) = ( ZeroMap (V,V) );
A1: (ex n st P[ n ]) by CKB31:1;
consider n such that A2: (P[ n ] & (for k holds (P[ k ] implies n <= k))) from NAT_1:sch 5(A1);
take n;
thus thesis by A2;
end;
uniqueness
proof
let i;
let j;
assume (((( f |^ i ) = ( ZeroMap (V,V) ) & (for k holds (( f |^ k ) = ( ZeroMap (V,V) ) implies i <= k))) & ( f |^ j ) = ( ZeroMap (V,V) )) & (for k holds (( f |^ k ) = ( ZeroMap (V,V) ) implies j <= k)));
then (i <= j & j <= i);
hence thesis by XXREAL_0:1;
end;
end;
