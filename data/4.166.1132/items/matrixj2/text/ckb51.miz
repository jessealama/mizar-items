begin
reserve K for Field;
theorem
Lm4: (for V1,V2 being (VectSp of K) holds (for f being (linear-transformation of V1,V2) holds (for W1 being (Subspace of V1) holds (for W2 being (Subspace of V2) holds (for F being (Function of W1,W2) holds (F = ( f | W1 ) implies F is (linear-transformation of W1,W2)))))))
proof
let V1 being (VectSp of K),V2 being (VectSp of K);
let f being (linear-transformation of V1,V2);
let W1 being (Subspace of V1);
let W2 being (Subspace of V2);
let F being (Function of W1,W2);
assume that
A1: F = ( f | W1 );
A2:now
let a being (Scalar of K);
let w being (Vector of W1);
thus ( F . ( a * w ) ) = ( a * ( ( f | W1 ) . w ) ) by A1,MOD_2:def 2
.= ( a * ( F . w ) ) by A1,VECTSP_4:14;
end;
now
let w1 being (Vector of W1),w2 being (Vector of W1);
thus ( F . ( w1 + w2 ) ) = ( ( ( f | W1 ) . w1 ) + ( ( f | W1 ) . w2 ) ) by A1,GRCAT_1:def 8
.= ( ( F . w1 ) + ( F . w2 ) ) by A1,VECTSP_4:13;
end;
then F is  additive  homogeneous by A2,GRCAT_1:def 8,MOD_2:def 2;
hence thesis;
end;
