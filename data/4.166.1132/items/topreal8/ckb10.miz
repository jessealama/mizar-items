theorem
Th8: (for f,g being FinSequence holds (for x being set holds (x in ( rng f ) implies ( x .. f ) = ( x .. ( f ^' g ) ))))
proof
let f being FinSequence,g being FinSequence;
let x being set;
assume A1: x in ( rng f );
then A2: ( f . ( x .. f ) ) = x by FINSEQ_4:19;
A3: ( x .. f ) in ( dom f ) by A1,FINSEQ_4:20;
then A4: ( x .. f ) <= ( len f ) by FINSEQ_3:25;
A5: (for i being Nat holds ((1 <= i & i < ( x .. f )) implies ( ( f ^' g ) . i ) <> x))
proof
let i being Nat;
assume that
A6: 1 <= i
and
A7: i < ( x .. f );
A8: i < ( len f ) by A4,A7,XXREAL_0:2;
then A9: i in ( dom f ) by A6,FINSEQ_3:25;
i in ( NAT ) by ORDINAL1:def 12;
then ( ( f ^' g ) . i ) = ( f . i ) by A6,A8,GRAPH_2:14;
hence thesis by A7,A9,FINSEQ_4:24;
end;
( len f ) <= ( len ( f ^' g ) ) by CKB9:1;
then A10: ( dom f ) c= ( dom ( f ^' g ) ) by FINSEQ_3:30;
1 <= ( x .. f ) by A3,FINSEQ_3:25;
then ( ( f ^' g ) . ( x .. f ) ) = x by A2,A4,GRAPH_2:14;
hence thesis by A3,A10,A5,FINSEQ_6:2;
end;
