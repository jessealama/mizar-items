environ
vocabularies GROUP_1,XBOOLE_0,SUBSET_1,GROUP_2,PRE_TOPC,RELAT_1,TARSKI,STRUCT_0;
notations TARSKI,XBOOLE_0,SUBSET_1,STRUCT_0,ALGSTR_0,GROUP_1,GROUP_2,GROUP_3,GROUP_4,CKB12,CKB13;
definitions XBOOLE_0,GROUP_2,TARSKI,GROUP_4,CKB12,CKB13;
theorems GROUP_1,GROUP_8,GROUP_2,GROUP_3,XBOOLE_0,XBOOLE_1,GROUP_4,STRUCT_0,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20;
registrations XBOOLE_0,STRUCT_0,GROUP_1,GROUP_2,GROUP_3;
constructors REALSET2,GROUP_4,CKB12,CKB13;
requirements SUBSET,BOOLE;
begin
reserve G for Group;
reserve A for non  empty (Subset of G);
reserve B for non  empty (Subset of G);
reserve N for (Subgroup of G);
reserve x for (Element of G);
theorem
( N ~ ( A \/ B ) ) = ( ( N ~ A ) \/ ( N ~ B ) )
proof
thus ( N ~ ( A \/ B ) ) c= ( ( N ~ A ) \/ ( N ~ B ) )
proof
let x being set;
assume A1: x in ( N ~ ( A \/ B ) );
then reconsider x as (Element of G);
( x * N ) meets ( A \/ B ) by A1,CKB16:1;
then (( x * N ) meets A or ( x * N ) meets B) by XBOOLE_1:70;
then (x in ( N ~ A ) or x in ( N ~ B ));
hence thesis by XBOOLE_0:def 3;
end;

let x being set;
assume A2: x in ( ( N ~ A ) \/ ( N ~ B ) );
then reconsider x as (Element of G);
(x in ( N ~ A ) or x in ( N ~ B )) by A2,XBOOLE_0:def 3;
then (( x * N ) meets A or ( x * N ) meets B) by CKB16:1;
then ( x * N ) meets ( A \/ B ) by XBOOLE_1:70;
hence thesis;
end;
