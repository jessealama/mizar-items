environ
vocabularies GROUP_1,XBOOLE_0,SUBSET_1,GROUP_2,PRE_TOPC,RELAT_1,TARSKI,STRUCT_0;
notations TARSKI,XBOOLE_0,SUBSET_1,STRUCT_0,ALGSTR_0,GROUP_1,GROUP_2,GROUP_3,GROUP_4;
definitions XBOOLE_0,GROUP_2,TARSKI,GROUP_4;
theorems GROUP_1,GROUP_8,GROUP_2,GROUP_3,XBOOLE_0,XBOOLE_1,GROUP_4,STRUCT_0;
registrations XBOOLE_0,STRUCT_0,GROUP_1,GROUP_2,GROUP_3;
constructors REALSET2,GROUP_4;
requirements SUBSET,BOOLE;
begin
reserve G for Group;
theorem
Th1: (for N being  normal (Subgroup of G) holds (for x1,x2 being (Element of G) holds ( ( x1 * N ) * ( x2 * N ) ) = ( ( x1 * x2 ) * N )))
proof
let N being  normal (Subgroup of G);
let x1 being (Element of G),x2 being (Element of G);
( ( x1 * N ) * ( x2 * N ) ) = ( ( ( x1 * N ) * x2 ) * N ) by GROUP_2:10
.= ( ( x1 * ( N * x2 ) ) * N ) by GROUP_2:29
.= ( ( x1 * ( x2 * N ) ) * N ) by GROUP_3:117
.= ( ( ( x1 * x2 ) * N ) * N ) by GROUP_2:105
.= ( ( x1 * x2 ) * ( N * N ) ) by GROUP_2:29;
hence thesis by GROUP_2:76;
end;
