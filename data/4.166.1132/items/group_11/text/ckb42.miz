environ
vocabularies GROUP_1,XBOOLE_0,SUBSET_1,GROUP_2,PRE_TOPC,RELAT_1,TARSKI,STRUCT_0;
notations TARSKI,XBOOLE_0,SUBSET_1,STRUCT_0,ALGSTR_0,GROUP_1,GROUP_2,GROUP_3,GROUP_4,CKB12,CKB13;
definitions XBOOLE_0,GROUP_2,TARSKI,GROUP_4,CKB12,CKB13;
theorems GROUP_1,GROUP_8,GROUP_2,GROUP_3,XBOOLE_0,XBOOLE_1,GROUP_4,STRUCT_0,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB37,CKB38,CKB39,CKB40,CKB41;
registrations XBOOLE_0,STRUCT_0,GROUP_1,GROUP_2,GROUP_3;
constructors REALSET2,GROUP_4,CKB12,CKB13;
requirements SUBSET,BOOLE;
begin
reserve G for Group;
reserve A for non empty (Subset of G);
reserve N for (Subgroup of G);
theorem
( N ~ ( N ` ( N ~ A ) ) ) = ( N ~ A )
proof
thus ( N ~ ( N ` ( N ~ A ) ) ) c= ( N ~ A )
proof
let x being set;
assume A1: x in ( N ~ ( N ` ( N ~ A ) ) );
then reconsider x as (Element of G);
( x * N ) meets ( N ` ( N ~ A ) ) by A1,CKB32:1;
then consider y being set such that A2: (y in ( x * N ) & y in ( N ` ( N ~ A ) )) by XBOOLE_0:3;
reconsider y as (Element of G) by A2;
( y * N ) c= ( N ~ A ) by A2,CKB33:1;
then A3: ( x * N ) c= ( N ~ A ) by A2,CKB2:1;
x in ( x * N ) by GROUP_2:108;
hence thesis by A3;
end;

thus ( N ~ A ) c= ( N ~ ( N ` ( N ~ A ) ) )
proof
A c= ( N ` ( N ~ A ) )
proof
let x being set;
assume A4: x in A;
then reconsider x as (Element of G);
( x * N ) c= ( N ~ A )
proof
let y being set;
assume A5: y in ( x * N );
then reconsider y as (Element of G);
( y * N ) = ( x * N ) by A5,CKB2:1;
then x in ( y * N ) by GROUP_2:108;
then ( y * N ) meets A by A4,XBOOLE_0:3;
hence thesis;
end;
hence thesis;
end;
hence thesis by CKB41:1;
end;

end;
