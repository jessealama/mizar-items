begin
reserve G for Group;
reserve N for (Subgroup of G);
reserve H for (Subgroup of G);
reserve x for (Element of G);
reserve N1 for (Subgroup of G);
reserve N2 for (Subgroup of G);
theorem
(for N,N1,N2 being (Subgroup of G) holds (N = ( N1 /\ N2 ) implies ( N ~ H ) c= ( ( N1 ~ H ) /\ ( N2 ~ H ) )))
proof
let N being (Subgroup of G);
let N1 being (Subgroup of G);
let N2 being (Subgroup of G);
assume N = ( N1 /\ N2 );
then A1: (N is (Subgroup of N1) & N is (Subgroup of N2)) by GROUP_2:88;
let x being set;
assume A2: x in ( N ~ H );
(( N ~ H ) c= ( N1 ~ H ) & ( N ~ H ) c= ( N2 ~ H )) by A1,CKB61:1;
hence thesis by A2,XBOOLE_0:def 4;
end;
