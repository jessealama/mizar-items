begin
reserve G for Group;
reserve A for non  empty (Subset of G);
reserve N for (Subgroup of G);
reserve x for (Element of G);
theorem
Th32: (for x being (Element of G) holds (x in ( N ~ ( N ~ A ) ) implies ( x * N ) meets ( N ~ A )))
proof
let x being (Element of G);
assume x in ( N ~ ( N ~ A ) );
then (ex x1 being (Element of G) st (x = x1 & ( x1 * N ) meets ( N ~ A )));
hence thesis;
end;
