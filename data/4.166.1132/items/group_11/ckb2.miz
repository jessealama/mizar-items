begin
theorem
Th2: (for G being Group holds (for N being (Subgroup of G) holds (for x,y being (Element of G) holds (y in ( x * N ) implies ( x * N ) = ( y * N )))))
proof
let G being Group;
let N being (Subgroup of G);
let x being (Element of G),y being (Element of G);
assume A1: y in ( x * N );
y in ( y * N ) by GROUP_2:108;
then ( x * N ) meets ( y * N ) by A1,XBOOLE_0:3;
hence thesis by GROUP_2:115;
end;
