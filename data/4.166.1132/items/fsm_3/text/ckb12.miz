begin
reserve E for non  empty set;
reserve u for (Element of ( E ^omega ));
reserve v for (Element of ( E ^omega ));
reserve w for (Element of ( E ^omega ));
reserve F for (Subset of ( E ^omega ));
reserve TS for non  empty transition-system over F;
theorem
Th12: (for TS being transition-system over ( Lex E ) holds ((the Tran of TS) is Function implies TS is  deterministic))
proof
let TS being transition-system over ( Lex E );
assume that
A1: (the Tran of TS) is Function;
A2:now
let p being (Element of TS);
let u;
let v;
assume that
A3: u <> v
and
A4: ([ p,u ] in ( dom (the Tran of TS) ) & [ p,v ] in ( dom (the Tran of TS) ));
(u in ( Lex E ) & v in ( Lex E )) by A4,ZFMISC_1:87;
hence (not (ex w st (( u ^ w ) = v or ( v ^ w ) = u))) by A3,CKB11:1;
end;
(not ( <%> E ) in ( rng ( dom (the Tran of TS) ) )) by CKB9:1;
hence thesis by A1,A2,REWRITE3:def 1;
end;
