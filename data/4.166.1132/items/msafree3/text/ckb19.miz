begin
theorem
Th10: (for S being ManySortedSign holds (for X being (ManySortedSet of (the carrier of S)) holds (for t being non empty Relation holds (for V being (ManySortedSubset of X) holds (V = ( X variables_in t ) iff (for s being set holds (s in (the carrier of S) implies ( V . s ) = ( ( X . s ) /\ { ( a `1 ) where a is (Element of ( rng t )): ( a `2 ) = s } ))))))))
proof
let S being ManySortedSign;
let X being (ManySortedSet of (the carrier of S));
let t being non empty Relation;
let V being (ManySortedSubset of X);
hereby
assume A1: V = ( X variables_in t );
let s being set;
assume A2: s in (the carrier of S);
then ( V . s ) = ( ( X . s ) /\ ( ( S variables_in t ) . s ) ) by A1,PBOOLE:def 5;
hence ( V . s ) = ( ( X . s ) /\ { ( a `1 ) where a is (Element of ( rng t )): ( a `2 ) = s } ) by A2,CKB17:def 1;
end;
assume A3: (for s being set holds (s in (the carrier of S) implies ( V . s ) = ( ( X . s ) /\ { ( a `1 ) where a is (Element of ( rng t )): ( a `2 ) = s } )));
now
let s being set;
assume A4: s in (the carrier of S);
hence ( V . s ) = ( ( X . s ) /\ { ( a `1 ) where a is (Element of ( rng t )): ( a `2 ) = s } ) by A3
.= ( ( X . s ) /\ ( ( S variables_in t ) . s ) ) by A4,CKB17:def 1;
end;
hence thesis by PBOOLE:def 5;
end;
