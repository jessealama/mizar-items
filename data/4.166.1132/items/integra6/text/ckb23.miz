constructors PARTFUN1,REAL_1,NAT_1,BINOP_2,COMPLEX1,RFUNCT_1,RFUNCT_2,FCONT_1,FDIFF_1,RFINSEQ,RSSPACE,MEASURE6,INTEGRA2,INTEGRA4,INTEGRA5,SEQ_1,XXREAL_2,RVSUM_1,SEQ_4,RELSET_1,FUNCOP_1,SEQ_2,INTEGRA3;
requirements NUMERALS,BOOLE,SUBSET,REAL,ARITHM;
begin
reserve a for  real number;
reserve b for  real number;
reserve c for  real number;
reserve d for  real number;
reserve f for (PartFunc of ( REAL ),( REAL ));
theorem
Lm4: (for a,b,c,d being  real number holds (for f being (PartFunc of ( REAL ),( REAL )) holds (((((((a <= b & c <= d) & f is_integrable_on [' a,b ']) & ( f | [' a,b '] ) is  bounded) & [' a,b '] c= ( dom f )) & c in [' a,b ']) & d in [' a,b ']) implies ( integral (f,a,d) ) = ( ( integral (f,a,c) ) + ( integral (f,c,d) ) ))))
proof
let a being  real number;
let b being  real number;
let c being  real number;
let d being  real number;
let f being (PartFunc of ( REAL ),( REAL ));
assume that
A1: a <= b
and
A2: c <= d
and
A3: (f is_integrable_on [' a,b '] & ( f | [' a,b '] ) is  bounded)
and
A4: [' a,b '] c= ( dom f )
and
A5: c in [' a,b ']
and
A6: d in [' a,b '];
A7: f is_integrable_on [' a,d '] by A1,A3,A4,A6,CKB19:1;
A8: [' a,b '] = [. a,b .] by A1,INTEGRA5:def 3;
then A9: a <= d by A6,XXREAL_1:1;
A10: d <= b by A6,A8,XXREAL_1:1;
then [' a,d '] c= [' a,b '] by A9,CKB20:1;
then A11: [' a,d '] c= ( dom f ) by A4,XBOOLE_1:1;
a <= c by A5,A8,XXREAL_1:1;
then A12: c in [' a,d '] by A2,CKB20:1;
( f | [' a,d '] ) is  bounded by A3,A4,A9,A10,CKB21:1;
hence thesis by A9,A12,A11,A7,CKB19:1;
end;
