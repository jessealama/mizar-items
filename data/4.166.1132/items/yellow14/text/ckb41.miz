begin
theorem
Th26: (for S,T being TopStruct holds (for A being (Subset of S) holds (for B being (Subset of T) holds (((A = B & ( the TopStruct of S ) = ( the TopStruct of T )) & A is  compact) implies B is  compact))))
proof
let S being TopStruct;
let T being TopStruct;
let A being (Subset of S);
let B being (Subset of T);
assume that
A1: A = B
and
A2: ( the TopStruct of S ) = ( the TopStruct of T )
and
A3: (for F being (Subset-Family of S) holds ((F is (Cover of A) & F is  open) implies (ex G being (Subset-Family of S) st ((G c= F & G is (Cover of A)) & G is  finite))));
let F being (Subset-Family of T);
assume that
A4: (F is (Cover of B) & F is  open);
reconsider K = F as (Subset-Family of S) by A2;
consider L being (Subset-Family of S) such that A5: ((L c= K & L is (Cover of A)) & L is  finite) by A1,A2,A3,A4,WAYBEL_9:19;
reconsider G = L as (Subset-Family of T) by A2;
take G;
thus thesis by A1,A5;
end;
