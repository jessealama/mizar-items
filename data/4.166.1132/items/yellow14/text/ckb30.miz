begin
theorem
(for S,T being non empty RelStr holds (for A being (Subset of S) holds (for f being (Function of S,T) holds ((f is  isomorphic &  ex_sup_of A,S) implies  ex_sup_of ( f .: A ),T))))
proof
let S being non empty RelStr,T being non empty RelStr;
let A being (Subset of S);
let f being (Function of S,T);
assume that
A1: f is  isomorphic;
A2: ( f " ( f .: A ) ) c= A by A1,FUNCT_1:82;
A3: ( rng f ) = ( [#] T ) by A1,WAYBEL_0:66;
A4: ( f /" ) = ( (f qua Function) " ) by A1,TOPS_2:def 4;
given a being (Element of S) such that
A5: A is_<=_than a
and
A6: (for b being (Element of S) holds (A is_<=_than b implies b >= a))
and
A7: (for c being (Element of S) holds ((A is_<=_than c & (for b being (Element of S) holds (A is_<=_than b implies b >= c))) implies c = a));

take ( f . a );
thus ( f .: A ) is_<=_than ( f . a ) by A1,A5,WAYBEL13:19;
A8: ( f /" ) is  isomorphic by A1,CKB23:1;
A9: ( dom f ) = (the carrier of S) by FUNCT_2:def 1;
then A c= ( f " ( f .: A ) ) by FUNCT_1:76;
then A10: ( f " ( f .: A ) ) = A by A2,XBOOLE_0:def 10;
hereby
let b being (Element of T);
assume ( f .: A ) is_<=_than b;
then ( ( f /" ) .: ( f .: A ) ) is_<=_than ( ( f /" ) . b ) by A8,WAYBEL13:19;
then ( f " ( f .: A ) ) is_<=_than ( ( f /" ) . b ) by A1,A3,TOPS_2:55;
then ( ( f /" ) . b ) >= a by A6,A10;
then ( f . ( ( f /" ) . b ) ) >= ( f . a ) by A1,WAYBEL_0:66;
hence b >= ( f . a ) by A1,A3,A4,FUNCT_1:35;
end;
let c being (Element of T);
assume A11: ( f .: A ) is_<=_than c;
assume A12: (for b being (Element of T) holds (( f .: A ) is_<=_than b implies b >= c));
A13: (for b being (Element of S) holds (A is_<=_than b implies b >= ( ( f /" ) . c )))
proof
let b being (Element of S);
assume A is_<=_than b;
then ( f .: A ) is_<=_than ( f . b ) by A1,WAYBEL13:19;
then ( f . b ) >= c by A12;
then ( ( f /" ) . ( f . b ) ) >= ( ( f /" ) . c ) by A8,WAYBEL_0:66;
hence thesis by A1,A4,A9,FUNCT_1:34;
end;
( ( f /" ) .: ( f .: A ) ) is_<=_than ( ( f /" ) . c ) by A8,A11,WAYBEL13:19;
then A is_<=_than ( ( f /" ) . c ) by A1,A3,A10,TOPS_2:55;
then ( ( f /" ) . c ) = a by A7,A13;
hence thesis by A1,A3,A4,FUNCT_1:35;
end;
