begin
definition
let S being non  void  Circuit-like non  empty ManySortedSign;
let A being  non-empty (Circuit of S);
let s being (State of A);
assume that
A1: s is  stabilizing;
func Result s -> (State of A) means 
:Def4: (it is  stable & (ex n being (Element of ( NAT )) st it = ( Following (s,n) )));
existence
proof
consider n being (Element of ( NAT )) such that A2: ( Following (s,n) ) is  stable by A1,CKB2:def 1;
take ( Following (s,n) );
thus thesis by A2;
end;
uniqueness
proof
let s1 being (State of A);
let s2 being (State of A);
assume that
A3: s1 is  stable
and
A4: (ex n being (Element of ( NAT )) st s1 = ( Following (s,n) ))
and
A5: s2 is  stable
and
A6: (ex n being (Element of ( NAT )) st s2 = ( Following (s,n) ));
consider n1 being (Element of ( NAT )) such that A7: s1 = ( Following (s,n1) ) by A4;
consider n2 being (Element of ( NAT )) such that A8: s2 = ( Following (s,n2) ) by A6;
per cases ;
suppose n1 <= n2;

hence thesis by A3,A7,A8,CIRCCMB2:4;
end;
suppose n2 <= n1;

hence thesis by A5,A7,A8,CIRCCMB2:4;
end;
end;
end;
