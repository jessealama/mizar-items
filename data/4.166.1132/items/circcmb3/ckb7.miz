begin
definition
let S being non void  Circuit-like non empty ManySortedSign;
let A being  non-empty (Circuit of S);
let s being (State of A);
assume that
A1: s is  stabilizing;
func stabilization-time s -> (Element of ( NAT ))
means
:Def5: (( Following (s,it) ) is  stable & (for n being (Element of ( NAT )) holds (n < it implies (not ( Following (s,n) ) is  stable))));
existence
proof
defpred P[ Nat ]
 means
( Following (s,$1) ) is  stable;
(ex k being (Element of ( NAT )) st P[ k ]) by A1,CKB2:def 1;
then A2: (ex k being Nat st P[ k ]);
consider m being Nat such that A3: (P[ m ] & (for n being Nat holds (P[ n ] implies m <= n))) from NAT_1:sch 5(A2);
reconsider m as (Element of ( NAT )) by ORDINAL1:def 12;
take m;
thus thesis by A3;
end;
uniqueness
proof
let n1 being (Element of ( NAT )),n2 being (Element of ( NAT ));
assume that
A4: ( Following (s,n1) ) is  stable
and
A5: ((for n being (Element of ( NAT )) holds (n < n1 implies (not ( Following (s,n) ) is  stable))) & ( Following (s,n2) ) is  stable)
and
A6: (for n being (Element of ( NAT )) holds (n < n2 implies (not ( Following (s,n) ) is  stable)));
assume A7: n1 <> n2;
per cases  by A7,XXREAL_0:1;
suppose n1 < n2;

hence contradiction by A4,A6;
end;
suppose n2 < n1;

hence contradiction by A5;
end;
end;
end;
