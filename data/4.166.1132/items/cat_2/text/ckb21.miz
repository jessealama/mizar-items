begin
reserve C for Category;
reserve E for (Subcategory of C);
theorem
Th15: (for f being (Morphism of E) holds (for f9 being (Morphism of C) holds (f = f9 implies (( dom f ) = ( dom f9 ) & ( cod f ) = ( cod f9 )))))
proof
let f being (Morphism of E);
let f9 being (Morphism of C);
assume that
A1: f = f9;
set a = ( dom f );
set b = ( cod f );
reconsider a9 = a,b9 = b as (Object of C) by CKB18:1;
(f in ( Hom (a,b) ) & ( Hom (a,b) ) c= ( Hom (a9,b9) )) by CKB16:def 1;
hence thesis by A1,CAT_1:1;
end;
