begin
reserve C for Category;
reserve D for Category;
theorem
(for c,c9 being (Object of C) holds (for f being (Morphism of c,c9) holds (for d,d9 being (Object of D) holds (for g being (Morphism of d,d9) holds ((( Hom (c,c9) ) <> ( {} ) & ( Hom (d,d9) ) <> ( {} )) implies [ f,g ] is (Morphism of [ c,d ],[ c9,d9 ]))))))
proof
let c being (Object of C),c9 being (Object of C);
let f being (Morphism of c,c9);
let d being (Object of D),d9 being (Object of D);
let g being (Morphism of d,d9);
assume A1: (( Hom (c,c9) ) <> ( {} ) & ( Hom (d,d9) ) <> ( {} ));
then (( cod f ) = c9 & ( cod g ) = d9) by CAT_1:5;
then A2: ( cod [ f,g ] ) = [ c9,d9 ] by CKB49:1;
(( dom f ) = c & ( dom g ) = d) by A1,CAT_1:5;
then ( dom [ f,g ] ) = [ c,d ] by CKB49:1;
hence thesis by A2,CAT_1:4;
end;
