begin
reserve B for Category;
reserve C for Category;
definition
let B;
let C;
let c being (Object of C);
func B --> c -> (Functor of B,C)
equals
( (the carrier' of B) --> ( id c ) );
coherence
proof
reconsider T = ( (the carrier' of B) --> ( id c ) ) as (Function of (the carrier' of B),(the carrier' of C));
now
thus (for b being (Object of B) holds (ex d being (Object of C) st ( T . ( id b ) ) = ( id d ))) by FUNCOP_1:7;
thus (for f being (Morphism of B) holds (( T . ( id ( dom f ) ) ) = ( id ( dom ( T . f ) ) ) & ( T . ( id ( cod f ) ) ) = ( id ( cod ( T . f ) ) )))
proof
let f being (Morphism of B);
( T . ( id ( cod f ) ) ) = ( id c ) by FUNCOP_1:7;
then A1: ( T . ( id ( cod f ) ) ) = ( id ( cod ( id c ) ) ) by CAT_1:19;
( T . ( id ( dom f ) ) ) = ( id c ) by FUNCOP_1:7;
then ( T . ( id ( dom f ) ) ) = ( id ( dom ( id c ) ) ) by CAT_1:19;
hence thesis by A1,FUNCOP_1:7;
end;

let f being (Morphism of B),g being (Morphism of B);
assume that
( dom g ) = ( cod f );
( Hom (c,c) ) <> ( {} ) by CAT_1:27;
then A2: (( T . f ) = ( id c ) & ( ( id c ) * ( id c ) ) = ( ( id c ) * (( id c ) qua (Morphism of C)) )) by CAT_1:def 10,FUNCOP_1:7;
(( T . ( g * f ) ) = ( id c ) & ( T . g ) = ( id c )) by FUNCOP_1:7;
hence ( T . ( g * f ) ) = ( ( T . g ) * ( T . f ) ) by A2,CAT_1:30;
end;
hence thesis by CAT_1:61;
end;
end;
