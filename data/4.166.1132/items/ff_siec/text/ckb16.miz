begin
theorem
Lm1: (for A,B,C,D being set holds (((B misses D & A c= B) & C c= D) implies A misses C))
proof
let A being set,B being set,C being set,D being set;
assume that
A1: B misses D
and
A2: A c= B
and
A3: C c= D;
assume A meets C;
then A4: (ex x being set st x in ( A /\ C )) by XBOOLE_0:4;
( A /\ C ) c= ( B /\ D ) by A2,A3,XBOOLE_1:27;
hence thesis by A1,A4,XBOOLE_0:4;
end;
