begin
theorem
Th36: (for V being non  empty addLoopStr holds (for M being (Subset of V) holds (for v being (Element of V) holds ( { v } + M ) = ( v + M ))))
proof
let V being non  empty addLoopStr;
let M being (Subset of V);
let v being (Element of V);
(for x being set holds (x in ( v + M ) implies x in ( { v } + M )))
proof
let x being set;
assume x in ( v + M );
then A1: (ex u being (Element of V) st (x = ( v + u ) & u in M));
v in { v } by TARSKI:def 1;
hence thesis by A1;
end;
then A2: ( v + M ) c= ( { v } + M ) by TARSKI:def 3;
(for x being set holds (x in ( { v } + M ) implies x in ( v + M )))
proof
let x being set;
assume x in ( { v } + M );
then consider v1,u1 being (Element of V) such that A3: x = ( v1 + u1 ) and A4: v1 in { v } and A5: u1 in M;
v1 = v by A4,TARSKI:def 1;
hence thesis by A3,A5;
end;
then ( { v } + M ) c= ( v + M ) by TARSKI:def 3;
hence thesis by A2,XBOOLE_0:def 10;
end;
