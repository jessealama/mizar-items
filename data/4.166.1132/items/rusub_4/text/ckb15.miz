begin
definition
let V being RealUnitarySpace;
assume A1: V is  finite-dimensional;
func dim V -> (Element of ( NAT )) means 
:Def3: (for I being (Basis of V) holds it = ( card I ));
existence
proof
consider A being  finite (Subset of V) such that A2: A is (Basis of V) by A1,CKB5:def 1;
consider n being (Element of ( NAT )) such that A3: n = ( card A );
(for I being (Basis of V) holds ( card I ) = n) by A1,A2,A3,CKB9:1;
hence thesis;
end;
uniqueness
proof
let n being (Element of ( NAT ));
let m being (Element of ( NAT ));
assume that
A4: (for I being (Basis of V) holds ( card I ) = n)
and
A5: (for I being (Basis of V) holds ( card I ) = m);
consider A being  finite (Subset of V) such that A6: A is (Basis of V) by A1,CKB5:def 1;
( card A ) = n by A4,A6;
hence thesis by A5,A6;
end;
end;
