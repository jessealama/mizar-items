begin
theorem
(for V being RealLinearSpace holds (for W being (Subspace of V) holds (( Up W ) is  Affine & ( 0. V ) in (the carrier of W))))
proof
let V being RealLinearSpace;
let W being (Subspace of V);
(for x,y being (VECTOR of V) holds (for a being Real holds ((x in ( Up W ) & y in ( Up W )) implies ( ( ( 1 - a ) * x ) + ( a * y ) ) in ( Up W ))))
proof
let x being (VECTOR of V),y being (VECTOR of V);
let a being Real;
assume that
A1: x in ( Up W )
and
A2: y in ( Up W );
y in W by A2,STRUCT_0:def 5;
then A3: ( a * y ) in W by RLSUB_1:21;
x in W by A1,STRUCT_0:def 5;
then ( ( 1 - a ) * x ) in W by RLSUB_1:21;
then ( ( ( 1 - a ) * x ) + ( a * y ) ) in W by A3,RLSUB_1:20;
hence thesis by STRUCT_0:def 5;
end;
hence ( Up W ) is  Affine by CKB32:def 1;
( 0. V ) in W by RLSUB_1:17;
hence thesis by STRUCT_0:def 5;
end;
