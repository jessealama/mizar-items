begin
theorem
(for V being RealUnitarySpace holds (for W being  strict (Subspace of V) holds (for M being (Subset of V) holds (for v being (VECTOR of V) holds (( Up W ) = M implies ( v + W ) = ( v + M ))))))
proof
let V being RealUnitarySpace;
let W being  strict (Subspace of V);
let M being (Subset of V);
let v being (VECTOR of V);
assume A1: ( Up W ) = M;
(for x being set holds (x in ( v + M ) implies x in ( v + W )))
proof
let x being set;
assume x in ( v + M );
then consider u being (Element of V) such that A2: x = ( v + u ) and A3: u in M;
u in W by A1,A3,STRUCT_0:def 5;
then x in { ( v + u9 ) where u9 is (VECTOR of V): u9 in W } by A2;
hence thesis by RUSUB_1:def 4;
end;
then A4: ( v + M ) c= ( v + W ) by TARSKI:def 3;
(for x being set holds (x in ( v + W ) implies x in ( v + M )))
proof
let x being set;
assume x in ( v + W );
then x in { ( v + u ) where u is (VECTOR of V): u in W } by RUSUB_1:def 4;
then consider u being (VECTOR of V) such that A5: x = ( v + u ) and A6: u in W;
u in M by A1,A6,STRUCT_0:def 5;
hence thesis by A5;
end;
then ( v + W ) c= ( v + M ) by TARSKI:def 3;
hence thesis by A4,XBOOLE_0:def 10;
end;
