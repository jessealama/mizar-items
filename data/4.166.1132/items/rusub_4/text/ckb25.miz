begin
theorem
(for V being  finite-dimensional RealUnitarySpace holds (for W1,W2 being (Subspace of V) holds (V is_the_direct_sum_of W1,W2 implies ( dim V ) = ( ( dim W1 ) + ( dim W2 ) ))))
proof
let V being  finite-dimensional RealUnitarySpace;
let W1 being (Subspace of V),W2 being (Subspace of V);
assume A1: V is_the_direct_sum_of W1,W2;
then A2: ( the UNITSTR of V ) = ( W1 + W2 ) by RUSUB_2:def 4;
( W1 /\ W2 ) = ( (0). V ) by A1,RUSUB_2:def 4;
then ( (Omega). ( W1 /\ W2 ) ) = ( (0). V ) by RUSUB_1:def 3
.= ( (0). ( W1 /\ W2 ) ) by RUSUB_1:30;
then ( dim ( W1 /\ W2 ) ) = ( 0 ) by CKB20:1;
then ( ( dim W1 ) + ( dim W2 ) ) = ( ( dim ( W1 + W2 ) ) + ( 0 ) ) by CKB23:1
.= ( dim ( (Omega). V ) ) by A2,RUSUB_1:def 3
.= ( dim V ) by CKB18:1;
hence thesis;
end;
