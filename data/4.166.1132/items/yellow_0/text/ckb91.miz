begin
theorem
Th63: (for L being non  empty RelStr holds (for S being non  empty (SubRelStr of L) holds (for X being (Subset of S) holds (for a being (Element of L) holds (for x being (Element of S) holds (x = a implies ((x is_<=_than X implies a is_<=_than X) & (x is_>=_than X implies a is_>=_than X))))))))
proof
let L being non  empty RelStr;
let S being non  empty (SubRelStr of L);
let X being (Subset of S);
let a being (Element of L);
let x being (Element of S);
assume A1: x = a;
hereby
assume A2: x is_<=_than X;
thus a is_<=_than X
proof
let b being (Element of L);
assume A3: b in X;
then reconsider y = b as (Element of S);
x <= y by A2,A3,LATTICE3:def 8;
hence thesis by A1,CKB88:1;
end;

end;
assume A4: x is_>=_than X;
let b being (Element of L);
assume A5: b in X;
then reconsider y = b as (Element of S);
x >= y by A4,A5,LATTICE3:def 9;
hence thesis by A1,CKB88:1;
end;
