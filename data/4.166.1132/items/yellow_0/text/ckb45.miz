environ
vocabularies XBOOLE_0,STRUCT_0,ORDERS_2,SUBSET_1,XXREAL_0,RELAT_1,RELAT_2,REWRITE1,LATTICE3,ZFMISC_1,ORDERS_1,ARYTM_3,TARSKI,LATTICES,XXREAL_2,EQREL_1,FILTER_0,PBOOLE,ORDINAL2,FINSET_1,CAT_1,WELLORD1,YELLOW_0,CARD_1;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,RELAT_2,RELSET_1,FINSET_1,TOLER_1,ORDERS_1,DOMAIN_1,CARD_1,STRUCT_0,LATTICES,ORDERS_2,LATTICE3,CKB2,CKB3,CKB4,CKB22,CKB23,CKB24,CKB30,CKB31,CKB44;
definitions STRUCT_0,RELAT_2,ORDERS_2,LATTICE3,WELLORD1,CKB2,CKB3,CKB4,CKB22,CKB23,CKB24,CKB30,CKB31,CKB44;
theorems TARSKI,ZFMISC_1,RELAT_2,ORDERS_2,LATTICE3,XBOOLE_0,XBOOLE_1,STRUCT_0,CKB2,CKB3,CKB4,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB22,CKB23,CKB24,CKB25,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB37,CKB38,CKB39,CKB40,CKB41,CKB42,CKB43,CKB44;
schemes FINSET_1,RELSET_1,CKB1;
registrations XBOOLE_0,RELSET_1,FINSET_1,STRUCT_0,ORDERS_2,LATTICE3,CARD_1,ORDINAL1,CKB5,CKB6,CKB7,CKB8,CKB21,CKB26,CKB27,CKB28,CKB29;
constructors TOLER_1,REALSET2,LATTICE3,CKB22,CKB23,CKB24,CKB30,CKB31,CKB44;
requirements BOOLE,SUBSET,NUMERALS;
begin
definition
let L being RelStr;
let X being set;
func "/\" (X,L) -> (Element of L) means 
:Def10: (X is_>=_than it & (for a being (Element of L) holds (X is_>=_than a implies a <= it))) if  ex_inf_of X,L;
uniqueness
proof
let a1 being (Element of L);
let a2 being (Element of L);
given a being (Element of L) such that
X is_>=_than a
and
(for b being (Element of L) holds (X is_>=_than b implies b <= a))
and
A3: (for c being (Element of L) holds ((X is_>=_than c & (for b being (Element of L) holds (X is_>=_than b implies b <= c))) implies c = a));

assume A4: (not thesis);
then a1 = a by A3;
hence contradiction by A3,A4;
end;
existence
proof
( ex_inf_of X,L implies (ex a being (Element of L) st ((X is_>=_than a & (for b being (Element of L) holds (X is_>=_than b implies b <= a))) & (for c being (Element of L) holds ((X is_>=_than c & (for b being (Element of L) holds (X is_>=_than b implies b <= c))) implies c = a))))) by CKB31:def 1;
hence thesis;
end;
correctness;
end;
