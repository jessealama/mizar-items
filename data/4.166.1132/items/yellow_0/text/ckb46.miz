begin
theorem
(for L1,L2 being RelStr holds (( the RelStr of L1 ) = ( the RelStr of L2 ) implies (for X being set holds ( ex_sup_of X,L1 implies ( "\/" (X,L1) ) = ( "\/" (X,L2) )))))
proof
let L1 being RelStr;
let L2 being RelStr;
assume that
A1: ( the RelStr of L1 ) = ( the RelStr of L2 );
let X being set;
reconsider c = ( "\/" (X,L1) ) as (Element of L2) by A1;
assume A2:  ex_sup_of X,L1;
then X is_<=_than ( "\/" (X,L1) ) by CKB44:def 1;
then A3: X is_<=_than c by A1,CKB10:1;
A4:now
let a being (Element of L2);
reconsider b = a as (Element of L1) by A1;
assume X is_<=_than a;
then X is_<=_than b by A1,CKB10:1;
then b >= ( "\/" (X,L1) ) by A2,CKB44:def 1;
hence a >= c by A1,CKB9:1;
end;
 ex_sup_of X,L2 by A1,A2,CKB32:1;
hence thesis by A3,A4,CKB44:def 1;
end;
