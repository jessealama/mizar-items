begin
definition
let A being non empty RelStr;
redefine attr A is  reflexive
means
(for x being (Element of A) holds x <= x);
compatibility
proof
thus (A is  reflexive implies (for x being (Element of A) holds x <= x)) by ORDERS_2:1;
assume A1: (for x being (Element of A) holds x <= x);
let x being set;
assume x in (the carrier of A);
then reconsider x as (Element of A);
x <= x by A1;
hence thesis by ORDERS_2:def 5;
end;
end;
