theorem
Th64: (for L being non  empty  transitive RelStr holds (for S being non  empty  full (SubRelStr of L) holds (for X being (Subset of S) holds (( ex_inf_of X,L & ( "/\" (X,L) ) in (the carrier of S)) implies ( ex_inf_of X,S & ( "/\" (X,S) ) = ( "/\" (X,L) ))))))
proof
let L being non  empty  transitive RelStr;
let S being non  empty  full (SubRelStr of L);
let X being (Subset of S);
assume that
A1:  ex_inf_of X,L
and
A2: ( "/\" (X,L) ) in (the carrier of S);
reconsider a = ( "/\" (X,L) ) as (Element of S) by A2;
A3:now
( "/\" (X,L) ) is_<=_than X by A1,CKB45:def 1;
hence a is_<=_than X by CKB90:1;
let b being (Element of S);
reconsider b9 = b as (Element of L) by CKB87:1;
assume b is_<=_than X;
then b9 is_<=_than X by CKB91:1;
then b9 <= ( "/\" (X,L) ) by A1,CKB45:def 1;
hence b <= a by CKB89:1;
end;
consider a9 being (Element of L) such that A4: X is_>=_than a9 and A5: (for b being (Element of L) holds (X is_>=_than b implies b <= a9)) and (for c being (Element of L) holds ((X is_>=_than c & (for b being (Element of L) holds (X is_>=_than b implies b <= c))) implies c = a9)) by A1,CKB31:def 1;
A6: a9 = ( "/\" (X,L) ) by A1,A4,A5,CKB45:def 1;
thus  ex_inf_of X,S
proof
take a;
thus (a is_<=_than X & (for b being (Element of S) holds (b is_<=_than X implies b <= a))) by A3;
let c being (Element of S);
reconsider c9 = c as (Element of L) by CKB87:1;
assume X is_>=_than c;
then A7: X is_>=_than c9 by CKB91:1;
assume (for b being (Element of S) holds (X is_>=_than b implies b <= c));
then A8: a <= c by A3;
now
let b being (Element of L);
assume X is_>=_than b;
then A9: b <= a9 by A5;
a9 <= c9 by A6,A8,CKB88:1;
hence b <= c9 by A9,ORDERS_2:3;
end;
hence thesis by A1,A7,CKB45:def 1;
end;

hence thesis by A3,CKB45:def 1;
end;
