begin
theorem
Th46: (for L being non  empty RelStr holds (for X,Y being set holds ((for x being (Element of L) holds (x is_>=_than X iff x is_>=_than Y)) implies ( ex_sup_of X,L implies  ex_sup_of Y,L))))
proof
let L being non  empty RelStr;
let X being set;
let Y being set;
assume that
A1: (for x being (Element of L) holds (x is_>=_than X iff x is_>=_than Y));
given a being (Element of L) such that
A2: X is_<=_than a
and
A3: (for b being (Element of L) holds (X is_<=_than b implies a <= b))
and
A4: (for c being (Element of L) holds ((X is_<=_than c & (for b being (Element of L) holds (X is_<=_than b implies c <= b))) implies c = a));

take a;
thus Y is_<=_than a by A1,A2;
hereby
let b being (Element of L);
assume Y is_<=_than b;
then X is_<=_than b by A1;
hence a <= b by A3;
end;
let c being (Element of L);
assume A5: Y is_<=_than c;
assume A6: (for b being (Element of L) holds (Y is_<=_than b implies c <= b));
A7:now
let b being (Element of L);
assume X is_<=_than b;
then Y is_<=_than b by A1;
hence c <= b by A6;
end;
X is_<=_than c by A1,A5;
hence thesis by A4,A7;
end;
