theorem
(for L being  antisymmetric  transitive RelStr holds (for X,Y being set holds ((( ex_inf_of X,L &  ex_inf_of Y,L) &  ex_inf_of ( X \/ Y ),L) implies ( "/\" (( X \/ Y ),L) ) = ( ( "/\" (X,L) ) "/\" ( "/\" (Y,L) ) ))))
proof
let L being  antisymmetric  transitive RelStr;
let X being set;
let Y being set;
assume that
A1: ( ex_inf_of X,L &  ex_inf_of Y,L)
and
A2:  ex_inf_of ( X \/ Y ),L;
set a = ( "/\" (X,L) );
set b = ( "/\" (Y,L) );
set c = ( "/\" (( X \/ Y ),L) );
A3: (a is_<=_than X & b is_<=_than Y) by A1,CKB51:1;
A4:now
let d being (Element of L);
assume (d <= a & d <= b);
then (d is_<=_than X & d is_<=_than Y) by A3,CKB12:1;
then d is_<=_than ( X \/ Y ) by CKB18:1;
hence c >= d by A2,CKB51:1;
end;
(c <= a & c <= b) by A1,A2,CKB55:1,XBOOLE_1:7;
hence thesis by A4,CKB37:1;
end;
