begin
reserve x for set;
reserve C for non  empty set;
reserve f for (Membership_Func of C);
reserve h for (Membership_Func of C);
reserve g for (Membership_Func of C);
reserve h1 for (Membership_Func of C);
theorem
(f = ( min (g,h) ) iff ((f c= g & f c= h) & (for h1 holds ((h1 c= g & h1 c= h) implies h1 c= f))))
proof
hereby
assume A1: f = ( min (g,h) );
hence (f c= g & f c= h) by CKB30:1;
let h1;
assume A2: (h1 c= g & h1 c= h);
thus h1 c= f
proof
let x being (Element of C);
(( h1 . x ) <= ( g . x ) & ( h1 . x ) <= ( h . x )) by A2,CKB8:def 1;
then ( min (( g . x ),( h . x )) ) >= ( h1 . x ) by XXREAL_0:20;
hence thesis by A1,CKB13:def 1;
end;

end;
assume that
A3: (f c= g & f c= h)
and
A4: (for h1 holds ((h1 c= g & h1 c= h) implies h1 c= f));
(( min (g,h) ) c= g & ( min (g,h) ) c= h) by CKB30:1;
then A5: ( min (g,h) ) c= f by A4;
f c= ( min (g,h) ) by A3,CKB37:1;
hence thesis by A5,CKB9:1;
end;
