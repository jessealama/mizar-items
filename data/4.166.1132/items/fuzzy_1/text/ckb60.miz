theorem
( max (( f \+\ g ),( min (f,g) )) ) c= ( max (f,g) )
proof
set f1 = ( 1_minus f );
set g1 = ( 1_minus g );
let x being (Element of C);
( max (( f \+\ g ),( min (f,g) )) ) = ( max (( min (f,g1) ),( max (( min (f1,g) ),( min (f,g) )) )) ) by CKB18:1
.= ( max (( min (f,g1) ),( min (( max (( min (f1,g) ),f) ),( max (g,( min (f1,g) )) )) )) ) by CKB20:1
.= ( max (( min (f,g1) ),( min (( max (( min (f1,g) ),f) ),g) )) ) by CKB19:1
.= ( min (( max (( min (f,g1) ),( max (f,( min (f1,g) )) )) ),( max (( min (f,g1) ),g) )) ) by CKB20:1
.= ( min (( max (( max (f,( min (f,g1) )) ),( min (f1,g) )) ),( max (( min (f,g1) ),g) )) ) by CKB18:1
.= ( min (( max (f,( min (f1,g) )) ),( max (( min (f,g1) ),g) )) ) by CKB19:1
.= ( min (( min (( max (f,f1) ),( max (f,g) )) ),( max (g,( min (f,g1) )) )) ) by CKB20:1
.= ( min (( min (( max (f,f1) ),( max (f,g) )) ),( min (( max (g,f) ),( max (g,g1) )) )) ) by CKB20:1
.= ( min (( min (( min (( max (f,f1) ),( max (f,g) )) ),( max (g,f) )) ),( max (g,g1) )) ) by CKB18:1
.= ( min (( min (( max (f,f1) ),( min (( max (f,g) ),( max (f,g) )) )) ),( max (g,g1) )) ) by CKB18:1
.= ( min (( max (f,g) ),( min (( max (f,f1) ),( max (g,g1) )) )) ) by CKB18:1;
then ( ( max (( f \+\ g ),( min (f,g) )) ) . x ) = ( min (( ( max (f,g) ) . x ),( ( min (( max (f,f1) ),( max (g,g1) )) ) . x )) ) by CKB13:def 1;
hence thesis by XXREAL_0:17;
end;
