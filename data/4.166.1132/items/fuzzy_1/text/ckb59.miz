begin
reserve x for set;
reserve C for non  empty set;
reserve f for (Membership_Func of C);
reserve g for (Membership_Func of C);
theorem
( max (( min (f,g) ),( min (( 1_minus f ),( 1_minus g )) )) ) c= ( 1_minus ( f \+\ g ) )
proof
set f1 = ( 1_minus f );
set g1 = ( 1_minus g );
let x being (Element of C);
( 1_minus ( f \+\ g ) ) = ( min (( 1_minus ( min (f,g1) ) ),( 1_minus ( min (f1,g) ) )) ) by CKB22:1
.= ( min (( max (f1,( 1_minus g1 )) ),( 1_minus ( min (f1,g) ) )) ) by CKB22:1
.= ( min (( max (f1,g) ),( max (( 1_minus f1 ),g1) )) ) by CKB22:1
.= ( max (( min (( max (f1,g) ),f) ),( min (( max (f1,g) ),g1) )) ) by CKB20:1
.= ( max (( max (( min (f,f1) ),( min (f,g) )) ),( min (( max (f1,g) ),g1) )) ) by CKB20:1
.= ( max (( max (( min (f,f1) ),( min (f,g) )) ),( max (( min (g1,f1) ),( min (g1,g) )) )) ) by CKB20:1
.= ( max (( max (( max (( min (f,g) ),( min (f,f1) )) ),( min (g1,f1) )) ),( min (g1,g) )) ) by CKB18:1
.= ( max (( max (( max (( min (g1,f1) ),( min (f,g) )) ),( min (f,f1) )) ),( min (g1,g) )) ) by CKB18:1
.= ( max (( max (( min (g1,f1) ),( min (f,g) )) ),( max (( min (f,f1) ),( min (g1,g) )) )) ) by CKB18:1;
then ( ( 1_minus ( f \+\ g ) ) . x ) = ( max (( ( max (( min (f,g) ),( min (f1,g1) )) ) . x ),( ( max (( min (f,f1) ),( min (g1,g) )) ) . x )) ) by CKB14:def 1;
hence thesis by XXREAL_0:25;
end;
