begin
reserve x for set;
reserve C for non  empty set;
reserve f for (Membership_Func of C);
theorem
Th14: (for a,b being (Element of ( REAL )) holds (for f being (PartFunc of C,( REAL )) holds ((( rng f ) c= [. a,b .] & a <= b) implies (for x being (Element of C) holds (x in ( dom f ) implies (a <= ( f . x ) & ( f . x ) <= b))))))
proof
let a being (Element of ( REAL ));
let b being (Element of ( REAL ));
let f being (PartFunc of C,( REAL ));
assume that
A1: ( rng f ) c= [. a,b .]
and
A2: a <= b;
(for x being (Element of C) holds (x in ( dom f ) implies (a <= ( f . x ) & ( f . x ) <= b)))
proof
reconsider A = [. a,b .] as non  empty  closed_interval (Subset of ( REAL )) by A2,MEASURE5:14;
let x being (Element of C);
A = [. ( lower_bound A ),( upper_bound A ) .] by INTEGRA1:4;
then A3: (a = ( lower_bound A ) & b = ( upper_bound A )) by INTEGRA1:5;
assume x in ( dom f );
then ( f . x ) in ( rng f ) by FUNCT_1:def 3;
hence thesis by A1,A3,INTEGRA2:1;
end;
hence thesis;
end;
