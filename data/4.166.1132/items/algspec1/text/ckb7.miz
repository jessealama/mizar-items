registrations XBOOLE_0,RELAT_1,FUNCT_1,FUNCT_2,PBOOLE,STRUCT_0,MSUALG_1,MSUALG_2,CIRCCOMB,MSUALG_6,INSTALG1,CATALG_1,RELSET_1;
constructors MSAFREE1,CIRCCOMB,PUA2MSS1,MSUALG_6,CATALG_1,RELSET_1;
requirements SUBSET,BOOLE;
begin
definition
let X being set;
let f being Function;
func X -indexing f -> (ManySortedSet of X)
equals
( ( id X ) +* ( f | X ) );
coherence
proof
( dom ( id X ) ) = X by RELAT_1:45;
then ( dom ( ( id X ) +* ( f | X ) ) ) = ( X \/ ( dom ( f | X ) ) ) by FUNCT_4:def 1;
then ( dom ( ( id X ) +* ( f | X ) ) ) = X by RELAT_1:58,XBOOLE_1:12;
hence thesis by PARTFUN1:def 2,RELAT_1:def 18;
end;
end;
