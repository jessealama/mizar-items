theorem
(for f1,f2 being Function holds (f1 c= f2 implies (for g1 being (rng-retract of f1) holds (ex g2 being (rng-retract of f2) st g1 c= g2))))
proof
let f1 being Function;
let f2 being Function;
assume that
A1: f1 c= f2;
A2: ( f2 +* f1 ) = f2 by A1,FUNCT_4:98;
set g = the (rng-retract of f2);
let g1 being (rng-retract of f1);
f1 tolerates f2 by A1,PARTFUN1:52;
then reconsider g2 = ( g +* g1 ) as (rng-retract of f2) by A2,CKB29:1;
take g2;
thus thesis by FUNCT_4:25;
end;
