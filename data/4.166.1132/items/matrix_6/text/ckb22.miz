environ
vocabularies NAT_1,VECTSP_1,SUBSET_1,MATRIX_1,REWRITE1,RELAT_1,MESFUNC1,ALGSTR_0,ARYTM_1,FINSEQ_1,TREES_1,ARYTM_3,SUPINF_2,XXREAL_0,CARD_1,QC_LANG1,ZFMISC_1,RELAT_2,FUNCOP_1,MATRIX_2,GROUP_1,MATRIX_6;
notations TARSKI,ZFMISC_1,NUMBERS,FINSEQ_1,RLVECT_1,GROUP_1,VECTSP_1,NAT_1,STRUCT_0,MATRIX_1,MATRIX_2,MATRIX_3,MATRIX_4,XXREAL_0,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB15;
definitions MATRIX_4,CKB1,CKB2,CKB3,CKB15;
theorems ZFMISC_1,RLVECT_1,MATRIX_1,MATRIX_2,MATRIX_4,MATRIX_3,VECTSP_1,NAT_1,CARD_2,XXREAL_0,CKB1,CKB2,CKB3,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21;
registrations RELSET_1,XREAL_0,STRUCT_0,VECTSP_1,ORDINAL1;
constructors XXREAL_0,FVSUM_1,MATRIX_3,MATRIX_4,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB15;
begin
reserve n for Nat;
reserve K for Field;
reserve M1 for (Matrix of n,K);
reserve M2 for (Matrix of n,K);
reserve M3 for (Matrix of n,K);
reserve M4 for (Matrix of n,K);
theorem
(for K being Field holds (for n being Nat holds (for M1,M2,M3,M4 being (Matrix of n,K) holds ((M3 is_reverse_of M1 & M4 is_reverse_of M2) implies ( M3 * M4 ) is_reverse_of ( M2 * M1 )))))
proof
let K;
let n;
let M1;
let M2;
let M3;
let M4;
A1: ( width M1 ) = n by MATRIX_1:24;
A2: (( width M2 ) = n & ( len M1 ) = n) by MATRIX_1:24;
A3: ( len M3 ) = n by MATRIX_1:24;
A4: (( width M3 ) = n & ( len M4 ) = n) by MATRIX_1:24;
assume that
A5: M3 is_reverse_of M1
and
A6: M4 is_reverse_of M2;
( width ( M2 * M1 ) ) = n by MATRIX_1:24;
then A7: ( ( M2 * M1 ) * ( M3 * M4 ) ) = ( ( ( M2 * M1 ) * M3 ) * M4 ) by A3,A4,MATRIX_3:33
.= ( ( M2 * ( M1 * M3 ) ) * M4 ) by A1,A2,A3,MATRIX_3:33
.= ( ( M2 * ( 1. (K,n) ) ) * M4 ) by A5,CKB2:def 1
.= ( M2 * M4 ) by MATRIX_3:19
.= ( 1. (K,n) ) by A6,CKB2:def 1;
A8: ( width M4 ) = n by MATRIX_1:24;
A9: ( len M2 ) = n by MATRIX_1:24;
( width ( M3 * M4 ) ) = n by MATRIX_1:24;
then ( ( M3 * M4 ) * ( M2 * M1 ) ) = ( ( ( M3 * M4 ) * M2 ) * M1 ) by A2,A9,MATRIX_3:33
.= ( ( M3 * ( M4 * M2 ) ) * M1 ) by A9,A8,A4,MATRIX_3:33
.= ( ( M3 * ( 1. (K,n) ) ) * M1 ) by A6,CKB2:def 1
.= ( M3 * M1 ) by MATRIX_3:19
.= ( 1. (K,n) ) by A5,CKB2:def 1;
hence thesis by A7,CKB2:def 1;
end;
