begin
reserve n for Nat;
reserve K for Field;
reserve M1 for (Matrix of n,K);
theorem
(for K being Field holds (for n being Nat holds (for M1 being (Matrix of n,K) holds (M1 is  symmetric implies ( - M1 ) is  symmetric))))
proof
let K;
let n;
let M1;
assume A1: M1 is  symmetric;
( ( - M1 ) @ ) = ( - ( M1 @ ) ) by CKB36:1
.= ( - M1 ) by A1,CKB28:def 1;
hence thesis by CKB28:def 1;
end;
