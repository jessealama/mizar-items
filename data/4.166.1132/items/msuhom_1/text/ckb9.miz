begin
reserve A for non  empty set;
theorem
Th8: (for I being set holds (for I0 being (Subset of I) holds (for A,B being (ManySortedSet of I) holds (for F being (ManySortedFunction of A,B) holds (for A0,B0 being (ManySortedSet of I0) holds ((A0 = ( A | I0 ) & B0 = ( B | I0 )) implies ( F | I0 ) is (ManySortedFunction of A0,B0)))))))
proof
let I being set;
let I0 being (Subset of I);
let A being (ManySortedSet of I);
let B being (ManySortedSet of I);
let F being (ManySortedFunction of A,B);
let A0 being (ManySortedSet of I0);
let B0 being (ManySortedSet of I0);
assume that
A1: A0 = ( A | I0 )
and
A2: B0 = ( B | I0 );
reconsider G = ( F | I0 ) as (ManySortedFunction of I0);
A3: (( dom A0 ) = I0 & ( dom ( F | I0 ) ) = I0) by PARTFUN1:def 2;
A4: ( dom B0 ) = I0 by PARTFUN1:def 2;
now
let i being set;
assume A5: i in I0;
then A6: ( B . i ) = ( B0 . i ) by A2,A4,FUNCT_1:47;
(( G . i ) = ( F . i ) & ( A . i ) = ( A0 . i )) by A1,A3,A5,FUNCT_1:47;
hence ( G . i ) is (Function of ( A0 . i ),( B0 . i )) by A5,A6,PBOOLE:def 15;
end;
hence thesis by PBOOLE:def 15;
end;
