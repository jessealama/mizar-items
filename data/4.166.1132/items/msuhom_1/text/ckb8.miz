begin
reserve n for Nat;
reserve A for non empty set;
theorem
(for g being Function holds (for a being (Element of A) holds (for k being Nat holds ((1 <= k & k <= n) implies ( ( a .--> g ) . ( ( n |-> a ) /. k ) ) = g))))
proof
let g being Function;
let a being (Element of A);
let k being Nat;
assume (1 <= k & k <= n);
then A1: k in ( Seg n ) by FINSEQ_1:1;
then k in ( dom ( n |-> a ) ) by FUNCOP_1:13;
then ( ( n |-> a ) /. k ) = ( ( n |-> a ) . k ) by PARTFUN1:def 6
.= a by A1,FUNCOP_1:7;
hence thesis by FUNCOP_1:72;
end;
