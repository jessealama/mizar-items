theorem
(for Omega1,Omega2 being non empty  finite set holds (for P1 being (Probability of ( Trivial-SigmaField Omega1 )) holds (for P2 being (Probability of ( Trivial-SigmaField Omega2 )) holds (for y1,y2 being set holds ((y1 in Omega1 & y2 in Omega2) implies ( ( Product-Probability (Omega1,Omega2,P1,P2) ) . { [ y1,y2 ] } ) = ( ( P1 . { y1 } ) * ( P2 . { y2 } ) ))))))
proof
let Omega1 being non empty  finite set,Omega2 being non empty  finite set;
let P1 being (Probability of ( Trivial-SigmaField Omega1 ));
let P2 being (Probability of ( Trivial-SigmaField Omega2 ));
let y1 being set,y2 being set;
assume A1: (y1 in Omega1 & y2 in Omega2);
then A2: { y1 } is  finite  finite (Subset of Omega1) by ZFMISC_1:31;
(for yy being set holds (yy in { y2 } implies yy in Omega2)) by A1,TARSKI:def 1;
then A3: { y2 } is  finite  finite (Subset of Omega2) by TARSKI:def 3;
[: { y1 },{ y2 } :] = { [ y1,y2 ] } by ZFMISC_1:29;
hence ( ( Product-Probability (Omega1,Omega2,P1,P2) ) . { [ y1,y2 ] } ) = ( ( P1 . { y1 } ) * ( P2 . { y2 } ) ) by CKB29:1,A2,A3;
end;
