begin
theorem
Lm2: (for Omega1,Omega2 being non  empty  finite set holds (for P1 being (Probability of ( Trivial-SigmaField Omega1 )) holds (for P2 being (Probability of ( Trivial-SigmaField Omega2 )) holds (for Q1,Q2 being (Function of [: Omega1,Omega2 :],( REAL )) holds (((for x,y being set holds ((x in Omega1 & y in Omega2) implies ( Q1 . (x,y) ) = ( ( P1 . { x } ) * ( P2 . { y } ) ))) & (for x,y being set holds ((x in Omega1 & y in Omega2) implies ( Q2 . (x,y) ) = ( ( P1 . { x } ) * ( P2 . { y } ) )))) implies Q1 = Q2)))))
proof
let Omega1 being non  empty  finite set;
let Omega2 being non  empty  finite set;
let P1 being (Probability of ( Trivial-SigmaField Omega1 ));
let P2 being (Probability of ( Trivial-SigmaField Omega2 ));
let Q1 being (Function of [: Omega1,Omega2 :],( REAL ));
let Q2 being (Function of [: Omega1,Omega2 :],( REAL ));
assume A1: ((for x,y being set holds ((x in Omega1 & y in Omega2) implies ( Q1 . (x,y) ) = ( ( P1 . { x } ) * ( P2 . { y } ) ))) & (for x,y being set holds ((x in Omega1 & y in Omega2) implies ( Q2 . (x,y) ) = ( ( P1 . { x } ) * ( P2 . { y } ) ))));
A2: (( dom Q1 ) = [: Omega1,Omega2 :] & ( dom Q2 ) = [: Omega1,Omega2 :]) by FUNCT_2:def 1;
now
let z being set;
assume z in ( dom Q1 );
then consider x,y being set such that A3: ((x in Omega1 & y in Omega2) & z = [ x,y ]) by ZFMISC_1:def 2;
thus ( Q1 . z ) = ( Q1 . (x,y) ) by A3
.= ( ( P1 . { x } ) * ( P2 . { y } ) ) by A3,A1
.= ( Q2 . (x,y) ) by A3,A1
.= ( Q2 . z ) by A3;
end;
hence thesis by A2,FUNCT_1:2;
end;
