begin
reserve Omega for non  empty set;
reserve Sigma for (SigmaField of Omega);
reserve P for (Probability of Sigma);
definition
let Omega1 being non  empty  finite set;
let Omega2 being non  empty  finite set;
let P1 being (Probability of ( Trivial-SigmaField Omega1 ));
let P2 being (Probability of ( Trivial-SigmaField Omega2 ));
func Product-Probability (Omega1,Omega2,P1,P2) -> (Probability of ( Trivial-SigmaField [: Omega1,Omega2 :] )) means 
:Def2: (ex Q being (Function of [: Omega1,Omega2 :],( REAL )) st ((for x,y being set holds ((x in Omega1 & y in Omega2) implies ( Q . (x,y) ) = ( ( P1 . { x } ) * ( P2 . { y } ) ))) & (for z being  finite (Subset of [: Omega1,Omega2 :]) holds ( it . z ) = ( setopfunc (z,[: Omega1,Omega2 :],( REAL ),Q,( addreal )) ))));
existence
proof
consider Q being (Function of [: Omega1,Omega2 :],( REAL )) such that A1: (for x,y being set holds ((x in Omega1 & y in Omega2) implies ( Q . (x,y) ) = ( ( P1 . { x } ) * ( P2 . { y } ) ))) by CKB12:1;
consider P being (Function of ( bool [: Omega1,Omega2 :] ),( REAL )) such that A2: (for z being  finite (Subset of [: Omega1,Omega2 :]) holds ( P . z ) = ( setopfunc (z,[: Omega1,Omega2 :],( REAL ),Q,( addreal )) )) by CKB14:1;
A3: (for x being set holds (x c= [: Omega1,Omega2 :] implies (( 0 ) <= ( P . x ) & ( P . x ) <= 1))) by CKB27:1,A1,A2;
( P . [: Omega1,Omega2 :] ) = 1 by A1,A2,CKB26:1;
then P is (Probability of ( Trivial-SigmaField [: Omega1,Omega2 :] )) by A2,A3,CKB11:1;
hence thesis by A1,A2;
end;
uniqueness
proof
let PP1 being (Probability of ( Trivial-SigmaField [: Omega1,Omega2 :] ));
let PP2 being (Probability of ( Trivial-SigmaField [: Omega1,Omega2 :] ));
assume A4: (ex Q1 being (Function of [: Omega1,Omega2 :],( REAL )) st ((for x,y being set holds ((x in Omega1 & y in Omega2) implies ( Q1 . (x,y) ) = ( ( P1 . { x } ) * ( P2 . { y } ) ))) & (for z being  finite (Subset of [: Omega1,Omega2 :]) holds ( PP1 . z ) = ( setopfunc (z,[: Omega1,Omega2 :],( REAL ),Q1,( addreal )) ))));
assume A5: (ex Q2 being (Function of [: Omega1,Omega2 :],( REAL )) st ((for x,y being set holds ((x in Omega1 & y in Omega2) implies ( Q2 . (x,y) ) = ( ( P1 . { x } ) * ( P2 . { y } ) ))) & (for z being  finite (Subset of [: Omega1,Omega2 :]) holds ( PP2 . z ) = ( setopfunc (z,[: Omega1,Omega2 :],( REAL ),Q2,( addreal )) ))));
consider Q1 being (Function of [: Omega1,Omega2 :],( REAL )) such that A6: ((for x,y being set holds ((x in Omega1 & y in Omega2) implies ( Q1 . (x,y) ) = ( ( P1 . { x } ) * ( P2 . { y } ) ))) & (for z being  finite (Subset of [: Omega1,Omega2 :]) holds ( PP1 . z ) = ( setopfunc (z,[: Omega1,Omega2 :],( REAL ),Q1,( addreal )) ))) by A4;
consider Q2 being (Function of [: Omega1,Omega2 :],( REAL )) such that A7: ((for x,y being set holds ((x in Omega1 & y in Omega2) implies ( Q2 . (x,y) ) = ( ( P1 . { x } ) * ( P2 . { y } ) ))) & (for z being  finite (Subset of [: Omega1,Omega2 :]) holds ( PP2 . z ) = ( setopfunc (z,[: Omega1,Omega2 :],( REAL ),Q2,( addreal )) ))) by A5;
Q1 = Q2 by A6,A7,CKB13:1;
hence PP1 = PP2 by CKB15:1,A6,A7;
end;
