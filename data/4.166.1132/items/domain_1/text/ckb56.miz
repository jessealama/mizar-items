begin
reserve D for non  empty set;
scheme SubsetFD2 { A,B,D() -> non  empty set,F(set,set) -> (Element of D()),P[set,set] } : { F(x,y) where x is (Element of A()),y is (Element of B()): P[ x,y ] } is (Subset of D())
proof
set X = { F(x,y) where x is (Element of A()),y is (Element of B()): P[ x,y ] };
X c= D()
proof
let w being set;
assume w in X;
then (ex x being (Element of A()) st (ex y being (Element of B()) st (w = F(x,y) & P[ x,y ])));
hence thesis;
end;
hence thesis;
end;
