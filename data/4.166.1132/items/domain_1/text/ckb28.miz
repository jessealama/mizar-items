begin
reserve X1 for non  empty set;
reserve x1 for (Element of X1);
reserve y1 for (Element of X1);
reserve z1 for (Element of X1);
scheme Fraenkel6 { P[set],Q[set] } : (for X1 holds ((for x1 holds (P[ x1 ] iff Q[ x1 ])) implies { y1: P[ y1 ] } = { z1: Q[ z1 ] }))
proof
let X1;
assume A1: (P[ x1 ] iff Q[ x1 ]);
(for X1 holds ((for x1 holds (P[ x1 ] implies Q[ x1 ])) implies { y1: P[ y1 ] } c= { z1: Q[ z1 ] })) from CKB27:sch 1;
hence { y1: P[ y1 ] } c= { z1: Q[ z1 ] } by A1;
(for X1 holds ((for x1 holds (Q[ x1 ] implies P[ x1 ])) implies { y1: Q[ y1 ] } c= { z1: P[ z1 ] })) from CKB27:sch 1;
hence thesis by A1;
end;
