begin
reserve X1 for non  empty set;
reserve X2 for non  empty set;
reserve X3 for non  empty set;
reserve X4 for non  empty set;
reserve x1 for (Element of X1);
reserve x2 for (Element of X2);
reserve x3 for (Element of X3);
reserve x for (Element of [: X1,X2,X3,X4 :]);
theorem
[: X1,X2,X3 :] = { [ x1,x2,x3 ]: (not contradiction) }
proof
defpred P[ set,set,set ]
 means
(not contradiction);
A1: (for x being (Element of [: X1,X2,X3 :]) holds x in { [ x1,x2,x3 ]: (not contradiction) })
proof
let x being (Element of [: X1,X2,X3 :]);
x = [ ( x `1 ),( x `2 ),( x `3 ) ] by MCART_1:44;
hence thesis;
end;
(for X1 holds (for X2 holds (for X3 holds { [ x1,x2,x3 ]: P[ x1,x2,x3 ] } is (Subset of [: X1,X2,X3 :])))) from CKB25:sch 1;
then { [ x1,x2,x3 ]: (not contradiction) } is (Subset of [: X1,X2,X3 :]);
hence thesis by A1,SUBSET_1:28;
end;
