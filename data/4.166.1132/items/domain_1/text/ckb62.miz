begin
registration
let A being non empty set;
cluster  c=-linear non empty for (Subset of A);
existence
proof
set a = the (Element of A);
reconsider B = { a } as non empty (Subset of A);
take B;
B is  c=-linear
proof
let x being set,y being set;
assume that
A1: x in B
and
A2: y in B;
x = a by A1,TARSKI:def 1;
hence thesis by A2,TARSKI:def 1;
end;
hence thesis;
end;
