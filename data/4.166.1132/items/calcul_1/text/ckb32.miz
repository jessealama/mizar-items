begin
reserve p for (Element of ( CQC-WFF ));
reserve A for non  empty set;
reserve J for (interpretation of A);
reserve v for (Element of ( Valuations_in A ));
reserve f for (FinSequence of ( CQC-WFF ));
theorem
Th17: (( len f ) > ( 0 ) implies ((J,v |= ( Ant f ) & J,v |= ( Suc f )) iff J,v |= f))
proof
assume A1: ( len f ) > ( 0 );
thus ((J,v |= ( Ant f ) & J,v |= ( Suc f )) implies J,v |= f)
proof
assume that
A2: J,v |= ( Ant f )
and
A3: J,v |= ( Suc f );
let p;
assume p in ( rng f );
then p in ( ( rng ( Ant f ) ) \/ { ( Suc f ) } ) by A1,CKB7:1;
then A4: (p in ( rng ( Ant f ) ) or p in { ( Suc f ) }) by XBOOLE_0:def 3;
J,v |= ( rng ( Ant f ) ) by A2,CKB28:def 1;
hence thesis by A3,A4,CKB25:def 1,TARSKI:def 1;
end;

thus (J,v |= f implies (J,v |= ( Ant f ) & J,v |= ( Suc f )))
proof
assume A5: J,v |= ( rng f );
thus J,v |= ( rng ( Ant f ) )
proof
A6: ( rng ( Ant f ) ) c= ( ( rng ( Ant f ) ) \/ { ( Suc f ) } ) by XBOOLE_1:7;
let p;
assume p in ( rng ( Ant f ) );
then p in ( ( rng ( Ant f ) ) \/ { ( Suc f ) } ) by A6;
then p in ( rng f ) by A1,CKB7:1;
hence thesis by A5,CKB25:def 1;
end;

( ( 0 ) + 1 ) <= ( len f ) by A1,NAT_1:13;
then A7: ( len f ) in ( dom f ) by FINSEQ_3:25;
( Suc f ) = ( f . ( len f ) ) by A1,CKB2:def 1;
then ( Suc f ) in ( rng f ) by A7,FUNCT_1:3;
hence J,v |= ( Suc f ) by A5,CKB25:def 1;
end;

end;
