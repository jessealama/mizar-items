begin
theorem
Lm7: (for x being  real number holds (for x1,x2 being (Element of ( REAL )) holds (x = [* x1,x2 *] implies (x2 = ( 0 ) & x = x1))))
proof
let x being  real number;
let x1 being (Element of ( REAL )),x2 being (Element of ( REAL ));
assume A1: x = [* x1,x2 *];
A2: x in ( REAL ) by XREAL_0:def 1;
hereby
assume x2 <> ( 0 );
then x = ( (( 0 ),1) --> (x1,x2) ) by A1,ARYTM_0:def 5;
hence contradiction by A2,ARYTM_0:8;
end;
hence thesis by A1,ARYTM_0:def 5;
end;
