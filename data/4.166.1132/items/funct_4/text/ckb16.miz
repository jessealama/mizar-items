environ
vocabularies XBOOLE_0,FUNCT_1,TARSKI,ZFMISC_1,RELAT_1,FUNCOP_1,SUBSET_1,PARTFUN1,FUNCT_2,ORDINAL1,FUNCT_4;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,ORDINAL1,RELAT_1,FUNCT_1,RELSET_1,FUNCT_2,BINOP_1,PARTFUN1,FUNCOP_1,CKB11;
definitions TARSKI,FUNCT_2,PARTFUN1,XBOOLE_0,BINOP_1,FUNCOP_1,FUNCT_1,CKB11;
theorems TARSKI,FUNCT_1,FUNCT_2,GRFUNC_1,PARTFUN1,FUNCOP_1,ZFMISC_1,RELAT_1,RELSET_1,XBOOLE_0,XBOOLE_1,ENUMSET1,ORDINAL1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15;
schemes FUNCT_1,PARTFUN1,XBOOLE_0;
registrations XBOOLE_0,RELAT_1,FUNCT_1,FUNCOP_1,RELSET_1,ZFMISC_1;
constructors PARTFUN1,BINOP_1,FUNCOP_1,RELSET_1,ORDINAL1,CKB11;
requirements BOOLE,SUBSET;
begin
reserve x for set;
reserve f for Function;
reserve g for Function;
reserve h for Function;
theorem
Th15: ( ( f +* g ) +* h ) = ( f +* ( g +* h ) )
proof
A1:now
let x;
assume that
x in ( ( dom f ) \/ ( dom ( g +* h ) ) );
hereby
assume A2: x in ( dom ( g +* h ) );
per cases  by A2,CKB14:1;
suppose A3: (x in ( dom g ) & (not x in ( dom h )));

hence ( ( ( f +* g ) +* h ) . x ) = ( ( f +* g ) . x ) by CKB13:1
.= ( g . x ) by A3,CKB15:1
.= ( ( g +* h ) . x ) by A3,CKB13:1;
end;
suppose A4: x in ( dom h );

hence ( ( ( f +* g ) +* h ) . x ) = ( h . x ) by CKB15:1
.= ( ( g +* h ) . x ) by A4,CKB15:1;
end;
end;
assume A5: (not x in ( dom ( g +* h ) ));
then A6: (not x in ( dom g )) by CKB14:1;
(not x in ( dom h )) by A5,CKB14:1;
hence ( ( ( f +* g ) +* h ) . x ) = ( ( f +* g ) . x ) by CKB13:1
.= ( f . x ) by A6,CKB13:1;
end;
( dom ( ( f +* g ) +* h ) ) = ( ( dom ( f +* g ) ) \/ ( dom h ) ) by CKB11:def 1
.= ( ( ( dom f ) \/ ( dom g ) ) \/ ( dom h ) ) by CKB11:def 1
.= ( ( dom f ) \/ ( ( dom g ) \/ ( dom h ) ) ) by XBOOLE_1:4
.= ( ( dom f ) \/ ( dom ( g +* h ) ) ) by CKB11:def 1;
hence thesis by A1,CKB11:def 1;
end;
