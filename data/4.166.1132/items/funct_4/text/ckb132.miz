theorem
(for R being Relation holds ((( dom R ) = { x } & ( rng R ) = { y }) implies R = ( x .--> y )))
proof
let R being Relation;
assume that
A1: ( dom R ) = { x }
and
A2: ( rng R ) = { y };
set g = ( x .--> y );
A3: g = { [ x,y ] } by ZFMISC_1:29;
(for a,b being set holds ([ a,b ] in R iff [ a,b ] in g))
proof
let a being set;
let b being set;
hereby
assume A4: [ a,b ] in R;
then b in ( rng R ) by RELAT_1:6;
then A5: b = y by A2,TARSKI:def 1;
a in ( dom R ) by A4,RELAT_1:6;
then a = x by A1,TARSKI:def 1;
hence [ a,b ] in g by A3,A5,TARSKI:def 1;
end;
assume [ a,b ] in g;
then A6: [ a,b ] = [ x,y ] by A3,TARSKI:def 1;
then A7: b = y by ZFMISC_1:27;
a = x by A6,ZFMISC_1:27;
then a in ( dom R ) by A1,TARSKI:def 1;
then consider z being set such that A8: [ a,z ] in R by RELAT_1:def 4;
z in ( rng R ) by A8,RELAT_1:6;
hence thesis by A2,A7,A8,TARSKI:def 1;
end;
hence thesis by RELAT_1:def 2;
end;
