theorem
(for f being Function holds (for a,b,c being set holds (a <> c implies ( ( f +* ( a .--> b ) ) . c ) = ( f . c ))))
proof
let f being Function;
let a being set,b being set,c being set;
assume that
A1: a <> c;
set g = ( a .--> b );
( dom g ) = { a } by FUNCOP_1:13;
then (not c in ( dom g )) by A1,TARSKI:def 1;
hence thesis by CKB13:1;
