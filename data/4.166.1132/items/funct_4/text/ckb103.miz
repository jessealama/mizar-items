begin
reserve x for set;
reserve D for non  empty set;
reserve f for Function;
reserve g for Function;
reserve h for Function;
reserve x for set;
theorem
(for f,g,h being Function holds (for D being set holds (( f | D ) = ( h | D ) implies ( ( h +* g ) | D ) = ( ( f +* g ) | D ))))
proof
let f being Function;
let g being Function;
let h being Function;
let D being set;
assume A1: ( f | D ) = ( h | D );
A2: ( dom ( ( f +* g ) | D ) ) = ( ( dom ( f +* g ) ) /\ D ) by RELAT_1:61
.= ( ( ( dom f ) \/ ( dom g ) ) /\ D ) by CKB11:def 1;
A3: ( dom ( ( h +* g ) | D ) ) = ( ( dom ( h +* g ) ) /\ D ) by RELAT_1:61
.= ( ( ( dom h ) \/ ( dom g ) ) /\ D ) by CKB11:def 1;
then A4: ( dom ( ( h +* g ) | D ) ) = ( ( ( dom h ) /\ D ) \/ ( ( dom g ) /\ D ) ) by XBOOLE_1:23
.= ( ( dom ( f | D ) ) \/ ( ( dom g ) /\ D ) ) by A1,RELAT_1:61
.= ( ( ( dom f ) /\ D ) \/ ( ( dom g ) /\ D ) ) by RELAT_1:61
.= ( ( ( dom f ) \/ ( dom g ) ) /\ D ) by XBOOLE_1:23;
now
let x being set;
assume A5: x in ( dom ( ( f +* g ) | D ) );
then A6: x in ( ( dom f ) \/ ( dom g ) ) by A2,XBOOLE_0:def 4;
A7: x in D by A2,A5,XBOOLE_0:def 4;
A8: (x in ( ( dom h ) \/ ( dom g ) ) & ( ( ( h +* g ) | D ) . x ) = ( ( h +* g ) . x )) by A2,A3,A4,A5,FUNCT_1:47,XBOOLE_0:def 4;
per cases ;
suppose A9: x in ( dom g );

( ( ( f +* g ) | D ) . x ) = ( ( f +* g ) . x ) by A5,FUNCT_1:47
.= ( g . x ) by A6,A9,CKB11:def 1;
hence ( ( ( h +* g ) | D ) . x ) = ( ( ( f +* g ) | D ) . x ) by A8,A9,CKB11:def 1;
end;
suppose A10: (not x in ( dom g ));

then A11: ( ( ( h +* g ) | D ) . x ) = ( h . x ) by A8,CKB11:def 1
.= ( ( h | D ) . x ) by A7,FUNCT_1:49;
thus ( ( ( f +* g ) | D ) . x ) = ( ( f +* g ) . x ) by A5,FUNCT_1:47
.= ( f . x ) by A6,A10,CKB11:def 1
.= ( ( ( h +* g ) | D ) . x ) by A1,A7,A11,FUNCT_1:49;
end;
end;
hence thesis by A2,A4,FUNCT_1:2;
end;
