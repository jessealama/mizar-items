requirements BOOLE,SUBSET;
begin
reserve x for set;
reserve f for Function;
reserve g for Function;
theorem
Th16: ((f tolerates g & x in ( dom f )) implies ( ( f +* g ) . x ) = ( f . x ))
proof
assume that
A1: f tolerates g
and
A2: x in ( dom f );
now
per cases ;
suppose x in ( dom g );

then (x in ( ( dom f ) /\ ( dom g ) ) & ( ( f +* g ) . x ) = ( g . x )) by A2,CKB15:1,XBOOLE_0:def 4;
hence thesis by A1,PARTFUN1:def 4;
end;
suppose (not x in ( dom g ));

hence thesis by CKB13:1;
end;
end;
hence thesis;
end;
