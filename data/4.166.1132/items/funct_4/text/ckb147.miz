theorem
Th5: (for f,g,h being Function holds (((f tolerates g & g tolerates h) & h tolerates f) implies ( f +* g ) tolerates h))
proof
let f being Function;
let g being Function;
let h being Function;
assume that
A1: f tolerates g
and
A2: g tolerates h
and
A3: h tolerates f;
let x being set;
assume A4: x in ( ( dom ( f +* g ) ) /\ ( dom h ) );
then x in ( dom ( f +* g ) ) by XBOOLE_0:def 4;
then A5: (x in ( dom f ) or x in ( dom g )) by CKB14:1;
x in ( dom h ) by A4,XBOOLE_0:def 4;
then ((x in ( ( dom h ) /\ ( dom f ) ) & ( ( f +* g ) . x ) = ( f . x )) or (x in ( ( dom g ) /\ ( dom h ) ) & ( ( f +* g ) . x ) = ( g . x ))) by A1,A5,CKB15:1,CKB17:1,XBOOLE_0:def 4;
hence thesis by A2,A3,PARTFUN1:def 4;
end;
