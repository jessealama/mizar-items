begin
theorem
(for G being _Graph holds (for W being (Walk of G) holds (for m,n being Nat holds (( W .cut (m,n) ) is non trivial implies W is non trivial))))
proof
let G being _Graph;
let W being (Walk of G);
let m being Nat,n being Nat;
assume that
A1: ( W .cut (m,n) ) is non trivial
and
A2: W is  trivial;
reconsider W as  trivial (Walk of G) by A2;
reconsider m9 = m,n9 = n as (Element of ( NAT )) by ORDINAL1:def 12;
( W .cut (m9,n9) ) is  trivial;
hence thesis by A1;
end;
