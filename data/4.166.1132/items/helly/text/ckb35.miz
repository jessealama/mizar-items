theorem
Th28: (for T being _Tree holds (for a,b being (Vertex of T) holds (( ( T .pathBetween (a,b) ) .first() ) = a & ( ( T .pathBetween (a,b) ) .last() ) = b)))
proof
let T being _Tree;
let a being (Vertex of T);
let b being (Vertex of T);
A1: ( T .pathBetween (a,b) ) is_Walk_from a,b by CKB34:def 1;
hence ( ( T .pathBetween (a,b) ) .first() ) = a by GLIB_001:def 23;
thus thesis by A1,GLIB_001:def 23;
end;
