begin
reserve T for non  empty TopSpace;
reserve F for (Subset-Family of T);
reserve A for (Subset of T);
reserve B for (Subset of T);
reserve C for (Subset of T);
reserve D for (Subset of T);
reserve O for  open (Subset of T);
reserve U for  open (Subset of T);
reserve p for (Point of T);
theorem
Th11: (for F holds (F is  discrete implies F is  locally_finite))
proof
let F;
assume A1: F is  discrete;
(for p holds (ex A st ((p in A & A is  open) & { D: (D in F & D meets A) } is  finite)))
proof
let p;
consider U such that A2: p in U and A3: (for A holds (for B holds ((A in F & B in F) implies ((U meets A & U meets B) implies A = B)))) by A1,CKB3:def 1;
set SF = { D: (D in F & D meets U) };
take O = U;
(SF <> ( {} ) implies (ex A st SF = { A }))
proof
assume SF <> ( {} );
then consider a being set such that A4: a in SF by XBOOLE_0:def 1;
consider D such that A5: a = D and A6: (D in F & D meets O) by A4;
now
let b being set;
assume b in SF;
then (ex C st ((b = C & C in F) & C meets O));
then b = D by A3,A6;
hence b in { D } by TARSKI:def 1;
end;
then A7: SF c= { D } by TARSKI:def 3;
{ D } c= SF by A4,A5,ZFMISC_1:31;
then SF = { D } by A7,XBOOLE_0:def 10;
hence thesis;
end;
hence thesis by A2;
end;
hence thesis by PCOMPS_1:def 1;
end;
