begin
theorem
Th5: (for G being  real-weighted WGraph holds (for EL being (FF:ELabeling of G) holds (for source being (Vertex of G) holds (for i,j being Nat holds (i <= j implies ( dom ( ( AP:CompSeq (EL,source) ) . i ) ) c= ( dom ( ( AP:CompSeq (EL,source) ) . j ) ))))))
proof
let G being  real-weighted WGraph;
let EL being (FF:ELabeling of G);
let source being (Vertex of G);
let i being Nat,j being Nat;
set CS = ( AP:CompSeq (EL,source) );
defpred P[ (Element of ( NAT )) ]
 means
( dom ( CS . i ) ) c= ( dom ( CS . ( i + $1 ) ) );
assume i <= j;
then consider k being Nat such that A1: j = ( i + k ) by NAT_1:10;
A2:now
let n being (Element of ( NAT ));
set Gn = ( CS . ( i + n ) );
set Gn1 = ( CS . ( i + ( n + 1 ) ) );
set Next = ( AP:NextBestEdges Gn );
set e = ( choose Next );
Gn1 = ( CS . ( ( i + n ) + 1 ) );
then A3: Gn1 = ( AP:Step Gn ) by CKB24:def 1;
assume A4: P[ n ];
now
per cases ;
suppose Next = ( {} );

hence P[ ( n + 1 ) ] by A4,A3,CKB21:def 1;
end;
suppose (Next <> ( {} ) & (not ( ( the_Source_of G ) . e ) in ( dom Gn )));

then Gn1 = ( Gn +* ( ( ( the_Source_of G ) . e ) .--> e ) ) by A3,CKB21:def 1;
then ( dom Gn ) c= ( dom Gn1 ) by FUNCT_4:10;
hence P[ ( n + 1 ) ] by A4,XBOOLE_1:1;
end;
suppose (Next <> ( {} ) & ( ( the_Source_of G ) . e ) in ( dom Gn ));

then Gn1 = ( Gn +* ( ( ( the_Target_of G ) . e ) .--> e ) ) by A3,CKB21:def 1;
then ( dom Gn ) c= ( dom Gn1 ) by FUNCT_4:10;
hence P[ ( n + 1 ) ] by A4,XBOOLE_1:1;
end;
end;
hence P[ ( n + 1 ) ];
end;
A5: P[ ( 0 ) ];
A6: (for n being (Element of ( NAT )) holds P[ n ]) from NAT_1:sch 1(A5,A2);
k in ( NAT ) by ORDINAL1:def 12;
hence thesis by A6,A1;
end;
