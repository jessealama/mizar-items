constructors DOMAIN_1,FUNCT_4,NAT_D,GRAPH_2,GRAPH_5,UPROOTS,GLIB_004,SEQ_1,XXREAL_2,RELSET_1,PBOOLE,CKB5,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16;
requirements ARITHM,BOOLE,NUMERALS,REAL,SUBSET;
begin
definition
let G being  real-weighted WGraph;
let EL being (FF:ELabeling of G);
let VL being (AP:VLabeling of EL);
func AP:NextBestEdges VL -> (Subset of ( the_Edges_of G )) means 
:Def9: (for e being set holds (e in it iff (e is_forward_edge_wrt VL or e is_backward_edge_wrt VL)));
existence
proof
defpred P[ set ]
 means
($1 is_forward_edge_wrt VL or $1 is_backward_edge_wrt VL);
consider IT being (Subset of ( the_Edges_of G )) such that A1: (for e being set holds (e in IT iff (e in ( the_Edges_of G ) & P[ e ]))) from SUBSET_1:sch 1;
take IT;
let e being set;
thus (e in IT implies P[ e ]) by A1;
assume A2: P[ e ];
then e in ( the_Edges_of G ) by CKB14:def 1,CKB15:def 1;
hence thesis by A1,A2;
end;
uniqueness
proof
let IT1 being (Subset of ( the_Edges_of G ));
let IT2 being (Subset of ( the_Edges_of G ));
assume that
A3: (for e being set holds (e in IT1 iff (e is_forward_edge_wrt VL or e is_backward_edge_wrt VL)))
and
A4: (for e being set holds (e in IT2 iff (e is_forward_edge_wrt VL or e is_backward_edge_wrt VL)));
now
let e being set;
hereby
assume e in IT1;
then (e is_forward_edge_wrt VL or e is_backward_edge_wrt VL) by A3;
hence e in IT2 by A4;
end;
assume e in IT2;
then (e is_forward_edge_wrt VL or e is_backward_edge_wrt VL) by A4;
hence e in IT1 by A3;
end;
hence thesis by TARSKI:1;
end;
end;
