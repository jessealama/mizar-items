begin
reserve a for Integer;
reserve b for Integer;
theorem
Th14: ((((a divides b iff a divides ( - b )) & (a divides b iff ( - a ) divides b)) & (a divides b iff ( - a ) divides ( - b ))) & (a divides ( - b ) iff ( - a ) divides b))
proof
A1: (a divides b implies a divides ( - b ))
proof
assume A2: a divides b;
b divides ( - b ) by CKB5:1;
hence thesis by A2,CKB6:1;
end;
A3: (a divides ( - b ) implies a divides b)
proof
assume A4: a divides ( - b );
( - b ) divides b by CKB5:1;
hence thesis by A4,CKB6:1;
end;
hence (a divides b iff a divides ( - b )) by A1;
A5: (( - a ) divides b implies a divides b)
proof
assume A6: ( - a ) divides b;
a divides ( - a ) by CKB5:1;
hence thesis by A6,CKB6:1;
end;
A7: (( - a ) divides ( - b ) implies a divides b)
proof
assume A8: ( - a ) divides ( - b );
( - b ) divides b by CKB5:1;
hence thesis by A5,A8,CKB6:1;
end;
A9: (a divides b implies ( - a ) divides b)
proof
assume A10: a divides b;
( - a ) divides a by CKB5:1;
hence thesis by A10,CKB6:1;
end;
hence (a divides b iff ( - a ) divides b) by A5;
(a divides b implies ( - a ) divides ( - b ))
proof
assume A11: a divides b;
( - a ) divides a by CKB5:1;
hence thesis by A1,A11,CKB6:1;
end;
hence (a divides b iff ( - a ) divides ( - b )) by A7;
thus thesis by A1,A3,A9,A5;
end;
