begin
reserve A for non empty set;
reserve a for (Element of A);
reserve b for (Element of A);
reserve c for (Element of A);
registration
let A;
cluster ( LinPreorders A ) -> non empty;
coherence
proof
[: A,A :] c= [: A,A :];
then reconsider R = [: A,A :] as (Relation of A);
((for a holds (for b holds ([ a,b ] in R or [ b,a ] in R))) & (for a holds (for b holds (for c holds (([ a,b ] in R & [ b,c ] in R) implies [ a,c ] in R))))) by ZFMISC_1:87;
hence thesis by CKB4:def 1;
end;
