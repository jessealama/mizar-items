begin
reserve A for non empty set;
reserve a for (Element of A);
reserve b for (Element of A);
theorem
Th13: (for o being (Element of ( LinOrders A )) holds (for o9 being (Element of ( LinPreorders A )) holds ((for a holds (for b holds (a <_ o,b implies a <_ o9,b))) iff (for a holds (for b holds (a <_ o,b iff a <_ o9,b))))))
proof
let o being (Element of ( LinOrders A ));
let o9 being (Element of ( LinPreorders A ));
hereby
assume A1: (for a holds (for b holds (a <_ o,b implies a <_ o9,b)));
let a;
let b;
per cases  by CKB19:1;
suppose a <_ o,b;

hence (a <_ o,b iff a <_ o9,b) by A1;
end;
suppose a = b;

hence (a <_ o,b iff a <_ o9,b) by CKB16:1;
end;
suppose A2: b <_ o,a;

then b <_ o9,a by A1;
hence (a <_ o,b iff a <_ o9,b) by A2,CKB17:1;
end;
end;
thus thesis;
