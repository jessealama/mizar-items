requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
theorem
(for V being non  empty RLSStruct holds (for M,N being (Subset of V) holds ((M is  cone & N is  cone) implies ( M /\ N ) is  cone)))
proof
let V being non  empty RLSStruct;
let M being (Subset of V);
let N being (Subset of V);
assume that
A1: M is  cone
and
A2: N is  cone;
let r being Real;
let v being (VECTOR of V);
assume that
A3: r > ( 0 )
and
A4: v in ( M /\ N );
v in N by A4,XBOOLE_0:def 4;
then A5: ( r * v ) in N by A2,A3,CKB11:def 1;
v in M by A4,XBOOLE_0:def 4;
then ( r * v ) in M by A1,A3,CKB11:def 1;
hence thesis by A5,XBOOLE_0:def 4;
end;
