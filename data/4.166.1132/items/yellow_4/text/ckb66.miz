begin
theorem
Th50: (for L being  antisymmetric  reflexive  with_infima RelStr holds (for A,B being  lower (Subset of L) holds ( A "/\" B ) = ( A /\ B )))
proof
let L being  antisymmetric  reflexive  with_infima RelStr;
let A being  lower (Subset of L);
let B being  lower (Subset of L);
thus ( A "/\" B ) c= ( A /\ B )
proof
let q being set;
assume q in ( A "/\" B );
then consider x,y being (Element of L) such that A1: q = ( x "/\" y ) and A2: x in A and A3: y in B;
A4: (ex z being (Element of L) st ((x >= z & y >= z) & (for c being (Element of L) holds ((x >= c & y >= c) implies z >= c)))) by LATTICE3:def 11;
then ( x "/\" y ) <= y by LATTICE3:def 14;
then A5: q in B by A1,A3,WAYBEL_0:def 19;
( x "/\" y ) <= x by A4,LATTICE3:def 14;
then q in A by A1,A2,WAYBEL_0:def 19;
hence thesis by A5,XBOOLE_0:def 4;
end;

thus thesis by CKB65:1;
end;
