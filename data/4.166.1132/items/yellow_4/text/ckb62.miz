begin
Lm3:now
let L being non  empty RelStr;
let x being (Element of L);
let y being (Element of L);
thus { ( a "/\" b ) where a is (Element of L),b is (Element of L): (a in { x } & b in { y }) } = { ( x "/\" y ) }
proof
thus { ( a "/\" b ) where a is (Element of L),b is (Element of L): (a in { x } & b in { y }) } c= { ( x "/\" y ) }
proof
let q being set;
assume q in { ( a "/\" b ) where a is (Element of L),b is (Element of L): (a in { x } & b in { y }) };
then consider u,v being (Element of L) such that A1: q = ( u "/\" v ) and A2: (u in { x } & v in { y });
(u = x & v = y) by A2,TARSKI:def 1;
hence thesis by A1,TARSKI:def 1;
end;

let q being set;
assume q in { ( x "/\" y ) };
then A3: q = ( x "/\" y ) by TARSKI:def 1;
(x in { x } & y in { y }) by TARSKI:def 1;
hence thesis by A3;
end;

end;
theorem
(for L being non  empty RelStr holds (for x,y being (Element of L) holds ( { x } "/\" { y } ) = { ( x "/\" y ) })) by Lm3;
