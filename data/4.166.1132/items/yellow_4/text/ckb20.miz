proof
let L being  with_suprema  antisymmetric  transitive RelStr;
let D1 being (Subset of L);
let D2 being (Subset of L);
let D3 being (Subset of L);
thus ( ( D1 "\/" D2 ) "\/" D3 ) c= ( D1 "\/" ( D2 "\/" D3 ) )
proof
let q being set;
