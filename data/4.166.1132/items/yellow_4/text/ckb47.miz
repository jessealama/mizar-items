begin
definition
let L being  with_infima  antisymmetric RelStr;
let D1 being (Subset of L);
let D2 being (Subset of L);
redefine func D1 "/\" D2;

commutativity
proof
let D1 being (Subset of L);
let D2 being (Subset of L);
thus ( D1 "/\" D2 ) c= ( D2 "/\" D1 )
proof
let q being set;
assume q in ( D1 "/\" D2 );
then (ex x,y being (Element of L) st ((q = ( x "/\" y ) & x in D1) & y in D2));
hence thesis;
end;

let q being set;
assume q in ( D2 "/\" D1 );
then (ex x,y being (Element of L) st ((q = ( x "/\" y ) & x in D2) & y in D1));
hence thesis;
end;
end;
