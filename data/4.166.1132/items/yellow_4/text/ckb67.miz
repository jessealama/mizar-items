begin
theorem
(for L being  with_infima  reflexive  antisymmetric RelStr holds (for D being (Subset of L) holds (for x being (Element of L) holds (x is_>=_than D implies ( { x } "/\" D ) = D))))
proof
let L being  with_infima  reflexive  antisymmetric RelStr;
let D being (Subset of L);
let x being (Element of L);
assume that
A1: x is_>=_than D;
A2: ( { x } "/\" D ) = { ( x "/\" y ) where y is (Element of L): y in D } by CKB58:1;
thus ( { x } "/\" D ) c= D
proof
let q being set;
assume q in ( { x } "/\" D );
then consider y being (Element of L) such that A3: q = ( x "/\" y ) and A4: y in D by A2;
x >= y by A1,A4,LATTICE3:def 9;
hence thesis by A3,A4,YELLOW_0:25;
end;

let q being set;
assume A5: q in D;
then reconsider D1 = D as non  empty (Subset of L);
reconsider y = q as (Element of D1) by A5;
x >= y by A1,LATTICE3:def 9;
then q = ( x "/\" y ) by YELLOW_0:25;
hence thesis by A2;
end;
