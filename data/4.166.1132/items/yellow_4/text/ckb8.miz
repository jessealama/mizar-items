environ
vocabularies ORDERS_2,SUBSET_1,YELLOW_0,XXREAL_0,EQREL_1,LATTICE3,LATTICES,SETFAM_1,XBOOLE_0,RELAT_2,WAYBEL_0,TARSKI,STRUCT_0,ORDINAL2,REWRITE1,YELLOW_1,ZFMISC_1,RELAT_1;
notations TARSKI,XBOOLE_0,SUBSET_1,LATTICE3,YELLOW_0,DOMAIN_1,STRUCT_0,ORDERS_2,YELLOW_1,WAYBEL_0,YELLOW_3,CKB3,CKB4,CKB5,CKB6;
definitions LATTICE3,TARSKI,WAYBEL_0,XBOOLE_0,CKB3,CKB4;
theorems LATTICE3,ORDERS_2,TARSKI,WAYBEL_0,YELLOW_0,YELLOW_1,YELLOW_2,YELLOW_3,XBOOLE_0,XBOOLE_1,CKB1,CKB2,CKB3,CKB4,CKB7;
registrations STRUCT_0,LATTICE3,YELLOW_0,WAYBEL_0,YELLOW_1,YELLOW_2;
constructors DOMAIN_1,LATTICE3,YELLOW_1,YELLOW_3,CKB3,CKB4,CKB5,CKB6;
requirements SUBSET,BOOLE;
begin
theorem
(for L being  transitive RelStr holds (for A,B,C being (Subset of L) holds ((A is_finer_than B & B is_finer_than C) implies A is_finer_than C)))
proof
let L being  transitive RelStr;
let A being (Subset of L),B being (Subset of L),C being (Subset of L);
assume that
A1: A is_finer_than B
and
A2: B is_finer_than C;
let a being (Element of L);
assume a in A;
then consider b being (Element of L) such that A3: b in B and A4: a <= b by A1,CKB3:def 1;
consider c being (Element of L) such that A5: (c in C & b <= c) by A2,A3,CKB3:def 1;
take c;
thus thesis by A4,A5,ORDERS_2:3;
end;
