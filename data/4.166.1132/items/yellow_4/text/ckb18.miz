begin
theorem
(for L being  antisymmetric  with_suprema RelStr holds (for A,B being (Subset of L) holds ( A "\/" B ) is_coarser_than A))
proof
let L being  antisymmetric  with_suprema RelStr;
let A being (Subset of L),B being (Subset of L);
let q being (Element of L);
assume q in ( A "\/" B );
then consider x being (Element of L),y being (Element of L) such that A1: q = ( x "\/" y ) and A2: x in A and y in B;
take x;
thus x in A by A2;
thus thesis by A1,YELLOW_0:22;
end;
