begin
theorem
(for L being  with_suprema  reflexive  antisymmetric RelStr holds (for D being (Subset of L) holds (for x being (Element of L) holds (x is_<=_than D implies ( { x } "\/" D ) = D))))
proof
let L being  with_suprema  reflexive  antisymmetric RelStr;
let D being (Subset of L);
let x being (Element of L);
assume that
A1: x is_<=_than D;
A2: ( { x } "\/" D ) = { ( x "\/" y ) where y is (Element of L): y in D } by CKB25:1;
thus ( { x } "\/" D ) c= D
proof
let q being set;
assume q in ( { x } "\/" D );
then consider y being (Element of L) such that A3: q = ( x "\/" y ) and A4: y in D by A2;
x <= y by A1,A4,LATTICE3:def 8;
hence thesis by A3,A4,YELLOW_0:24;
end;

let q being set;
assume A5: q in D;
then reconsider D1 = D as non empty (Subset of L);
reconsider y = q as (Element of D1) by A5;
x <= y by A1,LATTICE3:def 8;
then q = ( x "\/" y ) by YELLOW_0:24;
hence thesis by A2;
end;
