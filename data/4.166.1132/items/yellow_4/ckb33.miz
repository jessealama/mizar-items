begin
theorem
(for L being  with_suprema  antisymmetric RelStr holds (for D being (Subset of L) holds (for x being (Element of L) holds x is_<=_than ( { x } "\/" D ))))
proof
let L being  with_suprema  antisymmetric RelStr;
let D being (Subset of L);
let x being (Element of L);
let b being (Element of L);
A1: ( { x } "\/" D ) = { ( x "\/" h ) where h is (Element of L): h in D } by CKB25:1;
assume b in ( { x } "\/" D );
then consider h being (Element of L) such that A2: b = ( x "\/" h ) and h in D by A1;
(ex w being (Element of L) st ((x <= w & h <= w) & (for c being (Element of L) holds ((x <= c & h <= c) implies w <= c)))) by LATTICE3:def 10;
hence thesis by A2,LATTICE3:def 13;
end;
