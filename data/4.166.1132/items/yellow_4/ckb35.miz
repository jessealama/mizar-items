begin
theorem
Th25: (for L being  complete  transitive  antisymmetric non empty RelStr holds (for A being (Subset of L) holds (for B being non empty (Subset of L) holds A is_<=_than ( sup ( A "\/" B ) ))))
proof
let L being  complete  transitive  antisymmetric non empty RelStr;
let A being (Subset of L);
let B being non empty (Subset of L);
set b = the (Element of B);
let x being (Element of L);
assume x in A;
then A1: ( x "\/" b ) in ( A "\/" B );
(ex xx being (Element of L) st ((x <= xx & b <= xx) & (for c being (Element of L) holds ((x <= c & b <= c) implies xx <= c)))) by LATTICE3:def 10;
then A2: x <= ( x "\/" b ) by LATTICE3:def 13;
 ex_sup_of ( A "\/" B ),L by YELLOW_0:17;
then ( A "\/" B ) is_<=_than ( sup ( A "\/" B ) ) by YELLOW_0:def 9;
then ( x "\/" b ) <= ( sup ( A "\/" B ) ) by A1,LATTICE3:def 9;
hence thesis by A2,YELLOW_0:def 2;
end;
