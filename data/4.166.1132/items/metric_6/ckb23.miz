requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve n for (Element of ( NAT ));
reserve m for (Element of ( NAT ));
reserve m1 for (Element of ( NAT ));
reserve q for Real;
reserve r for Real;
reserve X for non empty MetrSpace;
reserve x for (Element of X);
reserve V for (Subset of X);
reserve S for (sequence of X);
theorem
Th31: ((for r holds (( 0 ) < r implies ( Ball (x,r) ) contains_almost_all_sequence S)) implies (for V holds ((x in V & V in ( Family_open_set X )) implies V contains_almost_all_sequence S)))
proof
assume A1: (for r holds (( 0 ) < r implies ( Ball (x,r) ) contains_almost_all_sequence S));
A2: (for r holds (( 0 ) < r implies (ex m st (for n holds (m <= n implies ( S . n ) in ( Ball (x,r) ))))))
proof
let r;
assume ( 0 ) < r;
then ( Ball (x,r) ) contains_almost_all_sequence S by A1;
hence thesis by CKB12:def 1;
end;
thus (for V holds ((x in V & V in ( Family_open_set X )) implies V contains_almost_all_sequence S))
proof
let V;
assume (x in V & V in ( Family_open_set X ));
then consider q such that A3: ( 0 ) < q and A4: ( Ball (x,q) ) c= V by PCOMPS_1:def 4;
consider m1 such that A5: (for n holds (m1 <= n implies ( S . n ) in ( Ball (x,q) ))) by A2,A3;
now
let n;
assume m1 <= n;
then ( S . n ) in ( Ball (x,q) ) by A5;
hence ( S . n ) in V by A4;
end;
hence thesis by CKB12:def 1;
end;

end;
