begin
reserve P for (Subset of ( TOP-REAL 2 ));
theorem
Th11: (for P being (Subset of (the carrier of ( TOP-REAL 2 ))) holds (for p1,p2,q1,q2 being (Point of ( TOP-REAL 2 )) holds (((P is_an_arc_of p1,p2 & p1 <> q1) & p2 <> q2) implies ((not p1 in ( Segment (P,p1,p2,q1,q2) )) & (not p2 in ( Segment (P,p1,p2,q1,q2) ))))))
proof
let P being (Subset of (the carrier of ( TOP-REAL 2 )));
let p1 being (Point of ( TOP-REAL 2 ));
let p2 being (Point of ( TOP-REAL 2 ));
let q1 being (Point of ( TOP-REAL 2 ));
let q2 being (Point of ( TOP-REAL 2 ));
assume ((P is_an_arc_of p1,p2 & p1 <> q1) & p2 <> q2);
then A1: ((not p2 in ( L_Segment (P,p1,p2,q2) )) & (not p1 in ( R_Segment (P,p1,p2,q1) ))) by JORDAN6:59,JORDAN6:60;
( Segment (P,p1,p2,q1,q2) ) = ( ( R_Segment (P,p1,p2,q1) ) /\ ( L_Segment (P,p1,p2,q2) ) ) by JORDAN6:def 5;
hence thesis by A1,XBOOLE_0:def 4;
end;
