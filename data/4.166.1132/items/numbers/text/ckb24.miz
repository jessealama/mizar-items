environ
vocabularies ORDINAL1,TARSKI,SUBSET_1,ARYTM_3,XBOOLE_0,CARD_1,ARYTM_2,ZFMISC_1,FUNCT_2,FUNCT_1,FUNCOP_1,RELAT_1,ORDINAL2,ORDINAL3,FINSET_1,NUMBERS;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,RELAT_1,FUNCT_1,FUNCT_2,FUNCT_4,ORDINAL1,ORDINAL2,ORDINAL3,FINSET_1,ARYTM_3,ARYTM_2,CKB2,CKB3,CKB5,CKB8,CKB9,CKB10,CKB11,CKB19;
definitions TARSKI,ARYTM_3,XBOOLE_0,CKB5,CKB8,CKB9,CKB10;
theorems XBOOLE_1,ARYTM_2,ZFMISC_1,ARYTM_3,XBOOLE_0,TARSKI,ORDINAL2,ORDINAL3,ENUMSET1,ORDINAL1,FUNCT_2,FUNCT_4,RELAT_1,FINSET_1,CKB1,CKB4,CKB5,CKB6,CKB8,CKB9,CKB10,CKB12,CKB13,CKB17,CKB18,CKB20,CKB21,CKB22,CKB23;
schemes DOMAIN_1;
registrations XBOOLE_0,ORDINAL1,FUNCT_2,ARYTM_3,ARYTM_2,RELAT_1,ORDINAL2,ORDINAL3,CARD_1,CKB7,CKB14,CKB15,CKB16;
constructors FUNCT_4,ORDINAL3,ARYTM_2,FINSET_1,CKB5,CKB8,CKB9,CKB10,CKB11,CKB19;
