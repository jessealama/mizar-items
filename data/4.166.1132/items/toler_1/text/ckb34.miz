environ
vocabularies XBOOLE_0,RELAT_2,RELAT_1,EQREL_1,WELLORD1,ZFMISC_1,PARTFUN1,SUBSET_1,TARSKI,ORDINAL1,TOLER_1;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,RELAT_1,RELSET_1,RELAT_2,PARTFUN1,ORDINAL1,WELLORD1,EQREL_1,CKB2,CKB3,CKB14,CKB16,CKB18,CKB20;
definitions TARSKI,RELAT_1,RELAT_2,WELLORD1,CKB16,CKB18;
theorems TARSKI,RELAT_1,RELSET_1,RELAT_2,ZFMISC_1,WELLORD1,ENUMSET1,ORDERS_1,EQREL_1,ORDINAL1,XBOOLE_0,XBOOLE_1,PARTFUN1,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB15,CKB16,CKB17,CKB18,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB33;
schemes XBOOLE_0,CKB32;
registrations XBOOLE_0,SUBSET_1,RELAT_1,PARTFUN1,EQREL_1,RELSET_1,CKB1,CKB19;
constructors ORDINAL1,WELLORD1,EQREL_1,RELSET_1,CKB3,CKB14,CKB16,CKB18;
requirements SUBSET,BOOLE;
begin
reserve X for set;
reserve Y for set;
reserve Z for set;
reserve x for set;
reserve y for set;
reserve T for (Tolerance of X);
reserve R for (Tolerance of X);
theorem
(for Y being set holds (for T,R being (Tolerance of ( union Y )) holds (((for x holds (for y holds ([ x,y ] in T iff (ex Z st ((Z in Y & x in Z) & y in Z))))) & (for x holds (for y holds ([ x,y ] in R iff (ex Z st ((Z in Y & x in Z) & y in Z)))))) implies T = R)))
proof
let Y being set;
let T being (Tolerance of ( union Y ));
let R being (Tolerance of ( union Y ));
assume that
A1: (for x holds (for y holds ([ x,y ] in T iff (ex Z st ((Z in Y & x in Z) & y in Z)))))
and
A2: (for x holds (for y holds ([ x,y ] in R iff (ex Z st ((Z in Y & x in Z) & y in Z)))));
(for x holds (for y holds ([ x,y ] in T iff [ x,y ] in R)))
proof
let x;
let y;
thus ([ x,y ] in T implies [ x,y ] in R)
proof
assume [ x,y ] in T;
then (ex Z st ((Z in Y & x in Z) & y in Z)) by A1;
hence thesis by A2;
end;

assume [ x,y ] in R;
then (ex Z st ((Z in Y & x in Z) & y in Z)) by A2;
hence thesis by A1;
end;
hence thesis by RELAT_1:def 2;
end;
