theorem
Th32: (for X being non  empty set holds (for seq being (sequence of ( C_Normed_Algebra_of_BoundedFunctions X )) holds (seq is  CCauchy implies seq is  convergent)))
proof
let X being non  empty set;
let vseq being (sequence of ( C_Normed_Algebra_of_BoundedFunctions X ));
defpred S1[ set,set ]
 means
(ex xseq being Complex_Sequence st (for n being (Element of ( NAT )) holds ((( xseq . n ) = ( ( modetrans (( vseq . n ),X) ) . $1 ) & xseq is  convergent) & $2 = ( lim xseq ))));
assume A1: vseq is  CCauchy;
A2: (for x being (Element of X) holds (ex y being (Element of ( COMPLEX )) st S1[ x,y ]))
proof
let x being (Element of X);
deffunc H1((Element of ( NAT ))) = ( ( modetrans (( vseq . $1 ),X) ) . x );
consider xseq being Complex_Sequence such that A3: (for n being (Element of ( NAT )) holds ( xseq . n ) = H1(n)) from FUNCT_2:sch 4;
take y = ( lim xseq );
A4: (for m,k being (Element of ( NAT )) holds |. ( ( xseq . m ) - ( xseq . k ) ) .| <= ||. ( ( vseq . m ) - ( vseq . k ) ) .||)
proof
let m being (Element of ( NAT ));
let k being (Element of ( NAT ));
( ( vseq . m ) - ( vseq . k ) ) in ( ComplexBoundedFunctions X );
then consider h1 being (Function of X,( COMPLEX )) such that A5: h1 = ( ( vseq . m ) - ( vseq . k ) ) and A6: ( h1 | X ) is  bounded;
( vseq . m ) in ( ComplexBoundedFunctions X );
then (ex vseqm being (Function of X,( COMPLEX )) st (( vseq . m ) = vseqm & ( vseqm | X ) is  bounded));
then A7: ( modetrans (( vseq . m ),X) ) = ( vseq . m ) by CKB27:1;
( vseq . k ) in ( ComplexBoundedFunctions X );
then (ex vseqk being (Function of X,( COMPLEX )) st (( vseq . k ) = vseqk & ( vseqk | X ) is  bounded));
then A8: ( modetrans (( vseq . k ),X) ) = ( vseq . k ) by CKB27:1;
(( xseq . m ) = ( ( modetrans (( vseq . m ),X) ) . x ) & ( xseq . k ) = ( ( modetrans (( vseq . k ),X) ) . x )) by A3;
then ( ( xseq . m ) - ( xseq . k ) ) = ( h1 . x ) by A7,A8,A5,CKB46:1;
hence |. ( ( xseq . m ) - ( xseq . k ) ) .| <= ||. ( ( vseq . m ) - ( vseq . k ) ) .|| by A5,A6,CKB37:1;
end;
now
let e being Real;
assume e > ( 0 );
then consider k being (Element of ( NAT )) such that A9: (for n,m being (Element of ( NAT )) holds ((n >= k & m >= k) implies ||. ( ( vseq . n ) - ( vseq . m ) ) .|| < e)) by A1,CSSPACE3:8;
take k;
hereby
let n being (Element of ( NAT ));
assume n >= k;
then A10: ||. ( ( vseq . n ) - ( vseq . k ) ) .|| < e by A9;
|. ( ( xseq . n ) - ( xseq . k ) ) .| <= ||. ( ( vseq . n ) - ( vseq . k ) ) .|| by A4;
hence |. ( ( xseq . n ) - ( xseq . k ) ) .| < e by A10,XXREAL_0:2;
end;
end;
then xseq is  convergent by COMSEQ_3:46;
hence S1[ x,y ] by A3;
end;
consider tseq being (Function of X,( COMPLEX )) such that A11: (for x being (Element of X) holds S1[ x,( tseq . x ) ]) from FUNCT_2:sch 3(A2);
now
let e1 being  real number;
assume A12: e1 > ( 0 );
reconsider e = e1 as Real by XREAL_0:def 1;
consider k being (Element of ( NAT )) such that A13: (for n,m being (Element of ( NAT )) holds ((n >= k & m >= k) implies ||. ( ( vseq . n ) - ( vseq . m ) ) .|| < e)) by A1,A12,CSSPACE3:8;
take k;
now
let m being (Element of ( NAT ));
A14: ||. ( vseq . m ) .|| = ( ||. vseq .|| . m ) by NORMSP_0:def 4;
assume m >= k;
then A15: ||. ( ( vseq . m ) - ( vseq . k ) ) .|| < e by A13;
(|. ( ||. ( vseq . m ) .|| - ||. ( vseq . k ) .|| ) .| <= ||. ( ( vseq . m ) - ( vseq . k ) ) .|| & ||. ( vseq . k ) .|| = ( ||. vseq .|| . k )) by CLVECT_1:110,NORMSP_0:def 4;
hence |. ( ( ||. vseq .|| . m ) - ( ||. vseq .|| . k ) ) .| < e1 by A15,A14,XXREAL_0:2;
end;
hence (for m being (Element of ( NAT )) holds (m >= k implies |. ( ( ||. vseq .|| . m ) - ( ||. vseq .|| . k ) ) .| < e1));
end;
then A16: ||. vseq .|| is  convergent by SEQ_4:41;
now
let x being set;
assume A17: x in ( X /\ ( dom tseq ) );
then consider xseq being Complex_Sequence such that A18: (for n being (Element of ( NAT )) holds ( xseq . n ) = ( ( modetrans (( vseq . n ),X) ) . x )) and A19: (xseq is  convergent & ( tseq . x ) = ( lim xseq )) by A11;
A20: (for n being (Element of ( NAT )) holds ( |. xseq .| . n ) <= ( ||. vseq .|| . n ))
proof
let n being (Element of ( NAT ));
A21: ( xseq . n ) = ( ( modetrans (( vseq . n ),X) ) . x ) by A18;
( vseq . n ) in ( ComplexBoundedFunctions X );
then A22: (ex h1 being (Function of X,( COMPLEX )) st (( vseq . n ) = h1 & ( h1 | X ) is  bounded));
then ( modetrans (( vseq . n ),X) ) = ( vseq . n ) by CKB27:1;
then |. ( xseq . n ) .| <= ||. ( vseq . n ) .|| by A17,A22,A21,CKB37:1;
then ( |. xseq .| . n ) <= ||. ( vseq . n ) .|| by VALUED_1:18;
hence ( |. xseq .| . n ) <= ( ||. vseq .|| . n ) by NORMSP_0:def 4;
end;
(|. xseq .| is  convergent & |. ( tseq . x ) .| = ( lim |. xseq .| )) by A19,COMSEQ_2:11;
hence |. ( tseq . x ) .| <= ( lim ||. vseq .|| ) by A16,A20,SEQ_2:18;
end;
then (for x being (Element of X) holds (x in ( X /\ ( dom tseq ) ) implies |. ( tseq /. x ) .| <= ( lim ||. vseq .|| )));
then ( tseq | X ) is  bounded by CFUNCT_1:69;
then tseq in ( ComplexBoundedFunctions X );
then reconsider tv = tseq as (Point of ( C_Normed_Algebra_of_BoundedFunctions X ));
A23: (for e being Real holds (e > ( 0 ) implies (ex k being (Element of ( NAT )) st (for n being (Element of ( NAT )) holds (n >= k implies (for x being (Element of X) holds |. ( ( ( modetrans (( vseq . n ),X) ) . x ) - ( tseq . x ) ) .| <= e))))))
proof
let e being Real;
assume e > ( 0 );
then consider k being (Element of ( NAT )) such that A24: (for n,m being (Element of ( NAT )) holds ((n >= k & m >= k) implies ||. ( ( vseq . n ) - ( vseq . m ) ) .|| < e)) by A1,CSSPACE3:8;
take k;
now
let n being (Element of ( NAT ));
assume A25: n >= k;
now
let x being (Element of X);
consider xseq being Complex_Sequence such that A26: (for n being (Element of ( NAT )) holds ( xseq . n ) = ( ( modetrans (( vseq . n ),X) ) . x )) and A27: xseq is  convergent and A28: ( tseq . x ) = ( lim xseq ) by A11;
reconsider fseq = ( ( NAT ) --> ( xseq . n ) ) as Complex_Sequence;
set wseq = ( xseq - fseq );
deffunc H1((Element of ( NAT ))) = |. ( ( xseq . $1 ) - ( xseq . n ) ) .|;
consider rseq being Real_Sequence such that A29: (for m being (Element of ( NAT )) holds ( rseq . m ) = H1(m)) from SEQ_1:sch 1;
A30: (for m,k being (Element of ( NAT )) holds |. ( ( xseq . m ) - ( xseq . k ) ) .| <= ||. ( ( vseq . m ) - ( vseq . k ) ) .||)
proof
let m being (Element of ( NAT ));
let k being (Element of ( NAT ));
( ( vseq . m ) - ( vseq . k ) ) in ( ComplexBoundedFunctions X );
then consider h1 being (Function of X,( COMPLEX )) such that A31: h1 = ( ( vseq . m ) - ( vseq . k ) ) and A32: ( h1 | X ) is  bounded;
( vseq . m ) in ( ComplexBoundedFunctions X );
then (ex vseqm being (Function of X,( COMPLEX )) st (( vseq . m ) = vseqm & ( vseqm | X ) is  bounded));
then A33: ( modetrans (( vseq . m ),X) ) = ( vseq . m ) by CKB27:1;
( vseq . k ) in ( ComplexBoundedFunctions X );
then (ex vseqk being (Function of X,( COMPLEX )) st (( vseq . k ) = vseqk & ( vseqk | X ) is  bounded));
then A34: ( modetrans (( vseq . k ),X) ) = ( vseq . k ) by CKB27:1;
(( xseq . m ) = ( ( modetrans (( vseq . m ),X) ) . x ) & ( xseq . k ) = ( ( modetrans (( vseq . k ),X) ) . x )) by A26;
then ( ( xseq . m ) - ( xseq . k ) ) = ( h1 . x ) by A33,A34,A31,CKB46:1;
hence |. ( ( xseq . m ) - ( xseq . k ) ) .| <= ||. ( ( vseq . m ) - ( vseq . k ) ) .|| by A31,A32,CKB37:1;
end;
A35: (for m being (Element of ( NAT )) holds (m >= k implies ( rseq . m ) <= e))
proof
let m being (Element of ( NAT ));
assume m >= k;
then A36: ||. ( ( vseq . n ) - ( vseq . m ) ) .|| < e by A24,A25;
( rseq . m ) = |. ( ( xseq . m ) - ( xseq . n ) ) .| by A29;
then ( rseq . m ) = |. ( ( xseq . n ) - ( xseq . m ) ) .| by COMPLEX1:60;
then ( rseq . m ) <= ||. ( ( vseq . n ) - ( vseq . m ) ) .|| by A30;
hence ( rseq . m ) <= e by A36,XXREAL_0:2;
end;
A37:now
let m being (Element of ( NAT ));
( ( xseq - fseq ) . m ) = ( ( xseq . m ) + ( ( - fseq ) . m ) ) by VALUED_1:1
.= ( ( xseq . m ) - ( fseq . m ) ) by VALUED_1:8;
hence ( ( xseq - fseq ) . m ) = ( ( xseq . m ) - ( xseq . n ) ) by FUNCOP_1:7;
end;
now
let x being set;
assume x in ( NAT );
then reconsider k = x as (Element of ( NAT ));
( rseq . x ) = |. ( ( xseq . k ) - ( xseq . n ) ) .| by A29;
then ( rseq . x ) = |. ( ( xseq - fseq ) . k ) .| by A37;
hence ( rseq . x ) = ( |. ( xseq - fseq ) .| . x ) by VALUED_1:18;
end;
then A38: rseq = |. ( xseq - fseq ) .| by FUNCT_2:12;
A39: fseq is  convergent by CFCONT_1:26;
A41: ( lim rseq ) <= e by A39,A35,A38,A27,RSSPACE2:5;
( lim fseq ) = ( fseq . ( 0 ) ) by CFCONT_1:28;
then ( lim fseq ) = ( xseq . n ) by FUNCOP_1:7;
then ( lim ( xseq - fseq ) ) = ( ( tseq . x ) - ( xseq . n ) ) by A27,A28,A39,COMSEQ_2:26;
then ( lim rseq ) = |. ( ( tseq . x ) - ( xseq . n ) ) .| by A39,A27,A38,COMSEQ_2:11;
then |. ( ( xseq . n ) - ( tseq . x ) ) .| <= e by A41,COMPLEX1:60;
hence |. ( ( ( modetrans (( vseq . n ),X) ) . x ) - ( tseq . x ) ) .| <= e by A26;
end;
hence (for x being (Element of X) holds |. ( ( ( modetrans (( vseq . n ),X) ) . x ) - ( tseq . x ) ) .| <= e);
end;
hence (for n being (Element of ( NAT )) holds (n >= k implies (for x being (Element of X) holds |. ( ( ( modetrans (( vseq . n ),X) ) . x ) - ( tseq . x ) ) .| <= e)));
end;
A42: (for e being Real holds (e > ( 0 ) implies (ex k being (Element of ( NAT )) st (for n being (Element of ( NAT )) holds (n >= k implies ||. ( ( vseq . n ) - tv ) .|| <= e)))))
proof
let e being Real;
assume e > ( 0 );
then consider k being (Element of ( NAT )) such that A43: (for n being (Element of ( NAT )) holds (n >= k implies (for x being (Element of X) holds |. ( ( ( modetrans (( vseq . n ),X) ) . x ) - ( tseq . x ) ) .| <= e))) by A23;
take k;
hereby
let n being (Element of ( NAT ));
assume A44: n >= k;
( vseq . n ) in ( ComplexBoundedFunctions X );
then consider f1 being (Function of X,( COMPLEX )) such that A45: f1 = ( vseq . n ) and ( f1 | X ) is  bounded;
( ( vseq . n ) - tv ) in ( ComplexBoundedFunctions X );
then consider h1 being (Function of X,( COMPLEX )) such that A46: h1 = ( ( vseq . n ) - tv ) and A47: ( h1 | X ) is  bounded;
A48:now
let t being (Element of X);
(( modetrans (( vseq . n ),X) ) = f1 & ( h1 . t ) = ( ( f1 . t ) - ( tseq . t ) )) by A45,A46,CKB21:def 1,CKB46:1;
hence |. ( h1 . t ) .| <= e by A43,A44;
end;
A49:now
let r being Real;
assume r in ( PreNorms h1 );
then (ex t being (Element of X) st r = |. ( h1 . t ) .|);
hence r <= e by A48;
end;
((for s being  real number holds (s in ( PreNorms h1 ) implies s <= e)) implies ( upper_bound ( PreNorms h1 ) ) <= e) by SEQ_4:45;
hence ||. ( ( vseq . n ) - tv ) .|| <= e by A46,A47,A49,CKB28:1;
end;
end;
(for e being Real holds (e > ( 0 ) implies (ex m being (Element of ( NAT )) st (for n being (Element of ( NAT )) holds (n >= m implies ||. ( ( vseq . n ) - tv ) .|| < e)))))
proof
let e being Real;
assume A50: e > ( 0 );
consider m being (Element of ( NAT )) such that A51: (for n being (Element of ( NAT )) holds (n >= m implies ||. ( ( vseq . n ) - tv ) .|| <= ( e / 2 ))) by A42,A50;
take m;
A52: ( e / 2 ) < e by A50,XREAL_1:216;
hereby
let n being (Element of ( NAT ));
assume n >= m;
then ||. ( ( vseq . n ) - tv ) .|| <= ( e / 2 ) by A51;
hence ||. ( ( vseq . n ) - tv ) .|| < e by A52,XXREAL_0:2;
end;
end;
hence thesis by CLVECT_1:def 15;
end;
