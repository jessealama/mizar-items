requirements NUMERALS,BOOLE,SUBSET,ARITHM,REAL;
begin
definition
let V being ComplexAlgebra;
let V1 being (Subset of V);
assume that
A1: V1 is  Cadditively-linearly-closed non  empty;
func Mult_ (V1,V) -> (Function of [: ( COMPLEX ),V1 :],V1) equals 
:Def11: ( (the Mult of V) | [: ( COMPLEX ),V1 :] );
correctness
proof
A2: ([: ( COMPLEX ),V1 :] c= [: ( COMPLEX ),(the carrier of V) :] & ( dom (the Mult of V) ) = [: ( COMPLEX ),(the carrier of V) :]) by FUNCT_2:def 1,ZFMISC_1:95;
A3: (for z being set holds (z in [: ( COMPLEX ),V1 :] implies ( ( (the Mult of V) | [: ( COMPLEX ),V1 :] ) . z ) in V1))
proof
let z being set;
assume that
A4: z in [: ( COMPLEX ),V1 :];
consider r,x being set such that A5: r in ( COMPLEX ) and A6: x in V1 and A7: z = [ r,x ] by A4,ZFMISC_1:def 2;
reconsider r as Complex by A5;
reconsider y = x as (VECTOR of V) by A6;
[ r,x ] in ( dom ( (the Mult of V) | [: ( COMPLEX ),V1 :] ) ) by A2,A4,A7,RELAT_1:62;
then ( ( (the Mult of V) | [: ( COMPLEX ),V1 :] ) . z ) = ( r * y ) by A7,FUNCT_1:47;
hence thesis by A1,A6,CKB4:def 1;
end;
( dom ( (the Mult of V) | [: ( COMPLEX ),V1 :] ) ) = [: ( COMPLEX ),V1 :] by A2,RELAT_1:62;
hence thesis by A3,FUNCT_2:3;
end;
end;
