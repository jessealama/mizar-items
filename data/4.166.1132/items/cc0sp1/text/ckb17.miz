begin
theorem
Th9: (for X being non  empty set holds (for a being Complex holds (for F,G being (VECTOR of ( C_Algebra_of_BoundedFunctions X )) holds (for f,g being (Function of X,( COMPLEX )) holds ((f = F & g = G) implies (G = ( a * F ) iff (for x being (Element of X) holds ( g . x ) = ( a * ( f . x ) ))))))))
proof
let X being non  empty set;
let a being Complex;
let F being (VECTOR of ( C_Algebra_of_BoundedFunctions X ));
let G being (VECTOR of ( C_Algebra_of_BoundedFunctions X ));
let f being (Function of X,( COMPLEX ));
let g being (Function of X,( COMPLEX ));
assume A1: (f = F & g = G);
A2: ( C_Algebra_of_BoundedFunctions X ) is (ComplexSubAlgebra of ( CAlgebra X )) by CKB11:1;
reconsider f1 = F,g1 = G as (VECTOR of ( CAlgebra X )) by TARSKI:def 3;
hereby
assume A3: G = ( a * F );
let x being (Element of X);
g1 = ( a * f1 ) by A2,A3,CKB12:1;
hence ( g . x ) = ( a * ( f . x ) ) by A1,CFUNCDOM:4;
end;
assume (for x being (Element of X) holds ( g . x ) = ( a * ( f . x ) ));
then g1 = ( a * f1 ) by A1,CFUNCDOM:4;
hence G = ( a * F ) by A2,CKB12:1;
end;
