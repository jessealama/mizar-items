theorem
Th1: (for v1 holds (for v2 holds (for v holds ((v1 is_a_prefix_of v & v2 is_a_prefix_of v) implies v1,v2 are_c=-comparable ))))
proof
let p being FinSequence;
let q being FinSequence;
let r being FinSequence;
assume p is_a_prefix_of r;
then A1: (ex p9 being FinSequence st r = ( p ^ p9 )) by TREES_1:1;
assume q is_a_prefix_of r;
then A2: (ex q9 being FinSequence st r = ( q ^ q9 )) by TREES_1:1;
(( len p ) <= ( len q ) or ( len q ) <= ( len p ));
then ((ex t being FinSequence st ( p ^ t ) = q) or (ex t being FinSequence st ( q ^ t ) = p)) by A1,A2,FINSEQ_1:47;
hence (p is_a_prefix_of q or q is_a_prefix_of p) by TREES_1:1;
end;
