requirements NUMERALS,REAL,BOOLE,SUBSET,ARITHM;
begin
reserve x for set;
reserve X for set;
reserve Y for set;
reserve W for Tree;
reserve B for (Branch of W);
theorem
(for X,Y being set holds (for B being  c=-linear (Subset of ( PFuncs (X,Y) )) holds ( union B ) in ( PFuncs (X,Y) )))
proof
let X being set;
let Y being set;
let B being  c=-linear (Subset of ( PFuncs (X,Y) ));
(for x being set holds (x in B implies x is Function));
then reconsider f = ( union B ) as Function by CKB69:1;
per cases ;
suppose B <> ( {} );

then reconsider D = B as non  empty  functional set;
A1:now
let x being set;
assume x in { ( dom g ) where g is (Element of D): (not contradiction) };
then consider g being (Element of D) such that A2: x = ( dom g );
g in ( PFuncs (X,Y) ) by TARSKI:def 3;
then (ex f being Function st ((g = f & ( dom f ) c= X) & ( rng f ) c= Y)) by PARTFUN1:def 3;
hence x c= X by A2;
end;
A3:now
let x being set;
assume x in { ( rng g ) where g is (Element of D): (not contradiction) };
then consider g being (Element of D) such that A4: x = ( rng g );
g in ( PFuncs (X,Y) ) by TARSKI:def 3;
then (ex f being Function st ((g = f & ( dom f ) c= X) & ( rng f ) c= Y)) by PARTFUN1:def 3;
hence x c= Y by A4;
end;
( rng f ) = ( union { ( rng g ) where g is (Element of D): (not contradiction) } ) by FUNCT_1:110;
then A5: ( rng f ) c= Y by A3,ZFMISC_1:76;
( dom f ) = ( union { ( dom g ) where g is (Element of D): (not contradiction) } ) by FUNCT_1:110;
then ( dom f ) c= X by A1,ZFMISC_1:76;
hence thesis by A5,PARTFUN1:def 3;
end;
suppose A6: B = ( {} );

( {} ) is (PartFunc of X,Y) by RELSET_1:12;
hence thesis by A6,PARTFUN1:45,ZFMISC_1:2;
end;
end;
