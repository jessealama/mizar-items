begin
reserve a for set;
reserve n for (Element of ( NAT ));
reserve w for (FinSequence of ( NAT ));
theorem
(for n being (Element of ( NAT )) holds ( ( TrivialInfiniteTree ) -level n ) = { ( n |-> ( 0 ) ) })
proof
set T = ( TrivialInfiniteTree );
let n being (Element of ( NAT ));
set L = { w where w is (Element of T): ( len w ) = n };
set f = ( n |-> ( 0 ) );
{ f } = L
proof
hereby
let a being set;
assume a in { f };
then A1: a = f by TARSKI:def 1;
(f in T & ( len f ) = n) by CARD_1:def 7;
hence a in L by A1;
end;
let a being set;
assume a in L;
then consider w being (Element of T) such that A2: (a = w & ( len w ) = n);
w in T;
then (ex k being (Element of ( NAT )) st w = ( k |-> ( 0 ) ));
then a = f by A2,CARD_1:def 7;
hence thesis by TARSKI:def 1;
end;
hence thesis;
end;
