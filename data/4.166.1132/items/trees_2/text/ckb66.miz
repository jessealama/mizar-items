reserve T1 for DecoratedTree;
definition
let T;
let p;
let T1;
assume A1: p in ( dom T );
func T with-replacement (p,T1) -> DecoratedTree means 
(( dom it ) = ( ( dom T ) with-replacement (p,( dom T1 )) ) & (for q holds (q in ( ( dom T ) with-replacement (p,( dom T1 )) ) implies (((not p is_a_prefix_of q) & ( it . q ) = ( T . q )) or (ex r st ((r in ( dom T1 ) & q = ( p ^ r )) & ( it . q ) = ( T1 . r )))))));
existence
proof
defpred X[ FinSequence,set ]
 means
(((not p is_a_prefix_of $1) & $2 = ( T . $1 )) or (ex r st ((r in ( dom T1 ) & $1 = ( p ^ r )) & $2 = ( T1 . r ))));
A2: (for q holds (q in ( ( dom T ) with-replacement (p,( dom T1 )) ) implies (ex x st X[ q,x ])))
proof
let q;
assume that
A3: q in ( ( dom T ) with-replacement (p,( dom T1 )) );
now
per cases  by XBOOLE_0:def 8;
suppose p is_a_proper_prefix_of q;

then consider r such that A4: (r in ( dom T1 ) & q = ( p ^ r )) by A1,A3,TREES_1:def 9;
thus thesis
proof
take ( T1 . r );
thus thesis by A4;
end;

end;
suppose A5: p = q;

thus thesis
proof
take ( T1 . ( {} ( NAT ) ) );
(( {} ( NAT ) ) in ( dom T1 ) & q = ( p ^ ( <*> ( NAT ) ) )) by A5,FINSEQ_1:34,TREES_1:22;
hence thesis;
end;

end;
suppose (not p is_a_prefix_of q);

hence thesis;
end;
end;
hence thesis;
end;
thus (ex TT being DecoratedTree st (( dom TT ) = ( ( dom T ) with-replacement (p,( dom T1 )) ) & (for q holds (q in ( ( dom T ) with-replacement (p,( dom T1 )) ) implies X[ q,( TT . q ) ])))) from CKB59:sch 1(A2);
end;
uniqueness
proof
let D1 being DecoratedTree;
let D2 being DecoratedTree;
assume that
A6: ( dom D1 ) = ( ( dom T ) with-replacement (p,( dom T1 )) )
and
A7: (for q holds (q in ( ( dom T ) with-replacement (p,( dom T1 )) ) implies (((not p is_a_prefix_of q) & ( D1 . q ) = ( T . q )) or (ex r st ((r in ( dom T1 ) & q = ( p ^ r )) & ( D1 . q ) = ( T1 . r ))))))
and
A8: ( dom D2 ) = ( ( dom T ) with-replacement (p,( dom T1 )) )
and
A9: (for q holds (q in ( ( dom T ) with-replacement (p,( dom T1 )) ) implies (((not p is_a_prefix_of q) & ( D2 . q ) = ( T . q )) or (ex r st ((r in ( dom T1 ) & q = ( p ^ r )) & ( D2 . q ) = ( T1 . r ))))));
now
let q;
assume that
A10: q in ( dom D1 )
and
A11: ( D1 . q ) <> ( D2 . q );
A12: (((not p is_a_prefix_of q) & ( D1 . q ) = ( T . q )) or (ex r st ((r in ( dom T1 ) & q = ( p ^ r )) & ( D1 . q ) = ( T1 . r )))) by A6,A7,A10;
(((not p is_a_prefix_of q) & ( D2 . q ) = ( T . q )) or (ex r st ((r in ( dom T1 ) & q = ( p ^ r )) & ( D2 . q ) = ( T1 . r )))) by A6,A9,A10;
hence contradiction by A11,A12,FINSEQ_1:33,TREES_1:1;
end;
hence thesis by A6,A8,CKB58:1;
end;
