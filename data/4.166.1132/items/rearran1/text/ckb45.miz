begin
reserve C for non  empty  finite set;
reserve D for non  empty  finite set;
theorem
Th31: (for F being (PartFunc of D,( REAL )) holds (for A being (RearrangmentGen of C) holds ((F is  total & ( card D ) = ( card C )) implies (( len ( FinS (( Rland (F,A) ),C) ) ) = ( card C ) & 1 <= ( len ( FinS (( Rland (F,A) ),C) ) )))))
proof
let F being (PartFunc of D,( REAL ));
let B being (RearrangmentGen of C);
set p = ( Rland (F,B) );
assume (F is  total & ( card D ) = ( card C ));
then A1: ( dom p ) = C by CKB27:1;
then A2: ( p | C ) = p by RELAT_1:68;
hence ( len ( FinS (p,C) ) ) = ( card C ) by A1,RFUNCT_3:67;
( ( 0 ) + 1 ) <= ( card C ) by NAT_1:13;
hence thesis by A1,A2,RFUNCT_3:67;
end;
