begin
reserve m for (Element of ( NAT ));
reserve D for non  empty  finite set;
theorem
(for A being (RearrangmentGen of D) holds ( Co_Gen ( Co_Gen A ) ) = A)
proof
let A being (RearrangmentGen of D);
set C = ( Co_Gen A );
set B = ( Co_Gen C );
A1: ( len A ) = ( card D ) by CKB11:1;
A2: ( len B ) = ( card D ) by CKB11:1;
A3: ( len C ) = ( card D ) by CKB11:1;
A4: (for m being Nat holds (m in ( Seg ( len A ) ) implies ( A . m ) = ( B . m )))
proof
let m being Nat;
assume m in ( Seg ( len A ) );
then A5: m in ( dom A ) by FINSEQ_1:def 3;
then A6: m <= ( len A ) by FINSEQ_3:25;
A7: 1 <= m by A5,FINSEQ_3:25;
now
per cases ;
case A8: m = ( len A );
then ( A . m ) = D by CKB13:1;
hence thesis by A1,A2,A8,CKB13:1;
end;
case m <> ( len A );
then m < ( len A ) by A6,XXREAL_0:1;
then A9: ( m + 1 ) <= ( len A ) by NAT_1:13;
then A10: m <= ( ( len A ) - 1 ) by XREAL_1:19;
( max (( 0 ),( ( len A ) - m )) ) = ( ( len A ) - m ) by A6,FINSEQ_2:4;
then reconsider k = ( ( len A ) - m ) as (Element of ( NAT )) by FINSEQ_2:5;
A11: ( ( len A ) - m ) <= ( ( len A ) - 1 ) by A7,XREAL_1:13;
1 <= ( ( len A ) - m ) by A9,XREAL_1:19;
then ( C . k ) = ( D \ ( A . ( ( len A ) - k ) ) ) by A3,A1,A11,CKB22:def 1
.= ( D \ ( A . m ) );
hence ( B . m ) = ( D \ ( D \ ( A . m ) ) ) by A3,A1,A2,A7,A10,CKB22:def 1
.= ( D /\ ( A . m ) ) by XBOOLE_1:48
.= ( A . m ) by A5,CKB17:1,XBOOLE_1:28;
end;
end;
hence thesis;
end;
(( dom A ) = ( Seg ( len A ) ) & ( dom B ) = ( Seg ( len B ) )) by FINSEQ_1:def 3;
hence thesis by A1,A2,A4,FINSEQ_1:13;
end;
