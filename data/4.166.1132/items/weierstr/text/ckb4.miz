theorem
Th5: (for T,S being non empty TopSpace holds (for f being (Function of T,S) holds (for B being (Subset-Family of S) holds ((f is  continuous & B is  open) implies ( f " B ) is  open))))
proof
let T being non empty TopSpace,S being non empty TopSpace;
let f being (Function of T,S);
let B being (Subset-Family of S);
assume that
A1: f is  continuous
and
A2: B is  open;
(for P being (Subset of T) holds (P in ( f " B ) implies P is  open))
proof
let P being (Subset of T);
assume A3: P in ( f " B );
thus P is  open
proof
consider C being (Subset of S) such that A4: C in B and A5: P = ( f " C ) by A3,FUNCT_2:def 9;
reconsider C as (Subset of S);
(( [#] S ) <> ( {} ) & C is  open) by A2,A4,TOPS_2:def 1;
hence thesis by A1,A5,TOPS_2:43;
end;

end;
hence thesis by TOPS_2:def 1;
