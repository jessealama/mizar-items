theorem
Th22: (for X being non empty set holds (for F being (BinOp of X) holds (F is  having_a_unity iff ( the_unity_wrt F ) is_a_unity_wrt F)))
proof
let X being non empty set;
let F being (BinOp of X);
thus (F is  having_a_unity implies ( the_unity_wrt F ) is_a_unity_wrt F)
proof
assume F is  having_a_unity;
then (ex x being (Element of X) st x is_a_unity_wrt F) by CKB18:def 1;
hence thesis by BINOP_1:def 8;
end;

thus thesis by CKB18:def 1;
