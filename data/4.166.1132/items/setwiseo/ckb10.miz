environ
vocabularies TARSKI,XBOOLE_0,FUNCT_1,RELAT_1,SUBSET_1,FINSUB_1,BINOP_1,FINSET_1,FUNCOP_1,SETWISEO;
notations TARSKI,XBOOLE_0,ENUMSET1,SUBSET_1,RELAT_1,FUNCT_1,FINSUB_1,RELSET_1,FINSET_1,PARTFUN1,FUNCT_2,BINOP_1,FUNCOP_1;
definitions BINOP_1,TARSKI,RELAT_1;
theorems FINSUB_1,ZFMISC_1,SUBSET_1,BINOP_1,TARSKI,FUNCT_1,FUNCT_2,FUNCOP_1,ENUMSET1,RELAT_1,XBOOLE_0,XBOOLE_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9;
schemes FINSET_1,FUNCT_2,BINOP_1,SUBSET_1;
registrations XBOOLE_0,SUBSET_1,RELSET_1,FUNCT_2,FINSET_1,FINSUB_1;
constructors ENUMSET1,PARTFUN1,BINOP_1,FUNCOP_1,FINSET_1,FINSUB_1,RELSET_1;
requirements BOOLE,SUBSET;
begin
reserve X for non empty set;
reserve Y for non empty set;
theorem
(for A being (Element of ( Fin X )) holds (for B being set holds (for f being (Function of X,Y) holds ((for x being (Element of X) holds (x in A implies ( f . x ) in B)) implies ( f .: A ) c= B))))
proof
let A being (Element of ( Fin X ));
let B being set;
let f being (Function of X,Y);
assume that
A1: (for x being (Element of X) holds (x in A implies ( f . x ) in B));
let x being set;
assume x in ( f .: A );
then consider y being set such that y in ( dom f ) and A2: y in A and A3: x = ( f . y ) by FUNCT_1:def 6;
reconsider y as (Element of X) by A2,CKB9:1;
x = ( f . y ) by A3;
hence thesis by A1,A2;
end;
