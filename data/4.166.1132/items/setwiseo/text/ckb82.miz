begin
reserve X for non empty set;
theorem
(for Y,Z being set holds (for f being (Function of X,( Fin Y )) holds (for g being (Function of ( Fin Y ),( Fin Z )) holds ((( g . ( {}. Y ) ) = ( {}. Z ) & (for x,y being (Element of ( Fin Y )) holds ( g . ( x \/ y ) ) = ( ( g . x ) \/ ( g . y ) ))) implies (for B being (Element of ( Fin X )) holds ( g . ( FinUnion (B,f) ) ) = ( FinUnion (B,( g * f )) ))))))
proof
let Y being set,Z being set;
let f being (Function of X,( Fin Y ));
let g being (Function of ( Fin Y ),( Fin Z ));
assume that
A1: ( g . ( {}. Y ) ) = ( {}. Z )
and
A2: (for x,y being (Element of ( Fin Y )) holds ( g . ( x \/ y ) ) = ( ( g . x ) \/ ( g . y ) ));
A3: ( g . ( {}. Y ) ) = ( the_unity_wrt ( FinUnion Z ) ) by A1,CKB63:1;
A4:now
let x being (Element of ( Fin Y )),y being (Element of ( Fin Y ));
thus ( g . ( x \/ y ) ) = ( ( g . x ) \/ ( g . y ) ) by A2
.= ( ( FinUnion Z ) . (( g . x ),( g . y )) ) by CKB56:def 1;
end;
let B being (Element of ( Fin X ));
A5: ( FinUnion Z ) is  idempotent by CKB57:1;
(( FinUnion Z ) is  associative & ( FinUnion Z ) is  commutative) by CKB58:1,CKB59:1;
hence thesis by A5,A3,A4,CKB61:1,CKB74:1;
end;
