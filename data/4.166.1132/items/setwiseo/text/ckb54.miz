begin
theorem
Th44: (for A,X,Y being non empty set holds (for F being (BinOp of A) holds ((((F is  idempotent & F is  commutative) & F is  associative) & F is  having_a_unity) implies (for B being (Element of ( Fin X )) holds (for f being (Function of X,Y) holds (for g being (Function of Y,A) holds ( F $$ (( f .: B ),g) ) = ( F $$ (B,( g * f )) )))))))
proof
let A being non empty set,X being non empty set,Y being non empty set;
let F being (BinOp of A);
assume that
A1: F is  idempotent
and
A2: (F is  commutative & F is  associative)
and
A3: F is  having_a_unity;
let B being (Element of ( Fin X ));
let f being (Function of X,Y);
let g being (Function of Y,A);
now
assume A4: B = ( {} );
then ( f .: B ) = ( {}. Y ) by RELAT_1:116;
then A5: ( F $$ (( f .: B ),g) ) = ( the_unity_wrt F ) by A2,A3,CKB50:1;
B = ( {}. X ) by A4;
hence thesis by A2,A3,A5,CKB50:1;
end;
hence thesis by A1,A2,CKB48:1;
end;
