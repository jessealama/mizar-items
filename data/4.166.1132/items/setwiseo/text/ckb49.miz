theorem
Th39: (((F is  commutative & F is  associative) & F is  idempotent) implies (for Z being non empty set holds (for G being (BinOp of Z) holds (((G is  commutative & G is  associative) & G is  idempotent) implies (for f being (Function of X,Y) holds (for g being (Function of Y,Z) holds ((for x,y being (Element of Y) holds ( g . ( F . (x,y) ) ) = ( G . (( g . x ),( g . y )) )) implies (for B being (Element of ( Fin X )) holds (B <> ( {} ) implies ( g . ( F $$ (B,f) ) ) = ( G $$ (B,( g * f )) ))))))))))
proof
assume that
A1: (F is  commutative & F is  associative)
and
A2: F is  idempotent;
let Z being non empty set;
let G being (BinOp of Z);
assume that
A3: (G is  commutative & G is  associative)
and
A4: G is  idempotent;
let f being (Function of X,Y);
let g being (Function of Y,Z);
assume that
A5: (for x,y being (Element of Y) holds ( g . ( F . (x,y) ) ) = ( G . (( g . x ),( g . y )) ));
defpred X[ (Element of ( Fin X )) ]
 means
($1 <> ( {} ) implies ( g . ( F $$ ($1,f) ) ) = ( G $$ ($1,( g * f )) ));
A6: (for B9 being (Element of ( Fin X )) holds (for b being (Element of X) holds (X[ B9 ] implies X[ ( B9 \/ {. b .} ) ])))
proof
let B being (Element of ( Fin X ));
let x being (Element of X);
assume that
A7: (B <> ( {} ) implies ( g . ( F $$ (B,f) ) ) = ( G $$ (B,( g * f )) ))
and
( B \/ { x } ) <> ( {} );
per cases ;
suppose A8: B = ( {} );

hence ( g . ( F $$ (( B \/ {. x .} ),f) ) ) = ( g . ( f . x ) ) by A1,CKB35:1
.= ( ( g * f ) . x ) by FUNCT_2:15
.= ( G $$ (( B \/ {. x .} ),( g * f )) ) by A3,A8,CKB35:1;
end;
suppose A9: B <> ( {} );

hence ( g . ( F $$ (( B \/ {. x .} ),f) ) ) = ( g . ( F . (( F $$ (B,f) ),( f . x )) ) ) by A1,A2,CKB38:1
.= ( G . (( g . ( F $$ (B,f) ) ),( g . ( f . x ) )) ) by A5
.= ( G . (( G $$ (B,( g * f )) ),( ( g * f ) . x )) ) by A7,A9,FUNCT_2:15
.= ( G $$ (( B \/ {. x .} ),( g * f )) ) by A3,A4,A9,CKB38:1;
end;
end;
A10: X[ ( {}. X ) ];
thus (for B being (Element of ( Fin X )) holds X[ B ]) from CKB32:sch 1(A10,A6);
end;
