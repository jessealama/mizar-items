begin
reserve p for (Element of ( NAT ));
theorem
Th13: (for G being  strict Group holds (for a being (Element of G) holds ((G is  finite & G = ( gr { a } )) implies (for G1 being  strict (Subgroup of G) holds (ex p st G1 = ( gr { ( a |^ p ) } ))))))
proof
let G being  strict Group;
let a being (Element of G);
assume that
A1: G is  finite
and
A2: G = ( gr { a } );
let G1 being  strict (Subgroup of G);
G is  cyclic  cyclic Group by A2,GR_CY_1:def 7;
then G1 is  cyclic  cyclic Group by A1,GR_CY_1:20;
then consider b being (Element of G1) such that A3: G1 = ( gr { b } ) by GR_CY_1:def 7;
reconsider b1 = b as (Element of G) by GROUP_2:42;
consider p such that A4: b1 = ( a |^ p ) by A1,A2,CKB6:1;
take p;
thus thesis by A3,A4,CKB3:1;
end;
