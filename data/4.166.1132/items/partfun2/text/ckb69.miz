begin
reserve x for set;
reserve Y for set;
theorem
(for Y holds (for f,g being Y -valued Function holds (f c= g implies (for x holds (x in ( dom f ) implies ( f /. x ) = ( g /. x ))))))
proof
let Y;
let f being Y -valued Function,g being Y -valued Function;
assume A1: f c= g;
then A2: ( dom f ) c= ( dom g ) by GRFUNC_1:2;
let x;
assume A3: x in ( dom f );
then ( f . x ) = ( g . x ) by A1,GRFUNC_1:2;
then ( f /. x ) = ( (g qua Function) . x ) by A3,PARTFUN1:def 6;
hence thesis by A2,A3,PARTFUN1:def 6;
end;
