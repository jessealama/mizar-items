begin
theorem
Th9: (for I being non empty set holds (for J being  TopSpace-yielding  non-Empty (ManySortedSet of I) holds (for i being (Element of I) holds (for xi being (Element of ( J . i )) holds (for Ai being (Subset of ( J . i )) holds (( ( proj (J,i) ) " { xi } ) meets ( ( proj (J,i) ) " Ai ) implies xi in Ai))))))
proof
let I being non empty set;
let J being  TopSpace-yielding  non-Empty (ManySortedSet of I);
let i being (Element of I);
let xi being (Element of ( J . i ));
let Ai being (Subset of ( J . i ));
assume ( ( ( proj (J,i) ) " { xi } ) /\ ( ( proj (J,i) ) " Ai ) ) <> ( {} );
then ( ( ( proj (( Carrier J ),i) ) " { xi } ) /\ ( ( proj (J,i) ) " Ai ) ) <> ( {} ) by WAYBEL18:def 4;
then ( ( ( proj (( Carrier J ),i) ) " { xi } ) /\ ( ( proj (( Carrier J ),i) ) " Ai ) ) <> ( {} ) by WAYBEL18:def 4;
then A1: ( ( proj (( Carrier J ),i) ) " { xi } ) meets ( ( proj (( Carrier J ),i) ) " Ai ) by XBOOLE_0:def 7;
Ai c= (the carrier of ( J . i ));
then Ai c= ( ( Carrier J ) . i ) by YELLOW_6:2;
hence thesis by A1,CKB1:1;
end;
