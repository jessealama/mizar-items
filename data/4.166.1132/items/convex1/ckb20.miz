begin
theorem
Lm6: (for V being non empty RLSStruct holds (for M,N being (Subset of V) holds (for r being Real holds (M c= N implies ( r * M ) c= ( r * N )))))
proof
let V being non empty RLSStruct;
let M being (Subset of V),N being (Subset of V);
let r being Real;
assume A1: M c= N;
(for x being (VECTOR of V) holds (x in ( r * M ) implies x in ( r * N )))
proof
let x being (VECTOR of V);
assume x in ( r * M );
then (ex w being (VECTOR of V) st (x = ( r * w ) & w in M));
hence thesis by A1;
end;
hence thesis by SUBSET_1:2;
end;
