environ
vocabularies STRUCT_0,GROUP_1,GROUP_2,SUBSET_1,GROUP_6,NEWTON,PRE_TOPC,RELAT_1,TARSKI,FUNCT_2,FUNCT_1,XBOOLE_0,BINOP_1,ALGSTR_0,ZFMISC_1,REALSET1,GROUP_5,WELLORD1,AUTGROUP;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,REALSET1,RELAT_1,FUNCT_1,RELSET_1,PARTFUN1,FUNCT_2,STRUCT_0,ALGSTR_0,BINOP_1,GROUP_1,GROUP_2,GROUP_3,GROUP_5,GROUP_6,CKB4;
definitions FUNCT_1,GROUP_1,GROUP_6,TARSKI,BINOP_1,REALSET1,GROUP_2,GROUP_3,ALGSTR_0,FUNCT_2,CKB4;
theorems BINOP_1,FUNCT_1,FUNCT_2,GROUP_1,GROUP_2,GROUP_3,GROUP_5,GROUP_6,REALSET1,RELAT_1,ZFMISC_1,TARSKI,XBOOLE_0,STRUCT_0,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11;
schemes BINOP_1,FUNCT_2;
registrations XBOOLE_0,FUNCT_1,RELSET_1,FUNCT_2,STRUCT_0,GROUP_1,GROUP_2,GROUP_3,GROUP_6,GR_CY_2;
constructors PARTFUN1,BINOP_1,REALSET1,GROUP_5,GROUP_6,RELSET_1,CKB4;
requirements SUBSET,BOOLE;
begin
reserve G for  strict Group;
definition
let G;
func AutComp G -> (BinOp of ( Aut G ))
means
:Def2: (for x,y being (Element of ( Aut G )) holds ( it . (x,y) ) = ( x * y ));
existence
proof
defpred P[ (Element of ( Aut G )),(Element of ( Aut G )),set ]
 means
$3 = ( $1 * $2 );
A1: (for x,y being (Element of ( Aut G )) holds (ex m being (Element of ( Aut G )) st P[ x,y,m ]))
proof
let x being (Element of ( Aut G )),y being (Element of ( Aut G ));
reconsider xx = x,yy = y as (Homomorphism of G,G) by CKB4:def 1;
reconsider m = ( xx * yy ) as (Element of ( Aut G )) by CKB11:1;
take m;
thus thesis;
end;
thus (ex M being (BinOp of ( Aut G )) st (for x,y being (Element of ( Aut G )) holds P[ x,y,( M . (x,y) ) ])) from BINOP_1:sch 3(A1);
end;
uniqueness
proof
let b1 being (BinOp of ( Aut G )),b2 being (BinOp of ( Aut G ));
assume that
A2: (for x,y being (Element of ( Aut G )) holds ( b1 . (x,y) ) = ( x * y ))
and
A3: (for x,y being (Element of ( Aut G )) holds ( b2 . (x,y) ) = ( x * y ));
(for x,y being (Element of ( Aut G )) holds ( b1 . (x,y) ) = ( b2 . (x,y) ))
proof
let x being (Element of ( Aut G )),y being (Element of ( Aut G ));
thus ( b1 . (x,y) ) = ( x * y ) by A2
.= ( b2 . (x,y) ) by A3;
end;
hence thesis by BINOP_1:2;
end;
end;
