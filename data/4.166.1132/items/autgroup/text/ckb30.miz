begin
reserve G for  strict Group;
reserve a for (Element of G);
reserve b for (Element of G);
reserve c for (Element of G);
theorem
Th23: (for a holds (for b holds ( Conjugate ( a * b ) ) = ( ( Conjugate b ) * ( Conjugate a ) )))
proof
let a;
let b;
set f1 = ( Conjugate ( a * b ) );
set f2 = ( ( Conjugate b ) * ( Conjugate a ) );
A1: (for c holds ( f1 . c ) = ( ( c |^ a ) |^ b ))
proof
let c;
( c |^ ( a * b ) ) = ( ( c |^ a ) |^ b ) by GROUP_3:24;
hence thesis by CKB29:def 1;
end;
A2: (for c holds ( f1 . c ) = ( ( Conjugate b ) . ( c |^ a ) ))
proof
let c;
( ( c |^ a ) |^ b ) = ( ( Conjugate b ) . ( c |^ a ) ) by CKB29:def 1;
hence thesis by A1;
end;
A3: (for c holds ( f1 . c ) = ( ( Conjugate b ) . ( ( Conjugate a ) . c ) ))
proof
let c;
( ( Conjugate b ) . ( c |^ a ) ) = ( ( Conjugate b ) . ( ( Conjugate a ) . c ) ) by CKB29:def 1;
hence thesis by A2;
end;
(for c holds ( f1 . c ) = ( f2 . c ))
proof
let c;
( ( Conjugate b ) . ( ( Conjugate a ) . c ) ) = ( f2 . c ) by FUNCT_2:15;
hence thesis by A3;
end;
hence thesis by FUNCT_2:63;
end;
