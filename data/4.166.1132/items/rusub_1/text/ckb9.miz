begin
theorem
(for V being RealUnitarySpace holds (for W being (Subspace of V) holds (for v1,v2 being (VECTOR of V) holds (for w1,w2 being (VECTOR of W) holds ((w1 = v1 & w2 = v2) implies ( w1 .|. w2 ) = ( v1 .|. v2 ))))))
proof
let V being RealUnitarySpace;
let W being (Subspace of V);
let v1 being (VECTOR of V),v2 being (VECTOR of V);
let w1 being (VECTOR of W),w2 being (VECTOR of W);
reconsider ww1 = w1,ww2 = w2 as (VECTOR of V) by CKB4:1;
assume (w1 = v1 & w2 = v2);
then A1: ( v1 .|. v2 ) = ( (the scalar of V) . [ ww1,ww2 ] ) by BHSP_1:def 1;
( w1 .|. w2 ) = ( (the scalar of W) . [ w1,w2 ] ) by BHSP_1:def 1
.= ( ( (the scalar of V) || (the carrier of W) ) . [ w1,w2 ] ) by CKB1:def 1;
hence thesis by A1,FUNCT_1:49;
end;
