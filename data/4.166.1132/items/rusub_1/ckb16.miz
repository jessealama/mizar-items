begin
theorem
Th14: (for V being RealUnitarySpace holds (for W being (Subspace of V) holds (for u,v being (VECTOR of V) holds ((u in W & v in W) implies ( u + v ) in W))))
proof
let V being RealUnitarySpace;
let W being (Subspace of V);
reconsider VW = (the carrier of W) as (Subset of V) by CKB1:def 1;
let u being (VECTOR of V),v being (VECTOR of V);
assume (u in W & v in W);
then A1: (u in (the carrier of W) & v in (the carrier of W)) by STRUCT_0:def 5;
VW is  linearly-closed by CKB15:1;
then ( u + v ) in (the carrier of W) by A1,RLSUB_1:def 1;
hence thesis by STRUCT_0:def 5;
end;
