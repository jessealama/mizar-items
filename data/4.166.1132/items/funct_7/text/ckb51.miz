begin
theorem
(for f being Function holds (for d,e,i being set holds ( ( f +* (i,d) ) +* (i,e) ) = ( f +* (i,e) )))
proof
let f being Function;
let d being set,e being set,i being set;
A1: ( dom ( i .--> d ) ) = { i } by FUNCOP_1:13
.= ( dom ( i .--> e ) ) by FUNCOP_1:13;
per cases ;
suppose A2: i in ( dom f );

then i in ( dom ( f +* (i,d) ) ) by CKB47:1;
hence ( ( f +* (i,d) ) +* (i,e) ) = ( ( f +* (i,d) ) +* ( i .--> e ) ) by CKB46:def 1
.= ( ( f +* ( i .--> d ) ) +* ( i .--> e ) ) by A2,CKB46:def 1
.= ( f +* ( ( i .--> d ) +* ( i .--> e ) ) ) by FUNCT_4:14
.= ( f +* ( i .--> e ) ) by A1,FUNCT_4:19
.= ( f +* (i,e) ) by A2,CKB46:def 1;
end;
suppose (not i in ( dom f ));

hence thesis by CKB46:def 1;
end;
end;
