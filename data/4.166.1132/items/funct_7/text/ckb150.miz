begin
reserve f for Function;
reserve g for Function;
reserve h for Function;
reserve x for set;
theorem
(for f,g,h being Function holds (( f +* h ) = ( g +* h ) iff f,g equal_outside ( dom h )))
proof
let f being Function;
let g being Function;
let h being Function;
thus (( f +* h ) = ( g +* h ) implies f,g equal_outside ( dom h ))
proof
assume ( f +* h ) = ( g +* h );
then A1: ( f +* h ),g equal_outside ( dom h ) by CKB42:1,CKB45:1;
f,( f +* h ) equal_outside ( dom h ) by CKB45:1;
hence thesis by A1,CKB43:1;
end;

assume A2: f,g equal_outside ( dom h );
then A3: ( ( dom f ) \ ( dom h ) ) = ( ( dom g ) \ ( dom h ) ) by CKB44:1;
A4: (for x being set holds (x in ( dom ( f +* h ) ) implies ( ( f +* h ) . x ) = ( ( g +* h ) . x )))
proof
let x being set;
assume A5: x in ( dom ( f +* h ) );
per cases ;
suppose A6: x in ( dom h );

hence ( ( f +* h ) . x ) = ( h . x ) by FUNCT_4:13
.= ( ( g +* h ) . x ) by A6,FUNCT_4:13;
end;
suppose A7: (not x in ( dom h ));

( dom ( f +* h ) ) = ( ( dom f ) \/ ( dom h ) ) by FUNCT_4:def 1;
then x in ( dom f ) by A5,A7,XBOOLE_0:def 3;
then A8: x in ( ( dom f ) \ ( dom h ) ) by A7,XBOOLE_0:def 5;
A9: ( f | ( ( dom f ) \ ( dom h ) ) ) = ( g | ( ( dom g ) \ ( dom h ) ) ) by A2,CKB40:def 1;
thus ( ( f +* h ) . x ) = ( f . x ) by A7,FUNCT_4:11
.= ( ( g | ( ( dom g ) \ ( dom h ) ) ) . x ) by A9,A8,FUNCT_1:49
.= ( g . x ) by A3,A8,FUNCT_1:49
.= ( ( g +* h ) . x ) by A7,FUNCT_4:11;
end;
end;
( dom ( f +* h ) ) = ( ( dom f ) \/ ( dom h ) ) by FUNCT_4:def 1
.= ( ( ( dom g ) \ ( dom h ) ) \/ ( dom h ) ) by A3,XBOOLE_1:39
.= ( ( dom g ) \/ ( dom h ) ) by XBOOLE_1:39
.= ( dom ( g +* h ) ) by FUNCT_4:def 1;
hence thesis by A4,FUNCT_1:2;
end;
