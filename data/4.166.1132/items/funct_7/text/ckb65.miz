begin
reserve x for set;
reserve p for  Function-yielding FinSequence;
reserve f for Function;
reserve g for Function;
theorem
( apply (( <* f *> ^ p ),x) ) = ( <* x *> ^ ( apply (p,( f . x )) ) )
proof
defpred P[  Function-yielding FinSequence ]
 means
( apply (( <* f *> ^ $1 ),x) ) = ( <* x *> ^ ( apply ($1,( f . x )) ) );
A1: ( len ( {} ) ) = ( 0 );
A2: (for p being  Function-yielding FinSequence holds (P[ p ] implies (for f being Function holds P[ ( p ^ <* f *> ) ])))
proof
let p;
assume that
A3: ( apply (( <* f *> ^ p ),x) ) = ( <* x *> ^ ( apply (p,( f . x )) ) );
let g being Function;
set p9 = ( <* f *> ^ p );
A4: ( len ( apply (p,( f . x )) ) ) = ( ( len p ) + 1 ) by CKB59:def 1;
( len <* f *> ) = 1 by FINSEQ_1:40;
then A5: ( len p9 ) = ( ( len p ) + 1 ) by FINSEQ_1:22;
then ( len p9 ) >= 1 by NAT_1:11;
then (( len <* x *> ) = 1 & ( len p9 ) in ( dom ( apply (p,( f . x )) ) )) by A4,A5,FINSEQ_1:40,FINSEQ_3:25;
then A6: ( ( apply (p9,x) ) . ( 1 + ( len p9 ) ) ) = ( ( apply (p,( f . x )) ) . ( ( len p ) + 1 ) ) by A3,A5,FINSEQ_1:def 7;
( apply (( p9 ^ <* g *> ),x) ) = ( ( apply (p9,x) ) ^ <* ( g . ( ( apply (p9,x) ) . ( ( len p9 ) + 1 ) ) ) *> ) by CKB63:1;
hence ( apply (( <* f *> ^ ( p ^ <* g *> ) ),x) ) = ( ( <* x *> ^ ( apply (p,( f . x )) ) ) ^ <* ( g . ( ( apply (p,( f . x )) ) . ( ( len p ) + 1 ) ) ) *> ) by A3,A6,FINSEQ_1:32
.= ( <* x *> ^ ( ( apply (p,( f . x )) ) ^ <* ( g . ( ( apply (p,( f . x )) ) . ( ( len p ) + 1 ) ) ) *> ) ) by FINSEQ_1:32
.= ( <* x *> ^ ( apply (( p ^ <* g *> ),( f . x )) ) ) by CKB63:1;
end;
(( <* f *> ^ ( {} ) ) = <* f *> & ( ( {} ) ^ <* f *> ) = <* f *>) by FINSEQ_1:34;
then ( apply (( <* f *> ^ ( {} ) ),x) ) = ( ( apply (( {} ),x) ) ^ <* ( f . ( ( apply (( {} ),x) ) . ( ( 0 ) + 1 ) ) ) *> ) by A1,CKB63:1
.= ( <* x *> ^ <* ( f . ( ( apply (( {} ),x) ) . 1 ) ) *> ) by CKB61:1
.= ( <* x *> ^ <* ( f . ( <* x *> . 1 ) ) *> ) by CKB61:1
.= ( <* x *> ^ <* ( f . x ) *> ) by FINSEQ_1:40
.= ( <* x *> ^ ( apply (( {} ),( f . x )) ) ) by CKB61:1;
then A7: P[ ( {} ) ];
(for p holds P[ p ]) from CKB37:sch 1(A7,A2);
hence thesis;
end;
