theorem
Th25: (for p,q being FinSequence holds (( p ^ q ) is  Function-yielding implies (p is  Function-yielding & q is  Function-yielding)))
proof
let p being FinSequence,q being FinSequence;
assume A1: (for x being set holds (x in ( dom ( p ^ q ) ) implies ( ( p ^ q ) . x ) is Function));
hereby
let x being set;
assume A2: x in ( dom p );
then ( ( p ^ q ) . x ) = ( p . x ) by FINSEQ_1:def 7;
hence ( p . x ) is Function by A1,A2,FINSEQ_3:22;
end;
let x being set;
assume A3: x in ( dom q );
then reconsider i = x as (Element of ( NAT ));
( ( p ^ q ) . ( ( len p ) + i ) ) = ( q . x ) by A3,FINSEQ_1:def 7;
hence thesis by A1,A3,FINSEQ_1:28;
end;
