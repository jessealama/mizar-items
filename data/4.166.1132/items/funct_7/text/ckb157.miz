begin
theorem
(for f,g being (FinSequence of ( INT )) holds (for m,n being (Element of ( NAT )) holds (((((1 <= n & n <= ( len f )) & 1 <= m) & m <= ( len f )) & g = ( ( f +* (m,( f /. n )) ) +* (n,( f /. m )) )) implies (((( f . m ) = ( g . n ) & ( f . n ) = ( g . m )) & (for k being set holds (((k <> m & k <> n) & k in ( dom f )) implies ( f . k ) = ( g . k )))) & f,g are_fiberwise_equipotent ))))
proof
let f being (FinSequence of ( INT )),g being (FinSequence of ( INT ));
let m being (Element of ( NAT )),n being (Element of ( NAT ));
assume that
A1: (1 <= n & n <= ( len f ))
and
A2: (1 <= m & m <= ( len f ))
and
A3: g = ( ( f +* (m,( f /. n )) ) +* (n,( f /. m )) );
A4: ( dom ( f +* (m,( f /. n )) ) ) = ( dom f ) by CKB47:1;
A5: n in ( dom f ) by A1,FINSEQ_3:25;
hence A6: ( g . n ) = ( f /. m ) by A3,A4,CKB48:1
.= ( f . m ) by A2,FINSEQ_4:15;
A7: m in ( dom f ) by A2,FINSEQ_3:25;
thus A8:now
per cases ;
suppose m = n;

hence ( g . m ) = ( f . n ) by A6;
end;
suppose m <> n;

hence ( g . m ) = ( ( f +* (m,( f /. n )) ) . m ) by A3,CKB49:1
.= ( f /. n ) by A7,CKB48:1
.= ( f . n ) by A1,FINSEQ_4:15;
end;
end;
A9:now
let k being set;
assume that
A10: k <> m
and
A11: k <> n
and
k in ( dom f );
thus ( g . k ) = ( ( f +* (m,( f /. n )) ) . k ) by A3,A11,CKB49:1
.= ( f . k ) by A10,CKB49:1;
end;
( dom g ) = ( dom f ) by A3,A4,CKB47:1;
hence thesis by A5,A7,A6,A8,A9,RFINSEQ:28;
end;
