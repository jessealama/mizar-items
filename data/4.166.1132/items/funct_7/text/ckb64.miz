begin
reserve X for set;
reserve p for  Function-yielding FinSequence;
reserve f for Function;
reserve g for Function;
theorem
( compose (( <* f *> ^ p ),X) ) = ( ( compose (p,( f .: X )) ) * ( f | X ) )
proof
defpred R[  Function-yielding FinSequence ]
 means
( compose (( <* f *> ^ $1 ),X) ) = ( ( compose ($1,( f .: X )) ) * ( f | X ) );
A1: (for p being  Function-yielding FinSequence holds (R[ p ] implies (for f being Function holds R[ ( p ^ <* f *> ) ])))
proof
let p being  Function-yielding FinSequence;
assume that
A2: ( compose (( <* f *> ^ p ),X) ) = ( ( compose (p,( f .: X )) ) * ( f | X ) );
let g being Function;
thus ( compose (( <* f *> ^ ( p ^ <* g *> ) ),X) ) = ( compose (( ( <* f *> ^ p ) ^ <* g *> ),X) ) by FINSEQ_1:32
.= ( g * ( compose (( <* f *> ^ p ),X) ) ) by CKB62:1
.= ( ( g * ( compose (p,( f .: X )) ) ) * ( f | X ) ) by A2,RELAT_1:36
.= ( ( compose (( p ^ <* g *> ),( f .: X )) ) * ( f | X ) ) by CKB62:1;
end;
(( <* f *> ^ ( {} ) ) = <* f *> & ( ( {} ) ^ <* f *> ) = <* f *>) by FINSEQ_1:34;
then ( compose (( <* f *> ^ ( {} ) ),X) ) = ( f * ( compose (( {} ),X) ) ) by CKB62:1
.= ( f * ( id X ) ) by CKB60:1
.= ( f | X ) by RELAT_1:65
.= ( ( id ( rng ( f | X ) ) ) * ( f | X ) ) by RELAT_1:54
.= ( ( id ( f .: X ) ) * ( f | X ) ) by RELAT_1:115
.= ( ( compose (( {} ),( f .: X )) ) * ( f | X ) ) by CKB60:1;
then A3: R[ ( {} ) ];
(for p holds R[ p ]) from CKB37:sch 1(A3,A1);
hence thesis;
end;
