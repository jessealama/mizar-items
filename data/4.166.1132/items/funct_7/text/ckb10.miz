begin
theorem
(for I,J being set holds (for M being (ManySortedSet of [: I,J :]) holds (for i,j being set holds ((i in I & j in J) implies ( (i,j) :-> ( M . (i,j) ) ) = ( M | ([: { i },{ j } :] qua set) )))))
proof
let I being set;
let J being set;
let M being (ManySortedSet of [: I,J :]);
let i being set;
let j being set;
assume (i in I & j in J);
then A1: [ i,j ] in [: I,J :] by ZFMISC_1:87;
thus ( (i,j) :-> ( M . (i,j) ) ) = ( [ i,j ] .--> ( M . [ i,j ] ) )
.= ( M | ({ [ i,j ] } qua set) ) by A1,CKB9:1
.= ( M | ([: { i },{ j } :] qua set) ) by ZFMISC_1:29;
end;
