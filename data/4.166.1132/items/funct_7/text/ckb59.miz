begin
reserve x for set;
definition
let p being  Function-yielding FinSequence;
let x being set;
func apply (p,x) -> FinSequence means 
:Def5: ((( len it ) = ( ( len p ) + 1 ) & ( it . 1 ) = x) & (for i being (Element of ( NAT )) holds (for f being Function holds ((i in ( dom p ) & f = ( p . i )) implies ( it . ( i + 1 ) ) = ( f . ( it . i ) )))));
existence
proof
defpred P[ (Element of ( NAT )),set,set ]
 means
(ex f being Function st (f = ( p . $1 ) & $3 = ( f . $2 )));
A1: (for i being (Element of ( NAT )) holds ((1 <= i & i < ( ( len p ) + 1 )) implies (for x being set holds (ex y being set st P[ i,x,y ]))))
proof
let i being (Element of ( NAT ));
assume 1 <= i;
assume i < ( ( len p ) + 1 );
reconsider f = ( p . i ) as Function;
let x being set;
take ( f . x );
take f;
thus thesis;
end;
consider q being FinSequence such that A2: (( len q ) = ( ( len p ) + 1 ) & (( q . 1 ) = x or ( ( len p ) + 1 ) = ( 0 ))) and A3: (for i being (Element of ( NAT )) holds ((1 <= i & i < ( ( len p ) + 1 )) implies P[ i,( q . i ),( q . ( i + 1 ) ) ])) from RECDEF_1:sch 3(A1);
take q;
thus (( len q ) = ( ( len p ) + 1 ) & ( q . 1 ) = x) by A2;
let i being (Element of ( NAT ));
let f being Function;
assume A4: i in ( dom p );
then i <= ( len p ) by FINSEQ_3:25;
then A5: i < ( ( len p ) + 1 ) by NAT_1:13;
i >= 1 by A4,FINSEQ_3:25;
then (ex f being Function st (f = ( p . i ) & ( q . ( i + 1 ) ) = ( f . ( q . i ) ))) by A3,A5;
hence thesis;
end;
correctness
proof
let q1 being FinSequence;
let q2 being FinSequence;
assume that
A6: ( len q1 ) = ( ( len p ) + 1 )
and
A7: ( q1 . 1 ) = x
and
A8: (for i being (Element of ( NAT )) holds (for f being Function holds ((i in ( dom p ) & f = ( p . i )) implies ( q1 . ( i + 1 ) ) = ( f . ( q1 . i ) ))))
and
A9: ( len q2 ) = ( ( len p ) + 1 )
and
A10: ( q2 . 1 ) = x
and
A11: (for i being (Element of ( NAT )) holds (for f being Function holds ((i in ( dom p ) & f = ( p . i )) implies ( q2 . ( i + 1 ) ) = ( f . ( q2 . i ) ))));
defpred P[ Nat ]
 means
($1 in ( dom q1 ) implies ( q1 . $1 ) = ( q2 . $1 ));
A12: (for i being Nat holds (P[ i ] implies P[ ( i + 1 ) ]))
proof
let i being Nat;
assume that
A13: (i in ( dom q1 ) implies ( q1 . i ) = ( q2 . i ))
and
A14: ( i + 1 ) in ( dom q1 );
( i + 1 ) <= ( len q1 ) by A14,FINSEQ_3:25;
then A15: i <= ( len p ) by A6,XREAL_1:6;
per cases ;
suppose i = ( 0 );

hence thesis by A7,A10;
end;
suppose A16: i > ( 0 );

reconsider f = ( p . i ) as Function;
i >= ( ( 0 ) + 1 ) by A16,NAT_1:13;
then A17: i in ( dom p ) by A15,FINSEQ_3:25;
hence ( q1 . ( i + 1 ) ) = ( f . ( q2 . i ) ) by A6,A8,A13,CKB24:1
.= ( q2 . ( i + 1 ) ) by A11,A17;
end;
end;
A18: P[ ( 0 ) ] by FINSEQ_3:25;
A19: (for i being Nat holds P[ i ]) from NAT_1:sch 2(A18,A12);
( dom q1 ) = ( dom q2 ) by A6,A9,FINSEQ_3:29;
hence thesis by A19,FINSEQ_1:13;
end;
end;
