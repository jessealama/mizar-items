theorem
(for f,g being Function holds (for x,y being set holds ((g c= f & (not x in ( dom g ))) implies g c= ( f +* (x,y) ))))
proof
let f being Function;
let g being Function;
let x being set;
let y being set;
assume that
A1: g c= f
and
A2: (not x in ( dom g ));
A3:now
let z being set;
assume A4: z in ( dom g );
hence ( g . z ) = ( f . z ) by A1,GRFUNC_1:2
.= ( ( f +* (x,y) ) . z ) by A2,A4,CKB49:1;
end;
( dom g ) c= ( dom f ) by A1,GRFUNC_1:2;
then ( dom g ) c= ( dom ( f +* (x,y) ) ) by CKB47:1;
hence thesis by A3,GRFUNC_1:2;
end;
