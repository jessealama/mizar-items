begin
reserve x for set;
reserve A for set;
reserve l for (Element of ( NAT ));
reserve m for (Element of ( NAT ));
reserve n for (Element of ( NAT ));
theorem
(for A being (Subset of ( NAT )) holds ((for n holds (for m holds ((n in A & m < n) implies m in A))) implies A is Cardinal))
proof
let A being (Subset of ( NAT ));
assume that
A1: (for n holds (for m holds ((n in A & m < n) implies m in A)));
per cases ;
suppose A = ( {} );

hence thesis;
end;
suppose that A2: A <> ( {} )
and
A3: (ex m being Nat st (for n being Nat holds (n in A implies n <= m)));
defpred P[ set ]
 means
$1 in A;
consider M being Nat such that A4: (for n being Nat holds (P[ n ] implies n <= M)) by A3;
(ex m being (Element of ( NAT )) st P[ m ]) by A2,SUBSET_1:4;
then A5: (ex m being Nat st P[ m ]);
consider m being Nat such that A6: P[ m ] and A7: (for n being Nat holds (P[ n ] implies n <= m)) from NAT_1:sch 6(A4,A5);
A = { l: l < ( m + 1 ) }
proof
thus A c= { l: l < ( m + 1 ) }
proof
let x being set;
assume A8: x in A;
then reconsider l = x as (Element of ( NAT ));
l <= m by A7,A8;
then l < ( m + 1 ) by NAT_1:13;
hence thesis;
end;

let x being set;
assume x in { l: l < ( m + 1 ) };
then consider l being (Element of ( NAT )) such that A9: x = l and A10: l < ( m + 1 );
l <= m by A10,NAT_1:13;
then (l < m or l = m) by XXREAL_0:1;
hence thesis by A1,A6,A9;
end;
hence thesis by AXIOMS:4;
end;
suppose A11: (for m being Nat holds (ex n being Nat st (n in A & n > m)));

( NAT ) c= A
proof
let x being set;
assume x in ( NAT );
then reconsider m = x as (Element of ( NAT ));
(ex n being Nat st (n in A & n > m)) by A11;
hence thesis by A1;
end;
hence thesis by XBOOLE_0:def 10;
end;
end;
