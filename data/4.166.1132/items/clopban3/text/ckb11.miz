theorem
Th10: (for X being ComplexNormSpace holds (for seq,seq1 being (sequence of X) holds ((seq1 is (subsequence of seq) & seq is  convergent) implies seq1 is  convergent)))
proof
let X being ComplexNormSpace;
let seq being (sequence of X),seq1 being (sequence of X);
assume that
A1: seq1 is (subsequence of seq)
and
A2: seq is  convergent;
consider g1 being (Element of X) such that A3: (for p being Real holds (( 0 ) < p implies (ex n being (Element of ( NAT )) st (for m being (Element of ( NAT )) holds (n <= m implies ||. ( ( seq . m ) - g1 ) .|| < p))))) by A2,CLVECT_1:def 15;
take g1;
let p being Real;
assume ( 0 ) < p;
then consider n1 being (Element of ( NAT )) such that A4: (for m being (Element of ( NAT )) holds (n1 <= m implies ||. ( ( seq . m ) - g1 ) .|| < p)) by A3;
take n = n1;
let m being (Element of ( NAT ));
assume that
A5: n <= m;
consider Nseq being  increasing (sequence of ( NAT )) such that A6: seq1 = ( seq * Nseq ) by A1,VALUED_0:def 17;
m <= ( Nseq . m ) by SEQM_3:14;
then A7: n <= ( Nseq . m ) by A5,XXREAL_0:2;
( seq1 . m ) = ( seq . ( Nseq . m ) ) by A6,FUNCT_2:15;
hence thesis by A4,A7;
end;
