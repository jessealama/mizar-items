let X being ComplexNormSpace;
let seq being (sequence of X);
let rseq1 being Real_Sequence;
assume (||. seq .|| is  non-increasing & (for n being (Element of ( NAT )) holds ( rseq1 . n ) = ( ( 2 to_power n ) * ( ||. seq .|| . ( 2 to_power n ) ) )));
