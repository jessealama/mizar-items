proof
let X being ComplexNormSpace;
let seq being (sequence of X);
A1:now
assume A2: (for p being Real holds (p > ( 0 ) implies (ex n being (Element of ( NAT )) st (for m being (Element of ( NAT )) holds (n <= m implies ||. ( ( seq . m ) - ( seq . n ) ) .|| < p)))));
