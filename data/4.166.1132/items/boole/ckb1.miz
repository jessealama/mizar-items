begin
theorem
(for X being set holds ( X \/ ( {} ) ) = X)
proof
let X being set;
thus ( X \/ ( {} ) ) c= X
proof
let x being set;
assume x in ( X \/ ( {} ) );
then (x in X or x in ( {} )) by XBOOLE_0:def 3;
hence thesis by XBOOLE_0:def 1;
end;

let x being set;
assume x in X;
hence thesis by XBOOLE_0:def 3;
end;
