begin
reserve x for set;
reserve X for set;
theorem
Lm1: (X is  empty implies X = ( {} ))
proof
assume (not (ex x st x in X));
then (x in ( {} ) iff x in X) by XBOOLE_0:def 1;
hence thesis by TARSKI:1;
end;
