begin
reserve G for AddGroup;
theorem
Th34: (((( dom ( ID G ) ) = G & ( cod ( ID G ) ) = G) & (for f being  strict GroupMorphism holds (( cod f ) = G implies ( ( ID G ) * f ) = f))) & (for g being  strict GroupMorphism holds (( dom g ) = G implies ( g * ( ID G ) ) = g)))
proof
set i = ( ID G );
thus ( dom i ) = G;
thus ( cod i ) = G;
thus (for f being  strict GroupMorphism holds (( cod f ) = G implies ( i * f ) = f))
proof
let f being  strict GroupMorphism;
assume that
A1: ( cod f ) = G;
set H = ( dom f );
reconsider f9 = f as (Morphism of H,G) by A1,CKB34:def 1;
consider m being (Function of H,G) such that A2: f9 = GroupMorphismStr (# H,G,m #) by CKB43:1;
(( dom i ) = G & ( ( id G ) * m ) = m) by FUNCT_2:17;
hence thesis by A1,A2,CKB47:def 1;
end;

thus (for g being  strict GroupMorphism holds (( dom g ) = G implies ( g * ( ID G ) ) = g))
proof
let f being  strict GroupMorphism;
assume that
A3: ( dom f ) = G;
set H = ( cod f );
reconsider f9 = f as (Morphism of G,H) by A3,CKB34:def 1;
consider m being (Function of G,H) such that A4: f9 = GroupMorphismStr (# G,H,m #) by CKB43:1;
(( cod i ) = G & ( m * ( id G ) ) = m) by FUNCT_2:17;
hence thesis by A3,A4,CKB47:def 1;
end;

end;
