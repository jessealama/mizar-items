requirements SUBSET,BOOLE;
begin
reserve x for set;
reserve y for set;
theorem
Th14: (for G1,G2,G3 being non  empty addMagma holds (for f being (Function of G1,G2) holds (for g being (Function of G2,G3) holds ((f is  additive & g is  additive) implies ( g * f ) is  additive))))
proof
let G1 being non  empty addMagma;
let G2 being non  empty addMagma;
let G3 being non  empty addMagma;
let f being (Function of G1,G2);
let g being (Function of G2,G3);
assume that
A1: f is  additive
and
A2: g is  additive;
set h = ( g * f );
now
let x being (Element of G1);
let y being (Element of G1);
A3: (( g . ( f . x ) ) = ( h . x ) & ( g . ( f . y ) ) = ( h . y )) by FUNCT_2:15;
thus ( h . ( x + y ) ) = ( g . ( f . ( x + y ) ) ) by FUNCT_2:15
.= ( g . ( ( f . x ) + ( f . y ) ) ) by A1,CKB16:def 1
.= ( ( h . x ) + ( h . y ) ) by A2,A3,CKB16:def 1;
end;
hence thesis by CKB16:def 1;
end;
