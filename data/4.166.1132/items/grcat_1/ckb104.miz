begin
reserve UN for Universe;
theorem
Lm3: (for b being (Object of ( GroupCat UN )) holds (((( dom ( id b ) ) = b & ( cod ( id b ) ) = b) & (for f being (Morphism of ( GroupCat UN )) holds (( cod f ) = b implies ( ( id b ) * f ) = f))) & (for g being (Morphism of ( GroupCat UN )) holds (( dom g ) = b implies ( g * ( id b ) ) = g))))
proof
set C = ( GroupCat UN );
set V = ( GroupObjects UN );
set X = ( Morphs V );
let b being (Object of C);
reconsider b9 = b as (Element of V);
reconsider b99 = b9 as AddGroup;
A1: ( id b ) = ( ID b9 ) by CKB100:1;
hence A2: ( dom ( id b ) ) = ( dom ( ID b99 ) ) by CKB90:def 1
.= b;
thus A3: ( cod ( id b ) ) = ( cod ( ID b99 ) ) by A1,CKB91:def 1
.= b;
thus (for f being (Morphism of C) holds (( cod f ) = b implies ( ( id b ) * f ) = f))
proof
let f being (Morphism of C);
assume that
A4: ( cod f ) = b;
reconsider f1 = f as  strict (Element of X) by CKB99:1;
reconsider f9 = f1 as  strict GroupMorphism;
A5: ( cod f9 ) = b99 by A4,CKB91:def 1;
thus ( ( id b ) * f ) = ( ( ID b99 ) * f9 ) by A1,A2,A4,CKB101:1
.= f by A5,CKB55:1;
end;

thus (for g being (Morphism of C) holds (( dom g ) = b implies ( g * ( id b ) ) = g))
proof
let f being (Morphism of C);
assume that
A6: ( dom f ) = b;
reconsider f1 = f as  strict (Element of X) by CKB99:1;
reconsider f9 = f1 as  strict GroupMorphism;
A7: ( dom f9 ) = b99 by A6,CKB90:def 1;
thus ( f * ( id b ) ) = ( f9 * ( ID b99 ) ) by A1,A3,A6,CKB101:1
.= f by A7,CKB55:1;
end;

end;
