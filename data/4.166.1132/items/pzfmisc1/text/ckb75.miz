begin
reserve i for set;
reserve I for set;
reserve x for (ManySortedSet of I);
reserve X for (ManySortedSet of I);
theorem
(X is  non-empty implies ([| { x },X |] is  non-empty & [| X,{ x } |] is  non-empty))
proof
assume A1: X is  non-empty;
thus [| { x },X |] is  non-empty
proof
let i;
assume A2: i in I;
then ( X . i ) is non empty by A1,PBOOLE:def 13;
then [: { ( x . i ) },( X . i ) :] is non empty by ZFMISC_1:107;
then [: ( { x } . i ),( X . i ) :] is non empty by A2,CKB4:def 1;
hence thesis by A2,PBOOLE:def 16;
end;

let i;
assume A3: i in I;
then ( X . i ) is non empty by A1,PBOOLE:def 13;
then [: ( X . i ),{ ( x . i ) } :] is non empty by ZFMISC_1:107;
then [: ( X . i ),( { x } . i ) :] is non empty by A3,CKB4:def 1;
hence thesis by A3,PBOOLE:def 16;
end;
