environ
vocabularies FUNCT_1,PBOOLE,RELAT_1,FUNCT_4,FUNCOP_1,XBOOLE_0,FINSET_1,TARSKI,ZFMISC_1,PZFMISC1;
notations TARSKI,XBOOLE_0,ZFMISC_1,RELAT_1,FUNCT_1,FUNCT_4,FINSET_1,FUNCOP_1,PBOOLE,MBOOLEAN;
definitions FINSET_1,PBOOLE,FUNCOP_1;
theorems FUNCOP_1,ENUMSET1,FUNCT_4,MBOOLEAN,PBOOLE,SETWISEO,TARSKI,ZFMISC_1,RELAT_1,XBOOLE_0,PARTFUN1;
schemes PBOOLE;
registrations XBOOLE_0,FUNCOP_1,FINSET_1;
constructors FUNCT_4,MBOOLEAN,PBOOLE,FINSET_1,RELSET_1;
requirements BOOLE;
begin
reserve i for set;
reserve I for set;
theorem
Th1: (for X being set holds (for M being (ManySortedSet of I) holds (i in I implies ( dom ( M +* ( i .--> X ) ) ) = I)))
proof
let X being set;
let M being (ManySortedSet of I);
assume that
A1: i in I;
thus ( dom ( M +* ( i .--> X ) ) ) = ( ( dom M ) \/ ( dom ( i .--> X ) ) ) by FUNCT_4:def 1
.= ( I \/ ( dom ( i .--> X ) ) ) by PARTFUN1:def 2
.= ( I \/ { i } ) by FUNCOP_1:13
.= I by A1,ZFMISC_1:40;
end;
