environ
vocabularies FUNCT_1,PBOOLE,RELAT_1,FUNCT_4,FUNCOP_1,XBOOLE_0,FINSET_1,TARSKI,ZFMISC_1,PZFMISC1;
notations TARSKI,XBOOLE_0,ZFMISC_1,RELAT_1,FUNCT_1,FUNCT_4,FINSET_1,FUNCOP_1,PBOOLE,MBOOLEAN;
definitions FINSET_1,PBOOLE,FUNCOP_1;
theorems FUNCOP_1,ENUMSET1,FUNCT_4,MBOOLEAN,PBOOLE,SETWISEO,TARSKI,ZFMISC_1,RELAT_1,XBOOLE_0,PARTFUN1,CKB1,CKB2,CKB3;
schemes PBOOLE;
registrations XBOOLE_0,FUNCOP_1,FINSET_1;
constructors FUNCT_4,MBOOLEAN,PBOOLE,FINSET_1,RELSET_1;
requirements BOOLE;
begin
reserve i for set;
reserve I for set;
reserve A for (ManySortedSet of I);
definition
let I;
let A;
func {A } -> (ManySortedSet of I)
means
:Def1: (for i holds (i in I implies ( it . i ) = { ( A . i ) }));
existence
proof
deffunc F(set) = { ( A . $1 ) };
thus (ex M being (ManySortedSet of I) st (for i holds (i in I implies ( M . i ) = F(i)))) from PBOOLE:sch 4;
end;
uniqueness
proof
let X being (ManySortedSet of I),Y being (ManySortedSet of I);
assume that
A1: (for i holds (i in I implies ( X . i ) = { ( A . i ) }))
and
A2: (for i holds (i in I implies ( Y . i ) = { ( A . i ) }));
now
let i;
assume A3: i in I;
hence ( X . i ) = { ( A . i ) } by A1
.= ( Y . i ) by A2,A3;
end;
hence X = Y by PBOOLE:3;
end;
end;
