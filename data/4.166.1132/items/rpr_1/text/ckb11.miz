environ
vocabularies NUMBERS,XBOOLE_0,SUBSET_1,FINSEQ_1,ZFMISC_1,TARSKI,FINSET_1,RELAT_1,CARD_1,ARYTM_3,XXREAL_0,REAL_1,ARYTM_1,RPR_1,BSPACE;
notations TARSKI,XBOOLE_0,SUBSET_1,FUNCT_1,DOMAIN_1,CARD_1,NUMBERS,XCMPLX_0,REAL_1,FINSEQ_1,FINSET_1,XXREAL_0,CKB10;
definitions XBOOLE_0,SUBSET_1;
theorems TARSKI,SUBSET_1,ZFMISC_1,FINSEQ_1,CARD_1,CARD_2,REALSET1,XBOOLE_0,XBOOLE_1,XCMPLX_0,XCMPLX_1,XREAL_1,XREAL_0,NAT_1,CKB1,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9;
registrations XBOOLE_0,RELSET_1,FINSET_1,XXREAL_0,XREAL_0,MEMBERED,SUBSET_1,CARD_1,CKB2;
constructors XXREAL_0,REAL_1,NAT_1,MEMBERED,FINSEQ_1,DOMAIN_1;
requirements REAL,NUMERALS,BOOLE,SUBSET,ARITHM;
begin
reserve E for non  empty set;
reserve A for (Subset of E);
reserve e for (Singleton of E);
theorem
(for E being non  empty set holds (for e being (Singleton of E) holds (for A being (Event of E) holds (e misses A or ( e /\ A ) = e))))
proof
let E being non  empty set;
let e being (Singleton of E);
let A being (Event of E);
(( e /\ E ) = e & ( A \/ ( A ` ) ) = ( [#] E )) by SUBSET_1:10,XBOOLE_1:28;
then e = ( ( e /\ A ) \/ ( e /\ ( A ` ) ) ) by XBOOLE_1:23;
then ( e /\ A ) c= e by XBOOLE_1:7;
then (( e /\ A ) = ( {} ) or ( e /\ A ) = e) by CKB1:1;
hence thesis by XBOOLE_0:def 7;
end;
