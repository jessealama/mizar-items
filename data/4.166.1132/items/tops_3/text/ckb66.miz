begin
reserve X for non  empty TopSpace;
reserve A for (Subset of X);
reserve B for (Subset of X);
reserve X0 for non  empty (SubSpace of X);
theorem
Th61: (for A being (Subset of X) holds (for B being (Subset of X0) holds (A c= B implies (A is  everywhere_dense implies B is  everywhere_dense))))
proof
let A being (Subset of X);
let B being (Subset of X0);
assume A1: A c= B;
then reconsider C = A as (Subset of X0) by XBOOLE_1:1;
assume A is  everywhere_dense;
then ( Int A ) is  dense by CKB40:1;
then ( Int C ) is  dense by CKB61:1,CKB64:1;
then ( Int B ) is  dense by A1,TOPS_1:19,TOPS_1:44;
hence thesis by CKB40:1;
end;
