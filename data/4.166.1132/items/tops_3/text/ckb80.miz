begin
reserve X1 for TopSpace;
reserve X2 for TopSpace;
theorem
(((the carrier of X1) = (the carrier of X2) & (for C1 being (Subset of X1) holds (for C2 being (Subset of X2) holds (C1 = C2 implies ( Int C1 ) = ( Int C2 ))))) implies ( the TopStruct of X1 ) = ( the TopStruct of X2 ))
proof
assume A1: (the carrier of X1) = (the carrier of X2);
assume A2: (for C1 being (Subset of X1) holds (for C2 being (Subset of X2) holds (C1 = C2 implies ( Int C1 ) = ( Int C2 ))));
now
let C1 being (Subset of X1);
let C2 being (Subset of X2);
assume A3: C1 = C2;
thus (C1 is  open implies C2 is  open)
proof
assume C1 is  open;
then C1 = ( Int C1 ) by TOPS_1:23;
then C2 = ( Int C2 ) by A2,A3;
hence thesis;
end;

thus (C2 is  open implies C1 is  open)
proof
assume C2 is  open;
then C2 = ( Int C2 ) by TOPS_1:23;
then C1 = ( Int C1 ) by A2,A3;
hence thesis;
end;

end;
hence thesis by A1,CKB78:1;
end;
