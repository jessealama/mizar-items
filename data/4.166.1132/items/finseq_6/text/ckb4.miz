begin
reserve x for set;
reserve y for set;
reserve z for set;
theorem
Lm2: ( rng <* x,y,z *> ) = { x,y,z }
proof
<* x,y,z *> = ( <* x,y *> ^ <* z *> ) by FINSEQ_1:43;
hence ( rng <* x,y,z *> ) = ( ( rng <* x,y *> ) \/ ( rng <* z *> ) ) by FINSEQ_1:31
.= ( { x,y } \/ ( rng <* z *> ) ) by CKB3:1
.= ( { x,y } \/ { z } ) by FINSEQ_1:38
.= { x,y,z } by ENUMSET1:3;
end;
