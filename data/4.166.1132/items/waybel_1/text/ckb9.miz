coherence
proof
let x being (Element of ( BoolePoset X ));
let y being (Element of ( BoolePoset X ));
let z being (Element of ( BoolePoset X ));
thus ( x "/\" ( y "\/" z ) ) = ( x /\ ( y "\/" z ) ) by YELLOW_1:17
.= ( x /\ ( y \/ z ) ) by YELLOW_1:17
.= ( ( x /\ y ) \/ ( x /\ z ) ) by XBOOLE_1:23
.= ( ( x "/\" y ) \/ ( x /\ z ) ) by YELLOW_1:17
.= ( ( x "/\" y ) \/ ( x "/\" z ) ) by YELLOW_1:17
.= ( ( x "/\" y ) "\/" ( x "/\" z ) ) by YELLOW_1:17;
end;
