begin
theorem
Th48: (for L being non empty Poset holds (for p being (Function of L,L) holds (p is  projection implies (for Lc being non empty (Subset of L) holds (for Lk being non empty (Subset of L) holds (Lc = { c where c is (Element of L): c <= ( p . c ) } implies ( p | Lc ) is (Function of ( subrelstr Lc ),( subrelstr Lc ))))))))
proof
let L being non empty Poset;
let p being (Function of L,L);
assume that
A1: p is  projection;
let Lc being non empty (Subset of L);
let Lk being non empty (Subset of L);
assume that
A2: Lc = { c where c is (Element of L): c <= ( p . c ) };
set Lk = { k where k is (Element of L): ( p . k ) <= k };
( rng p ) = ( Lc /\ Lk ) by A1,A2,CKB80:1;
then ( rng ( p | Lc ) ) = ( Lc /\ Lk ) by A1,A2,CKB82:1;
then A3: ( rng ( p | Lc ) ) c= Lc by XBOOLE_1:17;
(Lc = (the carrier of ( subrelstr Lc )) & ( p | Lc ) is (Function of Lc,(the carrier of L))) by FUNCT_2:32,YELLOW_0:def 15;
hence thesis by A3,FUNCT_2:6;
end;
