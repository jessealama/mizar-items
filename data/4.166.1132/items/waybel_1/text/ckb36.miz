begin
theorem
Th18: (for S,T being non empty Poset holds (for d being (Function of T,S) holds (T is  complete implies (d is  sups-preserving iff (d is  monotone & d is  lower_adjoint)))))
proof
let S being non empty Poset,T being non empty Poset;
let d being (Function of T,S);
assume A1: T is  complete;
hereby
assume d is  sups-preserving;
then (ex g being (Function of S,T) st ([ g,d ] is  Galois & (for s being (Element of S) holds ( g . s ) is_maximum_of ( d " ( downarrow s ) )))) by A1,CKB34:1;
hence (d is  monotone & d is  lower_adjoint) by CKB25:def 1,CKB28:1;
end;
assume d is  monotone;
assume (ex g being (Function of S,T) st [ g,d ] is  Galois);
then d is  lower_adjoint by CKB25:def 1;
hence thesis;
end;
