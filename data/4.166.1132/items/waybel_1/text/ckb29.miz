begin
theorem
Th13: (for S,T being non empty Poset holds (for g being (Function of S,T) holds (g is  upper_adjoint implies g is  infs-preserving)))
proof
let S being non empty Poset,T being non empty Poset;
let g being (Function of S,T);
given d being (Function of T,S) such that
A1: [ g,d ] is  Galois;

let X being (Subset of S);
set s = ( inf X );
assume A2:  ex_inf_of X,S;
A3: (for t being (Element of T) holds (t is_<=_than ( g .: X ) implies ( g . s ) >= t))
proof
let t being (Element of T);
assume A4: t is_<=_than ( g .: X );
( d . t ) is_<=_than X
proof
let si being (Element of S);
assume si in X;
then ( g . si ) in ( g .: X ) by FUNCT_2:35;
then t <= ( g . si ) by A4,LATTICE3:def 8;
hence ( d . t ) <= si by A1,CKB23:1;
end;
then ( d . t ) <= s by A2,YELLOW_0:31;
hence thesis by A1,CKB23:1;
end;
( g . s ) is_<=_than ( g .: X )
proof
let t being (Element of T);
assume t in ( g .: X );
then consider si being (Element of S) such that A5: si in X and A6: t = ( g . si ) by FUNCT_2:65;
A7: g is  monotone by A1,CKB23:1;
reconsider si as (Element of S);
s is_<=_than X by A2,YELLOW_0:31;
then s <= si by A5,LATTICE3:def 8;
hence ( g . s ) <= t by A7,A6,CKB2:def 1;
end;
hence thesis by A3,YELLOW_0:31;
end;
