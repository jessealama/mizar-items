begin
theorem
(for S,T being non empty Poset holds (for g being (Function of S,T) holds (S is  complete implies (g is  infs-preserving iff (g is  monotone & g is  upper_adjoint)))))
proof
let S being non empty Poset,T being non empty Poset;
let g being (Function of S,T);
assume A1: S is  complete;
hereby
assume g is  infs-preserving;
then (ex d being (Function of T,S) st ([ g,d ] is  Galois & (for t being (Element of T) holds ( d . t ) is_minimum_of ( g " ( uparrow t ) )))) by A1,CKB33:1;
hence (g is  monotone & g is  upper_adjoint) by CKB24:def 1,CKB27:1;
end;
assume g is  monotone;
assume (ex d being (Function of T,S) st [ g,d ] is  Galois);
then g is  upper_adjoint by CKB24:def 1;
hence thesis;
end;
