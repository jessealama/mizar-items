theorem
Th53: (for L being non  empty Poset holds (for p being (Function of L,L) holds (p is  monotone implies (for Lk being (Subset of L) holds (Lk = { k where k is (Element of L): ( p . k ) <= k } implies ( subrelstr Lk ) is  infs-inheriting)))))
proof
let L being non  empty Poset;
let p being (Function of L,L);
assume that
A1: p is  monotone;
let Lk being (Subset of L);
assume that
A2: Lk = { k where k is (Element of L): ( p . k ) <= k };
let X being (Subset of ( subrelstr Lk ));
assume A3:  ex_inf_of X,L;
( p . ( "/\" (X,L) ) ) is_<=_than X
proof
let x being (Element of L);
assume A4: x in X;
then x in (the carrier of ( subrelstr Lk ));
then x in Lk by YELLOW_0:def 15;
then A5: (ex l being (Element of L) st (x = l & l >= ( p . l ))) by A2;
( "/\" (X,L) ) is_<=_than X by A3,YELLOW_0:31;
then x >= ( "/\" (X,L) ) by A4,LATTICE3:def 8;
then ( p . x ) >= ( p . ( "/\" (X,L) ) ) by A1,CKB2:def 1;
hence thesis by A5,ORDERS_2:3;
end;
then ( "/\" (X,L) ) >= ( p . ( "/\" (X,L) ) ) by A3,YELLOW_0:31;
then ( "/\" (X,L) ) in Lk by A2;
hence thesis by YELLOW_0:def 15;
end;
