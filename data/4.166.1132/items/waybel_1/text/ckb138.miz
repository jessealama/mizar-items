theorem
Lm7: (for L being  bounded LATTICE holds ((for x being (Element of L) holds (ex x9 being (Element of L) st (for y being (Element of L) holds (( ( y "\/" x9 ) "/\" x ) <= y & y <= ( ( y "/\" x ) "\/" x9 ))))) implies (L is  Heyting & (for x being (Element of L) holds ( 'not' ( 'not' x ) ) = x))))
proof
let L being  bounded LATTICE;
defpred P[ (Element of L),(Element of L) ]
 means
(for y being (Element of L) holds (( ( y "\/" $2 ) "/\" $1 ) <= y & y <= ( ( y "/\" $1 ) "\/" $2 )));
assume A1: (for x being (Element of L) holds (ex x9 being (Element of L) st P[ x,x9 ]));
consider g9 being (Function of L,L) such that A2: (for x being (Element of L) holds P[ x,( g9 . x ) ]) from FUNCT_2:sch 3(A1);
A3:now
let y being (Element of L);
let g being (Function of L,L);
assume that
A4: (for z being (Element of L) holds ( g . z ) = ( ( g9 . y ) "\/" z ));
A5:now
let x being (Element of L);
let z being (Element of L);
hereby
assume x <= ( g . z );
then x <= ( ( g9 . y ) "\/" z ) by A4;
then A6: ( x "/\" y ) <= ( ( ( g9 . y ) "\/" z ) "/\" y ) by CKB3:1;
( ( ( g9 . y ) "\/" z ) "/\" y ) <= z by A2;
then ( x "/\" y ) <= z by A6,ORDERS_2:3;
hence ( ( y "/\" ) . x ) <= z by CKB97:def 1;
end;
assume ( ( y "/\" ) . x ) <= z;
then ( y "/\" x ) <= z by CKB97:def 1;
then A7: ( ( x "/\" y ) "\/" ( g9 . y ) ) <= ( z "\/" ( g9 . y ) ) by CKB4:1;
x <= ( ( x "/\" y ) "\/" ( g9 . y ) ) by A2;
then x <= ( z "\/" ( g9 . y ) ) by A7,ORDERS_2:3;
hence x <= ( g . z ) by A4;
end;
g is  monotone
proof
let z1 being (Element of L);
let z2 being (Element of L);
assume z1 <= z2;
then ( ( g9 . y ) "\/" z1 ) <= ( z2 "\/" ( g9 . y ) ) by CKB4:1;
then ( g . z1 ) <= ( ( g9 . y ) "\/" z2 ) by A4;
hence thesis by A4;
end;
hence [ g,( y "/\" ) ] is  Galois by A5,CKB23:1;
end;
thus A8: L is  Heyting
proof
thus L is LATTICE;
let y being (Element of L);
deffunc F((Element of L)) = ( ( g9 . y ) "\/" $1 );
consider g being (Function of L,L) such that A9: (for z being (Element of L) holds ( g . z ) = F(z)) from FUNCT_2:sch 4;
[ g,( y "/\" ) ] is  Galois by A3,A9;
hence thesis by CKB25:def 1;
end;

A10:now
let x being (Element of L);
deffunc F((Element of L)) = ( ( g9 . x ) "\/" $1 );
consider g being (Function of L,L) such that A11: (for z being (Element of L) holds ( g . z ) = F(z)) from FUNCT_2:sch 4;
[ g,( x "/\" ) ] is  Galois by A3,A11;
then g = ( x => ) by A8,CKB108:def 1;
hence ( 'not' x ) = ( ( Bottom L ) "\/" ( g9 . x ) ) by A11
.= ( g9 . x ) by CKB5:1;
end;
A12:now
let x being (Element of L);
( ( ( Bottom L ) "\/" ( g9 . x ) ) "/\" x ) <= ( Bottom L ) by A2;
then ( ( x "/\" ( Bottom L ) ) "\/" ( x "/\" ( g9 . x ) ) ) <= ( Bottom L ) by A8,CKB7:def 1;
then ( ( Bottom L ) "\/" ( x "/\" ( g9 . x ) ) ) <= ( Bottom L ) by CKB5:1;
then A13: ( x "/\" ( g9 . x ) ) <= ( Bottom L ) by CKB5:1;
( Bottom L ) <= ( x "/\" ( g9 . x ) ) by YELLOW_0:44;
hence ( Bottom L ) = ( x "/\" ( g9 . x ) ) by A13,ORDERS_2:2
.= ( x "/\" ( 'not' x ) ) by A10;
end;
let x being (Element of L);
A14:now
let x being (Element of L);
( Top L ) <= ( ( ( Top L ) "/\" x ) "\/" ( g9 . x ) ) by A2;
then A15: ( Top L ) <= ( x "\/" ( g9 . x ) ) by CKB6:1;
( x "\/" ( g9 . x ) ) <= ( Top L ) by YELLOW_0:45;
hence ( Top L ) = ( x "\/" ( g9 . x ) ) by A15,ORDERS_2:2
.= ( x "\/" ( 'not' x ) ) by A10;
end;
then ( ( ( 'not' x ) "\/" ( 'not' ( 'not' x ) ) ) "/\" x ) = ( ( Top L ) "/\" x );
then x = ( x "/\" ( ( 'not' x ) "\/" ( 'not' ( 'not' x ) ) ) ) by CKB6:1
.= ( ( x "/\" ( 'not' x ) ) "\/" ( x "/\" ( 'not' ( 'not' x ) ) ) ) by A8,CKB7:def 1
.= ( ( Bottom L ) "\/" ( x "/\" ( 'not' ( 'not' x ) ) ) ) by A12
.= ( x "/\" ( 'not' ( 'not' x ) ) ) by CKB5:1;
then A16: x <= ( 'not' ( 'not' x ) ) by YELLOW_0:25;
( ( Bottom L ) "\/" x ) = ( ( ( 'not' x ) "/\" ( 'not' ( 'not' x ) ) ) "\/" x ) by A12;
then x = ( x "\/" ( ( 'not' x ) "/\" ( 'not' ( 'not' x ) ) ) ) by CKB5:1
.= ( ( x "\/" ( 'not' x ) ) "/\" ( x "\/" ( 'not' ( 'not' x ) ) ) ) by A8,CKB8:1
.= ( ( Top L ) "/\" ( x "\/" ( 'not' ( 'not' x ) ) ) ) by A14
.= ( x "\/" ( 'not' ( 'not' x ) ) ) by CKB6:1;
hence thesis by A16,YELLOW_0:24;
end;
