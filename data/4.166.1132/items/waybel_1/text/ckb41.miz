environ
vocabularies XBOOLE_0,STRUCT_0,FUNCT_1,SUBSET_1,ORDERS_2,SEQM_3,XXREAL_0,RELAT_2,LATTICE3,LATTICES,EQREL_1,WAYBEL_0,YELLOW_1,YELLOW_0,RELAT_1,CAT_1,WELLORD1,ORDINAL2,TARSKI,REWRITE1,CARD_FIL,BINOP_1,FUNCT_2,GROUP_6,YELLOW_2,LATTICE2,XBOOLEAN,ZFMISC_1,XXREAL_2,WAYBEL_1;
notations TARSKI,XBOOLE_0,ZFMISC_1,FUNCT_1,RELSET_1,PARTFUN1,FUNCT_2,DOMAIN_1,STRUCT_0,WELLORD1,ORDERS_2,LATTICE3,QUANTAL1,ORDERS_3,YELLOW_0,YELLOW_1,WAYBEL_0,YELLOW_2,CKB1,CKB2,CKB7,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB17,CKB20,CKB21,CKB22,CKB24,CKB25;
definitions TARSKI,LATTICE3,QUANTAL1,ORDERS_3,YELLOW_0,WAYBEL_0,XBOOLE_0,YELLOW_2,STRUCT_0,CKB1,CKB2,CKB7,CKB10,CKB11,CKB14,CKB15,CKB17,CKB20,CKB22,CKB24,CKB25;
theorems ZFMISC_1,ORDERS_2,FUNCT_1,FUNCT_2,LATTICE3,RELAT_1,TARSKI,WELLORD1,ORDERS_3,YELLOW_0,YELLOW_1,YELLOW_2,WAYBEL_0,RELSET_1,XBOOLE_0,XBOOLE_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB10,CKB11,CKB14,CKB15,CKB17,CKB18,CKB19,CKB20,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB31,CKB33,CKB34,CKB35,CKB36,CKB37,CKB38,CKB39,CKB40;
schemes FUNCT_2,DOMAIN_1;
registrations RELAT_1,FUNCT_1,FUNCT_2,STRUCT_0,LATTICE3,YELLOW_0,WAYBEL_0,YELLOW_1,YELLOW_2,RELSET_1,CKB9,CKB16,CKB30,CKB32;
constructors DOMAIN_1,TOLER_1,QUANTAL1,ORDERS_3,YELLOW_2,RELSET_1,CKB1,CKB7,CKB10,CKB11,CKB14,CKB15,CKB17,CKB20,CKB21,CKB22,CKB24,CKB25;
requirements SUBSET,BOOLE;
begin
theorem
Th23: (for S,T being non  empty Poset holds (for g being (Function of S,T) holds (for d being (Function of T,S) holds (([ g,d ] is  Galois & g is  onto) implies (for t being (Element of T) holds ( d . t ) is_minimum_of ( g " { t } ))))))
proof
let S being non  empty Poset;
let T being non  empty Poset;
let g being (Function of S,T);
let d being (Function of T,S);
assume that
A1: [ g,d ] is  Galois
and
A2: g is  onto;
A3: g is  monotone by A1,CKB23:1;
let t being (Element of T);
A4: ( rng g ) = (the carrier of T) by A2,FUNCT_2:def 3;
then A5: ( g .: ( g " ( uparrow t ) ) ) = ( uparrow t ) by FUNCT_1:77;
A6: ( d . t ) is_minimum_of ( g " ( uparrow t ) ) by A1,CKB27:1;
then A7: ( d . t ) = ( inf ( g " ( uparrow t ) ) ) by CKB14:def 1;
then ( d . t ) in ( g " ( uparrow t ) ) by A6,CKB14:def 1;
then ( g . ( d . t ) ) in ( g .: ( g " ( uparrow t ) ) ) by FUNCT_2:35;
then A8: t <= ( g . ( d . t ) ) by A5,WAYBEL_0:18;
 ex_inf_of ( g " ( uparrow t ) ),S by A6,CKB14:def 1;
then A9: ( d . t ) is_<=_than ( g " ( uparrow t ) ) by A7,YELLOW_0:31;
consider s being set such that A10: s in (the carrier of S) and A11: ( g . s ) = t by A4,FUNCT_2:11;
reconsider s as (Element of S) by A10;
A12: t in { t } by TARSKI:def 1;
A13: { t } c= ( uparrow { t } ) by WAYBEL_0:16;
then s in ( g " ( uparrow t ) ) by A11,A12,FUNCT_2:38;
then ( d . t ) <= s by A9,LATTICE3:def 8;
then ( g . ( d . t ) ) <= t by A11,A3,CKB2:def 1;
then A14: ( g . ( d . t ) ) = t by A8,ORDERS_2:2;
then A15: ( d . t ) in ( g " { t } ) by A12,FUNCT_2:38;
A16: ( g " { t } ) c= ( g " ( uparrow t ) ) by RELAT_1:143,WAYBEL_0:16;
thus A17:  ex_inf_of ( g " { t } ),S
proof
take ( d . t );
thus ( g " { t } ) is_>=_than ( d . t ) by A9,A16,YELLOW_0:9;
thus (for b being (Element of S) holds (( g " { t } ) is_>=_than b implies b <= ( d . t ))) by A15,LATTICE3:def 8;
let c being (Element of S);
assume ( g " { t } ) is_>=_than c;
then A18: c <= ( d . t ) by A15,LATTICE3:def 8;
assume (for b being (Element of S) holds (( g " { t } ) is_>=_than b implies b <= c));
then ( d . t ) <= c by A9,A16,YELLOW_0:9;
hence thesis by A18,ORDERS_2:2;
end;

then ( inf ( g " { t } ) ) is_<=_than ( g " { t } ) by YELLOW_0:31;
then A19: ( inf ( g " { t } ) ) <= ( d . t ) by A15,LATTICE3:def 8;
 ex_inf_of ( g " ( uparrow t ) ),S by A6,CKB14:def 1;
then ( inf ( g " { t } ) ) >= ( d . t ) by A7,A13,A17,RELAT_1:143,YELLOW_0:35;
hence ( d . t ) = ( inf ( g " { t } ) ) by A19,ORDERS_2:2;
hence thesis by A12,A14,FUNCT_2:38;
end;
