begin
theorem
Th14: (for S,T being non empty Poset holds (for d being (Function of T,S) holds (d is  lower_adjoint implies d is  sups-preserving)))
proof
let S being non empty Poset,T being non empty Poset;
let d being (Function of T,S);
given g being (Function of S,T) such that
A1: [ g,d ] is  Galois;

let X being (Subset of T);
set t = ( sup X );
assume A2:  ex_sup_of X,T;
A3: (for s being (Element of S) holds (s is_>=_than ( d .: X ) implies ( d . t ) <= s))
proof
let s being (Element of S);
assume A4: s is_>=_than ( d .: X );
( g . s ) is_>=_than X
proof
let ti being (Element of T);
assume ti in X;
then ( d . ti ) in ( d .: X ) by FUNCT_2:35;
then s >= ( d . ti ) by A4,LATTICE3:def 9;
hence thesis by A1,CKB23:1;
end;
then ( g . s ) >= t by A2,YELLOW_0:30;
hence thesis by A1,CKB23:1;
end;
( d . t ) is_>=_than ( d .: X )
proof
let s being (Element of S);
assume s in ( d .: X );
then consider ti being (Element of T) such that A5: ti in X and A6: s = ( d . ti ) by FUNCT_2:65;
A7: d is  monotone by A1,CKB23:1;
reconsider ti as (Element of T);
t is_>=_than X by A2,YELLOW_0:30;
then t >= ti by A5,LATTICE3:def 9;
hence thesis by A7,A6,CKB2:def 1;
end;
hence thesis by A3,YELLOW_0:30;
end;
