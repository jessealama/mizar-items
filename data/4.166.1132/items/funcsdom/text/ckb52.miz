environ
vocabularies XBOOLE_0,BINOP_1,FUNCT_2,SUBSET_1,FUNCT_1,ZFMISC_1,NUMBERS,RELAT_1,FUNCOP_1,BINOP_2,REAL_1,CARD_1,ARYTM_3,RLVECT_1,ARYTM_1,STRUCT_0,ALGSTR_0,XREAL_0,ORDINAL1,SUPINF_2,GROUP_1,MESFUNC1,VECTSP_1,LATTICES,FUNCSDOM;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,NUMBERS,RELAT_1,FUNCT_1,FUNCT_2,BINOP_1,DOMAIN_1,FUNCOP_1,BINOP_2,ORDINAL1,XCMPLX_0,XREAL_0,STRUCT_0,ALGSTR_0,RLVECT_1,REAL_1,GROUP_1,VECTSP_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB34,CKB36,CKB42,CKB44,CKB46,CKB50;
definitions RLVECT_1,STRUCT_0,GROUP_1,BINOP_1,VECTSP_1,ALGSTR_0,FUNCOP_1,FUNCT_2,CKB5,CKB6,CKB7,CKB8,CKB9,CKB34,CKB36,CKB46,CKB50;
theorems FUNCT_2,BINOP_1,FUNCOP_1,TARSKI,BINOP_2,STRUCT_0,XREAL_0,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB39,CKB40,CKB43,CKB46,CKB49,CKB50;
schemes BINOP_1,FUNCT_2;
registrations XBOOLE_0,SUBSET_1,RELSET_1,NUMBERS,RLVECT_1,VECTSP_1,ALGSTR_0,BINOP_2,FUNCOP_1,FUNCT_3,REAL_1,XREAL_0,ORDINAL1,RELAT_1,VALUED_0,STRUCT_0,CARD_1,CKB37,CKB38,CKB41,CKB44,CKB45,CKB47,CKB48,CKB51;
constructors BINOP_1,DOMAIN_1,FUNCOP_1,REAL_1,BINOP_2,VECTSP_1,RLVECT_1,FUNCT_3,RELSET_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB34,CKB36,CKB44,CKB46,CKB50;
requirements NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve a for Real;
registration
let A being set;
cluster ( RAlgebra A ) ->  strict  Abelian  add-associative  right_zeroed  right_complementable  commutative  associative  right_unital  right-distributive  vector-associative  scalar-associative  vector-distributive  scalar-distributive;
coherence
proof
thus ( RAlgebra A ) is  strict;
thus ( RAlgebra A ) is  Abelian
proof
let x being (Element of ( RAlgebra A )),y being (Element of ( RAlgebra A ));
thus ( x + y ) = ( y + x ) by CKB15:1;
end;

thus ( RAlgebra A ) is  add-associative
proof
let x being (Element of ( RAlgebra A )),y being (Element of ( RAlgebra A )),z being (Element of ( RAlgebra A ));
thus ( ( x + y ) + z ) = ( x + ( y + z ) ) by CKB16:1;
end;

thus ( RAlgebra A ) is  right_zeroed
proof
let x being (Element of ( RAlgebra A ));
thus ( x + ( 0. ( RAlgebra A ) ) ) = ( ( RealFuncAdd A ) . (( RealFuncZero A ),x) ) by CKB15:1
.= x by CKB20:1;
end;

thus ( RAlgebra A ) is  right_complementable
proof
let x being (Element of ( RAlgebra A ));
reconsider f = x as (Element of ( Funcs (A,( REAL )) ));
reconsider t = ( ( RealFuncExtMult A ) . [ ( - 1 ),f ] ) as (Element of ( RAlgebra A ));
take t;
thus thesis by CKB21:1;
end;

thus ( RAlgebra A ) is  commutative
proof
let x being (Element of ( RAlgebra A )),y being (Element of ( RAlgebra A ));
thus ( x * y ) = ( y * x ) by CKB17:1;
end;

thus ( RAlgebra A ) is  associative
proof
let x being (Element of ( RAlgebra A )),y being (Element of ( RAlgebra A )),z being (Element of ( RAlgebra A ));
thus ( ( x * y ) * z ) = ( x * ( y * z ) ) by CKB18:1;
end;

thus ( RAlgebra A ) is  right_unital
proof
let x being (Element of ( RAlgebra A ));
thus ( x * ( 1. ( RAlgebra A ) ) ) = ( ( RealFuncMult A ) . (( RealFuncUnit A ),x) ) by CKB17:1
.= x by CKB19:1;
end;

thus ( RAlgebra A ) is  right-distributive
proof
let x being (Element of ( RAlgebra A )),y being (Element of ( RAlgebra A )),z being (Element of ( RAlgebra A ));
thus ( x * ( y + z ) ) = ( ( x * y ) + ( x * z ) ) by CKB26:1;
end;

thus ( RAlgebra A ) is  vector-associative
proof
let x being (Element of ( RAlgebra A )),y being (Element of ( RAlgebra A ));
let a;
thus ( ( a * x ) * y ) = ( a * ( x * y ) ) by CKB27:1;
end;

thus ( RAlgebra A ) is  scalar-associative
proof
let a being  real number,b being  real number;
let x being (Element of ( RAlgebra A ));
reconsider a,b as (Element of ( REAL )) by XREAL_0:def 1;
( ( a * b ) * x ) = ( a * ( b * x ) ) by CKB23:1;
hence thesis;
end;

thus ( RAlgebra A ) is  vector-distributive
proof
let a being  real number;
let v being (Element of ( RAlgebra A )),w being (Element of ( RAlgebra A ));
reconsider a as (Element of ( REAL )) by XREAL_0:def 1;
( a * ( v + w ) ) = ( ( a * v ) + ( a * w ) ) by CKB25:1;
hence thesis;
end;

let a being  real number,b being  real number;
let v being (Element of ( RAlgebra A ));
reconsider a,b as (Element of ( REAL )) by XREAL_0:def 1;
( ( a + b ) * v ) = ( ( a * v ) + ( b * v ) ) by CKB24:1;
hence thesis;
end;
end;
