begin
reserve A for non  empty set;
reserve a for Real;
reserve b for Real;
definition
let A being set;
func RealVectSpace A ->  strict RealLinearSpace equals 
RLSStruct (# ( Funcs (A,( REAL )) ),( RealFuncZero A ),( RealFuncAdd A ),( RealFuncExtMult A ) #);
coherence
proof
set S = RLSStruct (# ( Funcs (A,( REAL )) ),( RealFuncZero A ),( RealFuncAdd A ),( RealFuncExtMult A ) #);
A1: S is  add-associative
proof
let u being (Element of S);
let v being (Element of S);
let w being (Element of S);
thus thesis by CKB16:1;
end;
A2: S is  right_complementable
proof
let u being (Element of S);
reconsider u9 = u as (Element of ( Funcs (A,( REAL )) ));
reconsider w = ( ( RealFuncExtMult A ) . [ ( - 1 ),u9 ] ) as (VECTOR of S);
take w;
thus thesis by CKB21:1;
end;
A3: S is  vector-distributive  scalar-distributive  scalar-associative  scalar-unital
proof
thus (for a being  real number holds (for v,w being (Element of S) holds ( a * ( v + w ) ) = ( ( a * v ) + ( a * w ) )))
proof
let a being  real number;
reconsider a as Real by XREAL_0:def 1;
(for v,w being (Element of S) holds ( a * ( v + w ) ) = ( ( a * v ) + ( a * w ) )) by CKB25:1;
hence thesis;
end;

thus (for a,b being  real number holds (for v being (Element of S) holds ( ( a + b ) * v ) = ( ( a * v ) + ( b * v ) )))
proof
let a being  real number;
let b being  real number;
reconsider a,b as Real by XREAL_0:def 1;
(for v being (Element of S) holds ( ( a + b ) * v ) = ( ( a * v ) + ( b * v ) )) by CKB24:1;
hence thesis;
end;

thus (for a,b being  real number holds (for v being (Element of S) holds ( ( a * b ) * v ) = ( a * ( b * v ) )))
proof
let a being  real number;
let b being  real number;
reconsider a,b as Real by XREAL_0:def 1;
(for v being (Element of S) holds ( ( a * b ) * v ) = ( a * ( b * v ) )) by CKB23:1;
hence thesis;
end;

let v being (Element of S);
thus ( 1 * v ) = v by CKB22:1;
end;
A4: S is  right_zeroed
proof
let u being (Element of S);
reconsider u9 = u as (Element of ( Funcs (A,( REAL )) ));
thus ( u + ( 0. S ) ) = ( ( RealFuncAdd A ) . (( RealFuncZero A ),u9) ) by CKB15:1
.= u by CKB20:1;
end;
S is  Abelian
proof
let u being (Element of S);
let v being (Element of S);
thus thesis by CKB15:1;
end;
hence thesis by A1,A4,A2,A3;
end;
