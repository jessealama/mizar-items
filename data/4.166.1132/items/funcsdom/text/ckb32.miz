begin
reserve x1 for set;
reserve x2 for set;
reserve z for set;
reserve A for non  empty set;
reserve f for (Element of ( Funcs (A,( REAL )) ));
reserve g for (Element of ( Funcs (A,( REAL )) ));
reserve h for (Element of ( Funcs (A,( REAL )) ));
reserve a for Real;
reserve b for Real;
theorem
((A = { x1,x2 } & x1 <> x2) implies (ex f st (ex g st (for h holds (ex a st (ex b st h = ( ( RealFuncAdd A ) . (( ( RealFuncExtMult A ) . [ a,f ] ),( ( RealFuncExtMult A ) . [ b,g ] )) )))))))
proof
assume A1: (A = { x1,x2 } & x1 <> x2);
consider f,g such that A2: ((for z holds (z in A implies ((z = x1 implies ( f . z ) = 1) & (z <> x1 implies ( f . z ) = ( 0 ))))) & (for z holds (z in A implies ((z = x1 implies ( g . z ) = ( 0 )) & (z <> x1 implies ( g . z ) = 1))))) by CKB28:1;
take f;
take g;
let h;
thus thesis by A1,A2,CKB31:1;
end;
