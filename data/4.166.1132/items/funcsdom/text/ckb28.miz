begin
reserve x1 for set;
reserve z for set;
reserve A for non empty set;
reserve f for (Element of ( Funcs (A,( REAL )) ));
reserve g for (Element of ( Funcs (A,( REAL )) ));
theorem
Th28: (ex f st (ex g st ((for z holds (z in A implies ((z = x1 implies ( f . z ) = 1) & (z <> x1 implies ( f . z ) = ( 0 ))))) & (for z holds (z in A implies ((z = x1 implies ( g . z ) = ( 0 )) & (z <> x1 implies ( g . z ) = 1)))))))
proof
deffunc G(set) = 1;
deffunc F(set) = ( 0 );
defpred P[ set ]
 means
$1 = x1;
A1: (for z holds (z in A implies ((P[ z ] implies G(z) in ( REAL )) & ((not P[ z ]) implies F(z) in ( REAL )))));
consider f being (Function of A,( REAL )) such that A2: (for z holds (z in A implies ((P[ z ] implies ( f . z ) = G(z)) & ((not P[ z ]) implies ( f . z ) = F(z))))) from FUNCT_2:sch 5(A1);
A3: (for z holds (z in A implies ((P[ z ] implies F(z) in ( REAL )) & ((not P[ z ]) implies G(z) in ( REAL )))));
consider g being (Function of A,( REAL )) such that A4: (for z holds (z in A implies ((P[ z ] implies ( g . z ) = F(z)) & ((not P[ z ]) implies ( g . z ) = G(z))))) from FUNCT_2:sch 5(A3);
reconsider f,g as (Element of ( Funcs (A,( REAL )) )) by FUNCT_2:8;
take f;
take g;
thus thesis by A2,A4;
end;
