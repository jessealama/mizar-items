begin
reserve x1 for set;
reserve x2 for set;
reserve z for set;
reserve A for non  empty set;
reserve f for (Element of ( Funcs (A,( REAL )) ));
reserve g for (Element of ( Funcs (A,( REAL )) ));
reserve h for (Element of ( Funcs (A,( REAL )) ));
reserve a for Real;
reserve b for Real;
theorem
Th31: ((((A = { x1,x2 } & x1 <> x2) & (for z holds (z in A implies ((z = x1 implies ( f . z ) = 1) & (z <> x1 implies ( f . z ) = ( 0 )))))) & (for z holds (z in A implies ((z = x1 implies ( g . z ) = ( 0 )) & (z <> x1 implies ( g . z ) = 1))))) implies (for h holds (ex a st (ex b st h = ( ( RealFuncAdd A ) . (( ( RealFuncExtMult A ) . [ a,f ] ),( ( RealFuncExtMult A ) . [ b,g ] )) )))))
proof
assume that
A1: A = { x1,x2 }
and
A2: x1 <> x2
and
A3: ((for z holds (z in A implies ((z = x1 implies ( f . z ) = 1) & (z <> x1 implies ( f . z ) = ( 0 ))))) & (for z holds (z in A implies ((z = x1 implies ( g . z ) = ( 0 )) & (z <> x1 implies ( g . z ) = 1)))));
x2 in A by A1,TARSKI:def 2;
then A4: (( f . x2 ) = ( 0 ) & ( g . x2 ) = 1) by A2,A3;
x1 in A by A1,TARSKI:def 2;
then A5: (( f . x1 ) = 1 & ( g . x1 ) = ( 0 )) by A3;
let h;
reconsider x1,x2 as (Element of A) by A1,TARSKI:def 2;
take a = ( h . x1 );
take b = ( h . x2 );
now
let x being (Element of A);
A6: (x = x1 or x = x2) by A1,TARSKI:def 2;
A7: ( ( ( RealFuncAdd A ) . (( ( RealFuncExtMult A ) . [ a,f ] ),( ( RealFuncExtMult A ) . [ b,g ] )) ) . x2 ) = ( ( ( ( RealFuncExtMult A ) . [ a,f ] ) . x2 ) + ( ( ( RealFuncExtMult A ) . [ b,g ] ) . x2 ) ) by CKB11:1
.= ( ( a * ( f . x2 ) ) + ( ( ( RealFuncExtMult A ) . [ b,g ] ) . x2 ) ) by CKB14:1
.= ( ( 0 ) + ( b * 1 ) ) by A4,CKB14:1
.= ( h . x2 );
( ( ( RealFuncAdd A ) . (( ( RealFuncExtMult A ) . [ a,f ] ),( ( RealFuncExtMult A ) . [ b,g ] )) ) . x1 ) = ( ( ( ( RealFuncExtMult A ) . [ a,f ] ) . x1 ) + ( ( ( RealFuncExtMult A ) . [ b,g ] ) . x1 ) ) by CKB11:1
.= ( ( a * ( f . x1 ) ) + ( ( ( RealFuncExtMult A ) . [ b,g ] ) . x1 ) ) by CKB14:1
.= ( a + ( b * ( 0 ) ) ) by A5,CKB14:1
.= ( h . x1 );
hence ( h . x ) = ( ( ( RealFuncAdd A ) . (( ( RealFuncExtMult A ) . [ a,f ] ),( ( RealFuncExtMult A ) . [ b,g ] )) ) . x ) by A6,A7;
end;
hence thesis by FUNCT_2:63;
