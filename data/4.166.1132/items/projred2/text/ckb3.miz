environ
vocabularies INCPROJ,INCSP_1,SUBSET_1,AFF_2,ANALOAF,PARTFUN1,PROJRED1,FUNCT_1,RELAT_1,TARSKI,ZFMISC_1,PROJRED2;
notations TARSKI,ZFMISC_1,SUBSET_1,DOMAIN_1,RELAT_1,RELSET_1,INCSP_1,INCPROJ,PARTFUN1,FUNCT_1,PROJRED1,CKB1,CKB2;
definitions CKB1,CKB2;
theorems TARSKI,INCPROJ,FUNCT_1,PROJRED1,ZFMISC_1,INCSP_1,RELAT_1,CKB1,CKB2;
registrations FUNCT_1,RELSET_1,INCPROJ;
constructors DOMAIN_1,PROJRED1,RELSET_1,CKB1,CKB2;
requirements SUBSET,BOOLE;
begin
reserve IPP for  Desarguesian  2-dimensional IncProjSp;
reserve a for (POINT of IPP);
reserve A for (LINE of IPP);
reserve B for (LINE of IPP);
definition
let IPP;
mode Projection of IPP
 -> (PartFunc of (the Points of IPP),(the Points of IPP))
means :Def3: (ex a st (ex A st (ex B st (((not a on A) & (not a on B)) & it = ( IncProj (A,a,B) )))));
existence
proof
set A = the (LINE of IPP);
consider a such that A1: ((not a on A) & (not a on A)) by PROJRED1:6;
take ( IncProj (A,a,A) );
thus thesis by A1;
end;
end;
