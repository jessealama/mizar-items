requirements SUBSET,REAL,BOOLE,NUMERALS,ARITHM;
begin
reserve q for Rational;
reserve f for (PartFunc of ( REAL ),( REAL ));
definition
let q being Integer;
func #Z q -> (Function of ( REAL ),( REAL )) means 
:Def1: (for x being  real number holds ( it . x ) = ( x #Z q ));
existence
proof
deffunc U(Real) = ( $1 #Z q );
consider f being (Function of ( REAL ),( REAL )) such that A1: (for d being (Element of ( REAL )) holds ( f . d ) = U(d)) from FUNCT_2:sch 4;
take f;
let d being  real number;
d is Real by XREAL_0:def 1;
hence thesis by A1;
end;
uniqueness
proof
let f1 being (Function of ( REAL ),( REAL ));
let f2 being (Function of ( REAL ),( REAL ));
assume that
A2: (for d being  real number holds ( f1 . d ) = ( d #Z q ))
and
A3: (for d being  real number holds ( f2 . d ) = ( d #Z q ));
(for d being (Element of ( REAL )) holds ( f1 . d ) = ( f2 . d ))
proof
let d being (Element of ( REAL ));
thus ( f1 . d ) = ( d #Z q ) by A2
.= ( f2 . d ) by A3;
end;
hence f1 = f2 by FUNCT_2:63;
end;
end;
