begin
theorem
Th3: (for D being non empty set holds (for f being (FinSequence of D) holds (for G being (Matrix of D) holds (for p being (Element of D) holds (p in ( rng f ) implies (f is_sequence_on G implies ( f :- p ) is_sequence_on G))))))
proof
let D being non empty set;
let f being (FinSequence of D);
let G being (Matrix of D);
let p being (Element of D);
assume that
A1: p in ( rng f )
and
A2: f is_sequence_on G;
(ex i being (Element of ( NAT )) st (( i + 1 ) = ( p .. f ) & ( f :- p ) = ( f /^ i ))) by A1,FINSEQ_5:49;
hence thesis by A2,JORDAN8:2;
end;
