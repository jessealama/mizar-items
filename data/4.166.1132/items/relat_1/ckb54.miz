begin
reserve x for set;
reserve y for set;
reserve z for set;
reserve P for Relation;
reserve Q for Relation;
reserve R for Relation;
theorem
( P * ( R /\ Q ) ) c= ( ( P * R ) /\ ( P * Q ) )
proof
let x;
let y;
assume [ x,y ] in ( P * ( R /\ Q ) );
then consider z such that A1: [ x,z ] in P and A2: [ z,y ] in ( R /\ Q ) by CKB45:def 1;
[ z,y ] in Q by A2,XBOOLE_0:def 4;
then A3: [ x,y ] in ( P * Q ) by A1,CKB45:def 1;
[ z,y ] in R by A2,XBOOLE_0:def 4;
then [ x,y ] in ( P * R ) by A1,CKB45:def 1;
hence [ x,y ] in ( ( P * R ) /\ ( P * Q ) ) by A3,XBOOLE_0:def 4;
end;
