environ
vocabularies XBOOLE_0,TARSKI,ZFMISC_1,SUBSET_1,RELAT_1;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,CKB1,CKB3,CKB6;
definitions TARSKI,XBOOLE_0,ZFMISC_1,CKB1,CKB6;
theorems TARSKI,XBOOLE_0,ZFMISC_1,ENUMSET1,XBOOLE_1,CKB1,CKB6;
schemes XBOOLE_0,CKB5;
registrations XBOOLE_0,ZFMISC_1,CKB2,CKB4,CKB7,CKB8;
constructors TARSKI,SUBSET_1,ZFMISC_1,CKB1;
requirements SUBSET,BOOLE;
begin
reserve x for set;
reserve y for set;
reserve z for set;
reserve P for Relation;
reserve R for Relation;
registration
let P;
let R;
cluster ( P \/ R ) ->  Relation-like;
coherence
proof
let x;
A1: (x in R implies (ex y st (ex z st x = [ y,z ]))) by CKB1:def 1;
(x in P implies (ex y st (ex z st x = [ y,z ]))) by CKB1:def 1;
hence (x in ( P \/ R ) implies (ex y st (ex z st x = [ y,z ]))) by A1,XBOOLE_0:def 3;
end;
end;
