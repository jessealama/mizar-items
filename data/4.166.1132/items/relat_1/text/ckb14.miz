environ
vocabularies XBOOLE_0,TARSKI,ZFMISC_1,SUBSET_1,RELAT_1;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,CKB1,CKB3,CKB6;
definitions TARSKI,XBOOLE_0,ZFMISC_1,CKB1,CKB6;
theorems TARSKI,XBOOLE_0,ZFMISC_1,ENUMSET1,XBOOLE_1,CKB1,CKB6;
schemes XBOOLE_0,CKB5;
registrations XBOOLE_0,ZFMISC_1,CKB2,CKB4,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13;
constructors TARSKI,SUBSET_1,ZFMISC_1,CKB1;
requirements SUBSET,BOOLE;
begin
reserve A for set;
reserve a for set;
reserve b for set;
reserve x for set;
reserve y for set;
reserve z for set;
reserve P for Relation;
definition
let P;
let A;
redefine pred P c= A
means
(for a holds (for b holds ([ a,b ] in P implies [ a,b ] in A)))
;compatibility
proof
thus (P c= A implies (for a holds (for b holds ([ a,b ] in P implies [ a,b ] in A))));
assume A1: (for a holds (for b holds ([ a,b ] in P implies [ a,b ] in A)));
let x;
assume A2: x in P;
then (ex y st (ex z st x = [ y,z ])) by CKB1:def 1;
hence thesis by A1,A2;
end;
end;
