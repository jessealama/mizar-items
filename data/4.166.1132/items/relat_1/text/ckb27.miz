environ
vocabularies XBOOLE_0,TARSKI,ZFMISC_1,SUBSET_1,RELAT_1;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,CKB1,CKB3,CKB6,CKB14,CKB15,CKB16,CKB20,CKB21;
definitions TARSKI,XBOOLE_0,ZFMISC_1,CKB1,CKB6,CKB14,CKB15,CKB20;
theorems TARSKI,XBOOLE_0,ZFMISC_1,ENUMSET1,XBOOLE_1,CKB1,CKB6,CKB14,CKB15,CKB17,CKB18,CKB19,CKB20,CKB22,CKB23,CKB24,CKB25,CKB26;
schemes XBOOLE_0,CKB5;
registrations XBOOLE_0,ZFMISC_1,CKB2,CKB4,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13;
constructors TARSKI,SUBSET_1,ZFMISC_1,CKB1,CKB15,CKB20;
requirements SUBSET,BOOLE;
begin
reserve a for set;
reserve b for set;
reserve x for set;
reserve y for set;
reserve z for set;
reserve R for Relation;
theorem
Th23: (R = { [ x,y ] } implies (( dom R ) = { x } & ( rng R ) = { y }))
proof
assume A1: R = { [ x,y ] };
(z in ( dom R ) iff z in { x })
proof
thus (z in ( dom R ) implies z in { x })
proof
assume z in ( dom R );
then consider b such that A2: [ z,b ] in R by CKB15:def 1;
[ z,b ] = [ x,y ] by A1,A2,TARSKI:def 1;
then z = x by ZFMISC_1:27;
hence thesis by TARSKI:def 1;
end;

assume z in { x };
then z = x by TARSKI:def 1;
then [ z,y ] in R by A1,TARSKI:def 1;
hence thesis by CKB15:def 1;
end;
hence ( dom R ) = { x } by TARSKI:1;
(z in ( rng R ) iff z in { y })
proof
thus (z in ( rng R ) implies z in { y })
proof
assume z in ( rng R );
then consider a such that A3: [ a,z ] in R by CKB20:def 1;
[ a,z ] = [ x,y ] by A1,A3,TARSKI:def 1;
then z = y by ZFMISC_1:27;
hence thesis by TARSKI:def 1;
end;

assume z in { y };
then z = y by TARSKI:def 1;
then [ x,z ] in R by A1,TARSKI:def 1;
hence thesis by CKB20:def 1;
end;
hence thesis by TARSKI:1;
end;
