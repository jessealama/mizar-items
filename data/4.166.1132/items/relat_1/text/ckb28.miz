environ
vocabularies XBOOLE_0,TARSKI,ZFMISC_1,SUBSET_1,RELAT_1;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,CKB1,CKB3,CKB6,CKB14,CKB15,CKB16,CKB20,CKB21;
definitions TARSKI,XBOOLE_0,ZFMISC_1,CKB1,CKB6,CKB14,CKB15,CKB20;
theorems TARSKI,XBOOLE_0,ZFMISC_1,ENUMSET1,XBOOLE_1,CKB1,CKB6,CKB14,CKB15,CKB17,CKB18,CKB19,CKB20,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27;
schemes XBOOLE_0,CKB5;
registrations XBOOLE_0,ZFMISC_1,CKB2,CKB4,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13;
constructors TARSKI,SUBSET_1,ZFMISC_1,CKB1,CKB15,CKB20;
requirements SUBSET,BOOLE;
begin
reserve a for set;
reserve b for set;
reserve c for set;
reserve d for set;
reserve x for set;
reserve y for set;
reserve z for set;
reserve R for Relation;
theorem
(R = { [ a,b ],[ x,y ] } implies (( dom R ) = { a,x } & ( rng R ) = { b,y }))
proof
assume A1: R = { [ a,b ],[ x,y ] };
thus ( dom R ) = { a,x }
proof
thus ( dom R ) c= { a,x }
proof
let z;
assume z in ( dom R );
then consider c such that A2: [ z,c ] in R by CKB15:def 1;
([ z,c ] = [ a,b ] or [ z,c ] = [ x,y ]) by A1,A2,TARSKI:def 2;
then (z = a or z = x) by ZFMISC_1:27;
hence thesis by TARSKI:def 2;
end;

let z;
assume z in { a,x };
then (z = a or z = x) by TARSKI:def 2;
then ([ z,b ] in R or [ z,y ] in R) by A1,TARSKI:def 2;
hence thesis by CKB15:def 1;
end;

thus ( rng R ) c= { b,y }
proof
let z;
assume z in ( rng R );
then consider d such that A3: [ d,z ] in R by CKB20:def 1;
([ d,z ] = [ a,b ] or [ d,z ] = [ x,y ]) by A1,A3,TARSKI:def 2;
then (z = b or z = y) by ZFMISC_1:27;
hence thesis by TARSKI:def 2;
end;

let z;
assume z in { b,y };
then (z = b or z = y) by TARSKI:def 2;
then ([ a,z ] in R or [ x,z ] in R) by A1,TARSKI:def 2;
hence thesis by CKB20:def 1;
end;
