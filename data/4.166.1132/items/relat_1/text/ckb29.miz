begin
reserve x for set;
reserve y for set;
reserve P for Relation;
reserve R for Relation;
theorem
Th25: (P c= R implies (( dom P ) c= ( dom R ) & ( rng P ) c= ( rng R )))
proof
assume A1: P c= R;
thus ( dom P ) c= ( dom R )
proof
let x;
assume x in ( dom P );
then (ex y st [ x,y ] in P) by CKB15:def 1;
hence thesis by A1,CKB15:def 1;
end;

let y;
assume y in ( rng P );
then (ex x st [ x,y ] in P) by CKB20:def 1;
hence thesis by A1,CKB20:def 1;
end;
