func P * R -> Relation means 
:Def8: ([ x,y ] in it iff (ex z st ([ x,z ] in P & [ z,y ] in R)));
existence
proof
defpred Z[ set,set ]
 means
(ex z st ([ $1,z ] in P & [ z,$2 ] in R));
consider Q such that A1: (for x holds (for y holds ([ x,y ] in Q iff ((x in ( dom P ) & y in ( rng R )) & Z[ x,y ])))) from CKB5:sch 1;
take Q;
let x;
let y;
thus ([ x,y ] in Q implies (ex z st ([ x,z ] in P & [ z,y ] in R))) by A1;
given z such that
A2: ([ x,z ] in P & [ z,y ] in R);

(x in ( dom P ) & y in ( rng R )) by A2,CKB15:def 1,CKB20:def 1;
hence thesis by A1,A2;
end;
uniqueness
proof
let P1;
let P2;
assume that
A3: ([ x,y ] in P1 iff (ex z st ([ x,z ] in P & [ z,y ] in R)))
and
A4: ([ x,y ] in P2 iff (ex z st ([ x,z ] in P & [ z,y ] in R)));
let x;
let y;
([ x,y ] in P1 iff (ex z st ([ x,z ] in P & [ z,y ] in R))) by A3;
hence ([ x,y ] in P1 iff [ x,y ] in P2) by A4;
end;
