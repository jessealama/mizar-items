begin
reserve X for set;
reserve Y for set;
reserve x for set;
reserve y for set;
reserve R for Relation;
theorem
Th109: ( R | ( X \ Y ) ) = ( ( R | X ) \ ( R | Y ) )
proof
let x;
let y;
hereby
assume A1: [ x,y ] in ( R | ( X \ Y ) );
then A2: x in ( X \ Y ) by CKB90:def 1;
then (not x in Y) by XBOOLE_0:def 5;
then A3: (not [ x,y ] in ( R | Y )) by CKB90:def 1;
[ x,y ] in R by A1,CKB90:def 1;
then [ x,y ] in ( R | X ) by A2,CKB90:def 1;
hence [ x,y ] in ( ( R | X ) \ ( R | Y ) ) by A3,XBOOLE_0:def 5;
end;
assume A4: [ x,y ] in ( ( R | X ) \ ( R | Y ) );
then A5: [ x,y ] in R by CKB90:def 1;
(not [ x,y ] in ( R | Y )) by A4,XBOOLE_0:def 5;
then A6: ((not x in Y) or (not [ x,y ] in R)) by CKB90:def 1;
x in X by A4,CKB90:def 1;
then x in ( X \ Y ) by A4,A6,CKB90:def 1,XBOOLE_0:def 5;
hence [ x,y ] in ( R | ( X \ Y ) ) by A5,CKB90:def 1;
end;
