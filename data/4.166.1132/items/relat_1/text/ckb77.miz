begin
reserve X for set;
reserve x for set;
reserve y for set;
reserve P1 for Relation;
reserve P2 for Relation;
reserve R for Relation;
definition
let X;
func id X -> Relation
means
:Def10: ([ x,y ] in it iff (x in X & x = y));
existence
proof
defpred Z[ set,set ]
 means
$1 = $2;
consider R such that A1: (for x holds (for y holds ([ x,y ] in R iff ((x in X & y in X) & Z[ x,y ])))) from CKB5:sch 1;
take R;
let x;
let y;
thus thesis by A1;
end;
uniqueness
proof
let P1;
let P2;
assume that
A2: ([ x,y ] in P1 iff (x in X & x = y))
and
A3: ([ x,y ] in P2 iff (x in X & x = y));
let x;
let y;
([ x,y ] in P1 iff (x in X & x = y)) by A2;
hence ([ x,y ] in P1 iff [ x,y ] in P2) by A3;
end;
end;
