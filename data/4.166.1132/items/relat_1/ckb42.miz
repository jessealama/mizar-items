begin
reserve x for set;
reserve y for set;
reserve P for Relation;
reserve R for Relation;
theorem
( ( P /\ R ) ~ ) = ( ( P ~ ) /\ ( R ~ ) )
proof
let x;
let y;
([ x,y ] in ( ( P /\ R ) ~ ) iff [ y,x ] in ( P /\ R )) by CKB39:def 1;
then ([ x,y ] in ( ( P /\ R ) ~ ) iff ([ y,x ] in P & [ y,x ] in R)) by XBOOLE_0:def 4;
then ([ x,y ] in ( ( P /\ R ) ~ ) iff ([ x,y ] in ( P ~ ) & [ x,y ] in ( R ~ ))) by CKB39:def 1;
hence thesis by XBOOLE_0:def 4;
end;
