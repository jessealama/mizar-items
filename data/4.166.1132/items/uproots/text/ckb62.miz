begin
theorem
Th46: (for L being non  degenerated non  empty multLoopStr_0 holds (for n being (Element of ( NAT )) holds (for p being (Polynomial of L) holds (n <= ( len p ) implies ( ( len ( poly_shift (p,n) ) ) + n ) = ( len p )))))
proof
let L being non  degenerated non  empty multLoopStr_0;
let n being (Element of ( NAT ));
let p being (Polynomial of L);
assume that
A1: n <= ( len p );
set ps = ( poly_shift (p,n) );
set lpn = ( ( len p ) - n );
( n - n ) <= lpn by A1,XREAL_1:9;
then reconsider lpn as (Element of ( NAT )) by INT_1:3;
A2:now
let m being Nat;
assume that
A3: m is_at_least_length_of ps
and
A4: lpn > m;
lpn >= ( m + 1 ) by A4,NAT_1:13;
then A5: ( lpn - 1 ) >= ( ( m + 1 ) - 1 ) by XREAL_1:9;
then reconsider lpn1 = ( lpn - 1 ) as (Element of ( NAT )) by INT_1:3;
( ( n + lpn1 ) + 1 ) = ( len p );
then A6: ( p . ( n + lpn1 ) ) <> ( 0. L ) by ALGSEQ_1:10;
( ps . lpn1 ) = ( p . ( n + lpn1 ) ) by CKB59:def 1;
hence contradiction by A3,A5,A6,ALGSEQ_1:def 2;
end;
now
let i being Nat;
assume i >= lpn;
then A7: ( i + n ) >= ( len p ) by XREAL_1:20;
thus ( ps . i ) = ( p . ( n + i ) ) by CKB59:def 1
.= ( 0. L ) by A7,ALGSEQ_1:8;
end;
then lpn is_at_least_length_of ps by ALGSEQ_1:def 2;
hence ( ( len ( poly_shift (p,n) ) ) + n ) = ( lpn + n ) by A2,ALGSEQ_1:def 3
.= ( len p );
end;
