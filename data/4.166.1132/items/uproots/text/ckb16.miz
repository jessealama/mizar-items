begin
theorem
Th12: (for X being set holds (for S,T being  finite (Subset of X) holds (for n being (Element of ( NAT )) holds (S misses T implies ( (( S \/ T ),n) -bag ) = ( ( (S,n) -bag ) + ( (T,n) -bag ) )))))
proof
let X being set;
let S being  finite (Subset of X);
let T being  finite (Subset of X);
let n being (Element of ( NAT ));
assume S misses T;
then A1: ( S /\ T ) = ( {} ) by XBOOLE_0:def 7;
now
let i being set;
assume that
i in X;
per cases  by XBOOLE_0:def 3;
suppose A2: (not i in ( S \/ T ));

then A3: (not i in T) by XBOOLE_0:def 3;
A4: (not i in S) by A2,XBOOLE_0:def 3;
thus ( ( (( S \/ T ),n) -bag ) . i ) = ( 0 ) by A2,CKB12:1
.= ( ( ( (S,n) -bag ) . i ) + (( 0 ) qua Nat) ) by A4,CKB12:1
.= ( ( ( (S,n) -bag ) . i ) + ( ( (T,n) -bag ) . i ) ) by A3,CKB12:1
.= ( ( ( (S,n) -bag ) + ( (T,n) -bag ) ) . i ) by PRE_POLY:def 5;
end;
suppose A5: i in S;

then A6: (not i in T) by A1,XBOOLE_0:def 4;
i in ( S \/ T ) by A5,XBOOLE_0:def 3;
hence ( ( (( S \/ T ),n) -bag ) . i ) = n by CKB13:1
.= ( ( ( (S,n) -bag ) . i ) + (( 0 ) qua Nat) ) by A5,CKB13:1
.= ( ( ( (S,n) -bag ) . i ) + ( ( (T,n) -bag ) . i ) ) by A6,CKB12:1
.= ( ( ( (S,n) -bag ) + ( (T,n) -bag ) ) . i ) by PRE_POLY:def 5;
end;
suppose A7: i in T;

then A8: (not i in S) by A1,XBOOLE_0:def 4;
i in ( S \/ T ) by A7,XBOOLE_0:def 3;
hence ( ( (( S \/ T ),n) -bag ) . i ) = n by CKB13:1
.= ( ( ( (T,n) -bag ) . i ) + (( 0 ) qua Nat) ) by A7,CKB13:1
.= ( ( ( (S,n) -bag ) . i ) + ( ( (T,n) -bag ) . i ) ) by A8,CKB12:1
.= ( ( ( (S,n) -bag ) + ( (T,n) -bag ) ) . i ) by PRE_POLY:def 5;
end;
end;
hence thesis by PBOOLE:3;
end;
