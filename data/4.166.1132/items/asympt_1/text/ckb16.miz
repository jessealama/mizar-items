begin
reserve c for Real;
theorem
Lm6: (for a,b,c being  real number holds (((( 0 ) < a & a <= b) & c >= ( 0 )) implies ( a to_power c ) <= ( b to_power c )))
proof
let a being  real number;
let b being  real number;
let c being  real number;
assume that
A1: ( 0 ) < a
and
A2: a <= b
and
A3: c >= ( 0 );
per cases  by A3;
suppose A4: c = ( 0 );

then ( a to_power c ) = 1 by POWER:24;
hence thesis by A4,POWER:24;
end;
suppose A5: c > ( 0 );

per cases  by A2,XXREAL_0:1;
suppose a = b;

hence thesis;
end;
suppose a < b;

hence thesis by A1,A5,POWER:37;
end;
end;
end;
