theorem
Lm16: (for f,g being Real_Sequence holds (for M being (Element of ( NAT )) holds (for N holds (N >= ( M + 1 ) implies ((for n holds ((( M + 1 ) <= n & n <= N) implies ( f . n ) <= ( g . n ))) implies ( Sum (f,N,M) ) <= ( Sum (g,N,M) ))))))
proof
let f being Real_Sequence;
let g being Real_Sequence;
let M being (Element of ( NAT ));
defpred P[ Nat ]
 means
((for n holds ((( M + 1 ) <= n & n <= $1) implies ( f . n ) <= ( g . n ))) implies ( Sum (f,$1,M) ) <= ( Sum (g,$1,M) ));
A1: (for N1 being Nat holds ((N1 >= ( M + 1 ) & P[ N1 ]) implies P[ ( N1 + 1 ) ]))
proof
let N1 being Nat;
assume that
A2: N1 >= ( M + 1 )
and
A3: ((for n holds ((( M + 1 ) <= n & n <= N1) implies ( f . n ) <= ( g . n ))) implies ( Sum (f,N1,M) ) <= ( Sum (g,N1,M) ));
assume A4: (for n holds ((( M + 1 ) <= n & n <= ( N1 + 1 )) implies ( f . n ) <= ( g . n )));
A5:now
let n;
assume that
A6: ( M + 1 ) <= n
and
A7: n <= N1;
( n + ( 0 ) ) <= ( N1 + 1 ) by A7,XREAL_1:7;
hence ( f . n ) <= ( g . n ) by A4,A6;
end;
( N1 + 1 ) >= ( ( M + 1 ) + ( 0 ) ) by A2,XREAL_1:7;
then ( f . ( N1 + 1 ) ) <= ( g . ( N1 + 1 ) ) by A4;
then ( ( Sum (f,N1,M) ) + ( f . ( N1 + 1 ) ) ) <= ( ( g . ( N1 + 1 ) ) + ( Sum (g,N1,M) ) ) by A3,A5,XREAL_1:7;
then ( Sum (f,( N1 + 1 ),M) ) <= ( ( g . ( N1 + 1 ) ) + ( Sum (g,N1,M) ) ) by CKB27:1;
hence thesis by CKB27:1;
end;
A8: P[ ( M + 1 ) ]
proof
A9: ( Sum (g,( M + 1 ),M) ) = ( ( Sum (g,( M + 1 )) ) - ( Sum (g,M) ) ) by SERIES_1:def 6
.= ( ( ( Partial_Sums g ) . ( M + 1 ) ) - ( Sum (g,M) ) ) by SERIES_1:def 5
.= ( ( ( g . ( M + 1 ) ) + ( ( Partial_Sums g ) . M ) ) - ( Sum (g,M) ) ) by SERIES_1:def 1
.= ( ( ( g . ( M + 1 ) ) + ( Sum (g,M) ) ) - ( Sum (g,M) ) ) by SERIES_1:def 5
.= ( ( g . ( M + 1 ) ) + ( 0 ) );
A10: ( Sum (f,( M + 1 ),M) ) = ( ( Sum (f,( M + 1 )) ) - ( Sum (f,M) ) ) by SERIES_1:def 6
.= ( ( ( Partial_Sums f ) . ( M + 1 ) ) - ( Sum (f,M) ) ) by SERIES_1:def 5
.= ( ( ( f . ( M + 1 ) ) + ( ( Partial_Sums f ) . M ) ) - ( Sum (f,M) ) ) by SERIES_1:def 1
.= ( ( ( f . ( M + 1 ) ) + ( Sum (f,M) ) ) - ( Sum (f,M) ) ) by SERIES_1:def 5
.= ( ( f . ( M + 1 ) ) + ( 0 ) );
assume (for n holds ((( M + 1 ) <= n & n <= ( M + 1 )) implies ( f . n ) <= ( g . n )));
hence thesis by A10,A9;
end;
(for N being Nat holds (N >= ( M + 1 ) implies P[ N ])) from NAT_1:sch 8(A8,A1);
hence thesis;
end;
