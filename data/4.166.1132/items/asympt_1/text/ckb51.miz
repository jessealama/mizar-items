begin
reserve c for Real;
reserve n for (Element of ( NAT ));
reserve N for (Element of ( NAT ));
reserve M for (Element of ( NAT ));
theorem
Lm22: (for f,g being Real_Sequence holds (for N being (Element of ( NAT )) holds (for c being Real holds (((f is  convergent & ( lim f ) = c) & (for n holds (n >= N implies ( f . n ) = ( g . n )))) implies (g is  convergent & ( lim g ) = c)))))
proof
let f being Real_Sequence;
let g being Real_Sequence;
let N being (Element of ( NAT ));
let c being Real;
assume that
A1: f is  convergent
and
A2: ( lim f ) = c
and
A3: (for n holds (n >= N implies ( f . n ) = ( g . n )));
A4:now
let p being  real number;
assume p > ( 0 );
then consider M such that A5: (for n holds (n >= M implies ( abs ( ( f . n ) - c ) ) < p)) by A1,A2,SEQ_2:def 7;
set N1 = ( max (N,M) );
A6: N1 >= N by XXREAL_0:25;
take N1;
let n;
assume A7: n >= N1;
N1 >= M by XXREAL_0:25;
then n >= M by A7,XXREAL_0:2;
then ( abs ( ( f . n ) - c ) ) < p by A5;
hence ( abs ( ( g . n ) - c ) ) < p by A3,A6,A7,XXREAL_0:2;
end;
hence g is  convergent by SEQ_2:def 6;
hence thesis by A4,SEQ_2:def 7;
end;
