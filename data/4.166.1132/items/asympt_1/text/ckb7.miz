theorem
Lm3: (for a,b,c being Real holds (((a > ( 0 ) & c > ( 0 )) & c <> 1) implies ( a to_power b ) = ( c to_power ( b * ( log (c,a) ) ) )))
proof
let a being Real;
let b being Real;
let c being Real;
assume that
A1: a > ( 0 )
and
A2: c > ( 0 )
