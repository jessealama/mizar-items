begin
reserve n for (Element of ( NAT ));
theorem
Lm2: (for a being  logbase Real holds (for f being Real_Sequence holds ((a > 1 & (for n holds (n > ( 0 ) implies ( f . n ) = ( log (a,n) )))) implies f is  eventually-positive)))
proof
let a being  logbase Real;
let f being Real_Sequence;
assume that
A1: a > 1
and
A2: (for n holds (n > ( 0 ) implies ( f . n ) = ( log (a,n) )));
set N = [/ a \];
A3: [/ a \] >= a by INT_1:def 7;
A4: a > ( 0 ) by ASYMPT_0:def 1;
then A5: N > ( 0 ) by INT_1:def 7;
then reconsider N as (Element of ( NAT )) by INT_1:3;
A6: a <> 1 by ASYMPT_0:def 1;
now
A7: ( log (a,N) ) >= ( log (a,a) ) by A1,A3,PRE_FF:10;
let n;
assume A8: n >= ( N + 1 );
( N + 1 ) > ( N + ( 0 ) ) by XREAL_1:8;
then n > N by A8,XXREAL_0:2;
then ( log (a,n) ) > ( log (a,N) ) by A1,A5,POWER:57;
then ( log (a,n) ) > ( 0 ) by A4,A6,A7,POWER:52;
hence ( f . n ) > ( 0 ) by A2,A8;
end;
hence thesis by ASYMPT_0:def 4;
end;
