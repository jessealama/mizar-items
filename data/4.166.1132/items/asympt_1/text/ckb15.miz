begin
reserve c for Real;
reserve n for (Element of ( NAT ));
reserve N for (Element of ( NAT ));
reserve x for set;
theorem
Th4: (for f,g being  eventually-nonnegative Real_Sequence holds ((( Big_Oh f ) c= ( Big_Oh g ) & (not ( Big_Oh f ) = ( Big_Oh g ))) iff (f in ( Big_Oh g ) & (not f in ( Big_Omega g )))))
proof
let f being  eventually-nonnegative Real_Sequence;
let g being  eventually-nonnegative Real_Sequence;
hereby
assume that
A1: ( Big_Oh f ) c= ( Big_Oh g )
and
A2: (not ( Big_Oh f ) = ( Big_Oh g ));
A3: f in ( Big_Oh f ) by ASYMPT_0:10;
now
assume f in ( Big_Omega g );
then g in ( Big_Oh f ) by ASYMPT_0:19;
hence contradiction by A1,A2,A3,CKB14:1;
end;
hence (f in ( Big_Oh g ) & (not f in ( Big_Omega g ))) by A1,A3;
end;
assume that
A4: f in ( Big_Oh g )
and
A5: (not f in ( Big_Omega g ));
now
let x being set;
assume x in ( Big_Oh f );
then consider t being (Element of ( Funcs (( NAT ),( REAL )) )) such that A6: x = t and A7: (ex c st (ex N st (c > ( 0 ) & (for n holds (n >= N implies (( t . n ) <= ( c * ( f . n ) ) & ( t . n ) >= ( 0 )))))));
consider c,N such that c > ( 0 ) and A8: (for n holds (n >= N implies (( t . n ) <= ( c * ( f . n ) ) & ( t . n ) >= ( 0 )))) by A7;
now
take N;
let n;
assume n >= N;
hence ( t . n ) >= ( 0 ) by A8;
end;
then A9: t is  eventually-nonnegative by ASYMPT_0:def 2;
t in ( Big_Oh f ) by A7;
hence x in ( Big_Oh g ) by A4,A6,A9,ASYMPT_0:12;
end;
hence ( Big_Oh f ) c= ( Big_Oh g ) by TARSKI:def 3;
assume ( Big_Oh f ) = ( Big_Oh g );
then g in ( Big_Oh f ) by CKB14:1;
hence contradiction by A5,ASYMPT_0:19;
end;
