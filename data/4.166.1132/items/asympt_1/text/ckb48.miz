begin
reserve c for Real;
reserve n for (Element of ( NAT ));
reserve N for (Element of ( NAT ));
reserve x for set;
theorem
(for f,g being  eventually-nonnegative Real_Sequence holds (( Big_Oh f ) = ( Big_Oh g ) iff f in ( Big_Theta g )))
proof
let f being  eventually-nonnegative Real_Sequence;
let g being  eventually-nonnegative Real_Sequence;
hereby
assume A1: ( Big_Oh f ) = ( Big_Oh g );
then g in ( Big_Oh f ) by ASYMPT_0:10;
then A2: f in ( Big_Omega g ) by ASYMPT_0:19;
f in ( Big_Oh g ) by A1,ASYMPT_0:10;
hence f in ( Big_Theta g ) by A2,XBOOLE_0:def 4;
end;
assume A3: f in ( Big_Theta g );
now
let x being set;
hereby
assume x in ( Big_Oh f );
then consider t being (Element of ( Funcs (( NAT ),( REAL )) )) such that A4: x = t and A5: (ex c st (ex N st (c > ( 0 ) & (for n holds (n >= N implies (( t . n ) <= ( c * ( f . n ) ) & ( t . n ) >= ( 0 )))))));
consider c,N such that c > ( 0 ) and A6: (for n holds (n >= N implies (( t . n ) <= ( c * ( f . n ) ) & ( t . n ) >= ( 0 )))) by A5;
now
take N;
let n;
assume n >= N;
hence ( t . n ) >= ( 0 ) by A6;
end;
then A7: t is  eventually-nonnegative by ASYMPT_0:def 2;
A8: f in ( Big_Oh g ) by A3,XBOOLE_0:def 4;
t in ( Big_Oh f ) by A5;
hence x in ( Big_Oh g ) by A4,A7,A8,ASYMPT_0:12;
end;
assume x in ( Big_Oh g );
then consider t being (Element of ( Funcs (( NAT ),( REAL )) )) such that A9: x = t and A10: (ex c st (ex N st (c > ( 0 ) & (for n holds (n >= N implies (( t . n ) <= ( c * ( g . n ) ) & ( t . n ) >= ( 0 )))))));
consider c,N such that c > ( 0 ) and A11: (for n holds (n >= N implies (( t . n ) <= ( c * ( g . n ) ) & ( t . n ) >= ( 0 )))) by A10;
now
take N;
let n;
assume n >= N;
hence ( t . n ) >= ( 0 ) by A11;
end;
then A12: t is  eventually-nonnegative by ASYMPT_0:def 2;
f in ( Big_Omega g ) by A3,XBOOLE_0:def 4;
then A13: g in ( Big_Oh f ) by ASYMPT_0:19;
t in ( Big_Oh g ) by A10;
hence x in ( Big_Oh f ) by A9,A12,A13,ASYMPT_0:12;
end;
hence thesis by TARSKI:1;
end;
