begin
reserve p for polyhedron;
reserve k for Integer;
theorem
Th30: ( rng ( k -polytope-seq p ) ) = ( k -polytopes p )
proof
set F = (the PolytopsF of p);
per cases ;
suppose A1: k < ( - 1 );

then ( k -polytopes p ) = ( {} ) by CKB40:def 1;
hence thesis by A1,CKB45:def 1,RELAT_1:38;
end;
suppose A2: (( - 1 ) <= k & k <= ( dim p ));

per cases  by A2,XXREAL_0:1;
suppose k = ( - 1 );

then (( k -polytopes p ) = { ( {} ) } & ( k -polytope-seq p ) = <* ( {} ) *>) by CKB40:def 1,CKB45:def 1;
hence thesis by FINSEQ_1:38;
end;
suppose A3: (( - 1 ) < k & k < ( dim p ));

then ( k -polytopes p ) = ( rng ( F . ( k + 1 ) ) ) by CKB40:def 1;
hence thesis by A3,CKB45:def 1;
end;
suppose k = ( dim p );

then (( k -polytopes p ) = { p } & ( k -polytope-seq p ) = <* p *>) by CKB40:def 1,CKB45:def 1;
hence thesis by FINSEQ_1:38;
end;
end;
suppose A4: k > ( dim p );

then ( k -polytopes p ) = ( {} ) by CKB40:def 1;
hence thesis by A4,CKB45:def 1,RELAT_1:38;
end;
end;
