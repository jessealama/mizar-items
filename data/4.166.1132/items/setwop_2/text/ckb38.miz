begin
reserve D for non  empty set;
reserve E for non  empty set;
reserve d for (Element of D);
reserve d1 for (Element of D);
reserve d2 for (Element of D);
reserve F for (BinOp of D);
reserve H for (BinOp of E);
reserve h for (Function of D,E);
reserve p for (FinSequence of D);
reserve q for (FinSequence of D);
theorem
Th39: ((((F is  having_a_unity & H is  having_a_unity) & ( h . ( the_unity_wrt F ) ) = ( the_unity_wrt H )) & (for d1 holds (for d2 holds ( h . ( F . (d1,d2) ) ) = ( H . (( h . d1 ),( h . d2 )) )))) implies ( h . ( F "**" p ) ) = ( H "**" ( h * p ) ))
proof
assume that
A1: F is  having_a_unity
and
A2: H is  having_a_unity
and
A3: ( h . ( the_unity_wrt F ) ) = ( the_unity_wrt H )
and
A4: (for d1 holds (for d2 holds ( h . ( F . (d1,d2) ) ) = ( H . (( h . d1 ),( h . d2 )) )));
defpred X[ (FinSequence of D) ]
 means
( h . ( F "**" $1 ) ) = ( H "**" ( h * $1 ) );
A5: (for q holds (for d holds (X[ q ] implies X[ ( q ^ <* d *> ) ])))
proof
let q;
let d;
assume that
A6: ( h . ( F "**" q ) ) = ( H "**" ( h * q ) );
thus ( h . ( F "**" ( q ^ <* d *> ) ) ) = ( h . ( F . (( F "**" q ),d) ) ) by A1,FINSOP_1:4
.= ( H . (( H "**" ( h * q ) ),( h . d )) ) by A4,A6
.= ( H "**" ( ( h * q ) ^ <* ( h . d ) *> ) ) by A2,FINSOP_1:4
.= ( H "**" ( h * ( q ^ <* d *> ) ) ) by FINSEQOP:8;
end;
( h . ( F "**" ( <*> D ) ) ) = ( h . ( the_unity_wrt F ) ) by A1,FINSOP_1:10
.= ( H "**" ( <*> E ) ) by A2,A3,FINSOP_1:10
.= ( H "**" ( h * ( <*> D ) ) ) by RELAT_1:39;
then A7: X[ ( <*> D ) ];
X[ q ] from FINSEQ_2:sch 2(A7,A5);
hence thesis;
end;
