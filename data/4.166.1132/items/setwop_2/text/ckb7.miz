definitions BINOP_1,XBOOLE_0,RELAT_1;
theorems TARSKI,ENUMSET1,ZFMISC_1,NAT_1,FUNCT_1,FUNCT_2,FUNCOP_1,FINSEQ_1,FINSUB_1,BINOP_1,SETWISEO,FINSEQ_2,WELLORD2,CARD_1,FINSEQOP,RELAT_1,FUNCT_4,FINSOP_1,XBOOLE_0,XBOOLE_1,XREAL_1,ORDINAL1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6;
