begin
reserve GX for non  empty TopSpace;
reserve B for (Subset of GX);
theorem
Th27: (for B being (Subset of GX) holds (for p being (Point of GX) holds (p in B implies ( Component_of (p,B) ) = ( Component_of ( Down (p,B) ) ))))
proof
let B being (Subset of GX);
let p being (Point of GX);
assume A1: p in B;
then p = ( Down (p,B) ) by CKB28:def 1;
hence thesis by A1,CKB32:def 1;
end;
