begin
reserve n for (Element of ( NAT ));
reserve K for Field;
registration
let K;
let n;
cluster ( 0. (K,n) ) ->  anti-circular;
coherence
proof
set p = ( n |-> ( 0. K ) );
A1: ( width ( 0. (K,n) ) ) = n by MATRIX_1:24;
A2: ( 0. (K,n,n) ) = ( n |-> ( n |-> ( 0. K ) ) );
set M1 = ( 0. (K,n) );
A3: ( len ( n |-> ( 0. K ) ) ) = n by CARD_1:def 7;
A4: ( Indices ( 0. (K,n) ) ) = [: ( Seg n ),( Seg n ) :] by MATRIX_1:24;
A5: (for i,j being Nat holds (([ i,j ] in ( Indices M1 ) & i >= j) implies ( M1 * (i,j) ) = ( ( - p ) . ( ( ( j - i ) mod ( len p ) ) + 1 ) )))
proof
let i being Nat,j being Nat;
assume that
A6: [ i,j ] in ( Indices M1 )
and
i >= j;
A7: ( ( ( j - i ) mod n ) + 1 ) in ( Seg n ) by A4,A6,CKB3:1;
( ( - p ) . ( ( ( j - i ) mod ( len p ) ) + 1 ) ) = ( ( n |-> ( - ( 0. K ) ) ) . ( ( ( j - i ) mod n ) + 1 ) ) by A3,FVSUM_1:25
.= ( - ( 0. K ) ) by A7,FUNCOP_1:7
.= ( 0. K ) by VECTSP_2:3;
hence thesis by A2,A6,MATRIX_3:1;
end;
(for i,j being Nat holds (([ i,j ] in ( Indices M1 ) & i <= j) implies ( M1 * (i,j) ) = ( p . ( ( ( j - i ) mod ( len p ) ) + 1 ) )))
proof
let i being Nat,j being Nat;
assume that
A8: [ i,j ] in ( Indices M1 )
and
i <= j;
( ( ( j - i ) mod n ) + 1 ) in ( Seg n ) by A4,A8,CKB3:1;
then ( ( ( Seg n ) --> ( 0. K ) ) . ( ( ( j - i ) mod n ) + 1 ) ) = ( 0. K ) by FUNCOP_1:7;
hence thesis by A3,A2,A8,MATRIX_3:1;
end;
then M1 is_anti-circular_about p by A1,A3,A5,CKB68:def 1;
then consider p being (FinSequence of K) such that A9: (( len p ) = ( width M1 ) & M1 is_anti-circular_about p) by A1,A3;
take p;
thus thesis by A9;
end;
end;
