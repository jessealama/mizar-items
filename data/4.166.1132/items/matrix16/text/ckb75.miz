environ
vocabularies NUMBERS,SUBSET_1,VECTSP_1,FINSEQ_1,MATRIX_1,NAT_1,XXREAL_0,ARYTM_1,INT_1,ARYTM_3,CARD_1,ZFMISC_1,GROUP_1,RELAT_1,FUNCT_1,TARSKI,STRUCT_0,ALGSTR_0,FUNCOP_1,FVSUM_1,SUPINF_2,FINSEQ_2,TREES_1,XBOOLE_0,QC_LANG1,INCSP_1,PARTFUN1,MESFUNC1,MATRIX16;
notations MATRIX_1,VECTSP_1,GROUP_1,XBOOLE_0,ZFMISC_1,SUBSET_1,NAT_1,TARSKI,FINSEQ_1,FINSEQ_2,MATRIX_3,FVSUM_1,FUNCT_1,STRUCT_0,BINOP_1,XXREAL_0,MATRIX_2,FUNCOP_1,INT_1,NUMBERS,XCMPLX_0,MATRIX_6,RELAT_1,ALGSTR_0,PARTFUN1,MATRIX13,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB67,CKB68,CKB69,CKB70,CKB71;
definitions MATRIX_1,MATRIX_3,FVSUM_1,MATRIX_4,FINSEQ_1,FINSEQ_2,ALGSTR_0,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB68,CKB69,CKB70,CKB71;
theorems MATRIX_5,MATRIX_3,FINSEQ_1,FUNCT_1,ZFMISC_1,POLYNOM1,NAT_1,RLVECT_1,MATRIX_2,INT_1,VECTSP_1,FVSUM_1,MATRIX_1,FINSEQ_2,FUNCOP_1,MATRIXR1,XREAL_1,XXREAL_0,ORDINAL1,VECTSP_2,PARTFUN1,FINTOPO5,CARD_1,NAT_D,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB37,CKB38,CKB39,CKB40,CKB41,CKB42,CKB43,CKB44,CKB45,CKB46,CKB47,CKB48,CKB49,CKB50,CKB51,CKB52,CKB53,CKB54,CKB55,CKB56,CKB57,CKB58,CKB59,CKB60,CKB61,CKB62,CKB63,CKB64,CKB65,CKB66,CKB68,CKB69,CKB70,CKB71,CKB72,CKB73,CKB74;
registrations STRUCT_0,INT_1,RELSET_1,VECTSP_1,FINSEQ_2,XXREAL_0,FUNCOP_1,XREAL_0,XBOOLE_0,FUNCT_1,ORDINAL1,CARD_1,CKB14,CKB15,CKB16,CKB17,CKB18;
constructors REAL_1,MATRIX_6,XXREAL_0,MATRIX13,POLYNOM1,BINOP_2,FVSUM_1,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB68,CKB69,CKB70,CKB71;
requirements REAL,NUMERALS,BOOLE,SUBSET,ARITHM;
begin
reserve i for (Element of ( NAT ));
reserve j for (Element of ( NAT ));
reserve k for (Element of ( NAT ));
reserve n for (Element of ( NAT ));
reserve l for (Element of ( NAT ));
reserve K for Field;
reserve p for (FinSequence of K);
reserve M1 for (Matrix of n,K);
theorem
((((((M1 is  anti-circular & [ i,j ] in [: ( Seg n ),( Seg n ) :]) & k = ( i + 1 )) & l = ( j + 1 )) & i < n) & j < n) implies ( M1 * (k,l) ) = ( M1 * (i,j) ))
proof
now
per cases ;
case A1: i <= j;
then A2: ( i + 1 ) <= ( j + 1 ) by XREAL_1:6;
A3: ( Indices M1 ) = [: ( Seg n ),( Seg n ) :] by MATRIX_1:24;
assume that
A4: M1 is  anti-circular
and
A5: [ i,j ] in [: ( Seg n ),( Seg n ) :]
and
A6: k = ( i + 1 )
and
A7: l = ( j + 1 )
and
A8: i < n
and
A9: j < n;
j in ( Seg n ) by A5,ZFMISC_1:87;
then 1 <= j by FINSEQ_1:1;
then ( 1 + 1 ) <= ( j + 1 ) by XREAL_1:6;
then A10: 1 <= ( j + 1 ) by XXREAL_0:2;
( j + 1 ) <= n by A9,INT_1:7;
then A11: l in ( Seg n ) by A7,A10;
i in ( Seg n ) by A5,ZFMISC_1:87;
then 1 <= i by FINSEQ_1:1;
then ( 1 + 1 ) <= ( i + 1 ) by XREAL_1:6;
then A12: 1 <= ( i + 1 ) by XXREAL_0:2;
consider p being (FinSequence of K) such that ( len p ) = ( width M1 ) and A13: M1 is_anti-circular_about p by A4,CKB69:def 1;
( i + 1 ) <= n by A8,INT_1:7;
then k in ( Seg n ) by A6,A12;
then [ k,l ] in ( Indices M1 ) by A3,A11,ZFMISC_1:87;
then ( M1 * (k,l) ) = ( p . ( ( ( l - k ) mod ( len p ) ) + 1 ) ) by A6,A7,A13,A2,CKB68:def 1
.= ( p . ( ( ( j - i ) mod ( len p ) ) + 1 ) ) by A6,A7
.= ( M1 * (i,j) ) by A1,A5,A13,A3,CKB68:def 1;
hence thesis;
end;
case A14: i >= j;
then A15: ( i + 1 ) >= ( j + 1 ) by XREAL_1:6;
A16: ( Indices M1 ) = [: ( Seg n ),( Seg n ) :] by MATRIX_1:24;
assume that
A17: M1 is  anti-circular
and
A18: [ i,j ] in [: ( Seg n ),( Seg n ) :]
and
A19: k = ( i + 1 )
and
A20: l = ( j + 1 )
and
A21: i < n
and
A22: j < n;
j in ( Seg n ) by A18,ZFMISC_1:87;
then 1 <= j by FINSEQ_1:1;
then ( 1 + 1 ) <= ( j + 1 ) by XREAL_1:6;
then A23: 1 <= ( j + 1 ) by XXREAL_0:2;
( j + 1 ) <= n by A22,INT_1:7;
then A24: l in ( Seg n ) by A20,A23;
i in ( Seg n ) by A18,ZFMISC_1:87;
then 1 <= i by FINSEQ_1:1;
then ( 1 + 1 ) <= ( i + 1 ) by XREAL_1:6;
then A25: 1 <= ( i + 1 ) by XXREAL_0:2;
consider p being (FinSequence of K) such that ( len p ) = ( width M1 ) and A26: M1 is_anti-circular_about p by A17,CKB69:def 1;
( i + 1 ) <= n by A21,INT_1:7;
then k in ( Seg n ) by A19,A25;
then [ k,l ] in ( Indices M1 ) by A16,A24,ZFMISC_1:87;
then ( M1 * (k,l) ) = ( ( - p ) . ( ( ( l - k ) mod ( len p ) ) + 1 ) ) by A19,A20,A26,A15,CKB68:def 1
.= ( ( - p ) . ( ( ( j - i ) mod ( len p ) ) + 1 ) ) by A19,A20
.= ( M1 * (i,j) ) by A14,A18,A26,A16,CKB68:def 1;
hence thesis;
end;
end;
hence thesis;
end;
