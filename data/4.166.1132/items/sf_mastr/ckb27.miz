begin
reserve a for Int-Location;
reserve b for Int-Location;
reserve l for (Element of ( NAT ));
reserve f for FinSeq-Location;
reserve i for (Instruction of ( SCM+FSA ));
reserve k for (Element of ( NAT ));
theorem
Th27: ( UsedIntLoc i ) = ( UsedIntLoc ( IncAddr (i,k) ) )
proof
A1: ( InsCode i ) <= 12 by SCMFSA_2:16;
per cases  by A1,NAT_1:36;
suppose ( InsCode i ) = ( 0 );

then i = ( halt ( SCM+FSA ) ) by SCMFSA_2:95;
hence thesis by COMPOS_1:11;
end;
suppose ( InsCode i ) = 1;

then (ex a st (ex b st i = ( a := b ))) by SCMFSA_2:30;
hence thesis by COMPOS_1:11;
end;
suppose ( InsCode i ) = 2;

then (ex a st (ex b st i = ( AddTo (a,b) ))) by SCMFSA_2:31;
hence thesis by COMPOS_1:11;
end;
suppose ( InsCode i ) = 3;

then (ex a st (ex b st i = ( SubFrom (a,b) ))) by SCMFSA_2:32;
hence thesis by COMPOS_1:11;
end;
suppose ( InsCode i ) = 4;

then (ex a st (ex b st i = ( MultBy (a,b) ))) by SCMFSA_2:33;
hence thesis by COMPOS_1:11;
end;
suppose ( InsCode i ) = 5;

then (ex a st (ex b st i = ( Divide (a,b) ))) by SCMFSA_2:34;
hence thesis by COMPOS_1:11;
end;
suppose ( InsCode i ) = 6;

then consider l such that A4: i = ( goto l ) by SCMFSA_2:35;
( IncAddr (i,k) ) = ( goto ( l + k ) ) by A4,SCMFSA_4:1;
hence ( UsedIntLoc ( IncAddr (i,k) ) ) = ( {} ) by CKB17:1
.= ( UsedIntLoc i ) by A4,CKB17:1;
end;
suppose ( InsCode i ) = 7;

then consider l,a such that A5: i = ( a =0_goto l ) by SCMFSA_2:36;
( IncAddr (i,k) ) = ( a =0_goto ( l + k ) ) by A5,SCMFSA_4:2;
hence ( UsedIntLoc ( IncAddr (i,k) ) ) = { a } by CKB18:1
.= ( UsedIntLoc i ) by A5,CKB18:1;
end;
suppose ( InsCode i ) = 8;

then consider l,a such that A6: i = ( a >0_goto l ) by SCMFSA_2:37;
( IncAddr (i,k) ) = ( a >0_goto ( l + k ) ) by A6,SCMFSA_4:3;
hence ( UsedIntLoc ( IncAddr (i,k) ) ) = { a } by CKB18:1
.= ( UsedIntLoc i ) by A6,CKB18:1;
end;
suppose ( InsCode i ) = 9;

then (ex a st (ex b st (ex f st i = ( b := (f,a) )))) by SCMFSA_2:38;
hence thesis by COMPOS_1:11;
end;
suppose ( InsCode i ) = 10;

then (ex a st (ex b st (ex f st i = ( (f,a) := b )))) by SCMFSA_2:39;
hence thesis by COMPOS_1:11;
end;
suppose ( InsCode i ) = 11;

then (ex a st (ex f st i = ( a :=len f ))) by SCMFSA_2:40;
hence thesis by COMPOS_1:11;
end;
suppose ( InsCode i ) = 12;

then (ex a st (ex f st i = ( f :=<0,...,0> a ))) by SCMFSA_2:41;
hence thesis by COMPOS_1:11;
end;
end;
