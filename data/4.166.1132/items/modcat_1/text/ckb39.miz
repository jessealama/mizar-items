begin
reserve UN for Universe;
reserve R for Ring;
theorem
Th18: (for f,g being (Morphism of ( LModCat (UN,R) )) holds (for f9,g9 being (Element of ( Morphs ( LModObjects (UN,R) ) )) holds ((f = f9 & g = g9) implies (((((( dom g ) = ( cod f ) iff ( dom g9 ) = ( cod f9 )) & (( dom g ) = ( cod f ) iff [ g9,f9 ] in ( dom ( comp ( LModObjects (UN,R) ) ) ))) & (( dom g ) = ( cod f ) implies ( g * f ) = ( g9 * f9 ))) & (( dom f ) = ( dom g ) iff ( dom f9 ) = ( dom g9 ))) & (( cod f ) = ( cod g ) iff ( cod f9 ) = ( cod g9 ))))))
proof
set C = ( LModCat (UN,R) );
set V = ( LModObjects (UN,R) );
set X = ( Morphs V );
let f being (Morphism of C);
let g being (Morphism of C);
let f9 being (Element of X);
let g9 being (Element of X);
assume that
A1: f = f9
and
A2: g = g9;
A3: ( cod f ) = ( cod f9 ) by A1,CKB38:1;
hence (( dom g ) = ( cod f ) iff ( dom g9 ) = ( cod f9 )) by A2,CKB38:1;
( dom g ) = ( dom g9 ) by A2,CKB38:1;
hence A4: (( dom g ) = ( cod f ) iff [ g9,f9 ] in ( dom ( comp V ) )) by A3,CKB32:1;
thus (( dom g ) = ( cod f ) implies ( g * f ) = ( g9 * f9 ))
proof
assume A5: ( dom g ) = ( cod f );
then [ g,f ] in ( dom (the Comp of C) ) by CKB35:1;
hence ( g * f ) = ( ( comp V ) . (g9,f9) ) by A1,A2,CAT_1:def 1
.= ( g9 * f9 ) by A4,A5,CKB31:def 1;
end;

( dom f ) = ( dom f9 ) by A1,CKB38:1;
hence (( dom f ) = ( dom g ) iff ( dom f9 ) = ( dom g9 )) by A2,CKB38:1;
( cod g ) = ( cod g9 ) by A2,CKB38:1;
hence thesis by A1,CKB38:1;
end;
