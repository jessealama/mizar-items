environ
vocabularies XBOOLE_0,CLASSES2,FUNCSDOM,VECTSP_2,SUBSET_1,STRUCT_0,MOD_2,MIDSP_1,CAT_1,GRCAT_1,FUNCT_2,ENS_1,RELAT_1,GRAPH_1,FUNCT_1,ALGSTR_0,VECTSP_1,ZFMISC_1,FUNCT_5,MCART_1,TARSKI,ARYTM_3,PARTFUN1,MODCAT_1,MSSUBFAM,UNIALG_1;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,RELAT_1,FUNCT_2,PARTFUN1,BINOP_1,FUNCT_5,STRUCT_0,ALGSTR_0,VECTSP_1,FUNCSDOM,VECTSP_2,GRAPH_1,CAT_1,CLASSES2,GRCAT_1,MOD_2,FUNCT_3,CKB1,CKB2,CKB4,CKB5,CKB8,CKB11,CKB12,CKB13;
definitions ALGSTR_0,GRAPH_1,CKB1,CKB4,CKB8,CKB11,CKB13;
theorems CAT_1,GRCAT_1,FUNCT_2,MOD_2,TARSKI,ZFMISC_1,XBOOLE_0,BINOP_1,RELAT_1,SUBSET_1,CKB1,CKB4,CKB7,CKB8,CKB9,CKB10,CKB11,CKB13,CKB14,CKB15;
schemes FUNCT_2,BINOP_1,TARSKI;
registrations XBOOLE_0,RELSET_1,STRUCT_0,CAT_1,VECTSP_2,GRCAT_1,MOD_2,CKB3,CKB6;
constructors GRCAT_1,MOD_2,RELSET_1,CKB1,CKB2,CKB4,CKB5,CKB8,CKB11,CKB12,CKB13;
requirements SUBSET,BOOLE;
begin
reserve x for set;
reserve y for set;
reserve UN for Universe;
reserve R for Ring;
definition
let UN;
let R;
func LModObjects (UN,R) -> set
means
:Def6: (for y holds (y in it iff (ex x st (x in { [ G,f ] where G is (Element of ( GroupObjects UN )),f is (Element of ( Funcs ([: (the carrier of R),1 :],1) )): (not contradiction) } &  GO x,y,R))));
existence
proof
defpred P[ set,set ]
 means
 GO $1,$2,R;
set N = { [ G,f ] where G is (Element of ( GroupObjects UN )),f is (Element of ( Funcs ([: (the carrier of R),1 :],1) )): (not contradiction) };
A1: (for x,y1,y2 being set holds ((P[ x,y1 ] & P[ x,y2 ]) implies y1 = y2)) by CKB14:1;
consider Y being set such that A2: (for y holds (y in Y iff (ex x st (x in N & P[ x,y ])))) from TARSKI:sch 1(A1);
take Y;
thus thesis by A2;
end;
uniqueness
proof
set N = { [ G,f ] where G is (Element of ( GroupObjects UN )),f is (Element of ( Funcs ([: (the carrier of R),1 :],1) )): (not contradiction) };
let Y1 being set,Y2 being set;
assume that
A3: (for y holds (y in Y1 iff (ex x st (x in N &  GO x,y,R))))
and
A4: (for y holds (y in Y2 iff (ex x st (x in N &  GO x,y,R))));
now
let y;
(y in Y1 iff (ex x st (x in N &  GO x,y,R))) by A3;
hence (y in Y1 iff y in Y2) by A4;
end;
hence thesis by TARSKI:1;
end;
end;
