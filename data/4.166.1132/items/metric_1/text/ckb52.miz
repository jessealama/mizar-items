begin
reserve r for  real number;
theorem
Th16: (for M being MetrStruct holds (for p being (Element of M) holds ( Sphere (p,r) ) c= ( cl_Ball (p,r) )))
proof
let M being MetrStruct;
let p being (Element of M);
per cases ;
suppose M is non  empty;

then consider M9 being non  empty MetrStruct,p9 being (Element of M9) such that A1: (M9 = M & p9 = p) and ( Sphere (p,r) ) = { q where q is (Element of M9): ( dist (p9,q) ) = r } by CKB44:def 1;
now
let x being (Element of M9);
assume x in ( Sphere (p9,r) );
then ( dist (p9,x) ) = r by CKB50:1;
then x in { q where q is (Element of M9): ( dist (p9,q) ) <= r };
hence x in ( cl_Ball (p9,r) ) by CKB46:1;
end;
hence thesis by A1,SUBSET_1:2;
end;
suppose A2: M is  empty;

then ( Sphere (p,r) ) is  empty by CKB44:def 1;
hence thesis by A2,CKB43:def 1;
end;
end;
