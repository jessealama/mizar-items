environ
vocabularies NUMBERS,STRUCT_0,FUNCT_1,ZFMISC_1,XBOOLE_0,PARTFUN1,SUBSET_1,REAL_1,RELAT_1,CARD_1,FUNCT_5,TARSKI,VALUED_0,ORDINAL1,XXREAL_0,ARYTM_3,RELAT_2,FUNCT_3,COMPLEX1,ARYTM_1,XREAL_0,METRIC_1,FUNCOP_1;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,ORDINAL1,NUMBERS,XCMPLX_0,XXREAL_0,XREAL_0,COMPLEX1,REAL_1,RELAT_1,FUNCOP_1,FUNCT_1,PARTFUN1,FUNCT_2,FUNCT_3,BINOP_1,FUNCT_5,VALUED_0,STRUCT_0,CKB1,CKB3,CKB4,CKB5,CKB6,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB22,CKB27,CKB31,CKB32;
definitions STRUCT_0,XBOOLE_0,BINOP_1,FUNCT_5,CKB4,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB31,CKB32;
theorems TARSKI,BINOP_1,ZFMISC_1,FUNCT_2,RELSET_1,RELAT_1,FUNCT_3,ABSVALUE,SUBSET_1,PARTFUN1,CARD_1,FUNCT_1,XBOOLE_0,XBOOLE_1,XREAL_1,COMPLEX1,XXREAL_0,SQUARE_1,FUNCOP_1,CKB4,CKB7,CKB8,CKB9,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB23,CKB24,CKB25,CKB26,CKB28,CKB29,CKB30,CKB31,CKB32;
schemes FRAENKEL,BINOP_1;
registrations XBOOLE_0,RELAT_1,FUNCT_1,NUMBERS,XREAL_0,MEMBERED,STRUCT_0,VALUED_0,FUNCT_2,PARTFUN1,RELSET_1,CKB1,CKB2,CKB10,CKB11,CKB21,CKB33;
constructors BINOP_1,FUNCT_3,XXREAL_0,REAL_1,COMPLEX1,STRUCT_0,VALUED_1,FUNCT_5,PARTFUN1,RELSET_1,FUNCOP_1,CKB1,CKB3,CKB4,CKB6,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB27,CKB31,CKB32;
requirements BOOLE,REAL,NUMERALS,SUBSET,ARITHM;
begin
registration
let A being set;
cluster ( DiscreteSpace A ) ->  Reflexive  discerning  symmetric  triangle;
coherence
proof
set M = MetrStruct (# A,( discrete_dist A ) #);
A1: (the distance of M) is  discerning
proof
let a being (Element of M);
let b being (Element of M);
assume ( (the distance of M) . (a,b) ) = ( 0 );
hence thesis by CKB31:def 1;
end;
A2: (the distance of M) is  symmetric
proof
let a being (Element of M);
let b being (Element of M);
now
per cases ;
suppose A3: a <> b;

hence ( (the distance of M) . (a,b) ) = 1 by CKB31:def 1
.= ( (the distance of M) . (b,a) ) by A3,CKB31:def 1;
end;
suppose a = b;

hence thesis;
end;
end;
hence thesis;
end;
A4: (the distance of M) is  triangle
proof
let a being (Element of M);
let b being (Element of M);
let c being (Element of M);
A5: (( (the distance of M) . (a,b) ) = ( 0 ) iff a = b) by CKB31:def 1;
per cases ;
suppose (a = b & a = c);

hence thesis by A5;
end;
suppose (a = b & a <> c);

hence thesis by A5;
end;
suppose A6: (a = c & a <> b);

then A7: ( (the distance of M) . (b,c) ) = 1 by CKB31:def 1;
(( (the distance of M) . (a,c) ) = ( 0 ) & ( (the distance of M) . (a,b) ) = 1) by A6,CKB31:def 1;
hence thesis by A7;
end;
suppose A8: (b = c & a <> c);

then ( (the distance of M) . (b,c) ) = ( 0 ) by CKB31:def 1;
hence thesis by A8;
end;
suppose A9: ((a <> b & a <> c) & b <> c);

then A10: ( (the distance of M) . (b,c) ) = 1 by CKB31:def 1;
(( (the distance of M) . (a,c) ) = 1 & ( (the distance of M) . (a,b) ) = 1) by A9,CKB31:def 1;
hence thesis by A10;
end;
end;
(the distance of M) is  Reflexive
proof
let a being (Element of M);
thus thesis by CKB31:def 1;
end;
hence thesis by A1,A2,A4,CKB17:def 1,CKB18:def 1,CKB19:def 1,CKB20:def 1;
end;
end;
