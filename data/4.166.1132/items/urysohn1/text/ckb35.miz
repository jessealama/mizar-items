begin
theorem
(for T being non  empty TopSpace holds (T is  normal implies (for A,B being (Subset of T) holds ((((A is  open & B is  closed) & B <> ( {} )) & B c= A) implies (ex C being (Subset of T) st ((C is  open & B c= C) & ( Cl C ) c= A))))))
proof
let T being non  empty TopSpace;
assume A1: T is  normal;
let A being (Subset of T);
let B being (Subset of T);
assume that
A2: A is  open
and
A3: (B is  closed & B <> ( {} ))
and
A4: B c= A;
per cases ;
suppose A5: A <> ( [#] T );

reconsider V = ( ( [#] T ) \ A ) as (Subset of T);
A6: A = ( ( [#] T ) \ V ) by PRE_TOPC:3;
then A7: ( ( [#] T ) \ A ) <> ( {} ) by A5;
A8: B misses V
proof
assume ( B /\ V ) <> ( {} );
then consider z being set such that A9: z in ( B /\ V ) by XBOOLE_0:def 1;
(z in B & z in V) by A9,XBOOLE_0:def 4;
hence thesis by A4,XBOOLE_0:def 5;
end;
V is  closed by A2,A6,PRE_TOPC:def 3;
then consider C,Q being (Subset of T) such that A10: C is  open and A11: Q is  open and A12: B c= C and A13: V c= Q and A14: C misses Q by A1,A3,A7,A8,COMPTS_1:def 3;
C c= ( Q ` ) by A14,SUBSET_1:23;
then ( Cl C ) c= ( Q ` ) by A11,TOPS_1:5;
then Q misses ( Cl C ) by SUBSET_1:23;
then A15: V misses ( Cl C ) by A13,XBOOLE_1:63;
take C;
thus (C is  open & B c= C) by A10,A12;
( ( A ` ) ` ) = A;
hence thesis by A15,SUBSET_1:23;
end;
suppose A16: A = ( [#] T );

take ( [#] T );
thus thesis by A16;
end;
end;
