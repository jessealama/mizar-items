begin
definition
let n being Nat;
func dyad n -> FinSequence
means
:Def6: (( dom it ) = ( Seg ( ( 2 |^ n ) + 1 ) ) & (for i being (Element of ( NAT )) holds (i in ( dom it ) implies ( it . i ) = ( ( i - 1 ) / ( 2 |^ n ) ))));
existence
proof
deffunc F(Nat) = ( ( $1 - 1 ) / ( 2 |^ n ) );
consider FS being FinSequence such that A1: (( len FS ) = ( ( 2 |^ n ) + 1 ) & (for i being Nat holds (i in ( dom FS ) implies ( FS . i ) = F(i)))) from FINSEQ_1:sch 2;
take FS;
thus thesis by A1,FINSEQ_1:def 3;
end;
uniqueness
proof
let F1 being FinSequence,F2 being FinSequence;
assume that
A1: ( dom F1 ) = ( Seg ( ( 2 |^ n ) + 1 ) )
and
A2: (for i being (Element of ( NAT )) holds (i in ( dom F1 ) implies ( F1 . i ) = ( ( i - 1 ) / ( 2 |^ n ) )))
and
A3: ( dom F2 ) = ( Seg ( ( 2 |^ n ) + 1 ) )
and
A4: (for i being (Element of ( NAT )) holds (i in ( dom F2 ) implies ( F2 . i ) = ( ( i - 1 ) / ( 2 |^ n ) )));
consider X being set such that A5: X = ( dom F1 );
(for k being Nat holds (k in X implies ( F1 . k ) = ( F2 . k )))
proof
let k being Nat;
assume A6: k in X;
hence ( F1 . k ) = ( ( k - 1 ) / ( 2 |^ n ) ) by A2,A5
.= ( F2 . k ) by A1,A3,A4,A5,A6;
end;
hence thesis by A1,A3,A5,FINSEQ_1:13;
end;
end;
