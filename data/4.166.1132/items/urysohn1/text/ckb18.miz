begin
definition
let n being Nat;
let x being (Element of ( dyadic n ));
func axis x -> (Element of ( NAT ))
means
:Def7: x = ( it / ( 2 |^ n ) );
existence
proof
consider i being (Element of ( NAT )) such that i <= ( 2 |^ n ) and A1: x = ( i / ( 2 |^ n ) ) by CKB1:def 1;
take i;
thus thesis by A1;
end;
uniqueness
proof
let i1 being (Element of ( NAT )),i2 being (Element of ( NAT ));
assume A2: (x = ( i1 / ( 2 |^ n ) ) & x = ( i2 / ( 2 |^ n ) ));
A3: ( 2 |^ n ) <> ( 0 ) by NEWTON:83;
then i1 = ( ( i1 / ( 2 |^ n ) ) * ( 2 |^ n ) ) by XCMPLX_1:87
.= i2 by A2,A3,XCMPLX_1:87;
hence thesis;
end;
end;
