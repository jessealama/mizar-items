registrations SUBSET_1,RELSET_1,STRUCT_0,VECTSP_1,VECTSP_4,HAHNBAN1,VECTSP10,FUNCT_2,ZFMISC_1,CKB42,CKB43,CKB44,CKB45,CKB46,CKB47,CKB49,CKB50,CKB51,CKB52,CKB53,CKB54,CKB55,CKB56,CKB57,CKB58,CKB59,CKB60,CKB61,CKB62,CKB63,CKB64,CKB65,CKB66,CKB67,CKB68,CKB69,CKB70,CKB71,CKB72,CKB73,CKB74,CKB75,CKB76,CKB95,CKB96,CKB97,CKB98,CKB99,CKB100,CKB101,CKB102;
constructors BORSUK_1,VECTSP10,FUNCOP_1,CKB2,CKB3,CKB4,CKB5,CKB7,CKB9,CKB17,CKB18,CKB31,CKB38,CKB39,CKB40,CKB41,CKB92,CKB93,CKB94,CKB106,CKB107;
requirements SUBSET,BOOLE;
begin
definition
let K being  add-associative  right_zeroed  right_complementable  Abelian  associative  well-unital  distributive non  empty doubleLoopStr;
let V being (VectSp of K);
let W being non  empty VectSpStr over K;
let f being  additiveSAF  homogeneousSAF (Form of V,W);
func LQForm f ->  additiveSAF  homogeneousSAF (Form of ( VectQuot (V,( LKer f )) ),W) means 
:Def21: (for A being (Vector of ( VectQuot (V,( LKer f )) )) holds (for w being (Vector of W) holds (for v being (Vector of V) holds (A = ( v + ( LKer f ) ) implies ( it . (A,w) ) = ( f . (v,w) )))));
existence
proof
set L = ( LKer f );
set vq = ( VectQuot (V,L) );
set C = ( CosetSet (V,L) );
set aC = ( addCoset (V,L) );
set mC = ( lmultCoset (V,L) );
defpred P[ set,set,set ]
 means
(for a being (Vector of V) holds ($1 = ( a + L ) implies $3 = ( f . (a,$2) )));
A1:now
let A being (Vector of vq);
let w0 being (Vector of W);
consider a being (Vector of V) such that A2: A = ( a + L ) by VECTSP10:22;
take y = ( f . (a,w0) );
now
let a1 being (Vector of V);
assume A = ( a1 + L );
then a in ( a1 + L ) by A2,VECTSP_4:44;
then consider w being (Vector of V) such that A3: w in L and A4: a = ( a1 + w ) by VECTSP_4:42;
w in (the carrier of L) by A3,STRUCT_0:def 5;
then w in ( leftker f ) by CKB106:def 1;
then A5: (ex aa being (Vector of V) st (aa = w & (for ww being (Vector of W) holds ( f . (aa,ww) ) = ( 0. K ))));
thus y = ( ( f . (a1,w0) ) + ( f . (w,w0) ) ) by A4,CKB77:1
.= ( ( f . (a1,w0) ) + ( 0. K ) ) by A5
.= ( f . (a1,w0) ) by RLVECT_1:def 4;
end;
hence P[ A,w0,y ];
end;
consider ff being (Function of [: (the carrier of vq),(the carrier of W) :],(the carrier of K)) such that A6: (for A being (Vector of vq) holds (for w being (Vector of W) holds P[ A,w,( ff . (A,w) ) ])) from BINOP_1:sch 3(A1);
reconsider ff as (Form of vq,W);
A7: C = (the carrier of vq) by VECTSP10:def 6;
now
let w being (Vector of W);
set ffw = ( FunctionalSAF (ff,w) );
now
let A being (Vector of vq);
let B being (Vector of vq);
consider a being (Vector of V) such that A8: A = ( a + L ) by VECTSP10:22;
consider b being (Vector of V) such that A9: B = ( b + L ) by VECTSP10:22;
A10: ((the addF of vq) = ( addCoset (V,L) ) & ( aC . (A,B) ) = ( ( a + b ) + L )) by A7,A8,A9,VECTSP10:def 3,VECTSP10:def 6;
thus ( ffw . ( A + B ) ) = ( ff . (( A + B ),w) ) by CKB20:1
.= ( f . (( a + b ),w) ) by A6,A10,RLVECT_1:2
.= ( ( f . (a,w) ) + ( f . (b,w) ) ) by CKB77:1
.= ( ( ff . (A,w) ) + ( f . (b,w) ) ) by A6,A8
.= ( ( ff . (A,w) ) + ( ff . (B,w) ) ) by A6,A9
.= ( ( ffw . A ) + ( ff . (B,w) ) ) by CKB20:1
.= ( ( ffw . A ) + ( ffw . B ) ) by CKB20:1;
end;
hence ffw is  additive by GRCAT_1:def 8;
end;
then reconsider ff as  additiveSAF (Form of vq,W) by CKB39:def 1;
now
let w being (Vector of W);
set ffw = ( FunctionalSAF (ff,w) );
now
let A being (Vector of vq);
let r being (Element of K);
consider a being (Vector of V) such that A11: A = ( a + L ) by VECTSP10:22;
A12: ((the lmult of vq) = ( lmultCoset (V,L) ) & ( mC . (r,A) ) = ( ( r * a ) + L )) by A7,A11,VECTSP10:def 5,VECTSP10:def 6;
thus ( ffw . ( r * A ) ) = ( ff . (( r * A ),w) ) by CKB20:1
.= ( f . (( r * a ),w) ) by A6,A12,VECTSP_1:def 12
.= ( r * ( f . (a,w) ) ) by CKB82:1
.= ( r * ( ff . (A,w) ) ) by A6,A11
.= ( r * ( ffw . A ) ) by CKB20:1;
end;
hence ffw is  homogeneous by HAHNBAN1:def 8;
end;
then reconsider ff as  additiveSAF  homogeneousSAF (Form of vq,W) by CKB41:def 1;
take ff;
thus thesis by A6;
end;
uniqueness
proof
set L = ( LKer f );
set vq = ( VectQuot (V,L) );
let f1 being  additiveSAF  homogeneousSAF (Form of vq,W);
let f2 being  additiveSAF  homogeneousSAF (Form of vq,W);
assume that
A13: (for A being (Vector of vq) holds (for w being (Vector of W) holds (for a being (Vector of V) holds (A = ( a + ( LKer f ) ) implies ( f1 . (A,w) ) = ( f . (a,w) )))))
and
A14: (for A being (Vector of vq) holds (for w being (Vector of W) holds (for a being (Vector of V) holds (A = ( a + ( LKer f ) ) implies ( f2 . (A,w) ) = ( f . (a,w) )))));
now
let A being (Vector of vq);
let w being (Vector of W);
consider a being (Vector of V) such that A15: A = ( a + L ) by VECTSP10:22;
thus ( f1 . (A,w) ) = ( f . (a,w) ) by A13,A15
.= ( f2 . (A,w) ) by A14,A15;
end;
hence thesis by BINOP_1:2;
end;
end;
