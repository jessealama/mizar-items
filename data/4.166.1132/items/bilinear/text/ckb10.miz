begin
theorem
(for K being  right_zeroed non  empty addLoopStr holds (for V,W being non  empty VectSpStr over K holds (for f being (Form of V,W) holds ( f + ( NulForm (V,W) ) ) = f)))
proof
let K being  right_zeroed non  empty addLoopStr;
let V being non  empty VectSpStr over K;
let W being non  empty VectSpStr over K;
let f being (Form of V,W);
set g = ( NulForm (V,W) );
now
let v being (Vector of V);
let w being (Vector of W);
thus ( ( f + g ) . (v,w) ) = ( ( f . (v,w) ) + ( g . (v,w) ) ) by CKB3:def 1
.= ( ( f . (v,w) ) + ( 0. K ) ) by FUNCOP_1:70
.= ( f . (v,w) ) by RLVECT_1:def 4;
end;
hence thesis by BINOP_1:2;
end;
