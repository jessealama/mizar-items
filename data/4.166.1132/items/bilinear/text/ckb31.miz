begin
definition
let K being non  empty multMagma;
let V being non  empty VectSpStr over K;
let W being non  empty VectSpStr over K;
let f being (Functional of V);
let g being (Functional of W);
func FormFunctional (f,g) -> (Form of V,W) means 
:Def11: (for v being (Vector of V) holds (for w being (Vector of W) holds ( it . (v,w) ) = ( ( f . v ) * ( g . w ) )));
existence
proof
deffunc F((Vector of V),(Vector of W)) = ( ( f . $1 ) * ( g . $2 ) );
set c1 = (the carrier of V);
set c2 = (the carrier of W);
set k = (the carrier of K);
consider i being (Function of [: c1,c2 :],k) such that A1: (for x being (Element of c1) holds (for y being (Element of c2) holds ( i . (x,y) ) = F(x,y))) from BINOP_1:sch 4;
reconsider i as (Form of V,W);
take i;
thus thesis by A1;
end;
uniqueness
proof
let F1 being (Form of V,W);
let F2 being (Form of V,W);
assume that
A2: (for v being (Vector of V) holds (for y being (Vector of W) holds ( F1 . (v,y) ) = ( ( f . v ) * ( g . y ) )))
and
A3: (for v being (Vector of V) holds (for y being (Vector of W) holds ( F2 . (v,y) ) = ( ( f . v ) * ( g . y ) )));
now
let v being (Vector of V);
let y being (Vector of W);
thus ( F1 . (v,y) ) = ( ( f . v ) * ( g . y ) ) by A2
.= ( F2 . (v,y) ) by A3;
end;
hence thesis by BINOP_1:2;
end;
end;
