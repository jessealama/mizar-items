theorem
Th33: (for K being non  empty multMagma holds (for V,W being non  empty VectSpStr over K holds (for v being (Vector of V) holds (for w being (Vector of W) holds (for a being (Element of K) holds (for f being (Form of V,W) holds (f is  homogeneousFAF implies ( f . (v,( a * w )) ) = ( a * ( f . (v,w) ) ))))))))
proof
let K being non  empty multMagma;
let V being non  empty VectSpStr over K;
let W being non  empty VectSpStr over K;
let v being (Vector of V);
let y being (Vector of W);
let r being (Element of K);
let f being (Form of V,W);
set F = ( FunctionalFAF (f,v) );
assume f is  homogeneousFAF;
then A1: F is  homogeneous by CKB40:def 1;
thus ( f . (v,( r * y )) ) = ( F . ( r * y ) ) by CKB19:1
.= ( r * ( F . y ) ) by A1,HAHNBAN1:def 8
.= ( r * ( f . (v,y) ) ) by CKB19:1;
end;
