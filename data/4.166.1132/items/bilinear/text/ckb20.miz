begin
theorem
Th10: (for K being non  empty 1-sorted holds (for V,W being non  empty VectSpStr over K holds (for f being (Form of V,W) holds (for w being (Vector of W) holds ((( dom ( FunctionalSAF (f,w) ) ) = (the carrier of V) & ( rng ( FunctionalSAF (f,w) ) ) c= (the carrier of K)) & (for v being (Vector of V) holds ( ( FunctionalSAF (f,w) ) . v ) = ( f . (v,w) )))))))
proof
let K being non  empty 1-sorted;
let V being non  empty VectSpStr over K;
let W being non  empty VectSpStr over K;
let f being (Form of V,W);
let w being (Vector of W);
set F = ( FunctionalSAF (f,w) );
( dom f ) = [: (the carrier of V),(the carrier of W) :] by FUNCT_2:def 1;
then A1: (ex g being Function st (((( ( curry' f ) . w ) = g & ( dom g ) = (the carrier of V)) & ( rng g ) c= ( rng f )) & (for y being set holds (y in (the carrier of V) implies ( g . y ) = ( f . (y,w) ))))) by FUNCT_5:32;
hence (( dom F ) = (the carrier of V) & ( rng F ) c= (the carrier of K));
let v being (Vector of V);
thus thesis by A1;
end;
