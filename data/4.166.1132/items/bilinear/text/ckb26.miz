begin
theorem
Th16: (for K being non  empty doubleLoopStr holds (for V,W being non  empty VectSpStr over K holds (for f being (Form of V,W) holds (for a being (Element of K) holds (for v being (Vector of V) holds ( FunctionalFAF (( a * f ),v) ) = ( a * ( FunctionalFAF (f,v) ) ))))))
proof
let K being non  empty doubleLoopStr;
let V being non  empty VectSpStr over K;
let W being non  empty VectSpStr over K;
let f being (Form of V,W);
let a being (Element of K);
let w being (Vector of V);
now
let v being (Vector of W);
thus ( ( FunctionalFAF (( a * f ),w) ) . v ) = ( ( a * f ) . (w,v) ) by CKB19:1
.= ( a * ( f . (w,v) ) ) by CKB4:def 1
.= ( a * ( ( FunctionalFAF (f,w) ) . v ) ) by CKB19:1
.= ( ( a * ( FunctionalFAF (f,w) ) ) . v ) by HAHNBAN1:def 6;
end;
hence thesis by FUNCT_2:63;
end;
