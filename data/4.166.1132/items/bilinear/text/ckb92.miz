definition
let K being ZeroStr;
let V being non  empty VectSpStr over K;
let W being non  empty VectSpStr over K;
let f being (Form of V,W);
func leftker f -> (Subset of V) equals 
{ v where v is (Vector of V): (for w being (Vector of W) holds ( f . (v,w) ) = ( 0. K )) };
correctness
proof
set A = { v where v is (Vector of V): (for w being (Vector of W) holds ( f . (v,w) ) = ( 0. K )) };
A c= (the carrier of V)
proof
let x being set;
assume x in A;
then (ex v being (Vector of V) st (v = x & (for w being (Vector of W) holds ( f . (v,w) ) = ( 0. K ))));
hence thesis;
end;
hence thesis;
end;
