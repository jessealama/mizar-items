environ
vocabularies STRUCT_0,VECTSP_1,FUNCT_1,ZFMISC_1,XBOOLE_0,FUNCOP_1,SUPINF_2,ALGSTR_0,ARYTM_3,SUBSET_1,RELAT_1,ARYTM_1,RLVECT_1,MESFUNC1,BINOP_1,FUNCT_5,TARSKI,HAHNBAN,HAHNBAN1,MSSUBFAM,UNIALG_1,LATTICES,RLSUB_1,VECTSP10,GROUP_6,RELAT_2,SPPOL_1,BILINEAR;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,DOMAIN_1,FUNCT_1,RELAT_1,STRUCT_0,ALGSTR_0,RLVECT_1,BINOP_1,GROUP_1,VECTSP_1,RELSET_1,FUNCT_2,FUNCOP_1,FUNCT_5,VECTSP_4,HAHNBAN1,VECTSP10,GRCAT_1,CKB1,CKB2,CKB3,CKB4;
definitions HAHNBAN1,TARSKI,XBOOLE_0,VECTSP_4,BINOP_1,STRUCT_0,GRCAT_1,FUNCT_2,CKB2,CKB3,CKB4;
theorems FUNCT_5,XBOOLE_1,FUNCT_2,HAHNBAN1,VECTSP_1,FUNCOP_1,RLVECT_1,VECTSP_4,TARSKI,FUNCT_1,REALSET1,ZFMISC_1,DOMAIN_1,VECTSP10,GROUP_1,BINOP_1,STRUCT_0,VECTSP_2,GRCAT_1,CKB2,CKB3,CKB4;
schemes BINOP_1;
registrations SUBSET_1,RELSET_1,STRUCT_0,VECTSP_1,VECTSP_4,HAHNBAN1,VECTSP10,FUNCT_2,ZFMISC_1;
constructors BORSUK_1,VECTSP10,FUNCOP_1,CKB2,CKB3,CKB4;
requirements SUBSET,BOOLE;
begin
definition
let K being non  empty addLoopStr;
let V being non  empty VectSpStr over K;
let W being non  empty VectSpStr over K;
let f being (Form of V,W);
func - f -> (Form of V,W) means 
:Def5: (for v being (Vector of V) holds (for w being (Vector of W) holds ( it . (v,w) ) = ( - ( f . (v,w) ) )));
existence
proof
set X = (the carrier of V);
set Y = (the carrier of W);
set Z = (the carrier of K);
deffunc F((Element of X),(Element of Y)) = ( - ( f . ($1,$2) ) );
consider ff being (Function of [: X,Y :],Z) such that A1: (for x being (Element of X) holds (for y being (Element of Y) holds ( ff . (x,y) ) = F(x,y))) from BINOP_1:sch 4;
reconsider ff as (Form of V,W);
take ff;
thus thesis by A1;
end;
uniqueness
proof
let F being (Form of V,W);
let G being (Form of V,W);
assume that
A2: (for v being (Vector of V) holds (for w being (Vector of W) holds ( F . (v,w) ) = ( - ( f . (v,w) ) )))
and
A3: (for v being (Vector of V) holds (for w being (Vector of W) holds ( G . (v,w) ) = ( - ( f . (v,w) ) )));
now
let v being (Vector of V);
let w being (Vector of W);
thus ( F . (v,w) ) = ( - ( f . (v,w) ) ) by A2
.= ( G . (v,w) ) by A3;
end;
hence thesis by BINOP_1:2;
end;
end;
