begin
theorem
Th22: (for K being  add-associative  right_zeroed  right_complementable  left-distributive non  empty doubleLoopStr holds (for V,W being non  empty VectSpStr over K holds (for g being (Functional of W) holds (for v being (Vector of V) holds (for w being (Vector of W) holds ( ( FormFunctional (( 0Functional V ),g) ) . (v,w) ) = ( 0. K ))))))
proof
let K being  add-associative  right_zeroed  right_complementable  left-distributive non  empty doubleLoopStr;
let V being non  empty VectSpStr over K;
let W being non  empty VectSpStr over K;
let h being (Functional of W);
let v being (Vector of V);
let y being (Vector of W);
set 0F = ( 0Functional V );
set F = ( FormFunctional (0F,h) );
thus ( F . (v,y) ) = ( ( 0F . v ) * ( h . y ) ) by CKB31:def 1
.= ( ( 0. K ) * ( h . y ) ) by FUNCOP_1:7
.= ( 0. K ) by VECTSP_1:7;
end;
