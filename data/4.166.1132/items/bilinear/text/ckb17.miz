environ
vocabularies STRUCT_0,VECTSP_1,FUNCT_1,ZFMISC_1,XBOOLE_0,FUNCOP_1,SUPINF_2,ALGSTR_0,ARYTM_3,SUBSET_1,RELAT_1,ARYTM_1,RLVECT_1,MESFUNC1,BINOP_1,FUNCT_5,TARSKI,HAHNBAN,HAHNBAN1,MSSUBFAM,UNIALG_1,LATTICES,RLSUB_1,VECTSP10,GROUP_6,RELAT_2,SPPOL_1,BILINEAR;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,DOMAIN_1,FUNCT_1,RELAT_1,STRUCT_0,ALGSTR_0,RLVECT_1,BINOP_1,GROUP_1,VECTSP_1,RELSET_1,FUNCT_2,FUNCOP_1,FUNCT_5,VECTSP_4,HAHNBAN1,VECTSP10,GRCAT_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9;
definitions HAHNBAN1,TARSKI,XBOOLE_0,VECTSP_4,BINOP_1,STRUCT_0,GRCAT_1,FUNCT_2,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8;
theorems FUNCT_5,XBOOLE_1,FUNCT_2,HAHNBAN1,VECTSP_1,FUNCOP_1,RLVECT_1,VECTSP_4,TARSKI,FUNCT_1,REALSET1,ZFMISC_1,DOMAIN_1,VECTSP10,GROUP_1,BINOP_1,STRUCT_0,VECTSP_2,GRCAT_1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16;
schemes BINOP_1;
registrations SUBSET_1,RELSET_1,STRUCT_0,VECTSP_1,VECTSP_4,HAHNBAN1,VECTSP10,FUNCT_2,ZFMISC_1;
constructors BORSUK_1,VECTSP10,FUNCOP_1,CKB2,CKB3,CKB4,CKB5,CKB7,CKB9;
requirements SUBSET,BOOLE;
begin
Lm1:now
let K being non  empty 1-sorted;
let V being non  empty VectSpStr over K;
let W being non  empty VectSpStr over K;
let f being (Form of V,W);
let v being (Element of (the carrier of V));
let y being (Element of W);
A1: ( dom f ) = [: (the carrier of V),(the carrier of W) :] by FUNCT_2:def 1;
then consider g being Function such that A2: ( ( curry f ) . v ) = g and A3: (( dom g ) = (the carrier of W) & ( rng g ) c= ( rng f )) and (for y being set holds (y in (the carrier of W) implies ( g . y ) = ( f . (v,y) ))) by FUNCT_5:29;
reconsider g as (Function of (the carrier of W),(the carrier of K)) by A3,FUNCT_2:2,XBOOLE_1:1;
g = ( ( curry f ) . v ) by A2;
hence ( ( curry f ) . v ) is (Functional of W);
consider h being Function such that A4: ( ( curry' f ) . y ) = h and A5: (( dom h ) = (the carrier of V) & ( rng h ) c= ( rng f )) and (for x being set holds (x in (the carrier of V) implies ( h . x ) = ( f . (x,y) ))) by A1,FUNCT_5:32;
reconsider h as (Function of (the carrier of V),(the carrier of K)) by A5,FUNCT_2:2,XBOOLE_1:1;
h = ( ( curry' f ) . y ) by A4;
hence ( ( curry' f ) . y ) is (Functional of V);
end;
definition
let K being non  empty 1-sorted;
let V being non  empty VectSpStr over K;
let W being non  empty VectSpStr over K;
let f being (Form of V,W);
let v being (Vector of V);
func FunctionalFAF (f,v) -> (Functional of W) equals 
( ( curry f ) . v );
correctness by Lm1;
end;
