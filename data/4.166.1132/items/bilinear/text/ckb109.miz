requirements SUBSET,BOOLE;
begin
definition
let K being  add-associative  right_zeroed  right_complementable  Abelian  associative  well-unital  distributive non  empty doubleLoopStr;
let V being non  empty VectSpStr over K;
let W being (VectSp of K);
let f being  additiveFAF  homogeneousFAF (Form of V,W);
func RQForm f ->  additiveFAF  homogeneousFAF (Form of V,( VectQuot (W,( RKer f )) )) means 
:Def22: (for A being (Vector of ( VectQuot (W,( RKer f )) )) holds (for v being (Vector of V) holds (for w being (Vector of W) holds (A = ( w + ( RKer f ) ) implies ( it . (v,A) ) = ( f . (v,w) )))));
existence
proof
set L = ( RKer f );
set vq = ( VectQuot (W,L) );
set C = ( CosetSet (W,L) );
set aC = ( addCoset (W,L) );
set mC = ( lmultCoset (W,L) );
defpred P[ set,set,set ]
 means
(for w being (Vector of W) holds ($2 = ( w + L ) implies $3 = ( f . ($1,w) )));
A1:now
let v0 being (Vector of V);
let A being (Vector of vq);
consider a being (Vector of W) such that A2: A = ( a + L ) by VECTSP10:22;
take y = ( f . (v0,a) );
now
let a1 being (Vector of W);
assume A = ( a1 + L );
then a in ( a1 + L ) by A2,VECTSP_4:44;
then consider w being (Vector of W) such that A3: w in L and A4: a = ( a1 + w ) by VECTSP_4:42;
w in (the carrier of L) by A3,STRUCT_0:def 5;
then w in ( rightker f ) by CKB107:def 1;
then A5: (ex aa being (Vector of W) st (aa = w & (for vv being (Vector of V) holds ( f . (vv,aa) ) = ( 0. K ))));
thus y = ( ( f . (v0,a1) ) + ( f . (v0,w) ) ) by A4,CKB78:1
.= ( ( f . (v0,a1) ) + ( 0. K ) ) by A5
.= ( f . (v0,a1) ) by RLVECT_1:def 4;
end;
hence P[ v0,A,y ];
end;
consider ff being (Function of [: (the carrier of V),(the carrier of vq) :],(the carrier of K)) such that A6: (for v being (Vector of V) holds (for A being (Vector of vq) holds P[ v,A,( ff . (v,A) ) ])) from BINOP_1:sch 3(A1);
reconsider ff as (Form of V,vq);
A7: C = (the carrier of vq) by VECTSP10:def 6;
now
let v being (Vector of V);
set ffw = ( FunctionalFAF (ff,v) );
now
let A being (Vector of vq);
let B being (Vector of vq);
consider a being (Vector of W) such that A8: A = ( a + L ) by VECTSP10:22;
consider b being (Vector of W) such that A9: B = ( b + L ) by VECTSP10:22;
A10: ((the addF of vq) = ( addCoset (W,L) ) & ( aC . (A,B) ) = ( ( a + b ) + L )) by A7,A8,A9,VECTSP10:def 3,VECTSP10:def 6;
thus ( ffw . ( A + B ) ) = ( ff . (v,( A + B )) ) by CKB19:1
.= ( f . (v,( a + b )) ) by A6,A10,RLVECT_1:2
.= ( ( f . (v,a) ) + ( f . (v,b) ) ) by CKB78:1
.= ( ( ff . (v,A) ) + ( f . (v,b) ) ) by A6,A8
.= ( ( ff . (v,A) ) + ( ff . (v,B) ) ) by A6,A9
.= ( ( ffw . A ) + ( ff . (v,B) ) ) by CKB19:1
.= ( ( ffw . A ) + ( ffw . B ) ) by CKB19:1;
end;
hence ffw is  additive by GRCAT_1:def 8;
end;
then reconsider ff as  additiveFAF (Form of V,vq) by CKB38:def 1;
now
let v being (Vector of V);
set ffw = ( FunctionalFAF (ff,v) );
now
let A being (Vector of vq);
let r being (Element of K);
consider a being (Vector of W) such that A11: A = ( a + L ) by VECTSP10:22;
A12: ((the lmult of vq) = ( lmultCoset (W,L) ) & ( mC . (r,A) ) = ( ( r * a ) + L )) by A7,A11,VECTSP10:def 5,VECTSP10:def 6;
thus ( ffw . ( r * A ) ) = ( ff . (v,( r * A )) ) by CKB19:1
.= ( f . (v,( r * a )) ) by A6,A12,VECTSP_1:def 12
.= ( r * ( f . (v,a) ) ) by CKB83:1
.= ( r * ( ff . (v,A) ) ) by A6,A11
.= ( r * ( ffw . A ) ) by CKB19:1;
end;
hence ffw is  homogeneous by HAHNBAN1:def 8;
end;
then reconsider ff as  additiveFAF  homogeneousFAF (Form of V,vq) by CKB40:def 1;
take ff;
thus thesis by A6;
end;
uniqueness
proof
set L = ( RKer f );
set vq = ( VectQuot (W,L) );
let f1 being  additiveFAF  homogeneousFAF (Form of V,vq);
let f2 being  additiveFAF  homogeneousFAF (Form of V,vq);
assume that
A13: (for A being (Vector of vq) holds (for v being (Vector of V) holds (for a being (Vector of W) holds (A = ( a + ( RKer f ) ) implies ( f1 . (v,A) ) = ( f . (v,a) )))))
and
A14: (for A being (Vector of vq) holds (for v being (Vector of V) holds (for a being (Vector of W) holds (A = ( a + ( RKer f ) ) implies ( f2 . (v,A) ) = ( f . (v,a) )))));
now
let v being (Vector of V);
let A being (Vector of vq);
consider a being (Vector of W) such that A15: A = ( a + L ) by VECTSP10:22;
thus ( f1 . (v,A) ) = ( f . (v,a) ) by A13,A15
.= ( f2 . (v,A) ) by A14,A15;
end;
hence thesis by BINOP_1:2;
end;
end;
