begin
theorem
(for K being  add-associative  right_zeroed  right_complementable  right-distributive non  empty doubleLoopStr holds (for V,W being non  empty VectSpStr over K holds (for f being (Functional of V) holds ( FormFunctional (f,( 0Functional W )) ) = ( NulForm (V,W) ))))
proof
let K being  add-associative  right_zeroed  right_complementable  right-distributive non  empty doubleLoopStr;
let V being non  empty VectSpStr over K;
let W being non  empty VectSpStr over K;
let f being (Functional of V);
now
let v being (Vector of V);
let y being (Vector of W);
thus ( ( FormFunctional (f,( 0Functional W )) ) . (v,y) ) = ( 0. K ) by CKB32:1
.= ( ( NulForm (V,W) ) . (v,y) ) by FUNCOP_1:70;
end;
hence thesis by BINOP_1:2;
end;
