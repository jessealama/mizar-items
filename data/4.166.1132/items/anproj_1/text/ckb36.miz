begin
reserve V for non  trivial RealLinearSpace;
reserve u for (Element of V);
registration
let V;
cluster ( ProjectivePoints V ) -> non  empty;
coherence
proof
consider u being (Element of V) such that A1: u <> ( 0. V ) by STRUCT_0:def 18;
set Y = ( Dir u );
consider Z being (Subset-Family of ( NonZero V )) such that A2: Z = ( Class ( Proportionality_as_EqRel_of V ) ) and A3: ( ProjectivePoints V ) = Z by CKB34:def 1;
u in ( NonZero V ) by A1,ZFMISC_1:56;
then Y in Z by A2,EQREL_1:def 3;
hence thesis by A3;
end;
