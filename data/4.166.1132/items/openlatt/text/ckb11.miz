environ
vocabularies LATTICE2,FILTER_0,LATTICES,PRE_TOPC,SUBSET_1,XBOOLE_0,TOPS_1,TARSKI,RCOMP_1,SETFAM_1,BINOP_1,FUNCT_1,EQREL_1,STRUCT_0,PBOOLE,CARD_FIL,INT_2,RELAT_1,ZFMISC_1,ORDINAL1,GROUP_6,FUNCT_2,WELLORD1,XBOOLEAN,LATTICE4,OPENLATT;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,ORDINAL1,RELAT_1,FUNCT_1,FUNCT_2,BINOP_1,SETFAM_1,DOMAIN_1,STRUCT_0,PRE_TOPC,TOPS_1,LATTICES,LATTICE2,FILTER_0,LATTICE4,CKB5,CKB7,CKB8,CKB9,CKB10;
definitions TARSKI,PRE_TOPC,FILTER_0,LATTICES,LATTICE4,FUNCT_1,STRUCT_0,XBOOLE_0,SUBSET_1,FUNCT_2,CKB5,CKB9,CKB10;
theorems PRE_TOPC,LATTICES,TOPS_1,ZFMISC_1,FILTER_0,SETFAM_1,TARSKI,FUNCT_1,FUNCT_2,LATTICE4,RELAT_1,ORDINAL1,XBOOLE_0,XBOOLE_1,STRUCT_0,EQREL_1,CKB3,CKB4,CKB5,CKB9,CKB10;
schemes BINOP_1,DOMAIN_1,FUNCT_1,BINOP_2;
registrations XBOOLE_0,SUBSET_1,RELSET_1,STRUCT_0,LATTICES,PRE_TOPC,FILTER_0,LATTICE2,TOPS_1,CKB1,CKB2,CKB6;
constructors SETFAM_1,BINOP_1,DOMAIN_1,NAT_D,TOPS_1,LATTICE2,FILTER_1,LATTICE4,RELSET_1,FILTER_0,CKB5,CKB7,CKB8,CKB9,CKB10;
requirements BOOLE,SUBSET;
begin
reserve T for non empty TopSpace;
theorem
Th4: (for T being non empty TopSpace holds LattStr (# ( Topology_of T ),( Top_Union T ),( Top_Meet T ) #) is Lattice)
proof
let T;
set L = LattStr (# ( Topology_of T ),( Top_Union T ),( Top_Meet T ) #);
A1:now
let p being (Element of L),q being (Element of L);
thus ( p "\/" q ) = ( q \/ p ) by CKB9:def 1
.= ( q "\/" p ) by CKB9:def 1;
end;
A2:now
let p being (Element of L),q being (Element of L);
thus ( ( p "/\" q ) "\/" q ) = ( ( p "/\" q ) \/ q ) by CKB9:def 1
.= ( ( p /\ q ) \/ q ) by CKB10:def 1
.= q by XBOOLE_1:22;
end;
A3:now
let p being (Element of L),q being (Element of L);
thus ( p "/\" ( p "\/" q ) ) = ( p /\ ( p "\/" q ) ) by CKB10:def 1
.= ( p /\ ( p \/ q ) ) by CKB9:def 1
.= p by XBOOLE_1:21;
end;
A4:now
let p being (Element of L),q being (Element of L),r being (Element of L);
thus ( p "/\" ( q "/\" r ) ) = ( p /\ ( q "/\" r ) ) by CKB10:def 1
.= ( p /\ ( q /\ r ) ) by CKB10:def 1
.= ( ( p /\ q ) /\ r ) by XBOOLE_1:16
.= ( ( p "/\" q ) /\ r ) by CKB10:def 1
.= ( ( p "/\" q ) "/\" r ) by CKB10:def 1;
end;
A5:now
let p being (Element of L),q being (Element of L);
thus ( p "/\" q ) = ( q /\ p ) by CKB10:def 1
.= ( q "/\" p ) by CKB10:def 1;
end;
now
let p being (Element of L),q being (Element of L),r being (Element of L);
thus ( p "\/" ( q "\/" r ) ) = ( p \/ ( q "\/" r ) ) by CKB9:def 1
.= ( p \/ ( q \/ r ) ) by CKB9:def 1
.= ( ( p \/ q ) \/ r ) by XBOOLE_1:4
.= ( ( p "\/" q ) \/ r ) by CKB9:def 1
.= ( ( p "\/" q ) "\/" r ) by CKB9:def 1;
end;
then L is  join-commutative  join-associative  meet-absorbing  meet-commutative  meet-associative  join-absorbing by A1,A2,A5,A4,A3,LATTICES:def 4,LATTICES:def 5,LATTICES:def 6,LATTICES:def 7,LATTICES:def 8,LATTICES:def 9;
hence thesis;
end;
