theorem
(seq is  convergent implies seq is  Cauchy)
proof
assume seq is  convergent;
then consider h such that A1: (for r holds (r > ( 0 ) implies (ex k st (for n holds (n >= k implies ( dist (( seq . n ),h) ) < r))))) by BHSP_2:def 1;
let r;
assume r > ( 0 );
then consider m1 such that A2: (for n holds (n >= m1 implies ( dist (( seq . n ),h) ) < ( r / 2 ))) by A1,XREAL_1:215;
take k = m1;
let n;
let m;
assume (n >= k & m >= k);
then (( dist (( seq . n ),h) ) < ( r / 2 ) & ( dist (( seq . m ),h) ) < ( r / 2 )) by A2;
then (( dist (( seq . n ),( seq . m )) ) <= ( ( dist (( seq . n ),h) ) + ( dist (h,( seq . m )) ) ) & ( ( dist (( seq . n ),h) ) + ( dist (h,( seq . m )) ) ) < ( ( r / 2 ) + ( r / 2 ) )) by BHSP_1:35,XREAL_1:8;
hence thesis by XXREAL_0:2;
end;
