then ( dist (( ( a * seq ) . n ),( a * ( seq . m ) )) ) < r by NORMSP_1:def 5;
hence ( dist (( ( a * seq ) . n ),( ( a * seq ) . m )) ) < r by NORMSP_1:def 5;
end;
now
