environ
vocabularies NUMBERS,BHSP_1,PRE_TOPC,REAL_1,NAT_1,ORDINAL2,SEQ_1,SUBSET_1,SUPINF_2,XXREAL_0,CARD_1,METRIC_1,FUNCT_1,ARYTM_3,NORMSP_1,ARYTM_1,RELAT_1,COMPLEX1,SEQ_2,XXREAL_2,XREAL_0,ORDINAL1,VALUED_0,REWRITE1,BHSP_3;
notations ORDINAL1,SUBSET_1,NUMBERS,XREAL_0,COMPLEX1,REAL_1,SEQ_1,NAT_1,VALUED_0,STRUCT_0,PRE_TOPC,RLVECT_1,VFUNCT_1,NORMSP_1,BHSP_1,BHSP_2,XXREAL_0,CKB1,CKB11,CKB14,CKB21;
definitions CKB1,CKB11,CKB21;
theorems NAT_1,SEQM_3,FUNCT_2,RLVECT_1,BHSP_1,BHSP_2,NORMSP_1,XREAL_0,XCMPLX_0,XREAL_1,COMPLEX1,XXREAL_0,ORDINAL1,VALUED_0,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32;
schemes NAT_1;
registrations XBOOLE_0,ORDINAL1,RELSET_1,NUMBERS,XXREAL_0,XREAL_0,NAT_1,MEMBERED,STRUCT_0,VALUED_0,VFUNCT_1,FUNCT_2;
constructors PARTFUN1,FUNCT_3,XXREAL_0,REAL_1,COMPLEX1,SEQ_1,SEQM_3,BHSP_2,VALUED_1,RECDEF_1,NAT_1,RELSET_1,VFUNCT_1,CKB1,CKB11,CKB14,CKB21;
requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve X for RealUnitarySpace;
reserve r for Real;
reserve seq for (sequence of X);
reserve seq1 for (sequence of X);
reserve Nseq for  increasing (sequence of ( NAT ));
reserve l for (Element of ( NAT ));
reserve n for (Element of ( NAT ));
reserve m for (Element of ( NAT ));
theorem
Th34: ((seq is  Cauchy & seq1 is (subsequence of seq)) implies seq1 is  Cauchy)
proof
assume that
A1: seq is  Cauchy
and
A2: seq1 is (subsequence of seq);
consider Nseq such that A3: seq1 = ( seq * Nseq ) by A2,VALUED_0:def 17;
now
let r;
assume r > ( 0 );
then consider l such that A4: (for n holds (for m holds ((n >= l & m >= l) implies ( dist (( seq . n ),( seq . m )) ) < r))) by A1,CKB1:def 1;
take k = l;
let n;
let m;
assume that
A5: n >= k
and
A6: m >= k;
( Nseq . n ) >= n by SEQM_3:14;
then A7: ( Nseq . n ) >= k by A5,XXREAL_0:2;
( Nseq . m ) >= m by SEQM_3:14;
then A8: ( Nseq . m ) >= k by A6,XXREAL_0:2;
(( seq1 . n ) = ( seq . ( Nseq . n ) ) & ( seq1 . m ) = ( seq . ( Nseq . m ) )) by A3,FUNCT_2:15;
hence ( dist (( seq1 . n ),( seq1 . m )) ) < r by A4,A7,A8;
end;
hence thesis by CKB1:def 1;
end;
