environ
vocabularies NUMBERS,BHSP_1,PRE_TOPC,REAL_1,NAT_1,ORDINAL2,SEQ_1,SUBSET_1,SUPINF_2,XXREAL_0,CARD_1,METRIC_1,FUNCT_1,ARYTM_3,NORMSP_1,ARYTM_1,RELAT_1,COMPLEX1,SEQ_2,XXREAL_2,XREAL_0,ORDINAL1,VALUED_0,REWRITE1,BHSP_3;
notations ORDINAL1,SUBSET_1,NUMBERS,XREAL_0,COMPLEX1,REAL_1,SEQ_1,NAT_1,VALUED_0,STRUCT_0,PRE_TOPC,RLVECT_1,VFUNCT_1,NORMSP_1,BHSP_1,BHSP_2,XXREAL_0,CKB1,CKB11,CKB14,CKB21;
definitions CKB1,CKB11,CKB21;
theorems NAT_1,SEQM_3,FUNCT_2,RLVECT_1,BHSP_1,BHSP_2,NORMSP_1,XREAL_0,XCMPLX_0,XREAL_1,COMPLEX1,XXREAL_0,ORDINAL1,VALUED_0,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB37,CKB38,CKB39,CKB40,CKB41;
schemes NAT_1;
registrations XBOOLE_0,ORDINAL1,RELSET_1,NUMBERS,XXREAL_0,XREAL_0,NAT_1,MEMBERED,STRUCT_0,VALUED_0,VFUNCT_1,FUNCT_2;
constructors PARTFUN1,FUNCT_3,XXREAL_0,REAL_1,COMPLEX1,SEQ_1,SEQM_3,BHSP_2,VALUED_1,RECDEF_1,NAT_1,RELSET_1,VFUNCT_1,CKB1,CKB11,CKB14,CKB21;
requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve X for RealUnitarySpace;
reserve r for Real;
reserve seq for (sequence of X);
reserve seq1 for (sequence of X);
reserve k for (Element of ( NAT ));
reserve l1 for (Element of ( NAT ));
reserve l2 for (Element of ( NAT ));
reserve l3 for (Element of ( NAT ));
reserve n for (Element of ( NAT ));
reserve m for (Element of ( NAT ));
theorem
((seq is  Cauchy & (ex k st seq = ( seq1 ^\ k ))) implies seq1 is  Cauchy)
proof
assume that
A1: seq is  Cauchy
and
A2: (ex k st seq = ( seq1 ^\ k ));
consider k such that A3: seq = ( seq1 ^\ k ) by A2;
let r;
assume r > ( 0 );
then consider l1 such that A4: (for n holds (for m holds ((n >= l1 & m >= l1) implies ( dist (( seq . n ),( seq . m )) ) < r))) by A1,CKB1:def 1;
take l = ( l1 + k );
let n;
let m;
assume that
A5: n >= l
and
A6: m >= l;
consider m1 being Nat such that A7: n = ( ( l1 + k ) + m1 ) by A5,NAT_1:10;
reconsider m1 as (Element of ( NAT )) by ORDINAL1:def 12;
( n - k ) = ( l1 + m1 ) by A7;
then consider l2 such that A8: l2 = ( n - k );
consider m2 being Nat such that A9: m = ( ( l1 + k ) + m2 ) by A6,NAT_1:10;
reconsider m2 as (Element of ( NAT )) by ORDINAL1:def 12;
( m - k ) = ( l1 + m2 ) by A9;
then consider l3 such that A10: l3 = ( m - k );
A11:now
assume l2 < l1;
then ( l2 + k ) < ( l1 + k ) by XREAL_1:6;
hence contradiction by A5,A8;
end;
A12: ( l2 + k ) = n by A8;
now
assume l3 < l1;
then ( l3 + k ) < ( l1 + k ) by XREAL_1:6;
hence contradiction by A6,A10;
end;
then ( dist (( seq . l2 ),( seq . l3 )) ) < r by A4,A11;
then A13: ( dist (( seq1 . n ),( seq . l3 )) ) < r by A3,A12,NAT_1:def 3;
( l3 + k ) = m by A10;
hence thesis by A3,A13,NAT_1:def 3;
end;
