theorem
Th7: (seq is  Cauchy implies ( seq + x ) is  Cauchy)
proof
assume A1: seq is  Cauchy;
let r;
assume r > ( 0 );
then consider m1 such that A2: (for n holds (for m holds ((n >= m1 & m >= m1) implies ( dist (( seq . n ),( seq . m )) ) < r))) by A1,CKB1:def 1;
take k = m1;
let n;
let m;
( dist (( ( seq . n ) + x ),( ( seq . m ) + x )) ) <= ( ( dist (( seq . n ),( seq . m )) ) + ( dist (x,x) ) ) by BHSP_1:40;
then A3: ( dist (( ( seq . n ) + x ),( ( seq . m ) + x )) ) <= ( ( dist (( seq . n ),( seq . m )) ) + ( 0 ) ) by BHSP_1:34;
assume (n >= k & m >= k);
then ( dist (( seq . n ),( seq . m )) ) < r by A2;
then ( dist (( ( seq . n ) + x ),( ( seq . m ) + x )) ) < r by A3,XXREAL_0:2;
then ( dist (( ( seq + x ) . n ),( ( seq . m ) + x )) ) < r by BHSP_1:def 6;
hence thesis by BHSP_1:def 6;
end;
