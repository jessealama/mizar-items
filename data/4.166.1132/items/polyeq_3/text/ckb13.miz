definitions POLYEQ_1,SQUARE_1,XCMPLX_0,QUIN_1,CKB3,CKB9,CKB11;
theorems COMPLEX1,POLYEQ_1,SQUARE_1,NEWTON,COMPTRIG,NAT_1,POWER,SIN_COS,SIN_COS2,COMPLEX2,XCMPLX_0,XCMPLX_1,XREAL_1,CKB3,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12;
schemes NAT_1,COMPTRIG;
registrations XCMPLX_0,XXREAL_0,XREAL_0,SQUARE_1,NAT_1,MEMBERED,NEWTON,POLYEQ_1,XBOOLE_0,VALUED_0,RELSET_1,POWER,SIN_COS;
constructors FUNCT_4,ARYTM_0,REAL_1,SQUARE_1,NAT_1,SEQ_1,QUIN_1,PREPOWER,POWER,POLYEQ_1,BINARITH,SIN_COS,COMPTRIG,VALUED_1,BINOP_2,NAT_D,XREAL_0,ABIAN,NEWTON,CKB1,CKB2,CKB3,CKB4,CKB9;
requirements ARITHM,SUBSET,REAL,BOOLE,NUMERALS;
begin
reserve a for Real;
reserve b for Real;
reserve c for Real;
reserve d for Real;
reserve a9 for Real;
reserve b9 for Real;
reserve c9 for Real;
reserve d9 for Real;
theorem
((for z being  complex number holds ( Polynom (a,b,c,d,z) ) = ( Polynom (a9,b9,c9,d9,z) )) implies (((a = a9 & b = b9) & c = c9) & d = d9))
proof
assume A1: (for z being  complex number holds ( Polynom (a,b,c,d,z) ) = ( Polynom (a9,b9,c9,d9,z) ));
then A2: ( Polynom (a,b,c,d,( 0 )) ) = ( Polynom (a9,b9,c9,d9,( 0 )) );
A3: (( Polynom (a,b,c,d,1) ) = ( Polynom (a9,b9,c9,d9,1) ) & ( Polynom (a,b,c,d,( - 1 )) ) = ( Polynom (a9,b9,c9,d9,( - 1 )) )) by A1;
A4: ( Polynom (a,b,c,d,2) ) = ( Polynom (a9,b9,c9,d9,2) ) by A1;
hence a = a9 by A2,A3;
thus b = b9 by A2,A3;
thus c = c9 by A2,A3,A4;
thus thesis by A2;
end;
