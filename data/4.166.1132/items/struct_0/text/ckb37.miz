requirements BOOLE,SUBSET,NUMERALS;
begin
definition
let S being 1-sorted;
redefine attr S is  trivial
means
:Def10: (for x,y being (Element of S) holds x = y);
compatibility
proof
set I = (the carrier of S);
per cases ;
suppose A1: I is non  empty;

thus (S is  trivial implies (for x,y being (Element of I) holds x = y))
proof
assume Z: I is  trivial;
let x being (Element of I);
let y being (Element of I);
thus thesis by Z,A1,ZFMISC_1:def 10;
end;

assume A4: (for x,y being (Element of I) holds x = y);
let x being set;
let y being set;
thus thesis by A4;
end;
suppose A5: I is  empty;

(for x,y being (Element of I) holds x = y)
proof
let x being (Element of I);
let y being (Element of I);
thus x = ( {} ) by A5,SUBSET_1:def 1
.= y by A5,SUBSET_1:def 1;
end;
hence thesis by A5;
end;
end;
end;
