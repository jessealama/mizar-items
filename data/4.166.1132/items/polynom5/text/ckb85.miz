theorem
Th67: (for L being  unital non  empty multMagma holds (for x being (Element of L) holds ( FPower (x,( 0 )) ) = ( (the carrier of L) --> x )))
proof
let L being  unital non  empty multMagma;
let x being (Element of L);
reconsider f = ( (the carrier of L) --> x ) as (Function of L,L);
now
let y being (Element of L);
thus ( f . y ) = x by FUNCOP_1:7
.= ( x * ( 1_ L ) ) by GROUP_1:def 4
.= ( x * ( ( power L ) . (y,( 0 )) ) ) by GROUP_1:def 7;
end;
hence thesis by CKB82:def 1;
end;
