begin
definition
let p being (FinSequence of (the carrier of ( F_Complex )));
func |.p .| -> (FinSequence of ( REAL )) means 
:Def1: (( len it ) = ( len p ) & (for n being (Element of ( NAT )) holds (n in ( dom p ) implies ( it /. n ) = |. ( p /. n ) .|)));
existence
proof
deffunc F(Nat) = |. ( p /. $1 ) .|;
consider q being (FinSequence of ( REAL )) such that A1: ( len q ) = ( len p ) and A2: (for n being Nat holds (n in ( dom q ) implies ( q /. n ) = F(n))) from FINSEQ_4:sch 2;
take q;
( dom p ) = ( dom q ) by A1,FINSEQ_3:29;
hence thesis by A1,A2;
end;
uniqueness
proof
let q1 being (FinSequence of ( REAL ));
let q2 being (FinSequence of ( REAL ));
assume that
A3: ( len q1 ) = ( len p )
and
A4: (for n being (Element of ( NAT )) holds (n in ( dom p ) implies ( q1 /. n ) = |. ( p /. n ) .|))
and
A5: ( len q2 ) = ( len p )
and
A6: (for n being (Element of ( NAT )) holds (n in ( dom p ) implies ( q2 /. n ) = |. ( p /. n ) .|));
A7:now
let n being Nat;
assume A8: n in ( dom p );
hence ( q1 /. n ) = |. ( p /. n ) .| by A4
.= ( q2 /. n ) by A6,A8;
end;
(( dom q1 ) = ( dom p ) & ( dom q2 ) = ( dom p )) by A3,A5,FINSEQ_3:29;
hence thesis by A7,FINSEQ_5:12;
end;
end;
