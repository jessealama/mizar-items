begin
reserve X for set;
theorem
Th33: (for X being non  empty set holds (for s being (FinSequence of ( bspace X )) holds (for f being (Element of ( bspace X )) holds (for x being (Element of X) holds ( ( s ^ <* f *> ) @ x ) = ( ( s @ x ) ^ <* ( f @ x ) *> )))))
proof
let X being non  empty set;
let s being (FinSequence of ( bspace X ));
let f being (Element of ( bspace X ));
let x being (Element of X);
set L = ( ( s ^ <* f *> ) @ x );
set R = ( ( s @ x ) ^ <* ( f @ x ) *> );
A1: ( len L ) = ( len ( s ^ <* f *> ) ) by CKB46:def 1
.= ( ( len s ) + ( len <* f *> ) ) by FINSEQ_1:22
.= ( ( len s ) + 1 ) by FINSEQ_1:39;
A2: (for k being Nat holds ((1 <= k & k <= ( len L )) implies ( L . k ) = ( R . k )))
proof
let k being Nat;
assume that
A3: 1 <= k
and
A4: k <= ( len L );
A5: k in ( NAT ) by ORDINAL1:def 12;
per cases  by A1,A4,NAT_1:8;
suppose A6: k <= ( len s );

( dom ( s @ x ) ) = ( Seg ( len ( s @ x ) ) ) by FINSEQ_1:def 3
.= ( Seg ( len s ) ) by CKB46:def 1;
then k in ( dom ( s @ x ) ) by A3,A5,A6;
then A7: ( R . k ) = ( ( s @ x ) . k ) by FINSEQ_1:def 7
.= ( ( s . k ) @ x ) by A3,A6,CKB46:def 1;
( dom s ) = ( Seg ( len s ) ) by FINSEQ_1:def 3;
then A8: k in ( dom s ) by A3,A5,A6;
k <= ( len ( s ^ <* f *> ) ) by A4,CKB46:def 1;
then ( L . k ) = ( ( ( s ^ <* f *> ) . k ) @ x ) by A3,CKB46:def 1;
hence thesis by A7,A8,FINSEQ_1:def 7;
end;
suppose A9: k = ( len L );

( dom <* ( f @ x ) *> ) = { 1 } by FINSEQ_1:2,FINSEQ_1:def 8;
then A10: 1 in ( dom <* ( f @ x ) *> ) by TARSKI:def 1;
( len ( s @ x ) ) = ( len s ) by CKB46:def 1;
then A11: ( R . k ) = ( <* ( f @ x ) *> . 1 ) by A1,A9,A10,FINSEQ_1:def 7
.= ( f @ x ) by FINSEQ_1:def 8;
( dom <* f *> ) = { 1 } by FINSEQ_1:2,FINSEQ_1:def 8;
then 1 in ( dom <* f *> ) by TARSKI:def 1;
then A12: ( ( s ^ <* f *> ) . k ) = ( <* f *> . 1 ) by A1,A9,FINSEQ_1:def 7
.= f by FINSEQ_1:def 8;
k <= ( len ( s ^ <* f *> ) ) by A4,CKB46:def 1;
hence thesis by A3,A11,A12,CKB46:def 1;
end;
end;
( len ( ( s @ x ) ^ <* ( f @ x ) *> ) ) = ( ( len ( s @ x ) ) + ( len <* ( f @ x ) *> ) ) by FINSEQ_1:22
.= ( ( len s ) + ( len <* ( f @ x ) *> ) ) by CKB46:def 1
.= ( ( len s ) + 1 ) by FINSEQ_1:39;
hence thesis by A1,A2,FINSEQ_1:14;
end;
