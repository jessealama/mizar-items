theorem
Th37: ( singletons X ) is  linearly-independent
proof
per cases ;
suppose X is  empty;

hence thesis;
end;
suppose X is non empty;

then reconsider X as non empty set;
set V = ( bspace X );
set S = ( singletons X );
(for l being (Linear_Combination of S) holds (( Sum l ) = ( 0. V ) implies ( Carrier l ) = ( {} )))
proof
let l being (Linear_Combination of S);
assume that
A1: ( Sum l ) = ( 0. V );
reconsider s = ( Sum l ) as (Subset of X);
set C = ( Carrier l );
A2: ( l ! ( Carrier l ) ) = l by RANKNULL:24;
assume C <> ( {} );
then consider f being (Element of V) such that A3: f in C by SUBSET_1:4;
reconsider f as (Subset of X);
reconsider g = f as (Element of V);
A4: { g } c= ( Carrier l ) by A3,ZFMISC_1:31;
reconsider n = ( l ! { g } ) as (Linear_Combination of { g });
reconsider m = ( l ! ( C \ { g } ) ) as (Linear_Combination of ( C \ { g } ));
reconsider l as (Linear_Combination of C) by A2;
reconsider t = ( Sum m ),u = ( Sum n ) as (Subset of X);
g in { g } by TARSKI:def 1;
then A5: (( Sum n ) = ( ( n . g ) * g ) & ( n . g ) = ( l . g )) by RANKNULL:25,VECTSP_6:17;
( l . g ) <> ( 0. ( Z_2 ) ) by A3,VECTSP_6:2;
then ( l . g ) = ( 1_ ( Z_2 ) ) by CKB8:1,CKB9:1,CARD_1:50,TARSKI:def 2;
then A6: u = f by A5,VECTSP_1:def 17;
C c= S by VECTSP_6:def 4;
then f is 1 -element by A3,CKB44:1;
then consider x being (Element of X) such that A8: f = { x } by CARD_1:65;
x in f by A8,TARSKI:def 1;
then A9: ( f @ x ) = ( 1. ( Z_2 ) ) by CKB12:def 1;
A10: (for g being (Subset of X) holds ((g <> f & g in C) implies ( g @ x ) = ( 0. ( Z_2 ) )))
proof
let g being (Subset of X);
assume that
A11: g <> f
and
A12: g in C;
C c= S by VECTSP_6:def 4;
then g is 1 -element by A12,CKB44:1;
then consider y being (Element of X) such that A14: g = { y } by CARD_1:65;
now
assume ( g @ x ) <> ( 0. ( Z_2 ) );
then x in { y } by A14,CKB12:def 1;
hence contradiction by A8,A11,A14,TARSKI:def 1;
end;
hence thesis;
end;
A15: ( t @ x ) = ( 0 )
proof
consider F being (FinSequence of V) such that A16: (F is  one-to-one & ( rng F ) = ( Carrier m )) and A17: t = ( Sum ( m (#) F ) ) by VECTSP_6:def 6;
A18: ( ( Sum ( m (#) F ) ) @ x ) = ( Sum ( ( m (#) F ) @ x ) ) by CKB50:1;
(for F being (FinSequence of V) holds ((F is  one-to-one & ( rng F ) = ( Carrier m )) implies ( ( m (#) F ) @ x ) = ( ( len F ) |-> ( 0. ( Z_2 ) ) )))
proof
let F being (FinSequence of V);
assume that
F is  one-to-one
and
A19: ( rng F ) = ( Carrier m );
set R = ( ( len F ) |-> ( 0. ( Z_2 ) ) );
set L = ( ( m (#) F ) @ x );
A20: ( len ( m (#) F ) ) = ( len F ) by VECTSP_6:def 5;
then A21: ( len L ) = ( len F ) by CKB46:def 1;
A22: (for k being Nat holds ((1 <= k & k <= ( len L )) implies ( L . k ) = ( R . k )))
proof
let k being Nat;
assume that
A23: (1 <= k & k <= ( len L ));
A24: k in ( NAT ) by ORDINAL1:def 12;
then A25: k in ( Seg ( len F ) ) by A21,A23;
( len ( m (#) F ) ) = ( len F ) by VECTSP_6:def 5;
then ( dom ( m (#) F ) ) = ( Seg ( len F ) ) by FINSEQ_1:def 3;
then k in ( dom ( m (#) F ) ) by A21,A23,A24;
then A26: ( ( m (#) F ) . k ) = ( ( m . ( F /. k ) ) * ( F /. k ) ) by VECTSP_6:def 5;
reconsider Fk = ( F /. k ) as (Subset of X);
A27: ( Carrier m ) c= ( C \ { f } ) by VECTSP_6:def 4;
( dom F ) = ( Seg ( len F ) ) by FINSEQ_1:def 3;
then A28: k in ( dom F ) by A21,A23,A24;
then A29: ( F /. k ) = ( F . k ) by PARTFUN1:def 6;
then ( m . ( F /. k ) ) = ( 1_ ( Z_2 ) ) by A19,A28,CKB51:1,FUNCT_1:3;
then A30: ( ( m (#) F ) . k ) = Fk by A26,VECTSP_1:def 17;
A31: ( F /. k ) in ( Carrier m ) by A19,A28,A29,FUNCT_1:3;
then A32: Fk in C by A27,XBOOLE_0:def 5;
A33: Fk <> f
proof
assume Fk = f;
then (not f in { f }) by A31,A27,XBOOLE_0:def 5;
hence contradiction by TARSKI:def 1;
end;
( L . k ) = ( ( ( m (#) F ) . k ) @ x ) by A20,A21,A23,CKB46:def 1
.= ( 0. ( Z_2 ) ) by A10,A30,A33,A32;
hence thesis by A25,FUNCOP_1:7;
end;
( dom R ) = ( Seg ( len F ) ) by FUNCOP_1:13;
then ( len L ) = ( len R ) by A21,FINSEQ_1:def 3;
hence thesis by A22,FINSEQ_1:14;
end;
then ( ( m (#) F ) @ x ) = ( ( len F ) |-> ( 0. ( Z_2 ) ) ) by A16;
hence thesis by A17,A18,CKB8:1,MATRIX_3:11;
end;
l = ( n + m ) by A4,RANKNULL:27;
