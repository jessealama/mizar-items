requirements NUMERALS,BOOLE,SUBSET,ARITHM;
begin
reserve L for LATTICE;
reserve l for (Element of L);
theorem
Th17: (for L being  up-complete  lower-bounded LATTICE holds (for X being (Subset of L) holds (X is  order-generating iff (for l1,l2 being (Element of L) holds ((not l2 <= l1) implies (ex p being (Element of L) st ((p in X & l1 <= p) & (not l2 <= p))))))))
proof
let L being  up-complete  lower-bounded LATTICE;
let X being (Subset of L);
thus (X is  order-generating implies (for l1,l2 being (Element of L) holds ((not l2 <= l1) implies (ex p being (Element of L) st ((p in X & l1 <= p) & (not l2 <= p))))))
proof
assume A1: X is  order-generating;
let l1 being (Element of L);
let l2 being (Element of L);
consider P being (Subset of X) such that A2: l1 = ( "/\" (P,L) ) by A1,CKB30:1;
assume A3: (not l2 <= l1);
now
assume (for p being (Element of L) holds (p in P implies l2 <= p));
then l2 is_<=_than P by LATTICE3:def 8;
hence contradiction by A3,A2,YELLOW_0:33;
end;
then consider p being (Element of L) such that A4: (p in P & (not l2 <= p));
take p;
l1 is_<=_than P by A2,YELLOW_0:33;
hence thesis by A4,LATTICE3:def 8;
end;

thus ((for l1,l2 being (Element of L) holds ((not l2 <= l1) implies (ex p being (Element of L) st ((p in X & l1 <= p) & (not l2 <= p))))) implies X is  order-generating)
proof
assume A5: (for l1,l2 being (Element of L) holds ((not l2 <= l1) implies (ex p being (Element of L) st ((p in X & l1 <= p) & (not l2 <= p)))));
let l being (Element of L);
set y = ( inf ( ( uparrow l ) /\ X ) );
thus  ex_inf_of ( ( uparrow l ) /\ X ),L by YELLOW_0:17;
A6: y is_<=_than ( ( uparrow l ) /\ X ) by YELLOW_0:33;
now
l is_<=_than ( ( uparrow l ) /\ X )
proof
let b being (Element of L);
assume b in ( ( uparrow l ) /\ X );
then b in ( uparrow l ) by XBOOLE_0:def 4;
hence thesis by WAYBEL_0:18;
end;
then l <= y by YELLOW_0:33;
then A7: (not y < l) by ORDERS_2:6;
assume A8: y <> l;
now
per cases  by A7,ORDERS_2:def 6;
suppose (not y <= l);

then consider p being (Element of L) such that A9: p in X and A10: l <= p and A11: (not y <= p) by A5;
p in ( uparrow l ) by A10,WAYBEL_0:18;
then p in ( ( uparrow l ) /\ X ) by A9,XBOOLE_0:def 4;
hence contradiction by A6,A11,LATTICE3:def 8;
end;
suppose y = l;

hence contradiction by A8;
end;
end;
hence contradiction;
end;
hence thesis;
end;

end;
