theorem
Th18: (for L being  lower-bounded  continuous LATTICE holds (for X being (Subset of L) holds (X = ( ( IRR L ) \ { ( Top L ) } ) implies X is  order-generating)))
proof
let L being  lower-bounded  continuous LATTICE;
let X being (Subset of L);
assume A1: X = ( ( IRR L ) \ { ( Top L ) } );
(for l1,l2 being (Element of L) holds ((not l2 <= l1) implies (ex p being (Element of L) st ((p in X & l1 <= p) & (not l2 <= p)))))
proof
let x being (Element of L),y being (Element of L);
assume (not y <= x);
then consider p being (Element of L) such that A2: p is  irreducible and A3: x <= p and A4: (not y <= p) by CKB28:1;
(p <> ( Top L ) & p in ( IRR L )) by A2,A4,CKB22:def 1,YELLOW_0:45;
then p in X by A1,ZFMISC_1:56;
hence thesis by A3,A4;
end;
hence thesis by CKB32:1;
end;
