begin
scheme NonUniqExD1 { Z() -> non empty RelStr,X() -> (Subset of Z()),Y() -> non empty (Subset of Z()),P[set,set] } : (ex f being (Function of X(),Y()) st (for e being (Element of Z()) holds (e in X() implies (ex u being (Element of Z()) st ((u in Y() & u = ( f . e )) & P[ e,u ])))))
provided
A1: (for e being (Element of Z()) holds (e in X() implies (ex u being (Element of Z()) st (u in Y() & P[ e,u ]))))
proof
A2: (for e being set holds (e in X() implies (ex u being set st (u in Y() & P[ e,u ]))))
proof
let e being set;
assume A3: e in X();
then reconsider e1 = e as (Element of X());
reconsider e1 as (Element of Z()) by A3;
consider u being (Element of Z()) such that A4: (u in Y() & P[ e1,u ]) by A1,A3;
reconsider u1 = u as set;
take u1;
thus thesis by A4;
end;
consider f being Function such that A5: (( dom f ) = X() & ( rng f ) c= Y()) and A6: (for e being set holds (e in X() implies P[ e,( f . e ) ])) from FUNCT_1:sch 5(A2);
reconsider f as (Function of X(),Y()) by A5,FUNCT_2:def 1,RELSET_1:4;
take f;
(for e being (Element of Z()) holds (e in X() implies (ex u being (Element of Z()) st ((u in Y() & u = ( f . e )) & P[ e,u ]))))
proof
let e being (Element of Z());
assume A7: e in X();
then reconsider e1 = ( f . e ) as (Element of Y()) by FUNCT_2:5;
reconsider fe = e1 as (Element of Z());
take fe;
thus thesis by A6,A7;
end;
hence thesis;
end;
