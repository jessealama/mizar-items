theorem
Th23: (for L being sup-Semilattice holds (for x being (Element of L) holds (x is  co-prime iff (for A being  finite non  empty (Subset of L) holds (x <= ( sup A ) implies (ex a being (Element of L) st (a in A & x <= a)))))))
proof
let L being sup-Semilattice;
let x being (Element of L);
thus (x is  co-prime implies (for A being  finite non  empty (Subset of L) holds (x <= ( sup A ) implies (ex a being (Element of L) st (a in A & x <= a)))))
proof
assume x is  co-prime;
then A1: ( x ~ ) is  prime by CKB37:def 1;
let A being  finite non  empty (Subset of L);
reconsider A1 = A as  finite non  empty (Subset of ( L ~ ));
assume x <= ( sup A );
then A2: ( x ~ ) >= ( ( sup A ) ~ ) by LATTICE3:9;
A3:  ex_sup_of A,L by YELLOW_0:54;
then ( "\/" (A,L) ) is_>=_than A by YELLOW_0:def 9;
then A4: ( ( "\/" (A,L) ) ~ ) is_<=_than A by YELLOW_7:8;
A5:now
let y being (Element of ( L ~ ));
assume y is_<=_than A;
then ( ~ y ) is_>=_than A by YELLOW_7:9;
then ( ~ y ) >= ( "\/" (A,L) ) by A3,YELLOW_0:def 9;
hence y <= ( ( "\/" (A,L) ) ~ ) by YELLOW_7:2;
end;
 ex_inf_of A,( L ~ ) by A3,YELLOW_7:10;
then ( ( sup A ) ~ ) = ( inf A1 ) by A4,A5,YELLOW_0:def 10;
then consider a being (Element of ( L ~ )) such that A6: (a in A1 & ( x ~ ) >= a) by A1,A2,CKB41:1;
take ( ~ a );
thus thesis by A6,YELLOW_7:2;
end;

thus ((for A being  finite non  empty (Subset of L) holds (x <= ( sup A ) implies (ex a being (Element of L) st (a in A & x <= a)))) implies x is  co-prime)
proof
assume A7: (for A being  finite non  empty (Subset of L) holds (x <= ( sup A ) implies (ex a being (Element of L) st (a in A & x <= a))));
now
let a being (Element of ( L ~ ));
let b being (Element of ( L ~ ));
set A = { ( ~ a ),( ~ b ) };
assume ( a "/\" b ) <= ( x ~ );
then x <= ( ~ ( a "/\" b ) ) by YELLOW_7:2;
then (( sup A ) = ( ( ~ a ) "\/" ( ~ b ) ) & x <= ( ( ~ a ) "\/" ( ~ b ) )) by YELLOW_0:41,YELLOW_7:24;
then consider l being (Element of L) such that A8: l in A and A9: x <= l by A7;
(l = ( ~ a ) or l = ( ~ b )) by A8,TARSKI:def 2;
hence (a <= ( x ~ ) or b <= ( x ~ )) by A9,YELLOW_7:2;
end;
then ( x ~ ) is  prime by CKB35:def 1;
hence thesis by CKB37:def 1;
end;

end;
