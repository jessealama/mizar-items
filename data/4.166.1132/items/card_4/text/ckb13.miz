theorem
Th61: (for f holds ((( dom f ) is  countable & (for x holds (x in ( dom f ) implies ( f . x ) is  countable))) implies ( Union f ) is  countable))
proof
let f;
assume that
A1: ( card ( dom f ) ) c= ( omega )
and
A2: (for x holds (x in ( dom f ) implies ( f . x ) is  countable));
now
let x;
assume x in ( dom f );
then ( f . x ) is  countable by A2;
hence ( card ( f . x ) ) c= ( omega ) by CARD_3:def 14;
end;
hence ( card ( Union f ) ) c= ( omega ) by A1,CKB8:1,CARD_2:86;
end;
