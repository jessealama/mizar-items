proof
assume f is_hpartial_differentiable`23_in u;
then consider x0 being Real,y0 being Real,z0 being Real such that A1: (u = <* x0,y0,z0 *> & (ex N being (Neighbourhood of z0) st (N c= ( dom ( SVF1 (3,( pdiff1 (f,2) ),u) ) ) & (ex L st (ex R st (for z holds (z in N implies ( ( ( SVF1 (3,( pdiff1 (f,2) ),u) ) . z ) - ( ( SVF1 (3,( pdiff1 (f,2) ),u) ) . z0 ) ) = ( ( L . ( z - z0 ) ) + ( R . ( z - z0 ) ) )))))))) by CKB6:def 1;
thus thesis by A1,PDIFF_4:15;
end;

assume ( pdiff1 (f,2) ) is_partial_differentiable_in u,3;
then consider x0 being Real,y0 being Real,z0 being Real such that A2: (u = <* x0,y0,z0 *> & (ex N being (Neighbourhood of z0) st (N c= ( dom ( SVF1 (3,( pdiff1 (f,2) ),u) ) ) & (ex L st (ex R st (for z holds (z in N implies ( ( ( SVF1 (3,( pdiff1 (f,2) ),u) ) . z ) - ( ( SVF1 (3,( pdiff1 (f,2) ),u) ) . z0 ) ) = ( ( L . ( z - z0 ) ) + ( R . ( z - z0 ) ) )))))))) by PDIFF_4:15;
thus thesis by A2,CKB6:def 1;
end;
