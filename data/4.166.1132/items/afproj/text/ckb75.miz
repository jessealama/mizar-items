requirements SUBSET,BOOLE;
begin
reserve AS for AffinSpace;
reserve a for (POINT of ( IncProjSp_of AS ));
reserve b for (POINT of ( IncProjSp_of AS ));
reserve c for (POINT of ( IncProjSp_of AS ));
reserve A for (LINE of ( IncProjSp_of AS ));
theorem
((not AS is AffinPlane) implies ( ProjHorizon AS ) is IncProjSp)
proof
set XX = ( ProjHorizon AS );
A1: (for a,b being (Element of (the Points of XX)) holds (ex A being (Element of (the Lines of XX)) st (a on A & b on A))) by CKB54:1;
A2: (for A being (Element of (the Lines of XX)) holds (ex a,b,c being (Element of (the Points of XX)) st (((((a <> b & b <> c) & c <> a) & a on A) & b on A) & c on A)))
proof
let A being (Element of (the Lines of XX));
consider a,b,c being (Element of (the Points of XX)) such that A3: a on A and A4: b on A and A5: c on A and A6: a <> b and A7: b <> c and A8: c <> a by CKB53:1;
take a;
take b;
take c;
thus thesis by A3,A4,A5,A6,A7,A8;
end;
assume (not AS is AffinPlane);
then A9: (ex a being (Element of (the Points of XX)) st (ex A being (Element of (the Lines of XX)) st (not a on A))) by CKB55:1;
A10: (for a,b,c,d,p,q being (Element of (the Points of XX)) holds (for M,N,P,Q being (Element of (the Lines of XX)) holds (((((((((((((a on M & b on M) & c on N) & d on N) & p on M) & p on N) & a on P) & c on P) & b on Q) & d on Q) & (not p on P)) & (not p on Q)) & M <> N) implies (ex q being (Element of (the Points of XX)) st (q on P & q on Q))))) by CKB70:1;
(for a,b being (Element of (the Points of XX)) holds (for A,K being (Element of (the Lines of XX)) holds ((((a on A & b on A) & a on K) & b on K) implies (a = b or A = K)))) by CKB52:1;
hence thesis by A1,A9,A2,A10,INCPROJ:def 4,INCPROJ:def 5,INCPROJ:def 6,INCPROJ:def 7,INCPROJ:def 8;
end;
