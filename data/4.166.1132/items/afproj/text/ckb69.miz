assume A97: AC // BD;
take q;
q = q9 by A84,A81,A97,CKB17:1;
hence (q on P & q on Q) by A31,A34,A71,A73,A83,A79,A84,A81,A55,A56,A57,A45,A46,A47,A92,CKB45:1,AFF_4:19;
end;
A98: AC c= XP by A31,A34,A83,A55,A56,A57,AFF_4:19;
A99:now
given w such that
A100: w in AC
and
A101: w in BD;

