then A104: R is  being_line by AFF_1:def 3;
then reconsider q = ( LDir R ) as (POINT of ( IncProjSp_of AS )) by CKB34:1;
take q;
thus (q on P & q on Q) by A53,A42,A55,A57,A45,A47,A92,A98,A85,A100,A101,A102,A104,CKB45:1,AFF_4:19;
end;
t in Y2 by A35,AFF_4:def 3;
then AC c= X3 by A76,A87,A88,A89,A83,AFF_4:19;
hence thesis by A89,A84,A81,A90,A96,A99,AFF_4:22;
end;
hence thesis by A1,A2,A3,A4,A5,A6,A7,A8,A9,A10,A11,A12,A13,A14,CKB68:1;
end;
hence thesis by A1,A2,A3,A4,A5,A6,A7,A8,A9,A10,A11,A12,A13,CKB66:1;
