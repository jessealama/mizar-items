constructors DOMAIN_1,EQREL_1,AFF_1,AFF_2,TRANSLAC,INCPROJ,AFF_4,RELSET_1,CKB7,CKB8,CKB11,CKB12,CKB13,CKB14,CKB20,CKB21,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33;
requirements SUBSET,BOOLE;
begin
reserve AS for AffinSpace;
reserve X for (Subset of AS);
reserve Y for (Subset of AS);
reserve a for (POINT of ( IncProjSp_of AS ));
reserve b for (POINT of ( IncProjSp_of AS ));
reserve c for (POINT of ( IncProjSp_of AS ));
reserve d for (POINT of ( IncProjSp_of AS ));
reserve p for (POINT of ( IncProjSp_of AS ));
reserve q for (POINT of ( IncProjSp_of AS ));
reserve M for (LINE of ( IncProjSp_of AS ));
reserve N for (LINE of ( IncProjSp_of AS ));
reserve P for (LINE of ( IncProjSp_of AS ));
reserve Q for (LINE of ( IncProjSp_of AS ));
theorem
Lm10: (((((((((((((((a on M & b on M) & c on N) & d on N) & p on M) & p on N) & a on P) & c on P) & b on Q) & d on Q) & (not p on P)) & (not p on Q)) & M <> N) & (not p is (Element of AS))) & a is (Element of AS)) implies (ex q st (q on P & q on Q)))
proof
assume that
A1: a on M
and
A2: b on M
and
A3: c on N
and
A4: d on N
and
A5: p on M
and
A6: p on N
and
A7: a on P
and
A8: c on P
and
A9: b on Q
and
A10: d on Q
and
A11: (not p on P)
and
A12: (not p on Q)
and
A13: M <> N
and
A14: (not p is (Element of AS))
and
A15: a is (Element of AS);
reconsider x = a as (Element of AS) by A15;
consider XM being (Subset of AS) such that A16: ((M = [ XM,1 ] & XM is  being_line) or (M = [ ( PDir XM ),2 ] & XM is  being_plane)) by CKB37:1;
A17: x in XM by A1,A16,CKB40:1,CKB41:1;
A18: b <> d by A2,A4,A5,A6,A9,A12,A13,CKB56:1;
consider XN being (Subset of AS) such that A19: ((N = [ XN,1 ] & XN is  being_line) or (N = [ ( PDir XN ),2 ] & XN is  being_plane)) by CKB37:1;
consider XP being (Subset of AS) such that A20: ((P = [ XP,1 ] & XP is  being_line) or (P = [ ( PDir XP ),2 ] & XP is  being_plane)) by CKB37:1;
A21: x = a;
then reconsider y = b as (Element of AS) by A1,A2,A5,A9,A12,A14,A16,CKB41:1,CKB49:1;
A22: y in XM by A2,A16,CKB40:1,CKB41:1;
consider X such that A23: p = ( LDir X ) and A24: X is  being_line by A14,CKB34:1;
consider XQ being (Subset of AS) such that A25: ((Q = [ XQ,1 ] & XQ is  being_line) or (Q = [ ( PDir XQ ),2 ] & XQ is  being_plane)) by CKB37:1;
A26: x in XP by A7,A20,CKB40:1,CKB41:1;
then consider Y such that A27: XM c= Y and A28: XP c= Y and A29: Y is  being_plane by A1,A7,A16,A20,A17,CKB41:1,AFF_4:38;
A30: y = b;
A31: X '||' XM by A1,A5,A23,A24,A16,A21,CKB41:1,CKB42:1;
then A32: X // XM by A1,A24,A16,A21,CKB41:1,AFF_4:40;
A33: y in XQ by A9,A25,CKB40:1,CKB41:1;
A34: (not XM // XP) by A1,A5,A7,A11,A16,A20,A17,A26,CKB41:1,AFF_1:45;
A35:now
A36: X // XM by A1,A24,A16,A21,A31,CKB41:1,AFF_4:40;
assume that
A37: N = [ XN,1 ]
and
A38: XN is  being_line;
X '||' XN by A6,A23,A24,A37,A38,CKB42:1;
then X // XN by A24,A38,AFF_4:40;
then A39: XM // XN by A36,AFF_1:44;
c is (Element of AS)
proof
assume (not thesis);
then c = ( LDir XN ) by A3,A37,A38,CKB48:1;
then XN '||' XP by A7,A8,A20,A21,A38,CKB41:1,CKB42:1;
then XN // XP by A7,A20,A21,A38,CKB41:1,AFF_4:40;
hence contradiction by A34,A39,AFF_1:44;
end;
then reconsider z = c as (Element of AS);
z in XN by A3,A37,CKB40:1;
then A40: XN = ( z * XM ) by A1,A16,A21,A39,CKB41:1,AFF_4:def 3;
A41: (not XN // XQ)
proof
assume XN // XQ;
then XM // XQ by A39,AFF_1:44;
hence contradiction by A2,A5,A9,A12,A16,A25,A33,A22,CKB41:1,AFF_1:45;
end;
now
assume (not d is (Element of AS));
then consider Xd being (Subset of AS) such that A42: d = ( LDir Xd ) and A43: Xd is  being_line by CKB34:1;
Xd '||' XN by A4,A37,A38,A42,A43,CKB42:1;
then A44: Xd // XN by A38,A43,AFF_4:40;
Xd '||' XQ by A9,A10,A25,A30,A42,A43,CKB41:1,CKB42:1;
then Xd // XQ by A9,A25,A30,A43,CKB41:1,AFF_4:40;
hence contradiction by A41,A44,AFF_1:44;
end;
then reconsider w = d as (Element of AS);
w in XQ by A10,A25,CKB40:1,CKB41:1;
then A45: XQ = ( Line (y,w) ) by A9,A18,A25,A33,CKB41:1,AFF_1:57;
z in XP by A8,A20,CKB40:1,CKB41:1;
then A46: XN c= Y by A1,A16,A21,A27,A28,A29,A40,CKB41:1,AFF_4:28;
w in XN by A4,A37,CKB40:1;
then XQ c= Y by A18,A27,A29,A22,A46,A45,AFF_4:19;
hence thesis by A7,A9,A20,A25,A21,A28,A29,A30,CKB41:1,CKB65:1;
end;
A47: XP '||' Y by A7,A20,A21,A28,A29,CKB41:1,AFF_4:42;
A48: XM '||' Y by A1,A16,A21,A27,A29,CKB41:1,AFF_4:42;
now
assume that
A49: N = [ ( PDir XN ),2 ]
and
A50: XN is  being_plane;
(not c is (Element of AS)) by A3,A49,CKB41:1;
then c = ( LDir XP ) by A7,A8,A20,A21,CKB41:1,CKB48:1;
then A51: XP '||' XN by A3,A7,A20,A21,A49,A50,CKB41:1,CKB43:1;
(not d is (Element of AS)) by A4,A49,CKB41:1;
then d = ( LDir XQ ) by A9,A10,A25,A30,CKB41:1,CKB48:1;
then A52: XQ '||' XN by A4,A9,A25,A30,A49,A50,CKB41:1,CKB43:1;
X '||' XN by A6,A23,A24,A49,A50,CKB43:1;
then XM '||' XN by A32,AFF_4:56;
then XN '||' Y by A1,A5,A7,A11,A16,A20,A17,A26,A29,A48,A47,A50,A51,CKB5:1,CKB41:1,AFF_1:45;
then XQ '||' Y by A50,A52,AFF_4:59,AFF_4:60;
then XQ c= Y by A9,A25,A27,A29,A33,A22,CKB41:1,AFF_4:43;
hence thesis by A7,A9,A20,A25,A21,A28,A29,A30,CKB41:1,CKB65:1;
end;
hence thesis by A19,A35;
end;
