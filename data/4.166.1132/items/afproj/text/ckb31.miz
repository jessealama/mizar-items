begin
reserve AS for AffinSpace;
reserve A for (Subset of AS);
reserve X for (Subset of AS);
reserve x for set;
reserve y for set;
definition
let AS;
func Inc_of_Dir AS -> (Relation of ( Dir_of_Lines AS ),( Dir_of_Planes AS ))
means
:Def12: (for x holds (for y holds ([ x,y ] in it iff (ex A st (ex X st ((((x = ( LDir A ) & y = ( PDir X )) & A is  being_line) & X is  being_plane) & A '||' X))))));
existence
proof
defpred P[ set,set ]
 means
(ex A st (ex X st (((($1 = ( LDir A ) & $2 = ( PDir X )) & A is  being_line) & X is  being_plane) & A '||' X)));
set VV1 = ( Dir_of_Lines AS );
set VV2 = ( Dir_of_Planes AS );
consider P being (Relation of VV1,VV2) such that A1: ([ x,y ] in P iff ((x in VV1 & y in VV2) & P[ x,y ])) from RELSET_1:sch 1;
take P;
let x;
let y;
thus ([ x,y ] in P implies (ex A st (ex X st ((((x = ( LDir A ) & y = ( PDir X )) & A is  being_line) & X is  being_plane) & A '||' X)))) by A1;
assume A2: (ex A st (ex X st ((((x = ( LDir A ) & y = ( PDir X )) & A is  being_line) & X is  being_plane) & A '||' X)));
then A3: y in VV2 by CKB23:1;
x in VV1 by A2,CKB22:1;
hence thesis by A1,A2,A3;
end;
uniqueness
proof
let P being (Relation of ( Dir_of_Lines AS ),( Dir_of_Planes AS )),Q being (Relation of ( Dir_of_Lines AS ),( Dir_of_Planes AS ));
assume that
A4: ([ x,y ] in P iff (ex A st (ex X st ((((x = ( LDir A ) & y = ( PDir X )) & A is  being_line) & X is  being_plane) & A '||' X))))
and
A5: ([ x,y ] in Q iff (ex A st (ex X st ((((x = ( LDir A ) & y = ( PDir X )) & A is  being_line) & X is  being_plane) & A '||' X))));
(for x holds (for y holds ([ x,y ] in P iff [ x,y ] in Q)))
proof
let x;
let y;
([ x,y ] in P iff (ex A st (ex X st ((((x = ( LDir A ) & y = ( PDir X )) & A is  being_line) & X is  being_plane) & A '||' X)))) by A4;
hence thesis by A5;
end;
hence thesis by RELAT_1:def 2;
