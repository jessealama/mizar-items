and
A11: a9 in N
and
A12: b9 in N
and
A13: c9 in N
and
A14: a,b9 // b,a9
and
A15: b,c9 // c,b9
and
A16: ((a = b or b = c) or a = c);
A17: c <> b9 by A1,A2,A3,A4,A5,A7,A10,A12,AFF_1:18;
now
