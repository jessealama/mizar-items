theorem
Th48: (for M,N being (Subset of AS) holds (for o,a,b,c,a9,b9,c9 being (Element of AS) holds ((((((((((((((((M is  being_line & N is  being_line) & M <> N) & o in M) & o in N) & o <> b) & o <> b9) & o <> c9) & b in M) & c in M) & a9 in N) & b9 in N) & c9 in N) & a,b9 // b,a9) & b,c9 // c,b9) & ((a = b or b = c) or a = c)) implies a,c9 // c,a9)))
proof
let M being (Subset of AS);
let N being (Subset of AS);
let o being (Element of AS);
let a being (Element of AS);
let b being (Element of AS);
let c being (Element of AS);
let a9 being (Element of AS);
let b9 being (Element of AS);
