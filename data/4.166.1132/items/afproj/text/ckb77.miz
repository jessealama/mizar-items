then b,c9 // b,a9 by A14,A15,A17,AFF_1:5;
then a9 = c9 by A1,A2,A3,A4,A5,A6,A8,A9,A11,A13,AFF_4:9;
hence thesis by A18,AFF_1:2;
end;
