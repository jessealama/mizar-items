begin
theorem
Th46: (for X being non  empty set holds (for S being (SigmaField of X) holds (for M being (sigma_Measure of S) holds (for f,g being (PartFunc of X,( ExtREAL )) holds (((f is_simple_func_in S & g is_simple_func_in S) & (for x being set holds (x in ( dom ( f - g ) ) implies ( g . x ) <= ( f . x )))) implies ( f - g ) is  nonnegative)))))
proof
let X being non  empty set;
let S being (SigmaField of X);
let M being (sigma_Measure of S);
let f being (PartFunc of X,( ExtREAL ));
let g being (PartFunc of X,( ExtREAL ));
assume that
A1: f is_simple_func_in S
and
A2: g is_simple_func_in S
and
A3: (for x being set holds (x in ( dom ( f - g ) ) implies ( g . x ) <= ( f . x )));
g is  without-infty by A2,CKB22:1;
then (not ( -infty ) in ( rng g )) by CKB12:def 1;
then A4: ( g " { ( -infty ) } ) = ( {} ) by FUNCT_1:72;
f is  without+infty by A1,CKB22:1;
then (not ( +infty ) in ( rng f )) by CKB13:def 1;
then A5: ( f " { ( +infty ) } ) = ( {} ) by FUNCT_1:72;
then ( ( ( dom f ) /\ ( dom g ) ) \ ( ( ( f " { ( +infty ) } ) /\ ( g " { ( +infty ) } ) ) \/ ( ( f " { ( -infty ) } ) /\ ( g " { ( -infty ) } ) ) ) ) = ( ( dom f ) /\ ( dom g ) ) by A4;
then A6: ( dom ( f - g ) ) = ( ( dom f ) /\ ( dom g ) ) by MESFUNC1:def 4;
(for x being set holds (x in ( ( dom f ) /\ ( dom g ) ) implies ((( g . x ) <= ( f . x ) & ( -infty ) < ( g . x )) & ( f . x ) < ( +infty ))))
proof
let x being set;
assume A7: x in ( ( dom f ) /\ ( dom g ) );
hence ( g . x ) <= ( f . x ) by A3,A6;
x in ( dom g ) by A7,XBOOLE_0:def 4;
then (not ( g . x ) in { ( -infty ) }) by A4,FUNCT_1:def 7;
then (not ( g . x ) = ( -infty )) by TARSKI:def 1;
hence ( -infty ) < ( g . x ) by XXREAL_0:6;
x in ( dom f ) by A7,XBOOLE_0:def 4;
then (not ( f . x ) in { ( +infty ) }) by A5,FUNCT_1:def 7;
then (not ( f . x ) = ( +infty )) by TARSKI:def 1;
hence thesis by XXREAL_0:4;
end;
hence thesis by CKB30:1;
end;
