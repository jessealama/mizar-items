Th73: (for X being non  empty set holds (for S being (SigmaField of X) holds (for M being (sigma_Measure of S) holds (for f being (PartFunc of X,( ExtREAL )) holds (for A,B being (Element of S) holds (((f is_simple_func_in S & f is  nonnegative) & A misses B) implies ( integral' (M,( f | ( A \/ B ) )) ) = ( ( integral' (M,( f | A )) ) + ( integral' (M,( f | B )) ) )))))))
proof
let X being non  empty set;
let S being (SigmaField of X);
let M being (sigma_Measure of S);
let f being (PartFunc of X,( ExtREAL ));
let A being (Element of S);
let B being (Element of S);
assume that
A1: f is_simple_func_in S
and
A2: f is  nonnegative
and
A3: A misses B;
set g2 = ( f | B );
set g1 = ( f | A );
set g = ( f | ( A \/ B ) );
g is  nonnegative by A2,CKB23:1;
then A4: (for x being set holds (x in ( dom g ) implies ( 0 ) <= ( g . x ))) by SUPINF_2:51;
then consider G being (Finite_Sep_Sequence of S),b being (FinSequence of ( ExtREAL )) such that A5: G,b are_Re-presentation_of g and A6: ( b . 1 ) = ( 0 ) and A7: (for n being Nat holds ((2 <= n & n in ( dom b )) implies (( 0 ) < ( b . n ) & ( b . n ) < ( +infty )))) by A1,CKB44:1,MESFUNC3:14;
deffunc G1(Nat) = ( ( G . $1 ) /\ A );
consider G1 being FinSequence such that A8: (( len G1 ) = ( len G ) & (for k being Nat holds (k in ( dom G1 ) implies ( G1 . k ) = G1(k)))) from FINSEQ_1:sch 2;
A9: ( dom G1 ) = ( Seg ( len G ) ) by A8,FINSEQ_1:def 3;
A10: (for k being Nat holds (k in ( dom G ) implies ( G1 . k ) = ( ( G . k ) /\ A )))
proof
let k being Nat;
assume k in ( dom G );
then k in ( Seg ( len G ) ) by FINSEQ_1:def 3;
hence thesis by A8,A9;
end;
deffunc G2(Nat) = ( ( G . $1 ) /\ B );
consider G2 being FinSequence such that A11: (( len G2 ) = ( len G ) & (for k being Nat holds (k in ( dom G2 ) implies ( G2 . k ) = G2(k)))) from FINSEQ_1:sch 2;
A12: ( dom G2 ) = ( Seg ( len G ) ) by A11,FINSEQ_1:def 3;
A13: (for k being Nat holds (k in ( dom G ) implies ( G2 . k ) = ( ( G . k ) /\ B )))
proof
let k being Nat;
assume k in ( dom G );
then k in ( Seg ( len G ) ) by FINSEQ_1:def 3;
hence thesis by A11,A12;
end;
A14: ( dom G ) = ( Seg ( len G2 ) ) by A11,FINSEQ_1:def 3
.= ( dom G2 ) by FINSEQ_1:def 3;
then reconsider G2 as (Finite_Sep_Sequence of S) by A13,CKB45:1;
A15: ( dom ( g | B ) ) = ( ( dom g ) /\ B ) by RELAT_1:61
.= ( ( ( dom f ) /\ ( A \/ B ) ) /\ B ) by RELAT_1:61
.= ( ( dom f ) /\ ( ( A \/ B ) /\ B ) ) by XBOOLE_1:16
.= ( ( dom f ) /\ B ) by XBOOLE_1:21
.= ( dom g2 ) by RELAT_1:61;
(for x being set holds (x in ( dom ( g | B ) ) implies ( ( g | B ) . x ) = ( g2 . x )))
proof
let x being set;
assume A16: x in ( dom ( g | B ) );
then x in ( ( dom g ) /\ B ) by RELAT_1:61;
then A17: x in ( dom g ) by XBOOLE_0:def 4;
( ( g | B ) . x ) = ( g . x ) by A16,FUNCT_1:47
.= ( f . x ) by A17,FUNCT_1:47;
hence thesis by A15,A16,FUNCT_1:47;
end;
then ( g | B ) = g2 by A15,FUNCT_1:2;
then A18: G2,b are_Re-presentation_of g2 by A5,A14,A13,CKB46:1;
A19: ( dom G ) = ( Seg ( len G1 ) ) by A8,FINSEQ_1:def 3
.= ( dom G1 ) by FINSEQ_1:def 3;
then reconsider G1 as (Finite_Sep_Sequence of S) by A10,CKB45:1;
A20: ( dom ( g | A ) ) = ( ( dom g ) /\ A ) by RELAT_1:61
.= ( ( ( dom f ) /\ ( A \/ B ) ) /\ A ) by RELAT_1:61
.= ( ( dom f ) /\ ( ( A \/ B ) /\ A ) ) by XBOOLE_1:16
.= ( ( dom f ) /\ A ) by XBOOLE_1:21
.= ( dom g1 ) by RELAT_1:61;
(for x being set holds (x in ( dom ( g | A ) ) implies ( ( g | A ) . x ) = ( g1 . x )))
proof
let x being set;
assume A21: x in ( dom ( g | A ) );
then x in ( ( dom g ) /\ A ) by RELAT_1:61;
then A22: x in ( dom g ) by XBOOLE_0:def 4;
( ( g | A ) . x ) = ( g . x ) by A21,FUNCT_1:47
.= ( f . x ) by A22,FUNCT_1:47;
hence thesis by A20,A21,FUNCT_1:47;
end;
then ( g | A ) = g1 by A20,FUNCT_1:2;
then A23: G1,b are_Re-presentation_of g1 by A5,A19,A10,CKB46:1;
deffunc Fy(Nat) = ( ( b . $1 ) * ( ( M * G ) . $1 ) );
consider y being (FinSequence of ( ExtREAL )) such that A24: (( len y ) = ( len G ) & (for j being Nat holds (j in ( dom y ) implies ( y . j ) = Fy(j)))) from FINSEQ_2:sch 1;
A25: ( dom y ) = ( Seg ( len y ) ) by FINSEQ_1:def 3
.= ( dom G ) by A24,FINSEQ_1:def 3;
per cases ;
suppose A26: ( dom ( f | ( A \/ B ) ) ) = ( {} );

( ( dom f ) /\ B ) c= ( ( dom f ) /\ ( A \/ B ) ) by XBOOLE_1:7,XBOOLE_1:26;
then ( dom ( f | B ) ) c= ( ( dom f ) /\ ( A \/ B ) ) by RELAT_1:61;
then ( dom ( f | B ) ) c= ( dom ( f | ( A \/ B ) ) ) by RELAT_1:61;
then ( dom ( f | B ) ) = ( {} ) by A26;
then A27: ( integral' (M,g2) ) = ( 0 ) by CKB90:def 1;
( ( dom f ) /\ A ) c= ( ( dom f ) /\ ( A \/ B ) ) by XBOOLE_1:7,XBOOLE_1:26;
then ( dom ( f | A ) ) c= ( ( dom f ) /\ ( A \/ B ) ) by RELAT_1:61;
then ( dom ( f | A ) ) c= ( dom ( f | ( A \/ B ) ) ) by RELAT_1:61;
then ( dom ( f | A ) ) = ( {} ) by A26;
then A28: ( integral' (M,g1) ) = ( 0 ) by CKB90:def 1;
( integral' (M,g) ) = ( 0 ) by A26,CKB90:def 1;
hence thesis by A28,A27;
end;
suppose A29: ( dom ( f | ( A \/ B ) ) ) <> ( {} );

then ( integral (X,S,M,g) ) = ( Sum y ) by A1,A4,A5,A24,A25,CKB44:1,MESFUNC4:3;
then A30: ( integral' (M,g) ) = ( Sum y ) by A29,CKB90:def 1;
per cases ;
suppose A31: ( dom ( f | A ) ) = ( {} );

A32: ( dom ( f | ( A \/ B ) ) ) = ( ( dom f ) /\ ( A \/ B ) ) by RELAT_1:61
.= ( ( ( dom f ) /\ A ) \/ ( ( dom f ) /\ B ) ) by XBOOLE_1:23
.= ( ( dom ( f | A ) ) \/ ( ( dom f ) /\ B ) ) by RELAT_1:61
.= ( dom ( f | B ) ) by A31,RELAT_1:61;
now
let x being set;
assume A33: x in ( dom g );
then ( g . x ) = ( f . x ) by FUNCT_1:47;
hence ( g . x ) = ( g2 . x ) by A32,A33,FUNCT_1:47;
end;
then A34: g = g2 by A32,FUNCT_1:2;
( integral' (M,g1) ) = ( 0 ) by A31,CKB90:def 1;
hence thesis by A34,XXREAL_3:4;
end;
suppose A35: ( dom ( f | A ) ) <> ( {} );

per cases ;
suppose A36: ( dom ( f | B ) ) = ( {} );

A37: ( dom ( f | ( A \/ B ) ) ) = ( ( dom f ) /\ ( A \/ B ) ) by RELAT_1:61
.= ( ( ( dom f ) /\ B ) \/ ( ( dom f ) /\ A ) ) by XBOOLE_1:23
.= ( ( dom ( f | B ) ) \/ ( ( dom f ) /\ A ) ) by RELAT_1:61
.= ( dom ( f | A ) ) by A36,RELAT_1:61;
now
let x being set;
assume A38: x in ( dom g );
then ( g . x ) = ( f . x ) by FUNCT_1:47;
hence ( g . x ) = ( g1 . x ) by A37,A38,FUNCT_1:47;
end;
then A39: g = g1 by A37,FUNCT_1:2;
( integral' (M,g2) ) = ( 0 ) by A36,CKB90:def 1;
hence thesis by A39,XXREAL_3:4;
end;
suppose A40: ( dom ( f | B ) ) <> ( {} );

g2 is  nonnegative by A2,CKB23:1;
then A41: (for x being set holds (x in ( dom g2 ) implies ( 0 ) <= ( g2 . x ))) by SUPINF_2:51;
deffunc Fy2(Nat) = ( ( b . $1 ) * ( ( M * G2 ) . $1 ) );
consider y2 being (FinSequence of ( ExtREAL )) such that A42: (( len y2 ) = ( len G2 ) & (for j being Nat holds (j in ( dom y2 ) implies ( y2 . j ) = Fy2(j)))) from FINSEQ_2:sch 1;
A43: (for k being Nat holds (k in ( dom y2 ) implies ( 0 ) <= ( y2 . k )))
proof
let k being Nat;
assume A44: k in ( dom y2 );
then k in ( Seg ( len y2 ) ) by FINSEQ_1:def 3;
then A45: 1 <= k by FINSEQ_1:1;
A46: ( dom b ) = ( dom G ) by A5,MESFUNC3:def 1
.= ( Seg ( len y2 ) ) by A11,A42,FINSEQ_1:def 3
.= ( dom y2 ) by FINSEQ_1:def 3;
A47:now
per cases ;
suppose k = 1;

hence ( 0 ) <= ( b . k ) by A6;
end;
suppose k <> 1;

then 1 < k by A45,XXREAL_0:1;
then ( 1 + 1 ) <= k by NAT_1:13;
hence ( 0 ) <= ( b . k ) by A7,A44,A46;
end;
end;
k in ( Seg ( len G2 ) ) by A42,A44,FINSEQ_1:def 3;
then A48: k in ( dom G2 ) by FINSEQ_1:def 3;
then A49: ( ( M * G2 ) . k ) = ( M . ( G2 . k ) ) by FUNCT_1:13;
( G2 . k ) in ( rng G2 ) by A48,FUNCT_1:3;
then reconsider G2k = ( G2 . k ) as (Element of S);
A50: ( 0 ) <= ( M . G2k ) by SUPINF_2:39;
( y2 . k ) = ( ( b . k ) * ( ( M * G2 ) . k ) ) by A42,A44;
hence thesis by A47,A49,A50;
end;
A51: (for x being set holds (x in ( dom y2 ) implies (not ( y2 . x ) in { ( -infty ) })))
proof
let x being set;
assume A52: x in ( dom y2 );
then reconsider x as (Element of ( NAT ));
( 0 ) <= ( y2 . x ) by A43,A52;
hence thesis by TARSKI:def 1;
end;
(for x being set holds (not x in ( y2 " { ( -infty ) } )))
proof
let x being set;
(not (x in ( dom y2 ) & ( y2 . x ) in { ( -infty ) })) by A51;
hence thesis by FUNCT_1:def 7;
end;
then A53: ( y2 " { ( -infty ) } ) = ( {} ) by XBOOLE_0:def 1;
( dom y2 ) = ( Seg ( len G2 ) ) by A42,FINSEQ_1:def 3
.= ( dom G2 ) by FINSEQ_1:def 3;
then ( integral (X,S,M,g2) ) = ( Sum y2 ) by A1,A18,A40,A41,A42,CKB44:1,MESFUNC4:3;
then A54: ( integral' (M,g2) ) = ( Sum y2 ) by A40,CKB90:def 1;
g1 is  nonnegative by A2,CKB23:1;
then A55: (for x being set holds (x in ( dom g1 ) implies ( 0 ) <= ( g1 . x ))) by SUPINF_2:51;
deffunc Fy1(Nat) = ( ( b . $1 ) * ( ( M * G1 ) . $1 ) );
consider y1 being (FinSequence of ( ExtREAL )) such that A56: (( len y1 ) = ( len G1 ) & (for j being Nat holds (j in ( dom y1 ) implies ( y1 . j ) = Fy1(j)))) from FINSEQ_2:sch 1;
A57: ( dom y ) = ( ( Seg ( len G ) ) /\ ( Seg ( len G ) ) ) by A25,FINSEQ_1:def 3
.= ( ( dom y1 ) /\ ( Seg ( len G2 ) ) ) by A8,A11,A56,FINSEQ_1:def 3
.= ( ( dom y1 ) /\ ( dom y2 ) ) by A42,FINSEQ_1:def 3;
A58: (for n being (Element of ( NAT )) holds (n in ( dom y ) implies ( y . n ) = ( ( y1 . n ) + ( y2 . n ) )))
proof
let n being (Element of ( NAT ));
assume A59: n in ( dom y );
then n in ( Seg ( len G ) ) by A24,FINSEQ_1:def 3;
then A60: n in ( dom G ) by FINSEQ_1:def 3;
then A61: ( G2 . n ) = ( ( G . n ) /\ B ) by A13;
now
let v being set;
assume A62: v in ( G . n );
( G . n ) in ( rng G ) by A60,FUNCT_1:3;
then v in ( union ( rng G ) ) by A62,TARSKI:def 4;
then v in ( dom g ) by A5,MESFUNC3:def 1;
then v in ( ( dom f ) /\ ( A \/ B ) ) by RELAT_1:61;
hence v in ( A \/ B ) by XBOOLE_0:def 4;
end;
then ( G . n ) c= ( A \/ B ) by TARSKI:def 3;
then A63: ( G . n ) = ( ( G . n ) /\ ( A \/ B ) ) by XBOOLE_1:28
.= ( ( ( G . n ) /\ A ) \/ ( ( G . n ) /\ B ) ) by XBOOLE_1:23
.= ( ( G1 . n ) \/ ( G2 . n ) ) by A10,A60,A61;
A64: n in ( dom y2 ) by A57,A59,XBOOLE_0:def 4;
then n in ( Seg ( len G2 ) ) by A42,FINSEQ_1:def 3;
then A65: n in ( dom G2 ) by FINSEQ_1:def 3;
then ( G2 . n ) in ( rng G2 ) by FUNCT_1:3;
then reconsider G2n = ( G2 . n ) as (Element of S);
( 0 ) <= ( M . G2n ) by MEASURE1:def 2;
then A66: (( 0 ) = ( ( M * G2 ) . n ) or ( 0 ) < ( ( M * G2 ) . n )) by A65,FUNCT_1:13;
A67:now
assume ( G1 . n ) meets ( G2 . n );
then consider v being set such that A68: v in ( G1 . n ) and A69: v in ( G2 . n ) by XBOOLE_0:3;
v in ( ( G . n ) /\ B ) by A13,A60,A69;
then A70: v in B by XBOOLE_0:def 4;
v in ( ( G . n ) /\ A ) by A10,A60,A68;
then v in A by XBOOLE_0:def 4;
hence contradiction by A3,A70,XBOOLE_0:3;
end;
A71: n in ( dom y1 ) by A57,A59,XBOOLE_0:def 4;
then n in ( Seg ( len G1 ) ) by A56,FINSEQ_1:def 3;
then A72: n in ( dom G1 ) by FINSEQ_1:def 3;
then ( G1 . n ) in ( rng G1 ) by FUNCT_1:3;
then reconsider G1n = ( G1 . n ) as (Element of S);
( 0 ) <= ( M . G1n ) by MEASURE1:def 2;
then A73: (( 0 ) = ( ( M * G1 ) . n ) or ( 0 ) < ( ( M * G1 ) . n )) by A72,FUNCT_1:13;
( ( M * G ) . n ) = ( M . ( G . n ) ) by A60,FUNCT_1:13
.= ( ( M . G1n ) + ( M . G2n ) ) by A63,A67,MEASURE1:30
.= ( ( ( M * G1 ) . n ) + ( M . ( G2 . n ) ) ) by A72,FUNCT_1:13
.= ( ( ( M * G1 ) . n ) + ( ( M * G2 ) . n ) ) by A65,FUNCT_1:13;
then ( ( b . n ) * ( ( M * G ) . n ) ) = ( ( ( b . n ) * ( ( M * G1 ) . n ) ) + ( ( b . n ) * ( ( M * G2 ) . n ) ) ) by A73,A66,XXREAL_3:96;
then ( y . n ) = ( ( ( b . n ) * ( ( M * G1 ) . n ) ) + ( ( b . n ) * ( ( M * G2 ) . n ) ) ) by A24,A59;
then ( y . n ) = ( ( y1 . n ) + ( ( b . n ) * ( ( M * G2 ) . n ) ) ) by A56,A71;
hence thesis by A42,A64;
end;
A74: (for k being Nat holds (k in ( dom y1 ) implies ( 0 ) <= ( y1 . k )))
proof
let k being Nat;
assume A75: k in ( dom y1 );
