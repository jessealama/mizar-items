begin
theorem
Th56: (for seq being ExtREAL_sequence holds (seq is  convergent_to_+infty implies ((not seq is  convergent_to_-infty) & (not seq is  convergent_to_finite_number))))
proof
let seq being ExtREAL_sequence;
assume A1: seq is  convergent_to_+infty;
hereby
assume seq is  convergent_to_-infty;
then consider n1 being Nat such that A2: (for m being Nat holds (n1 <= m implies ( seq . m ) <= ( R_EAL ( - 1 ) ))) by CKB69:def 1;
consider n2 being Nat such that A3: (for m being Nat holds (n2 <= m implies ( R_EAL 1 ) <= ( seq . m ))) by A1,CKB68:def 1;
reconsider n1,n2 as (Element of ( NAT )) by ORDINAL1:def 12;
set m = ( max (n1,n2) );
( seq . m ) <= ( R_EAL ( - 1 ) ) by A2,XXREAL_0:25;
hence contradiction by A3,XXREAL_0:25;
end;
assume seq is  convergent_to_finite_number;
then consider g being  real number such that A4: (for p being  real number holds (( 0 ) < p implies (ex n being Nat st (for m being Nat holds (n <= m implies |. ( ( seq . m ) - ( R_EAL g ) ) .| < p))))) by CKB67:def 1;
per cases ;
suppose A5: g > ( 0 );

then consider n1 being Nat such that A6: (for m being Nat holds (n1 <= m implies |. ( ( seq . m ) - ( R_EAL g ) ) .| < ( R_EAL g ))) by A4;
A7:now
let m being Nat;
assume n1 <= m;
then |. ( ( seq . m ) - ( R_EAL g ) ) .| < ( R_EAL g ) by A6;
then ( ( seq . m ) - ( R_EAL g ) ) < ( R_EAL g ) by EXTREAL2:10;
then ( seq . m ) < ( R_EAL ( g + g ) ) by XXREAL_3:54;
hence ( seq . m ) < ( R_EAL ( 2 * g ) );
end;
consider n2 being Nat such that A8: (for m being Nat holds (n2 <= m implies ( R_EAL ( 2 * g ) ) <= ( seq . m ))) by A1,A5,CKB68:def 1;
reconsider n1,n2 as (Element of ( NAT )) by ORDINAL1:def 12;
set m = ( max (n1,n2) );
( seq . m ) < ( R_EAL ( 2 * g ) ) by A7,XXREAL_0:25;
hence contradiction by A8,XXREAL_0:25;
end;
suppose A9: g = ( 0 );

consider n1 being Nat such that A10: (for m being Nat holds (n1 <= m implies |. ( ( seq . m ) - ( R_EAL g ) ) .| < ( R_EAL 1 ))) by A4;
consider n2 being Nat such that A11: (for m being Nat holds (n2 <= m implies ( R_EAL 1 ) <= ( seq . m ))) by A1,CKB68:def 1;
reconsider n1,n2 as (Element of ( NAT )) by ORDINAL1:def 12;
set m = ( max (n1,n2) );
|. ( ( seq . m ) - ( R_EAL g ) ) .| < ( R_EAL 1 ) by A10,XXREAL_0:25;
then ( ( seq . m ) - ( R_EAL g ) ) < ( R_EAL 1 ) by EXTREAL2:10;
then ( seq . m ) < ( ( R_EAL 1 ) + ( R_EAL g ) ) by XXREAL_3:54;
then ( seq . m ) < ( R_EAL 1 ) by A9,XXREAL_3:4;
hence contradiction by A11,XXREAL_0:25;
end;
suppose A12: g < ( 0 );

set g1 = ( - g );
consider n1 being Nat such that A13: (for m being Nat holds (n1 <= m implies |. ( ( seq . m ) - ( R_EAL g ) ) .| < ( R_EAL g1 ))) by A4,A12;
A14:now
let m being (Element of ( NAT ));
assume n1 <= m;
then |. ( ( seq . m ) - ( R_EAL g ) ) .| < ( R_EAL g1 ) by A13;
then ( ( seq . m ) - ( R_EAL g ) ) < ( R_EAL g1 ) by EXTREAL2:10;
then ( seq . m ) < ( g1 + g ) by XXREAL_3:54;
hence ( seq . m ) < ( 0 );
end;
consider n2 being Nat such that A15: (for m being Nat holds (n2 <= m implies 1 <= ( seq . m ))) by A1,CKB68:def 1;
reconsider n1,n2 as (Element of ( NAT )) by ORDINAL1:def 12;
set m = ( max (n1,n2) );
( seq . m ) < ( 0 ) by A14,XXREAL_0:25;
hence contradiction by A15,XXREAL_0:25;
end;
end;
