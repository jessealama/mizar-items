begin
theorem
Th106: (for X being non  empty set holds (for S being (SigmaField of X) holds (for M being (sigma_Measure of S) holds (for f being (PartFunc of X,( ExtREAL )) holds ((ex A being (Element of S) st (A = ( dom f ) & f is_measurable_on A)) implies (f is_integrable_on M iff |. f .| is_integrable_on M))))))
proof
let X being non  empty set;
let S being (SigmaField of X);
let M being (sigma_Measure of S);
let f being (PartFunc of X,( ExtREAL ));
A1: ( dom |. f .| ) = ( dom ( max- |. f .| ) ) by MESFUNC2:def 3;
A2: ( dom f ) = ( dom ( max- f ) ) by MESFUNC2:def 3;
A3:now
let x being set;
assume x in ( dom |. f .| );
then ( |. f .| . x ) = |. ( f . x ) .| by MESFUNC1:def 10;
hence ( 0 ) <= ( |. f .| . x ) by EXTREAL2:3;
end;
A4: ( dom f ) = ( dom ( max+ f ) ) by MESFUNC2:def 2;
A5: |. f .| = ( ( max+ f ) + ( max- f ) ) by MESFUNC2:24;
A6: ( max+ f ) is  nonnegative by CKB31:1;
assume A7: (ex A being (Element of S) st (A = ( dom f ) & f is_measurable_on A));
then consider A being (Element of S) such that A8: A = ( dom f ) and A9: f is_measurable_on A;
A10: ( max- f ) is_measurable_on A by A8,A9,MESFUNC2:26;
A11: |. f .| is_measurable_on A by A8,A9,MESFUNC2:27;
A12: A = ( dom |. f .| ) by A8,MESFUNC1:def 10;
A13: ( max+ f ) is_measurable_on A by A9,MESFUNC2:25;
A14: ( dom |. f .| ) = ( dom ( max+ |. f .| ) ) by MESFUNC2:def 2;
hereby
A15:now
let x being set;
assume A16: x in ( dom |. f .| );
then ( |. f .| . x ) = |. ( f . x ) .| by MESFUNC1:def 10;
then A17: ( 0 ) <= ( |. f .| . x ) by EXTREAL2:3;
( ( max+ |. f .| ) . x ) = ( max (( |. f .| . x ),( 0 )) ) by A14,A16,MESFUNC2:def 2;
hence ( ( max+ |. f .| ) . x ) = ( |. f .| . x ) by A17,XXREAL_0:def 10;
end;
now
let x being (Element of X);
assume x in ( dom ( max- |. f .| ) );
then ( ( max+ |. f .| ) . x ) = ( |. f .| . x ) by A1,A15;
hence ( ( max- |. f .| ) . x ) = ( 0 ) by MESFUNC2:19;
end;
then A18: ( integral+ (M,( max- |. f .| )) ) = ( 0 ) by A1,A12,A11,CKB116:1,MESFUNC2:26;
( max- f ) is  nonnegative by CKB31:1;
then A19: ( integral+ (M,( ( max+ f ) + ( max- f ) )) ) = ( ( integral+ (M,( max+ f )) ) + ( integral+ (M,( max- f )) ) ) by A8,A4,A2,A13,A10,A6,CKB106:1;
assume A20: f is_integrable_on M;
then A21: ( integral+ (M,( max+ f )) ) < ( +infty ) by CKB126:def 1;
A22: ( integral+ (M,( max- f )) ) < ( +infty ) by A20,CKB126:def 1;
|. f .| = ( max+ |. f .| ) by A14,A15,FUNCT_1:2;
then ( integral+ (M,( max+ |. f .| )) ) < ( +infty ) by A5,A21,A22,A19,XXREAL_0:4,XXREAL_3:16;
hence |. f .| is_integrable_on M by A12,A11,A18,CKB126:def 1;
end;
assume |. f .| is_integrable_on M;
then ( Integral (M,|. f .|) ) < ( +infty ) by CKB127:1;
then A23: ( integral+ (M,( ( max+ f ) + ( max- f ) )) ) < ( +infty ) by A12,A11,A5,A3,CKB118:1,SUPINF_2:52;
( max- f ) is  nonnegative by CKB31:1;
then A24: ( integral+ (M,( ( max+ f ) + ( max- f ) )) ) = ( ( integral+ (M,( max+ f )) ) + ( integral+ (M,( max- f )) ) ) by A8,A4,A2,A13,A10,A6,CKB106:1;
( -infty ) <> ( integral+ (M,( max- f )) ) by A8,A2,A10,CKB31:1,CKB108:1;
then ( integral+ (M,( max+ f )) ) <> ( +infty ) by A24,A23,XXREAL_3:def 2;
then A25: ( integral+ (M,( max+ f )) ) < ( +infty ) by XXREAL_0:4;
( -infty ) <> ( integral+ (M,( max+ f )) ) by A8,A4,A13,CKB31:1,CKB108:1;
then ( integral+ (M,( max- f )) ) <> ( +infty ) by A24,A23,XXREAL_3:def 2;
then ( integral+ (M,( max- f )) ) < ( +infty ) by XXREAL_0:4;
hence thesis by A7,A25,CKB126:def 1;
end;
