begin
theorem
Th83: (for X being non  empty set holds (for S being (SigmaField of X) holds (for M being (sigma_Measure of S) holds (for f being (PartFunc of X,( ExtREAL )) holds ((f is_simple_func_in S & f is  nonnegative) implies ( integral+ (M,f) ) = ( integral' (M,f) ))))))
proof
let X being non  empty set;
let S being (SigmaField of X);
let M being (sigma_Measure of S);
let f being (PartFunc of X,( ExtREAL ));
assume that
A1: f is_simple_func_in S
and
A2: f is  nonnegative;
deffunc PF(Nat) = f;
consider F being (Functional_Sequence of X,( ExtREAL )) such that A3: (for n being Nat holds ( F . n ) = PF(n)) from SEQFUNC:sch 1;
A4: (for n,m being Nat holds (n <= m implies (for x being (Element of X) holds (x in ( dom f ) implies ( ( F . n ) . x ) <= ( ( F . m ) . x )))))
proof
let n being Nat;
let m being Nat;
assume n <= m;
let x being (Element of X);
assume x in ( dom f );
( ( F . n ) . x ) = ( f . x ) by A3;
hence thesis by A3;
end;
deffunc PK(Nat) = ( integral' (M,( F . $1 )) );
consider K being (Function of ( NAT ),( ExtREAL )) such that A5: (for n being (Element of ( NAT )) holds ( K . n ) = PK(n)) from FUNCT_2:sch 4;
A6:now
let n being Nat;
n in ( NAT ) by ORDINAL1:def 12;
hence ( K . n ) = PK(n) by A5;
end;
A7: (for n being Nat holds ( K . n ) = ( integral' (M,f) ))
proof
let n being Nat;
thus ( K . n ) = ( integral' (M,( F . n )) ) by A6
.= ( integral' (M,f) ) by A3;
end;
then A8: ( lim K ) = ( integral' (M,f) ) by CKB82:1;
(ex GF being (Finite_Sep_Sequence of S) st (( dom f ) = ( union ( rng GF ) ) & (for n being Nat holds (for x,y being (Element of X) holds (((n in ( dom GF ) & x in ( GF . n )) & y in ( GF . n )) implies ( f . x ) = ( f . y )))))) by A1,MESFUNC2:def 4;
then reconsider A = ( dom f ) as (Element of S) by MESFUNC2:31;
A9: f is_measurable_on A by A1,MESFUNC2:34;
A10: (for x being (Element of X) holds (x in ( dom f ) implies (( F # x ) is  convergent & ( lim ( F # x ) ) = ( f . x ))))
proof
let x being (Element of X);
assume x in ( dom f );
now
let n being Nat;
thus ( ( F # x ) . n ) = ( ( F . n ) . x ) by CKB87:def 1
.= ( f . x ) by A3;
end;
hence thesis by CKB82:1;
end;
A11: (for n being Nat holds ( F . n ) is  nonnegative) by A2,A3;
A12: (for n being Nat holds (( F . n ) is_simple_func_in S & ( dom ( F . n ) ) = ( dom f ))) by A1,A3;
K is  convergent by A7,CKB82:1;
hence thesis by A2,A9,A6,A12,A11,A4,A10,A8,CKB104:def 1;
end;
