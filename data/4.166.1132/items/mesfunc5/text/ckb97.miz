begin
theorem
Th76: (for X being non  empty set holds (for S being (SigmaField of X) holds (for M being (sigma_Measure of S) holds (for f,g being (PartFunc of X,( ExtREAL )) holds (((((f is_simple_func_in S & g is_simple_func_in S) & f is  nonnegative) & g is  nonnegative) & (for x being set holds (x in ( dom ( f - g ) ) implies ( g . x ) <= ( f . x )))) implies ( integral' (M,( g | ( dom ( f - g ) ) )) ) <= ( integral' (M,( f | ( dom ( f - g ) ) )) ))))))
proof
let X being non  empty set;
let S being (SigmaField of X);
let M being (sigma_Measure of S);
let f being (PartFunc of X,( ExtREAL ));
let g being (PartFunc of X,( ExtREAL ));
assume that
A1: f is_simple_func_in S
and
A2: g is_simple_func_in S
and
A3: f is  nonnegative
and
A4: g is  nonnegative
and
A5: (for x being set holds (x in ( dom ( f - g ) ) implies ( g . x ) <= ( f . x )));
( ( - 1 ) (#) g ) is_simple_func_in S by A2,CKB51:1;
then ( - g ) is_simple_func_in S by MESFUNC2:9;
then ( f + ( - g ) ) is_simple_func_in S by A1,CKB50:1;
then A6: ( f - g ) is_simple_func_in S by MESFUNC2:8;
A7: ( integral' (M,( f | ( dom ( f - g ) ) )) ) = ( ( integral' (M,( f - g )) ) + ( integral' (M,( g | ( dom ( f - g ) ) )) ) ) by A1,A2,A3,A4,A5,CKB96:1;
now
assume ( integral' (M,( f | ( dom ( f - g ) ) )) ) <> ( +infty );
( 0 ) <= ( integral' (M,( f - g )) ) by A1,A2,A5,A6,CKB52:1,CKB94:1;
hence thesis by A7,XXREAL_3:39;
end;
hence thesis by XXREAL_0:4;
end;
