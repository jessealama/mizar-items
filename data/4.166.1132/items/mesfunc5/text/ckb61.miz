theorem
Lm6: (for X being non  empty set holds (for S being (SigmaField of X) holds (for M being (sigma_Measure of S) holds (for A being (Element of S) holds (for f being (PartFunc of X,( ExtREAL )) holds (( f | A ) is_measurable_on A iff f is_measurable_on A))))))
proof
let X being non  empty set;
let S being (SigmaField of X);
let M being (sigma_Measure of S);
let A being (Element of S);
let f being (PartFunc of X,( ExtREAL ));
now
assume A1: ( f | A ) is_measurable_on A;
now
let r being  real number;
( A /\ ( less_dom (( f | A ),( R_EAL r )) ) ) in S by A1,MESFUNC1:def 16;
then ( A /\ ( A /\ ( less_dom (f,( R_EAL r )) ) ) ) in S by CKB60:1;
then ( ( A /\ A ) /\ ( less_dom (f,( R_EAL r )) ) ) in S by XBOOLE_1:16;
hence ( A /\ ( less_dom (f,( R_EAL r )) ) ) in S;
end;
hence f is_measurable_on A by MESFUNC1:def 16;
end;
hence (( f | A ) is_measurable_on A implies f is_measurable_on A);
assume A2: f is_measurable_on A;
now
let r being  real number;
( ( A /\ A ) /\ ( less_dom (f,( R_EAL r )) ) ) in S by A2,MESFUNC1:def 16;
then ( A /\ ( A /\ ( less_dom (f,( R_EAL r )) ) ) ) in S by XBOOLE_1:16;
hence ( A /\ ( less_dom (( f | A ),( R_EAL r )) ) ) in S by CKB60:1;
end;
hence thesis by MESFUNC1:def 16;
end;
