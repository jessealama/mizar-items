begin
theorem
Th75: (for X being non  empty set holds (for S being (SigmaField of X) holds (for M being (sigma_Measure of S) holds (for f,g being (PartFunc of X,( ExtREAL )) holds (((((f is_simple_func_in S & f is  nonnegative) & g is_simple_func_in S) & g is  nonnegative) & (for x being set holds (x in ( dom ( f - g ) ) implies ( g . x ) <= ( f . x )))) implies (( dom ( f - g ) ) = ( ( dom f ) /\ ( dom g ) ) & ( integral' (M,( f | ( dom ( f - g ) ) )) ) = ( ( integral' (M,( f - g )) ) + ( integral' (M,( g | ( dom ( f - g ) ) )) ) )))))))
proof
let X being non  empty set;
let S being (SigmaField of X);
let M being (sigma_Measure of S);
let f being (PartFunc of X,( ExtREAL ));
let g being (PartFunc of X,( ExtREAL ));
assume that
A1: f is_simple_func_in S
and
A2: f is  nonnegative
and
A3: g is_simple_func_in S
and
A4: g is  nonnegative
and
A5: (for x being set holds (x in ( dom ( f - g ) ) implies ( g . x ) <= ( f . x )));
A6: ( f | ( dom ( f - g ) ) ) is  nonnegative by A2,CKB23:1;
( ( - 1 ) (#) g ) is_simple_func_in S by A3,CKB51:1;
then ( - g ) is_simple_func_in S by MESFUNC2:9;
then ( f + ( - g ) ) is_simple_func_in S by A1,CKB50:1;
then ( f - g ) is_simple_func_in S by MESFUNC2:8;
then A7: ( dom ( f - g ) ) is (Element of S) by CKB47:1;
then A8: ( g | ( dom ( f - g ) ) ) is_simple_func_in S by A3,CKB44:1;
A9: ( g | ( dom ( f - g ) ) ) is  nonnegative by A4,CKB23:1;
g is  without-infty by A3,CKB22:1;
then (not ( -infty ) in ( rng g )) by CKB12:def 1;
then A10: ( g " { ( -infty ) } ) = ( {} ) by FUNCT_1:72;
f is  without+infty by A1,CKB22:1;
then (not ( +infty ) in ( rng f )) by CKB13:def 1;
then A11: ( f " { ( +infty ) } ) = ( {} ) by FUNCT_1:72;
then A12: ( ( ( dom f ) /\ ( dom g ) ) \ ( ( ( f " { ( +infty ) } ) /\ ( g " { ( +infty ) } ) ) \/ ( ( f " { ( -infty ) } ) /\ ( g " { ( -infty ) } ) ) ) ) = ( ( dom f ) /\ ( dom g ) ) by A10;
hence A13: ( dom ( f - g ) ) = ( ( dom f ) /\ ( dom g ) ) by MESFUNC1:def 4;
( dom ( f | ( dom ( f - g ) ) ) ) = ( ( dom f ) /\ ( dom ( f - g ) ) ) by RELAT_1:61;
then A14: ( dom ( f | ( dom ( f - g ) ) ) ) = ( ( ( dom f ) /\ ( dom f ) ) /\ ( dom g ) ) by A13,XBOOLE_1:16;
A15: (for x being set holds (x in ( dom ( f | ( dom ( f - g ) ) ) ) implies ( ( g | ( dom ( f - g ) ) ) . x ) <= ( ( f | ( dom ( f - g ) ) ) . x )))
proof
let x being set;
assume A16: x in ( dom ( f | ( dom ( f - g ) ) ) );
then ( g . x ) <= ( f . x ) by A5,A13,A14;
then ( ( g | ( dom ( f - g ) ) ) . x ) <= ( f . x ) by A13,A14,A16,FUNCT_1:49;
hence thesis by A13,A14,A16,FUNCT_1:49;
end;
( dom ( g | ( dom ( f - g ) ) ) ) = ( ( dom g ) /\ ( dom ( f - g ) ) ) by RELAT_1:61;
then A17: ( dom ( g | ( dom ( f - g ) ) ) ) = ( ( ( dom g ) /\ ( dom g ) ) /\ ( dom f ) ) by A13,XBOOLE_1:16;
A18: ( f | ( dom ( f - g ) ) ) is_simple_func_in S by A1,A7,CKB44:1;
thus ( integral' (M,( f | ( dom ( f - g ) ) )) ) = ( ( integral' (M,( f - g )) ) + ( integral' (M,( g | ( dom ( f - g ) ) )) ) )
proof
per cases ;
suppose A19: ( dom ( f - g ) ) = ( {} );

( dom ( g | ( dom ( f - g ) ) ) ) = ( ( dom g ) /\ ( dom ( f - g ) ) ) by RELAT_1:61;
then A20: ( integral' (M,( g | ( dom ( f - g ) ) )) ) = ( 0 ) by A19,CKB90:def 1;
( dom ( f | ( dom ( f - g ) ) ) ) = ( ( dom f ) /\ ( dom ( f - g ) ) ) by RELAT_1:61;
then A21: ( integral' (M,( f | ( dom ( f - g ) ) )) ) = ( 0 ) by A19,CKB90:def 1;
( integral' (M,( f - g )) ) = ( 0 ) by A19,CKB90:def 1;
hence thesis by A21,A20;
end;
suppose A22: ( dom ( f - g ) ) <> ( {} );

A23: ( ( g | ( dom ( f - g ) ) ) " { ( -infty ) } ) = ( ( dom ( f - g ) ) /\ ( g " { ( -infty ) } ) ) by FUNCT_1:70;
( ( f | ( dom ( f - g ) ) ) " { ( +infty ) } ) = ( ( dom ( f - g ) ) /\ ( f " { ( +infty ) } ) ) by FUNCT_1:70;
then ( ( ( dom ( f | ( dom ( f - g ) ) ) ) /\ ( dom ( g | ( dom ( f - g ) ) ) ) ) \ ( ( ( ( f | ( dom ( f - g ) ) ) " { ( +infty ) } ) /\ ( ( g | ( dom ( f - g ) ) ) " { ( +infty ) } ) ) \/ ( ( ( f | ( dom ( f - g ) ) ) " { ( -infty ) } ) /\ ( ( g | ( dom ( f - g ) ) ) " { ( -infty ) } ) ) ) ) = ( dom ( f - g ) ) by A11,A10,A12,A14,A17,A23,MESFUNC1:def 4;
then A24: ( dom ( ( f | ( dom ( f - g ) ) ) - ( g | ( dom ( f - g ) ) ) ) ) = ( dom ( f - g ) ) by MESFUNC1:def 4;
A25: (for x being (Element of X) holds (x in ( dom ( ( f | ( dom ( f - g ) ) ) - ( g | ( dom ( f - g ) ) ) ) ) implies ( ( ( f | ( dom ( f - g ) ) ) - ( g | ( dom ( f - g ) ) ) ) . x ) = ( ( f - g ) . x )))
proof
let x being (Element of X);
assume A26: x in ( dom ( ( f | ( dom ( f - g ) ) ) - ( g | ( dom ( f - g ) ) ) ) );
then ( ( ( f | ( dom ( f - g ) ) ) - ( g | ( dom ( f - g ) ) ) ) . x ) = ( ( ( f | ( dom ( f - g ) ) ) . x ) - ( ( g | ( dom ( f - g ) ) ) . x ) ) by MESFUNC1:def 4
.= ( ( f . x ) - ( ( g | ( dom ( f - g ) ) ) . x ) ) by A24,A26,FUNCT_1:49
.= ( ( f . x ) - ( g . x ) ) by A24,A26,FUNCT_1:49;
hence thesis by A24,A26,MESFUNC1:def 4;
end;
( integral (X,S,M,( f | ( dom ( f - g ) ) )) ) = ( ( integral (X,S,M,( ( f | ( dom ( f - g ) ) ) - ( g | ( dom ( f - g ) ) ) )) ) + ( integral (X,S,M,( g | ( dom ( f - g ) ) )) ) ) by A13,A18,A8,A6,A9,A14,A17,A15,A22,CKB95:1;
then A27: ( integral (X,S,M,( f | ( dom ( f - g ) ) )) ) = ( ( integral (X,S,M,( f - g )) ) + ( integral (X,S,M,( g | ( dom ( f - g ) ) )) ) ) by A24,A25,PARTFUN1:5;
A28: ( integral (X,S,M,( g | ( dom ( f - g ) ) )) ) = ( integral' (M,( g | ( dom ( f - g ) ) )) ) by A13,A17,A22,CKB90:def 1;
( integral (X,S,M,( f | ( dom ( f - g ) ) )) ) = ( integral' (M,( f | ( dom ( f - g ) ) )) ) by A13,A14,A22,CKB90:def 1;
hence thesis by A22,A27,A28,CKB90:def 1;
end;
