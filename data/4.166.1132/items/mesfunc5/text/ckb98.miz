begin
theorem
Th77: (for X being non  empty set holds (for S being (SigmaField of X) holds (for M being (sigma_Measure of S) holds (for f being (PartFunc of X,( ExtREAL )) holds (for c being R_eal holds (((( 0 ) <= c & f is_simple_func_in S) & (for x being set holds (x in ( dom f ) implies ( f . x ) = c))) implies ( integral' (M,f) ) = ( c * ( M . ( dom f ) ) )))))))
proof
let X being non  empty set;
let S being (SigmaField of X);
let M being (sigma_Measure of S);
let f being (PartFunc of X,( ExtREAL ));
let c being R_eal;
assume that
A1: ( 0 ) <= c
and
A2: f is_simple_func_in S
and
A3: (for x being set holds (x in ( dom f ) implies ( f . x ) = c));
A4: (for x being set holds (x in ( dom f ) implies ( 0 ) <= ( f . x ))) by A1,A3;
reconsider A = ( dom f ) as (Element of S) by A2,CKB47:1;
per cases ;
suppose A5: ( dom f ) = ( {} );

then A6: ( M . A ) = ( 0 ) by VALUED_0:def 19;
( integral' (M,f) ) = ( 0 ) by A5,CKB90:def 1;
hence thesis by A6;
end;
suppose A7: ( dom f ) <> ( {} );

set x = <* ( c * ( M . A ) ) *>;
reconsider a = <* c *> as (FinSequence of ( ExtREAL ));
set F = <* ( dom f ) *>;
reconsider x as (FinSequence of ( ExtREAL ));
A8: ( rng F ) = { A } by FINSEQ_1:38;
( rng F ) c= S
proof
let z being set;
assume z in ( rng F );
then z = A by A8,TARSKI:def 1;
hence thesis;
end;
then reconsider F as (FinSequence of S) by FINSEQ_1:def 4;
(for i,j being Nat holds (((i in ( dom F ) & j in ( dom F )) & i <> j) implies ( F . i ) misses ( F . j )))
proof
let i being Nat;
let j being Nat;
assume that
A9: i in ( dom F )
and
A10: j in ( dom F )
and
A11: i <> j;
A12: ( dom F ) = { 1 } by FINSEQ_1:2,FINSEQ_1:38;
then i = 1 by A9,TARSKI:def 1;
hence thesis by A10,A11,A12,TARSKI:def 1;
end;
then reconsider F as (Finite_Sep_Sequence of S) by MESFUNC3:4;
A13: ( dom F ) = ( Seg 1 ) by FINSEQ_1:38
.= ( dom a ) by FINSEQ_1:38;
A14: (for n being Nat holds (n in ( dom F ) implies (for x being set holds (x in ( F . n ) implies ( f . x ) = ( a . n )))))
proof
let n being Nat;
assume n in ( dom F );
then n in { 1 } by FINSEQ_1:2,FINSEQ_1:38;
then A15: n = 1 by TARSKI:def 1;
let x being set;
assume x in ( F . n );
then x in ( dom f ) by A15,FINSEQ_1:40;
then ( f . x ) = c by A3;
hence thesis by A15,FINSEQ_1:40;
end;
A16: (for n being Nat holds (n in ( dom x ) implies ( x . n ) = ( c * ( M . A ) )))
proof
let n being Nat;
assume n in ( dom x );
then n in { 1 } by FINSEQ_1:2,FINSEQ_1:38;
then n = 1 by TARSKI:def 1;
hence thesis by FINSEQ_1:40;
end;
A17: ( dom x ) = ( Seg 1 ) by FINSEQ_1:38
.= ( dom F ) by FINSEQ_1:38;
A18: (for n being Nat holds (n in ( dom x ) implies ( x . n ) = ( ( a . n ) * ( ( M * F ) . n ) )))
proof
let n being Nat;
assume A19: n in ( dom x );
then n in { 1 } by FINSEQ_1:2,FINSEQ_1:38;
then A20: n = 1 by TARSKI:def 1;
then A21: ( x . n ) = ( c * ( M . A ) ) by FINSEQ_1:40;
( ( M * F ) . n ) = ( M . ( F . n ) ) by A17,A19,FUNCT_1:13
.= ( M . A ) by A20,FINSEQ_1:40;
hence thesis by A20,A21,FINSEQ_1:40;
end;
( dom f ) = ( union ( rng F ) ) by A8,ZFMISC_1:25;
then F,a are_Re-presentation_of f by A13,A14,MESFUNC3:def 1;
then ( integral (X,S,M,f) ) = ( Sum x ) by A2,A4,A7,A17,A18,MESFUNC4:3;
then A22: ( integral' (M,f) ) = ( Sum x ) by A7,CKB90:def 1;
1 = ( len x ) by FINSEQ_1:40;
then ( Sum x ) = ( ( R_EAL 1 ) * ( c * ( M . A ) ) ) by A16,MESFUNC3:18;
hence thesis by A22,XXREAL_3:81;
end;
end;
