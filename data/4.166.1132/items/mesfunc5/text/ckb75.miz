Th59: (for F being (FinSequence of ( ExtREAL )) holds ((for n being Nat holds (n in ( dom F ) implies ( 0 ) <= ( F . n ))) implies ( 0 ) <= ( Sum F )))
proof
let F being (FinSequence of ( ExtREAL ));
