begin
theorem
(for A being Interval holds (for x being Real holds (( 0 ) <= x implies (for y being Real holds (y = ( diameter A ) implies ( x * y ) = ( diameter ( x ** A ) ))))))
proof
let A being Interval;
let x being Real;
assume that
A1: ( 0 ) <= x;
let y being Real;
assume that
A2: y = ( diameter A );
per cases ;
suppose A3: A is  empty;

A4:now
assume ( x ** A ) is non empty;
then consider y being  real number such that A5: y in ( x ** A ) by MEMBERED:9;
reconsider y as Real by XREAL_0:def 1;
(ex z being Real st (z in A & y = ( x * z ))) by A5,INTEGRA2:39;
hence contradiction by A3;
end;
thus ( x * y ) = ( 0 ) by A2,A3,MEASURE5:10
.= ( diameter ( x ** A ) ) by A4,MEASURE5:10;
end;
suppose A6: A is non empty;

then consider z being  real number such that A7: z in A by MEMBERED:9;
reconsider z as Real by XREAL_0:def 1;
A8: ( x * z ) in ( x ** A ) by A7,MEMBER_1:193;
reconsider AA = A as non empty (Subset of ( REAL )) by A6;
reconsider u = x as R_eal by XXREAL_0:def 1;
A9: ( inf ( x ** AA ) ) = ( u * ( inf AA ) ) by A1,CKB22:1;
reconsider z = x as R_eal by XXREAL_0:def 1;
thus ( x * y ) = ( z * ( diameter A ) ) by A2,EXTREAL1:1
.= ( z * ( ( sup A ) - ( inf A ) ) ) by A6,MEASURE5:def 6
.= ( ( z * ( sup A ) ) - ( z * ( inf A ) ) ) by XXREAL_3:100
.= ( ( sup ( x ** A ) ) - ( inf ( x ** A ) ) ) by A1,A9,CKB20:1
.= ( diameter ( x ** A ) ) by A8,MEASURE5:def 6;
end;
end;
