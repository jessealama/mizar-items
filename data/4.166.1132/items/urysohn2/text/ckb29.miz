begin
theorem
(for A being non empty (Subset of ( ExtREAL )) holds (for a,b being R_eal holds (A c= [. a,b .] implies (a <= ( inf A ) & ( sup A ) <= b))))
proof
let A being non empty (Subset of ( ExtREAL ));
let a being R_eal,b being R_eal;
assume A1: A c= [. a,b .];
then reconsider B = [. a,b .] as non empty (Subset of ( ExtREAL )) by MEMBERED:2;
(for x being  ext-real number holds (x in B implies x <= b)) by XXREAL_1:1;
then b is (UpperBound of B) by XXREAL_2:def 1;
then A2: b is (UpperBound of A) by A1,XXREAL_2:6;
(for x being  ext-real number holds (x in B implies a <= x)) by XXREAL_1:1;
then a is (LowerBound of B) by XXREAL_2:def 2;
then a is (LowerBound of A) by A1,XXREAL_2:5;
hence thesis by A2,XXREAL_2:def 3,XXREAL_2:def 4;
end;
