environ
vocabularies NUMBERS,REAL_1,SUBSET_1,RCOMP_1,SEQ_1,PARTFUN1,VALUED_0,SEQ_2,ORDINAL2,CARD_1,ARYTM_3,FUNCT_1,RELAT_1,FUNCOP_1,VALUED_1,FUNCT_2,NAT_1,TARSKI,ARYTM_1,XREAL_0,ORDINAL1,XXREAL_1,XBOOLE_0,XXREAL_0,COMPLEX1,FCONT_1,XXREAL_2,FDIFF_1;
notations TARSKI,XBOOLE_0,SUBSET_1,ORDINAL1,NUMBERS,XCMPLX_0,XREAL_0,COMPLEX1,REAL_1,NAT_1,FUNCT_2,FUNCOP_1,VALUED_0,VALUED_1,SEQ_1,SEQ_2,RELSET_1,PARTFUN1,PARTFUN2,RCOMP_1,FCONT_1,XXREAL_0,CKB2,CKB4,CKB6,CKB7,CKB9,CKB16,CKB17,CKB18;
definitions TARSKI,FCONT_1,XBOOLE_0,VALUED_1,RCOMP_1,SUBSET_1,CKB2,CKB4,CKB7,CKB16,CKB17,CKB18;
theorems TARSKI,NAT_1,FUNCT_1,ABSVALUE,ZFMISC_1,SEQ_1,SEQ_2,SEQM_3,SEQ_4,PARTFUN1,PARTFUN2,RFUNCT_1,RFUNCT_2,RCOMP_1,FCONT_1,FUNCT_2,RELAT_1,XREAL_0,XBOOLE_0,XBOOLE_1,XCMPLX_0,XCMPLX_1,FUNCOP_1,XREAL_1,COMPLEX1,VALUED_1,VALUED_0,ORDINAL1,CKB1,CKB2,CKB4,CKB7,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21;
schemes SEQ_1;
registrations FUNCT_1,ORDINAL1,RELSET_1,NUMBERS,XXREAL_0,XREAL_0,NAT_1,MEMBERED,RCOMP_1,VALUED_0,VALUED_1,FUNCT_2,FUNCOP_1,SEQ_4,REALSET1,SETFAM_1,RELAT_1,ZFMISC_1,CARD_1,FINSET_1,CKB3,CKB5,CKB8;
constructors PARTFUN1,REAL_1,NAT_1,COMPLEX1,SEQ_2,SEQM_3,RCOMP_1,PARTFUN2,RFUNCT_2,FCONT_1,VALUED_1,SEQ_1,REALSET1,FINSET_1,SETFAM_1,RELSET_1,BINOP_2,RVSUM_1,CKB2,CKB4,CKB7,CKB16,CKB17,CKB18;
requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve y for set;
reserve X for set;
reserve x for Real;
reserve f for (PartFunc of ( REAL ),( REAL ));
definition
let f;
let X;
assume A1: f is_differentiable_on X;
func f `| X -> (PartFunc of ( REAL ),( REAL )) means 
:Def8: (( dom it ) = X & (for x holds (x in X implies ( it . x ) = ( diff (f,x) ))));
existence
proof
deffunc F(Real) = ( diff (f,$1) );
defpred P[ set ]
 means
$1 in X;
consider F being (PartFunc of ( REAL ),( REAL )) such that A2: ((for x holds (x in ( dom F ) iff P[ x ])) & (for x holds (x in ( dom F ) implies ( F . x ) = F(x)))) from SEQ_1:sch 3;
take F;
now
A3: X is (Subset of ( REAL )) by A1,CKB19:1;
let y;
assume y in X;
hence y in ( dom F ) by A2,A3;
end;
then A4: X c= ( dom F ) by TARSKI:def 3;
(for y holds (y in ( dom F ) implies y in X)) by A2;
then ( dom F ) c= X by TARSKI:def 3;
hence ( dom F ) = X by A4,XBOOLE_0:def 10;
now
let x;
assume x in X;
then x in ( dom F ) by A2;
hence ( F . x ) = ( diff (f,x) ) by A2;
end;
hence thesis;
end;
uniqueness
proof
let F being (PartFunc of ( REAL ),( REAL ));
let G being (PartFunc of ( REAL ),( REAL ));
assume that
A5: ( dom F ) = X
and
A6: (for x holds (x in X implies ( F . x ) = ( diff (f,x) )))
and
A7: ( dom G ) = X
and
A8: (for x holds (x in X implies ( G . x ) = ( diff (f,x) )));
now
let x;
assume A9: x in ( dom F );
then ( F . x ) = ( diff (f,x) ) by A5,A6;
hence ( F . x ) = ( G . x ) by A5,A8,A9;
end;
hence thesis by A5,A7,PARTFUN1:5;
end;
end;
