theorem
Th19: ((for n being Nat holds ( F . n ) is_integrable_on M) implies (for m being Nat holds ( ( Partial_Sums F ) . m ) is_integrable_on M))
proof
assume A1: (for n being Nat holds ( F . n ) is_integrable_on M);
let m being Nat;
(for n being Nat holds ( ( R_EAL F ) . n ) is_integrable_on M)
proof
let n being Nat;
( F . n ) is_integrable_on M by A1;
then ( R_EAL ( F . n ) ) is_integrable_on M by MESFUNC6:def 4;
hence ( ( R_EAL F ) . n ) is_integrable_on M;
end;
then ( ( Partial_Sums ( R_EAL F ) ) . m ) is_integrable_on M by MESFUNC9:45;
then ( ( R_EAL ( Partial_Sums F ) ) . m ) is_integrable_on M by CKB9:1;
then ( R_EAL ( ( Partial_Sums F ) . m ) ) is_integrable_on M;
hence ( ( Partial_Sums F ) . m ) is_integrable_on M by MESFUNC6:def 4;
end;
