begin
registration
let X being  extremally_disconnected non empty TopSpace;
cluster  open ->  extremally_disconnected for non empty non empty (SubSpace of X);
coherence
proof
let X0 being non empty (SubSpace of X);
assume that
A1: X0 is  open;
reconsider B = (the carrier of X0) as (Subset of X) by TSEP_1:1;
now
let A0 being (Subset of X0);
A0 c= B;
then reconsider A = A0 as (Subset of X) by XBOOLE_1:1;
assume A0 is  open;
then A is  open by A1,TSEP_1:17;
then A2: ( Cl A ) is  open by CKB48:def 1;
( Cl A0 ) = ( ( Cl A ) /\ ( [#] X0 ) ) by PRE_TOPC:17;
hence ( Cl A0 ) is  open by A2,TOPS_2:24;
end;
hence thesis by CKB48:def 1;
