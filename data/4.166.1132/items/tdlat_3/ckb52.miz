notations TARSKI,XBOOLE_0,SUBSET_1,SETFAM_1,STRUCT_0,REALSET1,PRE_TOPC,TOPS_1,TOPS_2,BORSUK_1,TSEP_1,BINOP_1,LATTICES,LATTICE3,TDLAT_1,TDLAT_2,CKB10,CKB11,CKB20,CKB48;
