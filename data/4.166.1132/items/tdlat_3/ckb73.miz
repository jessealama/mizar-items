begin
reserve Y for  extremally_disconnected non empty TopSpace;
theorem
Th44: ( Domains_of Y ) = ( Open_Domains_of Y )
proof
now
let S being set;
assume A1: S in ( Domains_of Y );
then reconsider A = S as (Subset of Y);
A in { D where D is (Subset of Y): D is  condensed } by A1,TDLAT_1:def 1;
then (ex D being (Subset of Y) st (D = A & D is  condensed));
then A is  open_condensed by CKB56:1;
then A in { E where E is (Subset of Y): E is  open_condensed };
hence S in ( Open_Domains_of Y ) by TDLAT_1:def 9;
end;
then A2: ( Domains_of Y ) c= ( Open_Domains_of Y ) by TARSKI:def 3;
( Open_Domains_of Y ) c= ( Domains_of Y ) by TDLAT_1:35;
hence thesis by A2,XBOOLE_0:def 10;
end;
