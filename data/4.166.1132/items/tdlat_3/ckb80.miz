environ
vocabularies PRE_TOPC,SUBSET_1,TOPS_1,XBOOLE_0,STRUCT_0,RCOMP_1,TARSKI,NATTRA_1,ZFMISC_1,SETFAM_1,TDLAT_1,REALSET1,LATTICES,FUNCT_1,EQREL_1,TDLAT_2,PBOOLE,TDLAT_3;
notations TARSKI,XBOOLE_0,SUBSET_1,SETFAM_1,STRUCT_0,REALSET1,PRE_TOPC,TOPS_1,TOPS_2,BORSUK_1,TSEP_1,BINOP_1,LATTICES,LATTICE3,TDLAT_1,TDLAT_2,CKB10,CKB11,CKB20,CKB48,CKB60;
definitions TARSKI,BORSUK_1,TSEP_1,XBOOLE_0,REALSET1,SUBSET_1,STRUCT_0,CKB10,CKB11,CKB20,CKB48,CKB60;
theorems TARSKI,ZFMISC_1,PRE_TOPC,TOPS_1,TOPS_2,PCOMPS_1,BORSUK_1,TSEP_1,LATTICES,TDLAT_1,TDLAT_2,SUBSET_1,XBOOLE_0,XBOOLE_1,RELSET_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB15,CKB16,CKB19,CKB20,CKB25,CKB26,CKB27,CKB30,CKB31,CKB32,CKB35,CKB36,CKB37,CKB38,CKB40,CKB41,CKB48,CKB50,CKB51,CKB52,CKB53,CKB54,CKB55,CKB56,CKB57,CKB58,CKB59,CKB60,CKB64,CKB69,CKB70,CKB71,CKB72,CKB73,CKB74,CKB75,CKB76,CKB77,CKB78,CKB79;
registrations XBOOLE_0,SUBSET_1,RELSET_1,STRUCT_0,LATTICES,PRE_TOPC,TOPS_1,BORSUK_1,TSEP_1,CKB14,CKB17,CKB18,CKB21,CKB22,CKB23,CKB24,CKB28,CKB29,CKB33,CKB34,CKB39,CKB42,CKB43,CKB44,CKB45,CKB46,CKB47,CKB49,CKB61,CKB62,CKB63,CKB65,CKB66,CKB67,CKB68;
constructors SETFAM_1,REALSET1,TOPS_1,TOPS_2,BORSUK_1,LATTICE3,TDLAT_1,TDLAT_2,TSEP_1,BINOP_1,CKB10,CKB11,CKB20,CKB48,CKB60;
requirements BOOLE,SUBSET;
begin
reserve X for non empty TopSpace;
theorem
Th51: (X is  extremally_disconnected iff ( Domains_Lattice X ) is M_Lattice)
proof
thus (X is  extremally_disconnected implies ( Domains_Lattice X ) is M_Lattice)
proof
assume X is  extremally_disconnected;
then ( Domains_Lattice X ) = ( Open_Domains_Lattice X ) by CKB75:1;
hence thesis;
end;

assume A1: ( Domains_Lattice X ) is M_Lattice;
assume (not X is  extremally_disconnected);
then consider D being (Subset of X) such that A2: D is  condensed and A3: ( Int ( Cl D ) ) <> ( Cl ( Int D ) ) by CKB57:1;
set A = ( Int ( Cl D ) );
set C = ( Cl ( Int D ) );
set B = ( C ` );
A4: D c= C by A2,TOPS_1:def 6;
A c= D by A2,TOPS_1:def 6;
then A5: A c= C by A4,XBOOLE_1:1;
A6: A is  open_condensed by TDLAT_1:23;
then A7: A = ( Int ( Cl A ) ) by TOPS_1:def 8;
B misses C by XBOOLE_1:79;
then A8: ( B /\ C ) = ( {} X ) by XBOOLE_0:def 7;
A9: C is  closed_condensed by TDLAT_1:22;
then A10: C = ( Cl ( Int C ) ) by TOPS_1:def 7;
C is  condensed by A9,TOPS_1:66;
then A11: C in { E where E is (Subset of X): E is  condensed };
then A12: C in ( Domains_of X ) by TDLAT_1:def 1;
A13: ( Domains_Lattice X ) = LattStr (# ( Domains_of X ),( D-Union X ),( D-Meet X ) #) by TDLAT_1:def 4;
then reconsider c = C as (Element of ( Domains_Lattice X )) by A11,TDLAT_1:def 1;
A is  condensed by A6,TOPS_1:67;
then A14: A in { E where E is (Subset of X): E is  condensed };
then reconsider a = A as (Element of ( Domains_Lattice X )) by A13,TDLAT_1:def 1;
A15: B = ( Int ( ( Int D ) ` ) ) by CKB3:1
.= ( Int ( Cl ( D ` ) ) ) by CKB2:1;
( B ` ) is  closed_condensed by TDLAT_1:22;
then B is  open_condensed by TOPS_1:61;
then B is  condensed by TOPS_1:67;
then B in { E where E is (Subset of X): E is  condensed };
then reconsider b = B as (Element of ( Domains_Lattice X )) by A13,TDLAT_1:def 1;
A16: ( A \/ ( {} X ) ) = A;
( Cl ( A \/ B ) ) = ( ( Cl A ) \/ ( Cl B ) ) by PRE_TOPC:20
.= ( Cl ( Int ( ( Cl D ) \/ ( Cl ( D ` ) ) ) ) ) by A15,TDLAT_1:6
.= ( Cl ( Int ( Cl ( D \/ ( D ` ) ) ) ) ) by PRE_TOPC:20
.= ( Cl ( Int ( Cl ( [#] X ) ) ) ) by PRE_TOPC:2
.= ( Cl ( Int ( [#] X ) ) ) by TOPS_1:2
.= ( Cl ( [#] X ) ) by TOPS_1:15
.= ( [#] X ) by TOPS_1:2;
then ( a "\/" b ) = ( ( Int ( [#] X ) ) \/ ( A \/ B ) ) by A13,TDLAT_2:87
.= ( ( [#] X ) \/ ( A \/ B ) ) by TOPS_1:15
.= ( [#] X ) by XBOOLE_1:12;
then A17: ( ( a "\/" b ) "/\" c ) = ( ( Cl ( Int ( ( [#] X ) /\ C ) ) ) /\ ( ( [#] X ) /\ C ) ) by A13,TDLAT_2:87
.= ( ( Cl ( Int C ) ) /\ ( ( [#] X ) /\ C ) ) by XBOOLE_1:28
.= ( ( Cl ( Int C ) ) /\ C ) by XBOOLE_1:28
.= C by A10;
A in ( Domains_of X ) by A14,TDLAT_1:def 1;
then A18: a [= c by A12,A5,TDLAT_2:89;
( b "/\" c ) = ( ( Cl ( Int ( B /\ C ) ) ) /\ ( B /\ C ) ) by A13,TDLAT_2:87
.= ( {} X ) by A8;
then ( a "\/" ( b "/\" c ) ) = ( ( Int ( Cl A ) ) \/ A ) by A13,A16,TDLAT_2:87
.= A by A7;
hence contradiction by A1,A3,A18,A17,LATTICES:def 12;
end;
