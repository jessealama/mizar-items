requirements BOOLE,SUBSET;
begin
theorem
(for Y being TopStruct holds ((Y is  discrete & Y is  anti-discrete) implies ( bool (the carrier of Y) ) = { ( {} ),(the carrier of Y) }))
proof
let Y being TopStruct;
assume that
A1: Y is  discrete
and
A2: Y is  anti-discrete;
(the topology of Y) = ( bool (the carrier of Y) ) by A1,CKB10:def 1;
hence thesis by A2,CKB11:def 1;
end;
