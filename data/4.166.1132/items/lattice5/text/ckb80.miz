begin
reserve X for non  empty set;
reserve e for (Equivalence_Relation of X);
reserve e1 for (Equivalence_Relation of X);
reserve e2 for (Equivalence_Relation of X);
reserve e19 for (Equivalence_Relation of X);
reserve e29 for (Equivalence_Relation of X);
reserve x for set;
reserve y for set;
reserve L for  lower-bounded LATTICE;
Lm2:now
let j being (Element of ( NAT ));
assume that
A1: 1 <= j
and
A2: j < 5;
j < ( 4 + 1 ) by A2;
then j <= 4 by NAT_1:13;
then ((((j = ( 0 ) or j = 1) or j = 2) or j = 3) or j = 4) by NAT_1:28;
hence (((j = 1 or j = 2) or j = 3) or j = 4) by A1;
end;
Lm3:now
let m being (Element of ( NAT ));
assume A1: m in ( Seg 5 );
then m <= 5 by FINSEQ_1:1;
then (((((m = ( 0 ) or m = 1) or m = 2) or m = 3) or m = 4) or m = 5) by NAT_1:29;
hence ((((m = 1 or m = 2) or m = 3) or m = 4) or m = 5) by A1,FINSEQ_1:1;
end;
theorem
Th46: (for S being (ExtensionSeq of (the carrier of L),( BasicDF L )) holds (for FS being non  empty set holds (for FD being (distance_function of FS,L) holds (for f being (Homomorphism of L,( EqRelLATT FS )) holds (for x,y being (Element of FS) holds (for e1,e2 being (Equivalence_Relation of FS) holds (for x holds (for y holds ((((((f = ( alpha FD ) & FS = ( union { ( ( S . i ) `1 ) where i is (Element of ( NAT )): (not contradiction) } )) & FD = ( union { ( ( S . i ) `2 ) where i is (Element of ( NAT )): (not contradiction) } )) & e1 in (the carrier of ( Image f ))) & e2 in (the carrier of ( Image f ))) & [ x,y ] in ( e1 "\/" e2 )) implies (ex F being non  empty (FinSequence of FS) st (( len F ) = ( 3 + 2 ) & x,y are_joint_by F,e1,e2)))))))))))
proof
let S being (ExtensionSeq of (the carrier of L),( BasicDF L ));
let FS being non  empty set;
let FD being (distance_function of FS,L);
let f being (Homomorphism of L,( EqRelLATT FS ));
let x being (Element of FS);
let y being (Element of FS);
let e1 being (Equivalence_Relation of FS);
let e2 being (Equivalence_Relation of FS);
let x;
let y;
assume that
A1: f = ( alpha FD )
and
A2: (FS = ( union { ( ( S . i ) `1 ) where i is (Element of ( NAT )): (not contradiction) } ) & FD = ( union { ( ( S . i ) `2 ) where i is (Element of ( NAT )): (not contradiction) } ))
and
A3: e1 in (the carrier of ( Image f ))
and
A4: e2 in (the carrier of ( Image f ))
and
A5: [ x,y ] in ( e1 "\/" e2 );
A6: 4 in ( Seg 5 );
( field ( e1 "\/" e2 ) ) = FS by ORDERS_1:12;
then reconsider u = x,v = y as (Element of FS) by A5,RELAT_1:15;
A7: 1 in ( Seg 5 );
( Image f ) = ( subrelstr ( rng f ) ) by YELLOW_2:def 2;
then A8: (the carrier of ( Image f )) = ( rng f ) by YELLOW_0:def 15;
then consider a being set such that A9: a in ( dom f ) and A10: e1 = ( f . a ) by A3,FUNCT_1:def 3;
consider b being set such that A11: b in ( dom f ) and A12: e2 = ( f . b ) by A4,A8,FUNCT_1:def 3;
reconsider a,b as (Element of L) by A9,A11;
reconsider a,b as (Element of L);
consider e being (Equivalence_Relation of FS) such that A13: e = ( f . ( a "\/" b ) ) and A14: (for u,v being (Element of FS) holds ([ u,v ] in e iff ( FD . (u,v) ) <= ( a "\/" b ))) by A1,CKB32:def 1;
consider e19 being (Equivalence_Relation of FS) such that A15: e19 = ( f . a ) and A16: (for u,v being (Element of FS) holds ([ u,v ] in e19 iff ( FD . (u,v) ) <= a)) by A1,CKB32:def 1;
consider e29 being (Equivalence_Relation of FS) such that A17: e29 = ( f . b ) and A18: (for u,v being (Element of FS) holds ([ u,v ] in e29 iff ( FD . (u,v) ) <= b)) by A1,CKB32:def 1;
A19: 3 in ( Seg 5 );
e = ( ( f . a ) "\/" ( f . b ) ) by A13,WAYBEL_6:2
.= ( e1 "\/" e2 ) by A10,A12,CKB12:1;
then ( FD . (u,v) ) <= ( a "\/" b ) by A5,A14;
then consider z1,z2,z3 being (Element of FS) such that A20: ( FD . (u,z1) ) = a and A21: ( FD . (z2,z3) ) = a and A22: ( FD . (z1,z2) ) = b and A23: ( FD . (z3,v) ) = b by A2,CKB79:1;
defpred P[ set,set ]
 means
((((($1 = 1 implies $2 = u) & ($1 = 2 implies $2 = z1)) & ($1 = 3 implies $2 = z2)) & ($1 = 4 implies $2 = z3)) & ($1 = 5 implies $2 = v));
A24: (for m being Nat holds (m in ( Seg 5 ) implies (ex w being set st P[ m,w ])))
proof
let m being Nat;
assume A25: m in ( Seg 5 );
per cases  by A25,Lm3;
suppose A26: m = 1;

take x;
thus thesis by A26;
end;
suppose A27: m = 2;

take z1;
thus thesis by A27;
end;
suppose A28: m = 3;

take z2;
thus thesis by A28;
end;
suppose A29: m = 4;

take z3;
thus thesis by A29;
end;
suppose A30: m = 5;

take y;
thus thesis by A30;
end;
end;
(ex p being FinSequence st (( dom p ) = ( Seg 5 ) & (for k being Nat holds (k in ( Seg 5 ) implies P[ k,( p . k ) ])))) from FINSEQ_1:sch 1(A24);
then consider h being FinSequence such that A31: ( dom h ) = ( Seg 5 ) and A32: (for m being Nat holds (m in ( Seg 5 ) implies (((((m = 1 implies ( h . m ) = u) & (m = 2 implies ( h . m ) = z1)) & (m = 3 implies ( h . m ) = z2)) & (m = 4 implies ( h . m ) = z3)) & (m = 5 implies ( h . m ) = v))));
A33: ( len h ) = 5 by A31,FINSEQ_1:def 3;
A34: 5 in ( Seg 5 );
A35: 2 in ( Seg 5 );
( rng h ) c= FS
proof
let w being set;
assume w in ( rng h );
then consider j being set such that A36: j in ( dom h ) and A37: w = ( h . j ) by FUNCT_1:def 3;
per cases  by A31,A36,Lm3;
suppose j = 1;

then ( h . j ) = u by A32,A7;
hence thesis by A37;
end;
suppose j = 2;

then ( h . j ) = z1 by A32,A35;
hence thesis by A37;
end;
suppose j = 3;

then ( h . j ) = z2 by A32,A19;
hence thesis by A37;
end;
suppose j = 4;

then ( h . j ) = z3 by A32,A6;
hence thesis by A37;
end;
suppose j = 5;

then ( h . j ) = v by A32,A34;
hence thesis by A37;
end;
end;
then reconsider h as (FinSequence of FS) by FINSEQ_1:def 4;
reconsider h as non  empty (FinSequence of FS) by A31;
A38: ( h . 1 ) = x by A32,A7;
A39: (for j being (Element of ( NAT )) holds ((1 <= j & j < ( len h )) implies ((j is  odd implies [ ( h . j ),( h . ( j + 1 ) ) ] in e1) & (j is  even implies [ ( h . j ),( h . ( j + 1 ) ) ] in e2))))
proof
let j being (Element of ( NAT ));
assume A40: (1 <= j & j < ( len h ));
per cases  by A33,A40,Lm2;
suppose A41: j = 1;

[ u,z1 ] in e19 by A16,A20;
then [ ( h . 1 ),z1 ] in e19 by A32,A7;
hence thesis by A10,A15,A32,A35,A41;
end;
suppose A42: j = 3;

[ z2,z3 ] in e19 by A16,A21;
then A43: [ ( h . 3 ),z3 ] in e19 by A32,A19;
( ( 2 * 1 ) + 1 ) = j by A42;
hence thesis by A10,A15,A32,A6,A43;
end;
suppose A44: j = 2;

[ z1,z2 ] in e29 by A18,A22;
then A45: [ ( h . 2 ),z2 ] in e29 by A32,A35;
( 2 * 1 ) = j by A44;
hence thesis by A12,A17,A32,A19,A45;
end;
suppose A46: j = 4;

[ z3,v ] in e29 by A18,A23;
then A47: [ ( h . 4 ),v ] in e29 by A32,A6;
( 2 * 2 ) = j by A46;
hence thesis by A12,A17,A32,A34,A47;
end;
end;
take h;
thus ( len h ) = ( 3 + 2 ) by A31,FINSEQ_1:def 3;
( h . ( len h ) ) = ( h . 5 ) by A31,FINSEQ_1:def 3
.= y by A32,A34;
hence thesis by A38,A39,CKB20:def 1;
end;
