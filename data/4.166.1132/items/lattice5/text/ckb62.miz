reserve O3 for Ordinal;
theorem
Th35: (for d being (BiFunction of A,L) holds (for q being (QuadrSeq of d) holds (O1 c= O2 implies ( ConsecutiveDelta (q,O1) ) c= ( ConsecutiveDelta (q,O2) ))))
proof
let d being (BiFunction of A,L);
let q being (QuadrSeq of d);
defpred X[ Ordinal ]
 means
(O1 c= $1 implies ( ConsecutiveDelta (q,O1) ) c= ( ConsecutiveDelta (q,$1) ));
A1: (for O2 holds (((O2 <> ( {} ) & O2 is  limit_ordinal) & (for O3 holds (O3 in O2 implies X[ O3 ]))) implies X[ O2 ]))
proof
deffunc U(Ordinal) = ( ConsecutiveDelta (q,$1) );
let O2;
assume that
A2: (O2 <> ( {} ) & O2 is  limit_ordinal)
and
(for O3 holds (O3 in O2 implies (O1 c= O3 implies ( ConsecutiveDelta (q,O1) ) c= ( ConsecutiveDelta (q,O3) ))));
consider L being T-Sequence such that A3: (( dom L ) = O2 & (for O3 being Ordinal holds (O3 in O2 implies ( L . O3 ) = U(O3)))) from ORDINAL2:sch 2;
A4: ( ConsecutiveDelta (q,O2) ) = ( union ( rng L ) ) by A2,A3,CKB57:1;
assume A5: O1 c= O2;
per cases ;
suppose O1 = O2;

hence thesis;
end;
suppose O1 <> O2;

then A6: O1 c< O2 by A5,XBOOLE_0:def 8;
then O1 in O2 by ORDINAL1:11;
then A7: ( L . O1 ) in ( rng L ) by A3,FUNCT_1:def 3;
( L . O1 ) = ( ConsecutiveDelta (q,O1) ) by A3,A6,ORDINAL1:11;
hence thesis by A4,A7,ZFMISC_1:74;
end;
end;
A8: (for O2 holds (X[ O2 ] implies X[ ( succ O2 ) ]))
proof
let O2;
assume A9: (O1 c= O2 implies ( ConsecutiveDelta (q,O1) ) c= ( ConsecutiveDelta (q,O2) ));
assume A10: O1 c= ( succ O2 );
per cases ;
suppose O1 = ( succ O2 );

hence thesis;
end;
suppose O1 <> ( succ O2 );

then O1 c< ( succ O2 ) by A10,XBOOLE_0:def 8;
then A11: O1 in ( succ O2 ) by ORDINAL1:11;
( ConsecutiveDelta (q,( succ O2 )) ) = ( new_bi_fun (( BiFun (( ConsecutiveDelta (q,O2) ),( ConsecutiveSet (A,O2) ),L) ),( Quadr (q,O2) )) ) by CKB56:1
.= ( new_bi_fun (( ConsecutiveDelta (q,O2) ),( Quadr (q,O2) )) ) by CKB53:def 1;
then ( ConsecutiveDelta (q,O2) ) c= ( ConsecutiveDelta (q,( succ O2 )) ) by CKB41:1;
hence thesis by A9,A11,ORDINAL1:22,XBOOLE_1:1;
end;
end;
A12: X[ ( {} ) ];
(for O2 holds X[ O2 ]) from ORDINAL2:sch 1(A12,A8,A1);
hence thesis;
end;
