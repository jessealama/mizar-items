begin
reserve A for non  empty set;
reserve O for Ordinal;
reserve O1 for Ordinal;
reserve O2 for Ordinal;
registration
let A;
let O;
cluster ( ConsecutiveSet (A,O) ) -> non  empty;
coherence
proof
defpred X[ Ordinal ]
 means
( ConsecutiveSet (A,$1) ) is non  empty;
A1: (for O holds (X[ O ] implies X[ ( succ O ) ]))
proof
let O1;
assume ( ConsecutiveSet (A,O1) ) is non  empty;
( ConsecutiveSet (A,( succ O1 )) ) = ( new_set ( ConsecutiveSet (A,O1) ) ) by CKB46:1;
hence thesis;
end;
A2: (for O holds (((O <> ( {} ) & O is  limit_ordinal) & (for B being Ordinal holds (B in O implies X[ B ]))) implies X[ O ]))
proof
deffunc U(Ordinal) = ( ConsecutiveSet (A,$1) );
let O1;
assume that
A3: O1 <> ( {} )
and
A4: O1 is  limit_ordinal
and
(for O2 holds (O2 in O1 implies ( ConsecutiveSet (A,O2) ) is non  empty));
A5: ( {} ) in O1 by A3,ORDINAL3:8;
consider Ls being T-Sequence such that A6: (( dom Ls ) = O1 & (for O2 being Ordinal holds (O2 in O1 implies ( Ls . O2 ) = U(O2)))) from ORDINAL2:sch 2;
( Ls . ( {} ) ) = ( ConsecutiveSet (A,( {} )) ) by A3,A6,ORDINAL3:8
.= A by CKB45:1;
then A7: A in ( rng Ls ) by A6,A5,FUNCT_1:def 3;
( ConsecutiveSet (A,O1) ) = ( union ( rng Ls ) ) by A3,A4,A6,CKB47:1;
then A c= ( ConsecutiveSet (A,O1) ) by A7,ZFMISC_1:74;
hence thesis;
end;
A8: X[ ( {} ) ] by CKB45:1;
(for O holds X[ O ]) from ORDINAL2:sch 1(A8,A1,A2);
hence thesis;
end;
end;
