begin
reserve X for non  empty set;
reserve x for (Element of X);
reserve V for Algebra;
reserve V1 for (Subset of V);
reserve a for Real;
theorem
Th8: (for V being Algebra holds (for V1 being (Subalgebra of V) holds (((((for v1,w1 being (Element of V1) holds (for v,w being (Element of V) holds ((v1 = v & w1 = w) implies ( v1 + w1 ) = ( v + w )))) & (for v1,w1 being (Element of V1) holds (for v,w being (Element of V) holds ((v1 = v & w1 = w) implies ( v1 * w1 ) = ( v * w ))))) & (for v1 being (Element of V1) holds (for v being (Element of V) holds (for a being Real holds (v1 = v implies ( a * v1 ) = ( a * v )))))) & ( 1_ V1 ) = ( 1_ V )) & ( 0. V1 ) = ( 0. V ))))
proof
let V being Algebra;
let V1 being (Subalgebra of V);
hereby
let x1 being (Element of V1);
let y1 being (Element of V1);
let x being (Element of V);
let y being (Element of V);
assume A1: (x1 = x & y1 = y);
( x1 + y1 ) = ( ( (the addF of V) || (the carrier of V1) ) . [ x1,y1 ] ) by CKB18:def 1;
hence ( x1 + y1 ) = ( x + y ) by A1,FUNCT_1:49;
end;
hereby
let x1 being (Element of V1);
let y1 being (Element of V1);
let x being (Element of V);
let y being (Element of V);
assume A2: (x1 = x & y1 = y);
( x1 * y1 ) = ( ( (the multF of V) || (the carrier of V1) ) . [ x1,y1 ] ) by CKB18:def 1;
hence ( x1 * y1 ) = ( x * y ) by A2,FUNCT_1:49;
end;
hereby
let v1 being (Element of V1);
let v being (Element of V);
let a being Real;
assume A3: v1 = v;
( a * v1 ) = ( ( (the Mult of V) | [: ( REAL ),(the carrier of V1) :] ) . [ a,v1 ] ) by CKB18:def 1;
hence ( a * v1 ) = ( a * v ) by A3,FUNCT_1:49;
end;
thus thesis by CKB18:def 1;
end;
