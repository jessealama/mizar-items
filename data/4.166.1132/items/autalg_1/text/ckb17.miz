begin
reserve I for set;
reserve A for (ManySortedSet of I);
theorem
Th12: (for x being set holds (for A being (ManySortedSet of { x }) holds A = ( x .--> ( A . x ) )))
proof
let x being set;
let A being (ManySortedSet of { x });
A1: ( dom A ) = { x } by PARTFUN1:def 2;
then ( rng A ) = { ( A . x ) } by FUNCT_1:4;
hence thesis by A1,FUNCOP_1:9;
end;
