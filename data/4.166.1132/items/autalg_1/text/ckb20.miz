begin
reserve UA for Universal_Algebra;
reserve f for (Function of UA,UA);
theorem
Th16: (for F,G being  Function-yielding Function holds ((F is  "1-1" & G is  "1-1") implies ( G ** F ) is  "1-1"))
proof
let F being  Function-yielding Function;
let G being  Function-yielding Function;
assume that
A1: F is  "1-1"
and
A2: G is  "1-1";
let i being set;
let f being Function;
assume that
A3: i in ( dom ( G ** F ) )
and
A4: ( ( G ** F ) . i ) = f;
A5: ( dom ( G ** F ) ) = ( ( dom G ) /\ ( dom F ) ) by PBOOLE:def 19;
then i in ( dom F ) by A3,XBOOLE_0:def 4;
then A6: ( F . i ) is  one-to-one by A1,MSUALG_3:def 2;
i in ( dom G ) by A3,A5,XBOOLE_0:def 4;
then ( G . i ) is  one-to-one by A2,MSUALG_3:def 2;
then ( ( G . i ) * ( F . i ) ) is  one-to-one by A6;
hence thesis by A3,A4,PBOOLE:def 19;
end;
