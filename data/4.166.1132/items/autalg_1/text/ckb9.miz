begin
reserve UA for Universal_Algebra;
definition
let UA;
func UAAutComp UA -> (BinOp of ( UAAut UA ))
means
:Def2: (for x,y being (Element of ( UAAut UA )) holds ( it . (x,y) ) = ( y * x ));
existence
proof
defpred P[ (Element of ( UAAut UA )),(Element of ( UAAut UA )),set ]
 means
$3 = ( $2 * $1 );
A1: (for x,y being (Element of ( UAAut UA )) holds (ex m being (Element of ( UAAut UA )) st P[ x,y,m ]))
proof
let x being (Element of ( UAAut UA )),y being (Element of ( UAAut UA ));
reconsider xx = x,yy = y as (Function of UA,UA);
reconsider m = ( yy * xx ) as (Element of ( UAAut UA )) by CKB8:1;
take m;
thus thesis;
end;
thus (ex IT being (BinOp of ( UAAut UA )) st (for x,y being (Element of ( UAAut UA )) holds P[ x,y,( IT . (x,y) ) ])) from BINOP_1:sch 3(A1);
end;
uniqueness
proof
let b1 being (BinOp of ( UAAut UA )),b2 being (BinOp of ( UAAut UA ));
assume that
A2: (for x,y being (Element of ( UAAut UA )) holds ( b1 . (x,y) ) = ( y * x ))
and
A3: (for x,y being (Element of ( UAAut UA )) holds ( b2 . (x,y) ) = ( y * x ));
(for x,y being (Element of ( UAAut UA )) holds ( b1 . (x,y) ) = ( b2 . (x,y) ))
proof
let x being (Element of ( UAAut UA )),y being (Element of ( UAAut UA ));
thus ( b1 . (x,y) ) = ( y * x ) by A2
.= ( b2 . (x,y) ) by A3;
end;
hence thesis by BINOP_1:2;
end;
end;
