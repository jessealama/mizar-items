environ
vocabularies NUMBERS,SUBSET_1,AFINSQ_1,NAT_1,XREAL_0,ORDINAL1,ARYTM_3,XXREAL_0,ARYTM_1,CARD_1,ORDINAL4,XBOOLE_0,FINSEQ_1,PRE_POLY,NEWTON,TARSKI,RELAT_1,SETFAM_1,MODAL_1;
notations TARSKI,XBOOLE_0,SUBSET_1,NUMBERS,XCMPLX_0,NAT_1,ORDINAL1,SETFAM_1,XREAL_0,XXREAL_0,AFINSQ_1,CATALAN2,FLANG_1,CKB19;
definitions TARSKI,CKB19;
theorems AFINSQ_1,NAT_1,SUBSET_1,TARSKI,XBOOLE_0,XBOOLE_1,XREAL_1,ZFMISC_1,FLANG_1,XXREAL_0,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB37,CKB38,CKB39,CKB40;
schemes CARD_FIL,DOMAIN_1,NAT_1;
registrations SUBSET_1,NAT_1,ORDINAL1,AFINSQ_1,XREAL_0,XBOOLE_0,XXREAL_0,FUNCT_1,CATALAN2;
constructors XXREAL_0,NAT_1,XREAL_0,FLANG_1,CKB19;
requirements NUMERALS,REAL,BOOLE,SUBSET,ARITHM;
begin
reserve E for set;
reserve A for (Subset of ( E ^omega ));
reserve k for Nat;
reserve m for Nat;
reserve n for Nat;
theorem
Th40: ( ( A |^ (m,n) ) |^ k ) = ( A |^ (( m * k ),( n * k )) )
proof
per cases ;
suppose A1: m <= n;

defpred P[ Nat ]
 means
( ( A |^ (m,n) ) |^ $1 ) = ( A |^ (( m * $1 ),( n * $1 )) );
A2:now
let k;
A3: ( m * k ) <= ( n * k ) by A1,XREAL_1:64;
assume P[ k ];
then ( ( A |^ (m,n) ) |^ ( k + 1 ) ) = ( ( A |^ (( m * k ),( n * k )) ) ^^ ( A |^ (m,n) ) ) by FLANG_1:23
.= ( A |^ (( ( m * k ) + m ),( ( n * k ) + n )) ) by A1,A3,CKB38:1
.= ( A |^ (( m * ( k + 1 ) ),( n * ( k + 1 ) )) );
hence P[ ( k + 1 ) ];
end;
( ( A |^ (m,n) ) |^ ( 0 ) ) = { ( <%> E ) } by FLANG_1:24
.= ( A |^ ( 0 ) ) by FLANG_1:24
.= ( A |^ (( m * ( 0 ) ),( n * ( 0 ) )) ) by CKB23:1;
then A4: P[ ( 0 ) ];
(for k holds P[ k ]) from NAT_1:sch 2(A4,A2);
hence thesis;
end;
suppose A5: k = ( 0 );

hence ( ( A |^ (m,n) ) |^ k ) = { ( <%> E ) } by FLANG_1:24
.= ( A |^ ( 0 ) ) by FLANG_1:24
.= ( A |^ (( m * k ),( n * k )) ) by A5,CKB23:1;
end;
suppose A6: (m > n & k <> ( 0 ));

then ( A |^ (m,n) ) = ( {} ) by CKB22:1;
then A7: ( ( A |^ (m,n) ) |^ k ) = ( {} ) by A6,FLANG_1:27;
( m * k ) > ( n * k ) by A6,XREAL_1:68;
hence thesis by A7,CKB22:1;
end;
end;
