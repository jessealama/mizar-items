environ
vocabularies NUMBERS,SUBSET_1,AFINSQ_1,NAT_1,XREAL_0,ORDINAL1,ARYTM_3,XXREAL_0,ARYTM_1,CARD_1,ORDINAL4,XBOOLE_0,FINSEQ_1,PRE_POLY,NEWTON,TARSKI,RELAT_1,SETFAM_1,MODAL_1;
notations TARSKI,XBOOLE_0,SUBSET_1,NUMBERS,XCMPLX_0,NAT_1,ORDINAL1,SETFAM_1,XREAL_0,XXREAL_0,AFINSQ_1,CATALAN2,FLANG_1,CKB19;
definitions TARSKI,CKB19;
theorems AFINSQ_1,NAT_1,SUBSET_1,TARSKI,XBOOLE_0,XBOOLE_1,XREAL_1,ZFMISC_1,FLANG_1,XXREAL_0,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30;
schemes CARD_FIL,DOMAIN_1,NAT_1;
registrations SUBSET_1,NAT_1,ORDINAL1,AFINSQ_1,XREAL_0,XBOOLE_0,XXREAL_0,FUNCT_1,CATALAN2;
constructors XXREAL_0,NAT_1,XREAL_0,FLANG_1,CKB19;
requirements NUMERALS,REAL,BOOLE,SUBSET,ARITHM;
begin
reserve E for set;
reserve x for set;
reserve A for (Subset of ( E ^omega ));
reserve m for Nat;
reserve n for Nat;
theorem
Th30: (((x in A & x <> ( <%> E )) & (m > ( 0 ) or n > ( 0 ))) implies ( A |^ (m,n) ) <> { ( <%> E ) })
proof
assume that
A1: x in A
and
A2: x <> ( <%> E )
and
A3: (m > ( 0 ) or n > ( 0 ));
per cases  by A3;
suppose m > n;

hence thesis by CKB22:1;
end;
suppose A4: (m <= n & m > ( 0 ));

A5: ( A |^ m ) <> ( {} ) by A1,FLANG_1:27;
( A |^ m ) <> { ( <%> E ) } by A1,A2,A4,CKB7:1;
then A6: (ex x st (x in ( A |^ m ) & x <> ( <%> E ))) by A5,ZFMISC_1:35;
( A |^ m ) c= ( A |^ (m,n) ) by A4,CKB21:1;
hence thesis by A6,TARSKI:def 1;
end;
suppose A7: (m <= n & n > ( 0 ));

A8: ( A |^ n ) <> ( {} ) by A1,FLANG_1:27;
( A |^ n ) <> { ( <%> E ) } by A1,A2,A7,CKB7:1;
then A9: (ex x st (x in ( A |^ n ) & x <> ( <%> E ))) by A8,ZFMISC_1:35;
( A |^ n ) c= ( A |^ (m,n) ) by A7,CKB21:1;
hence thesis by A9,TARSKI:def 1;
end;
end;
