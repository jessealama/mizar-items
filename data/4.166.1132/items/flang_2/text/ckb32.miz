environ
vocabularies NUMBERS,SUBSET_1,AFINSQ_1,NAT_1,XREAL_0,ORDINAL1,ARYTM_3,XXREAL_0,ARYTM_1,CARD_1,ORDINAL4,XBOOLE_0,FINSEQ_1,PRE_POLY,NEWTON,TARSKI,RELAT_1,SETFAM_1,MODAL_1;
notations TARSKI,XBOOLE_0,SUBSET_1,NUMBERS,XCMPLX_0,NAT_1,ORDINAL1,SETFAM_1,XREAL_0,XXREAL_0,AFINSQ_1,CATALAN2,FLANG_1,CKB19;
definitions TARSKI,CKB19;
theorems AFINSQ_1,NAT_1,SUBSET_1,TARSKI,XBOOLE_0,XBOOLE_1,XREAL_1,ZFMISC_1,FLANG_1,XXREAL_0,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31;
schemes CARD_FIL,DOMAIN_1,NAT_1;
registrations SUBSET_1,NAT_1,ORDINAL1,AFINSQ_1,XREAL_0,XBOOLE_0,XXREAL_0,FUNCT_1,CATALAN2;
constructors XXREAL_0,NAT_1,XREAL_0,FLANG_1,CKB19;
requirements NUMERALS,REAL,BOOLE,SUBSET,ARITHM;
begin
reserve E for set;
reserve x for set;
reserve A for (Subset of ( E ^omega ));
reserve k for Nat;
reserve m for Nat;
reserve n for Nat;
theorem
Th31: (( A |^ (m,n) ) = { ( <%> E ) } iff (((m <= n & A = { ( <%> E ) }) or (m = ( 0 ) & n = ( 0 ))) or (m = ( 0 ) & A = ( {} ))))
proof
thus (( A |^ (m,n) ) = { ( <%> E ) } implies (((m <= n & A = { ( <%> E ) }) or (m = ( 0 ) & n = ( 0 ))) or (m = ( 0 ) & A = ( {} ))))
proof
assume that
A1: ( A |^ (m,n) ) = { ( <%> E ) }
and
A2: (((m > n or A <> { ( <%> E ) }) & (m <> ( 0 ) or n <> ( 0 ))) & (m <> ( 0 ) or A <> ( {} )));
per cases  by A2;
suppose m > n;

hence contradiction by A1,CKB22:1;
end;
suppose A3: ((m <= n & A <> { ( <%> E ) }) & (m <> ( 0 ) or (n <> ( 0 ) & A <> ( {} ))));

per cases  by A3;
suppose A4: m <> ( 0 );

per cases ;
suppose A = ( {} );

hence contradiction by A1,A4,CKB22:1;
end;
suppose A <> ( {} );

then (ex x st (x in A & x <> ( <%> E ))) by A3,ZFMISC_1:35;
hence contradiction by A1,A4,CKB31:1;
end;
end;
suppose A5: (n <> ( 0 ) & A <> ( {} ));

then (ex x st (x in A & x <> ( <%> E ))) by A3,ZFMISC_1:35;
hence contradiction by A1,A5,CKB31:1;
end;
end;
end;

assume A6: (((m <= n & A = { ( <%> E ) }) or (m = ( 0 ) & n = ( 0 ))) or (m = ( 0 ) & A = ( {} )));
per cases  by A6;
suppose A7: (m <= n & A = { ( <%> E ) });

A8:now
let x;
assume x in { ( <%> E ) };
then A9: x in ( { ( <%> E ) } |^ m ) by FLANG_1:29;
( { ( <%> E ) } |^ m ) c= ( { ( <%> E ) } |^ (m,n) ) by A7,CKB21:1;
hence x in ( A |^ (m,n) ) by A7,A9;
end;
now
let x;
assume x in ( A |^ (m,n) );
then (ex k st ((m <= k & k <= n) & x in ( { ( <%> E ) } |^ k ))) by A7,CKB20:1;
hence x in { ( <%> E ) } by FLANG_1:29;
end;
hence thesis by A8,TARSKI:1;
end;
suppose A10: (m = ( 0 ) & n = ( 0 ));

( A |^ (( 0 ),( 0 )) ) = ( A |^ ( 0 ) ) by CKB23:1
.= { ( <%> E ) } by FLANG_1:29;
hence thesis by A10;
end;
suppose A11: (m = ( 0 ) & A = ( {} ));

now
let x;
assume x in ( A |^ (1,n) );
then (ex k st ((1 <= k & k <= n) & x in ( A |^ k ))) by CKB20:1;
hence contradiction by A11,FLANG_1:27;
end;
then A12: ( A |^ (1,n) ) = ( {} ) by XBOOLE_0:def 1;
( A |^ (( 0 ),n) ) = ( ( A |^ (( 0 ),( 0 )) ) \/ ( A |^ (( ( 0 ) + 1 ),n) ) ) by CKB26:1
.= ( ( A |^ ( 0 ) ) \/ ( A |^ (( ( 0 ) + 1 ),n) ) ) by CKB23:1
.= { ( <%> E ) } by A12,FLANG_1:29;
hence thesis by A11;
end;
end;
