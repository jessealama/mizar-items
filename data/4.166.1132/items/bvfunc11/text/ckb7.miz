environ
vocabularies XBOOLE_0,SUBSET_1,FUNCT_2,MARGREL1,PARTIT1,EQREL_1,TARSKI,BVFUNC_2,FUNCOP_1,FUNCT_1,ZFMISC_1,SETFAM_1,RELAT_1,BVFUNC_1,XBOOLEAN;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,MARGREL1,RELAT_1,SETFAM_1,FUNCT_1,FUNCT_2,EQREL_1,FUNCOP_1,PARTIT1,BVFUNC_1,BVFUNC_2,FUNCT_4;
definitions TARSKI,BVFUNC_1,XBOOLE_0,XBOOLEAN,FUNCOP_1;
theorems TARSKI,FUNCT_1,SETFAM_1,EQREL_1,ZFMISC_1,PARTIT1,BVFUNC_1,BVFUNC_2,ENUMSET1,FUNCT_4,FUNCOP_1,MSSUBFAM,PARTIT_2,BVFUNC_4,XBOOLE_0,XBOOLE_1,XBOOLEAN,MARGREL1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6;
registrations XBOOLE_0,FUNCOP_1,FUNCT_4,EQREL_1,MARGREL1;
constructors SETFAM_1,FUNCT_4,XCMPLX_0,BVFUNC_1,BVFUNC_2,RELSET_1;
requirements SUBSET,BOOLE,ARITHM;
begin
reserve Y for non  empty set;
reserve G for (Subset of ( PARTITIONS Y ));
reserve A for (a_partition of Y);
reserve B for (a_partition of Y);
theorem
(for G being (Subset of ( PARTITIONS Y )) holds (for A,B being (a_partition of Y) holds ((G = { A,B } & A <> B) implies ( CompF (A,G) ) = B)))
proof
let G being (Subset of ( PARTITIONS Y ));
let A being (a_partition of Y);
let B being (a_partition of Y);
assume that
A1: G = { A,B }
and
A2: A <> B;
A3: ( '/\' { B } ) c= B
proof
let x being set;
assume x in ( '/\' { B } );
then consider h being Function,F being (Subset-Family of Y) such that A4: ( dom h ) = { B } and A5: ( rng h ) = F and A6: (for d being set holds (d in { B } implies ( h . d ) in d)) and A7: x = ( Intersect F ) and x <> ( {} ) by BVFUNC_2:def 1;
( rng h ) = { ( h . B ) } by A4,FUNCT_1:4;
then A8: x = ( meet { ( h . B ) } ) by A5,A7,SETFAM_1:def 9;
B in { B } by TARSKI:def 1;
then ( h . B ) in B by A6;
hence thesis by A8,SETFAM_1:10;
end;
A9: B c= ( '/\' { B } )
proof
let x being set;
set h0 = ( B .--> x );
A10: ( dom h0 ) = { B } by FUNCOP_1:13;
assume A11: x in B;
A12: (for d being set holds (d in { B } implies ( h0 . d ) in d))
proof
let d being set;
assume d in { B };
then d = B by TARSKI:def 1;
hence thesis by A11,FUNCOP_1:72;
end;
A13: ( rng h0 ) = { x } by FUNCOP_1:8;
( rng h0 ) c= ( bool Y )
proof
let m being set;
assume m in ( rng h0 );
then m = x by A13,TARSKI:def 1;
hence thesis by A11;
end;
then reconsider F0 = ( rng h0 ) as (Subset-Family of Y);
( meet F0 ) = ( Intersect F0 ) by A13,SETFAM_1:def 9;
then A14: x = ( Intersect F0 ) by A13,SETFAM_1:10;
x <> ( {} ) by A11,EQREL_1:def 4;
hence thesis by A10,A12,A14,BVFUNC_2:def 1;
end;
A in { A } by TARSKI:def 1;
then A15: ( { A } \ { A } ) = ( {} ) by ZFMISC_1:60;
( G \ { A } ) = ( ( { A } \/ { B } ) \ { A } ) by A1,ENUMSET1:1
.= ( ( { A } \ { A } ) \/ ( { B } \ { A } ) ) by XBOOLE_1:42
.= ( ( { A } \ { A } ) \/ { B } ) by A2,ZFMISC_1:14;
then ( CompF (A,G) ) = ( '/\' { B } ) by A15,BVFUNC_2:def 7;
hence thesis by A3,A9,XBOOLE_0:def 10;
end;
