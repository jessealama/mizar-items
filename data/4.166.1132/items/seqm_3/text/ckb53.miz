begin
reserve seq for Real_Sequence;
reserve seq1 for Real_Sequence;
theorem
((seq is  monotone & seq1 is (subsequence of seq)) implies seq1 is  monotone)
proof
assume that
A1: seq is  monotone
and
A2: seq1 is (subsequence of seq);
A3:now
assume seq is  non-increasing;
then seq1 is  non-increasing by A2,CKB52:1;
hence thesis by CKB14:def 1;
end;
now
assume seq is  non-decreasing;
then seq1 is  non-decreasing by A2,CKB51:1;
hence thesis by CKB14:def 1;
end;
hence thesis by A1,A3,CKB14:def 1;
end;
