theorem
Th10: (seq is  decreasing iff (for n holds (for m holds (n < m implies ( seq . m ) < ( seq . n )))))
proof
thus (seq is  decreasing implies (for n holds (for m holds (n < m implies ( seq . m ) < ( seq . n )))))
proof
assume seq is  decreasing;
then (for n holds (for k holds ( seq . ( ( n + 1 ) + k ) ) < ( seq . n ))) by CKB17:1;
hence thesis by CKB3:1;
end;

assume A1: (for n holds (for m holds (n < m implies ( seq . m ) < ( seq . n ))));
let n;
let m;
assume that
n in ( dom seq )
and
m in ( dom seq )
and
A2: n < m;
thus thesis by A1,A2;
end;
