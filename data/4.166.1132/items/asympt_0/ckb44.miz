requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve c for Real;
reserve n for (Element of ( NAT ));
reserve N for (Element of ( NAT ));
theorem
Th11: (for f,g being  eventually-nonnegative Real_Sequence holds (f in ( Big_Oh g ) implies ( Big_Oh f ) c= ( Big_Oh g )))
proof
let f being  eventually-nonnegative Real_Sequence,g being  eventually-nonnegative Real_Sequence;
assume f in ( Big_Oh g );
then consider t being (Element of ( Funcs (( NAT ),( REAL )) )) such that A1: t = f and A2: (ex c st (ex N st (c > ( 0 ) & (for n holds (n >= N implies (( t . n ) <= ( c * ( g . n ) ) & ( t . n ) >= ( 0 )))))));
consider ct being Real,Nt being (Element of ( NAT )) such that ct > ( 0 ) and A3: (for n holds (n >= Nt implies (( t . n ) <= ( ct * ( g . n ) ) & ( t . n ) >= ( 0 )))) by A2;
consider Ng being (Element of ( NAT )) such that A4: (for n holds (n >= Ng implies ( g . n ) >= ( 0 ))) by CKB11:def 1;
let x being set;
assume x in ( Big_Oh f );
then consider s being (Element of ( Funcs (( NAT ),( REAL )) )) such that A5: s = x and A6: (ex c st (ex N st (c > ( 0 ) & (for n holds (n >= N implies (( s . n ) <= ( c * ( f . n ) ) & ( s . n ) >= ( 0 )))))));
consider cs being Real,Ns being (Element of ( NAT )) such that A7: cs > ( 0 ) and A8: (for n holds (n >= Ns implies (( s . n ) <= ( cs * ( f . n ) ) & ( s . n ) >= ( 0 )))) by A6;
now
take c = ( max (( cs * ct ),( max (cs,ct) )) );
c >= ( max (cs,ct) ) by XXREAL_0:25;
hence c > ( 0 ) by A7,XXREAL_0:25;
take N = ( max (( max (Ns,Nt) ),Ng) );
let n;
assume A9: n >= N;
A10: N >= ( max (Ns,Nt) ) by XXREAL_0:25;
( max (Ns,Nt) ) >= Nt by XXREAL_0:25;
then N >= Nt by A10,XXREAL_0:2;
then n >= Nt by A9,XXREAL_0:2;
then ( t . n ) <= ( ct * ( g . n ) ) by A3;
then A11: ( cs * ( t . n ) ) <= ( cs * ( ct * ( g . n ) ) ) by A7,XREAL_1:64;
N >= Ng by XXREAL_0:25;
then n >= Ng by A9,XXREAL_0:2;
then ( g . n ) >= ( 0 ) by A4;
then ( ( cs * ct ) * ( g . n ) ) <= ( c * ( g . n ) ) by XREAL_1:64,XXREAL_0:25;
then A12: ( cs * ( t . n ) ) <= ( c * ( g . n ) ) by A11,XXREAL_0:2;
( max (Ns,Nt) ) >= Ns by XXREAL_0:25;
then N >= Ns by A10,XXREAL_0:2;
then A13: n >= Ns by A9,XXREAL_0:2;
then ( s . n ) <= ( cs * ( f . n ) ) by A8;
hence ( s . n ) <= ( c * ( g . n ) ) by A1,A12,XXREAL_0:2;
thus ( s . n ) >= ( 0 ) by A8,A13;
end;
hence thesis by A5;
end;
