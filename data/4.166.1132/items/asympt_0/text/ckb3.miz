begin
reserve n for (Element of ( NAT ));
reserve m for (Element of ( NAT ));
reserve N for (Element of ( NAT ));
reserve x for set;
scheme FinImInit2 { N() -> Nat,X() -> non  empty set,F(set) -> (Element of X()) } : { F(n) where n is (Element of ( NAT )): n < N() } is  finite non  empty  finite non  empty (Subset of X())
provided
A1: N() > ( 0 )
proof
set S = { F(n) where n is (Element of ( NAT )): n < N() };
consider m being Nat such that A2: N() = ( m + 1 ) by A1,NAT_1:6;
reconsider m as (Element of ( NAT )) by ORDINAL1:def 12;
set T = { F(n) where n is (Element of ( NAT )): n <= m };
A3:now
let x being set;
hereby
assume x in S;
then consider n being (Element of ( NAT )) such that A4: x = F(n) and A5: n < N();
n <= m by A2,A5,NAT_1:13;
hence x in T by A4;
end;
assume x in T;
then consider n being (Element of ( NAT )) such that A6: x = F(n) and A7: n <= m;
n < ( m + 1 ) by A7,NAT_1:13;
hence x in S by A2,A6;
end;
T is  finite non  empty  finite non  empty (Subset of X()) from CKB2:sch 1;
hence thesis by A3,TARSKI:1;
end;
