begin
reserve n for (Element of ( NAT ));
reserve N for (Element of ( NAT ));
theorem
Th4: (for f,g being Real_Sequence holds (((f is  convergent & g is  convergent) & g majorizes f) implies ( lim f ) <= ( lim g )))
proof
let f being Real_Sequence;
let g being Real_Sequence;
assume that
A1: (f is  convergent & g is  convergent)
and
A2: (ex N st (for n holds (n >= N implies ( f . n ) <= ( g . n ))));
consider N such that A3: (for n holds (n >= N implies ( f . n ) <= ( g . n ))) by A2;
now
let n;
assume n >= N;
then ( f . n ) <= ( g . n ) by A3;
then A4: ( ( f . n ) - ( f . n ) ) <= ( ( g . n ) - ( f . n ) ) by XREAL_1:9;
( ( g - f ) . n ) = ( ( g . n ) + ( ( - f ) . n ) ) by SEQ_1:7
.= ( ( g . n ) + ( - ( f . n ) ) ) by SEQ_1:10
.= ( ( g . n ) - ( f . n ) );
hence ( 0 ) <= ( ( g - f ) . n ) by A4;
end;
then A5: ( g - f ) is  eventually-nonnegative by CKB11:def 1;
A6: ( lim ( g - f ) ) = ( ( lim g ) - ( lim f ) ) by A1,SEQ_2:12;
( g - f ) is  convergent by A1,SEQ_2:11;
then ( 0 ) <= ( lim ( g - f ) ) by A5,CKB35:1;
then ( ( 0 ) + ( lim f ) ) <= ( ( ( lim g ) - ( lim f ) ) + ( lim f ) ) by A6,XREAL_1:6;
hence thesis;
end;
