begin
reserve n for (Element of ( NAT ));
reserve m for (Element of ( NAT ));
theorem
Th1: (for f being Real_Sequence holds (for N being (Element of ( NAT )) holds ((for n holds (n >= N implies ( f . n ) <= ( f . ( n + 1 ) ))) implies (for n,m being (Element of ( NAT )) holds ((N <= n & n <= m) implies ( f . n ) <= ( f . m ))))))
proof
let f being Real_Sequence;
let N being (Element of ( NAT ));
assume A1: (for n holds (n >= N implies ( f . n ) <= ( f . ( n + 1 ) )));
let n;
let m;
defpred P[ Nat ]
 means
( f . n ) <= ( f . $1 );
assume A2: n >= N;
A3: (for m being Nat holds ((m >= n & P[ m ]) implies P[ ( m + 1 ) ]))
proof
let m being Nat;
assume that
A4: m >= n
and
A5: ( f . n ) <= ( f . m );
(m in ( NAT ) & m >= N) by A2,A4,ORDINAL1:def 12,XXREAL_0:2;
then ( f . m ) <= ( f . ( m + 1 ) ) by A1;
hence thesis by A5,XXREAL_0:2;
end;
A6: P[ n ];
(for m being Nat holds (m >= n implies P[ m ])) from NAT_1:sch 8(A6,A3);
hence thesis;
end;
