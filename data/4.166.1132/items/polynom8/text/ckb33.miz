begin
theorem
Th21: (for L being Field holds (for i,j being Integer holds (for x being (Element of L) holds (x <> ( 0. L ) implies ( ( pow (x,i) ) * ( pow (x,j) ) ) = ( pow (x,( i + j )) )))))
proof
let L being Field;
let i being Integer;
let j being Integer;
let x being (Element of L);
defpred P[ Integer ]
 means
(for i being Integer holds ( pow (x,( i + $1 )) ) = ( ( pow (x,i) ) * ( pow (x,$1) ) ));
assume A1: x <> ( 0. L );
A2: (for j being Integer holds (P[ j ] implies (P[ ( j - 1 ) ] & P[ ( j + 1 ) ])))
proof
let j being Integer;
assume A3: (for i being Integer holds ( pow (x,( i + j )) ) = ( ( pow (x,i) ) * ( pow (x,j) ) ));
thus (for i being Integer holds ( pow (x,( i + ( j - 1 ) )) ) = ( ( pow (x,i) ) * ( pow (x,( j - 1 )) ) ))
proof
let i being Integer;
thus ( pow (x,( i + ( j - 1 ) )) ) = ( pow (x,( ( i - 1 ) + j )) )
.= ( ( pow (x,( i - 1 )) ) * ( pow (x,j) ) ) by A3
.= ( ( ( pow (x,i) ) * ( pow (x,( - 1 )) ) ) * ( pow (x,j) ) ) by A1,CKB32:1
.= ( ( pow (x,i) ) * ( ( pow (x,( - 1 )) ) * ( pow (x,j) ) ) ) by GROUP_1:def 3
.= ( ( pow (x,i) ) * ( pow (x,( j + ( - 1 ) )) ) ) by A3
.= ( ( pow (x,i) ) * ( pow (x,( j - 1 )) ) );
end;

let i being Integer;
thus ( pow (x,( i + ( j + 1 ) )) ) = ( pow (x,( ( i + 1 ) + j )) )
.= ( ( pow (x,( i + 1 )) ) * ( pow (x,j) ) ) by A3
.= ( ( ( pow (x,i) ) * ( pow (x,1) ) ) * ( pow (x,j) ) ) by A1,CKB31:1
.= ( ( pow (x,i) ) * ( ( pow (x,1) ) * ( pow (x,j) ) ) ) by GROUP_1:def 3
.= ( ( pow (x,i) ) * ( pow (x,( j + 1 )) ) ) by A3;
end;
A4: P[ ( 0 ) ]
proof
let i being Integer;
thus ( pow (x,( i + ( 0 ) )) ) = ( ( pow (x,i) ) * ( 1. L ) ) by VECTSP_1:def 4
.= ( ( pow (x,i) ) * ( pow (x,( 0 )) ) ) by CKB20:1;
end;
(for j being Integer holds P[ j ]) from INT_1:sch 4(A4,A2);
hence thesis;
end;
