requirements BOOLE,SUBSET,NUMERALS,REAL;
begin
reserve x for  real number;
reserve y for  real number;
reserve z for  real number;
reserve R for  real non  empty RelStr;
registration
cluster  interval ->  bounded for non  empty non  empty RelStr;
coherence
proof
let R being non  empty RelStr;
assume A1: R is  real;
given x being  real number,y being  real number such that
A2: x <= y
and
A3: (the carrier of R) = [. x,y .];

(ex x st (x in (the carrier of R) & (for y holds (y in (the carrier of R) implies x <= y))))
proof
take x;
thus x in (the carrier of R) by A2,A3,XXREAL_1:1;
let z;
assume z in (the carrier of R);
hence thesis by A3,XXREAL_1:1;
end;
then A4: R is  lower-bounded by A1,CKB25:1;
(ex x st (x in (the carrier of R) & (for y holds (y in (the carrier of R) implies x >= y))))
proof
take y;
thus y in (the carrier of R) by A2,A3,XXREAL_1:1;
let z;
assume z in (the carrier of R);
hence thesis by A3,XXREAL_1:1;
end;
then R is  upper-bounded by A1,CKB26:1;
hence thesis by A4;
end;
end;
