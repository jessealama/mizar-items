requirements BOOLE,SUBSET,NUMERALS,REAL;
begin
reserve y for  real number;
reserve z for  real number;
reserve R for  real non  empty RelStr;
reserve a for (Element of R);
reserve b for (Element of R);
theorem
Th8: (for R being  interval non  empty RelStr holds (for X being set holds  ex_sup_of X,R))
proof
let R being  interval non  empty RelStr;
let X being set;
consider a,b being  real number such that A1: a <= b and A2: (the carrier of R) = [. a,b .] by CKB2:def 1;
reconsider a,b as Real by XREAL_0:def 1;
reconsider Y = ( X /\ [. a,b .] ) as (Subset of ( REAL ));
[. a,b .] is non  empty  closed_interval by A1,MEASURE5:14;
then A3: [. a,b .] is  bounded_above by INTEGRA1:3;
then A4: Y is  bounded_above by XBOOLE_1:17,XXREAL_2:43;
A5: ( X /\ [. a,b .] ) c= [. a,b .] by XBOOLE_1:17;
(ex a being (Element of R) st (Y is_<=_than a & (for b being (Element of R) holds (Y is_<=_than b implies a <<= b))))
proof
per cases ;
suppose A6: Y is  empty;

reconsider x = a as (Element of R) by A1,A2,XXREAL_1:1;
take x;
thus Y is_<=_than x by A6,YELLOW_0:6;
let y being (Element of R);
x <= y by A2,XXREAL_1:1;
hence thesis by CKB15:1;
end;
suppose A7: Y is non  empty;

set c = the (Element of Y);
A8: c in Y by A7;
then reconsider c as Real;
A9: a <= c by A5,A8,XXREAL_1:1;
c <= ( upper_bound Y ) by A4,A7,SEQ_4:def 1;
then A10: a <= ( upper_bound Y ) by A9,XXREAL_0:2;
( upper_bound [. a,b .] ) = b by A1,JORDAN5A:19;
then ( upper_bound Y ) <= b by A3,A7,SEQ_4:48,XBOOLE_1:17;
then reconsider x = ( upper_bound Y ) as (Element of R) by A2,A10,XXREAL_1:1;
A11: (for y being (Element of R) holds (Y is_<=_than y implies x <<= y))
proof
let y being (Element of R);
assume A12: Y is_<=_than y;
(for z being  real number holds (z in Y implies z <= y))
proof
let z being  real number;
assume A13: z in Y;
then reconsider z as (Element of R) by A2,XBOOLE_0:def 4;
z <<= y by A12,A13,LATTICE3:def 9;
hence thesis by CKB15:1;
end;
then ( upper_bound Y ) <= y by A7,SEQ_4:144;
hence thesis by CKB15:1;
end;
take x;
(for b being (Element of R) holds (b in Y implies b <<= x))
proof
let b being (Element of R);
assume b in Y;
then b <= ( upper_bound Y ) by A4,SEQ_4:def 1;
hence thesis by CKB15:1;
end;
hence thesis by A11,LATTICE3:def 9;
end;
end;
then  ex_sup_of Y,R by YELLOW_0:15;
hence thesis by A2,YELLOW_0:50;
end;
