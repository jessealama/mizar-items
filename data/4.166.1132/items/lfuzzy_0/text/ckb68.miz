theorem
(for X,Y,Z,W being non  empty set holds (for R being (RMembership_Func of X,Y) holds (for S being (RMembership_Func of Y,Z) holds (for T being (RMembership_Func of Z,W) holds ( ( R (#) S ) (#) T ) = ( R (#) ( S (#) T ) )))))
proof
let X being non  empty set;
let Y being non  empty set;
let Z being non  empty set;
let W being non  empty set;
let R being (RMembership_Func of X,Y);
let S being (RMembership_Func of Y,Z);
let T being (RMembership_Func of Z,W);
A1: (for x being set holds (for w being set holds ((x in X & w in W) implies ( ( ( R (#) S ) (#) T ) . (x,w) ) = ( ( R (#) ( S (#) T ) ) . (x,w) ))))
proof
let x being set;
let w being set;
assume that
A2: x in X
and
A3: w in W;
reconsider w as (Element of W) by A3;
reconsider x as (Element of X) by A2;
set A = { ( "\/" ({ ( ( ( R . (x,y) ) "/\" ( S . (y,z) ) ) "/\" ( T . (z,w) ) ) where y is (Element of Y): (not contradiction) },( RealPoset [. ( 0 ),1 .] )) ) where z is (Element of Z): (not contradiction) };
set B = { ( ( ( R (#) S ) . (x,z) ) "/\" ( T . (z,w) ) ) where z is (Element of Z): (not contradiction) };
set C = { ( "\/" ({ ( ( ( R . (x,y) ) "/\" ( S . (y,z) ) ) "/\" ( T . (z,w) ) ) where z is (Element of Z): (not contradiction) },( RealPoset [. ( 0 ),1 .] )) ) where y is (Element of Y): (not contradiction) };
set D = { ( ( R . (x,y) ) "/\" ( ( S (#) T ) . (y,w) ) ) where y is (Element of Y): (not contradiction) };
defpred X[ set ]
 means
(not contradiction);
deffunc U((Element of Y),(Element of Z)) = ( ( ( R . (x,$1) ) "/\" ( S . ($1,$2) ) ) "/\" ( T . ($2,w) ) );
A4: (for a being set holds (a in A iff a in B))
proof
let a being set;
hereby
assume a in A;
then consider z being (Element of Z) such that A5: a = ( "\/" ({ ( ( ( R . (x,y) ) "/\" ( S . (y,z) ) ) "/\" ( T . (z,w) ) ) where y is (Element of Y): (not contradiction) },( RealPoset [. ( 0 ),1 .] )) );
a = ( ( ( R (#) S ) . (x,z) ) "/\" ( T . (z,w) ) ) by A5,CKB66:1;
hence a in B;
end;
assume a in B;
then consider z being (Element of Z) such that A6: a = ( ( ( R (#) S ) . (x,z) ) "/\" ( T . (z,w) ) );
a = ( "\/" ({ ( ( ( R . (x,y) ) "/\" ( S . (y,z) ) ) "/\" ( T . (z,w) ) ) where y is (Element of Y): (not contradiction) },( RealPoset [. ( 0 ),1 .] )) ) by A6,CKB66:1;
hence thesis;
end;
A7: (for y being (Element of Y) holds (for z being (Element of Z) holds ((X[ y ] & X[ z ]) implies U(y,z) = U(y,z))));
A8: ( "\/" ({ ( "\/" ({ U(y,z) where z is (Element of Z): X[ z ] },( RealPoset [. ( 0 ),1 .] )) ) where y is (Element of Y): X[ y ] },( RealPoset [. ( 0 ),1 .] )) ) = ( "\/" ({ ( "\/" ({ U(y1,z1) where y1 is (Element of Y): X[ y1 ] },( RealPoset [. ( 0 ),1 .] )) ) where z1 is (Element of Z): X[ z1 ] },( RealPoset [. ( 0 ),1 .] )) ) from CKB62:sch 1(A7);
(for c being set holds (c in C iff c in D))
proof
let c being set;
hereby
assume c in C;
then consider y being (Element of Y) such that A9: c = ( "\/" ({ ( ( ( R . (x,y) ) "/\" ( S . (y,z) ) ) "/\" ( T . (z,w) ) ) where z is (Element of Z): (not contradiction) },( RealPoset [. ( 0 ),1 .] )) );
c = ( ( R . (x,y) ) "/\" ( ( S (#) T ) . (y,w) ) ) by A9,CKB67:1;
hence c in D;
end;
assume c in D;
then consider y being (Element of Y) such that A10: c = ( ( R . (x,y) ) "/\" ( ( S (#) T ) . (y,w) ) );
c = ( "\/" ({ ( ( ( R . (x,y) ) "/\" ( S . (y,z) ) ) "/\" ( T . (z,w) ) ) where z is (Element of Z): (not contradiction) },( RealPoset [. ( 0 ),1 .] )) ) by A10,CKB67:1;
hence thesis;
end;
then A11: C = D by TARSKI:1;
(( ( ( R (#) S ) (#) T ) . (x,w) ) = ( "\/" ({ ( ( ( R (#) S ) . (x,z) ) "/\" ( T . (z,w) ) ) where z is (Element of Z): (not contradiction) },( RealPoset [. ( 0 ),1 .] )) ) & ( ( R (#) ( S (#) T ) ) . (x,w) ) = ( "\/" ({ ( ( R . (x,y) ) "/\" ( ( S (#) T ) . (y,w) ) ) where y is (Element of Y): (not contradiction) },( RealPoset [. ( 0 ),1 .] )) )) by CKB65:1;
hence thesis by A4,A11,A8,TARSKI:1;
end;
(( dom ( ( R (#) S ) (#) T ) ) = [: X,W :] & ( dom ( R (#) ( S (#) T ) ) ) = [: X,W :]) by FUNCT_2:def 1;
hence thesis by A1,FUNCT_3:6;
