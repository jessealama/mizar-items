definitions TARSKI,XBOOLE_0,RELAT_1,STRUCT_0,LATTICE3,YELLOW_0,WAYBEL_0,WAYBEL_1,BINOP_1,CKB1,CKB2,CKB9,CKB40,CKB46,CKB48;
theorems ZFMISC_1,ORDERS_2,WAYBEL_1,FUNCT_1,XBOOLE_1,LATTICE3,YELLOW_0,INTEGRA1,XREAL_0,XBOOLE_0,TARSKI,SEQ_4,JORDAN5A,FUNCT_2,YELLOW_2,WAYBEL_3,YELLOW_3,CARD_3,YELLOW_1,FUNCOP_1,FUZZY_2,FUZZY_4,FUNCT_3,FUZZY_1,PARTFUN1,MONOID_0,WAYBEL_0,XXREAL_0,XXREAL_1,XXREAL_2,RELSET_1,RELAT_1,MEASURE5,CKB1,CKB2,CKB5,CKB7,CKB9,CKB15,CKB23,CKB24,CKB25,CKB26,CKB28,CKB34,CKB35,CKB36,CKB37,CKB38,CKB40,CKB41,CKB42,CKB44,CKB45,CKB46,CKB47,CKB48,CKB52,CKB53,CKB54,CKB55,CKB56,CKB57;
