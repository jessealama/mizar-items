definitions TARSKI,STRUCT_0,CKB16,CKB17;
theorems MCART_1,VECTSP_1,BINOP_1,FUNCT_2,TARSKI,RLVECT_1,GROUP_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18;
