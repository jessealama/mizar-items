begin
reserve X for non  empty set;
theorem
Th10: (for R1,R2 being (Relation of X) holds (R1 c= R2 implies ( R1 [*] ) c= ( R2 [*] )))
proof
let R1 being (Relation of X);
let R2 being (Relation of X);
assume A1: R1 c= R2;
now
A2: ( field R1 ) c= ( field R2 ) by A1,RELAT_1:16;
let p being set;
assume that
A3: p in ( R1 [*] );
consider x,y being set such that A4: p = [ x,y ] by A3,RELAT_1:def 1;
consider r being FinSequence such that A5: ((( len r ) >= 1 & ( r . 1 ) = x) & ( r . ( len r ) ) = y) and A6: (for i being Nat holds ((i >= 1 & i < ( len r )) implies [ ( r . i ),( r . ( i + 1 ) ) ] in R1)) by A3,A4,FINSEQ_1:def 16;
A7:now
let i being Nat;
assume (i >= 1 & i < ( len r ));
then [ ( r . i ),( r . ( i + 1 ) ) ] in R1 by A6;
hence [ ( r . i ),( r . ( i + 1 ) ) ] in R2 by A1;
end;
(x in ( field R1 ) & y in ( field R1 )) by A3,A4,FINSEQ_1:def 16;
hence p in ( R2 [*] ) by A4,A5,A2,A7,FINSEQ_1:def 16;
end;
hence thesis by TARSKI:def 3;
end;
