begin
reserve A for non  empty set;
reserve X for non  empty set;
reserve f for (PartFunc of [: X,X :],( REAL ));
reserve a for  real number;
theorem
Th27: (for X being  finite non  empty (Subset of ( REAL )) holds (for f being (Function of [: X,X :],( REAL )) holds (for z being  finite non  empty (Subset of ( REAL )) holds (for A being  real number holds ((z = ( rng f ) & A >= ( max z )) implies (for R being (Equivalence_Relation of X) holds (R = ( ( low_toler (f,A) ) [*] ) implies ( Class R ) = { X })))))))
proof
let X being  finite non  empty (Subset of ( REAL ));
let f being (Function of [: X,X :],( REAL ));
let z being  finite non  empty (Subset of ( REAL ));
let A being  real number;
assume that
A1: (z = ( rng f ) & A >= ( max z ));
now
let R being (Equivalence_Relation of X);
assume that
A2: R = ( ( low_toler (f,A) ) [*] );
A3: (for x being set holds (x in X implies X = ( Class (R,x) )))
proof
let x being set;
assume x in X;
then reconsider x9 = x as (Element of X);
now
let x1 being set;
assume x1 in X;
then reconsider x19 = x1 as (Element of X);
( f . (x19,x9) ) <= A by A1,CKB32:1;
then A4: [ x1,x ] in ( low_toler (f,A) ) by CKB19:def 1;
( low_toler (f,A) ) c= ( ( low_toler (f,A) ) [*] ) by LANG1:18;
hence x1 in ( Class (R,x) ) by A2,A4,EQREL_1:19;
end;
then X c= ( Class (R,x) ) by TARSKI:def 3;
hence thesis by XBOOLE_0:def 10;
end;
now
let a being set;
assume a in { X };
then A5: a = X by TARSKI:def 1;
consider x being set such that A6: x in X by XBOOLE_0:def 1;
X = ( Class (R,x) ) by A3,A6;
hence a in ( Class R ) by A5,A6,EQREL_1:def 3;
end;
then A7: { X } c= ( Class R ) by TARSKI:def 3;
now
let a being set;
assume a in ( Class R );
then (ex x being set st (x in X & a = ( Class (R,x) ))) by EQREL_1:def 3;
then a = X by A3;
hence a in { X } by TARSKI:def 1;
end;
then ( Class R ) c= { X } by TARSKI:def 3;
hence ( Class R ) = { X } by A7,XBOOLE_0:def 10;
end;
hence thesis;
end;
