begin
reserve A for non  empty set;
reserve X for non  empty set;
reserve f for (PartFunc of [: X,X :],( REAL ));
theorem
Th31: (for X being non  empty set holds (for f being (PartFunc of [: X,X :],( REAL )) holds ( fam_class f ) is (Classification of X)))
proof
let X being non  empty set;
let f being (PartFunc of [: X,X :],( REAL ));
(for A,B being (a_partition of X) holds ((A in ( fam_class f ) & B in ( fam_class f )) implies (A is_finer_than B or B is_finer_than A)))
proof
let A being (a_partition of X);
let B being (a_partition of X);
assume that
A1: A in ( fam_class f )
and
A2: B in ( fam_class f );
consider a1 being non  negative  real number,R1 being (Equivalence_Relation of X) such that A3: R1 = ( ( low_toler (f,a1) ) [*] ) and A4: ( Class R1 ) = A by A1,CKB35:def 1;
consider a2 being non  negative  real number,R2 being (Equivalence_Relation of X) such that A5: R2 = ( ( low_toler (f,a2) ) [*] ) and A6: ( Class R2 ) = B by A2,CKB35:def 1;
now
per cases ;
suppose A7: a1 <= a2;

now
let x being set;
assume x in A;
then consider c being set such that A8: c in X and A9: x = ( Class (R1,c) ) by A4,EQREL_1:def 3;
consider y being set such that A10: y = ( Class (R2,c) );
take y;
thus y in B by A6,A8,A10,EQREL_1:def 3;
thus x c= y by A3,A5,A7,A9,A10,CKB28:1;
end;
hence thesis by SETFAM_1:def 2;
end;
suppose A11: a1 > a2;

now
let y being set;
assume y in B;
then consider c being set such that A12: c in X and A13: y = ( Class (R2,c) ) by A6,EQREL_1:def 3;
consider x being set such that A14: x = ( Class (R1,c) );
take x;
thus x in A by A4,A12,A14,EQREL_1:def 3;
thus y c= x by A3,A5,A11,A13,A14,CKB28:1;
end;
hence thesis by SETFAM_1:def 2;
end;
end;
hence thesis;
end;
hence thesis by CKB13:def 1;
end;
