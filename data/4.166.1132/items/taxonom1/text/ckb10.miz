begin
reserve X for non  empty set;
theorem
Th9: (for X being non  empty set holds (for R being (Relation of X) holds ((R is_reflexive_in X & R is_symmetric_in X) implies ( R [*] ) is (Equivalence_Relation of X))))
proof
let X being non  empty set;
let R being (Relation of X);
assume that
A1: R is_reflexive_in X
and
A2: R is_symmetric_in X;
( R [*] ) is_reflexive_in X by A1,CKB6:1;
then A3: (( dom ( R [*] ) ) = X & ( field ( R [*] ) ) = X) by ORDERS_1:13;
(( R [*] ) is_symmetric_in X & ( R [*] ) is_transitive_in X) by A1,A2,CKB8:1,CKB9:1;
hence thesis by A3,PARTFUN1:def 2,RELAT_2:def 11,RELAT_2:def 16;
end;
