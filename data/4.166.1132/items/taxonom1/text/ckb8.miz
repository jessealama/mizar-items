theorem
Th7: (for X being set holds (for R being (Relation of X) holds (R is_symmetric_in X implies ( R [*] ) is_symmetric_in X)))
proof
let X being set;
let R being (Relation of X);
assume that
A1: R is_symmetric_in X;
now
let x being set;
let y being set;
assume that
x in X
and
