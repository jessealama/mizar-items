begin
reserve a for  real number;
theorem
Th33: (for M being non  empty MetrStruct holds (for a being  real number holds ( dist_toler (M,a) ) = ( low_toler ((the distance of M),a) )))
proof
let M being non  empty MetrStruct;
let a being  real number;
now
let z being set;
assume that
A1: z in ( low_toler ((the distance of M),a) );
consider x,y being set such that A2: (x in (the carrier of M) & y in (the carrier of M)) and A3: z = [ x,y ] by A1,ZFMISC_1:def 2;
reconsider x1 = x,y1 = y as (Element of M) by A2;
( dist (x1,y1) ) = ( (the distance of M) . (x1,y1) ) by METRIC_1:def 1;
then ( dist (x1,y1) ) <= a by A1,A3,CKB19:def 1;
then x1,y1 are_in_tolerance_wrt a by CKB40:def 1;
hence z in ( dist_toler (M,a) ) by A3,CKB41:def 1;
end;
then A4: ( low_toler ((the distance of M),a) ) c= ( dist_toler (M,a) ) by TARSKI:def 3;
now
let z being set;
assume that
A5: z in ( dist_toler (M,a) );
consider x,y being set such that A6: (x in (the carrier of M) & y in (the carrier of M)) and A7: z = [ x,y ] by A5,ZFMISC_1:def 2;
reconsider x1 = x,y1 = y as (Element of M) by A6;
(( (the distance of M) . (x1,y1) ) = ( dist (x1,y1) ) & x1,y1 are_in_tolerance_wrt a) by A5,A7,CKB41:def 1,METRIC_1:def 1;
then ( (the distance of M) . (x1,y1) ) <= a by CKB40:def 1;
hence z in ( low_toler ((the distance of M),a) ) by A7,CKB19:def 1;
end;
then ( dist_toler (M,a) ) c= ( low_toler ((the distance of M),a) ) by TARSKI:def 3;
hence thesis by A4,XBOOLE_0:def 10;
end;
