begin
reserve X for non  empty set;
theorem
Th5: (for X being set holds (for R being (Relation of X) holds (R is_reflexive_in X implies ( R [*] ) is_reflexive_in X)))
proof
let X being set;
let R being (Relation of X);
assume that
A1: R is_reflexive_in X;
now
let x being set;
assume x in X;
then A2: [ x,x ] in R by A1,RELAT_2:def 1;
R c= ( R [*] ) by LANG1:18;
hence [ x,x ] in ( R [*] ) by A2;
end;
hence thesis by RELAT_2:def 1;
end;
