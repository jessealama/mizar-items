theorem
Th1: (for p being FinSequence holds (for k being Nat holds ((( k + 1 ) in ( dom p ) & (not k in ( dom p ))) implies k = ( 0 ))))
proof
let p being FinSequence;
let k being Nat;
assume that
A1: ( k + 1 ) in ( dom p )
and
A2: (not k in ( dom p ));
assume k <> ( 0 );
then A3: k >= 1 by NAT_1:14;
(k <= ( k + 1 ) & ( k + 1 ) <= ( len p )) by A1,FINSEQ_3:25,NAT_1:12;
