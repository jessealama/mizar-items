Th30: (for X being non  empty  compact TopSpace holds (for Y being (Subset of ( C_Normed_Algebra_of_BoundedFunctions (the carrier of X) )) holds (Y = ( CContinuousFunctions X ) implies Y is  closed)))
proof
let X being non  empty  compact TopSpace;
let Y being (Subset of ( C_Normed_Algebra_of_BoundedFunctions (the carrier of X) ));
assume A1: Y = ( CContinuousFunctions X );
now
let seq being (sequence of ( C_Normed_Algebra_of_BoundedFunctions (the carrier of X) ));
