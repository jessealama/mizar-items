environ
vocabularies STRUCT_0,XREAL_0,ORDINAL1,FUNCOP_1,NUMBERS,PRE_TOPC,ORDINAL2,SUBSET_1,RELAT_1,XBOOLE_0,FUNCT_1,TARSKI,CARD_1,ARYTM_3,ARYTM_1,COMPLEX1,ALGSTR_0,FUNCSDOM,REAL_1,FUNCT_2,C0SP1,IDEAL_1,VALUED_1,MESFUNC1,RSSPACE,BINOP_1,SUPINF_2,GROUP_1,REALSET1,ZFMISC_1,XXREAL_2,XXREAL_0,NORMSP_1,CFUNCDOM,CLVECT_1,VECTSP_1,RELAT_2,CFUNCT_1,CLOPBAN2,METRIC_1,XCMPLX_0,RCOMP_1,CONNSP_2,CC0SP1,CC0SP2,RLVECT_1,XXREAL_1,NAT_1,CLOPBAN1,SEQ_2,RSSPACE3,LOPBAN_2,SEQFUNC,REWRITE1,CSSPACE4,PARTFUN1,PRE_POLY,RLSUB_1,SEQ_4;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,RELAT_1,FUNCT_1,ORDINAL1,RELSET_1,PARTFUN1,FUNCT_2,BINOP_1,DOMAIN_1,FUNCOP_1,NUMBERS,XCMPLX_0,XXREAL_0,XREAL_0,REAL_1,COMPLEX1,XXREAL_2,REALSET1,VALUED_1,SEQ_2,SEQ_4,RCOMP_1,CFUNCT_1,CFDIFF_1,SEQFUNC,PRE_POLY,STRUCT_0,ALGSTR_0,PRE_TOPC,RLVECT_1,GROUP_1,VECTSP_1,COMPTS_1,NORMSP_0,CONNSP_2,PSCOMP_1,IDEAL_1,CLVECT_1,CSSPACE,CSSPACE3,CLOPBAN1,CFUNCDOM,CLOPBAN2,NCFCONT1,C0SP1,CC0SP1,CKB1,CKB2;
definitions REALSET1,ALGSTR_0,TARSKI,XCMPLX_0,STRUCT_0,GROUP_1,BINOP_1,VALUED_1,CFUNCDOM,CLVECT_1,COMPLEX1,NORMSP_0,SUBSET_1,CC0SP1,CKB1,CKB2;
theorems FUNCT_1,SEQ_2,COMPLEX1,ZFMISC_1,TARSKI,IDEAL_1,RELAT_1,XREAL_0,XXREAL_0,FUNCT_2,XBOOLE_0,XREAL_1,XCMPLX_0,FUNCOP_1,VECTSP_1,VALUED_1,C0SP1,CFUNCDOM,CLVECT_1,CFUNCT_1,NORMSP_0,XBOOLE_1,XCMPLX_1,ABSVALUE,CFDIFF_1,PRE_TOPC,TOPS_1,CONNSP_2,CC0SP1,RLVECT_1,RCOMP_1,C0SP2,CLOPBAN1,NCFCONT1,CSSPACE3,NORMSP_1,PARTFUN1,CLOPBAN2,PRE_POLY,COMPTS_1,CSSPACE,RFUNCT_1,JORDAN_A,TOPREAL6,MEMBERED,CKB1,CKB2,CKB3,CKB6,CKB7,CKB8;
registrations XBOOLE_0,SUBSET_1,STRUCT_0,XREAL_0,REALSET1,NUMBERS,MEMBERED,RELAT_1,VECTSP_1,VECTSP_2,VALUED_0,XXREAL_0,FUNCT_2,VALUED_1,COMPLEX1,CFUNCDOM,XCMPLX_0,PRE_TOPC,CC0SP1,PSCOMP_1,ORDINAL1,NORMSP_0,CLOPBAN2,COMPTS_1,TOPREAL6,RELSET_1,CKB4,CKB5;
constructors REAL_1,REALSET1,IDEAL_1,SEQ_1,C0SP1,PARTFUN3,BINOP_2,SEQ_4,MEASURE6,CSSPACE3,COMSEQ_3,CFDIFF_1,CC0SP1,WEIERSTR,PSCOMP_1,NCFCONT1,SEQFUNC,INTEGRA2,PRE_POLY,C0SP2,RLSUB_1,RSSPACE,TOPREAL6,CKB1,CKB2;
requirements NUMERALS,BOOLE,SUBSET,ARITHM,REAL;
begin
theorem
Th5: (for X being non  empty TopSpace holds (for a being  complex number holds (for f being  continuous (Function of (the carrier of X),( COMPLEX )) holds ( a (#) f ) is  continuous  continuous (Function of (the carrier of X),( COMPLEX )))))
proof
let X being non  empty TopSpace;
let a being  complex number;
let f being  continuous (Function of (the carrier of X),( COMPLEX ));
set h = ( a (#) f );
A1: (for x being (Point of X) holds ( h . x ) = ( a * ( f . x ) )) by VALUED_1:6;
now
per cases ;
suppose L1: a <> ( 0 );

(for p being (Point of X) holds (for V being (Subset of ( COMPLEX )) holds ((( h . p ) in V & V is  open) implies (ex W being (Subset of X) st ((p in W & W is  open) & ( h .: W ) c= V)))))
proof
let p being (Point of X);
let V being (Subset of ( COMPLEX ));
assume A2: (( h . p ) in V & V is  open);
reconsider z0 = ( h . p ) as Complex;
consider N being (Neighbourhood of z0) such that A3: N c= V by A2,CFDIFF_1:9;
consider r being Real such that A4: (( 0 ) < r & { y where y is Complex: |. ( y - z0 ) .| < r } c= N) by CFDIFF_1:def 5;
set S = { y where y is Complex: |. ( y - z0 ) .| < r };
A5: |. a .| > ( 0 ) by L1;
A6: ( r / |. a .| ) is Real by XREAL_0:def 1;
A7: ( r / |. a .| ) > ( 0 ) by L1,A4;
reconsider z1 = ( f . p ) as Complex;
set S1 = { y where y is Complex: |. ( y - z1 ) .| < ( r / |. a .| ) };
S1 c= ( COMPLEX )
proof
let z being set;
assume z in S1;
then (ex y being Complex st (z = y & |. ( y - z1 ) .| < ( r / |. a .| )));
hence z in ( COMPLEX );
end;
then reconsider T1 = S1 as (Subset of ( COMPLEX ));
A8: T1 is  open by CFDIFF_1:13,A6;
|. ( z1 - z1 ) .| = ( 0 );
then z1 in S1 by A7;
then consider W1 being (Subset of X) such that A9: ((p in W1 & W1 is  open) & ( f .: W1 ) c= S1) by A8,CKB7:1;
set W = W1;
A10: W is  open by A9;
A11: p in W by A9;
( h .: W ) c= S
proof
let z3 being set;
assume z3 in ( h .: W );
then consider x3 being set such that A12: ((x3 in ( dom h ) & x3 in W) & ( h . x3 ) = z3) by FUNCT_1:def 6;
reconsider px = x3 as (Point of X) by A12;
px in (the carrier of X);
then px in ( dom f ) by FUNCT_2:def 1;
then ( f . px ) in ( f .: W1 ) by A12,FUNCT_1:def 6;
then A14: ( f . px ) in S1 by A9;
reconsider a1 = ( f . px ) as Complex;
(ex aa1 being Complex st (( f . px ) = aa1 & |. ( aa1 - z1 ) .| < ( r / |. a .| ))) by A14;
then A15: |. ( a1 - z1 ) .| < ( r / |. a .| );
A16: |. ( ( h . x3 ) - z0 ) .| = |. ( ( a * ( f . px ) ) - z0 ) .| by A1
.= |. ( ( a * ( f . px ) ) - ( a * ( f . p ) ) ) .| by A1
.= |. ( a * ( ( f . px ) - ( f . p ) ) ) .|
.= ( |. a .| * |. ( a1 - z1 ) .| ) by COMPLEX1:65;
A17: ( |. a .| * |. ( a1 - z1 ) .| ) < ( |. a .| * ( r / |. a .| ) ) by A5,A15,XREAL_1:68;
( |. a .| * ( r / |. a .| ) ) = ( r * ( |. a .| / |. a .| ) )
.= ( r * 1 ) by XCMPLX_1:60,A5
.= r;
then |. ( ( h . px ) - z0 ) .| < r by A16,A17;
hence z3 in S by A12;
end;
then ( h .: W ) c= N by A4,XBOOLE_1:1;
hence (ex W being (Subset of X) st ((p in W & W is  open) & ( h .: W ) c= V)) by A10,A11,A3,XBOOLE_1:1;
end;
hence ( a (#) f ) is  continuous by CKB7:1;
end;
suppose L2: a = ( 0 );

set g = ( X --> ( 0c ) );
set CX = (the carrier of X);
A18: ( dom g ) = CX by FUNCOP_1:13;
( dom h ) = CX by FUNCT_2:def 1;
then A19: ( dom g ) = ( dom h ) by A18;
(for z being set holds (z in ( dom h ) implies ( g . z ) = ( h . z )))
proof
let z being set;
assume A20: z in ( dom h );
( h . z ) = ( ( 0 ) * ( f . z ) ) by L2,VALUED_1:6
.= ( 0 );
hence thesis by A20,FUNCOP_1:7;
end;
hence ( a (#) f ) is  continuous by A19,FUNCT_1:def 11;
end;
end;
hence thesis;
end;
