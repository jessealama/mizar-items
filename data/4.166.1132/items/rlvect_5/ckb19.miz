constructors PARTFUN1,XXREAL_0,REAL_1,NAT_1,FINSEQ_3,FINSEQ_4,REALSET1,RFINSEQ,RLSUB_2,RLVECT_2,CLASSES1,RLVECT_3,RELSET_1;
requirements REAL,NUMERALS,BOOLE,SUBSET,ARITHM;
begin
reserve V for RealLinearSpace;
theorem
Th19: (for I being (Basis of V) holds (for A being non empty (Subset of V) holds (A misses I implies (for B being (Subset of V) holds (B = ( I \/ A ) implies B is  linearly-dependent)))))
proof
let I being (Basis of V);
let A being non empty (Subset of V);
assume that
A1: A misses I;
consider v being set such that A2: v in A by XBOOLE_0:def 1;
let B being (Subset of V);
assume that
A3: B = ( I \/ A );
A4: A c= B by A3,XBOOLE_1:7;
reconsider v as (VECTOR of V) by A2;
reconsider Bv = ( B \ { v } ) as (Subset of V);
A5: ( I \ { v } ) c= ( B \ { v } ) by A3,XBOOLE_1:7,XBOOLE_1:33;
(not v in I) by A1,A2,XBOOLE_0:3;
then I c= Bv by A5,ZFMISC_1:57;
then A6: ( Lin I ) is (Subspace of ( Lin Bv )) by RLVECT_3:20;
assume A7: B is  linearly-independent;
v in ( Lin I ) by CKB14:1;
hence contradiction by A7,A2,A4,A6,CKB18:1,RLSUB_1:8;
end;
