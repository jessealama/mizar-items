begin
theorem
Th13: (for L being Lattice holds (for L9 being (SubLattice of L) holds (for X being (Subset of L9) holds (for a being (Element of L) holds (for a9 being (Element of L9) holds (a = a9 implies (a is_less_than X iff a9 is_less_than X)))))))
proof
let L being Lattice;
let L9 being (SubLattice of L);
let X being (Subset of L9);
let a being (Element of L);
let a9 being (Element of L9);
assume A1: a = a9;
thus (a is_less_than X implies a9 is_less_than X)
proof
assume A2: a is_less_than X;
now
let q9 being (Element of L9);
(the carrier of L9) c= (the carrier of L) by NAT_LAT:def 12;
then reconsider q = q9 as (Element of L) by TARSKI:def 3;
assume q9 in X;
then A3: a [= q by A2,LATTICE3:def 16;
( a9 "/\" q9 ) = ( a "/\" q ) by A1,CKB16:1
.= a9 by A1,A3,LATTICES:4;
hence a9 [= q9 by LATTICES:4;
end;
hence thesis by LATTICE3:def 16;
end;

thus (a9 is_less_than X implies a is_less_than X)
proof
assume A4: a9 is_less_than X;
now
let q being (Element of L);
assume A5: q in X;
then reconsider q9 = q as (Element of L9);
A6: a9 [= q9 by A4,A5,LATTICE3:def 16;
( a "/\" q ) = ( a9 "/\" q9 ) by A1,CKB16:1
.= a by A1,A6,LATTICES:4;
hence a [= q by LATTICES:4;
end;
hence thesis by LATTICE3:def 16;
end;

end;
