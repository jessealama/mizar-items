begin
theorem
Th16: (for L being  complete Lattice holds (for L9 being (SubLattice of L) holds (L9 is  \/-inheriting implies L9 is  complete)))
proof
let L being  complete Lattice;
let L9 being (SubLattice of L);
assume that
A1: L9 is  \/-inheriting;
(for X being (Subset of L9) holds (ex a being (Element of L9) st (X is_less_than a & (for b being (Element of L9) holds (X is_less_than b implies a [= b)))))
proof
let X being (Subset of L9);
set a = ( "\/" (X,L) );
reconsider a9 = a as (Element of L9) by A1,CKB15:def 1;
take a9;
X is_less_than a by LATTICE3:def 21;
hence X is_less_than a9 by CKB18:1;
let b9 being (Element of L9);
(the carrier of L9) c= (the carrier of L) by NAT_LAT:def 12;
then reconsider b = b9 as (Element of L) by TARSKI:def 3;
assume X is_less_than b9;
then X is_less_than b by CKB18:1;
then A2: a [= b by LATTICE3:def 21;
( a9 "/\" b9 ) = ( a "/\" b ) by CKB16:1
.= a9 by A2,LATTICES:4;
hence thesis by LATTICES:4;
end;
hence thesis by CKB10:def 1;
end;
