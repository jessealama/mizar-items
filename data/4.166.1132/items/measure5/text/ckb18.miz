environ
vocabularies NUMBERS,SUPINF_1,SUBSET_1,TARSKI,XREAL_0,ORDINAL1,XXREAL_1,ARYTM_3,XXREAL_0,CARD_1,RELAT_2,XXREAL_2,ORDINAL2,REAL_1,SUPINF_2,MEMBERED,ARYTM_1,XBOOLE_0,MEASURE5;
notations TARSKI,XBOOLE_0,SUBSET_1,ORDINAL1,NUMBERS,MEMBERED,XXREAL_0,XREAL_0,REAL_1,XXREAL_1,RCOMP_1,XXREAL_2,SUPINF_1,SUPINF_2,CKB2,CKB3,CKB4,CKB7,CKB8,CKB9,CKB10,CKB13;
definitions TARSKI,XXREAL_1,MEMBERED,XBOOLE_0,SUPINF_2,XXREAL_3,CKB2,CKB3,CKB4,CKB7,CKB9;
theorems TARSKI,XREAL_0,XBOOLE_0,XREAL_1,XXREAL_0,NUMBERS,XXREAL_1,XXREAL_2,XXREAL_3,CKB2,CKB3,CKB4,CKB7,CKB9;
schemes CKB1;
registrations XBOOLE_0,NUMBERS,XXREAL_0,XREAL_0,MEMBERED,XXREAL_1,XXREAL_2,SUBSET_1,FINSET_1,CKB5,CKB6,CKB11,CKB12,CKB14,CKB15,CKB16,CKB17;
constructors DOMAIN_1,REAL_1,RCOMP_1,SUPINF_2,SUPINF_1,FINSET_1,CKB2,CKB3,CKB4,CKB7,CKB9;
requirements SUBSET,BOOLE,NUMERALS,ARITHM,REAL;
begin
reserve a for R_eal;
reserve b for R_eal;
theorem
(for I being  interval (Subset of ( REAL )) holds (((I is  open_interval or I is  closed_interval) or I is  right_open_interval) or I is  left_open_interval))
proof
let I being  interval (Subset of ( REAL ));
per cases ;
suppose A1: I is  left_end  right_end;

reconsider a = ( inf I ),b = ( sup I ) as R_eal;
A2: (a in I & I = [. a,b .]) by A1,XXREAL_2:75,XXREAL_2:def 5;
thus thesis by A1,A2,CKB4:def 1;
end;
suppose A3: I is non  left_end  right_end;

set a = ( inf I );
set b = ( sup I );
A4: I = ]. a,b .] by A3,XXREAL_2:76;
A5: b in I by A3,XXREAL_2:def 6;
thus thesis by A5,A4,CKB9:def 1;
end;
suppose A6: I is  left_end non  right_end;

set a = ( inf I );
set b = ( sup I );
A7: I = [. a,b .[ by A6,XXREAL_2:77;
A8: a in I by A6,XXREAL_2:def 5;
thus thesis by A8,A7,CKB7:def 1;
end;
suppose I is non  left_end non  right_end;

then consider a,b being  ext-real number such that A9: a <= b and A10: I = ]. a,b .[ by XXREAL_2:79;
reconsider a,b as R_eal by XXREAL_0:def 1;
a <= b by A9;
hence thesis by A10,CKB3:def 1;
end;
end;
