environ
vocabularies XBOOLE_0,TARSKI,ZFMISC_1;
notations TARSKI,XBOOLE_0,ENUMSET1,CKB5,CKB6,CKB7,CKB8;
definitions TARSKI,XBOOLE_0,CKB5,CKB6,CKB7,CKB8;
theorems TARSKI,XBOOLE_0,ENUMSET1,XBOOLE_1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15;
schemes XBOOLE_0;
registrations XBOOLE_0,CKB1;
constructors TARSKI,XBOOLE_0,ENUMSET1,CKB5,CKB6,CKB7,CKB8;
requirements BOOLE;
begin
reserve x for set;
reserve X for set;
theorem
Lm4: (( { x } \/ X ) c= X implies x in X)
proof
assume A1: ( { x } \/ X ) c= X;
assume (not x in X);
then (not x in ( { x } \/ X )) by A1,TARSKI:def 3;
then (not x in { x }) by XBOOLE_0:def 3;
hence thesis by TARSKI:def 1;
end;
