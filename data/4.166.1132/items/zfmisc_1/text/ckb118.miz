begin
reserve x for set;
reserve y for set;
reserve z for set;
reserve X for set;
reserve Y for set;
reserve Z for set;
theorem
(X c= [: X,X :] implies X = ( {} ))
proof
assume A1: X c= [: X,X :];
assume X <> ( {} );
then consider z being set such that A2: z in X by XBOOLE_0:7;
z in ( X \/ ( union X ) ) by A2,XBOOLE_0:def 3;
then consider Y such that A3: Y in ( X \/ ( union X ) ) and A4: (not (ex x st (x in ( X \/ ( union X ) ) & x in Y))) by TARSKI:2;
now
assume A5: Y in X;
then Y in [: X,X :] by A1,TARSKI:def 3;
then consider x,y such that A6: Y = [ x,y ] by CKB117:1;
A7: { x } in Y by A6,TARSKI:def 2;
Y c= ( union X ) by A5,CKB44:1;
then { x } in ( union X ) by A7,TARSKI:def 3;
then { x } in ( X \/ ( union X ) ) by XBOOLE_0:def 3;
hence contradiction by A4,A7;
end;
then Y in ( union X ) by A3,XBOOLE_0:def 3;
then consider Z such that A8: Y in Z and A9: Z in X by TARSKI:def 4;
Z in [: X,X :] by A1,A9,TARSKI:def 3;
then consider x,y such that A10: x in X and y in X and A11: Z = [ x,y ] by CKB6:def 1;
(Y = { x } or Y = { x,y }) by A8,A11,TARSKI:def 2;
then A12: x in Y by TARSKI:def 1,TARSKI:def 2;
x in ( X \/ ( union X ) ) by A10,XBOOLE_0:def 3;
hence contradiction by A4,A12;
end;
