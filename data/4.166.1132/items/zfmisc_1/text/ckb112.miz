theorem
Lm18: (((A c= [: X1,Y1 :] & B c= [: X2,Y2 :]) & (for x holds (for y holds ([ x,y ] in A iff [ x,y ] in B)))) implies A = B)
proof
assume that
A1: A c= [: X1,Y1 :]
and
A2: B c= [: X2,Y2 :]
and
A3: (for x holds (for y holds ([ x,y ] in A iff [ x,y ] in B)));
now
let z;
A4: (z in B implies (ex x st (ex y st ((x in X2 & y in Y2) & z = [ x,y ])))) by A2,CKB106:1;
(z in A implies (ex x st (ex y st ((x in X1 & y in Y1) & z = [ x,y ])))) by A1,CKB106:1;
hence (z in A iff z in B) by A3,A4;
end;
hence thesis by TARSKI:1;
end;
