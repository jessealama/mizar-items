reserve X for set;
reserve Y for set;
theorem
((for X holds (for Y holds (((X <> Y & X in ( A \/ B )) & Y in ( A \/ B )) implies X misses Y))) implies ( union ( A /\ B ) ) = ( ( union A ) /\ ( union B ) ))
proof
assume A1: (for X holds (for Y holds (((X <> Y & X in ( A \/ B )) & Y in ( A \/ B )) implies X misses Y)));
A2: ( ( union A ) /\ ( union B ) ) c= ( union ( A /\ B ) )
proof
let x;
assume A3: x in ( ( union A ) /\ ( union B ) );
then x in ( union A ) by XBOOLE_0:def 4;
then consider X such that A4: x in X and A5: X in A by TARSKI:def 4;
x in ( union B ) by A3,XBOOLE_0:def 4;
then consider Y such that A6: x in Y and A7: Y in B by TARSKI:def 4;
now
x in ( X /\ Y ) by A4,A6,XBOOLE_0:def 4;
then A8: X meets Y by XBOOLE_0:4;
assume A9: X <> Y;
(X in ( A \/ B ) & Y in ( A \/ B )) by A5,A7,XBOOLE_0:def 3;
hence contradiction by A1,A9,A8;
end;
then Y in ( A /\ B ) by A5,A7,XBOOLE_0:def 4;
hence thesis by A6,TARSKI:def 4;
end;
( union ( A /\ B ) ) c= ( ( union A ) /\ ( union B ) ) by CKB101:1;
hence thesis by A2,XBOOLE_0:def 10;
end;
