environ
vocabularies XBOOLE_0,TARSKI,ZFMISC_1;
notations TARSKI,XBOOLE_0,ENUMSET1;
definitions TARSKI,XBOOLE_0;
theorems TARSKI,XBOOLE_0,ENUMSET1,XBOOLE_1,CKB2;
schemes XBOOLE_0;
registrations XBOOLE_0,CKB1;
constructors TARSKI,XBOOLE_0,ENUMSET1;
requirements BOOLE;
begin
reserve x for set;
reserve y for set;
reserve X for set;
reserve Y for set;
theorem
Lm2: ((Y c= X & (not x in Y)) implies Y c= ( X \ { x } ))
proof
assume A1: (Y c= X & (not x in Y));
let y;
assume y in Y;
then (y in X & (not y in { x })) by A1,TARSKI:def 1,TARSKI:def 3;
hence thesis by XBOOLE_0:def 5;
end;
