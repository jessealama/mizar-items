A12: (([ x,y ] in [: X,Z :] & [ x,y ] in [: Y,Z :]) iff ([ y,x ] in [: Z,X :] & [ y,x ] in [: Z,Y :])) by CKB110:1;
([ y,x ] in [: Z,( X /\ Y ) :] iff [ x,y ] in [: ( X /\ Y ),Z :]) by CKB110:1;
hence thesis by A10,A12,XBOOLE_0:def 4;
end;
( [: Z,X :] /\ [: Z,Y :] ) c= [: Z,X :] by XBOOLE_1:17;
hence thesis by A11,CKB112:1;
