theorem
([: ( X /\ Y ),Z :] = ( [: X,Z :] /\ [: Y,Z :] ) & [: Z,( X /\ Y ) :] = ( [: Z,X :] /\ [: Z,Y :] ))
proof
A1: (for x holds (for y holds ([ x,y ] in [: ( X /\ Y ),Z :] iff [ x,y ] in ( [: X,Z :] /\ [: Y,Z :] ))))
proof
let x;
let y;
thus ([ x,y ] in [: ( X /\ Y ),Z :] implies [ x,y ] in ( [: X,Z :] /\ [: Y,Z :] ))
proof
assume A2: [ x,y ] in [: ( X /\ Y ),Z :];
then A3: x in ( X /\ Y ) by CKB49:1;
A4: y in Z by A2,CKB49:1;
x in Y by A3,XBOOLE_0:def 4;
