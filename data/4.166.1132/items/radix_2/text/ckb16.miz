registrations ORDINAL1,RELSET_1,NUMBERS,XXREAL_0,XREAL_0,NAT_1,INT_1,MEMBERED,NEWTON,XBOOLE_0,VALUED_0,FINSEQ_2,CARD_1,FINSEQ_1;
constructors REAL_1,NAT_D,NEWTON,POWER,BINARITH,WSIERP_1,GR_CY_1,RADIX_1,SEQ_1,RECDEF_1,CKB12,CKB13,CKB14,CKB15;
requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve k for Nat;
definition
let k being Nat;
let n being Nat;
let x being Nat;
func DecSD2 (x,n,k) -> (Tuple of n,( NAT )) means 
:Def5: (for i being Nat holds (i in ( Seg n ) implies ( it . i ) = ( DigitDC2 (x,i,k) )));
existence
proof
deffunc F(Nat) = ( DigitDC2 (x,$1,k) );
consider z being (FinSequence of ( NAT )) such that A1: ( len z ) = n and A2: (for j being Nat holds (j in ( dom z ) implies ( z . j ) = F(j))) from FINSEQ_2:sch 1;
A3: ( dom z ) = ( Seg n ) by A1,FINSEQ_1:def 3;
reconsider z as (Tuple of n,( NAT )) by A1,CARD_1:def 7;
take z;
let i being Nat;
assume i in ( Seg n );
hence thesis by A2,A3;
end;
uniqueness
proof
let k1 being (Tuple of n,( NAT ));
let k2 being (Tuple of n,( NAT ));
assume that
A4: (for i being Nat holds (i in ( Seg n ) implies ( k1 . i ) = ( DigitDC2 (x,i,k) )))
and
A5: (for i being Nat holds (i in ( Seg n ) implies ( k2 . i ) = ( DigitDC2 (x,i,k) )));
A6: ( len k1 ) = n by CARD_1:def 7;
then A7: ( dom k1 ) = ( Seg n ) by FINSEQ_1:def 3;
A8:now
let j being Nat;
assume A9: j in ( dom k1 );
then ( k1 . j ) = ( DigitDC2 (x,j,k) ) by A4,A7;
hence ( k1 . j ) = ( k2 . j ) by A5,A7,A9;
end;
( len k2 ) = n by CARD_1:def 7;
hence thesis by A6,A8,FINSEQ_2:9;
end;
end;
