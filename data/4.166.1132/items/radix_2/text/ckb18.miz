theorem
Th13: (for n,k being Nat holds (for x being (Tuple of n,( NAT )) holds (for y being (Tuple of n,( k -SD )) holds (x = y implies ( SDDec2 (x,k) ) = ( SDDec y )))))
proof
let n being Nat,k being Nat;
let x being (Tuple of n,( NAT ));
let y being (Tuple of n,( k -SD ));
assume x = y;
then ( SDDec2 (x,k) ) = ( Sum ( DigitSD y ) ) by CKB17:1;
hence thesis by RADIX_1:def 7;
end;
