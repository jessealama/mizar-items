theorem
Th34: (Q is  closed implies (for P being (Subset of A) holds (P = Q implies P is  closed)))
proof
assume A1: Q is  closed;
let P being (Subset of A);
assume P = Q;
then ( Q /\ ( [#] A ) ) = P by XBOOLE_1:28;
hence thesis by A1,PRE_TOPC:13;
end;
