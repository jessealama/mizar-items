begin
theorem
Th58: (for T,V being TopStruct holds (for S being non empty TopStruct holds (for f being (Function of T,S) holds (for g being (Function of S,V) holds ((f is  continuous & g is  continuous) implies ( g * f ) is  continuous)))))
proof
let T being TopStruct,V being TopStruct;
let S being non empty TopStruct;
let f being (Function of T,S);
let g being (Function of S,V);
assume that
A1: f is  continuous
and
A2: g is  continuous;
let P being (Subset of V);
assume P is  closed;
then (( ( g * f ) " P ) = ( f " ( g " P ) ) & ( g " P ) is  closed) by A2,PRE_TOPC:def 6,RELAT_1:146;
hence thesis by A1,PRE_TOPC:def 6;
end;
