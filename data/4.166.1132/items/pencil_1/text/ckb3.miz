begin
theorem
Th3: (for X being set holds (2 c= ( card X ) implies (for x being set holds (ex y being set st (y in X & x <> y)))))
proof
let X being set;
assume 2 c= ( card X );
then consider a,b being set such that A1: a in X and A2: (b in X & a <> b) by CKB2:1;
let x being set;
per cases ;
suppose A3: x = a;

take b;
thus thesis by A2,A3;
end;
suppose A4: x <> a;

take a;
thus thesis by A1,A4;
end;
end;
