begin
theorem
(for I being non empty set holds (for A being (ManySortedSet of I) holds (for i,S being set holds ( { A } +* (i,S) ) is  Segre-like)))
proof
let I being non empty set;
let A being (ManySortedSet of I);
let i being set,S being set;
per cases ;
suppose (not i in I);

then (not i in ( dom { A } )) by PARTFUN1:def 2;
hence thesis by FUNCT_7:def 3;
end;
suppose i in I;

then reconsider i as (Element of I);
take i;
let j being (Element of I);
assume that
A1: i <> j;
( { A } . j ) = { ( A . j ) } by PZFMISC1:def 1;
hence thesis by A1,FUNCT_7:32;
end;
end;
