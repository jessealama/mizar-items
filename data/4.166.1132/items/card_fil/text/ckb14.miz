begin
reserve X for non empty set;
reserve Y1 for (Subset of X);
reserve Y2 for (Subset of X);
definition
let X;
mode Ideal of X
 -> non empty (Subset-Family of X)
means :Def2: ((not X in it) & (for Y1 holds (for Y2 holds (((Y1 in it & Y2 in it) implies ( Y1 \/ Y2 ) in it) & ((Y1 in it & Y2 c= Y1) implies Y2 in it)))));
existence
proof
set F = the (Filter of X);
take ( dual F );
(for Y1 holds (Y1 in ( dual F ) iff ( Y1 ` ) in F)) by SETFAM_1:def 7;
hence thesis by CKB9:1;
end;
end;
