begin
theorem
(for U0 being  with_const_op Universal_Algebra holds (for l1,l2 being (Element of ( UnSubAlLattice U0 )) holds (for U1,U2 being  strict (SubAlgebra of U0) holds ((l1 = U1 & l2 = U2) implies (l1 [= l2 iff U1 is (SubAlgebra of U2))))))
proof
let U0 being  with_const_op Universal_Algebra;
let l1 being (Element of ( UnSubAlLattice U0 )),l2 being (Element of ( UnSubAlLattice U0 ));
let U1 being  strict (SubAlgebra of U0),U2 being  strict (SubAlgebra of U0);
assume that
A1: (l1 = U1 & l2 = U2);
thus (l1 [= l2 implies U1 is (SubAlgebra of U2))
proof
assume l1 [= l2;
then (the carrier of U1) c= (the carrier of U2) by A1,CKB24:1;
hence thesis by UNIALG_2:11;
end;

thus (U1 is (SubAlgebra of U2) implies l1 [= l2)
proof
assume U1 is (SubAlgebra of U2);
then (the carrier of U1) is (Subset of U2) by UNIALG_2:def 7;
hence thesis by A1,CKB24:1;
end;

end;
