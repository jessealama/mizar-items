begin
reserve CNS for ComplexNormSpace;
theorem
Th3: (for x0 being (Point of CNS) holds (for g being Real holds (( 0 ) < g implies { y where y is (Point of CNS): ||. ( y - x0 ) .|| < g } is (Neighbourhood of x0))))
proof
let x0 being (Point of CNS);
let g being Real;
assume that
A1: g > ( 0 );
set N = { y where y is (Point of CNS): ||. ( y - x0 ) .|| < g };
N c= (the carrier of CNS)
proof
let x being set;
assume x in { y where y is (Point of CNS): ||. ( y - x0 ) .|| < g };
then (ex y being (Point of CNS) st (x = y & ||. ( y - x0 ) .|| < g));
hence thesis;
end;
hence thesis by A1,CKB2:def 1;
end;
