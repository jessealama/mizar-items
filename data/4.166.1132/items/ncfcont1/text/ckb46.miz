begin
reserve r for Real;
reserve CNS for ComplexNormSpace;
reserve RNS for RealNormSpace;
theorem
Th57: (for f being (PartFunc of CNS,RNS) holds (for x0 being (Point of CNS) holds (for r being Real holds (f is_continuous_in x0 implies ( r (#) f ) is_continuous_in x0))))
proof
let f being (PartFunc of CNS,RNS);
let x0 being (Point of CNS);
let r being Real;
assume A1: f is_continuous_in x0;
then x0 in ( dom f ) by CKB9:def 1;
hence A2: x0 in ( dom ( r (#) f ) ) by VFUNCT_1:def 4;
let s1 being (sequence of CNS);
assume that
A3: ( rng s1 ) c= ( dom ( r (#) f ) )
and
A4: (s1 is  convergent & ( lim s1 ) = x0);
A5: ( rng s1 ) c= ( dom f ) by A3,VFUNCT_1:def 4;
then A6: ( f /. x0 ) = ( lim ( f /* s1 ) ) by A1,A4,CKB9:def 1;
A7: ( f /* s1 ) is  convergent by A1,A4,A5,CKB9:def 1;
then ( r * ( f /* s1 ) ) is  convergent by NORMSP_1:22;
hence ( ( r (#) f ) /* s1 ) is  convergent by A5,CKB37:1;
thus ( ( r (#) f ) /. x0 ) = ( r * ( f /. x0 ) ) by A2,VFUNCT_1:def 4
.= ( lim ( r * ( f /* s1 ) ) ) by A7,A6,NORMSP_1:28
.= ( lim ( ( r (#) f ) /* s1 ) ) by A5,CKB37:1;
end;
