begin
reserve k for Nat;
reserve n for Nat;
scheme CompInd { P[Nat] } : (for k being Nat holds P[ k ])
provided
A1: (for k being Nat holds ((for n being Nat holds (n < k implies P[ n ])) implies P[ k ]))
proof
let k being Nat;
defpred P1[ Nat ]
 means
(for n being Nat holds (n < $1 implies P[ n ]));
A2: (for k being Nat holds (P1[ k ] implies P1[ ( k + 1 ) ]))
proof
let k being Nat;
assume A3: (for n being Nat holds (n < k implies P[ n ]));
let n being Nat;
assume n < ( k + 1 );
then n <= k by CKB21:1;
then (n < k or (n = k & n <= k)) by XXREAL_0:1;
hence thesis by A1,A3;
end;
A4: P1[ ( 0 ) ] by CKB11:1;
(for k being Nat holds P1[ k ]) from CKB7:sch 1(A4,A2);
then (for n being Nat holds (n < k implies P[ n ]));
hence thesis by A1;
end;
