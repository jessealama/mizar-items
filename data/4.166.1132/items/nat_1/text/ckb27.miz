constructors NUMBERS,XCMPLX_0,XXREAL_0,XREAL_0,CARD_1,WELLORD2,FUNCT_2,PARTFUN1,FUNCOP_1,FUNCT_4,ENUMSET1,RELSET_1,PBOOLE,CKB4,CKB5,CKB9,CKB10;
requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve m for Nat;
scheme Max { P[Nat],N() -> Nat } : (ex k being Nat st (P[ k ] & (for n being Nat holds (P[ n ] implies n <= k))))
provided
A1: (for k being Nat holds (P[ k ] implies k <= N()))
and
A2: (ex k being Nat st P[ k ])
proof
defpred P1[ Nat ]
 means
(for n being Nat holds (P[ n ] implies n <= $1));
A3: (ex k being Nat st P1[ k ]) by A1;
consider k being Nat such that A4: P1[ k ] and A5: (for m being Nat holds (P1[ m ] implies k <= m)) from CKB26:sch 1(A3);
take k;
thus P[ k ]
proof
consider n being Nat such that A6: P[ n ] by A2;
A7: n <= k by A4,A6;
assume A8: (not P[ k ]);
then n <> k by A6;
then k <> ( 0 ) by A7,CKB11:1;
then consider m such that A9: k = ( m + 1 ) by CKB15:1;
(for n being Nat holds (P[ n ] implies n <= m)) by A4,A8,A9,CKB21:1;
then A10: k <= m by A5;
( ( ( - m ) + m ) + 1 ) = ( ( - m ) + ( m + 1 ) );
hence contradiction by A9,A10,XREAL_1:6;
end;

thus thesis by A4;
end;
