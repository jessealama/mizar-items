begin
reserve i for Nat;
scheme Ind1 { M() -> Nat,P[Nat] } : (for i being Nat holds (M() <= i implies P[ i ]))
provided
A1: P[ M() ]
and
A2: (for j being Nat holds (M() <= j implies (P[ j ] implies P[ ( j + 1 ) ])))
proof
reconsider M = M() as (Element of ( NAT )) by ORDINAL1:def 12;
defpred Q[ Nat ]
 means
P[ ( M() + $1 ) ];
A3:now
let m being (Element of ( NAT ));
assume Q[ m ];
then P[ ( ( M + m ) + 1 ) ] by A2,CKB24:1;
hence Q[ ( m + 1 ) ];
end;
let i;
assume M() <= i;
then consider m being Nat such that A4: i = ( M() + m ) by CKB23:1;
A5: Q[ ( 0 ) ] by A1;
A6: (for m being (Element of ( NAT )) holds Q[ m ]) from CKB6:sch 1(A5,A3);
m in ( NAT ) by ORDINAL1:def 12;
hence thesis by A6,A4;
end;
