begin
reserve k for Nat;
scheme Min { P[Nat] } : (ex k being Nat st (P[ k ] & (for n being Nat holds (P[ n ] implies k <= n))))
provided
A1: (ex k being Nat st P[ k ])
proof
defpred P1[ Nat ]
 means
(not P[ $1 ]);
assume A2: (not thesis);
A3: (for k being Nat holds ((for n being Nat holds (n < k implies P1[ n ])) implies P1[ k ]))
proof
let k being Nat;
assume A4: (for n being Nat holds (n < k implies (not P[ n ])));
((not (ex n being Nat st (P[ n ] & (not k <= n)))) implies (not P[ k ])) by A2;
hence thesis by A4;
end;
(for k being Nat holds P1[ k ]) from CKB25:sch 1(A3);
hence contradiction by A1;
end;
