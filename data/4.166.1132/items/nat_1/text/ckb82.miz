begin
reserve k for Nat;
reserve n for Nat;
scheme MinIndex { F(Nat) -> Nat } : (ex k st (F(k) = ( 0 ) & (for n holds (F(n) = ( 0 ) implies k <= n))))
provided
A1: (for k holds (F(( k + 1 )) < F(k) or F(k) = ( 0 )))
proof
defpred X[ Nat ]
 means
F($1) = ( 0 );
defpred P[ Nat ]
 means
(ex n being Nat st $1 = F(n));
A2: (for k being Nat holds ((k <> ( 0 ) & P[ k ]) implies (ex m being Nat st (m < k & P[ m ]))))
proof
let k being Nat;
assume that
A3: k <> ( 0 )
and
A4: P[ k ];
consider n being Nat such that A5: k = F(n) by A4;
take F(( n + 1 ));
thus thesis by A1,A3,A5;
end;
F(( 0 )) is Nat;
then A6: (ex k being Nat st P[ k ]);
P[ ( 0 ) ] from CKB33:sch 1(A6,A2);
then A7: (ex k being Nat st X[ k ]);
consider k being Nat such that A8: (X[ k ] & (for n being Nat holds (X[ n ] implies k <= n))) from CKB26:sch 1(A7);
take k;
thus thesis by A8;
end;
