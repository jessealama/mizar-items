begin
reserve n for Nat;
definition
let s being (ManySortedSet of ( NAT ));
let k being Nat;
func s ^\ k -> (ManySortedSet of ( NAT ))
means
:Def3: (for n being Nat holds ( it . n ) = ( s . ( n + k ) ));
existence
proof
defpred P[ set,set ]
 means
(ex n being (Element of ( NAT )) st (n = $1 & $2 = ( s . ( n + k ) )));
A0: (for i being set holds (i in ( NAT ) implies (ex j being set st P[ i,j ])))
proof
let i being set;
assume i in ( NAT );
then reconsider n = i as (Element of ( NAT ));
take j = ( s . ( n + k ) );
thus P[ i,j ];
end;
consider f being (ManySortedSet of ( NAT )) such that A1: (for i being set holds (i in ( NAT ) implies P[ i,( f . i ) ])) from PBOOLE:sch 3(A0);
take f;
let n being Nat;
n in ( NAT ) by ORDINAL1:def 12;
then P[ n,( f . n ) ] by A1;
hence thesis;
end;
uniqueness
proof
let s1 being (ManySortedSet of ( NAT )),s2 being (ManySortedSet of ( NAT ));
assume that
A2: (for n holds ( s1 . n ) = ( s . ( n + k ) ))
and
A3: (for n holds ( s2 . n ) = ( s . ( n + k ) ));
now
let n being set;
assume n in ( NAT );
then reconsider nn = n as (Element of ( NAT ));
thus ( s1 . n ) = ( s . ( nn + k ) ) by A2
.= ( s2 . n ) by A3;
end;
hence s1 = s2 by PBOOLE:3;
end;
end;
