begin
reserve k for Nat;
scheme Regr { P[Nat] } : P[ ( 0 ) ]
provided
A1: (ex k being Nat st P[ k ])
and
A2: (for k being Nat holds ((k <> ( 0 ) & P[ k ]) implies (ex n being Nat st (n < k & P[ n ]))))
proof
consider k being Nat such that A3: (P[ k ] & (for n being Nat holds (P[ n ] implies k <= n))) from CKB26:sch 1(A1);
now
assume k <> ( 0 );
then (ex n being Nat st (n < k & P[ n ])) by A2,A3;
hence contradiction by A3;
end;
hence thesis by A3;
end;
