begin
reserve k for Nat;
reserve n for Nat;
theorem
(k <= n implies ( n - k ) is (Element of ( NAT )))
proof
assume A1: k <= n;
per cases  by A1,XXREAL_0:1;
suppose k < n;

then ( k + 1 ) <= n by CKB29:1;
then consider j being Nat such that A2: n = ( ( k + 1 ) + j ) by CKB23:1;
reconsider j as (Element of ( NAT )) by ORDINAL1:def 12;
( n - k ) = ( 1 + j ) by A2;
hence thesis;
end;
suppose k = n;

then ( n - k ) = ( 0 );
hence thesis;
end;
end;
