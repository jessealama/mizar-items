environ
vocabularies XBOOLE_0,PRE_TOPC,SUBSET_1,TMAP_1,RCOMP_1,TARSKI,STRUCT_0,TOPS_1,TOPS_3,ZFMISC_1,SETFAM_1,TDLAT_3,NATTRA_1,COMPTS_1,TEX_1,CARD_1;
notations TARSKI,XBOOLE_0,ENUMSET1,SUBSET_1,SETFAM_1,DOMAIN_1,CARD_1,STRUCT_0,PRE_TOPC,TOPS_1,TMAP_1,TDLAT_3,TOPS_3,COMPTS_1,CKB9,CKB19,CKB21,CKB38;
definitions PRE_TOPC,TDLAT_3,COMPTS_1,SUBSET_1,STRUCT_0,CKB9,CKB19,CKB21,CKB38;
theorems TARSKI,SUBSET_1,ZFMISC_1,ENUMSET1,PRE_TOPC,TOPS_1,PCOMPS_1,TMAP_1,TDLAT_3,TOPS_3,STRUCT_0,XBOOLE_0,XBOOLE_1,TSEP_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB12,CKB19,CKB21,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB32,CKB33,CKB34,CKB35,CKB38,CKB41,CKB42,CKB43;
registrations XBOOLE_0,SUBSET_1,STRUCT_0,PRE_TOPC,TOPS_1,TDLAT_3,TMAP_1,CARD_1,ORDINAL1,CKB10,CKB11,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB20,CKB22,CKB23,CKB31,CKB36,CKB37,CKB39,CKB40;
constructors SETFAM_1,DOMAIN_1,TOPS_1,COMPTS_1,PCOMPS_1,TDLAT_3,TMAP_1,TOPS_3,CKB19,CKB21,CKB38;
requirements BOOLE,SUBSET,NUMERALS;
begin
reserve D for non  empty set;
reserve d0 for (Element of D);
theorem
(( D \ { d0 } ) is non  empty implies (for A being (Subset of ( STS (D,d0) )) holds ((A = ( D \ { d0 } ) implies (A is  open & A is  dense)) & (((A <> D & A is  open) & A is  dense) implies A = ( D \ { d0 } )))))
proof
assume A1: ( D \ { d0 } ) is non  empty;
reconsider P = { d0 } as (Subset of ( STS (D,d0) ));
let A being (Subset of ( STS (D,d0) ));
set Z = ( A ` );
thus (A = ( D \ { d0 } ) implies (A is  open & A is  dense))
proof
assume A2: A = ( D \ { d0 } );
hence A is  open by CKB43:1;
( ( [#] ( STS (D,d0) ) ) \ ( ( [#] ( STS (D,d0) ) ) \ P ) ) = Z by A2;
then P = Z by PRE_TOPC:3;
then Z is  boundary by A1,CKB42:1;
hence thesis by TOPS_3:18;
end;

thus (((A <> D & A is  open) & A is  dense) implies A = ( D \ { d0 } ))
proof
assume A <> D;
then A3: Z <> ( {} ( STS (D,d0) ) ) by TOPS_3:2;
assume A4: A is  open;
assume A is  dense;
then Z is  boundary by TOPS_3:18;
then Z = { d0 } by A1,A3,A4,CKB42:1;
then A = ( P ` );
hence thesis;
end;

end;
