environ
vocabularies XBOOLE_0,PRE_TOPC,SUBSET_1,TMAP_1,RCOMP_1,TARSKI,STRUCT_0,TOPS_1,TOPS_3,ZFMISC_1,SETFAM_1,TDLAT_3,NATTRA_1,COMPTS_1,TEX_1,CARD_1;
notations TARSKI,XBOOLE_0,ENUMSET1,SUBSET_1,SETFAM_1,DOMAIN_1,CARD_1,STRUCT_0,PRE_TOPC,TOPS_1,TMAP_1,TDLAT_3,TOPS_3,COMPTS_1,CKB9,CKB19,CKB21,CKB38;
definitions PRE_TOPC,TDLAT_3,COMPTS_1,SUBSET_1,STRUCT_0,CKB9,CKB19,CKB21,CKB38;
theorems TARSKI,SUBSET_1,ZFMISC_1,ENUMSET1,PRE_TOPC,TOPS_1,PCOMPS_1,TMAP_1,TDLAT_3,TOPS_3,STRUCT_0,XBOOLE_0,XBOOLE_1,TSEP_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB12,CKB19,CKB21,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB32,CKB33,CKB34,CKB35,CKB38;
registrations XBOOLE_0,SUBSET_1,STRUCT_0,PRE_TOPC,TOPS_1,TDLAT_3,TMAP_1,CARD_1,ORDINAL1,CKB10,CKB11,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB20,CKB22,CKB23,CKB31,CKB36,CKB37,CKB39,CKB40;
constructors SETFAM_1,DOMAIN_1,TOPS_1,COMPTS_1,PCOMPS_1,TDLAT_3,TMAP_1,TOPS_3,CKB19,CKB21,CKB38;
requirements BOOLE,SUBSET,NUMERALS;
begin
reserve D for non  empty set;
reserve d0 for (Element of D);
theorem
Th24: (for A being (Subset of ( STS (D,d0) )) holds (({ d0 } c= A implies A is  closed) & ((A is non  empty & A is  closed) implies { d0 } c= A)))
proof
let A being (Subset of ( STS (D,d0) ));
set Z = ( A ` );
set G = { P where P is (Subset of D): (d0 in P & P <> D) };
thus ({ d0 } c= A implies A is  closed)
proof
A1: d0 in { d0 } by TARSKI:def 1;
assume { d0 } c= A;
then (not (ex Q being (Subset of D) st ((Q = Z & d0 in Q) & Q <> D))) by A1,XBOOLE_0:def 5;
then (not Z in G);
then Z in (the topology of ( STS (D,d0) )) by XBOOLE_0:def 5;
then Z is  open by PRE_TOPC:def 2;
hence thesis by TOPS_1:3;
end;

assume A is non  empty;
then A2: Z <> D by TOPS_3:1;
assume A is  closed;
then Z in (the topology of ( STS (D,d0) )) by PRE_TOPC:def 2;
then (not Z in G) by XBOOLE_0:def 5;
then (not d0 in Z) by A2;
then d0 in A by SUBSET_1:29;
hence thesis by ZFMISC_1:31;
end;
