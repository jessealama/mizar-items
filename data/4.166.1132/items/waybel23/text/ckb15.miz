begin
theorem
(for L being non empty RelStr holds (for S being non empty (SubRelStr of L) holds (for x being (Element of L) holds (for y being (Element of S) holds (x = y implies ( downarrow y ) c= ( downarrow x ))))))
proof
let L being non empty RelStr;
let S being non empty (SubRelStr of L);
let x being (Element of L);
let y being (Element of S);
A1: ( downarrow x ) = ( downarrow { x } ) by WAYBEL_0:def 17;
A2: ( downarrow y ) = ( downarrow { y } ) by WAYBEL_0:def 17;
assume x = y;
hence thesis by A1,A2,CKB13:1;
end;
