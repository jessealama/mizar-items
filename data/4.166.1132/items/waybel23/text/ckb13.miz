begin
theorem
Th11: (for L being non empty RelStr holds (for S being non empty (SubRelStr of L) holds (for X being (Subset of L) holds (for Y being (Subset of S) holds (X = Y implies ( downarrow Y ) c= ( downarrow X ))))))
proof
let L being non empty RelStr;
let S being non empty (SubRelStr of L);
let X being (Subset of L);
let Y being (Subset of S);
assume A1: X = Y;
let x being set;
assume A2: x in ( downarrow Y );
then reconsider x1 = x as (Element of S);
consider y1 being (Element of S) such that A3: y1 >= x1 and A4: y1 in Y by A2,WAYBEL_0:def 15;
reconsider x2 = x1,y2 = y1 as (Element of L) by YELLOW_0:58;
y2 >= x2 by A3,YELLOW_0:59;
hence thesis by A1,A4,WAYBEL_0:def 15;
end;
