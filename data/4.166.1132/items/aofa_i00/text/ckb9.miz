begin
definition
let t1 being  integer-yielding Function;
let t2 being  integer-yielding Function;
func gt (t1,t2) ->  integer-yielding Function means 
:Def6: (( dom it ) = ( ( dom t1 ) /\ ( dom t2 ) ) & (for s being set holds (s in ( dom it ) implies ( it . s ) = ( IFGT (( t1 . s ),( t2 . s ),1,( 0 )) ))));
correctness
proof
deffunc F(set) = ( IFGT (( t1 . $1 ),( t2 . $1 ),1,( 0 )) );
consider f being Function such that A19: (( dom f ) = ( ( dom t1 ) /\ ( dom t2 ) ) & (for x being set holds (x in ( ( dom t1 ) /\ ( dom t2 ) ) implies ( f . x ) = F(x)))) from FUNCT_1:sch 3;
f is  integer-yielding
proof
let x being set;
assume x in ( dom f );
then ( f . x ) = F(x) by A19;
hence thesis;
end;
hence (ex f being  integer-yielding Function st (( dom f ) = ( ( dom t1 ) /\ ( dom t2 ) ) & (for x being set holds (x in ( dom f ) implies ( f . x ) = F(x))))) by A19;
let f1 being  integer-yielding Function;
let f2 being  integer-yielding Function;
assume that
A20: ( dom f1 ) = ( ( dom t1 ) /\ ( dom t2 ) )
and
A21: (for s being set holds (s in ( dom f1 ) implies ( f1 . s ) = F(s)))
and
A22: ( dom f2 ) = ( ( dom t1 ) /\ ( dom t2 ) )
and
A23: (for s being set holds (s in ( dom f2 ) implies ( f2 . s ) = F(s)));
now
let s being set;
assume A24: s in ( ( dom t1 ) /\ ( dom t2 ) );
hence ( f1 . s ) = F(s) by A20,A21
.= ( f2 . s ) by A22,A23,A24;
end;
hence thesis by A20,A22,FUNCT_1:2;
end;
end;
