requirements NUMERALS,REAL,BOOLE,SUBSET,ARITHM;
begin
theorem
Th5: (for X being non empty set holds (for A being set holds (for S being (SigmaField of X) holds (for F being (Finite_Sep_Sequence of S) holds (for G being (FinSequence of S) holds ((( dom G ) = ( dom F ) & (for i being Nat holds (i in ( dom G ) implies ( G . i ) = ( A /\ ( F . i ) )))) implies G is (Finite_Sep_Sequence of S)))))))
proof
let X being non empty set;
let A being set;
let S being (SigmaField of X);
let F being (Finite_Sep_Sequence of S);
let G being (FinSequence of S);
assume that
A1: ( dom G ) = ( dom F )
and
A2: (for i being Nat holds (i in ( dom G ) implies ( G . i ) = ( A /\ ( F . i ) )));
now
let i being Nat,j being Nat;
assume that
A3: i in ( dom G )
and
A4: j in ( dom G )
and
A5: i <> j;
A6: ( F . i ) misses ( F . j ) by A1,A3,A4,A5,CKB4:1;
( ( A /\ ( F . i ) ) /\ ( A /\ ( F . j ) ) ) = ( A /\ ( ( F . i ) /\ ( A /\ ( F . j ) ) ) ) by XBOOLE_1:16
.= ( A /\ ( ( ( F . i ) /\ ( F . j ) ) /\ A ) ) by XBOOLE_1:16
.= ( A /\ ( ( {} ) /\ A ) ) by A6,XBOOLE_0:def 7
.= ( {} );
then ( A /\ ( F . i ) ) misses ( A /\ ( F . j ) ) by XBOOLE_0:def 7;
then ( G . i ) misses ( A /\ ( F . j ) ) by A2,A3;
hence ( G . i ) misses ( G . j ) by A2,A4;
end;
hence thesis by CKB4:1;
end;
