x in Y;
((not x in ( rng f )) implies thesis);
hence thesis;
end;
A5: (for x holds (for y holds (for z holds (((x in Y & P[ x,y ]) & P[ x,z ]) implies y = z))));
consider g such that A6: (( dom g ) = Y & (for y holds (y in Y implies P[ y,( g . y ) ]))) from FUNCT_1:sch 2(A5,A4);
take g;
thus ( dom g ) = Y by A6;
let x;
