begin
definition
let n being  natural number;
func prime_exponents n -> (ManySortedSet of ( SetPrimes ))
means
:Def8: (for p being Prime holds ( it . p ) = ( p |-count n ));
existence
proof
deffunc F( natural number) = ( $1 |-count n );
defpred P[ set,set ]
 means
(ex w being  natural number st (w = $1 & $2 = F(w)));
A1: (for i being set holds (i in ( SetPrimes ) implies (ex j being set st P[ i,j ])))
proof
let i being set;
assume i in ( SetPrimes );
then reconsider i as  prime (Element of ( NAT )) by NEWTON:def 6;
take F(i);
take i;
thus thesis;
end;
consider f being (ManySortedSet of ( SetPrimes )) such that A2: (for i being set holds (i in ( SetPrimes ) implies P[ i,( f . i ) ])) from PBOOLE:sch 3(A1);
take f;
let i being Prime;
i in ( SetPrimes ) by NEWTON:def 6;
then P[ i,( f . i ) ] by A2;
hence thesis;
end;
uniqueness
proof
let f being (ManySortedSet of ( SetPrimes )),g being (ManySortedSet of ( SetPrimes ));
assume that
A3: (for i being Prime holds ( f . i ) = ( i |-count n ))
and
A4: (for i being Prime holds ( g . i ) = ( i |-count n ));
now
let i being set;
assume i in ( SetPrimes );
then reconsider a = i as  prime (Element of ( NAT )) by NEWTON:def 6;
thus ( f . i ) = ( a |-count n ) by A3
.= ( g . i ) by A4;
end;
hence thesis by PBOOLE:3;
end;
end;
