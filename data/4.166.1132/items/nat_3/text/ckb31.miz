begin
definition
let X being set;
let b1 being  real-valued (ManySortedSet of X);
let b2 being  real-valued (ManySortedSet of X);
func min (b1,b2) -> (ManySortedSet of X)
means
:Def3: (for i being set holds ((( b1 . i ) <= ( b2 . i ) implies ( it . i ) = ( b1 . i )) & (( b1 . i ) > ( b2 . i ) implies ( it . i ) = ( b2 . i ))));
existence
proof
deffunc G(set) = ( b2 . $1 );
deffunc F(set) = ( b1 . $1 );
defpred P[ set ]
 means
( b1 . $1 ) <= ( b2 . $1 );
consider f being (ManySortedSet of X) such that A1: (for i being (Element of X) holds (i in X implies ((P[ i ] implies ( f . i ) = F(i)) & ((not P[ i ]) implies ( f . i ) = G(i))))) from PRE_CIRC:sch 2;
take f;
let i being set;
per cases ;
suppose i in X;

hence thesis by A1;
end;
suppose A2: (not i in X);

then (not i in ( dom f )) by PARTFUN1:def 2;
then A3: ( f . i ) = ( 0 ) by FUNCT_1:def 2;
((not i in ( dom b1 )) & (not i in ( dom b2 ))) by A2,PARTFUN1:def 2;
hence thesis by A3,FUNCT_1:def 2;
end;
end;
uniqueness
proof
let f being (ManySortedSet of X),g being (ManySortedSet of X);
assume that
A4: (for i being set holds ((( b1 . i ) <= ( b2 . i ) implies ( f . i ) = ( b1 . i )) & (( b1 . i ) > ( b2 . i ) implies ( f . i ) = ( b2 . i ))))
and
A5: (for i being set holds ((( b1 . i ) <= ( b2 . i ) implies ( g . i ) = ( b1 . i )) & (( b1 . i ) > ( b2 . i ) implies ( g . i ) = ( b2 . i ))));
now
let i being set;
assume i in X;
per cases ;
suppose A6: ( b1 . i ) <= ( b2 . i );

hence ( f . i ) = ( b1 . i ) by A4
.= ( g . i ) by A5,A6;
end;
suppose A7: ( b1 . i ) > ( b2 . i );

hence ( f . i ) = ( b2 . i ) by A4
.= ( g . i ) by A5,A7;
end;
end;
hence thesis by PBOOLE:3;
end;
end;
