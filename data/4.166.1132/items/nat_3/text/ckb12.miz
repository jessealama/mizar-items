begin
reserve a for  natural number;
reserve f for (FinSequence of ( REAL ));
reserve p for Prime;
definition
let f being  real-valued FinSequence;
let a being  natural number;
func f |^ a -> FinSequence means 
:Def1: (( len it ) = ( len f ) & (for i being set holds (i in ( dom it ) implies ( it . i ) = ( ( f . i ) |^ a ))));
existence
proof
deffunc F(Nat) = ( ( f . $1 ) |^ a );
consider p being FinSequence such that A1: ( len p ) = ( len f ) and A2: (for k being Nat holds (k in ( dom p ) implies ( p . k ) = F(k))) from FINSEQ_1:sch 2;
take p;
thus ( len p ) = ( len f ) by A1;
let i being set;
assume i in ( dom p );
hence thesis by A2;
end;
uniqueness
proof
let g being FinSequence;
let h being FinSequence;
assume that
A3: ( len g ) = ( len f )
and
A4: (for i being set holds (i in ( dom g ) implies ( g . i ) = ( ( f . i ) |^ a )))
and
A5: ( len h ) = ( len f )
and
A6: (for i being set holds (i in ( dom h ) implies ( h . i ) = ( ( f . i ) |^ a )));
A7: (( dom g ) = ( Seg ( len g ) ) & ( dom h ) = ( Seg ( len h ) )) by FINSEQ_1:def 3;
(for k being Nat holds (k in ( dom g ) implies ( g . k ) = ( h . k )))
proof
let k being Nat;
assume that
A8: k in ( dom g );
thus ( g . k ) = ( ( f . k ) |^ a ) by A4,A8
.= ( h . k ) by A3,A5,A6,A7,A8;
end;
hence thesis by A3,A5,A7,FINSEQ_1:13;
end;
end;
