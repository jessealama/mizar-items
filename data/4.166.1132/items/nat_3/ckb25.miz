begin
definition
let X being set;
let b being  real-valued (ManySortedSet of X);
let a being  natural number;
func a * b -> (ManySortedSet of X)
means
:Def2: (for i being set holds ( it . i ) = ( a * ( b . i ) ));
existence
proof
deffunc F(set) = ( a * ( b . $1 ) );
consider f being (ManySortedSet of X) such that A1: (for i being set holds (i in X implies ( f . i ) = F(i))) from PBOOLE:sch 4;
take f;
let i being set;
per cases ;
suppose i in X;

hence thesis by A1;
end;
suppose A2: (not i in X);

A3: ( dom b ) = X by PARTFUN1:def 2;
( dom f ) = X by PARTFUN1:def 2;
hence ( f . i ) = ( a * ( 0 ) ) by A2,FUNCT_1:def 2
.= ( a * ( b . i ) ) by A2,A3,FUNCT_1:def 2;
end;
end;
uniqueness
proof
let f being (ManySortedSet of X),g being (ManySortedSet of X);
assume that
A4: (for i being set holds ( f . i ) = ( a * ( b . i ) ))
and
A5: (for i being set holds ( g . i ) = ( a * ( b . i ) ));
(for i being set holds (i in X implies ( f . i ) = ( g . i )))
proof
let i being set;
assume i in X;
thus ( f . i ) = ( a * ( b . i ) ) by A4
.= ( g . i ) by A5;
end;
hence thesis by PBOOLE:3;
end;
end;
