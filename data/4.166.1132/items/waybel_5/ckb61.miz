constructors FINSUB_1,CLASSES1,CLASSES2,BORSUK_1,LATTICE3,PRALG_1,PRALG_2,MSUALG_3,ORDERS_3,WAYBEL_1,WAYBEL_3,YELLOW_6,CKB13,CKB23,CKB24,CKB25,CKB26,CKB45,CKB55;
requirements SUBSET,BOOLE,NUMERALS;
begin
reserve i for set;
theorem
Lm15: (for L being  completely-distributive LATTICE holds (for X being non empty (Subset of L) holds (for x being (Element of L) holds ( x "/\" ( sup X ) ) = ( "\/" ({ ( x "/\" y ) where y is (Element of L): y in X },L) ))))
proof
let L being  completely-distributive LATTICE;
let X being non empty (Subset of L);
let x being (Element of L);
set A = { ( x "/\" y ) where y is (Element of L): y in X };
set J = { 1,2 };
set K = ( (1,2) --> ({ 1 },X) );
set F = ( (1,2) --> (( { 1 } --> x ),( id X )) );
A1: ( F . 1 ) = ( { 1 } --> x ) by FUNCT_4:63;
A2: ( dom K ) = J by FUNCT_4:62;
A3: ( K . 2 ) = X by FUNCT_4:63;
A4: ( dom F ) = J by FUNCT_4:62;
reconsider j1 = 1,j2 = 2 as (Element of J) by TARSKI:def 2;
A5: ( F . 2 ) = ( id X ) by FUNCT_4:63;
reconsider F as (ManySortedSet of J) by A4,PARTFUN1:def 2,RELAT_1:def 18;
A6: ( K . 1 ) = { 1 } by FUNCT_4:63;
reconsider K as (ManySortedSet of J) by A2,PARTFUN1:def 2,RELAT_1:def 18;
(i in J implies ( K . i ) is non empty) by A6,A3,TARSKI:def 2;
then reconsider K as  non-empty (ManySortedSet of J) by PBOOLE:def 13;
(for j being set holds (j in J implies ( F . j ) is (Function of ( K . j ),( ( J --> (the carrier of L) ) . j ))))
proof
let j being set;
assume A7: j in J;
then A8: ( ( J --> (the carrier of L) ) . j ) = (the carrier of L) by FUNCOP_1:7;
per cases  by A7,TARSKI:def 2;
suppose j = 1;

hence thesis by A1,A6,A7,FUNCOP_1:7;
end;
suppose j = 2;

hence thesis by A5,A3,A8,FUNCT_2:7;
end;
end;
then reconsider F as (DoubleIndexedSet of K,L) by PBOOLE:def 15;
( rng ( Infs ( Frege F ) ) ) is_<=_than ( "\/" ({ ( x "/\" y ) where y is (Element of L): y in X },L) )
proof
let y being (Element of L);
assume y in ( rng ( Infs ( Frege F ) ) );
then consider f being set such that A9: f in ( dom ( Frege F ) ) and A10: y = ( //\ (( ( Frege F ) . f ),L) ) by CKB33:1;
reconsider f as Function by A9,CKB16:1;
A11: ( f . j2 ) in ( K . j2 ) by A9,CKB21:1;
then reconsider f2 = ( f . 2 ) as (Element of X) by FUNCT_4:63;
A12: ( f . j1 ) in ( K . j1 ) by A9,CKB21:1;
A13: { x,f2 } c= ( rng ( ( Frege F ) . f ) )
proof
let z being set;
assume z in { x,f2 };
then (z = x or z = ( f . 2 )) by TARSKI:def 2;
then (z = ( ( F . j1 ) . ( f . j1 ) ) or z = ( ( F . j2 ) . ( f . j2 ) )) by A1,A5,A6,A3,A12,A11,FUNCOP_1:7,FUNCT_1:18;
hence thesis by A9,CKB20:1;
end;
( x "/\" f2 ) in A;
then A14: ( x "/\" f2 ) <= ( "\/" (A,L) ) by YELLOW_2:22;
A15: ( ex_inf_of ( rng ( ( Frege F ) . f ) ),L &  ex_inf_of { x,( f . 2 ) },L) by YELLOW_0:17;
y = ( "/\" (( rng ( ( Frege F ) . f ) ),L) ) by A10,YELLOW_2:def 6;
then y <= ( inf { x,f2 } ) by A15,A13,YELLOW_0:35;
then y <= ( x "/\" f2 ) by YELLOW_0:40;
hence thesis by A14,YELLOW_0:def 2;
end;
then ( sup ( rng ( Infs ( Frege F ) ) ) ) <= ( "\/" ({ ( x "/\" y ) where y is (Element of L): y in X },L) ) by YELLOW_0:32;
then A16: ( Sup ( Infs ( Frege F ) ) ) <= ( "\/" ({ ( x "/\" y ) where y is (Element of L): y in X },L) ) by YELLOW_2:def 5;
( ( x "/\" ) .: X ) = A by WAYBEL_1:61;
then reconsider A9 = A as (Subset of L);
( ex_sup_of X,L &  ex_sup_of A9,L) by YELLOW_0:17;
then A17: ( x "/\" ( sup X ) ) >= ( sup A9 ) by CKB60:1;
( x "/\" ( sup X ) ) is_<=_than ( rng ( Sups F ) )
proof
let y being (Element of L);
assume y in ( rng ( Sups F ) );
then consider j being (Element of J) such that A18: y = ( Sup ( F . j ) ) by CKB34:1;
per cases  by TARSKI:def 2;
suppose j = 1;

then ( rng ( F . j ) ) = { x } by A1,FUNCOP_1:8;
then ( Sup ( F . j ) ) = ( sup { x } ) by YELLOW_2:def 5;
then y = x by A18,YELLOW_0:39;
hence ( x "/\" ( sup X ) ) <= y by YELLOW_0:23;
end;
suppose j = 2;

then ( rng ( F . j ) ) = X by A5,RELAT_1:45;
then y = ( sup X ) by A18,YELLOW_2:def 5;
hence ( x "/\" ( sup X ) ) <= y by YELLOW_0:23;
end;
end;
then ( x "/\" ( sup X ) ) <= ( inf ( rng ( Sups F ) ) ) by YELLOW_0:33;
then ( x "/\" ( sup X ) ) <= ( Inf ( Sups F ) ) by YELLOW_2:def 6;
then ( x "/\" ( sup X ) ) <= ( Sup ( Infs ( Frege F ) ) ) by CKB55:def 1;
then ( x "/\" ( sup X ) ) <= ( "\/" ({ ( x "/\" y ) where y is (Element of L): y in X },L) ) by A16,YELLOW_0:def 2;
hence thesis by A17,YELLOW_0:def 3;
end;
