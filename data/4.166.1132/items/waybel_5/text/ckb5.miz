requirements SUBSET,BOOLE,NUMERALS;
begin
reserve L for  up-complete Semilattice;
theorem
Lm4: ((for x being (Element of L) holds (ex I being (Ideal of L) st (x <= ( sup I ) & (for J being (Ideal of L) holds (x <= ( sup J ) implies I c= J))))) implies L is  continuous)
proof
assume A1: (for x being (Element of L) holds (ex I being (Ideal of L) st (x <= ( sup I ) & (for J being (Ideal of L) holds (x <= ( sup J ) implies I c= J)))));
now
let x being (Element of L);
consider I being (Ideal of L) such that A2: x <= ( sup I ) and A3: (for J being (Ideal of L) holds (x <= ( sup J ) implies I c= J)) by A1;
now
let y being set;
assume A4: y in I;
then reconsider y9 = y as (Element of L);
(for J being (Ideal of L) holds (x <= ( sup J ) implies y in J))
proof
let J being (Ideal of L);
assume x <= ( sup J );
then I c= J by A3;
hence thesis by A4;
end;
then y9 << x by WAYBEL_3:21;
hence y in ( waybelow x ) by WAYBEL_3:7;
end;
then A5: I c= ( waybelow x ) by TARSKI:def 3;
now
let y being set;
assume A6: y in ( waybelow x );
then reconsider y9 = y as (Element of L);
y9 << x by A6,WAYBEL_3:7;
hence y in I by A2,WAYBEL_3:20;
end;
then ( waybelow x ) c= I by TARSKI:def 3;
then ( waybelow x ) = I by A5,XBOOLE_0:def 10;
hence ((( waybelow x ) is (Ideal of L) & x <= ( sup ( waybelow x ) )) & (for I being (Ideal of L) holds (x <= ( sup I ) implies ( waybelow x ) c= I))) by A2,A3;
end;
hence thesis by CKB2:1;
end;
