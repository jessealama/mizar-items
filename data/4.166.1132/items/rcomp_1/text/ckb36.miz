requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve s for  real number;
reserve g for  real number;
reserve r for  real number;
definition
let g being  ext-real number;
let s being  real number;
redefine func ].g,s .] -> (Subset of ( REAL )) equals 
{ r where r is Real: (g < r & r <= s) };
coherence
proof
now
let e being set;
assume e in ]. g,s .];
then A1: (ex r being (Element of ( ExtREAL )) st ((e = r & g < r) & r <= s));
s in ( REAL ) by XREAL_0:def 1;
hence e in ( REAL ) by A1,XXREAL_0:47;
end;
hence thesis by TARSKI:def 3;
end;
compatibility
proof
set Y = { r where r is Real: (g < r & r <= s) };
let X being (Subset of ( REAL ));
A2: ]. g,s .] c= Y
proof
let e being set;
assume e in ]. g,s .];
then consider r being (Element of ( ExtREAL )) such that A3: e = r and A4: (g < r & r <= s);
s in ( REAL ) by XREAL_0:def 1;
then r in ( REAL ) by A4,XXREAL_0:47;
hence thesis by A3,A4;
end;
Y c= ]. g,s .]
proof
let e being set;
assume e in Y;
then consider r being (Element of ( REAL )) such that A5: ((e = r & g < r) & r <= s);
r in ( REAL );
hence thesis by A5,NUMBERS:31;
end;
hence thesis by A2,XBOOLE_0:def 10;
end;
end;
