environ
vocabularies NUMBERS,SUBSET_1,XREAL_0,ORDINAL1,SEQ_1,ORDINAL2,NAT_1,XXREAL_1,REAL_1,XXREAL_0,TARSKI,ARYTM_3,ARYTM_1,COMPLEX1,RELAT_1,VALUED_0,SEQ_2,XXREAL_2,FUNCT_1,FUNCOP_1,CARD_1,XBOOLE_0,SEQ_4,RCOMP_1;
notations TARSKI,XBOOLE_0,SUBSET_1,ORDINAL1,NUMBERS,XCMPLX_0,XREAL_0,COMPLEX1,REAL_1,NAT_1,RELAT_1,FUNCT_2,FUNCOP_1,SEQ_1,SEQ_2,VALUED_0,SEQ_4,XXREAL_0,XXREAL_1,XXREAL_2,RECDEF_1,CKB1,CKB2,CKB6,CKB7,CKB8,CKB25;
definitions TARSKI,SEQ_2,XBOOLE_0,SUBSET_1,XXREAL_1,CKB1,CKB2,CKB6,CKB7,CKB8,CKB25;
theorems TARSKI,SUBSET_1,NAT_1,FUNCT_1,FUNCT_2,SEQ_2,SEQM_3,SEQ_4,ABSVALUE,RELAT_1,XREAL_0,XBOOLE_0,XBOOLE_1,XCMPLX_0,XCMPLX_1,FUNCOP_1,XREAL_1,COMPLEX1,XXREAL_0,NUMBERS,XXREAL_1,MEMBERED,VALUED_0,XXREAL_2,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB15,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB25,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33;
schemes SUBSET_1,FUNCT_2,SEQ_1;
registrations ORDINAL1,RELSET_1,NUMBERS,XXREAL_0,XREAL_0,NAT_1,MEMBERED,VALUED_0,XXREAL_2,FUNCOP_1,SEQ_4,FUNCT_2,CKB13,CKB14,CKB16,CKB24,CKB26;
constructors FUNCOP_1,REAL_1,NAT_1,XXREAL_1,COMPLEX1,PARTFUN1,SEQ_1,SEQ_2,SEQM_3,SEQ_4,RECDEF_1,XXREAL_2,MEMBERED,RELSET_1,RVSUM_1,BINOP_2,CKB1,CKB2,CKB6,CKB7,CKB8,CKB25;
requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve g for  real number;
reserve g1 for  real number;
reserve g2 for  real number;
reserve p for  real number;
reserve X for (Subset of ( REAL ));
theorem
((X is  open  bounded & (for g1 holds (for g2 holds ((g1 in X & g2 in X) implies [. g1,g2 .] c= X)))) implies (ex p st (ex g st X = ]. p,g .[)))
proof
assume that
A1: X is  open
and
A2: X is  bounded
and
A3: (for g1 holds (for g2 holds ((g1 in X & g2 in X) implies [. g1,g2 .] c= X)));
per cases ;
suppose A4: X = ( {} );

take 1;
take ( 0 );
thus thesis by A4,XXREAL_1:28;
end;
suppose A5: X <> ( {} );

take p = ( lower_bound X );
take g = ( upper_bound X );
now
let r being Real;
thus (r in X implies r in ]. p,g .[)
proof
assume A6: r in X;
then (p <> r & p <= r) by A1,A2,CKB33:1,SEQ_4:def 2;
then A7: p < r by XXREAL_0:1;
(g <> r & r <= g) by A1,A2,A6,CKB32:1,SEQ_4:def 1;
then r < g by XXREAL_0:1;
hence thesis by A7;
end;

assume r in ]. p,g .[;
then A8: (ex s being Real st ((s = r & p < s) & s < g));
then ( g - r ) > ( 0 ) by XREAL_1:50;
then consider g2 such that A9: (g2 in X & ( g - ( g - r ) ) < g2) by A2,A5,SEQ_4:def 1;
( r - p ) > ( 0 ) by A8,XREAL_1:50;
then consider g1 such that A10: (g1 in X & g1 < ( p + ( r - p ) )) by A2,A5,SEQ_4:def 2;
reconsider g1,g2 as Real by XREAL_0:def 1;
(r in { s where s is Real: (g1 <= s & s <= g2) } & [. g1,g2 .] c= X) by A3,A9,A10;
hence r in X;
end;
hence thesis by SUBSET_1:3;
end;
end;
