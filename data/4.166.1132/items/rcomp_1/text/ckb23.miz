environ
vocabularies NUMBERS,SUBSET_1,XREAL_0,ORDINAL1,SEQ_1,ORDINAL2,NAT_1,XXREAL_1,REAL_1,XXREAL_0,TARSKI,ARYTM_3,ARYTM_1,COMPLEX1,RELAT_1,VALUED_0,SEQ_2,XXREAL_2,FUNCT_1,FUNCOP_1,CARD_1,XBOOLE_0,SEQ_4,RCOMP_1;
notations TARSKI,XBOOLE_0,SUBSET_1,ORDINAL1,NUMBERS,XCMPLX_0,XREAL_0,COMPLEX1,REAL_1,NAT_1,RELAT_1,FUNCT_2,FUNCOP_1,SEQ_1,SEQ_2,VALUED_0,SEQ_4,XXREAL_0,XXREAL_1,XXREAL_2,RECDEF_1,CKB1,CKB2,CKB6,CKB7,CKB8;
definitions TARSKI,SEQ_2,XBOOLE_0,SUBSET_1,XXREAL_1,CKB1,CKB2,CKB6,CKB7,CKB8;
theorems TARSKI,SUBSET_1,NAT_1,FUNCT_1,FUNCT_2,SEQ_2,SEQM_3,SEQ_4,ABSVALUE,RELAT_1,XREAL_0,XBOOLE_0,XBOOLE_1,XCMPLX_0,XCMPLX_1,FUNCOP_1,XREAL_1,COMPLEX1,XXREAL_0,NUMBERS,XXREAL_1,MEMBERED,VALUED_0,XXREAL_2,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB15,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22;
schemes SUBSET_1,FUNCT_2,SEQ_1;
registrations ORDINAL1,RELSET_1,NUMBERS,XXREAL_0,XREAL_0,NAT_1,MEMBERED,VALUED_0,XXREAL_2,FUNCOP_1,SEQ_4,FUNCT_2,CKB13,CKB14,CKB16;
constructors FUNCOP_1,REAL_1,NAT_1,XXREAL_1,COMPLEX1,PARTFUN1,SEQ_1,SEQ_2,SEQM_3,SEQ_4,RECDEF_1,XXREAL_2,MEMBERED,RELSET_1,RVSUM_1,BINOP_2,CKB1,CKB2,CKB6,CKB7,CKB8;
requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve g for  real number;
reserve g1 for  real number;
reserve g2 for  real number;
reserve r for  real number;
reserve p for  real number;
reserve X for (Subset of ( REAL ));
theorem
((X is  compact & (for g1 holds (for g2 holds ((g1 in X & g2 in X) implies [. g1,g2 .] c= X)))) implies (ex p st (ex g st X = [. p,g .])))
proof
assume that
A1: X is  compact
and
A2: (for g1 holds (for g2 holds ((g1 in X & g2 in X) implies [. g1,g2 .] c= X)));
per cases ;
suppose A3: X = ( {} );

take 1;
take ( 0 );
thus thesis by A3,XXREAL_1:29;
end;
suppose A4: X <> ( {} );

take p = ( lower_bound X );
take g = ( upper_bound X );
A5: X is  bounded  closed by A1,CKB18:1;
now
let r being Real;
assume r in X;
then (r <= g & p <= r) by A5,SEQ_4:def 1,SEQ_4:def 2;
hence r in [. p,g .];
end;
then A6: X c= [. p,g .] by SUBSET_1:2;
(( upper_bound X ) in X & ( lower_bound X ) in X) by A4,A5,CKB20:1,CKB21:1;
then [. p,g .] c= X by A2;
hence thesis by A6,XBOOLE_0:def 10;
end;
end;
