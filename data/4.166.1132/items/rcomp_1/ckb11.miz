registrations ORDINAL1,RELSET_1,NUMBERS,XXREAL_0,XREAL_0,NAT_1,MEMBERED,VALUED_0,XXREAL_2,FUNCOP_1,SEQ_4,FUNCT_2;
constructors FUNCOP_1,REAL_1,NAT_1,XXREAL_1,COMPLEX1,PARTFUN1,SEQ_1,SEQ_2,SEQM_3,SEQ_4,RECDEF_1,XXREAL_2,MEMBERED,RELSET_1,RVSUM_1,BINOP_2,CKB1,CKB2,CKB6,CKB7,CKB8;
requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve s for  real number;
reserve g for  real number;
reserve s1 for Real_Sequence;
reserve s2 for Real_Sequence;
reserve Nseq for  increasing (sequence of ( NAT ));
theorem
[. s,g .] is  compact
proof
(for s1 holds (( rng s1 ) c= [. s,g .] implies (ex s2 st ((s2 is (subsequence of s1) & s2 is  convergent) & ( lim s2 ) in [. s,g .]))))
proof
let s1;
A1: [. s,g .] is  closed by CKB10:1;
assume A2: ( rng s1 ) c= [. s,g .];
then consider s2 being Real_Sequence such that A3: s2 is (subsequence of s1) and A4: s2 is  convergent by CKB9:1,SEQ_4:40;
take s2;
(ex Nseq st s2 = ( s1 * Nseq )) by A3,VALUED_0:def 17;
then ( rng s2 ) c= ( rng s1 ) by RELAT_1:26;
then ( rng s2 ) c= [. s,g .] by A2,XBOOLE_1:1;
hence thesis by A3,A4,A1,CKB7:def 1;
end;
hence thesis by CKB6:def 1;
end;
