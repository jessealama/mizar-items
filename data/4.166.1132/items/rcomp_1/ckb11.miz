definitions TARSKI,SEQ_2,XBOOLE_0,SUBSET_1,XXREAL_1,CKB1,CKB2,CKB6,CKB7,CKB8;
theorems TARSKI,SUBSET_1,NAT_1,FUNCT_1,FUNCT_2,SEQ_2,SEQM_3,SEQ_4,ABSVALUE,RELAT_1,XREAL_0,XBOOLE_0,XBOOLE_1,XCMPLX_0,XCMPLX_1,FUNCOP_1,XREAL_1,COMPLEX1,XXREAL_0,NUMBERS,XXREAL_1,MEMBERED,VALUED_0,XXREAL_2,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10;
