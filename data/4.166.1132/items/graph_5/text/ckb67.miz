begin
reserve i for (Element of ( NAT ));
reserve x for set;
reserve f for Function;
definition
let G being Graph;
let p being (FinSequence of (the carrier' of G));
let W being Function;
assume A1: W is_weight_of G;
func RealSequence (p,W) -> (FinSequence of ( REAL ))
means
:Def15: (( dom p ) = ( dom it ) & (for i being (Element of ( NAT )) holds (i in ( dom p ) implies ( it . i ) = ( W . ( p . i ) ))));
existence
proof
deffunc F(set) = ( W . ( p . $1 ) );
consider f such that A2: (( dom f ) = ( dom p ) & (for x holds (x in ( dom p ) implies ( f . x ) = F(x)))) from FUNCT_1:sch 3;
A3:now
let i being Nat;
A4: W is (Function of (the carrier' of G),( REAL )) by A1,CKB66:def 1;
assume A5: i in ( dom f );
then ( f . i ) = ( W . ( p . i ) ) by A2;
hence ( f . i ) in ( REAL ) by A2,A5,A4,FINSEQ_2:11,FUNCT_2:5;
end;
( dom f ) = ( Seg ( len p ) ) by A2,FINSEQ_1:def 3;
then f is FinSequence by FINSEQ_1:def 2;
then reconsider g = f as (FinSequence of ( REAL )) by A3,FINSEQ_2:12;
take g;
thus ( dom p ) = ( dom g ) by A2;
let i;
assume i in ( dom p );
hence thesis by A2;
end;
uniqueness
proof
let f1 being (FinSequence of ( REAL )),f2 being (FinSequence of ( REAL ));
assume that
A6: ( dom p ) = ( dom f1 )
and
A7: (for i being (Element of ( NAT )) holds (i in ( dom p ) implies ( f1 . i ) = ( W . ( p . i ) )));
assume that
A8: ( dom p ) = ( dom f2 )
and
A9: (for i being (Element of ( NAT )) holds (i in ( dom p ) implies ( f2 . i ) = ( W . ( p . i ) )));
now
let i being Nat;
assume A10: i in ( dom f1 );
hence ( f1 . i ) = ( W . ( p . i ) ) by A6,A7
.= ( f2 . i ) by A6,A9,A10;
end;
hence thesis by A6,A8,FINSEQ_1:13;
end;
end;
