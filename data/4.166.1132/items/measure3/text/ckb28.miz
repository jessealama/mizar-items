environ
vocabularies FUNCT_1,NUMBERS,SUBSET_1,SUPINF_2,XXREAL_0,ORDINAL1,RELAT_1,SUPINF_1,ORDINAL2,PROB_1,MEASURE2,MEASURE1,TARSKI,SETFAM_1,CARD_1,XBOOLE_0,ARYTM_3,NAT_1,ARYTM_1,REAL_1,XXREAL_2,ZFMISC_1,MEASURE3;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,ORDINAL1,NUMBERS,XXREAL_0,FUNCT_1,RELSET_1,PARTFUN1,FUNCT_2,REAL_1,NAT_1,SETFAM_1,PROB_1,XXREAL_2,SUPINF_1,SUPINF_2,MEASURE1,MEASURE2,CKB3,CKB4,CKB17,CKB18,CKB19,CKB20,CKB27;
definitions TARSKI,XBOOLE_0,XXREAL_3,CKB17,CKB18,CKB19,CKB20,CKB27;
theorems TARSKI,FUNCT_1,FUNCT_2,ZFMISC_1,NAT_1,SETFAM_1,SUPINF_2,MEASURE1,MEASURE2,PROB_2,XBOOLE_0,XBOOLE_1,XXREAL_0,PROB_1,FINSUB_1,ORDINAL1,XXREAL_2,VALUED_0,XXREAL_3,RELAT_1,CKB1,CKB2,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB26,CKB27;
schemes NAT_1,FUNCT_2,XBOOLE_0;
registrations XBOOLE_0,SUBSET_1,ORDINAL1,FUNCT_2,NUMBERS,XREAL_0,MEMBERED,MEASURE1,VALUED_0,XXREAL_3,RELSET_1,CKB25;
constructors PARTFUN1,REAL_1,NAT_1,PROB_2,MEASURE1,MEASURE2,SUPINF_1,RELSET_1,CKB4,CKB17,CKB18,CKB19,CKB20,CKB27;
requirements NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve X for set;
theorem
(for S being (SigmaField of X) holds (for M being (sigma_Measure of S) holds ( COM M ) is_complete ( COM (S,M) )))
proof
let S being (SigmaField of X);
let M being (sigma_Measure of S);
(for A being (Subset of X) holds (for B being set holds (B in ( COM (S,M) ) implies ((A c= B & ( ( COM M ) . B ) = ( 0. )) implies A in ( COM (S,M) )))))
proof
let A being (Subset of X);
let B being set;
assume A1: B in ( COM (S,M) );
assume that
A2: A c= B
and
A3: ( ( COM M ) . B ) = ( 0. );
(ex B1 being set st (B1 in S & (ex C1 being (thin of M) st A = ( B1 \/ C1 ))))
proof
take ( {} );
consider B2 being set such that A4: B2 in S and A5: (ex C2 being (thin of M) st B = ( B2 \/ C2 )) by A1,CKB19:def 1;
A6: ( M . B2 ) = ( 0. ) by A3,A4,A5,CKB27:def 1;
consider C2 being (thin of M) such that A7: B = ( B2 \/ C2 ) by A5;
set C1 = ( ( A /\ B2 ) \/ ( A /\ C2 ) );
consider D2 being set such that A8: D2 in S and A9: C2 c= D2 and A10: ( M . D2 ) = ( 0. ) by CKB18:def 1;
set O = ( B2 \/ D2 );
( A /\ C2 ) c= C2 by XBOOLE_1:17;
then A11: (( A /\ B2 ) c= B2 & ( A /\ C2 ) c= D2) by A9,XBOOLE_1:1,XBOOLE_1:17;
(ex O being set st ((O in S & C1 c= O) & ( M . O ) = ( 0. )))
proof
reconsider B2,D2 as (Element of S) by A4,A8;
reconsider O1 = O as (Element of S) by A4,A8,FINSUB_1:def 1;
take O;
(( M . ( B2 \/ D2 ) ) <= ( ( 0. ) + ( 0. ) ) & ( 0. ) <= ( M . O1 )) by A6,A10,MEASURE1:33,MEASURE1:def 2;
hence thesis by A11,XBOOLE_1:13,XXREAL_0:1;
end;
then A12: C1 is (thin of M) by CKB18:def 1;
A = ( A /\ ( B2 \/ C2 ) ) by A2,A7,XBOOLE_1:28
.= ( ( {} ) \/ C1 ) by XBOOLE_1:23;
hence thesis by A12,PROB_1:4;
end;
hence thesis by CKB19:def 1;
end;
hence thesis by CKB17:def 1;
end;
