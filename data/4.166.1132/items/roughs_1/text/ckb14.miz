begin
theorem
Th4: (for A being non  diagonal non  empty RelStr holds (ex x,y being (Element of A) st (x <> y & [ x,y ] in (the InternalRel of A))))
proof
let A being non  diagonal non  empty RelStr;
now
assume A1: (for x,y being (Element of A) holds ([ x,y ] in (the InternalRel of A) implies x = y));
(for a,b being set holds ([ a,b ] in (the InternalRel of A) implies [ a,b ] in ( id (the carrier of A) )))
proof
let a being set;
let b being set;
assume A2: [ a,b ] in (the InternalRel of A);
then A3: a is (Element of A) by ZFMISC_1:87;
b is (Element of A) by A2,ZFMISC_1:87;
then a = b by A1,A2,A3;
hence thesis by A3,RELAT_1:def 10;
end;
then (the InternalRel of A) c= ( id (the carrier of A) ) by RELAT_1:def 3;
hence thesis by CKB3:def 1;
end;
hence thesis;
end;
