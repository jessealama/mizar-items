definition
let A being  finite Tolerance_Space;
let X being (FinSequence of ( bool (the carrier of A) ));
let x being (Element of A);
func FinSeqM (x,X) -> (FinSequence of ( REAL )) means 
:Def10: (( dom it ) = ( dom X ) & (for n being Nat holds (n in ( dom X ) implies ( it . n ) = ( ( MemberFunc (( X . n ),A) ) . x ))));
existence
proof
deffunc F(Nat) = ( ( MemberFunc (( X . $1 ),A) ) . x );
(ex z being (FinSequence of ( REAL )) st (( len z ) = ( len X ) & (for j being Nat holds (j in ( dom z ) implies ( z . j ) = F(j))))) from FINSEQ_2:sch 1;
then consider z being (FinSequence of ( REAL )) such that A1: ( len z ) = ( len X ) and A2: (for j being Nat holds (j in ( dom z ) implies ( z . j ) = F(j)));
take z;
thus ( dom z ) = ( Seg ( len z ) ) by FINSEQ_1:def 3
.= ( dom X ) by A1,FINSEQ_1:def 3;
let n being Nat;
assume n in ( dom X );
then A3: n in ( Seg ( len X ) ) by FINSEQ_1:def 3;
( dom z ) = ( Seg ( len X ) ) by A1,FINSEQ_1:def 3;
hence thesis by A2,A3;
end;
uniqueness
proof
let A1 being (FinSequence of ( REAL ));
let A2 being (FinSequence of ( REAL ));
assume that
A4: ( dom A1 ) = ( dom X )
and
A5: (for n being Nat holds (n in ( dom X ) implies ( A1 . n ) = ( ( MemberFunc (( X . n ),A) ) . x )))
and
A6: ( dom A2 ) = ( dom X )
and
A7: (for n being Nat holds (n in ( dom X ) implies ( A2 . n ) = ( ( MemberFunc (( X . n ),A) ) . x )));
(for n being Nat holds (n in ( dom A1 ) implies ( A1 . n ) = ( A2 . n )))
proof
let n being Nat;
assume A8: n in ( dom A1 );
reconsider n as (Element of ( NAT )) by ORDINAL1:def 12;
( A1 . n ) = ( ( MemberFunc (( X . n ),A) ) . x ) by A4,A5,A8
.= ( A2 . n ) by A4,A7,A8;
hence thesis;
end;
hence thesis by A4,A6,FINSEQ_1:13;
end;
end;
