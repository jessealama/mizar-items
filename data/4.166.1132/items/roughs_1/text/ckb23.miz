begin
theorem
Th7: (for X,x,y being set holds (for T being (Tolerance of X) holds (x in ( Class (T,y) ) implies y in ( Class (T,x) ))))
proof
let X being set,x being set,y being set;
let T being (Tolerance of X);
assume x in ( Class (T,y) );
then [ x,y ] in T by EQREL_1:19;
then [ y,x ] in T by EQREL_1:6;
hence thesis by EQREL_1:19;
end;
