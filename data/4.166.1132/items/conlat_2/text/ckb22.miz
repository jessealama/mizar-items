begin
theorem
Lm3: (for C being FormalContext holds (for CS being ConceptStr over C holds (( ( ObjectDerivation C ) . (the Extent of CS) ) = (the Intent of CS) implies CS is non empty)))
proof
let C being FormalContext;
let CS being ConceptStr over C;
assume A1: ( ( ObjectDerivation C ) . (the Extent of CS) ) = (the Intent of CS);
per cases ;
suppose (the Extent of CS) is  empty;

then (the Intent of CS) = (the carrier' of C) by A1,CONLAT_1:17;
hence thesis by CONLAT_1:def 8;
end;
suppose (the Extent of CS) is non empty;

hence thesis by CONLAT_1:def 8;
end;
end;
