theorem
Lm8: (for T1,T2 being TopSpace holds ((T1,T2 are_homeomorphic  & T1 is  finite-ind) implies (T2 is  finite-ind & ( ind T2 ) <= ( ind T1 ))))
proof
defpred P[ Nat ]
 means
(for T1,T2 being TopSpace holds (((T1,T2 are_homeomorphic  & T1 is  finite-ind) & ( ind T1 ) <= $1) implies (T2 is  finite-ind & ( ind T2 ) <= ( ind T1 ))));
let T1 being TopSpace;
let T2 being TopSpace;
assume that
A1: T1,T2 are_homeomorphic 
and
A2: T1 is  finite-ind;
reconsider i = ( ( ind T1 ) + 1 ) as Nat by A2,CKB25:1;
A3: (for n holds (P[ n ] implies P[ ( n + 1 ) ]))
proof
let n;
assume that
A4: P[ n ];
set n1 = ( n + 1 );
let T1 being TopSpace;
let T2 being TopSpace;
assume that
A5: T1,T2 are_homeomorphic 
and
A6: T1 is  finite-ind
and
A7: ( ind T1 ) <= ( n + 1 );
consider f being (Function of T1,T2) such that A8: f is  being_homeomorphism by A5,T_0TOPSP:def 1;
set f9 = ( f " );
A9: ( dom f ) = ( [#] T1 ) by A8,TOPS_2:def 5;
A10: ( rng f ) = ( [#] T2 ) by A8,TOPS_2:def 5;
B1: f is  onto by A10,FUNCT_2:def 3;
A11: f is  one-to-one by A8,TOPS_2:def 5;
per cases ;
suppose A12: ( [#] T1 ) = ( {} T1 );

then ( ind T1 ) = ( - 1 ) by CKB24:1;
hence thesis by A10,A12,CKB16:def 1,CKB24:1;
end;
suppose A13: ( [#] T1 ) <> ( {} T1 );

then T1 is non  empty;
then reconsider i = ( ind T1 ) as Nat by A6;
A14: T1 is non  empty by A13;
A15: T2 is non  empty by A9,A13;
per cases  by A7,NAT_1:8;
suppose i <= n;

hence thesis by A4,A5,A6;
end;
suppose A16: ( ind T1 ) = ( n + 1 );

A17: ( dom f9 ) = ( [#] T2 ) by A10,A11,A15,TOPS_2:49;
A18: (for p being (Point of T2) holds (for U being  open (Subset of T2) holds (p in U implies (ex W being  open (Subset of T2) st (((p in W & W c= U) & ( Fr W ) is  finite-ind) & ( ind ( Fr W ) ) <= ( n1 - 1 ))))))
proof
reconsider F = f as Function;
let p being (Point of T2);
let U being  open (Subset of T2);
assume p in U;
then A19: ( f9 . p ) in ( f9 .: U ) by A17,FUNCT_1:def 6;
(( f " U ) = ( f9 .: U ) & ( f " U ) is  open) by A8,A10,A11,A14,A15,TOPGRP_1:26,TOPS_2:55;
then consider W being  open (Subset of T1) such that A20: ( f9 . p ) in W and A21: W c= ( f " U ) and A22: ( Fr W ) is  finite-ind and A23: ( ind ( Fr W ) ) <= ( n1 - 1 ) by A6,A16,A19,CKB41:1;
set FW = ( Fr W );
A24: ( ind ( T1 | FW ) ) = ( ind FW ) by A22,CKB42:1;
FW,( f .: FW ) are_homeomorphic  by A8,METRIZTS:3;
then A25: ( T1 | FW ),( T2 | ( f .: FW ) ) are_homeomorphic  by METRIZTS:def 1;
then ( T2 | ( f .: FW ) ) is  finite-ind by A4,A22,A23,A24;
then A26: ( f .: FW ) is  finite-ind by CKB48:1;
( F " ) = f9 by B1,A11,TOPS_2:def 4;
then A27: ( f . ( f9 . p ) ) = p by A10,A11,A15,FUNCT_1:35;
( ind ( T2 | ( f .: FW ) ) ) <= ( ind ( T1 | FW ) ) by A4,A22,A23,A24,A25;
then A28: ( ind ( T2 | ( f .: FW ) ) ) <= ( n1 - 1 ) by A23,A24,XXREAL_0:2;
reconsider W9 = ( f .: W ) as  open (Subset of T2) by A8,A14,A15,TOPGRP_1:25;
take W9;
W9 c= ( f .: ( f " U ) ) by A21,RELAT_1:123;
hence (p in W9 & W9 c= U) by A9,A10,A20,A27,FUNCT_1:77,FUNCT_1:def 6;
( f .: FW ) = ( f .: ( ( Cl W ) \ W ) ) by TOPS_1:42
.= ( ( f .: ( Cl W ) ) \ W9 ) by A11,FUNCT_1:64
.= ( ( Cl W9 ) \ W9 ) by A8,A15,TOPS_2:60
.= ( Fr W9 ) by TOPS_1:42;
hence thesis by A26,A28,CKB42:1;
end;
then T2 is  finite-ind by CKB40:1;
hence thesis by A16,A18,CKB41:1;
end;
