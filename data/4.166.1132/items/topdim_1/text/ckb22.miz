begin
reserve T for TopSpace;
reserve A for (Subset of T);
reserve n for Nat;
reserve I for Integer;
definition
let T;
let A;
assume that
A1: A is  finite-ind;
func ind A -> Integer means 
:Def5: (A in ( ( Seq_of_ind T ) . ( it + 1 ) ) & (not A in ( ( Seq_of_ind T ) . it )));
existence
proof
defpred P[ Nat ]
 means
A in ( ( Seq_of_ind T ) . $1 );
A2: (ex n st P[ n ]) by A1,CKB6:def 1;
consider MIN being Nat such that A3: P[ MIN ] and A4: (for n holds (P[ n ] implies MIN <= n)) from NAT_1:sch 5(A2);
take I = ( MIN - 1 );
now
assume A5: A in ( ( Seq_of_ind T ) . I );
then I in ( dom ( Seq_of_ind T ) ) by FUNCT_1:def 2;
then reconsider i = I as (Element of ( NAT ));
MIN <= i by A4,A5;
then MIN < ( i + 1 ) by NAT_1:13;
hence contradiction;
end;
hence thesis by A3;
end;
uniqueness
proof
let I1 being Integer;
let I2 being Integer;
assume that
A6: A in ( ( Seq_of_ind T ) . ( I1 + 1 ) )
and
A7: (not A in ( ( Seq_of_ind T ) . I1 ))
and
A8: A in ( ( Seq_of_ind T ) . ( I2 + 1 ) )
and
A9: (not A in ( ( Seq_of_ind T ) . I2 ));
(( I1 + 1 ) in ( dom ( Seq_of_ind T ) ) & ( I2 + 1 ) in ( dom ( Seq_of_ind T ) )) by A6,A8,FUNCT_1:def 2;
then reconsider i11 = ( I1 + 1 ),i21 = ( I2 + 1 ) as (Element of ( NAT ));
A10: I1 <= I2
proof
assume I1 > I2;
then A11: I1 >= i21 by INT_1:7;
then reconsider i1 = I1 as (Element of ( NAT )) by INT_1:3;
( ( Seq_of_ind T ) . i21 ) c= ( ( Seq_of_ind T ) . i1 ) by A11,PROB_1:def 5;
hence contradiction by A7,A8;
end;
I2 <= I1
proof
assume I1 < I2;
then A12: I2 >= i11 by INT_1:7;
then reconsider i2 = I2 as (Element of ( NAT )) by INT_1:3;
( ( Seq_of_ind T ) . i11 ) c= ( ( Seq_of_ind T ) . i2 ) by A12,PROB_1:def 5;
hence contradiction by A6,A9;
end;
hence thesis by A10,XXREAL_0:1;
end;
end;
