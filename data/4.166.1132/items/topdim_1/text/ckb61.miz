begin
reserve T for TopSpace;
reserve A for (Subset of T);
reserve G for (Subset-Family of T);
theorem
(for Ga being (Subset-Family of ( T | A )) holds ((Ga is  finite-ind & Ga = G) implies (G is  finite-ind & ( ind G ) = ( ind Ga ))))
proof
let G1 being (Subset-Family of ( T | A ));
assume that
A1: G1 is  finite-ind
and
A2: G1 = G;
A3: (for B being (Subset of T) holds (B in G implies (B is  finite-ind & ( ind B ) <= ( ind G1 ))))
proof
let B being (Subset of T);
assume A4: B in G;
then reconsider B9 = B as (Subset of ( T | A )) by A2;
A5: B9 is  finite-ind by A1,A2,A4,CKB10:1;
then ( ind B ) = ( ind B9 ) by CKB52:1;
hence thesis by A1,A2,A4,A5,CKB32:1,CKB52:1;
end;
A6: ( - 1 ) <= ( ind G1 ) by A1,CKB32:1;
then A7: ( ind G ) <= ( ind G1 ) by A3,CKB32:1;
A8: G is  finite-ind by A3,A6,CKB32:1;
A9: (for B being (Subset of ( T | A )) holds (B in G1 implies (B is  finite-ind & ( ind B ) <= ( ind G ))))
proof
let B being (Subset of ( T | A ));
assume A10: B in G1;
then reconsider B9 = B as (Subset of T) by A2;
B9 is  finite-ind by A2,A3,A10;
then ( ind B ) = ( ind B9 ) by CKB51:1;
hence thesis by A1,A2,A6,A8,A10,CKB32:1;
end;
( - 1 ) <= ( ind G ) by A8,CKB32:1;
then ( ind G1 ) <= ( ind G ) by A9,CKB32:1;
hence thesis by A3,A6,A7,CKB32:1,XXREAL_0:1;
end;
