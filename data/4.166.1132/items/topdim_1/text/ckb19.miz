begin
theorem
Lm2: (for X being set holds X in ( ( Seq_of_ind ( 1TopSp X ) ) . 1 ))
proof
let X being set;
set T = ( 1TopSp X );
set CT = ( [#] T );
now
let p being (Point of ( T | CT ));
let U being  open (Subset of ( T | CT ));
assume that
A1: p in U;
take W = U;
A2: ( [#] ( T | CT ) ) = CT by PRE_TOPC:def 5;
then reconsider W9 = U as (Subset of T);
( W9 ` ) is  open by PRE_TOPC:def 2;
then (W9 is  open & W9 is  closed) by PRE_TOPC:def 2,TOPS_1:3;
then ( Fr W9 ) = ( {} T ) by TOPGEN_1:14;
then A3: ( ( Fr W9 ) /\ CT ) = ( {} );
W = ( W /\ ( [#] T ) ) by A2,XBOOLE_1:28;
then ( Fr W ) c= ( {} T ) by A3,CKB1:1;
then A4: ( Fr W ) = ( {} T );
( ( Seq_of_ind T ) . ( 0 ) ) = { ( {} T ) } by CKB3:def 1;
hence ((p in W & W c= U) & ( Fr W ) in ( ( Seq_of_ind T ) . ( 0 ) )) by A1,A4,TARSKI:def 1;
end;
then CT in ( ( Seq_of_ind T ) . ( ( 0 ) + 1 ) ) by CKB3:def 1;
hence thesis;
end;
