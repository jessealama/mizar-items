definition
let T;
let G;
assume that
A1: G is  finite-ind;
func ind G -> Integer means 
:Def6: ((G c= ( ( Seq_of_ind T ) . ( it + 1 ) ) & ( - 1 ) <= it) & (for i being Integer holds ((( - 1 ) <= i & G c= ( ( Seq_of_ind T ) . ( i + 1 ) )) implies it <= i)));
existence
proof
defpred P[ Nat ]
 means
G c= ( ( Seq_of_ind T ) . $1 );
A2: (ex n st P[ n ]) by A1,CKB8:def 1;
consider MIN being Nat such that A3: P[ MIN ] and A4: (for n holds (P[ n ] implies MIN <= n)) from NAT_1:sch 5(A2);
take I = ( MIN - 1 );
A5:now
let i being Integer;
assume that
A6: ( - 1 ) <= i
and
A7: G c= ( ( Seq_of_ind T ) . ( i + 1 ) );
( ( - 1 ) + 1 ) <= ( i + 1 ) by A6,XREAL_1:6;
then ( i + 1 ) in ( NAT ) by INT_1:3;
then reconsider I1 = ( i + 1 ) as Nat;
(MIN = ( I + 1 ) & MIN <= I1) by A4,A7;
hence I <= i by XREAL_1:6;
end;
I >= ( ( 0 ) - 1 ) by XREAL_1:9;
hence thesis by A3,A5;
end;
uniqueness
proof
let I1 being Integer;
let I2 being Integer;
assume ((((G c= ( ( Seq_of_ind T ) . ( I1 + 1 ) ) & ( - 1 ) <= I1) & (for i being Integer holds ((( - 1 ) <= i & G c= ( ( Seq_of_ind T ) . ( i + 1 ) )) implies I1 <= i))) & G c= ( ( Seq_of_ind T ) . ( I2 + 1 ) )) & (( - 1 ) <= I2 & (for i being Integer holds ((( - 1 ) <= i & G c= ( ( Seq_of_ind T ) . ( i + 1 ) )) implies I2 <= i))));
then (I2 <= I1 & I1 <= I2);
hence thesis by XXREAL_0:1;
