theorem
Lm6: (for A being (Subset of Tf) holds (( Tf | A ) is  finite-ind & ( ind ( Tf | A ) ) <= ( ind Tf )))
proof
defpred P[ Nat ]
 means
(for T holds ((T is  finite-ind & ( ind T ) = ( $1 - 1 )) implies (for A being (Subset of T) holds (( T | A ) is  finite-ind & ( ind ( T | A ) ) <= ( ind T )))));
( [#] Tf ) is  finite-ind by CKB16:def 1;
then reconsider i = ( ( ind Tf ) + 1 ) as Nat by CKB25:1;
A1: (for k being Nat holds ((for n holds (n < k implies P[ n ])) implies P[ k ]))
proof
let n9 being Nat;
assume that
A2: (for n holds (n < n9 implies P[ n ]));
per cases ;
suppose A3: n9 = ( 0 );

let T;
assume that
A4: T is  finite-ind
and
A5: ( ind T ) = ( n9 - 1 );
let A being (Subset of T);
( [#] T ) is  finite-ind by A4,CKB16:def 1;
then ( [#] T ) = ( {} T ) by A3,A5,CKB24:1;
hence thesis by A3,A5,CKB24:1;
end;
suppose n9 > ( 0 );

then reconsider n = ( n9 - 1 ) as Nat by NAT_1:20;
let T;
assume that
A6: T is  finite-ind
and
A7: ( ind T ) = ( n9 - 1 );
let A being (Subset of T);
set TA = ( T | A );
A8: ( [#] TA ) = A by PRE_TOPC:def 5;
A9:now
