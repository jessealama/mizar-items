constructors EQREL_1,ORDERS_3,RELSET_1,MSAFREE,CKB5,CKB6,CKB7,CKB10,CKB14,CKB16,CKB17,CKB20,CKB26,CKB30,CKB31,CKB38,CKB39,CKB40,CKB41,CKB42;
requirements BOOLE,SUBSET;
begin
reserve SM for  monotone OrderSortedSign;
reserve o for (OperSymbol of SM);
reserve o1 for (OperSymbol of SM);
reserve o2 for (OperSymbol of SM);
reserve w1 for (Element of ( (the carrier of SM) * ));
theorem
Th11: (SM is  regular iff (for o holds (for w1 holds (w1 <= ( the_arity_of o ) implies (ex o1 st o1 has_least_rank_for o,w1)))))
proof
hereby
assume A1: SM is  regular;
let o;
let w1;
assume A2: w1 <= ( the_arity_of o );
o is  regular by A1,CKB42:def 1;
then consider o1 such that A3: o1 has_least_args_for o,w1 by A2,CKB41:def 1;
take o1;
o1 has_least_sort_for o,w1
proof
thus A4: (o ~= o1 & w1 <= ( the_arity_of o1 )) by A3,CKB38:def 1;
let o2;
assume that
A5: o ~= o2
and
A6: w1 <= ( the_arity_of o2 );
A7: o1 ~= o2 by A4,A5,CKB15:1;
( the_arity_of o1 ) <= ( the_arity_of o2 ) by A3,A5,A6,CKB38:def 1;
hence thesis by A7,CKB30:def 1;
end;
hence o1 has_least_rank_for o,w1 by A3,CKB40:def 1;
end;
assume A8: (for o holds (for w1 holds (w1 <= ( the_arity_of o ) implies (ex o1 st o1 has_least_rank_for o,w1))));
let o;
thus o is  monotone;
let w1;
assume w1 <= ( the_arity_of o );
then consider o1 such that A9: o1 has_least_rank_for o,w1 by A8;
take o1;
thus thesis by A9,CKB40:def 1;
end;
