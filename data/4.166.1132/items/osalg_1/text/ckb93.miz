begin
theorem
Th34: (for SR being  regular  monotone OrderSortedSign holds (for op1,op2 being (OperSymbol of SR) holds (for w being (Element of ( (the carrier of SR) * )) holds (((op1 ~= op2 & w <= ( the_arity_of op1 )) & w <= ( the_arity_of op2 )) implies ( LBound (op1,w) ) = ( LBound (op2,w) )))))
proof
let SR being  regular  monotone OrderSortedSign;
let op1 being (OperSymbol of SR),op2 being (OperSymbol of SR);
let w being (Element of ( (the carrier of SR) * ));
assume that
A1: op1 ~= op2
and
A2: w <= ( the_arity_of op1 )
and
A3: w <= ( the_arity_of op2 );
set Lo1 = ( LBound (op1,w) );
set Lo2 = ( LBound (op2,w) );
A4: ( LBound (op1,w) ) has_least_args_for op1,w by A2,CKB49:def 1;
then A5: op1 ~= Lo1 by CKB38:def 1;
A6: ( LBound (op2,w) ) has_least_args_for op2,w by A3,CKB49:def 1;
then A7: (for o2 being (OperSymbol of SR) holds ((op2 ~= o2 & w <= ( the_arity_of o2 )) implies ( the_arity_of Lo2 ) <= ( the_arity_of o2 ))) by CKB38:def 1;
op2 ~= Lo2 by A6,CKB38:def 1;
then A8: op1 ~= Lo2 by A1,CKB15:1;
then A9: Lo1 ~= Lo2 by A5,CKB15:1;
w <= ( the_arity_of Lo1 ) by A4,CKB38:def 1;
then A10: ( the_arity_of Lo2 ) <= ( the_arity_of Lo1 ) by A1,A5,A7,CKB15:1;
then A11: ( the_result_sort_of Lo2 ) <= ( the_result_sort_of Lo1 ) by A9,CKB30:def 1;
w <= ( the_arity_of Lo2 ) by A6,CKB38:def 1;
then A12: ( the_arity_of Lo1 ) <= ( the_arity_of Lo2 ) by A4,A8,CKB38:def 1;
then A13: ( the_arity_of Lo1 ) = ( the_arity_of Lo2 ) by A10,CKB27:1;
( the_result_sort_of Lo1 ) <= ( the_result_sort_of Lo2 ) by A9,A12,CKB30:def 1;
then ( the_result_sort_of Lo1 ) = ( the_result_sort_of Lo2 ) by A11,ORDERS_2:2;
hence thesis by A9,A13,CKB16:def 1;
end;
