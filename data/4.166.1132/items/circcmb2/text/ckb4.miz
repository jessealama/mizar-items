begin
theorem
Th3: (for S being non  void  Circuit-like non  empty ManySortedSign holds (for A being  non-empty (Circuit of S) holds (for s being (State of A) holds (s is  stable implies (for n being Nat holds ( Following (s,n) ) = s)))))
proof
let S being non  void  Circuit-like non  empty ManySortedSign;
let A being  non-empty (Circuit of S);
let s being (State of A);
assume that
A1: s is  stable;
defpred P[ Nat ]
 means
( Following (s,$1) ) = s;
A2:now
let n being Nat;
assume P[ n ];
then ( Following ( Following (s,n) ) ) = s by A1,CIRCUIT2:def 6;
hence P[ ( n + 1 ) ] by FACIRC_1:12;
end;
A3: P[ ( 0 ) ] by FACIRC_1:11;
thus (for n being Nat holds P[ n ]) from NAT_1:sch 2(A3,A2);
end;
