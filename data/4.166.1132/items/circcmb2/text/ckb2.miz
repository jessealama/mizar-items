begin
theorem
Th1: (for X being  finite non  empty set holds (for n being Nat holds (for p being (FinSeqLen of n) holds (for f being (Function of ( n -tuples_on X ),X) holds (for o being (OperSymbol of ( 1GateCircStr (p,f) )) holds (for s being (State of ( 1GateCircuit (p,f) )) holds ( o depends_on_in s ) = ( s * p )))))))
proof
let X being  finite non  empty set;
let n being Nat;
let p being (FinSeqLen of n);
let f being (Function of ( n -tuples_on X ),X);
let o being (OperSymbol of ( 1GateCircStr (p,f) ));
let s being (State of ( 1GateCircuit (p,f) ));
( o depends_on_in s ) = ( s * ( the_arity_of o ) ) by CIRCUIT1:def 3
.= ( s * p ) by CIRCCOMB:41;
hence thesis;
end;
