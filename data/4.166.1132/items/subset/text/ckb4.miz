environ
vocabularies SUBSET_1,XBOOLE_0,TARSKI,ZFMISC_1;
notations TARSKI,XBOOLE_0,SUBSET_1,ZFMISC_1;
theorems TARSKI,SUBSET_1,ZFMISC_1,CKB1,CKB2,CKB3;
registrations SUBSET_1;
constructors TARSKI,SUBSET_1;
requirements BOOLE;
begin
theorem
(for a,b,c being set holds ((a in b & b is (Subset of c)) implies a is (Element of c)))
proof
let a being set,b being set,c being set;
assume that
A1: a in b
and
A2: b is (Subset of c);
b c= c by A2,CKB3:1;
then a in c by A1,TARSKI:def 3;
hence thesis by SUBSET_1:def 1;
end;
