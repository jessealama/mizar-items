begin
reserve s for set;
reserve c for  complex number;
reserve F for  complex-valued FinSequence;
reserve F1 for  complex-valued FinSequence;
definition
let F;
let c;
redefine func c * F -> (FinSequence of ( COMPLEX )) equals 
( ( c multcomplex ) * F );
coherence by CKB7:1;
compatibility
proof
let F1 being (FinSequence of ( COMPLEX ));
A1: ( dom ( c * F ) ) = ( dom F ) by VALUED_1:def 5;
A2: (( rng F ) c= ( COMPLEX ) & ( dom ( c multcomplex ) ) = ( COMPLEX )) by FUNCT_2:def 1;
then A3: ( dom ( ( c multcomplex ) * F ) ) = ( dom F ) by RELAT_1:27;
thus (F1 = ( c * F ) implies F1 = ( ( c multcomplex ) * F ))
proof
assume A4: F1 = ( c * F );
now
let s;
assume A5: s in ( dom F1 );
hence ( F1 . s ) = ( c * ( F . s ) ) by A4,VALUED_1:def 5
.= ( ( c multcomplex ) . ( F . s ) ) by CKB5:1
.= ( ( ( c multcomplex ) * F ) . s ) by A1,A4,A5,FUNCT_1:13;
end;
hence thesis by A1,A3,A4,FUNCT_1:2;
end;

assume A6: F1 = ( ( c multcomplex ) * F );
now
let s;
assume A7: s in ( dom F1 );
thus ( ( c * F ) . s ) = ( c * ( F . s ) ) by VALUED_1:6
.= ( ( c multcomplex ) . ( F . s ) ) by CKB5:1
.= ( ( ( c multcomplex ) * F ) . s ) by A6,A7,FUNCT_1:12;
end;
hence thesis by A1,A2,A6,FUNCT_1:2,RELAT_1:27;
end;
end;
