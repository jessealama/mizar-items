theorem
Th4: (for T being  with_suprema Poset holds (for D being non empty  directed  finite (Subset of T) holds ( sup D ) in D))
proof
let T being  reflexive  transitive  antisymmetric  with_suprema RelStr;
let D being non empty  directed  finite (Subset of T);
D c= D;
then reconsider E = D as  finite (Subset of D);
consider x being (Element of T) such that A1: x in D and A2: x is_>=_than E by WAYBEL_0:1;
A3: (for b being (Element of T) holds (D is_<=_than b implies b >= x)) by A1,LATTICE3:def 9;
(for c being (Element of T) holds ((D is_<=_than c & (for b being (Element of T) holds (D is_<=_than b implies b >= c))) implies c = x))
proof
let c being (Element of T);
assume that
A4: D is_<=_than c
and
A5: (for b being (Element of T) holds (D is_<=_than b implies b >= c));
A6: x >= c by A2,A5;
c >= x by A1,A4,LATTICE3:def 9;
hence thesis by A6,ORDERS_2:2;
end;
then  ex_sup_of D,T by A2,A3,YELLOW_0:def 7;
hence thesis by A1,A2,A3,YELLOW_0:def 9;
end;
