Th1: (for L being  complete LATTICE holds (for X,Y being (Subset of L) holds (Y is_coarser_than X implies ( "/\" (X,L) ) <= ( "/\" (Y,L) ))))
proof
let L being  complete LATTICE;
let X being (Subset of L);
let Y being (Subset of L);
assume that
A1: Y is_coarser_than X;
( "/\" (X,L) ) is_<=_than Y
proof
