theorem
Lm9: (for R1,R2 being non  empty RelStr holds ((( the RelStr of R1 ) = ( the RelStr of R2 ) & R1 is LATTICE) implies R2 is LATTICE))
proof
let R1 being non  empty RelStr;
let R2 being non  empty RelStr;
assume that
A1: ( the RelStr of R1 ) = ( the RelStr of R2 )
and
A2: R1 is LATTICE;
A3: R2 is  with_infima
proof
let x being (Element of R2);
let y being (Element of R2);
reconsider x9 = x,y9 = y as (Element of R1) by A1;
consider z9 being (Element of R1) such that A4: z9 <= x9 and A5: z9 <= y9 and A6: (for w9 being (Element of R1) holds ((w9 <= x9 & w9 <= y9) implies w9 <= z9)) by A2,LATTICE3:def 11;
reconsider z = z9 as (Element of R2) by A1;
take z;
thus (z <= x & z <= y) by A1,A4,A5,CKB1:1;
let w being (Element of R2);
reconsider w9 = w as (Element of R1) by A1;
assume that
A7: w <= x
and
A8: w <= y;
A9: w9 <= x9 by A1,A7,CKB1:1;
w9 <= y9 by A1,A8,CKB1:1;
then w9 <= z9 by A6,A9;
hence thesis by A1,CKB1:1;
end;
A10: R2 is  with_suprema
proof
let x being (Element of R2);
let y being (Element of R2);
reconsider x9 = x,y9 = y as (Element of R1) by A1;
consider z9 being (Element of R1) such that A11: z9 >= x9 and A12: z9 >= y9 and A13: (for w9 being (Element of R1) holds ((w9 >= x9 & w9 >= y9) implies w9 >= z9)) by A2,LATTICE3:def 10;
reconsider z = z9 as (Element of R2) by A1;
take z;
thus (z >= x & z >= y) by A1,A11,A12,CKB1:1;
let w being (Element of R2);
reconsider w9 = w as (Element of R1) by A1;
assume that
A14: w >= x
and
A15: w >= y;
A16: w9 >= x9 by A1,A14,CKB1:1;
w9 >= y9 by A1,A15,CKB1:1;
then w9 >= z9 by A13,A16;
hence thesis by A1,CKB1:1;
end;
A17: R2 is  reflexive
proof
let x being (Element of R2);
reconsider x9 = x as (Element of R1) by A1;
x9 <= x9 by A2,YELLOW_0:def 1;
hence thesis by A1,CKB1:1;
end;
A18: R2 is  transitive
proof
let x being (Element of R2);
let y being (Element of R2);
let z being (Element of R2);
reconsider x9 = x,y9 = y,z9 = z as (Element of R1) by A1;
assume that
A19: x <= y
and
A20: y <= z;
A21: x9 <= y9 by A1,A19,CKB1:1;
y9 <= z9 by A1,A20,CKB1:1;
then x9 <= z9 by A2,A21,YELLOW_0:def 2;
hence thesis by A1,CKB1:1;
end;
R2 is  antisymmetric
proof
let x being (Element of R2);
let y being (Element of R2);
reconsider x9 = x,y9 = y as (Element of R1) by A1;
assume that
A22: x <= y
and
A23: y <= x;
A24: x9 <= y9 by A1,A22,CKB1:1;
y9 <= x9 by A1,A23,CKB1:1;
hence thesis by A2,A24,YELLOW_0:def 3;
end;
hence thesis by A3,A10,A17,A18;
end;
