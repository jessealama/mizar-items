begin
theorem
(for D being non  empty set holds (for f,g being (FinSequence of D) holds (for n,m being (Element of ( NAT )) holds ((m >= n & g is_substring_of f,m) implies g is_substring_of f,n))))
proof
let D being non  empty set;
let f being (FinSequence of D);
let g being (FinSequence of D);
let n being (Element of ( NAT ));
let m being (Element of ( NAT ));
assume that
A1: m >= n
and
A2: g is_substring_of f,m;
now
per cases ;
case ( len g ) > ( 0 );
then consider i being (Element of ( NAT )) such that A3: m <= i and A4: i <= ( len f ) and A5: ( mid (f,i,( ( i -' 1 ) + ( len g ) )) ) = g by A2,CKB26:def 1;
n <= i by A1,A3,XXREAL_0:2;
hence thesis by A4,A5,CKB26:def 1;
end;
case ( len g ) <= ( 0 );
hence thesis by CKB26:def 1;
end;
end;
hence thesis;
end;
