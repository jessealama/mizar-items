begin
reserve n for Nat;
reserve p for (Point of ( TOP-REAL n ));
reserve q for (Point of ( TOP-REAL n ));
reserve u for (Point of ( TOP-REAL n ));
reserve w for (Point of ( TOP-REAL n ));
theorem
Th2: (((u in ( halfline (p,q) ) & w in ( halfline (p,q) )) & |. ( u - p ) .| = |. ( w - p ) .|) implies u = w)
proof
set r1 = u;
set r2 = w;
assume that
A1: r1 in ( halfline (p,q) )
and
A2: r2 in ( halfline (p,q) )
and
A3: |. ( r1 - p ) .| = |. ( r2 - p ) .|;
A4: n in ( NAT ) by ORDINAL1:def 12;
per cases ;
suppose p <> q;

then A5: |. ( q - p ) .| <> ( 0 ) by A4,TOPRNS_1:28;
consider a1 being  real number such that A6: r1 = ( ( ( 1 - a1 ) * p ) + ( a1 * q ) ) and A7: a1 >= ( 0 ) by A1,A4,TOPREAL9:26;
A8: ( abs a1 ) = a1 by A7,ABSVALUE:def 1;
(a1 in ( REAL ) & ( r1 - p ) = ( a1 * ( q - p ) )) by A6,CKB1:1,XREAL_0:def 1;
then A9: |. ( r1 - p ) .| = ( ( abs a1 ) * |. ( q - p ) .| ) by A4,TOPRNS_1:7;
consider a2 being  real number such that A10: r2 = ( ( ( 1 - a2 ) * p ) + ( a2 * q ) ) and A11: a2 >= ( 0 ) by A2,A4,TOPREAL9:26;
(a2 in ( REAL ) & ( r2 - p ) = ( a2 * ( q - p ) )) by A10,CKB1:1,XREAL_0:def 1;
then A12: |. ( r2 - p ) .| = ( ( abs a2 ) * |. ( q - p ) .| ) by A4,TOPRNS_1:7;
( abs a2 ) = a2 by A11,ABSVALUE:def 1;
then a1 = a2 by A3,A5,A9,A12,A8,XCMPLX_1:5;
hence thesis by A6,A10;
end;
suppose A13: p = q;

then r1 in { p } by A1,A4,TOPREAL9:29;
then A14: r1 = p by TARSKI:def 1;
r2 in { p } by A2,A4,A13,TOPREAL9:29;
hence thesis by A14,TARSKI:def 1;
end;
end;
