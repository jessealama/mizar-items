registrations ORDINAL1,RELSET_1,XREAL_0,NAT_1,INT_1,BINOP_2,XBOOLEAN,MARGREL1,XBOOLE_0,FINSEQ_2,CARD_1,FINSEQ_1;
constructors PARTFUN1,BINOP_1,SETWISEO,XXREAL_0,NAT_1,INT_1,FINSEQ_4,FINSOP_1,SERIES_1,BINARITH,BINOP_2,NAT_D,RELSET_1,CKB2,CKB3,CKB4,CKB5,CKB6;
requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve i for Nat;
reserve m for non  empty Nat;
theorem
Th9: ( Bin1 ( m + 1 ) ) = ( ( Bin1 m ) ^ <* ( FALSE ) *> )
proof
A1: (( len ( Bin1 ( m + 1 ) ) ) = ( m + 1 ) & ( len ( ( Bin1 m ) ^ <* ( FALSE ) *> ) ) = ( m + 1 )) by CARD_1:def 7;
(for i holds (i in ( Seg ( m + 1 ) ) implies ( ( Bin1 ( m + 1 ) ) /. i ) = ( ( ( Bin1 m ) ^ <* ( FALSE ) *> ) /. i )))
proof
let i;
assume that
A2: i in ( Seg ( m + 1 ) );
per cases  by A2,FINSEQ_2:7;
suppose A3: i in ( Seg m );

thus ( ( Bin1 ( m + 1 ) ) /. i ) = ( ( ( Bin1 m ) ^ <* ( FALSE ) *> ) /. i )
proof
per cases ;
suppose A4: i = 1;

( ( ( Bin1 m ) ^ <* ( FALSE ) *> ) /. i ) = ( ( Bin1 m ) /. i ) by A3,BINARITH:1
.= ( TRUE ) by A3,A4,CKB11:1;
hence thesis by A2,A4,CKB11:1;
end;
suppose A5: i <> 1;

( ( ( Bin1 m ) ^ <* ( FALSE ) *> ) /. i ) = ( ( Bin1 m ) /. i ) by A3,BINARITH:1
.= ( FALSE ) by A3,A5,CKB12:1;
hence thesis by A2,A5,CKB12:1;
end;
end;

end;
suppose A6: i = ( m + 1 );

1 <> ( m + 1 ) by NAT_1:14;
then ( ( Bin1 ( m + 1 ) ) /. i ) = ( FALSE ) by A2,A6,CKB12:1;
hence thesis by A6,BINARITH:2;
end;
end;
hence thesis by A1,CKB1:1;
end;
