begin
reserve I for (Element of ( Segm 14 ));
theorem
(for x being (Element of ( SCMPDS-Instr )) holds (for mk being (Element of ( SCM-Data-Loc )) holds (for r being Integer holds (x = [ I,( {} ),<* mk,r *> ] implies (( x P21address ) = mk & ( x P22const ) = r)))))
proof
let x being (Element of ( SCMPDS-Instr ));
let mk being (Element of ( SCM-Data-Loc ));
let r being Integer;
r in ( INT ) by INT_1:def 2;
then A1: (mk is (Element of ( ( SCM-Data-Loc ) \/ ( INT ) )) & r is (Element of ( ( SCM-Data-Loc ) \/ ( INT ) ))) by XBOOLE_0:def 3;
assume A2: x = [ I,( {} ),<* mk,r *> ];
then consider f being (FinSequence of ( ( SCM-Data-Loc ) \/ ( INT ) )) such that A3: f = ( x `3_3 ) and A4: ( x P21address ) = ( f /. 1 ) by CKB34:def 1;
f = <* mk,r *> by A2,A3,RECDEF_2:def 3;
hence ( x P21address ) = mk by A4,A1,FINSEQ_4:17;
consider f being (FinSequence of ( ( SCM-Data-Loc ) \/ ( INT ) )) such that A5: f = ( x `3_3 ) and A6: ( x P22const ) = ( f /. 2 ) by A2,CKB35:def 1;
f = <* mk,r *> by A2,A5,RECDEF_2:def 3;
hence thesis by A1,A6,FINSEQ_4:17;
end;
