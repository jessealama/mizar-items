begin
reserve p for (FinSequence of ( NAT ));
reserve q for (FinSequence of ( NAT ));
reserve r for (FinSequence of ( NAT ));
reserve s for (FinSequence of ( NAT ));
theorem
Th1: (for p,q,r,s being FinSequence holds (( p ^ q ) = ( s ^ r ) implies p,s are_c=-comparable ))
proof
let p being FinSequence;
let q being FinSequence;
let r being FinSequence;
let s being FinSequence;
assume A1: ( p ^ q ) = ( s ^ r );
then (p is_a_prefix_of ( s ^ r ) & s is_a_prefix_of ( p ^ q )) by TREES_1:1;
hence thesis by A1,TREES_2:1;
end;
