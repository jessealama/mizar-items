end;
end;
hence contradiction;
end;
then ((( ( LSeg (f,1) ) /\ ( LSeg (f,2) ) ) = { ( f . 1 ) } & (( f . 1 ) = ( f . 2 ) or ( f . 1 ) = ( f . ( 2 + 1 ) ))) or (( ( LSeg (f,1) ) /\ ( LSeg (f,2) ) ) = { ( f . ( 1 + 1 ) ) } & (( f . ( 1 + 1 ) ) = ( f . 2 ) or ( f . ( 1 + 1 ) ) = ( f . ( 2 + 1 ) )))) by A1,A8,CKB18:def 1;
then A22: ( ( LSeg (f,1) ) /\ ( LSeg (f,( 1 + 1 )) ) ) c= { ( f /. ( 1 + 1 ) ) } by A3,A13,A15,A5,FINSEQ_4:15,FUNCT_1:def 4;
A23: ( ( len f ) - 1 ) = ( ( len f ) -' 1 ) by A3,XREAL_1:233,XXREAL_0:2;
