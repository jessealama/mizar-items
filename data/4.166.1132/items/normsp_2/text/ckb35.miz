begin
reserve X for RealNormSpace;
theorem
(for X being RealNormSpace holds (for x being (Point of X) holds (for r being Real holds (for V being (Subset of ( TopSpaceNorm X )) holds (V = { y where y is (Point of X): ||. ( x - y ) .|| <= r } implies V is  closed)))))
proof
let X being RealNormSpace;
let x being (Point of X);
let r being Real;
let V being (Subset of ( TopSpaceNorm X ));
assume A1: V = { y where y is (Point of X): ||. ( x - y ) .|| <= r };
reconsider z = x as (Element of ( MetricSpaceNorm X ));
(ex t being (Point of X) st (t = x & ( cl_Ball (z,r) ) = { y where y is (Point of X): ||. ( t - y ) .|| <= r })) by CKB9:1;
hence thesis by A1,TOPREAL6:57;
end;
