begin
reserve X for RealNormSpace;
theorem
Th26: (for X being RealNormSpace holds (for S being (sequence of ( TopSpaceNorm X )) holds (for St being (sequence of ( LinearTopSpaceNorm X )) holds (for x being (Point of ( TopSpaceNorm X )) holds (for xt being (Point of ( LinearTopSpaceNorm X )) holds ((S = St & x = xt) implies (St is_convergent_to xt iff S is_convergent_to x)))))))
proof
let X being RealNormSpace;
let S being (sequence of ( TopSpaceNorm X ));
let St being (sequence of ( LinearTopSpaceNorm X ));
let x being (Point of ( TopSpaceNorm X ));
let xt being (Point of ( LinearTopSpaceNorm X ));
assume that
A1: S = St
and
A2: x = xt;
A3:now
assume A4: S is_convergent_to x;
now
let U1t being (Subset of ( LinearTopSpaceNorm X ));
assume that
A5: U1t is  open
and
A6: xt in U1t;
reconsider U1 = U1t as  open (Subset of ( TopSpaceNorm X )) by A5,CKB29:def 1,CKB31:1;
consider n being (Element of ( NAT )) such that A7: (for m being (Element of ( NAT )) holds (n <= m implies ( S . m ) in U1)) by A2,A4,A6,FRECHET:def 3;
take n;
let m being (Element of ( NAT ));
assume n <= m;
hence ( St . m ) in U1t by A1,A7;
end;
hence St is_convergent_to xt by FRECHET:def 3;
end;
now
assume A8: St is_convergent_to xt;
now
let U1 being (Subset of ( TopSpaceNorm X ));
assume that
A9: U1 is  open
and
A10: x in U1;
reconsider U1t = U1 as  open (Subset of ( LinearTopSpaceNorm X )) by A9,CKB29:def 1,CKB31:1;
consider n being (Element of ( NAT )) such that A11: (for m being (Element of ( NAT )) holds (n <= m implies ( St . m ) in U1t)) by A2,A8,A10,FRECHET:def 3;
take n;
let m being (Element of ( NAT ));
assume n <= m;
hence ( S . m ) in U1 by A1,A11;
end;
hence S is_convergent_to x by FRECHET:def 3;
end;
hence thesis by A3;
end;
