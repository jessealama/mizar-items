begin
theorem
(for X,Y being RealNormSpace holds (for f being (Function of ( TopSpaceNorm X ),( TopSpaceNorm Y )) holds (for ft being (Function of ( LinearTopSpaceNorm X ),( LinearTopSpaceNorm Y )) holds (f = ft implies (f is  continuous iff ft is  continuous)))))
proof
let X being RealNormSpace,Y being RealNormSpace;
let f being (Function of ( TopSpaceNorm X ),( TopSpaceNorm Y ));
let ft being (Function of ( LinearTopSpaceNorm X ),( LinearTopSpaceNorm Y ));
assume A1: f = ft;
hereby
assume A2: f is  continuous;
now
let xt being (Point of ( LinearTopSpaceNorm X ));
reconsider x = xt as (Point of ( TopSpaceNorm X )) by CKB29:def 1;
f is_continuous_at x by A2,TMAP_1:44;
hence ft is_continuous_at xt by A1,CKB51:1;
end;
hence ft is  continuous by TMAP_1:44;
end;
assume A3: ft is  continuous;
now
let x being (Point of ( TopSpaceNorm X ));
reconsider xt = x as (Point of ( LinearTopSpaceNorm X )) by CKB29:def 1;
ft is_continuous_at xt by A3,TMAP_1:44;
hence f is_continuous_at x by A1,CKB51:1;
end;
hence thesis by TMAP_1:44;
end;
