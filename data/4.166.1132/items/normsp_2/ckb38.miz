constructors SETFAM_1,DOMAIN_1,REAL_1,SQUARE_1,NAT_1,NEWTON,CARD_3,COMPTS_1,CONNSP_2,PCOMPS_1,TBSP_1,RUSUB_4,CONVEX1,TMAP_1,METRIC_6,WAYBEL_3,YELLOW_8,FRECHET,FRECHET2,NFCONT_1,RLTOPSP1,SEQ_1,NORMSP_1,RELSET_1,TOPS_2,CKB2,CKB7,CKB13,CKB29;
requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
theorem
Th25: (for X being RealNormSpace holds (for S being (Subset-Family of ( TopSpaceNorm X )) holds (for St being (Subset-Family of ( LinearTopSpaceNorm X )) holds (for x being (Point of ( TopSpaceNorm X )) holds (for xt being (Point of ( LinearTopSpaceNorm X )) holds ((S = St & x = xt) implies (St is (Basis of xt) iff S is (Basis of x))))))))
proof
let X being RealNormSpace;
let S being (Subset-Family of ( TopSpaceNorm X ));
let St being (Subset-Family of ( LinearTopSpaceNorm X ));
let x being (Point of ( TopSpaceNorm X ));
let xt being (Point of ( LinearTopSpaceNorm X ));
assume that
A1: S = St
and
A2: x = xt;
A3: ( Intersect S ) = ( Intersect St ) by A1,CKB29:def 1;
hereby
assume A4: St is (Basis of xt);
then St c= (the topology of ( LinearTopSpaceNorm X )) by TOPS_2:64;
then A5: S c= (the topology of ( TopSpaceNorm X )) by A1,CKB29:def 1;
A6:now
let U being (Subset of ( TopSpaceNorm X ));
assume that
A7: U is  open
and
A8: x in U;
reconsider Ut = U as  open (Subset of ( LinearTopSpaceNorm X )) by A7,CKB29:def 1,CKB31:1;
consider Vt being (Subset of ( LinearTopSpaceNorm X )) such that A9: (Vt in St & Vt c= Ut) by A2,A4,A8,YELLOW_8:def 1;
reconsider V = Vt as (Subset of ( TopSpaceNorm X )) by CKB29:def 1;
take V;
thus (V in S & V c= U) by A1,A9;
end;
x in ( Intersect S ) by A2,A3,A4,YELLOW_8:def 1;
hence S is (Basis of x) by A5,A6,TOPS_2:64,YELLOW_8:def 1;
end;
assume A10: S is (Basis of x);
then S c= (the topology of ( TopSpaceNorm X )) by TOPS_2:64;
then A11: St c= (the topology of ( LinearTopSpaceNorm X )) by A1,CKB29:def 1;
A12:now
let Ut being (Subset of ( LinearTopSpaceNorm X ));
assume that
A13: Ut is  open
and
A14: xt in Ut;
reconsider U = Ut as  open (Subset of ( TopSpaceNorm X )) by A13,CKB29:def 1,CKB31:1;
consider V being (Subset of ( TopSpaceNorm X )) such that A15: (V in S & V c= U) by A2,A10,A14,YELLOW_8:def 1;
reconsider Vt = V as (Subset of ( LinearTopSpaceNorm X )) by CKB29:def 1;
take Vt;
thus (Vt in St & Vt c= Ut) by A1,A15;
end;
xt in ( Intersect St ) by A2,A3,A10,YELLOW_8:def 1;
hence thesis by A11,A12,TOPS_2:64,YELLOW_8:def 1;
end;
