environ
vocabularies PRE_TOPC,SUBSET_1,TOPS_1,TARSKI,RCOMP_1,XBOOLE_0,SETFAM_1,PCOMPS_1,ZFMISC_1,STRUCT_0,FINSET_1,FINSEQ_1,RELAT_1,NAT_1,XXREAL_0,ARYTM_3,CARD_1,FUNCT_1,TDLAT_1,LATTICES,EQREL_1,PBOOLE,LATTICE3,REWRITE1,TDLAT_2;
notations TARSKI,XBOOLE_0,SUBSET_1,RELAT_1,SETFAM_1,STRUCT_0,FUNCT_1,NUMBERS,FINSET_1,FINSEQ_1,NAT_1,PRE_TOPC,TOPS_1,TOPS_2,PCOMPS_1,BINOP_1,LATTICES,LATTICE3,TDLAT_1,XXREAL_0,CKB7,CKB17;
definitions LATTICE3,RELAT_1,STRUCT_0,CKB17;
theorems TARSKI,ZFMISC_1,SETFAM_1,FINSEQ_1,FUNCT_1,NAT_1,PRE_TOPC,TOPS_1,TOPS_2,PCOMPS_1,LATTICES,LATTICE2,LATTICE3,TDLAT_1,RELAT_1,XBOOLE_0,XBOOLE_1,XREAL_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23;
schemes SUBSET_1,NAT_1;
registrations XBOOLE_0,SUBSET_1,MEMBERED,STRUCT_0,PRE_TOPC,TOPS_1,TDLAT_1,XREAL_0,FINSEQ_1;
constructors SETFAM_1,XXREAL_0,NAT_1,MEMBERED,FINSEQ_1,TOPS_1,TOPS_2,PCOMPS_1,LATTICE3,TDLAT_1,CKB17;
requirements NUMERALS,BOOLE,SUBSET,ARITHM;
begin
reserve T for TopSpace;
theorem
Th22: (for F,G being (Subset-Family of T) holds ( Int ( F \/ G ) ) = ( ( Int F ) \/ ( Int G ) ))
proof
let F being (Subset-Family of T),G being (Subset-Family of T);
(for X being set holds (X in ( Int ( F \/ G ) ) iff X in ( ( Int F ) \/ ( Int G ) )))
proof
let X being set;
A1:now
assume A2: X in ( ( Int F ) \/ ( Int G ) );
now
per cases  by A2,XBOOLE_0:def 3;
suppose A3: X in ( Int F );

then reconsider X0 = X as (Subset of T);
(ex W being (Subset of T) st (X0 = ( Int W ) & W in ( F \/ G )))
proof
consider Z being (Subset of T) such that A4: X0 = ( Int Z ) and A5: Z in F by A3,CKB17:def 1;
take Z;
thus thesis by A4,A5,XBOOLE_0:def 3;
end;
hence X in ( Int ( F \/ G ) ) by CKB17:def 1;
end;
suppose A6: X in ( Int G );

then reconsider X0 = X as (Subset of T);
(ex W being (Subset of T) st (X0 = ( Int W ) & W in ( F \/ G )))
proof
consider Z being (Subset of T) such that A7: X0 = ( Int Z ) and A8: Z in G by A6,CKB17:def 1;
take Z;
thus thesis by A7,A8,XBOOLE_0:def 3;
end;
hence X in ( Int ( F \/ G ) ) by CKB17:def 1;
end;
end;
hence X in ( Int ( F \/ G ) );
end;
now
assume A9: X in ( Int ( F \/ G ) );
then reconsider X0 = X as (Subset of T);
consider W being (Subset of T) such that A10: X0 = ( Int W ) and A11: W in ( F \/ G ) by A9,CKB17:def 1;
now
per cases  by A11,XBOOLE_0:def 3;
suppose W in F;

then X0 in ( Int F ) by A10,CKB17:def 1;
hence X0 in ( ( Int F ) \/ ( Int G ) ) by XBOOLE_0:def 3;
end;
suppose W in G;

then X0 in ( Int G ) by A10,CKB17:def 1;
hence X0 in ( ( Int F ) \/ ( Int G ) ) by XBOOLE_0:def 3;
end;
end;
hence X in ( ( Int F ) \/ ( Int G ) );
end;
hence thesis by A1;
end;
hence thesis by TARSKI:1;
end;
