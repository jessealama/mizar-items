begin
reserve T for TopSpace;
theorem
Th20: (for A being (Subset of T) holds (for F being (Subset-Family of T) holds (F = { A } implies ( Int F ) = { ( Int A ) })))
proof
let A being (Subset of T);
let F being (Subset-Family of T);
reconsider C = ( Int F ) as set;
assume A1: F = { A };
(for B being set holds (B in C iff B = ( Int A )))
proof
let B being set;
A2:now
assume A3: B = ( Int A );
(ex M being (Subset of T) st (B = ( Int M ) & M in F))
proof
take A;
thus thesis by A1,A3,TARSKI:def 1;
end;
hence B in C by CKB17:def 1;
end;
now
assume A4: B in C;
then reconsider B0 = B as (Subset of T);
(ex M being (Subset of T) st (B0 = ( Int M ) & M in F)) by A4,CKB17:def 1;
hence B = ( Int A ) by A1,TARSKI:def 1;
end;
hence thesis by A2;
end;
hence thesis by TARSKI:def 1;
end;
