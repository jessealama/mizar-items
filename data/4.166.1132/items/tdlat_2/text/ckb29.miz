begin
reserve T for TopSpace;
theorem
(for F being (Subset-Family of T) holds ( meet ( Int F ) ) c= ( meet F ))
proof
let F being (Subset-Family of T);
A1: (for A being set holds (A in F implies ( meet ( Int F ) ) c= A))
proof
let A being set;
assume A2: A in F;
then reconsider A0 = A as (Subset of T);
set C = ( Int A0 );
C in { P where P is (Subset of T): (ex Q being (Subset of T) st (P = ( Int Q ) & Q in F)) } by A2;
then C in ( Int F ) by CKB18:1;
then A3: ( meet ( Int F ) ) c= C by SETFAM_1:3;
C c= A0 by TOPS_1:16;
hence thesis by A3,XBOOLE_1:1;
end;
now
per cases ;
suppose F = ( {} );

hence thesis by CKB21:1;
end;
suppose F <> ( {} );

hence thesis by A1,SETFAM_1:5;
end;
end;
hence thesis;
end;
