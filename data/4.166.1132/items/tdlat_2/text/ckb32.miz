begin
reserve T for TopSpace;
theorem
(for F being (Subset-Family of T) holds (F is  finite implies ( Int ( meet F ) ) = ( meet ( Int F ) )))
proof
let F being (Subset-Family of T);
assume A1: F is  finite;
A2: ( meet ( Int F ) ) c= ( Int ( meet F ) )
proof
consider p being FinSequence such that A3: ( rng p ) = F by A1,FINSEQ_1:52;
consider n being Nat such that A4: ( dom p ) = ( Seg n ) by FINSEQ_1:def 2;
defpred X[ Nat ]
 means
(for G being (Subset-Family of T) holds (((G = ( p .: ( Seg $1 ) ) & $1 <= n) & 1 <= n) implies ( meet ( Int G ) ) c= ( Int ( meet G ) )));
A5: (for k being Nat holds (X[ k ] implies X[ ( k + 1 ) ]))
proof
let k being Nat;
assume A6: (for G being (Subset-Family of T) holds (((G = ( p .: ( Seg k ) ) & k <= n) & 1 <= n) implies ( meet ( Int G ) ) c= ( Int ( meet G ) )));
let G being (Subset-Family of T);
assume A7: G = ( p .: ( Seg ( k + 1 ) ) );
assume that
A8: ( k + 1 ) <= n
and
A9: 1 <= n;
A10:now
reconsider G2 = ( Im (p,( k + 1 )) ) as (Subset-Family of T) by A3,RELAT_1:111,TOPS_2:2;
reconsider G1 = ( p .: ( Seg k ) ) as (Subset-Family of T) by A3,RELAT_1:111,TOPS_2:2;
A11: ( ( 0 ) + 1 ) = 1;
( 0 ) <= k by NAT_1:2;
then 1 <= ( k + 1 ) by A11,XREAL_1:7;
then A12: ( k + 1 ) in ( dom p ) by A4,A8,FINSEQ_1:1;
then A13: G2 = { ( p . ( k + 1 ) ) } by FUNCT_1:59;
then ( meet G2 ) = ( p . ( k + 1 ) ) by SETFAM_1:10;
then reconsider G3 = ( p . ( k + 1 ) ) as (Subset of T);
A14: ( meet ( Int G2 ) ) = ( meet { ( Int G3 ) } ) by A13,CKB22:1
.= ( Int G3 ) by SETFAM_1:10
.= ( Int ( meet G2 ) ) by A13,SETFAM_1:10;
( k + 1 ) <= ( n + 1 ) by A8,NAT_1:12;
then k <= n by XREAL_1:6;
then A15: ( meet ( Int G1 ) ) c= ( Int ( meet G1 ) ) by A6,A9;
assume ( 0 ) < k;
then A16: ( ( 0 ) + 1 ) <= k by NAT_1:13;
then A17: k in ( Seg k ) by FINSEQ_1:1;
k <= n by A8,NAT_1:13;
then k in ( dom p ) by A4,A16,FINSEQ_1:1;
then A18: ( p . k ) in G1 by A17,FUNCT_1:def 6;
( k + 1 ) in { ( k + 1 ) } by TARSKI:def 1;
then A19: ( p . ( k + 1 ) ) in G2 by A12,FUNCT_1:def 6;
then A20: ( Int G2 ) <> ( {} ) by CKB21:1;
A21: ( p .: ( Seg ( k + 1 ) ) ) = ( p .: ( ( Seg k ) \/ { ( k + 1 ) } ) ) by FINSEQ_1:9
.= ( ( p .: ( Seg k ) ) \/ ( p .: { ( k + 1 ) } ) ) by RELAT_1:120;
then ( Int ( meet G ) ) = ( Int ( ( meet G1 ) /\ ( meet G2 ) ) ) by A7,A18,A19,SETFAM_1:9
.= ( ( Int ( meet G1 ) ) /\ ( Int ( meet G2 ) ) ) by TOPS_1:17;
then A22: ( ( meet ( Int G1 ) ) /\ ( meet ( Int G2 ) ) ) c= ( Int ( meet G ) ) by A14,A15,XBOOLE_1:27;
( Int G1 ) <> ( {} ) by A18,CKB21:1;
then ( meet ( ( Int G1 ) \/ ( Int G2 ) ) ) c= ( Int ( meet G ) ) by A20,A22,SETFAM_1:9;
hence thesis by A7,A21,CKB24:1;
end;
now
assume A23: k = ( 0 );
then 1 in ( dom p ) by A4,A8,FINSEQ_1:1;
then A24: ( Im (p,1) ) = { ( p . 1 ) } by FUNCT_1:59;
then ( union G ) = ( p . 1 ) by A7,A23,FINSEQ_1:2,ZFMISC_1:25;
then reconsider G1 = ( p . 1 ) as (Subset of T);
( Int ( meet G ) ) = ( Int G1 ) by A7,A23,A24,FINSEQ_1:2,SETFAM_1:10
.= ( meet { ( Int G1 ) } ) by SETFAM_1:10
.= ( meet ( Int G ) ) by A7,A23,A24,CKB22:1,FINSEQ_1:2;
hence thesis;
end;
hence thesis by A10,NAT_1:3;
end;
A25: X[ ( 0 ) ]
proof
let G being (Subset-Family of T);
assume that
A26: G = ( p .: ( Seg ( 0 ) ) )
and
( 0 ) <= n
and
1 <= n;
G = ( {} ) by A26,RELAT_1:116;
hence thesis by CKB21:1,SETFAM_1:1;
end;
A27: (for k being Nat holds X[ k ]) from NAT_1:sch 2(A25,A5);
A28:now
assume A29: 1 <= n;
F = ( p .: ( Seg n ) ) by A3,A4,RELAT_1:113;
hence thesis by A27,A29;
end;
A30:now
assume n <> ( 0 );
then A31: ( 0 ) < n by NAT_1:3;
1 = ( ( 0 ) + 1 );
hence 1 <= n by A31,NAT_1:13;
end;
now
assume n = ( 0 );
then ( Seg n ) = ( {} );
then F = ( p .: ( {} ) ) by A3,A4,RELAT_1:113;
then F = ( {} ) by RELAT_1:116;
hence thesis by CKB21:1,SETFAM_1:1;
end;
hence thesis by A28,A30;
end;
( Int ( meet F ) ) c= ( meet ( Int F ) ) by CKB31:1;
hence thesis by A2,XBOOLE_0:def 10;
end;
