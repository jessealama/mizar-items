registrations XBOOLE_0,SUBSET_1,MEMBERED,STRUCT_0,PRE_TOPC,TOPS_1,TDLAT_1,XREAL_0,FINSEQ_1;
constructors SETFAM_1,XXREAL_0,NAT_1,MEMBERED,FINSEQ_1,TOPS_1,TOPS_2,PCOMPS_1,LATTICE3,TDLAT_1,CKB17;
requirements NUMERALS,BOOLE,SUBSET,ARITHM;
begin
reserve T for TopSpace;
theorem
(for F being (Subset-Family of T) holds ( Int F ) = ( Int ( Int F ) ))
proof
let F being (Subset-Family of T);
A1: ( Int ( Int F ) ) = { D where D is (Subset of T): (ex B being (Subset of T) st (D = ( Int B ) & B in ( Int F ))) } by CKB18:1;
A2: ( Int F ) = { A where A is (Subset of T): (ex B being (Subset of T) st (A = ( Int B ) & B in F)) } by CKB18:1;
(for X being set holds (X in ( Int F ) iff X in ( Int ( Int F ) )))
proof
let X being set;
A3:now
assume A4: X in ( Int ( Int F ) );
then reconsider C = X as (Subset of T);
(ex Q being (Subset of T) st (Q = C & (ex B being (Subset of T) st (Q = ( Int B ) & B in ( Int F ))))) by A1,A4;
then consider B being (Subset of T) such that A5: C = ( Int B ) and A6: B in ( Int F );
(ex Q being (Subset of T) st (Q = B & (ex R being (Subset of T) st (Q = ( Int R ) & R in F)))) by A2,A6;
hence X in ( Int F ) by A5,CKB17:def 1;
end;
now
assume A7: X in ( Int F );
then reconsider C = X as (Subset of T);
consider B being (Subset of T) such that A8: C = ( Int B ) and A9: B in F by A7,CKB17:def 1;
A10: C = ( Int ( Int B ) ) by A8;
( Int B ) in ( Int F ) by A9,CKB17:def 1;
hence X in ( Int ( Int F ) ) by A1,A10;
end;
hence thesis by A3;
end;
hence thesis by TARSKI:1;
end;
