begin
reserve T for non empty TopSpace;
theorem
(for F being (Subset-Family of T) holds (F is  closed-domains-family implies (for X being (Subset of ( Closed_Domains_Lattice T )) holds (X = F implies ((X <> ( {} ) implies ( "/\" (X,( Closed_Domains_Lattice T )) ) = ( Cl ( Int ( meet F ) ) )) & (X = ( {} ) implies ( "/\" (X,( Closed_Domains_Lattice T )) ) = ( [#] T )))))))
proof
let F being (Subset-Family of T);
assume A1: F is  closed-domains-family;
let X being (Subset of ( Closed_Domains_Lattice T ));
assume A2: X = F;
thus (X <> ( {} ) implies ( "/\" (X,( Closed_Domains_Lattice T )) ) = ( Cl ( Int ( meet F ) ) ))
proof
set A = ( Cl ( Int ( meet F ) ) );
A is  closed_condensed by A1,CKB80:1;
then A in { C where C is (Subset of T): C is  closed_condensed };
then A3: A in ( Closed_Domains_of T ) by TDLAT_1:def 5;
then reconsider a = A as (Element of ( Closed_Domains_Lattice T )) by CKB99:1;
A4: a is_less_than X
proof
let b being (Element of ( Closed_Domains_Lattice T ));
reconsider B = b as (Element of ( Closed_Domains_of T )) by CKB99:1;
assume b in X;
then A c= B by A1,A2,CKB78:1,CKB82:1;
hence thesis by A3,CKB102:1;
end;
assume A5: X <> ( {} );
A6: (for b being (Element of ( Closed_Domains_Lattice T )) holds (b is_less_than X implies b [= a))
proof
let b being (Element of ( Closed_Domains_Lattice T ));
reconsider B = b as (Element of ( Closed_Domains_of T )) by CKB99:1;
assume A7: b is_less_than X;
A8: (for C being (Subset of T) holds (C in F implies B c= C))
proof
let C being (Subset of T);
reconsider C1 = C as (Subset of T);
assume A9: C in F;
then C1 is  closed_condensed by A1,CKB75:def 1;
then C in { P where P is (Subset of T): P is  closed_condensed };
then A10: C in ( Closed_Domains_of T ) by TDLAT_1:def 5;
then reconsider c = C as (Element of ( Closed_Domains_Lattice T )) by CKB99:1;
b [= c by A2,A7,A9,LATTICE3:def 16;
hence thesis by A10,CKB102:1;
end;
B in ( Closed_Domains_of T );
then B in { C where C is (Subset of T): C is  closed_condensed } by TDLAT_1:def 5;
then (ex C being (Subset of T) st (C = B & C is  closed_condensed));
then B c= A by A2,A5,A8,CKB82:1;
hence thesis by A3,CKB102:1;
end;
( Closed_Domains_Lattice T ) is  complete by CKB104:1;
hence thesis by A4,A6,LATTICE3:34;
end;

thus (X = ( {} ) implies ( "/\" (X,( Closed_Domains_Lattice T )) ) = ( [#] T ))
proof
set A = ( [#] T );
A is  closed_condensed by TDLAT_1:19;
then A in { C where C is (Subset of T): C is  closed_condensed };
then A11: A in ( Closed_Domains_of T ) by TDLAT_1:def 5;
then reconsider a = A as (Element of ( Closed_Domains_Lattice T )) by CKB99:1;
A12: (for b being (Element of ( Closed_Domains_Lattice T )) holds (b is_less_than X implies b [= a))
proof
let b being (Element of ( Closed_Domains_Lattice T ));
reconsider B = b as (Element of ( Closed_Domains_of T )) by CKB99:1;
assume b is_less_than X;
B c= A;
hence thesis by A11,CKB102:1;
end;
assume A13: X = ( {} );
A14: a is_less_than X
proof
let b being (Element of ( Closed_Domains_Lattice T ));
assume b in X;
hence thesis by A13;
end;
( Closed_Domains_Lattice T ) is  complete by CKB104:1;
hence thesis by A14,A12,LATTICE3:34;
end;

end;
