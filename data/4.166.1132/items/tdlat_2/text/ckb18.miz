begin
reserve T for TopSpace;
theorem
Th16: (for F being (Subset-Family of T) holds ( Int F ) = { A where A is (Subset of T): (ex B being (Subset of T) st (A = ( Int B ) & B in F)) })
proof
let F being (Subset-Family of T);
set P = { A where A is (Subset of T): (ex B being (Subset of T) st (A = ( Int B ) & B in F)) };
now
let C being set;
assume C in P;
then (ex A being (Subset of T) st (C = A & (ex B being (Subset of T) st (A = ( Int B ) & B in F))));
hence C in ( bool (the carrier of T) );
end;
then reconsider P as (Subset-Family of T) by TARSKI:def 3;
reconsider P as (Subset-Family of T);
(for X being set holds (X in ( Int F ) iff X in P))
proof
let X being set;
A1:now
assume A2: X in P;
then reconsider C = X as (Subset of T);
(ex D being (Subset of T) st (D = C & (ex B being (Subset of T) st (D = ( Int B ) & B in F)))) by A2;
hence X in ( Int F ) by CKB17:def 1;
end;
now
assume A3: X in ( Int F );
then reconsider C = X as (Subset of T);
(ex B being (Subset of T) st (C = ( Int B ) & B in F)) by A3,CKB17:def 1;
hence X in P;
end;
hence thesis by A1;
end;
hence thesis by TARSKI:1;
end;
