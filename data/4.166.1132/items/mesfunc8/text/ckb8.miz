begin
definition
let X being set;
let Y being set;
let F being (Functional_Sequence of X,Y);
redefine attr F is  with_the_same_dom
means
:Def2: (for n,m being  natural number holds ( dom ( F . n ) ) = ( dom ( F . m ) ));
correctness
proof
A1: ((for n,m being  natural number holds ( dom ( F . n ) ) = ( dom ( F . m ) )) implies F is  with_the_same_dom)
proof
assume A2: (for n,m being  natural number holds ( dom ( F . n ) ) = ( dom ( F . m ) ));
now
let f being Function,g being Function;
assume that
A3: f in ( rng F )
and
A4: g in ( rng F );
consider m being set such that A5: m in ( dom F ) and A6: g = ( F . m ) by A4,FUNCT_1:def 3;
consider n being set such that A7: n in ( dom F ) and A8: f = ( F . n ) by A3,FUNCT_1:def 3;
reconsider n,m as Nat by A7,A5;
( dom ( F . n ) ) = ( dom ( F . m ) ) by A2;
hence ( dom f ) = ( dom g ) by A8,A6;
end;
then ( rng F ) is  with_common_domain by CARD_3:def 10;
hence thesis by CKB7:def 1;
end;
(F is  with_the_same_dom implies (for n,m being  natural number holds ( dom ( F . n ) ) = ( dom ( F . m ) )))
proof
assume F is  with_the_same_dom;
then A9: ( rng F ) is  with_common_domain by CKB7:def 1;
hereby
let n being  natural number,m being  natural number;
A10: ( dom F ) = ( NAT ) by FUNCT_2:def 1;
then m in ( dom F ) by ORDINAL1:def 12;
then A11: ( F . m ) in ( rng F ) by FUNCT_1:3;
n in ( dom F ) by A10,ORDINAL1:def 12;
then ( F . n ) in ( rng F ) by FUNCT_1:3;
hence ( dom ( F . n ) ) = ( dom ( F . m ) ) by A9,A11,CARD_3:def 10;
end;
end;
hence thesis by A1;
end;
end;
