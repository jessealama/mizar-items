constructors SEQ_2,DYNKIN,REAL_1,SEQ_1,PROB_3,SERIES_1,KURATO_0,RINFSUP1,SETFAM_1,BINOP_2,RVSUM_1,WELLORD2,FINSEQOP,SETWISEO,RELSET_1,MEASURE1,CKB4;
requirements SUBSET,NUMERALS,BOOLE,ARITHM;
begin
reserve Omega for non  empty set;
reserve Sigma for (SigmaField of Omega);
reserve P for (Probability of Sigma);
reserve B for non  empty (Subset of Sigma);
reserve b for (Element of B);
reserve n for (Element of ( NAT ));
theorem
Th5: ( Indep (B,P) ) is (Dynkin_System of Omega)
proof
A1: (for b holds ( P . ( ( {} ) /\ b ) ) = ( ( P . ( {} ) ) * ( P . b ) ))
proof
let b;
reconsider b as (Event of Sigma);
( P . ( ( {} ) /\ b ) ) = ( 0 ) by VALUED_0:def 19;
hence thesis;
end;
reconsider Indp = ( Indep (B,P) ) as (Subset-Family of Omega) by XBOOLE_1:1;
( {} ) is (Element of Sigma) by PROB_1:22;
then A2: ( {} ) in ( Indep (B,P) ) by A1,CKB4:def 1;
A3: (for g being (SetSequence of Omega) holds ((( rng g ) c= ( Indep (B,P) ) & g is  disjoint_valued) implies ( Union g ) in ( Indep (B,P) )))
proof
let g being (SetSequence of Omega);
assume that
A4: ( rng g ) c= ( Indep (B,P) )
and
A5: g is  disjoint_valued;
now
let n;
( g . n ) is (Element of Sigma)
proof
per cases ;
suppose n in ( dom g );

then ( g . n ) in ( rng g ) by FUNCT_1:3;
hence thesis by A4,TARSKI:def 3;
end;
suppose (not n in ( dom g ));

then ( g . n ) = ( {} ) by FUNCT_1:def 2;
hence thesis by PROB_1:4;
end;
end;
hence ( g . n ) is (Event of Sigma);
end;
then reconsider g as (SetSequence of Sigma) by PROB_1:25;
reconsider Ug = ( Union g ) as (Element of Sigma) by PROB_1:26;
(for n holds (for b holds ( P . ( ( g . n ) /\ b ) ) = ( ( P . ( g . n ) ) * ( P . b ) )))
proof
let n;
let b;
( g . n ) in ( Indep (B,P) )
proof
per cases ;
suppose n in ( dom g );

then ( g . n ) in ( rng g ) by FUNCT_1:3;
hence thesis by A4;
end;
suppose (not n in ( dom g ));

hence thesis by A2,FUNCT_1:def 2;
end;
end;
hence thesis by CKB4:def 1;
end;
then (for b holds ( P . ( Ug /\ b ) ) = ( ( P . Ug ) * ( P . b ) )) by A5,CKB5:1;
hence thesis by CKB4:def 1;
end;
(for Z being (Subset of Omega) holds (Z in ( Indep (B,P) ) implies ( Z ` ) in ( Indep (B,P) )))
proof
let Z being (Subset of Omega);
assume A6: Z in ( Indep (B,P) );
then reconsider Z as (Event of Sigma);
reconsider Z9 = ( Z ` ) as (Element of Sigma) by PROB_1:20;
(for b being (Element of B) holds ( P . ( ( Z ` ) /\ b ) ) = ( ( P . ( Z ` ) ) * ( P . b ) ))
proof
let b being (Element of B);
( P . ( b /\ Z ) ) = ( ( P . b ) * ( P . Z ) ) by A6,CKB4:def 1;
then b,Z are_independent_respect_to P by PROB_2:def 4;
then b,( ( [#] Sigma ) \ Z ) are_independent_respect_to P by PROB_2:25;
hence thesis by PROB_2:def 4;
end;
then Z9 in ( Indep (B,P) ) by CKB4:def 1;
hence thesis;
end;
then Indp is (Dynkin_System of Omega) by A2,A3,DYNKIN:def 5;
hence thesis;
end;
