begin
reserve Omega for non empty set;
reserve I for non empty set;
reserve Sigma for (SigmaField of Omega);
reserve P for (Probability of Sigma);
reserve p for (Event of Sigma);
reserve q for (Event of Sigma);
reserve u for (Event of Sigma);
reserve v for (Event of Sigma);
theorem
Th14: (for F being (ManySortedSigmaField of I,Sigma) holds (for J,K being non empty (Subset of I) holds ((F is_independent_wrt P & J misses K) implies (for u holds (for v holds ((u in ( sigUn (F,J) ) & v in ( sigUn (F,K) )) implies ( P . ( u /\ v ) ) = ( ( P . u ) * ( P . v ) )))))))
proof
let F being (ManySortedSigmaField of I,Sigma);
let J being non empty (Subset of I),K being non empty (Subset of I);
A1: (( MeetSections (J,F) ) is non empty non empty (Subset of Sigma) & ( MeetSections (K,F) ) is non empty non empty (Subset of Sigma)) by CKB26:1;
assume A2: (F is_independent_wrt P & J misses K);
A3: (for p holds (for q holds ((p in ( MeetSections (J,F) ) & q in ( MeetSections (K,F) )) implies p,q are_independent_respect_to P)))
proof
let p;
let q;
assume A4: (p in ( MeetSections (J,F) ) & q in ( MeetSections (K,F) ));
reconsider p,q as (Subset of Omega);
( P . ( p /\ q ) ) = ( ( P . p ) * ( P . q ) ) by A2,A4,CKB25:1;
hence thesis by PROB_2:def 4;
end;
let u;
let v;
assume (u in ( sigUn (F,J) ) & v in ( sigUn (F,K) ));
then (u in ( sigma ( MeetSections (J,F) ) ) & v in ( sigma ( MeetSections (K,F) ) )) by CKB24:1;
then u,v are_independent_respect_to P by A1,A3,CKB11:1;
hence thesis by PROB_2:def 4;
end;
