requirements NUMERALS,BOOLE,SUBSET,REAL,ARITHM;
begin
theorem
Th2: (for N being  with_non-empty_elements non  empty set holds (for I being (Instruction of ( STC N )) holds (for s being (State of ( STC N )) holds (for k being  natural number holds ( Exec (I,( IncIC (s,k) )) ) = ( IncIC (( Exec (I,s) ),k) )))))
proof
let N being  with_non-empty_elements non  empty set;
let I being (Instruction of ( STC N ));
let s being (State of ( STC N ));
let k being  natural number;
per cases  by AMISTD_1:6;
suppose A1: ( InsCode I ) = 1;

hence ( Exec (I,( IncIC (s,k) )) ) = ( IncIC (( IncIC (s,k) ),1) ) by AMISTD_1:20
.= ( IncIC (s,( 1 + k )) ) by MEMSTR_0:57
.= ( IncIC (( IncIC (s,1) ),k) ) by MEMSTR_0:57
.= ( IncIC (( Exec (I,s) ),k) ) by A1,AMISTD_1:20;
end;
suppose ( InsCode I ) = ( 0 );

then A2: I is  halting by AMISTD_1:4;
hence ( Exec (I,( IncIC (s,k) )) ) = ( IncIC (s,k) ) by EXTPRO_1:def 3
.= ( IncIC (( Exec (I,s) ),k) ) by A2,EXTPRO_1:def 3;
end;
end;
