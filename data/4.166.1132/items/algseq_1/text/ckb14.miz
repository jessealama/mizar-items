begin
reserve k for Nat;
reserve m for Nat;
reserve n for Nat;
reserve R for non  empty ZeroStr;
reserve p for (AlgSequence of R);
definition
let R;
let p;
func len p -> (Element of ( NAT )) means 
:Def4: (it is_at_least_length_of p & (for m holds (m is_at_least_length_of p implies it <= m)));
existence
proof
defpred P[ Nat ]
 means
$1 is_at_least_length_of p;
A1: (ex m being Nat st P[ m ]) by CKB12:1;
(ex k st (P[ k ] & (for n holds (P[ n ] implies k <= n)))) from NAT_1:sch 5(A1);
then consider k such that A2: (k is_at_least_length_of p & (for n holds (n is_at_least_length_of p implies k <= n)));
take k;
thus thesis by A2,ORDINAL1:def 12;
end;
uniqueness by CKB13:1;
