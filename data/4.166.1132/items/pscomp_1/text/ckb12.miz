begin
reserve r for  real number;
reserve s for  real number;
reserve t for  real number;
theorem
(for T being non  empty TopSpace holds (for f being (RealMap of T) holds (((for p being (Point of T) holds ( f . p ) <= r) & (for t holds ((for p being (Point of T) holds ( f . p ) <= t) implies r <= t))) implies r = ( upper_bound f ))))
proof
let T being non  empty TopSpace;
let f being (RealMap of T);
set c = (the carrier of T);
set fc = ( f .: (the carrier of T) );
assume that
A1: (for p being (Point of T) holds ( f . p ) <= r)
and
A2: (for t holds ((for p being (Point of T) holds ( f . p ) <= t) implies r <= t));
A3:now
let t;
assume (for s holds (s in fc implies s <= t));
then (for s being (Point of T) holds ( f . s ) <= t) by FUNCT_2:35;
hence r <= t by A2;
end;
now
let s;
assume s in fc;
then (ex x being set st ((x in c & x in c) & s = ( f . x ))) by FUNCT_2:64;
hence s <= r by A1;
end;
hence thesis by A3,SEQ_4:46;
end;
