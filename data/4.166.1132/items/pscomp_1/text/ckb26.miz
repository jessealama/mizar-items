begin
theorem
Th60: (for T being non empty TopSpace holds ((for f being (RealMap of T) holds (f is  continuous implies f is  with_max)) iff (for f being (RealMap of T) holds (f is  continuous implies f is  with_min))))
proof
let T being non empty TopSpace;
hereby
assume A1: (for f being (RealMap of T) holds (f is  continuous implies f is  with_max));
let f being (RealMap of T);
assume f is  continuous;
then ( - f ) is  continuous by CKB18:1;
then ( - f ) is  with_max by A1;
hence f is  with_min by MEASURE6:66;
end;
assume A2: (for f being (RealMap of T) holds (f is  continuous implies f is  with_min));
let f being (RealMap of T);
assume f is  continuous;
then ( - f ) is  continuous by CKB18:1;
then ( - ( - f ) ) is  with_max by A2,CKB2:1;
hence thesis;
end;
