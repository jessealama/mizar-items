requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve r for (Element of ( REAL ));
reserve seq for Real_Sequence;
reserve n for (Element of ( NAT ));
reserve h for (PartFunc of ( REAL ),( REAL ));
theorem
Th24: (for r being  real number holds (( rng seq ) c= ( dom h ) implies ( ( r (#) h ) /* seq ) = ( r (#) ( h /* seq ) )))
proof
let r being  real number;
assume A1: ( rng seq ) c= ( dom h );
then A2: ( rng seq ) c= ( dom ( r (#) h ) ) by VALUED_1:def 5;
now
let n;
A3: ( seq . n ) in ( rng seq ) by VALUED_0:28;
thus ( ( ( r (#) h ) /* seq ) . n ) = ( ( r (#) h ) . ( seq . n ) ) by A2,FUNCT_2:108
.= ( r * ( h . ( seq . n ) ) ) by A2,A3,VALUED_1:def 5
.= ( r * ( ( h /* seq ) . n ) ) by A1,FUNCT_2:108;
end;
hence thesis by SEQ_1:9;
end;
