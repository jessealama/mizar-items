requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve g for (Element of ( REAL ));
reserve p for (Element of ( REAL ));
reserve p1 for (Element of ( REAL ));
reserve p2 for (Element of ( REAL ));
reserve h for (PartFunc of ( REAL ),( REAL ));
theorem
((( h | [. p,g .] ) is  increasing or ( h | [. p,g .] ) is  decreasing) implies ( h | [. p,g .] ) is  one-to-one)
proof
assume A1: (( h | [. p,g .] ) is  increasing or ( h | [. p,g .] ) is  decreasing);
now
per cases  by A1;
suppose A2: ( h | [. p,g .] ) is  increasing;

now
let p1;
let p2;
assume that
A3: p1 in ( dom ( h | [. p,g .] ) )
and
A4: p2 in ( dom ( h | [. p,g .] ) )
and
A5: ( ( h | [. p,g .] ) . p1 ) = ( ( h | [. p,g .] ) . p2 );
A6: (p1 in ( [. p,g .] /\ ( dom h ) ) & p2 in ( [. p,g .] /\ ( dom h ) )) by A3,A4,RELAT_1:61;
A7: ( h . p1 ) = ( ( h | [. p,g .] ) . p2 ) by A3,A5,FUNCT_1:47
.= ( h . p2 ) by A4,FUNCT_1:47;
thus p1 = p2
proof
assume A8: p1 <> p2;
now
per cases  by A8,XXREAL_0:1;
suppose p1 < p2;

hence contradiction by A2,A7,A6,CKB25:1;
end;
suppose p2 < p1;

hence contradiction by A2,A7,A6,CKB25:1;
end;
end;
hence contradiction;
end;

end;
hence thesis by PARTFUN1:8;
end;
suppose A9: ( h | [. p,g .] ) is  decreasing;

now
let p1;
let p2;
assume that
A10: p1 in ( dom ( h | [. p,g .] ) )
and
A11: p2 in ( dom ( h | [. p,g .] ) )
and
A12: ( ( h | [. p,g .] ) . p1 ) = ( ( h | [. p,g .] ) . p2 );
A13: (p1 in ( [. p,g .] /\ ( dom h ) ) & p2 in ( [. p,g .] /\ ( dom h ) )) by A10,A11,RELAT_1:61;
A14: ( h . p1 ) = ( ( h | [. p,g .] ) . p2 ) by A10,A12,FUNCT_1:47
.= ( h . p2 ) by A11,FUNCT_1:47;
thus p1 = p2
proof
assume A15: p1 <> p2;
now
per cases  by A15,XXREAL_0:1;
suppose p1 < p2;

hence contradiction by A9,A14,A13,CKB26:1;
end;
suppose p2 < p1;

hence contradiction by A9,A14,A13,CKB26:1;
end;
end;
hence contradiction;
end;

end;
hence thesis by PARTFUN1:8;
end;
end;
hence thesis;
end;
