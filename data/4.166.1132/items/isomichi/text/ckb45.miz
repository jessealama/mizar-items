environ
vocabularies NUMBERS,PRE_TOPC,SUBSET_1,ZFMISC_1,TEX_1,TDLAT_3,XBOOLE_0,STRUCT_0,TOPS_1,RCOMP_1,TARSKI,DECOMP_1,TOPMETR,XREAL_0,ORDINAL1,XXREAL_1,XXREAL_0,BORSUK_5,ARYTM_3,LIMFUNC1,ISOMICHI;
notations TARSKI,XBOOLE_0,SUBSET_1,ORDINAL1,XREAL_0,LIMFUNC1,RCOMP_1,NUMBERS,DOMAIN_1,ZFMISC_1,STRUCT_0,PRE_TOPC,TOPS_1,TDLAT_3,TEX_1,TOPMETR,BORSUK_5,SEQ_4,DECOMP_1,XXREAL_0,CKB5,CKB6,CKB10,CKB22,CKB23;
definitions TARSKI,SUBSET_1,STRUCT_0,LIMFUNC1,PROB_1,CKB5,CKB6,CKB10;
theorems SUBSET_1,XBOOLE_0,XBOOLE_1,PRE_TOPC,TOPS_1,TDLAT_1,TDLAT_3,PCOMPS_1,TEX_1,BORSUK_5,TOPMETR,TOPREAL6,DECOMP_1,TOPGEN_1,TOPS_3,XXREAL_0,XXREAL_1,CKB3,CKB4,CKB5,CKB6,CKB8,CKB9,CKB10,CKB11,CKB15,CKB16,CKB17,CKB18,CKB21,CKB24,CKB27,CKB28,CKB31,CKB35,CKB38,CKB39,CKB44;
registrations XBOOLE_0,XXREAL_0,XREAL_0,MEMBERED,RCOMP_1,ZFMISC_1,STRUCT_0,TOPS_1,TEX_1,TEX_2,TOPMETR,FCONT_3,PRE_TOPC,CKB1,CKB2,CKB7,CKB12,CKB13,CKB14,CKB19,CKB20,CKB25,CKB26,CKB29,CKB30,CKB32,CKB33,CKB34,CKB36,CKB37,CKB40,CKB41,CKB42,CKB43;
constructors RAT_1,PROB_1,SEQ_4,RCOMP_1,LIMFUNC1,TOPS_1,TDLAT_3,TEX_1,TOPMETR,BORSUK_5,DECOMP_1,CKB5,CKB6;
requirements BOOLE,SUBSET,NUMERALS,REAL;
begin
reserve T for TopSpace;
reserve A for (Subset of T);
reserve B for (Subset of T);
theorem
(A is  condensed iff (ex B st ((B is  regular_closed & ( Int B ) c= A) & A c= B)))
proof
thus (A is  condensed implies (ex B st ((B is  regular_closed & ( Int B ) c= A) & A c= B)))
proof
assume A1: A is  condensed;
then A2: ( Cl ( Int A ) ) = ( Cl A ) by CKB10:def 1;
take ( Cl ( Int A ) );
( Int ( Cl A ) ) = ( Int A ) by A1,CKB10:def 1;
hence thesis by A2,PRE_TOPC:18,TOPS_1:16;
end;

given B such that
A3: B is  regular_closed
and
A4: ( Int B ) c= A
and
A5: A c= B;

A6: ( Cl ( Int B ) ) = B by A3,TOPS_1:def 7;
( Cl A ) c= ( Cl B ) by A5,PRE_TOPC:19;
then ( Int ( Cl A ) ) c= ( Int ( Cl B ) ) by TOPS_1:19;
then A7: ( Int ( Cl A ) ) c= ( Int B ) by A3,CKB5:def 1;
( Cl ( Int B ) ) c= ( Cl A ) by A4,PRE_TOPC:19;
then ( Int B ) c= ( Int ( Cl A ) ) by A6,TOPS_1:19;
then A8: ( Int B ) = ( Int ( Cl A ) ) by A7,XBOOLE_0:def 10;
( Int A ) c= ( Int B ) by A5,TOPS_1:19;
then A9: ( Cl ( Int A ) ) c= ( Cl ( Int B ) ) by PRE_TOPC:19;
( Int ( Int B ) ) c= ( Int A ) by A4,TOPS_1:19;
then ( Cl ( Int ( Int B ) ) ) c= ( Cl ( Int A ) ) by PRE_TOPC:19;
then ( Cl ( Int A ) ) = B by A6,A9,XBOOLE_0:def 10;
hence thesis by A4,A5,A8,CKB21:1;
end;
