registrations XBOOLE_0,XXREAL_0,XREAL_0,MEMBERED,RCOMP_1,ZFMISC_1,STRUCT_0,TOPS_1,TEX_1,TEX_2,TOPMETR,FCONT_3,PRE_TOPC,CKB1,CKB2,CKB7;
constructors RAT_1,PROB_1,SEQ_4,RCOMP_1,LIMFUNC1,TOPS_1,TDLAT_3,TEX_1,TOPMETR,BORSUK_5,DECOMP_1,CKB5,CKB6;
requirements BOOLE,SUBSET,NUMERALS,REAL;
begin
reserve T for TopSpace;
reserve A for (Subset of T);
theorem
Th5: (A is  condensed iff (A is  subcondensed & A is  supercondensed))
proof
thus (A is  condensed implies (A is  subcondensed & A is  supercondensed))
proof
assume A1: A is  condensed;
then A2: ( Int ( Cl A ) ) = ( Int A ) by CKB10:def 1;
( Cl ( Int A ) ) = ( Cl A ) by A1,CKB10:def 1;
hence thesis by A2,CKB5:def 1,CKB6:def 1;
end;

assume that
A3: A is  subcondensed
and
A4: A is  supercondensed;
A5: ( Cl ( Int A ) ) = ( Cl A ) by A3,CKB6:def 1;
( Int ( Cl A ) ) = ( Int A ) by A4,CKB5:def 1;
hence thesis by A5,CKB10:def 1;
end;
