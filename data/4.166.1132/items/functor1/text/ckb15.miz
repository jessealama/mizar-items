begin
theorem
Th12: (for C1 being non empty AltGraph holds (for C2,C3 being non empty  reflexive AltGraph holds (for F being  feasible FunctorStr over C1,C2 holds (for G being FunctorStr over C2,C3 holds ((F is  surjective & G is  surjective) implies ( G * F ) is  surjective)))))
proof
let C1 being non empty AltGraph;
let C2 being non empty  reflexive AltGraph,C3 being non empty  reflexive AltGraph;
let F being  feasible FunctorStr over C1,C2;
let G being FunctorStr over C2,C3;
assume A1: (F is  surjective & G is  surjective);
then (F is  onto & G is  onto) by FUNCTOR0:def 34;
then A2: ( G * F ) is  onto by CKB12:1;
(F is  full & G is  full) by A1,FUNCTOR0:def 34;
then ( G * F ) is  full by CKB13:1;
hence thesis by A2,FUNCTOR0:def 34;
end;
