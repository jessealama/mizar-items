begin
reserve i for (Element of ( NAT ));
reserve j for (Element of ( NAT ));
reserve n for (Element of ( NAT ));
reserve C for  compact non vertical non horizontal (Subset of ( TOP-REAL 2 ));
theorem
Th20: (([ i,j ] in ( Indices ( Gauge (C,n) ) ) & [ ( i + 1 ),j ] in ( Indices ( Gauge (C,n) ) )) implies ( dist (( ( Gauge (C,n) ) * (i,j) ),( ( Gauge (C,n) ) * (( i + 1 ),j) )) ) = ( ( ( E-bound C ) - ( W-bound C ) ) / ( 2 |^ n ) ))
proof
set G = ( Gauge (C,n) );
set a = ( N-bound C );
set e = ( E-bound C );
set s = ( S-bound C );
set w = ( W-bound C );
set p1 = ( G * (i,j) );
set p2 = ( G * (( i + 1 ),j) );
assume that
A1: [ i,j ] in ( Indices G )
and
A2: [ ( i + 1 ),j ] in ( Indices G );
A3: p2 = |[ ( w + ( ( ( e - w ) / ( 2 |^ n ) ) * ( ( i + 1 ) - 2 ) ) ),( s + ( ( ( a - s ) / ( 2 |^ n ) ) * ( j - 2 ) ) ) ]| by A2,JORDAN8:def 1;
e >= w by SPRECT_1:21;
then A4: ( e - w ) >= ( w - w ) by XREAL_1:9;
set x = ( ( e - w ) / ( 2 |^ n ) );
consider p being (Point of ( Euclid 2 )),q being (Point of ( Euclid 2 )) such that A5: (p = p1 & q = p2) and A6: ( dist (p1,p2) ) = ( dist (p,q) ) by TOPREAL6:def 1;
A7: p1 = |[ ( w + ( ( ( e - w ) / ( 2 |^ n ) ) * ( i - 2 ) ) ),( s + ( ( ( a - s ) / ( 2 |^ n ) ) * ( j - 2 ) ) ) ]| by A1,JORDAN8:def 1;
( dist (p,q) ) = ( ( Pitag_dist 2 ) . (p,q) ) by METRIC_1:def 1
.= ( sqrt ( ( ( ( p1 `1 ) - ( p2 `1 ) ) ^2 ) + ( ( ( p1 `2 ) - ( p2 `2 ) ) ^2 ) ) ) by A5,TOPREAL3:7
.= ( sqrt ( ( ( ( p1 `1 ) - ( p2 `1 ) ) ^2 ) + ( ( ( s + ( ( ( a - s ) / ( 2 |^ n ) ) * ( j - 2 ) ) ) - ( p2 `2 ) ) ^2 ) ) ) by A7,EUCLID:52
.= ( sqrt ( ( ( ( p1 `1 ) - ( p2 `1 ) ) ^2 ) + ( ( ( s + ( ( ( a - s ) / ( 2 |^ n ) ) * ( j - 2 ) ) ) - ( s + ( ( ( a - s ) / ( 2 |^ n ) ) * ( j - 2 ) ) ) ) ^2 ) ) ) by A3,EUCLID:52
.= ( abs ( ( p1 `1 ) - ( p2 `1 ) ) ) by COMPLEX1:72
.= ( abs ( ( w + ( x * ( i - 2 ) ) ) - ( p2 `1 ) ) ) by A7,EUCLID:52
.= ( abs ( ( w + ( x * ( i - 2 ) ) ) - ( w + ( x * ( ( i + 1 ) - 2 ) ) ) ) ) by A3,EUCLID:52
.= ( abs ( - x ) )
.= ( abs x ) by COMPLEX1:52
.= ( ( abs ( e - w ) ) / ( abs ( 2 |^ n ) ) ) by COMPLEX1:67
.= ( ( e - w ) / ( abs ( 2 |^ n ) ) ) by A4,ABSVALUE:def 1;
hence thesis by A6,ABSVALUE:def 1;
end;
