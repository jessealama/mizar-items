constructors FUNCT_4,REAL_1,SQUARE_1,NAT_1,CARD_3,RCOMP_1,FINSEQ_4,NEWTON,POWER,NAT_D,TOPS_1,CONNSP_1,COMPTS_1,REALSET2,TBSP_1,MONOID_0,TOPREAL4,GOBOARD2,SPPOL_1,JORDAN1,PSCOMP_1,MEASURE6,GOBOARD9,SPRECT_1,SPRECT_2,JORDAN2C,TOPREAL6,JORDAN8,SEQ_1,XXREAL_2,GOBOARD1,SEQ_4,RELSET_1,CONVEX1,BINOP_2,RVSUM_1;
