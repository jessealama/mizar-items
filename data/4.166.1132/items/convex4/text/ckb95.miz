begin
reserve V for non  empty CLSStruct;
reserve x for set;
theorem
Th61: (for V being  vector-distributive  scalar-distributive  scalar-associative  scalar-unital non  empty CLSStruct holds (for M being (Subset of V) holds (for z1,z2 being Complex holds ( ( z1 + z2 ) * M ) c= ( ( z1 * M ) + ( z2 * M ) ))))
proof
let V being  vector-distributive  scalar-distributive  scalar-associative  scalar-unital non  empty CLSStruct;
let M being (Subset of V);
let z1 being Complex;
let z2 being Complex;
(for x being (VECTOR of V) holds (x in ( ( z1 + z2 ) * M ) implies x in ( ( z1 * M ) + ( z2 * M ) )))
proof
let x being (VECTOR of V);
assume x in ( ( z1 + z2 ) * M );
then consider w being (VECTOR of V) such that A1: x = ( ( z1 + z2 ) * w ) and A2: w in M;
A3: ( z2 * w ) in ( z2 * M ) by A2;
(x = ( ( z1 * w ) + ( z2 * w ) ) & ( z1 * w ) in ( z1 * M )) by A1,A2,CLVECT_1:def 3;
hence thesis by A3;
end;
hence thesis by SUBSET_1:2;
end;
