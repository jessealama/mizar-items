begin
reserve V for non  empty CLSStruct;
reserve u for (VECTOR of V);
reserve v for (VECTOR of V);
reserve F for (FinSequence of (the carrier of V));
theorem
Th68: (for V being non  empty CLSStruct holds (for F being (Subset-Family of V) holds ((for M being (Subset of V) holds (M in F implies M is  convex)) implies ( meet F ) is  convex)))
proof
let V being non  empty CLSStruct;
let F being (Subset-Family of V);
assume A1: (for M being (Subset of V) holds (M in F implies M is  convex));
per cases ;
suppose F = ( {} );

then ( meet F ) = ( {} ) by SETFAM_1:def 1;
hence thesis by CKB93:1;
end;
suppose A2: F <> ( {} );

thus ( meet F ) is  convex
proof
let u being (VECTOR of V);
let v being (VECTOR of V);
let z being Complex;
assume that
A3: (ex r being Real st ((z = r & ( 0 ) < r) & r < 1))
and
A4: u in ( meet F )
and
A5: v in ( meet F );
(for M being set holds (M in F implies ( ( z * u ) + ( ( ( 1r ) - z ) * v ) ) in M))
proof
let M being set;
assume A6: M in F;
then reconsider M as (Subset of V);
A7: v in M by A5,A6,SETFAM_1:def 1;
(M is  convex & u in M) by A1,A4,A6,SETFAM_1:def 1;
hence thesis by A3,A7,CKB78:def 1;
end;
hence thesis by A2,SETFAM_1:def 1;
end;

end;
