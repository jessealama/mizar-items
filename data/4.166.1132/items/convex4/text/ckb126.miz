begin
reserve V for non  empty CLSStruct;
reserve v1 for (VECTOR of V);
reserve v2 for (VECTOR of V);
reserve L for (C_Linear_Combination of V);
theorem
(for V being ComplexLinearSpace holds (for v1,v2 being (VECTOR of V) holds (for L being (C_Linear_Combination of { v1,v2 }) holds ((v1 <> v2 & L is  convex) implies ((ex r1,r2 being  real number st (((r1 = ( L . v1 ) & r2 = ( L . v2 )) & r1 >= ( 0 )) & r2 >= ( 0 ))) & ( Sum L ) = ( ( ( L . v1 ) * v1 ) + ( ( L . v2 ) * v2 ) ))))))
proof
let V being ComplexLinearSpace;
let v1 being (VECTOR of V);
let v2 being (VECTOR of V);
let L being (C_Linear_Combination of { v1,v2 });
assume that
A1: v1 <> v2
and
A2: L is  convex;
A3: ( Carrier L ) c= { v1,v2 } by CKB9:def 1;
A4: ( Carrier L ) <> ( {} ) by A2,CKB118:1;
(ex r1,r2 being  real number st (((r1 = ( L . v1 ) & r2 = ( L . v2 )) & r1 >= ( 0 )) & r2 >= ( 0 )))
proof
per cases  by A3,A4,ZFMISC_1:36;
suppose A5: ( Carrier L ) = { v1 };

then (not v2 in ( Carrier L )) by A1,TARSKI:def 1;
then A6: ( 0 ) = ( L . v2 );
(ex r being Real st (r = ( L . v1 ) & r = 1)) by A2,A5,CKB121:1;
hence thesis by A6;
end;
suppose A7: ( Carrier L ) = { v2 };

then (not v1 in ( Carrier L )) by A1,TARSKI:def 1;
then A8: ( 0 ) = ( L . v1 );
(ex r being Real st (r = ( L . v2 ) & r = 1)) by A2,A7,CKB121:1;
hence thesis by A8;
end;
suppose ( Carrier L ) = { v1,v2 };

then (ex r1,r2 being Real st ((((r1 = ( L . v1 ) & r2 = ( L . v2 )) & ( r1 + r2 ) = 1) & r1 >= ( 0 )) & r2 >= ( 0 ))) by A1,A2,CKB122:1;
hence thesis;
end;
end;
hence thesis by A1,CKB24:1;
end;
