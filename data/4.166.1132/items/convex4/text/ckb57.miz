definition
let V;
func C_LCAdd V -> (BinOp of ( C_LinComb V )) means 
:Def15: (for e1 holds (for e2 holds ( it . (e1,e2) ) = ( ( @ e1 ) + ( @ e2 ) )));
existence
proof
deffunc F((Element of ( C_LinComb V )),(Element of ( C_LinComb V ))) = ( @ ( ( @ $1 ) + ( @ $2 ) ) );
consider o being (BinOp of ( C_LinComb V )) such that A1: (for e1 holds (for e2 holds ( o . (e1,e2) ) = F(e1,e2))) from BINOP_1:sch 4;
take o;
let e1;
let e2;
thus ( o . (e1,e2) ) = ( @ ( ( @ e1 ) + ( @ e2 ) ) ) by A1
.= ( ( @ e1 ) + ( @ e2 ) );
end;
uniqueness
proof
let o1 being (BinOp of ( C_LinComb V ));
let o2 being (BinOp of ( C_LinComb V ));
assume A2: (for e1 holds (for e2 holds ( o1 . (e1,e2) ) = ( ( @ e1 ) + ( @ e2 ) )));
assume A3: (for e1 holds (for e2 holds ( o2 . (e1,e2) ) = ( ( @ e1 ) + ( @ e2 ) )));
now
let e1;
let e2;
thus ( o1 . (e1,e2) ) = ( ( @ e1 ) + ( @ e2 ) ) by A2
.= ( o2 . (e1,e2) ) by A3;
end;
hence thesis by BINOP_1:2;
end;
