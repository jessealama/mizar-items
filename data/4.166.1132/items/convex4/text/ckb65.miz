begin
reserve V for non  empty CLSStruct;
reserve u for (VECTOR of V);
reserve v for (VECTOR of V);
reserve A for (Subset of V);
reserve l for (C_Linear_Combination of A);
reserve l1 for (C_Linear_Combination of A);
reserve l2 for (C_Linear_Combination of A);
reserve x for set;
reserve a for Complex;
definition
let V;
let A;
func LC_CLSpace A ->  strict (Subspace of ( LC_CLSpace V )) means 
(the carrier of it) = { l: (not contradiction) };
existence
proof
set X = { l: (not contradiction) };
X c= (the carrier of ( LC_CLSpace V ))
proof
let x;
assume x in X;
then (ex l st x = l);
hence thesis by CKB53:def 1;
end;
then reconsider X as (Subset of ( LC_CLSpace V ));
A1: (for v,u being (VECTOR of ( LC_CLSpace V )) holds ((v in X & u in X) implies ( v + u ) in X))
proof
let v being (VECTOR of ( LC_CLSpace V ));
let u being (VECTOR of ( LC_CLSpace V ));
assume that
A2: v in X
and
A3: u in X;
consider l1 such that A4: v = l1 by A2;
consider l2 such that A5: u = l2 by A3;
A6: u = ( vector (( LC_CLSpace V ),l2) ) by A5,RLVECT_2:1;
v = ( vector (( LC_CLSpace V ),l1) ) by A4,RLVECT_2:1;
then ( v + u ) = ( l1 + l2 ) by A6,CKB61:1;
hence thesis;
end;
( ZeroCLC V ) is (C_Linear_Combination of A) by CKB11:1;
then A7: ( ZeroCLC V ) in X;
(for a being Complex holds (for v being (VECTOR of ( LC_CLSpace V )) holds (v in X implies ( a * v ) in X)))
proof
let a being Complex;
let v being (VECTOR of ( LC_CLSpace V ));
assume v in X;
then consider l such that A8: v = l;
( a * v ) = ( a * ( vector (( LC_CLSpace V ),l) ) ) by A8,RLVECT_2:1
.= ( a * l ) by CKB62:1;
then ( a * v ) is (C_Linear_Combination of A) by CKB39:1;
hence thesis;
end;
then X is  linearly-closed by A1,CLVECT_1:def 7;
hence thesis by A7,CLVECT_1:54;
end;
uniqueness by CLVECT_1:49;
end;
