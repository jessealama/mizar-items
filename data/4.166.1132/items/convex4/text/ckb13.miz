constructors SETFAM_1,BINOP_1,FUNCOP_1,XXREAL_0,REAL_1,FINSEQ_4,COMPLEX1,REALSET1,BINOP_2,FINSOP_1,RVSUM_1,RLVECT_2,RUSUB_5,CSSPACE,RELSET_1,CKB1,CKB3,CKB6,CKB9;
requirements REAL,NUMERALS,BOOLE,SUBSET,ARITHM;
begin
reserve x for set;
reserve y for set;
reserve i for Nat;
definition
let V being non  empty CLSStruct;
let F being (FinSequence of (the carrier of V));
let f being (Function of (the carrier of V),( COMPLEX ));
func f (#) F -> (FinSequence of (the carrier of V)) means 
:Def5: (( len it ) = ( len F ) & (for i holds (i in ( dom it ) implies ( it . i ) = ( ( f . ( F /. i ) ) * ( F /. i ) ))));
existence
proof
deffunc Q(set) = ( ( f . ( F /. $1 ) ) * ( F /. $1 ) );
consider G being FinSequence such that A1: ( len G ) = ( len F ) and A2: (for n being Nat holds (n in ( dom G ) implies ( G . n ) = Q(n))) from FINSEQ_1:sch 2;
( rng G ) c= (the carrier of V)
proof
let x;
assume x in ( rng G );
then consider y such that A3: y in ( dom G ) and A4: ( G . y ) = x by FUNCT_1:def 3;
reconsider y as (Element of ( NAT )) by A3;
( G . y ) = ( ( f . ( F /. y ) ) * ( F /. y ) ) by A2,A3;
hence thesis by A4;
end;
then reconsider G as (FinSequence of (the carrier of V)) by FINSEQ_1:def 4;
take G;
thus thesis by A1,A2;
end;
uniqueness
proof
let H1 being (FinSequence of (the carrier of V));
let H2 being (FinSequence of (the carrier of V));
assume that
A5: ( len H1 ) = ( len F )
and
A6: (for i holds (i in ( dom H1 ) implies ( H1 . i ) = ( ( f . ( F /. i ) ) * ( F /. i ) )))
and
A7: ( len H2 ) = ( len F )
and
A8: (for i holds (i in ( dom H2 ) implies ( H2 . i ) = ( ( f . ( F /. i ) ) * ( F /. i ) )));
now
let k being Nat;
assume (1 <= k & k <= ( len H1 ));
then A9: k in ( Seg ( len H1 ) ) by FINSEQ_1:1;
then k in ( dom H1 ) by FINSEQ_1:def 3;
then A10: ( H1 . k ) = ( ( f . ( F /. k ) ) * ( F /. k ) ) by A6;
k in ( dom H2 ) by A5,A7,A9,FINSEQ_1:def 3;
hence ( H1 . k ) = ( H2 . k ) by A8,A10;
end;
hence thesis by A5,A7,FINSEQ_1:14;
end;
end;
