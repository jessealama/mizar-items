environ
vocabularies NUMBERS,REAL_1,XCMPLX_0,ORDINAL1,ARYTM_1,SUBSET_1,RELAT_1,XBOOLE_0,MEMBERED,PARTFUN1,FUNCT_1,ABIAN,TARSKI,ARYTM_3,CARD_1,COMPLEX1,VALUED_1,SQUARE_1,ABSVALUE,XREAL_0,XXREAL_0,EUCLID,SIN_COS,SIN_COS2,XXREAL_1,SIN_COS9,SIN_COS4,FUNCT_8;
notations COMPLEX1,XCMPLX_0,ORDINAL1,NUMBERS,XREAL_0,XXREAL_0,REAL_1,MEMBERED,ABSVALUE,PARTFUN1,TARSKI,XBOOLE_0,SUBSET_1,EUCLID,RELAT_1,FUNCT_1,FUNCT_2,SIN_COS,SIN_COS2,VALUED_1,RCOMP_1,SIN_COS4,SQUARE_1,SIN_COS9,RELSET_1,FDIFF_9,CKB1,CKB4,CKB7,CKB8,CKB11,CKB12,CKB13,CKB16;
