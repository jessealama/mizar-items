begin
reserve x for set;
reserve y for set;
reserve S for non  void non  empty ManySortedSign;
reserve o for (OperSymbol of S);
reserve U0 for MSAlgebra over S;
definition
let S being non  void non  empty ManySortedSign;
let U0 being  non-empty MSAlgebra over S;
func MSAlg_meet U0 -> (BinOp of ( MSSub U0 )) means 
:Def22: (for x,y being (Element of ( MSSub U0 )) holds (for U1,U2 being  strict (MSSubAlgebra of U0) holds ((x = U1 & y = U2) implies ( it . (x,y) ) = ( U1 /\ U2 ))));
existence
proof
defpred P[ (Element of ( MSSub U0 )),(Element of ( MSSub U0 )),(Element of ( MSSub U0 )) ]
 means
(for U1,U2 being  strict (MSSubAlgebra of U0) holds (($1 = U1 & $2 = U2) implies $3 = ( U1 /\ U2 )));
A1: (for x,y being (Element of ( MSSub U0 )) holds (ex z being (Element of ( MSSub U0 )) st P[ x,y,z ]))
proof
let x being (Element of ( MSSub U0 ));
let y being (Element of ( MSSub U0 ));
reconsider U1 = x,U2 = y as  strict (MSSubAlgebra of U0) by CKB63:def 1;
reconsider z = ( U1 /\ U2 ) as (Element of ( MSSub U0 )) by CKB63:def 1;
take z;
thus thesis;
end;
consider o being (BinOp of ( MSSub U0 )) such that A2: (for a,b being (Element of ( MSSub U0 )) holds P[ a,b,( o . (a,b) ) ]) from BINOP_1:sch 3(A1);
take o;
thus thesis by A2;
end;
uniqueness
proof
let o1 being (BinOp of ( MSSub U0 ));
let o2 being (BinOp of ( MSSub U0 ));
assume that
A3: (for x,y being (Element of ( MSSub U0 )) holds (for U1,U2 being  strict (MSSubAlgebra of U0) holds ((x = U1 & y = U2) implies ( o1 . (x,y) ) = ( U1 /\ U2 ))))
and
A4: (for x,y being (Element of ( MSSub U0 )) holds (for U1,U2 being  strict (MSSubAlgebra of U0) holds ((x = U1 & y = U2) implies ( o2 . (x,y) ) = ( U1 /\ U2 ))));
(for x being (Element of ( MSSub U0 )) holds (for y being (Element of ( MSSub U0 )) holds ( o1 . (x,y) ) = ( o2 . (x,y) )))
proof
let x being (Element of ( MSSub U0 ));
let y being (Element of ( MSSub U0 ));
reconsider U1 = x,U2 = y as  strict (MSSubAlgebra of U0) by CKB63:def 1;
( o1 . (x,y) ) = ( U1 /\ U2 ) by A3;
hence thesis by A4;
end;
hence thesis by BINOP_1:2;
end;
