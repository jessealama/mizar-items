begin
reserve x for set;
reserve S for non  void non  empty ManySortedSign;
reserve U0 for MSAlgebra over S;
definition
let S being non  void non  empty ManySortedSign;
let U0 being MSAlgebra over S;
func MSSub U0 -> set means 
:Def20: (for x holds (x in it iff x is  strict  strict (MSSubAlgebra of U0)));
existence
proof
reconsider X = { ( GenMSAlg ( @ A ) ) where A is (Element of ( SubSort U0 )): (not contradiction) } as set;
take X;
let x;
thus (x in X implies x is  strict  strict (MSSubAlgebra of U0))
proof
assume x in X;
then (ex A being (Element of ( SubSort U0 )) st x = ( GenMSAlg ( @ A ) ));
hence thesis;
end;

assume x is  strict  strict (MSSubAlgebra of U0);
then reconsider a = x as  strict (MSSubAlgebra of U0);
reconsider A = (the Sorts of a) as (MSSubset of U0) by CKB20:def 1;
A is  opers_closed by CKB20:def 1;
then reconsider h = A as (Element of ( SubSort U0 )) by CKB40:1;
a = ( GenMSAlg ( @ h ) ) by CKB55:1;
hence thesis;
end;
uniqueness
proof
defpred P[ set ]
 means
$1 is  strict  strict (MSSubAlgebra of U0);
(for X1,X2 being set holds (((for x being set holds (x in X1 iff P[ x ])) & (for x being set holds (x in X2 iff P[ x ]))) implies X1 = X2)) from XBOOLE_0:sch 3;
hence thesis;
end;
end;
