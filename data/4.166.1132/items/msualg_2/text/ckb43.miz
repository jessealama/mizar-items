requirements BOOLE,SUBSET;
begin
definition
let S being non void non empty ManySortedSign;
let U0 being MSAlgebra over S;
let A being (MSSubset of U0);
func MSSubSort A -> (MSSubset of U0)
means
:Def15: (for s being (SortSymbol of S) holds ( it . s ) = ( meet ( SubSort (A,s) ) ));
existence
proof
deffunc F((SortSymbol of S)) = ( meet ( SubSort (A,$1) ) );
consider f being Function such that A1: (( dom f ) = (the carrier of S) & (for s being (SortSymbol of S) holds ( f . s ) = F(s))) from FUNCT_1:sch 4;
reconsider f as (ManySortedSet of (the carrier of S)) by A1,PARTFUN1:def 2,RELAT_1:def 18;
f c= (the Sorts of U0)
proof
reconsider u0 = (the Sorts of U0) as (MSSubset of U0) by PBOOLE:def 18;
let x being set;
A2: (A c= u0 & ( Constants U0 ) c= u0) by PBOOLE:def 18;
assume x in (the carrier of S);
then reconsider s = x as (SortSymbol of S);
u0 is  opers_closed by CKB18:1;
then u0 in ( SubSort A ) by A2,CKB39:1;
then A3: ( (the Sorts of U0) . s ) in ( SubSort (A,s) ) by CKB41:def 1;
( f . s ) = ( meet ( SubSort (A,s) ) ) by A1;
hence thesis by A3,SETFAM_1:3;
end;
then reconsider f as (MSSubset of U0) by PBOOLE:def 18;
take f;
thus thesis by A1;
end;
uniqueness
proof
let W1 being (MSSubset of U0),W2 being (MSSubset of U0);
assume that
A4: (for s being (SortSymbol of S) holds ( W1 . s ) = ( meet ( SubSort (A,s) ) ))
and
A5: (for s being (SortSymbol of S) holds ( W2 . s ) = ( meet ( SubSort (A,s) ) ));
(for s being set holds (s in (the carrier of S) implies ( W1 . s ) = ( W2 . s )))
proof
let s being set;
assume s in (the carrier of S);
then reconsider s as (SortSymbol of S);
( W1 . s ) = ( meet ( SubSort (A,s) ) ) by A4;
hence thesis by A5;
end;
hence thesis by PBOOLE:3;
end;
end;
