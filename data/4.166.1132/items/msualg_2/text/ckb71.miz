constructors SETFAM_1,BINOP_1,CARD_3,LATTICES,MSUALG_1,RELSET_1,CKB5,CKB6,CKB9,CKB10,CKB13,CKB14,CKB16,CKB17,CKB20,CKB33,CKB36,CKB38,CKB41,CKB43,CKB50,CKB51,CKB52,CKB57,CKB63,CKB65,CKB66;
requirements BOOLE,SUBSET;
begin
definition
let S being non void non empty ManySortedSign;
let U0 being  non-empty MSAlgebra over S;
func MSSubAlLattice U0 ->  strict Lattice
equals
LattStr (# ( MSSub U0 ),( MSAlg_join U0 ),( MSAlg_meet U0 ) #);
coherence
proof
set L = LattStr (# ( MSSub U0 ),( MSAlg_join U0 ),( MSAlg_meet U0 ) #);
A1: (for a,b,c being (Element of L) holds ( a "\/" ( b "\/" c ) ) = ( ( a "\/" b ) "\/" c ))
proof
let a being (Element of L),b being (Element of L),c being (Element of L);
( MSAlg_join U0 ) is  associative by CKB68:1;
hence thesis by BINOP_1:def 3;
end;
A2: (for a,b being (Element of L) holds ( a "/\" b ) = ( b "/\" a ))
proof
let a being (Element of L),b being (Element of L);
( MSAlg_meet U0 ) is  commutative by CKB69:1;
hence thesis by BINOP_1:def 2;
end;
A3: (for a,b being (Element of L) holds ( a "/\" ( a "\/" b ) ) = a)
proof
let a being (Element of L),b being (Element of L);
reconsider x = a,y = b as (Element of ( MSSub U0 ));
( ( MSAlg_meet U0 ) . (x,( ( MSAlg_join U0 ) . (x,y) )) ) = x
proof
reconsider U1 = x,U2 = y as  strict (MSSubAlgebra of U0) by CKB63:def 1;
( ( MSAlg_join U0 ) . (x,y) ) = ( U1 "\/" U2 ) by CKB65:def 1;
hence ( ( MSAlg_meet U0 ) . (x,( ( MSAlg_join U0 ) . (x,y) )) ) = ( U1 /\ ( U1 "\/" U2 ) ) by CKB66:def 1
.= x by CKB61:1;
end;
hence thesis;
end;
A4: (for a,b being (Element of L) holds ( ( a "/\" b ) "\/" b ) = b)
proof
let a being (Element of L),b being (Element of L);
reconsider x = a,y = b as (Element of ( MSSub U0 ));
( ( MSAlg_join U0 ) . (( ( MSAlg_meet U0 ) . (x,y) ),y) ) = y
proof
reconsider U1 = x,U2 = y as  strict (MSSubAlgebra of U0) by CKB63:def 1;
( ( MSAlg_meet U0 ) . (x,y) ) = ( U1 /\ U2 ) by CKB66:def 1;
hence ( ( MSAlg_join U0 ) . (( ( MSAlg_meet U0 ) . (x,y) ),y) ) = ( ( U1 /\ U2 ) "\/" U2 ) by CKB65:def 1
.= y by CKB62:1;
end;
hence thesis;
end;
A5: (for a,b,c being (Element of L) holds ( a "/\" ( b "/\" c ) ) = ( ( a "/\" b ) "/\" c ))
proof
let a being (Element of L),b being (Element of L),c being (Element of L);
( MSAlg_meet U0 ) is  associative by CKB70:1;
hence thesis by BINOP_1:def 3;
end;
(for a,b being (Element of L) holds ( a "\/" b ) = ( b "\/" a ))
proof
let a being (Element of L),b being (Element of L);
( MSAlg_join U0 ) is  commutative by CKB67:1;
hence thesis by BINOP_1:def 2;
end;
then L is  strict  join-commutative  join-associative  meet-absorbing  meet-commutative  meet-associative  join-absorbing by A1,A4,A2,A5,A3,LATTICES:def 4,LATTICES:def 5,LATTICES:def 6,LATTICES:def 7,LATTICES:def 8,LATTICES:def 9;
hence thesis;
end;
end;
