schemes CLASSES1,BINOP_1,FUNCT_1,XBOOLE_0;
registrations XBOOLE_0,SUBSET_1,RELAT_1,FUNCT_1,PBOOLE,STRUCT_0,LATTICES,MSUALG_1,FINSEQ_1,FUNCT_2,PARTFUN1,RELSET_1,CKB1,CKB2,CKB7,CKB8,CKB11,CKB12,CKB22,CKB23,CKB24,CKB35,CKB37,CKB42;
constructors SETFAM_1,BINOP_1,CARD_3,LATTICES,MSUALG_1,RELSET_1,CKB5,CKB6,CKB9,CKB10,CKB13,CKB14,CKB16,CKB17,CKB20,CKB33,CKB36,CKB38,CKB41,CKB43,CKB50,CKB51;
requirements BOOLE,SUBSET;
begin
definition
let S being non void non empty ManySortedSign;
let U0 being MSAlgebra over S;
let A being (MSSubset of U0);
func GenMSAlg A ->  strict (MSSubAlgebra of U0)
means
:Def18: (A is (MSSubset of it) & (for U1 being (MSSubAlgebra of U0) holds (A is (MSSubset of U1) implies it is (MSSubAlgebra of U1))));
existence
proof
set a = ( MSSubSort A );
reconsider u1 = ( U0 | a ) as  strict (MSSubAlgebra of U0);
take u1;
A1: u1 = MSAlgebra (# a,(( Opers (U0,a) ) qua (ManySortedFunction of ( ( a # ) * (the Arity of S) ),( a * (the ResultSort of S) ))) #) by CKB50:def 1,CKB49:1;
A c= a by CKB49:1;
hence A is (MSSubset of u1) by A1,PBOOLE:def 18;
let U2 being (MSSubAlgebra of U0);
reconsider B = (the Sorts of U2) as (MSSubset of U0) by CKB20:def 1;
assume A is (MSSubset of U2);
then A2: A c= B by PBOOLE:def 18;
( Constants U0 ) is (MSSubset of U2) by CKB30:1;
then A3: ( Constants U0 ) c= B by PBOOLE:def 18;
B is  opers_closed by CKB20:def 1;
then A4: B in ( SubSort A ) by A2,A3,CKB39:1;
(the Sorts of u1) c= (the Sorts of U2)
proof
let i being set;
assume i in (the carrier of S);
then reconsider s = i as (SortSymbol of S);
(( (the Sorts of u1) . s ) = ( meet ( SubSort (A,s) ) ) & ( B . s ) in ( SubSort (A,s) )) by A1,A4,CKB41:def 1,CKB43:def 1;
hence thesis by SETFAM_1:3;
end;
hence thesis by CKB28:1;
end;
uniqueness
proof
let W1 being  strict (MSSubAlgebra of U0),W2 being  strict (MSSubAlgebra of U0);
assume (((A is (MSSubset of W1) & (for U1 being (MSSubAlgebra of U0) holds (A is (MSSubset of U1) implies W1 is (MSSubAlgebra of U1)))) & A is (MSSubset of W2)) & (for U2 being (MSSubAlgebra of U0) holds (A is (MSSubset of U2) implies W2 is (MSSubAlgebra of U2))));
then (W1 is  strict  strict (MSSubAlgebra of W2) & W2 is  strict  strict (MSSubAlgebra of W1));
hence thesis by CKB27:1;
end;
end;
