environ
vocabularies PBOOLE,RELAT_1,XBOOLE_0,FUNCT_1,SUBSET_1,CARD_3,TARSKI,STRUCT_0,MSUALG_1,UNIALG_2,FINSEQ_1,MARGREL1,PARTFUN1,FUNCT_2,ZFMISC_1,QC_LANG1,SETFAM_1,EQREL_1,BINOP_1,LATTICES,XXREAL_2,MSUALG_2;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,STRUCT_0,RELAT_1,FUNCT_1,FUNCT_2,FINSEQ_1,SETFAM_1,FINSEQ_2,LATTICES,BINOP_1,CARD_3,PBOOLE,MSUALG_1,CKB4,CKB5,CKB6,CKB9,CKB10,CKB13,CKB14,CKB16,CKB17,CKB20,CKB33,CKB36,CKB38,CKB41,CKB43,CKB50,CKB51,CKB52;
definitions TARSKI,PBOOLE,LATTICES,XBOOLE_0,CKB5,CKB6,CKB9,CKB10,CKB13,CKB14,CKB16,CKB17,CKB20,CKB33,CKB36,CKB38,CKB41,CKB43,CKB50,CKB51,CKB52;
theorems TARSKI,FUNCT_1,FINSEQ_1,PBOOLE,CARD_3,MSUALG_1,FUNCT_2,ZFMISC_1,SETFAM_1,BINOP_1,LATTICES,RELAT_1,RELSET_1,XBOOLE_0,XBOOLE_1,FUNCOP_1,PARTFUN1,FINSEQ_2,CKB3,CKB5,CKB6,CKB9,CKB10,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB36,CKB38,CKB39,CKB40,CKB41,CKB43,CKB44,CKB45,CKB46,CKB47,CKB48,CKB49,CKB50,CKB51,CKB52;
schemes CLASSES1,BINOP_1,FUNCT_1,XBOOLE_0;
registrations XBOOLE_0,SUBSET_1,RELAT_1,FUNCT_1,PBOOLE,STRUCT_0,LATTICES,MSUALG_1,FINSEQ_1,FUNCT_2,PARTFUN1,RELSET_1,CKB1,CKB2,CKB7,CKB8,CKB11,CKB12,CKB22,CKB23,CKB24,CKB35,CKB37,CKB42;
constructors SETFAM_1,BINOP_1,CARD_3,LATTICES,MSUALG_1,RELSET_1,CKB5,CKB6,CKB9,CKB10,CKB13,CKB14,CKB16,CKB17,CKB20,CKB33,CKB36,CKB38,CKB41,CKB43,CKB50,CKB51,CKB52;
requirements BOOLE,SUBSET;
begin
registration
let S being non void non empty ManySortedSign;
let U0 being  non-empty MSAlgebra over S;
let A being  non-empty (MSSubset of U0);
cluster ( GenMSAlg A ) ->  non-empty;
coherence
proof
( ( Constants U0 ) \/ A ) is  non-empty;
then reconsider a = ( MSSubSort A ) as  non-empty (MSSubset of U0) by CKB45:1;
( U0 | a ) = MSAlgebra (# a,(( Opers (U0,a) ) qua (ManySortedFunction of ( ( a # ) * (the Arity of S) ),( a * (the ResultSort of S) ))) #) by CKB50:def 1,CKB49:1;
then reconsider u1 = ( U0 | a ) as  strict  non-empty (MSSubAlgebra of U0) by MSUALG_1:def 3;
A1: A c= a by CKB49:1;
now
A2: u1 = MSAlgebra (# a,(( Opers (U0,a) ) qua (ManySortedFunction of ( ( a # ) * (the Arity of S) ),( a * (the ResultSort of S) ))) #) by CKB50:def 1,CKB49:1;
hence A is (MSSubset of u1) by A1,PBOOLE:def 18;
let U2 being (MSSubAlgebra of U0);
reconsider B = (the Sorts of U2) as (MSSubset of U0) by CKB20:def 1;
assume A is (MSSubset of U2);
then A3: A c= B by PBOOLE:def 18;
( Constants U0 ) is (MSSubset of U2) by CKB30:1;
then A4: ( Constants U0 ) c= B by PBOOLE:def 18;
B is  opers_closed by CKB20:def 1;
then A5: B in ( SubSort A ) by A3,A4,CKB39:1;
(the Sorts of u1) c= (the Sorts of U2)
proof
let i being set;
assume i in (the carrier of S);
then reconsider s = i as (SortSymbol of S);
(( (the Sorts of u1) . s ) = ( meet ( SubSort (A,s) ) ) & ( B . s ) in ( SubSort (A,s) )) by A2,A5,CKB41:def 1,CKB43:def 1;
hence thesis by SETFAM_1:3;
end;
hence u1 is (MSSubAlgebra of U2) by CKB28:1;
end;
hence thesis by CKB52:def 1;
end;
end;
