constructors BINOP_1,REALSET2,LATTICE3,WAYBEL_1,YELLOW_6,OPOSET_1,SHEFFER1,FUNCT_5,RELSET_1,PARTIT_2,CKB1,CKB2,CKB3;
requirements SUBSET,BOOLE,NUMERALS;
begin
theorem
Th4: (for L being non empty LattStr holds (L is  meet-Associative  join-Associative  meet-Absorbing  join-absorbing implies L is  meet-associative  join-associative))
proof
let L being non empty LattStr;
assume A1: L is  meet-Associative  join-Associative  meet-Absorbing  join-absorbing;
then A2: L is  meet-commutative  join-commutative by CKB5:1;
A3: (for x,y,z being (Element of L) holds ( x "\/" ( y "\/" z ) ) = ( ( x "\/" y ) "\/" z ))
proof
let a being (Element of L),b being (Element of L),c being (Element of L);
( a "\/" ( b "\/" c ) ) = ( a "\/" ( c "\/" b ) ) by A2,LATTICES:def 4
.= ( c "\/" ( a "\/" b ) ) by A1,CKB1:def 1
.= ( ( a "\/" b ) "\/" c ) by A2,LATTICES:def 4;
hence thesis;
end;
(for x,y,z being (Element of L) holds ( x "/\" ( y "/\" z ) ) = ( ( x "/\" y ) "/\" z ))
proof
let a being (Element of L),b being (Element of L),c being (Element of L);
( a "/\" ( b "/\" c ) ) = ( a "/\" ( c "/\" b ) ) by A2,LATTICES:def 6
.= ( c "/\" ( a "/\" b ) ) by A1,CKB2:def 1
.= ( ( a "/\" b ) "/\" c ) by A2,LATTICES:def 6;
hence thesis;
end;
hence thesis by A3,LATTICES:def 5,LATTICES:def 7;
end;
