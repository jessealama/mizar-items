environ
vocabularies NUMBERS,REAL_1,SUBSET_1,RCOMP_1,PARTFUN1,TARSKI,RELAT_1,TAYLOR_1,FUNCT_1,ARYTM_3,XXREAL_0,CARD_1,FDIFF_1,ARYTM_1,VALUED_1,XBOOLE_0,PREPOWER,NEWTON,ORDINAL4,SQUARE_1,SIN_COS;
notations TARSKI,XBOOLE_0,SUBSET_1,RELAT_1,PARTFUN1,PARTFUN2,RCOMP_1,NUMBERS,REAL_1,NAT_1,VALUED_1,SEQ_1,FDIFF_1,NEWTON,PREPOWER,TAYLOR_1,SIN_COS,RFUNCT_1,SQUARE_1,SIN_COS4,XXREAL_0;
definitions SQUARE_1,VALUED_1;
theorems FDIFF_2,TAYLOR_1,XBOOLE_1,XCMPLX_1,FDIFF_1,FUNCT_1,TARSKI,PREPOWER,RFUNCT_1,XREAL_1,XCMPLX_0,NEWTON,SIN_COS,SIN_COS5,SIN_COS4,WSIERP_1,VALUED_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12;
registrations ORDINAL1,RELSET_1,NUMBERS,INT_1,MEMBERED,RCOMP_1,VALUED_0;
constructors PARTFUN1,REAL_1,SQUARE_1,NAT_1,RCOMP_1,PARTFUN2,RFUNCT_1,LIMFUNC1,FDIFF_1,PREPOWER,SIN_COS,SIN_COS4,TAYLOR_1,SEQ_1,VALUED_1,RELSET_1,NEWTON;
requirements SUBSET,REAL,NUMERALS,ARITHM,BOOLE;
begin
reserve y for set;
reserve x for Real;
reserve a for Real;
reserve b for Real;
reserve c for Real;
reserve Z for  open (Subset of ( REAL ));
reserve f1 for (PartFunc of ( REAL ),( REAL ));
reserve f2 for (PartFunc of ( REAL ),( REAL ));
theorem
Th13: (((Z c= ( dom ( ( ln ) * ( f1 + ( c (#) f2 ) ) ) ) & f2 = ( #Z 2 )) & (for x holds (x in Z implies (( f1 . x ) = ( a + ( b * x ) ) & ( ( f1 + ( c (#) f2 ) ) . x ) > ( 0 ))))) implies (( ( ln ) * ( f1 + ( c (#) f2 ) ) ) is_differentiable_on Z & (for x holds (x in Z implies ( ( ( ( ln ) * ( f1 + ( c (#) f2 ) ) ) `| Z ) . x ) = ( ( b + ( ( 2 * c ) * x ) ) / ( ( a + ( b * x ) ) + ( c * ( x |^ 2 ) ) ) )))))
proof
assume that
A1: Z c= ( dom ( ( ln ) * ( f1 + ( c (#) f2 ) ) ) )
and
A2: f2 = ( #Z 2 )
and
A3: (for x holds (x in Z implies (( f1 . x ) = ( a + ( b * x ) ) & ( ( f1 + ( c (#) f2 ) ) . x ) > ( 0 ))));
(for y holds (y in Z implies y in ( dom ( f1 + ( c (#) f2 ) ) ))) by A1,FUNCT_1:11;
then A4: Z c= ( dom ( f1 + ( c (#) f2 ) ) ) by TARSKI:def 3;
then Z c= ( ( dom f1 ) /\ ( dom ( c (#) f2 ) ) ) by VALUED_1:def 1;
then A5: Z c= ( dom ( c (#) f2 ) ) by XBOOLE_1:18;
A6: (for x holds (x in Z implies ( f1 . x ) = ( a + ( b * x ) ))) by A3;
then A7: ( f1 + ( c (#) f2 ) ) is_differentiable_on Z by A2,A4,CKB12:1;
A8: (for x holds (x in Z implies ( ( ln ) * ( f1 + ( c (#) f2 ) ) ) is_differentiable_in x))
proof
let x;
assume x in Z;
then (( f1 + ( c (#) f2 ) ) is_differentiable_in x & ( ( f1 + ( c (#) f2 ) ) . x ) > ( 0 )) by A3,A7,FDIFF_1:9;
hence thesis by TAYLOR_1:20;
end;
then A9: ( ( ln ) * ( f1 + ( c (#) f2 ) ) ) is_differentiable_on Z by A1,FDIFF_1:9;
(for x holds (x in Z implies ( ( ( ( ln ) * ( f1 + ( c (#) f2 ) ) ) `| Z ) . x ) = ( ( b + ( ( 2 * c ) * x ) ) / ( ( a + ( b * x ) ) + ( c * ( x |^ 2 ) ) ) )))
proof
let x;
assume A10: x in Z;
then x in ( dom ( f1 + ( c (#) f2 ) ) ) by A1,FUNCT_1:11;
then A11: ( ( f1 + ( c (#) f2 ) ) . x ) = ( ( f1 . x ) + ( ( c (#) f2 ) . x ) ) by VALUED_1:def 1
.= ( ( f1 . x ) + ( c * ( f2 . x ) ) ) by A5,A10,VALUED_1:def 5
.= ( ( a + ( b * x ) ) + ( c * ( f2 . x ) ) ) by A3,A10
.= ( ( a + ( b * x ) ) + ( c * ( x #Z 2 ) ) ) by A2,TAYLOR_1:def 1
.= ( ( a + ( b * x ) ) + ( c * ( x |^ 2 ) ) ) by PREPOWER:36;
(( f1 + ( c (#) f2 ) ) is_differentiable_in x & ( ( f1 + ( c (#) f2 ) ) . x ) > ( 0 )) by A3,A7,A10,FDIFF_1:9;
then ( diff (( ( ln ) * ( f1 + ( c (#) f2 ) ) ),x) ) = ( ( diff (( f1 + ( c (#) f2 ) ),x) ) / ( ( f1 + ( c (#) f2 ) ) . x ) ) by TAYLOR_1:20
.= ( ( ( ( f1 + ( c (#) f2 ) ) `| Z ) . x ) / ( ( f1 + ( c (#) f2 ) ) . x ) ) by A7,A10,FDIFF_1:def 7
.= ( ( b + ( ( 2 * c ) * x ) ) / ( ( a + ( b * x ) ) + ( c * ( x |^ 2 ) ) ) ) by A2,A4,A6,A10,A11,CKB12:1;
hence thesis by A9,A10,FDIFF_1:def 7;
end;
hence thesis by A1,A8,FDIFF_1:9;
end;
