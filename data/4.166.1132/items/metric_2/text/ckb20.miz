begin
theorem
Th19: (for M being non  empty MetrSpace holds (for x,y being (Element of M) holds (y in ( x -neighbour ) iff y = x)))
proof
let M being non  empty MetrSpace;
let x being (Element of M);
let y being (Element of M);
hereby
assume y in ( x -neighbour );
then (ex q being (Element of M) st (y = q & x tolerates q));
hence y = x by CKB19:1;
end;
assume y = x;
then x tolerates y by CKB19:1;
hence thesis;
end;
