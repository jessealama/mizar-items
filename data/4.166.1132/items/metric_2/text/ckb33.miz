begin
theorem
Th32: (for M being PseudoMetricSpace holds (for V,Q being (Element of ( M -neighbour )) holds (for v being (Element of ( REAL )) holds (V,Q is_dst v implies Q,V is_dst v))))
proof
let M being PseudoMetricSpace;
let V being (Element of ( M -neighbour )),Q being (Element of ( M -neighbour ));
let v being (Element of ( REAL ));
assume V,Q is_dst v;
then (for q,p being (Element of M) holds ((q in Q & p in V) implies ( dist (q,p) ) = v)) by CKB31:def 1;
hence thesis by CKB31:def 1;
end;
