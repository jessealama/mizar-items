environ
vocabularies NUMBERS,SUBSET_1,REAL_1,SEQ_1,PARTFUN1,CARD_1,ARYTM_3,RELAT_1,COMPLEX1,ARYTM_1,FUNCT_1,TARSKI,XBOOLE_0,XXREAL_0,VALUED_1,XXREAL_2,XREAL_0,ORDINAL1,ORDINAL2,FCONT_1,RCOMP_1,VALUED_0,SEQ_2,FUNCT_2,NAT_1,SEQ_4,XXREAL_1,FUNCOP_1,ORDINAL4,SEQM_3,FCONT_2;
notations TARSKI,XBOOLE_0,SUBSET_1,ORDINAL1,NUMBERS,XCMPLX_0,XXREAL_0,XREAL_0,XXREAL_2,VALUED_0,COMPLEX1,REAL_1,NAT_1,FUNCT_1,RELSET_1,PARTFUN1,FUNCT_2,SQUARE_1,VALUED_1,SEQ_1,SEQ_2,SEQ_4,PARTFUN2,RFUNCT_1,RFUNCT_2,RCOMP_1,FCONT_1,RECDEF_1,CKB1;
definitions VALUED_1,CKB1;
theorems TARSKI,SUBSET_1,NAT_1,FUNCT_1,ABSVALUE,SEQ_2,SEQ_4,PARTFUN2,RFUNCT_1,RFUNCT_2,RCOMP_1,FCONT_1,RELAT_1,FUNCT_2,XREAL_0,XBOOLE_0,XBOOLE_1,XCMPLX_0,XCMPLX_1,FUNCOP_1,XREAL_1,COMPLEX1,XXREAL_0,ORDINAL1,VALUED_1,XXREAL_2,XXREAL_1,VALUED_0,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18;
schemes NAT_1,SEQ_1,FUNCT_2;
registrations XBOOLE_0,RELSET_1,NUMBERS,XXREAL_0,XREAL_0,NAT_1,MEMBERED,RFUNCT_2,VALUED_0,VALUED_1,FUNCT_2,ORDINAL1,XXREAL_2,FCONT_1;
constructors PARTFUN1,REAL_1,SQUARE_1,NAT_1,COMPLEX1,VALUED_1,SEQ_2,SEQM_3,SEQ_4,RCOMP_1,PARTFUN2,RFUNCT_1,RFUNCT_2,FCONT_1,SEQ_1,RECDEF_1,XXREAL_2,RELSET_1,BINOP_2,CKB1;
requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve g for Real;
reserve r for Real;
reserve t for Real;
reserve p for Real;
reserve x1 for Real;
reserve x2 for Real;
reserve f for (PartFunc of ( REAL ),( REAL ));
theorem
((((f is  one-to-one & p <= g) & [. p,g .] c= ( dom f )) & ( f | [. p,g .] ) is  continuous) implies ((( lower_bound ( f .: [. p,g .] ) ) = ( f . p ) & ( upper_bound ( f .: [. p,g .] ) ) = ( f . g )) or (( lower_bound ( f .: [. p,g .] ) ) = ( f . g ) & ( upper_bound ( f .: [. p,g .] ) ) = ( f . p ))))
proof
assume that
A1: f is  one-to-one
and
A2: p <= g
and
A3: [. p,g .] c= ( dom f )
and
A4: ( f | [. p,g .] ) is  continuous;
A5: p in [. p,g .] by A2,XXREAL_1:1;
then A6: ( f . p ) in ( f .: [. p,g .] ) by A3,FUNCT_1:def 6;
A7: g in [. p,g .] by A2,XXREAL_1:1;
then A8: ( f . g ) in ( f .: [. p,g .] ) by A3,FUNCT_1:def 6;
A9: g in ( [. p,g .] /\ ( dom f ) ) by A3,A7,XBOOLE_0:def 4;
A10: p in ( [. p,g .] /\ ( dom f ) ) by A3,A5,XBOOLE_0:def 4;
[. p,g .] <> ( {} ) by A2,XXREAL_1:1;
then consider x1,x2 being  real number such that A11: x1 in [. p,g .] and A12: x2 in [. p,g .] and A13: ( f . x1 ) = ( upper_bound ( f .: [. p,g .] ) ) and A14: ( f . x2 ) = ( lower_bound ( f .: [. p,g .] ) ) by A3,A4,FCONT_1:31,RCOMP_1:6;
A15: x2 in ( [. p,g .] /\ ( dom f ) ) by A3,A12,XBOOLE_0:def 4;
x2 in { t: (p <= t & t <= g) } by A12,RCOMP_1:def 1;
then A16: (ex r st ((r = x2 & p <= r) & r <= g));
x1 in { r: (p <= r & r <= g) } by A11,RCOMP_1:def 1;
then A17: (ex r st ((r = x1 & p <= r) & r <= g));
[. p,g .] is  compact by RCOMP_1:6;
then A18: ( f .: [. p,g .] ) is  bounded by A3,A4,FCONT_1:29,RCOMP_1:10;
A19: x1 in ( [. p,g .] /\ ( dom f ) ) by A3,A11,XBOOLE_0:def 4;
now
per cases  by A1,A2,A3,A4,CKB18:1;
suppose A20: ( f | [. p,g .] ) is  increasing;

A21:now
assume x1 <> g;
then x1 < g by A17,XXREAL_0:1;
then ( f . x1 ) < ( f . g ) by A9,A19,A20,RFUNCT_2:20;
hence contradiction by A8,A18,A13,SEQ_4:def 1;
end;
now
assume x2 <> p;
then p < x2 by A16,XXREAL_0:1;
then ( f . p ) < ( f . x2 ) by A10,A15,A20,RFUNCT_2:20;
hence contradiction by A6,A18,A14,SEQ_4:def 2;
end;
hence thesis by A13,A14,A21;
end;
suppose A22: ( f | [. p,g .] ) is  decreasing;

A23:now
assume x2 <> g;
then x2 < g by A16,XXREAL_0:1;
then ( f . g ) < ( f . x2 ) by A9,A15,A22,RFUNCT_2:21;
hence contradiction by A8,A18,A14,SEQ_4:def 2;
end;
now
assume x1 <> p;
then p < x1 by A17,XXREAL_0:1;
then ( f . x1 ) < ( f . p ) by A10,A19,A22,RFUNCT_2:21;
hence contradiction by A6,A18,A13,SEQ_4:def 1;
end;
hence thesis by A13,A14,A23;
end;
end;
hence thesis;
end;
