environ
vocabularies NUMBERS,SUBSET_1,REAL_1,SEQ_1,PARTFUN1,CARD_1,ARYTM_3,RELAT_1,COMPLEX1,ARYTM_1,FUNCT_1,TARSKI,XBOOLE_0,XXREAL_0,VALUED_1,XXREAL_2,XREAL_0,ORDINAL1,ORDINAL2,FCONT_1,RCOMP_1,VALUED_0,SEQ_2,FUNCT_2,NAT_1,SEQ_4,XXREAL_1,FUNCOP_1,ORDINAL4,SEQM_3,FCONT_2;
notations TARSKI,XBOOLE_0,SUBSET_1,ORDINAL1,NUMBERS,XCMPLX_0,XXREAL_0,XREAL_0,XXREAL_2,VALUED_0,COMPLEX1,REAL_1,NAT_1,FUNCT_1,RELSET_1,PARTFUN1,FUNCT_2,SQUARE_1,VALUED_1,SEQ_1,SEQ_2,SEQ_4,PARTFUN2,RFUNCT_1,RFUNCT_2,RCOMP_1,FCONT_1,RECDEF_1,CKB1;
definitions VALUED_1,CKB1;
theorems TARSKI,SUBSET_1,NAT_1,FUNCT_1,ABSVALUE,SEQ_2,SEQ_4,PARTFUN2,RFUNCT_1,RFUNCT_2,RCOMP_1,FCONT_1,RELAT_1,FUNCT_2,XREAL_0,XBOOLE_0,XBOOLE_1,XCMPLX_0,XCMPLX_1,FUNCOP_1,XREAL_1,COMPLEX1,XXREAL_0,ORDINAL1,VALUED_1,XXREAL_2,XXREAL_1,VALUED_0,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16;
schemes NAT_1,SEQ_1,FUNCT_2;
registrations XBOOLE_0,RELSET_1,NUMBERS,XXREAL_0,XREAL_0,NAT_1,MEMBERED,RFUNCT_2,VALUED_0,VALUED_1,FUNCT_2,ORDINAL1,XXREAL_2,FCONT_1;
constructors PARTFUN1,REAL_1,SQUARE_1,NAT_1,COMPLEX1,VALUED_1,SEQ_2,SEQM_3,SEQ_4,RCOMP_1,PARTFUN2,RFUNCT_1,RFUNCT_2,FCONT_1,SEQ_1,RECDEF_1,XXREAL_2,RELSET_1,BINOP_2,CKB1;
requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve s for Real;
reserve g for Real;
reserve r for Real;
reserve p for Real;
reserve f for (PartFunc of ( REAL ),( REAL ));
theorem
Th17: (((p <= g & [. p,g .] c= ( dom f )) & ( f | [. p,g .] ) is  continuous) implies (for r holds (r in [. ( lower_bound ( f .: [. p,g .] ) ),( upper_bound ( f .: [. p,g .] ) ) .] implies (ex s st (s in [. p,g .] & r = ( f . s ))))))
proof
assume that
A1: p <= g
and
A2: [. p,g .] c= ( dom f )
and
A3: ( f | [. p,g .] ) is  continuous;
[. p,g .] is  compact by RCOMP_1:6;
then A4: ( f .: [. p,g .] ) is  bounded by A2,A3,FCONT_1:29,RCOMP_1:10;
set ub = ( upper_bound ( f .: [. p,g .] ) );
set lb = ( lower_bound ( f .: [. p,g .] ) );
[. p,g .] <> ( {} ) by A1,XXREAL_1:1;
then consider x2 being  real number,x1 being  real number such that A5: x2 in [. p,g .] and A6: x1 in [. p,g .] and A7: ( f . x2 ) = ub and A8: ( f . x1 ) = lb by A2,A3,FCONT_1:31,RCOMP_1:6;
reconsider x1,x2 as Real by XREAL_0:def 1;
let r;
assume that
A9: r in [. lb,ub .];
( f . x1 ) in ( f .: [. p,g .] ) by A2,A6,FUNCT_1:def 6;
then A10: [. lb,ub .] = ( [. lb,ub .] \/ [. ub,lb .] ) by A4,SEQ_4:11,XXREAL_1:222;
now
per cases ;
suppose A11: x1 <= x2;

A12: [. x1,x2 .] c= [. p,g .] by A5,A6,XXREAL_2:def 12;
A13: [. x1,x2 .] c= [. p,g .] by A5,A6,XXREAL_2:def 12;
then ( f | [. x1,x2 .] ) is  continuous by A3,FCONT_1:16;
then consider s such that A14: s in [. x1,x2 .] and A15: r = ( f . s ) by A2,A9,A7,A8,A10,A11,A12,CKB16:1,XBOOLE_1:1;
take s;
thus (s in [. p,g .] & r = ( f . s )) by A13,A14,A15;
end;
suppose A16: x2 <= x1;

A17: [. x2,x1 .] c= [. p,g .] by A5,A6,XXREAL_2:def 12;
A18: [. x2,x1 .] c= [. p,g .] by A5,A6,XXREAL_2:def 12;
then ( f | [. x2,x1 .] ) is  continuous by A3,FCONT_1:16;
then consider s such that A19: s in [. x2,x1 .] and A20: r = ( f . s ) by A2,A9,A7,A8,A10,A16,A17,CKB16:1,XBOOLE_1:1;
take s;
thus (s in [. p,g .] & r = ( f . s )) by A18,A19,A20;
end;
end;
hence thesis;
end;
