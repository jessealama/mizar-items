environ
vocabularies NUMBERS,SUBSET_1,REAL_1,SEQ_1,PARTFUN1,CARD_1,ARYTM_3,RELAT_1,COMPLEX1,ARYTM_1,FUNCT_1,TARSKI,XBOOLE_0,XXREAL_0,VALUED_1,XXREAL_2,XREAL_0,ORDINAL1,ORDINAL2,FCONT_1,RCOMP_1,VALUED_0,SEQ_2,FUNCT_2,NAT_1,SEQ_4,XXREAL_1,FUNCOP_1,ORDINAL4,SEQM_3,FCONT_2;
notations TARSKI,XBOOLE_0,SUBSET_1,ORDINAL1,NUMBERS,XCMPLX_0,XXREAL_0,XREAL_0,XXREAL_2,VALUED_0,COMPLEX1,REAL_1,NAT_1,FUNCT_1,RELSET_1,PARTFUN1,FUNCT_2,SQUARE_1,VALUED_1,SEQ_1,SEQ_2,SEQ_4,PARTFUN2,RFUNCT_1,RFUNCT_2,RCOMP_1,FCONT_1,RECDEF_1,CKB1;
definitions VALUED_1,CKB1;
theorems TARSKI,SUBSET_1,NAT_1,FUNCT_1,ABSVALUE,SEQ_2,SEQ_4,PARTFUN2,RFUNCT_1,RFUNCT_2,RCOMP_1,FCONT_1,RELAT_1,FUNCT_2,XREAL_0,XBOOLE_0,XBOOLE_1,XCMPLX_0,XCMPLX_1,FUNCOP_1,XREAL_1,COMPLEX1,XXREAL_0,ORDINAL1,VALUED_1,XXREAL_2,XXREAL_1,VALUED_0,CKB1,CKB2,CKB3,CKB4,CKB5;
schemes NAT_1,SEQ_1,FUNCT_2;
registrations XBOOLE_0,RELSET_1,NUMBERS,XXREAL_0,XREAL_0,NAT_1,MEMBERED,RFUNCT_2,VALUED_0,VALUED_1,FUNCT_2,ORDINAL1,XXREAL_2,FCONT_1;
constructors PARTFUN1,REAL_1,SQUARE_1,NAT_1,COMPLEX1,VALUED_1,SEQ_2,SEQM_3,SEQ_4,RCOMP_1,PARTFUN2,RFUNCT_1,RFUNCT_2,FCONT_1,SEQ_1,RECDEF_1,XXREAL_2,RELSET_1,BINOP_2,CKB1;
requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve X for set;
reserve s for Real;
reserve r for Real;
reserve p for Real;
reserve x1 for Real;
reserve x2 for Real;
reserve f for (PartFunc of ( REAL ),( REAL ));
theorem
Th5: ((X c= ( dom f ) & ( f | X ) is  uniformly_continuous) implies ( ( p (#) f ) | X ) is  uniformly_continuous)
proof
assume X c= ( dom f );
then A1: X c= ( dom ( p (#) f ) ) by VALUED_1:def 5;
assume A2: ( f | X ) is  uniformly_continuous;
per cases ;
suppose A3: p = ( 0 );

now
let r;
assume A4: ( 0 ) < r;
then consider s such that A5: ( 0 ) < s and (for x1 holds (for x2 holds (((x1 in ( dom ( f | X ) ) & x2 in ( dom ( f | X ) )) & ( abs ( x1 - x2 ) ) < s) implies ( abs ( ( f . x1 ) - ( f . x2 ) ) ) < r))) by A2,CKB2:1;
take s;
thus ( 0 ) < s by A5;
let x1;
let x2;
assume that
A6: x1 in ( dom ( ( p (#) f ) | X ) )
and
A7: x2 in ( dom ( ( p (#) f ) | X ) )
and
( abs ( x1 - x2 ) ) < s;
A8: x2 in X by A7,RELAT_1:57;
x1 in X by A6,RELAT_1:57;
then ( abs ( ( ( p (#) f ) . x1 ) - ( ( p (#) f ) . x2 ) ) ) = ( abs ( ( p * ( f . x1 ) ) - ( ( p (#) f ) . x2 ) ) ) by A1,VALUED_1:def 5
.= ( abs ( ( 0 ) - ( p * ( f . x2 ) ) ) ) by A1,A3,A8,VALUED_1:def 5
.= ( 0 ) by A3,ABSVALUE:2;
hence ( abs ( ( ( p (#) f ) . x1 ) - ( ( p (#) f ) . x2 ) ) ) < r by A4;
end;
hence thesis by CKB2:1;
end;
suppose A9: p <> ( 0 );

then A10: ( 0 ) < ( abs p ) by COMPLEX1:47;
A11: ( 0 ) <> ( abs p ) by A9,COMPLEX1:47;
now
let r;
assume ( 0 ) < r;
then ( 0 ) < ( r / ( abs p ) ) by A10,XREAL_1:139;
then consider s such that A12: ( 0 ) < s and A13: (for x1 holds (for x2 holds (((x1 in ( dom ( f | X ) ) & x2 in ( dom ( f | X ) )) & ( abs ( x1 - x2 ) ) < s) implies ( abs ( ( f . x1 ) - ( f . x2 ) ) ) < ( r / ( abs p ) )))) by A2,CKB2:1;
take s;
thus ( 0 ) < s by A12;
let x1;
let x2;
assume that
A14: x1 in ( dom ( ( p (#) f ) | X ) )
and
A15: x2 in ( dom ( ( p (#) f ) | X ) )
and
A16: ( abs ( x1 - x2 ) ) < s;
A17: x2 in X by A15,RELAT_1:57;
A18: x1 in X by A14,RELAT_1:57;
then A19: ( abs ( ( ( p (#) f ) . x1 ) - ( ( p (#) f ) . x2 ) ) ) = ( abs ( ( p * ( f . x1 ) ) - ( ( p (#) f ) . x2 ) ) ) by A1,VALUED_1:def 5
.= ( abs ( ( p * ( f . x1 ) ) - ( p * ( f . x2 ) ) ) ) by A1,A17,VALUED_1:def 5
.= ( abs ( p * ( ( f . x1 ) - ( f . x2 ) ) ) )
.= ( ( abs p ) * ( abs ( ( f . x1 ) - ( f . x2 ) ) ) ) by COMPLEX1:65;
x2 in ( dom ( p (#) f ) ) by A15,RELAT_1:57;
then x2 in ( dom f ) by VALUED_1:def 5;
then A20: x2 in ( dom ( f | X ) ) by A17,RELAT_1:57;
x1 in ( dom ( p (#) f ) ) by A14,RELAT_1:57;
then x1 in ( dom f ) by VALUED_1:def 5;
then x1 in ( dom ( f | X ) ) by A18,RELAT_1:57;
then ( ( abs p ) * ( abs ( ( f . x1 ) - ( f . x2 ) ) ) ) < ( ( r / ( abs p ) ) * ( abs p ) ) by A10,A13,A16,A20,XREAL_1:68;
hence ( abs ( ( ( p (#) f ) . x1 ) - ( ( p (#) f ) . x2 ) ) ) < r by A11,A19,XCMPLX_1:87;
end;
hence thesis by CKB2:1;
end;
end;
