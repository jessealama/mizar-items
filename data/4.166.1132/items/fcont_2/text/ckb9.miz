environ
vocabularies NUMBERS,SUBSET_1,REAL_1,SEQ_1,PARTFUN1,CARD_1,ARYTM_3,RELAT_1,COMPLEX1,ARYTM_1,FUNCT_1,TARSKI,XBOOLE_0,XXREAL_0,VALUED_1,XXREAL_2,XREAL_0,ORDINAL1,ORDINAL2,FCONT_1,RCOMP_1,VALUED_0,SEQ_2,FUNCT_2,NAT_1,SEQ_4,XXREAL_1,FUNCOP_1,ORDINAL4,SEQM_3,FCONT_2;
notations TARSKI,XBOOLE_0,SUBSET_1,ORDINAL1,NUMBERS,XCMPLX_0,XXREAL_0,XREAL_0,XXREAL_2,VALUED_0,COMPLEX1,REAL_1,NAT_1,FUNCT_1,RELSET_1,PARTFUN1,FUNCT_2,SQUARE_1,VALUED_1,SEQ_1,SEQ_2,SEQ_4,PARTFUN2,RFUNCT_1,RFUNCT_2,RCOMP_1,FCONT_1,RECDEF_1,CKB1;
definitions VALUED_1,CKB1;
theorems TARSKI,SUBSET_1,NAT_1,FUNCT_1,ABSVALUE,SEQ_2,SEQ_4,PARTFUN2,RFUNCT_1,RFUNCT_2,RCOMP_1,FCONT_1,RELAT_1,FUNCT_2,XREAL_0,XBOOLE_0,XBOOLE_1,XCMPLX_0,XCMPLX_1,FUNCOP_1,XREAL_1,COMPLEX1,XXREAL_0,ORDINAL1,VALUED_1,XXREAL_2,XXREAL_1,VALUED_0,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8;
schemes NAT_1,SEQ_1,FUNCT_2;
registrations XBOOLE_0,RELSET_1,NUMBERS,XXREAL_0,XREAL_0,NAT_1,MEMBERED,RFUNCT_2,VALUED_0,VALUED_1,FUNCT_2,ORDINAL1,XXREAL_2,FCONT_1;
constructors PARTFUN1,REAL_1,SQUARE_1,NAT_1,COMPLEX1,VALUED_1,SEQ_2,SEQM_3,SEQ_4,RCOMP_1,PARTFUN2,RFUNCT_1,RFUNCT_2,FCONT_1,SEQ_1,RECDEF_1,XXREAL_2,RELSET_1,BINOP_2,CKB1;
requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve X for set;
reserve X1 for set;
reserve Z for set;
reserve Z1 for set;
reserve s for Real;
reserve g for Real;
reserve r for Real;
reserve p for Real;
reserve x1 for Real;
reserve x2 for Real;
reserve f1 for (PartFunc of ( REAL ),( REAL ));
reserve f2 for (PartFunc of ( REAL ),( REAL ));
theorem
((((((X c= ( dom f1 ) & X1 c= ( dom f2 )) & ( f1 | X ) is  uniformly_continuous) & ( f2 | X1 ) is  uniformly_continuous) & ( f1 | Z ) is  bounded) & ( f2 | Z1 ) is  bounded) implies ( ( f1 (#) f2 ) | ( ( ( X /\ Z ) /\ X1 ) /\ Z1 ) ) is  uniformly_continuous)
proof
assume that
A1: X c= ( dom f1 )
and
A2: X1 c= ( dom f2 );
assume that
A3: ( f1 | X ) is  uniformly_continuous
and
A4: ( f2 | X1 ) is  uniformly_continuous
and
A5: ( f1 | Z ) is  bounded
and
A6: ( f2 | Z1 ) is  bounded;
consider x1 being  real number such that A7: (for r being set holds (r in ( Z /\ ( dom f1 ) ) implies ( abs ( f1 . r ) ) <= x1)) by A5,RFUNCT_1:73;
consider x2 being  real number such that A8: (for r being set holds (r in ( Z1 /\ ( dom f2 ) ) implies ( abs ( f2 . r ) ) <= x2)) by A6,RFUNCT_1:73;
reconsider x1,x2 as Real by XREAL_0:def 1;
set M1 = ( ( abs x1 ) + 1 );
set M2 = ( ( abs x2 ) + 1 );
set M = ( max (M1,M2) );
A9:now
let r;
A10: M1 <= M by XXREAL_0:25;
assume r in ( ( ( X /\ Z ) /\ X1 ) /\ Z1 );
then A11: r in ( ( X /\ Z ) /\ ( X1 /\ Z1 ) ) by XBOOLE_1:16;
then A12: r in ( X /\ Z ) by XBOOLE_0:def 4;
then A13: r in Z by XBOOLE_0:def 4;
r in X by A12,XBOOLE_0:def 4;
then r in ( Z /\ ( dom f1 ) ) by A1,A13,XBOOLE_0:def 4;
then A14: ( abs ( f1 . r ) ) <= x1 by A7;
( x1 + ( 0 ) ) < M1 by ABSVALUE:4,XREAL_1:8;
then ( abs ( f1 . r ) ) < M1 by A14,XXREAL_0:2;
hence ( abs ( f1 . r ) ) < M by A10,XXREAL_0:2;
A15: M2 <= M by XXREAL_0:25;
A16: r in ( X1 /\ Z1 ) by A11,XBOOLE_0:def 4;
then A17: r in Z1 by XBOOLE_0:def 4;
r in X1 by A16,XBOOLE_0:def 4;
then r in ( Z1 /\ ( dom f2 ) ) by A2,A17,XBOOLE_0:def 4;
then A18: ( abs ( f2 . r ) ) <= x2 by A8;
( x2 + ( 0 ) ) < M2 by ABSVALUE:4,XREAL_1:8;
then ( abs ( f2 . r ) ) < M2 by A18,XXREAL_0:2;
hence ( abs ( f2 . r ) ) < M by A15,XXREAL_0:2;
end;
A19: ( ( 0 ) + ( 0 ) ) < ( ( abs x2 ) + 1 ) by COMPLEX1:46,XREAL_1:8;
( ( 0 ) + ( 0 ) ) < ( ( abs x1 ) + 1 ) by COMPLEX1:46,XREAL_1:8;
then A20: ( 0 ) < M by A19,XXREAL_0:16;
then A21: ( 0 ) < ( 2 * M ) by XREAL_1:129;
(for r holds (( 0 ) < r implies (ex s st (( 0 ) < s & (for x1 holds (for x2 holds (((x1 in ( dom ( ( f1 (#) f2 ) | ( ( ( X /\ Z ) /\ X1 ) /\ Z1 ) ) ) & x2 in ( dom ( ( f1 (#) f2 ) | ( ( ( X /\ Z ) /\ X1 ) /\ Z1 ) ) )) & ( abs ( x1 - x2 ) ) < s) implies ( abs ( ( ( f1 (#) f2 ) . x1 ) - ( ( f1 (#) f2 ) . x2 ) ) ) < r)))))))
proof
let r;
assume ( 0 ) < r;
then A22: ( 0 ) < ( r / ( 2 * M ) ) by A21,XREAL_1:139;
then consider s such that A23: ( 0 ) < s and A24: (for x1 holds (for x2 holds (((x1 in ( dom ( f1 | X ) ) & x2 in ( dom ( f1 | X ) )) & ( abs ( x1 - x2 ) ) < s) implies ( abs ( ( f1 . x1 ) - ( f1 . x2 ) ) ) < ( r / ( 2 * M ) )))) by A3,CKB2:1;
consider g such that A25: ( 0 ) < g and A26: (for x1 holds (for x2 holds (((x1 in ( dom ( f2 | X1 ) ) & x2 in ( dom ( f2 | X1 ) )) & ( abs ( x1 - x2 ) ) < g) implies ( abs ( ( f2 . x1 ) - ( f2 . x2 ) ) ) < ( r / ( 2 * M ) )))) by A4,A22,CKB2:1;
take p = ( min (s,g) );
thus ( 0 ) < p by A23,A25,XXREAL_0:15;
let y1 being Real;
let y2 being Real;
assume that
A27: y1 in ( dom ( ( f1 (#) f2 ) | ( ( ( X /\ Z ) /\ X1 ) /\ Z1 ) ) )
and
A28: y2 in ( dom ( ( f1 (#) f2 ) | ( ( ( X /\ Z ) /\ X1 ) /\ Z1 ) ) );
assume A29: ( abs ( y1 - y2 ) ) < p;
A30: y2 in ( ( ( X /\ Z ) /\ X1 ) /\ Z1 ) by A28,RELAT_1:57;
then A31: y2 in ( ( X /\ Z ) /\ ( X1 /\ Z1 ) ) by XBOOLE_1:16;
then y2 in ( X /\ Z ) by XBOOLE_0:def 4;
then y2 in X by XBOOLE_0:def 4;
then A32: y2 in ( dom ( f1 | X ) ) by A1,RELAT_1:62;
A33: y1 in ( ( ( X /\ Z ) /\ X1 ) /\ Z1 ) by A27,RELAT_1:57;
then A34: y1 in ( ( X /\ Z ) /\ ( X1 /\ Z1 ) ) by XBOOLE_1:16;
then y1 in ( X /\ Z ) by XBOOLE_0:def 4;
then y1 in X by XBOOLE_0:def 4;
then A35: y1 in ( dom ( f1 | X ) ) by A1,RELAT_1:62;
y2 in ( X1 /\ Z1 ) by A31,XBOOLE_0:def 4;
then y2 in X1 by XBOOLE_0:def 4;
then A36: y2 in ( dom ( f2 | X1 ) ) by A2,RELAT_1:62;
y1 in ( X1 /\ Z1 ) by A34,XBOOLE_0:def 4;
then y1 in X1 by XBOOLE_0:def 4;
then A37: y1 in ( dom ( f2 | X1 ) ) by A2,RELAT_1:62;
p <= g by XXREAL_0:17;
then ( abs ( y1 - y2 ) ) < g by A29,XXREAL_0:2;
then A38: ( abs ( ( f2 . y1 ) - ( f2 . y2 ) ) ) < ( r / ( 2 * M ) ) by A26,A37,A36;
( abs ( ( ( f1 (#) f2 ) . y1 ) - ( ( f1 (#) f2 ) . y2 ) ) ) = ( abs ( ( ( f1 . y1 ) * ( f2 . y1 ) ) - ( ( f1 (#) f2 ) . y2 ) ) ) by VALUED_1:5
.= ( abs ( ( ( ( f1 . y1 ) * ( f2 . y1 ) ) + ( ( ( f1 . y1 ) * ( f2 . y2 ) ) - ( ( f1 . y1 ) * ( f2 . y2 ) ) ) ) - ( ( f1 . y2 ) * ( f2 . y2 ) ) ) ) by VALUED_1:5
.= ( abs ( ( ( f1 . y1 ) * ( ( f2 . y1 ) - ( f2 . y2 ) ) ) + ( ( ( f1 . y1 ) - ( f1 . y2 ) ) * ( f2 . y2 ) ) ) );
then A39: ( abs ( ( ( f1 (#) f2 ) . y1 ) - ( ( f1 (#) f2 ) . y2 ) ) ) <= ( ( abs ( ( f1 . y1 ) * ( ( f2 . y1 ) - ( f2 . y2 ) ) ) ) + ( abs ( ( ( f1 . y1 ) - ( f1 . y2 ) ) * ( f2 . y2 ) ) ) ) by COMPLEX1:56;
A40: ( abs ( ( f1 . y1 ) * ( ( f2 . y1 ) - ( f2 . y2 ) ) ) ) = ( ( abs ( f1 . y1 ) ) * ( abs ( ( f2 . y1 ) - ( f2 . y2 ) ) ) ) by COMPLEX1:65;
A41: ( abs ( f2 . y2 ) ) < M by A9,A30;
A42: ( 0 ) <= ( abs ( f2 . y2 ) ) by COMPLEX1:46;
A43: ( 0 ) <= ( abs ( ( f1 . y1 ) - ( f1 . y2 ) ) ) by COMPLEX1:46;
A44: ( abs ( ( ( f1 . y1 ) - ( f1 . y2 ) ) * ( f2 . y2 ) ) ) = ( ( abs ( f2 . y2 ) ) * ( abs ( ( f1 . y1 ) - ( f1 . y2 ) ) ) ) by COMPLEX1:65;
p <= s by XXREAL_0:17;
then ( abs ( y1 - y2 ) ) < s by A29,XXREAL_0:2;
then ( abs ( ( f1 . y1 ) - ( f1 . y2 ) ) ) < ( r / ( 2 * M ) ) by A24,A35,A32;
then A45: ( abs ( ( ( f1 . y1 ) - ( f1 . y2 ) ) * ( f2 . y2 ) ) ) < ( M * ( r / ( 2 * M ) ) ) by A44,A41,A43,A42,XREAL_1:96;
A46: ( 0 ) <= ( abs ( f1 . y1 ) ) by COMPLEX1:46;
A47: ( 0 ) <= ( abs ( ( f2 . y1 ) - ( f2 . y2 ) ) ) by COMPLEX1:46;
( abs ( f1 . y1 ) ) < M by A9,A33;
then ( abs ( ( f1 . y1 ) * ( ( f2 . y1 ) - ( f2 . y2 ) ) ) ) < ( M * ( r / ( 2 * M ) ) ) by A40,A38,A47,A46,XREAL_1:96;
then A48: ( ( abs ( ( f1 . y1 ) * ( ( f2 . y1 ) - ( f2 . y2 ) ) ) ) + ( abs ( ( ( f1 . y1 ) - ( f1 . y2 ) ) * ( f2 . y2 ) ) ) ) < ( ( M * ( r / ( 2 * M ) ) ) + ( M * ( r / ( 2 * M ) ) ) ) by A45,XREAL_1:8;
( ( M * ( r / ( 2 * M ) ) ) + ( M * ( r / ( 2 * M ) ) ) ) = ( M * ( ( r / ( M * 2 ) ) + ( r / ( M * 2 ) ) ) )
.= ( ( r / M ) * M ) by XCMPLX_1:118
.= r by A20,XCMPLX_1:87;
hence thesis by A39,A48,XXREAL_0:2;
end;
hence thesis by CKB2:1;
end;
