begin
reserve Omega for non empty set;
theorem
Th26: (for E being (Subset-Family of Omega) holds (E is  intersection_stable implies ( generated_Dynkin_System E ) is  intersection_stable))
proof
let E being (Subset-Family of Omega);
assume that
A1: E is  intersection_stable;
reconsider G = ( generated_Dynkin_System E ) as (Subset-Family of Omega);
(for a,b being set holds ((a in G & b in G) implies ( a /\ b ) in G)) by A1,CKB35:1;
hence thesis by FINSUB_1:def 2;
end;
