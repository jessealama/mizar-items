begin
reserve x for set;
reserve y for set;
reserve z for set;
reserve u for set;
reserve Y for set;
theorem
Th7: (for Y being non  empty set holds (for x holds (x c= ( meet Y ) iff (for y being (Element of Y) holds x c= y))))
proof
let Y being non  empty set;
let x;
hereby
assume A1: x c= ( meet Y );
let y being (Element of Y);
(for z holds (z in x implies z in y)) by A1,SETFAM_1:def 1;
hence x c= y by TARSKI:def 3;
end;
assume A2: (for y being (Element of Y) holds x c= y);
(for z holds (z in x implies z in ( meet Y )))
proof
let z;
assume A3: z in x;
now
let u;
assume u in Y;
then x c= u by A2;
hence z in u by A3;
end;
hence thesis by SETFAM_1:def 1;
end;
hence thesis by TARSKI:def 3;
end;
