requirements NUMERALS,BOOLE,SUBSET,ARITHM;
begin
reserve Omega for non  empty set;
reserve D for non  empty (Subset-Family of Omega);
theorem
(for E being (Subset-Family of Omega) holds (E is  intersection_stable implies (for D being (Dynkin_System of Omega) holds (E c= D implies ( sigma E ) c= D))))
proof
let E being (Subset-Family of Omega);
assume that
A1: E is  intersection_stable;
reconsider G = ( generated_Dynkin_System E ) as (Dynkin_System of Omega);
G is  intersection_stable by A1,CKB36:1;
then A2: G is (SigmaField of Omega) by CKB30:1;
let D being (Dynkin_System of Omega);
assume E c= D;
then A3: G c= D by CKB31:def 1;
E c= G by CKB31:def 1;
then ( sigma E ) c= G by A2,PROB_1:def 9;
hence thesis by A3,XBOOLE_1:1;
end;
