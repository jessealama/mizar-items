requirements SUBSET,REAL,BOOLE,NUMERALS,ARITHM;
begin
reserve y for set;
reserve S for non trivial RealNormSpace;
reserve T for non trivial RealNormSpace;
definition
let S;
let T;
let f being (PartFunc of S,T);
let X being set;
assume A1: f is_differentiable_on X;
func f `| X -> (PartFunc of S,( R_NormSpace_of_BoundedLinearOperators (S,T) ))
means
:Def9: (( dom it ) = X & (for x being (Point of S) holds (x in X implies ( it /. x ) = ( diff (f,x) ))));
existence
proof
deffunc F((Point of S)) = ( diff (f,$1) );
defpred P[ (Point of S) ]
 means
$1 in X;
consider F being (PartFunc of S,( R_NormSpace_of_BoundedLinearOperators (S,T) )) such that A2: ((for x being (Point of S) holds (x in ( dom F ) iff P[ x ])) & (for x being (Point of S) holds (x in ( dom F ) implies ( F . x ) = F(x)))) from SEQ_1:sch 3;
take F;
now
A3: X is (Subset of S) by A1,CKB42:1;
let y;
assume y in X;
hence y in ( dom F ) by A2,A3;
end;
then A4: X c= ( dom F ) by TARSKI:def 3;
(for y holds (y in ( dom F ) implies y in X)) by A2;
then ( dom F ) c= X by TARSKI:def 3;
hence ( dom F ) = X by A4,XBOOLE_0:def 10;
now
let x being (Point of S);
assume x in X;
then A5: x in ( dom F ) by A2;
then ( F . x ) = ( diff (f,x) ) by A2;
hence ( F /. x ) = ( diff (f,x) ) by A5,PARTFUN1:def 6;
end;
hence thesis;
end;
uniqueness
proof
let F being (PartFunc of S,( R_NormSpace_of_BoundedLinearOperators (S,T) )),G being (PartFunc of S,( R_NormSpace_of_BoundedLinearOperators (S,T) ));
assume that
A6: ( dom F ) = X
and
A7: (for x being (Point of S) holds (x in X implies ( F /. x ) = ( diff (f,x) )))
and
A8: ( dom G ) = X
and
A9: (for x being (Point of S) holds (x in X implies ( G /. x ) = ( diff (f,x) )));
now
let x being (Point of S);
assume A10: x in ( dom F );
then ( F /. x ) = ( diff (f,x) ) by A6,A7;
hence ( F /. x ) = ( G /. x ) by A6,A9,A10;
end;
hence thesis by A6,A8,PARTFUN2:1;
end;
end;
