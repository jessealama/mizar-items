constructors PARTFUN1,REAL_1,SQUARE_1,NAT_1,COMPLEX1,SEQ_2,PARTFUN2,FDIFF_1,BHSP_3,RSSPACE,VFUNCT_1,LOPBAN_3,NFCONT_1,SEQ_1,VALUED_1,RECDEF_1,NORMSP_1,RELSET_1,BINOP_2,RVSUM_1,CKB9,CKB10,CKB21,CKB29,CKB39,CKB40,CKB41;
requirements SUBSET,REAL,BOOLE,NUMERALS,ARITHM;
begin
reserve S for non trivial RealNormSpace;
reserve T for non trivial RealNormSpace;
reserve R for (REST of S,T);
reserve L for (Point of ( R_NormSpace_of_BoundedLinearOperators (S,T) ));
theorem
Th36: (for f being (PartFunc of S,T) holds (for Z being (Subset of S) holds (Z is  open implies (f is_differentiable_on Z iff (Z c= ( dom f ) & (for x being (Point of S) holds (x in Z implies f is_differentiable_in x)))))))
proof
let f being (PartFunc of S,T);
let Z being (Subset of S);
assume that
A1: Z is  open;
thus (f is_differentiable_on Z implies (Z c= ( dom f ) & (for x being (Point of S) holds (x in Z implies f is_differentiable_in x))))
proof
assume A2: f is_differentiable_on Z;
hence A3: Z c= ( dom f ) by CKB41:def 1;
let x0 being (Point of S);
assume A4: x0 in Z;
then ( f | Z ) is_differentiable_in x0 by A2,CKB41:def 1;
then consider N being (Neighbourhood of x0) such that A5: N c= ( dom ( f | Z ) ) and A6: (ex L st (ex R st (for x being (Point of S) holds (x in N implies ( ( ( f | Z ) /. x ) - ( ( f | Z ) /. x0 ) ) = ( ( L . ( x - x0 ) ) + ( R /. ( x - x0 ) ) ))))) by CKB39:def 1;
consider L,R such that A7: (for x being (Point of S) holds (x in N implies ( ( ( f | Z ) /. x ) - ( ( f | Z ) /. x0 ) ) = ( ( L . ( x - x0 ) ) + ( R /. ( x - x0 ) ) ))) by A6;
take N;
A8: ( dom ( f | Z ) ) = ( ( dom f ) /\ Z ) by RELAT_1:61;
then ( dom ( f | Z ) ) c= ( dom f ) by XBOOLE_1:17;
hence N c= ( dom f ) by A5,XBOOLE_1:1;
take L;
take R;
let x being (Point of S);
assume A9: x in N;
then ( ( ( f | Z ) /. x ) - ( ( f | Z ) /. x0 ) ) = ( ( L . ( x - x0 ) ) + ( R /. ( x - x0 ) ) ) by A7;
then ( ( f /. x ) - ( ( f | Z ) /. x0 ) ) = ( ( L . ( x - x0 ) ) + ( R /. ( x - x0 ) ) ) by A5,A8,A9,PARTFUN2:16;
hence thesis by A3,A4,PARTFUN2:17;
end;

assume that
A10: Z c= ( dom f )
and
A11: (for x being (Point of S) holds (x in Z implies f is_differentiable_in x));
thus Z c= ( dom f ) by A10;
let x0 being (Point of S);
assume A12: x0 in Z;
then consider N1 being (Neighbourhood of x0) such that A13: N1 c= Z by A1,CKB2:1;
f is_differentiable_in x0 by A11,A12;
then consider N being (Neighbourhood of x0) such that A14: N c= ( dom f ) and A15: (ex L st (ex R st (for x being (Point of S) holds (x in N implies ( ( f /. x ) - ( f /. x0 ) ) = ( ( L . ( x - x0 ) ) + ( R /. ( x - x0 ) ) ))))) by CKB39:def 1;
consider N2 being (Neighbourhood of x0) such that A16: N2 c= N1 and A17: N2 c= N by CKB1:1;
A18: N2 c= Z by A13,A16,XBOOLE_1:1;
take N2;
N2 c= ( dom f ) by A14,A17,XBOOLE_1:1;
then A19: N2 c= ( ( dom f ) /\ Z ) by A18,XBOOLE_1:19;
hence N2 c= ( dom ( f | Z ) ) by RELAT_1:61;
A20: x0 in N2 by NFCONT_1:4;
consider L,R such that A21: (for x being (Point of S) holds (x in N implies ( ( f /. x ) - ( f /. x0 ) ) = ( ( L . ( x - x0 ) ) + ( R /. ( x - x0 ) ) ))) by A15;
take L;
take R;
let x being (Point of S);
assume A22: x in N2;
then ( ( f /. x ) - ( f /. x0 ) ) = ( ( L . ( x - x0 ) ) + ( R /. ( x - x0 ) ) ) by A17,A21;
then ( ( ( f | Z ) /. x ) - ( f /. x0 ) ) = ( ( L . ( x - x0 ) ) + ( R /. ( x - x0 ) ) ) by A19,A22,PARTFUN2:16;
hence thesis by A19,A20,PARTFUN2:16;
end;
