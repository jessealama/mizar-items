let x being (Element of V);
let y being (Element of V);
let p being Real;
let q being Real;
assume that
A2: p > ( 0 )
and
A3: q > ( 0 );
consider f being (FinSequence of (the carrier of V)) such that A4: ((( f /. 1 ) = x & ( f /. ( len f ) ) = y) & (for i being (Element of ( NAT )) holds ((1 <= i & i <= ( ( len f ) - 1 )) implies ( dist (( f /. i ),( f /. ( i + 1 ) )) ) < p))) and A5: (for F being (FinSequence of ( REAL )) holds ((( len F ) = ( ( len f ) - 1 ) & (for i being (Element of ( NAT )) holds ((1 <= i & i <= ( len F )) implies ( F /. i ) = ( dist (( f /. i ),( f /. ( i + 1 ) )) )))) implies ( dist (x,y) ) = ( Sum F ))) by A1,A2,CKB3:1;
take f;
thus ((( f /. 1 ) = x & ( f /. ( len f ) ) = y) & (for i being (Element of ( NAT )) holds ((1 <= i & i <= ( ( len f ) - 1 )) implies ( dist (( f /. i ),( f /. ( i + 1 ) )) ) < p))) by A4;
let F being (FinSequence of ( REAL ));
assume (( len F ) = ( ( len f ) - 1 ) & (for i being (Element of ( NAT )) holds ((1 <= i & i <= ( len F )) implies ( F /. i ) = ( dist (( f /. i ),( f /. ( i + 1 ) )) ))));
then ( dist (x,y) ) = ( Sum F ) by A5;
hence ( abs ( ( dist (x,y) ) - ( Sum F ) ) ) < q by A3,ABSVALUE:2;
end;
