vocabularies NUMBERS,XREAL_0,ORDINAL1,XCMPLX_0,RELAT_1,ARYTM_3,REAL_1,CARD_1,ARYTM_1,SQUARE_1,FUNCT_3,XXREAL_0,NEWTON,POWER,PREPOWER,POLYEQ_1,ABIAN;
notations ORDINAL1,NUMBERS,XCMPLX_0,XREAL_0,REAL_1,NAT_1,SQUARE_1,NEWTON,PREPOWER,POWER,QUIN_1,XXREAL_0,ABIAN,CKB1,CKB4,CKB8,CKB11,CKB19,CKB21,CKB23,CKB26,CKB30,CKB32;
definitions SQUARE_1,CKB1,CKB8,CKB19,CKB23,CKB30;
theorems SQUARE_1,PREPOWER,POWER,QUIN_1,NEWTON,XREAL_0,XCMPLX_0,XCMPLX_1,XREAL_1,XXREAL_0,ABIAN,CKB1,CKB5,CKB6,CKB7,CKB8,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB22,CKB23,CKB27,CKB28,CKB29,CKB30,CKB33;
registrations XCMPLX_0,XREAL_0,SQUARE_1,MEMBERED,QUIN_1,NEWTON,CKB2,CKB3,CKB9,CKB10,CKB20,CKB24,CKB25,CKB31;
constructors REAL_1,SQUARE_1,NAT_1,MEMBERED,QUIN_1,NEWTON,PREPOWER,POWER,ABIAN,CKB1,CKB4,CKB8,CKB11,CKB19,CKB21,CKB23,CKB26,CKB30,CKB32;
requirements NUMERALS,SUBSET,REAL,ARITHM,BOOLE;
begin
reserve h for  real number;
reserve y for  real number;
theorem
Th14: ( ( y + h ) |^ 3 ) = ( ( ( y |^ 3 ) + ( ( ( 3 * h ) * ( y ^2 ) ) + ( ( 3 * ( h ^2 ) ) * y ) ) ) + ( h |^ 3 ) )
proof
( ( y + h ) |^ 3 ) = ( ( y + h ) |^ ( 2 + 1 ) );
then A1: ( ( y + h ) |^ 3 ) = ( ( ( y + h ) |^ ( 1 + 1 ) ) * ( y + h ) ) by NEWTON:6
.= ( ( ( ( y + h ) |^ 1 ) * ( y + h ) ) * ( y + h ) ) by NEWTON:6
.= ( ( ( y + h ) |^ 1 ) * ( ( y + h ) ^2 ) )
.= ( ( ( y + h ) to_power 1 ) * ( ( ( y ^2 ) + ( ( 2 * y ) * h ) ) + ( h ^2 ) ) ) by POWER:41
.= ( ( y + h ) * ( ( ( y ^2 ) + ( ( 2 * y ) * h ) ) + ( h ^2 ) ) ) by POWER:25
.= ( ( ( y * ( y ^2 ) ) + ( ( ( 3 * h ) * ( y ^2 ) ) + ( ( ( 2 + 1 ) * ( h ^2 ) ) * y ) ) ) + ( h * ( h ^2 ) ) );
( y |^ 3 ) = ( y |^ ( 2 + 1 ) )
.= ( ( y |^ ( 1 + 1 ) ) * y ) by NEWTON:6
.= ( ( ( y |^ 1 ) * y ) * y ) by NEWTON:6
.= ( ( y |^ 1 ) * ( y ^2 ) );
then A2: ( y |^ 3 ) = ( ( y to_power 1 ) * ( y ^2 ) ) by POWER:41;
( h |^ 3 ) = ( h |^ ( 2 + 1 ) )
.= ( ( h |^ ( 1 + 1 ) ) * h ) by NEWTON:6
.= ( ( ( h |^ 1 ) * h ) * h ) by NEWTON:6
.= ( ( h |^ 1 ) * ( h ^2 ) )
.= ( ( h to_power 1 ) * ( h ^2 ) ) by POWER:41
.= ( h * ( h ^2 ) ) by POWER:25;
hence thesis by A1,A2,POWER:25;
end;
