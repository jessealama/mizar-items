environ
vocabularies NUMBERS,SUBSET_1,AFINSQ_1,NAT_1,XREAL_0,ORDINAL1,TARSKI,RELAT_1,PRE_POLY,XXREAL_0,NEWTON,SETFAM_1,XBOOLE_0,CARD_1,ARYTM_3,ORDINAL4,MODAL_1,FLANG_3;
notations TARSKI,XBOOLE_0,SUBSET_1,NUMBERS,XCMPLX_0,NAT_1,ORDINAL1,SETFAM_1,XREAL_0,XXREAL_0,AFINSQ_1,FLANG_1,FLANG_2,CKB2;
definitions TARSKI,CKB2;
theorems NAT_1,TARSKI,XBOOLE_0,XBOOLE_1,XREAL_1,ZFMISC_1,FLANG_1,XXREAL_0,FLANG_2,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15;
schemes CARD_FIL,DOMAIN_1,NAT_1;
registrations SUBSET_1,NAT_1,ORDINAL1,AFINSQ_1,XREAL_0,XBOOLE_0,XXREAL_0;
constructors XXREAL_0,NAT_1,XREAL_0,FLANG_1,FLANG_2,CKB2;
requirements NUMERALS,REAL,BOOLE,SUBSET,ARITHM;
begin
reserve E for set;
reserve x for set;
reserve A for (Subset of ( E ^omega ));
reserve k for Nat;
reserve n for Nat;
theorem
Th15: (( A |^.. n ) = { ( <%> E ) } iff (A = { ( <%> E ) } or (n = ( 0 ) & A = ( {} ))))
proof
thus (( A |^.. n ) = { ( <%> E ) } implies (A = { ( <%> E ) } or (n = ( 0 ) & A = ( {} ))))
proof
assume that
A1: ( A |^.. n ) = { ( <%> E ) }
and
A2: A <> { ( <%> E ) }
and
A3: (n <> ( 0 ) or A <> ( {} ));
per cases  by A3;
suppose A4: n <> ( 0 );

( <%> E ) in ( A |^.. n ) by A1,ZFMISC_1:31;
then consider k such that A5: n <= k and A6: ( <%> E ) in ( A |^ k ) by CKB3:1;
k > ( 0 ) by A4,A5;
then A7: ( <%> E ) in A by A6,FLANG_1:31;
(not (ex x st (x in A & x <> ( <%> E )))) by A1,CKB15:1;
hence contradiction by A2,A7,ZFMISC_1:35;
end;
suppose A <> ( {} );

then (ex x st (x in A & x <> ( <%> E ))) by A2,ZFMISC_1:35;
hence contradiction by A1,CKB15:1;
end;
end;

assume A8: (A = { ( <%> E ) } or (n = ( 0 ) & A = ( {} )));
per cases  by A8;
suppose A9: A = { ( <%> E ) };

A10:now
let x;
assume x in { ( <%> E ) };
then x in ( A |^ n ) by A9,FLANG_1:28;
hence x in ( A |^.. n ) by CKB3:1;
end;
now
let x;
assume x in ( A |^.. n );
then (ex k st (n <= k & x in ( A |^ k ))) by CKB3:1;
hence x in { ( <%> E ) } by A9,FLANG_1:28;
end;
hence thesis by A10,TARSKI:1;
end;
suppose A11: (n = ( 0 ) & A = ( {} ));

hence ( A |^.. n ) = ( A * ) by CKB12:1
.= { ( <%> E ) } by A11,FLANG_1:47;
end;
end;
