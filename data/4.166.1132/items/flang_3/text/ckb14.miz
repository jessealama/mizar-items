theorems NAT_1,TARSKI,XBOOLE_0,XBOOLE_1,XREAL_1,ZFMISC_1,FLANG_1,XXREAL_0,FLANG_2,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13;
schemes CARD_FIL,DOMAIN_1,NAT_1;
registrations SUBSET_1,NAT_1,ORDINAL1,AFINSQ_1,XREAL_0,XBOOLE_0,XXREAL_0;
constructors XXREAL_0,NAT_1,XREAL_0,FLANG_1,FLANG_2,CKB2;
requirements NUMERALS,REAL,BOOLE,SUBSET,ARITHM;
begin
reserve E for set;
reserve x for set;
reserve A for (Subset of ( E ^omega ));
reserve B for (Subset of ( E ^omega ));
reserve k for Nat;
reserve n for Nat;
theorem
Th13: (A c= B implies ( A |^.. n ) c= ( B |^.. n ))
proof
assume A1: A c= B;
now
let x;
assume x in ( A |^.. n );
then consider k such that A2: n <= k and A3: x in ( A |^ k ) by CKB3:1;
( A |^ k ) c= ( B |^ k ) by A1,FLANG_1:37;
hence x in ( B |^.. n ) by A2,A3,CKB3:1;
end;
hence thesis by TARSKI:def 3;
end;
