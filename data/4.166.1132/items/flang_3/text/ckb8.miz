environ
vocabularies NUMBERS,SUBSET_1,AFINSQ_1,NAT_1,XREAL_0,ORDINAL1,TARSKI,RELAT_1,PRE_POLY,XXREAL_0,NEWTON,SETFAM_1,XBOOLE_0,CARD_1,ARYTM_3,ORDINAL4,MODAL_1,FLANG_3;
notations TARSKI,XBOOLE_0,SUBSET_1,NUMBERS,XCMPLX_0,NAT_1,ORDINAL1,SETFAM_1,XREAL_0,XXREAL_0,AFINSQ_1,FLANG_1,FLANG_2,CKB2;
definitions TARSKI,CKB2;
theorems NAT_1,TARSKI,XBOOLE_0,XBOOLE_1,XREAL_1,ZFMISC_1,FLANG_1,XXREAL_0,FLANG_2,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7;
schemes CARD_FIL,DOMAIN_1,NAT_1;
registrations SUBSET_1,NAT_1,ORDINAL1,AFINSQ_1,XREAL_0,XBOOLE_0,XXREAL_0;
constructors XXREAL_0,NAT_1,XREAL_0,FLANG_1,FLANG_2,CKB2;
requirements NUMERALS,REAL,BOOLE,SUBSET,ARITHM;
begin
reserve E for set;
reserve x for set;
reserve A for (Subset of ( E ^omega ));
reserve k for Nat;
reserve m for Nat;
reserve n for Nat;
theorem
Th7: (m <= ( n + 1 ) implies ( ( A |^ (m,n) ) \/ ( A |^.. ( n + 1 ) ) ) = ( A |^.. m ))
proof
assume m <= ( n + 1 );
then A1: ( A |^.. ( n + 1 ) ) c= ( A |^.. m ) by CKB6:1;
now
let x;
assume x in ( A |^.. m );
then consider k such that A2: m <= k and A3: x in ( A |^ k ) by CKB3:1;
A4:now
assume k > n;
then k >= ( n + 1 ) by NAT_1:13;
hence x in ( A |^.. ( n + 1 ) ) by A3,CKB3:1;
end;
(k <= n implies x in ( A |^ (m,n) )) by A2,A3,FLANG_2:19;
hence x in ( ( A |^ (m,n) ) \/ ( A |^.. ( n + 1 ) ) ) by A4,XBOOLE_0:def 3;
end;
then A5: ( A |^.. m ) c= ( ( A |^ (m,n) ) \/ ( A |^.. ( n + 1 ) ) ) by TARSKI:def 3;
( A |^ (m,n) ) c= ( A |^.. m ) by CKB7:1;
then ( ( A |^ (m,n) ) \/ ( A |^.. ( n + 1 ) ) ) c= ( A |^.. m ) by A1,XBOOLE_1:8;
hence thesis by A5,XBOOLE_0:def 10;
end;
