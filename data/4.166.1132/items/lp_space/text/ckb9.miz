constructors PARTFUN1,BINOP_1,XXREAL_0,REAL_1,NAT_1,COMPLEX1,SEQ_2,SEQM_3,PREPOWER,SERIES_1,REALSET1,RLSUB_1,RSSPACE3,LOPBAN_1,SEQ_1,VALUED_1,RELSET_1,BINOP_2,RVSUM_1,CKB1,CKB2;
requirements SUBSET,REAL,BOOLE,NUMERALS,ARITHM;
begin
reserve a for Real;
reserve b for Real;
theorem
Th3: (for a,b being Real_Sequence holds (for p being Real holds (((1 <= p & ( a rto_power p ) is  summable) & ( b rto_power p ) is  summable) implies (( ( a + b ) rto_power p ) is  summable & ( ( Sum ( ( a + b ) rto_power p ) ) to_power ( 1 / p ) ) <= ( ( ( Sum ( a rto_power p ) ) to_power ( 1 / p ) ) + ( ( Sum ( b rto_power p ) ) to_power ( 1 / p ) ) )))))
proof
let a being Real_Sequence;
let b being Real_Sequence;
let p being Real;
assume that
A1: 1 <= p
and
A2: ( a rto_power p ) is  summable
and
A3: ( b rto_power p ) is  summable;
set ab = ( ( a + b ) rto_power p );
set bp = ( b rto_power p );
set ap = ( a rto_power p );
A4:now
let n being (Element of ( NAT ));
thus ( ap . n ) = ( ( abs ( a . n ) ) to_power p ) by CKB1:def 1;
thus ( bp . n ) = ( ( abs ( b . n ) ) to_power p ) by CKB1:def 1;
( ( ( a + b ) rto_power p ) . n ) = ( ( abs ( ( a + b ) . n ) ) to_power p ) by CKB1:def 1
.= ( ( abs ( ( a . n ) + ( b . n ) ) ) to_power p ) by SEQ_1:7;
hence ( ab . n ) = ( ( abs ( ( a . n ) + ( b . n ) ) ) to_power p );
end;
now
per cases  by A1,XXREAL_0:1;
case p > 1;
hence (( ( Sum ab ) to_power ( 1 / p ) ) <= ( ( ( Sum ap ) to_power ( 1 / p ) ) + ( ( Sum bp ) to_power ( 1 / p ) ) ) & ab is  summable) by A2,A3,A4,HOLDER_1:13;
end;
case p = 1;
hence (( ( Sum ab ) to_power ( 1 / p ) ) <= ( ( ( Sum ap ) to_power ( 1 / p ) ) + ( ( Sum bp ) to_power ( 1 / p ) ) ) & ab is  summable) by A2,A3,CKB8:1;
end;
end;
hence thesis;
end;
