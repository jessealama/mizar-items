begin
reserve b for Real;
reserve c for Real;
theorem
Lm10: (for p being Real holds (( 0 ) < p implies (for c being Real holds (for seq,seq1 being Real_Sequence holds ((seq is  convergent & seq1 is  convergent) implies (for rseq being Real_Sequence holds ((for i being (Element of ( NAT )) holds ( rseq . i ) = ( ( ( abs ( ( seq . i ) - c ) ) to_power p ) + ( seq1 . i ) )) implies (rseq is  convergent & ( lim rseq ) = ( ( ( abs ( ( lim seq ) - c ) ) to_power p ) + ( lim seq1 ) )))))))))
proof
let p being Real;
assume that
A1: ( 0 ) < p;
let c being Real;
let seq being Real_Sequence;
let seq1 being Real_Sequence;
assume that
A2: seq is  convergent
and
A3: seq1 is  convergent;
deffunc F(set) = ( ( abs ( ( seq . $1 ) - c ) ) to_power p );
consider b being Real_Sequence such that A4: (for n being (Element of ( NAT )) holds ( b . n ) = F(n)) from SEQ_1:sch 1;
let rseq being Real_Sequence;
assume that
A5: (for i being (Element of ( NAT )) holds ( rseq . i ) = ( ( ( abs ( ( seq . i ) - c ) ) to_power p ) + ( seq1 . i ) ));
now
let n being (Element of ( NAT ));
thus ( rseq . n ) = ( ( ( abs ( ( seq . n ) - c ) ) to_power p ) + ( seq1 . n ) ) by A5
.= ( ( b . n ) + ( seq1 . n ) ) by A4;
end;
then A6: rseq = ( b + seq1 ) by SEQ_1:7;
A7: b is  convergent by A1,A2,A4,CKB26:1;
hence rseq is  convergent by A3,A6,SEQ_2:5;
( lim b ) = ( ( abs ( ( lim seq ) - c ) ) to_power p ) by A1,A2,A4,CKB26:1;
hence thesis by A3,A7,A6,SEQ_2:6;
end;
