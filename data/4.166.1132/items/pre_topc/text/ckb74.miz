theorem
Th62: (for S,T being TopSpace holds (for f being (Function of S,T) holds (for g being (Function of ( the TopStruct of S ),T) holds (f = g implies (f is  continuous iff g is  continuous)))))
proof
let S being TopSpace,T being TopSpace;
let f being (Function of S,T);
let g being (Function of ( the TopStruct of S ),T);
assume that
A1: f = g;
thus (f is  continuous implies g is  continuous)
proof
assume A2: f is  continuous;
let P1 being (Subset of T);
assume P1 is  closed;
then ( f " P1 ) is  closed by A2,CKB27:def 1;
hence ( g " P1 ) is  closed by A1,CKB73:1;
end;

assume A3: g is  continuous;
let P1 being (Subset of T);
assume P1 is  closed;
then ( g " P1 ) is  closed by A3,CKB27:def 1;
hence ( f " P1 ) is  closed by A1,CKB73:1;
end;
