begin
reserve T for TopStruct;
theorem
Th48: (for A being (Subset of T) holds A c= ( Cl A ))
proof
let A being (Subset of T);
now
let x being set;
assume A1: x in A;
assume (not x in ( Cl A ));
then (ex C being (Subset of T) st ((C is  closed & A c= C) & (not x in C))) by A1,CKB37:1;
hence contradiction by A1;
end;
hence thesis by TARSKI:def 3;
end;
