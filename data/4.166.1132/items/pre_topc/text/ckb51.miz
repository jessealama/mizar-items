requirements BOOLE,SUBSET;
begin
registration
let X being TopSpace;
let Y being non empty TopSpace;
let y being (Point of Y);
cluster ( X --> y ) ->  continuous;
coherence
proof
let P1 being (Subset of Y);
assume that
P1 is  closed;
set F = ( X --> y );
set H = ( [#] X );
per cases ;
suppose y in P1;

then ( F " P1 ) = H by FUNCOP_1:14;
hence thesis;
end;
suppose (not y in P1);

then ( F " P1 ) = ( {} X ) by FUNCOP_1:16;
hence thesis;
end;
end;
end;
