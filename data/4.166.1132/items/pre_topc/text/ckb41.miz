begin
reserve T for TopStruct;
theorem
Th49: (for A,B being (Subset of T) holds (A c= B implies ( Cl A ) c= ( Cl B )))
proof
let A being (Subset of T);
let B being (Subset of T);
assume that
A1: A c= B;
now
let x being set;
assume A2: x in ( Cl A );
now
let D1 being (Subset of T);
assume A3: D1 is  closed;
assume B c= D1;
then A c= D1 by A1,XBOOLE_1:1;
hence x in D1 by A2,A3,CKB37:1;
end;
hence x in ( Cl B ) by A2,CKB37:1;
end;
hence thesis by TARSKI:def 3;
end;
