environ
vocabularies STRUCT_0,SETFAM_1,TARSKI,SUBSET_1,XBOOLE_0,ZFMISC_1,RCOMP_1,RELAT_1,FUNCT_1,ORDINAL2,FUNCOP_1,CARD_5,CARD_1,PRE_TOPC;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,SETFAM_1,RELSET_1,CARD_1,STRUCT_0,CKB1,CKB2,CKB4,CKB5,CKB13,CKB14,CKB15,CKB20,CKB27,CKB36;
definitions TARSKI,STRUCT_0,SUBSET_1,CKB2,CKB13,CKB14,CKB15,CKB20,CKB27,CKB36;
theorems TARSKI,SUBSET_1,ZFMISC_1,SETFAM_1,XBOOLE_0,XBOOLE_1,FUNCT_1,FUNCT_2,RELAT_1,FUNCOP_1,CARD_1,CKB2,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB20,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB34,CKB35,CKB36,CKB37,CKB38;
schemes SUBSET_1;
registrations XBOOLE_0,SUBSET_1,STRUCT_0,RELSET_1,CKB1,CKB3,CKB6,CKB17,CKB18,CKB19,CKB21,CKB22,CKB23,CKB31,CKB32,CKB33;
constructors SETFAM_1,PARTFUN1,STRUCT_0,FUNCOP_1,CARD_1,DOMAIN_1,RELSET_1,CKB1,CKB2,CKB13,CKB14,CKB15,CKB20,CKB27,CKB36;
requirements BOOLE,SUBSET;
begin
reserve T for TopStruct;
theorem
(for X9 being (SubSpace of T) holds (for A being (Subset of T) holds (for A1 being (Subset of X9) holds (A = A1 implies ( Cl A1 ) = ( ( Cl A ) /\ ( [#] X9 ) )))))
proof
let X9 being (SubSpace of T);
let A being (Subset of T);
let A1 being (Subset of X9);
assume that
A1: A = A1;
(for p being set holds (p in ( Cl A1 ) iff p in ( ( Cl A ) /\ ( [#] X9 ) )))
proof
let p being set;
thus (p in ( Cl A1 ) implies p in ( ( Cl A ) /\ ( [#] X9 ) ))
proof
reconsider DD = ( Cl A1 ) as (Subset of T) by CKB29:1;
assume A2: p in ( Cl A1 );
A3: (for D1 being (Subset of T) holds (D1 is  closed implies (A c= D1 implies p in D1)))
proof
let D1 being (Subset of T);
assume that
A4: D1 is  closed;
set D3 = ( D1 /\ ( [#] X9 ) );
assume A c= D1;
then A5: A1 c= ( D1 /\ ( [#] X9 ) ) by A1,XBOOLE_1:19;
D3 is  closed by A4,CKB34:1;
then p in D3 by A2,A5,CKB37:1;
hence thesis by XBOOLE_0:def 4;
end;
p in DD by A2;
then p in ( Cl A ) by A3,CKB37:1;
hence thesis by A2,XBOOLE_0:def 4;
end;

assume A6: p in ( ( Cl A ) /\ ( [#] X9 ) );
then A7: p in ( Cl A ) by XBOOLE_0:def 4;
now
let D1 being (Subset of X9);
assume D1 is  closed;
then consider D2 being (Subset of T) such that A8: D2 is  closed and A9: D1 = ( D2 /\ ( [#] X9 ) ) by CKB34:1;
A10: ( D2 /\ ( [#] X9 ) ) c= D2 by XBOOLE_1:17;
assume A1 c= D1;
then A c= D2 by A1,A9,A10,XBOOLE_1:1;
then p in D2 by A7,A8,CKB37:1;
hence p in D1 by A6,A9,XBOOLE_0:def 4;
end;
hence thesis by A6,CKB37:1;
end;
hence thesis by TARSKI:1;
end;
