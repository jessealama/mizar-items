environ
vocabularies STRUCT_0,SETFAM_1,TARSKI,SUBSET_1,XBOOLE_0,ZFMISC_1,RCOMP_1,RELAT_1,FUNCT_1,ORDINAL2,FUNCOP_1,CARD_5,CARD_1,PRE_TOPC;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,SETFAM_1,RELSET_1,CARD_1,STRUCT_0,CKB1,CKB2,CKB4,CKB5,CKB13,CKB14,CKB15,CKB20,CKB27,CKB36;
definitions TARSKI,STRUCT_0,SUBSET_1,CKB2,CKB13,CKB14,CKB15,CKB20,CKB27,CKB36;
theorems TARSKI,SUBSET_1,ZFMISC_1,SETFAM_1,XBOOLE_0,XBOOLE_1,FUNCT_1,FUNCT_2,RELAT_1,FUNCOP_1,CARD_1,CKB2,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB20,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB34,CKB35,CKB36,CKB37,CKB38,CKB39,CKB40,CKB41;
schemes SUBSET_1;
registrations XBOOLE_0,SUBSET_1,STRUCT_0,RELSET_1,CKB1,CKB3,CKB6,CKB17,CKB18,CKB19,CKB21,CKB22,CKB23,CKB31,CKB32,CKB33;
constructors SETFAM_1,PARTFUN1,STRUCT_0,FUNCOP_1,CARD_1,DOMAIN_1,RELSET_1,CKB1,CKB2,CKB13,CKB14,CKB15,CKB20,CKB27,CKB36;
requirements BOOLE,SUBSET;
begin
reserve GX for TopSpace;
theorem
(for A,B being (Subset of GX) holds ( Cl ( A \/ B ) ) = ( ( Cl A ) \/ ( Cl B ) ))
proof
let A being (Subset of GX),B being (Subset of GX);
now
let x being set;
assume A1: x in ( Cl ( A \/ B ) );
assume A2: (not x in ( ( Cl A ) \/ ( Cl B ) ));
then (not x in ( Cl A )) by XBOOLE_0:def 3;
then consider G1 being (Subset of GX) such that A3: G1 is  open and A4: x in G1 and A5: A misses G1 by A1,CKB36:def 1;
A6: ( A /\ G1 ) = ( {} ) by A5,XBOOLE_0:def 7;
(not x in ( Cl B )) by A2,XBOOLE_0:def 3;
then consider G2 being (Subset of GX) such that A7: G2 is  open and A8: x in G2 and A9: B misses G2 by A1,CKB36:def 1;
A10: G2 in (the topology of GX) by A7,CKB13:def 1;
A11: ( B /\ G2 ) = ( {} ) by A9,XBOOLE_0:def 7;
( ( A \/ B ) /\ ( G1 /\ G2 ) ) = ( ( A /\ ( G1 /\ G2 ) ) \/ ( B /\ ( G2 /\ G1 ) ) ) by XBOOLE_1:23
.= ( ( ( A /\ G1 ) /\ G2 ) \/ ( B /\ ( G2 /\ G1 ) ) ) by XBOOLE_1:16
.= ( ( {} ) \/ ( ( {} ) /\ G1 ) ) by A6,A11,XBOOLE_1:16
.= ( {} GX );
then A12: ( A \/ B ) misses ( G1 /\ G2 ) by XBOOLE_0:def 7;
G1 in (the topology of GX) by A3,CKB13:def 1;
then ( G1 /\ G2 ) in (the topology of GX) by A10,CKB2:def 1;
then A13: ( G1 /\ G2 ) is  open by CKB13:def 1;
x in ( G1 /\ G2 ) by A4,A8,XBOOLE_0:def 4;
hence contradiction by A1,A13,A12,CKB36:def 1;
end;
then A14: ( Cl ( A \/ B ) ) c= ( ( Cl A ) \/ ( Cl B ) ) by TARSKI:def 3;
(( Cl A ) c= ( Cl ( A \/ B ) ) & ( Cl B ) c= ( Cl ( A \/ B ) )) by CKB41:1,XBOOLE_1:7;
then ( ( Cl A ) \/ ( Cl B ) ) c= ( Cl ( A \/ B ) ) by XBOOLE_1:8;
hence thesis by A14,XBOOLE_0:def 10;
end;
