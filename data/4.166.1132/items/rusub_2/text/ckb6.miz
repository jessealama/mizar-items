begin
theorem
Lm1: (for V being RealUnitarySpace holds (for W1,W2 being (Subspace of V) holds ( W1 + W2 ) = ( W2 + W1 )))
proof
let V being RealUnitarySpace;
let W1 being (Subspace of V);
let W2 being (Subspace of V);
set A = { ( v + u ) where v is (VECTOR of V),u is (VECTOR of V): (v in W1 & u in W2) };
set B = { ( v + u ) where v is (VECTOR of V),u is (VECTOR of V): (v in W2 & u in W1) };
A1: B c= A
proof
let x being set;
assume x in B;
then (ex v,u being (VECTOR of V) st ((x = ( v + u ) & v in W2) & u in W1));
hence thesis;
end;
A2: (the carrier of ( W1 + W2 )) = A by CKB1:def 1;
A c= B
proof
let x being set;
assume x in A;
then (ex v,u being (VECTOR of V) st ((x = ( v + u ) & v in W1) & u in W2));
hence thesis;
end;
then A = B by A1,XBOOLE_0:def 10;
hence thesis by A2,CKB1:def 1;
end;
