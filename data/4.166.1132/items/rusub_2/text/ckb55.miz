theorem
Lm15: (for V being RealUnitarySpace holds (for W1,W2 being (Subspace of V) holds (V is_the_direct_sum_of W1,W2 implies V is_the_direct_sum_of W2,W1)))
proof
let V being RealUnitarySpace;
let W1 being (Subspace of V),W2 being (Subspace of V);
assume A1: V is_the_direct_sum_of W1,W2;
then ( W1 /\ W2 ) = ( (0). V ) by CKB49:def 1;
then A2: ( W2 /\ W1 ) = ( (0). V ) by CKB19:1;
( the UNITSTR of V ) = ( W1 + W2 ) by A1,CKB49:def 1;
then ( the UNITSTR of V ) = ( W2 + W1 ) by CKB6:1;
hence thesis by A2,CKB49:def 1;
end;
