begin
theorem
(for V being RealUnitarySpace holds (for W1 being (Subspace of V) holds (for W2,W3 being  strict (Subspace of V) holds (W1 is (Subspace of W2) implies ( W1 + W3 ) is (Subspace of ( W2 + W3 ))))))
proof
let V being RealUnitarySpace;
let W1 being (Subspace of V);
let W2 being  strict (Subspace of V);
let W3 being  strict (Subspace of V);
assume A1: W1 is (Subspace of W2);
( ( W1 + W3 ) + ( W2 + W3 ) ) = ( ( W1 + W3 ) + ( W3 + W2 ) ) by CKB6:1
.= ( ( ( W1 + W3 ) + W3 ) + W2 ) by CKB11:1
.= ( ( W1 + ( W3 + W3 ) ) + W2 ) by CKB11:1
.= ( ( W1 + W3 ) + W2 ) by CKB8:1
.= ( W1 + ( W3 + W2 ) ) by CKB11:1
.= ( W1 + ( W2 + W3 ) ) by CKB6:1
.= ( ( W1 + W2 ) + W3 ) by CKB11:1
.= ( W2 + W3 ) by A1,CKB13:1;
hence thesis by CKB13:1;
end;
