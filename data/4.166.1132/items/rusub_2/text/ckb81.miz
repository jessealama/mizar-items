theorem
Th54: (for V being RealUnitarySpace holds LattStr (# ( Subspaces V ),( SubJoin V ),( SubMeet V ) #) is Lattice)
proof
let V being RealUnitarySpace;
set S = LattStr (# ( Subspaces V ),( SubJoin V ),( SubMeet V ) #);
A1: (for A,B being (Element of S) holds ( A "/\" B ) = ( B "/\" A ))
proof
let A being (Element of S);
let B being (Element of S);
reconsider W1 = A,W2 = B as (Subspace of V) by CKB46:def 1;
thus ( A "/\" B ) = ( ( SubMeet V ) . (A,B) ) by LATTICES:def 2
.= ( W1 /\ W2 ) by CKB80:def 1
.= ( W2 /\ W1 ) by CKB19:1
.= ( ( SubMeet V ) . (B,A) ) by CKB80:def 1
.= ( B "/\" A ) by LATTICES:def 2;
end;
A2: (for A,B being (Element of S) holds ( ( A "/\" B ) "\/" B ) = B)
proof
let A being (Element of S);
let B being (Element of S);
reconsider W1 = A,W2 = B as  strict (Subspace of V) by CKB46:def 1;
reconsider AB = ( W1 /\ W2 ) as (Element of S) by CKB46:def 1;
thus ( ( A "/\" B ) "\/" B ) = ( ( SubJoin V ) . (( A "/\" B ),B) ) by LATTICES:def 1
.= ( ( SubJoin V ) . (( ( SubMeet V ) . (A,B) ),B) ) by LATTICES:def 2
.= ( ( SubJoin V ) . (AB,B) ) by CKB80:def 1
.= ( ( W1 /\ W2 ) + W2 ) by CKB79:def 1
.= B by CKB30:1,RUSUB_1:24;
end;
A3: (for A,B,C being (Element of S) holds ( A "\/" ( B "\/" C ) ) = ( ( A "\/" B ) "\/" C ))
proof
let A being (Element of S);
let B being (Element of S);
let C being (Element of S);
reconsider W1 = A,W2 = B,W3 = C as (Subspace of V) by CKB46:def 1;
reconsider AB = ( W1 + W2 ),BC = ( W2 + W3 ) as (Element of S) by CKB46:def 1;
thus ( A "\/" ( B "\/" C ) ) = ( ( SubJoin V ) . (A,( B "\/" C )) ) by LATTICES:def 1
.= ( ( SubJoin V ) . (A,( ( SubJoin V ) . (B,C) )) ) by LATTICES:def 1
.= ( ( SubJoin V ) . (A,BC) ) by CKB79:def 1
.= ( W1 + ( W2 + W3 ) ) by CKB79:def 1
.= ( ( W1 + W2 ) + W3 ) by CKB11:1
.= ( ( SubJoin V ) . (AB,C) ) by CKB79:def 1
.= ( ( SubJoin V ) . (( ( SubJoin V ) . (A,B) ),C) ) by CKB79:def 1
.= ( ( SubJoin V ) . (( A "\/" B ),C) ) by LATTICES:def 1
.= ( ( A "\/" B ) "\/" C ) by LATTICES:def 1;
end;
A4: (for A,B being (Element of S) holds ( A "/\" ( A "\/" B ) ) = A)
proof
let A being (Element of S);
let B being (Element of S);
reconsider W1 = A,W2 = B as  strict (Subspace of V) by CKB46:def 1;
reconsider AB = ( W1 + W2 ) as (Element of S) by CKB46:def 1;
thus ( A "/\" ( A "\/" B ) ) = ( ( SubMeet V ) . (A,( A "\/" B )) ) by LATTICES:def 2
.= ( ( SubMeet V ) . (A,( ( SubJoin V ) . (A,B) )) ) by LATTICES:def 1
.= ( ( SubMeet V ) . (A,AB) ) by CKB79:def 1
.= ( W1 /\ ( W1 + W2 ) ) by CKB80:def 1
.= A by CKB32:1,RUSUB_1:24;
end;
A5: (for A,B,C being (Element of S) holds ( A "/\" ( B "/\" C ) ) = ( ( A "/\" B ) "/\" C ))
proof
let A being (Element of S);
let B being (Element of S);
let C being (Element of S);
reconsider W1 = A,W2 = B,W3 = C as (Subspace of V) by CKB46:def 1;
reconsider AB = ( W1 /\ W2 ),BC = ( W2 /\ W3 ) as (Element of S) by CKB46:def 1;
thus ( A "/\" ( B "/\" C ) ) = ( ( SubMeet V ) . (A,( B "/\" C )) ) by LATTICES:def 2
.= ( ( SubMeet V ) . (A,( ( SubMeet V ) . (B,C) )) ) by LATTICES:def 2
.= ( ( SubMeet V ) . (A,BC) ) by CKB80:def 1
.= ( W1 /\ ( W2 /\ W3 ) ) by CKB80:def 1
.= ( ( W1 /\ W2 ) /\ W3 ) by CKB20:1
.= ( ( SubMeet V ) . (AB,C) ) by CKB80:def 1
.= ( ( SubMeet V ) . (( ( SubMeet V ) . (A,B) ),C) ) by CKB80:def 1
.= ( ( SubMeet V ) . (( A "/\" B ),C) ) by LATTICES:def 2
.= ( ( A "/\" B ) "/\" C ) by LATTICES:def 2;
end;
(for A,B being (Element of S) holds ( A "\/" B ) = ( B "\/" A ))
proof
let A being (Element of S);
let B being (Element of S);
reconsider W1 = A,W2 = B as (Subspace of V) by CKB46:def 1;
thus ( A "\/" B ) = ( ( SubJoin V ) . (A,B) ) by LATTICES:def 1
.= ( W1 + W2 ) by CKB79:def 1
.= ( W2 + W1 ) by CKB6:1
.= ( ( SubJoin V ) . (B,A) ) by CKB79:def 1
.= ( B "\/" A ) by LATTICES:def 1;
end;
then S is  join-commutative  join-associative  meet-absorbing  meet-commutative  meet-associative  join-absorbing by A3,A2,A1,A5,A4,LATTICES:def 4,LATTICES:def 5,LATTICES:def 6,LATTICES:def 7,LATTICES:def 8,LATTICES:def 9;
hence thesis;
end;
