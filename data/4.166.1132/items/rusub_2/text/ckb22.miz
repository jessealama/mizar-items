begin
theorem
Th16: (for V being RealUnitarySpace holds (for W1,W2 being (Subspace of V) holds (( W1 /\ W2 ) is (Subspace of W1) & ( W1 /\ W2 ) is (Subspace of W2))))
proof
let V being RealUnitarySpace;
let W1 being (Subspace of V),W2 being (Subspace of V);
(the carrier of ( W1 /\ W2 )) c= (the carrier of W1) by CKB21:1;
hence ( W1 /\ W2 ) is (Subspace of W1) by RUSUB_1:22;
(the carrier of ( W2 /\ W1 )) c= (the carrier of W2) by CKB21:1;
then (the carrier of ( W1 /\ W2 )) c= (the carrier of W2) by CKB19:1;
hence thesis by RUSUB_1:22;
end;
