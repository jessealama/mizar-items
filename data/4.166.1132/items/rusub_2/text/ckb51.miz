begin
theorem
Lm13: (for V being RealUnitarySpace holds (for W1,W2 being (Subspace of V) holds (( W1 + W2 ) = ( the UNITSTR of V ) iff (for v being (VECTOR of V) holds (ex v1,v2 being (VECTOR of V) st ((v1 in W1 & v2 in W2) & v = ( v1 + v2 )))))))
proof
let V being RealUnitarySpace;
let W1 being (Subspace of V),W2 being (Subspace of V);
thus (( W1 + W2 ) = ( the UNITSTR of V ) implies (for v being (VECTOR of V) holds (ex v1,v2 being (VECTOR of V) st ((v1 in W1 & v2 in W2) & v = ( v1 + v2 )))))
proof
assume A1: ( W1 + W2 ) = ( the UNITSTR of V );
let v being (VECTOR of V);
v in ( the UNITSTR of V ) by RLVECT_1:1;
hence thesis by A1,CKB3:1;
end;

assume A2: (for v being (VECTOR of V) holds (ex v1,v2 being (VECTOR of V) st ((v1 in W1 & v2 in W2) & v = ( v1 + v2 ))));
now
let u being (VECTOR of V);
(ex v1,v2 being (VECTOR of V) st ((v1 in W1 & v2 in W2) & u = ( v1 + v2 ))) by A2;
hence u in ( W1 + W2 ) by CKB3:1;
end;
hence thesis by CKB50:1;
end;
