begin
theorem
Th3: (for V being RealUnitarySpace holds (for W1,W2 being (Subspace of V) holds (for x being set holds (x in ( W1 /\ W2 ) iff (x in W1 & x in W2)))))
proof
let V being RealUnitarySpace;
let W1 being (Subspace of V),W2 being (Subspace of V);
let x being set;
(x in ( W1 /\ W2 ) iff x in (the carrier of ( W1 /\ W2 ))) by STRUCT_0:def 5;
then (x in ( W1 /\ W2 ) iff x in ( (the carrier of W1) /\ (the carrier of W2) )) by CKB2:def 1;
then (x in ( W1 /\ W2 ) iff (x in (the carrier of W1) & x in (the carrier of W2))) by XBOOLE_0:def 4;
hence thesis by STRUCT_0:def 5;
end;
