begin
theorem
Th20: (for V being RealUnitarySpace holds (for W being  strict (Subspace of V) holds (( ( (Omega). V ) /\ W ) = W & ( W /\ ( (Omega). V ) ) = W)))
proof
let V being RealUnitarySpace;
let W being  strict (Subspace of V);
( (Omega). V ) = ( the UNITSTR of V ) by RUSUB_1:def 3;
then A1: (the carrier of ( ( (Omega). V ) /\ W )) = ( (the carrier of V) /\ (the carrier of W) ) by CKB2:def 1;
(the carrier of W) c= (the carrier of V) by RUSUB_1:def 1;
hence ( ( (Omega). V ) /\ W ) = W by A1,RUSUB_1:24,XBOOLE_1:28;
hence thesis by CKB19:1;
end;
