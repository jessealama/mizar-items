requirements NUMERALS,REAL,BOOLE,SUBSET,ARITHM;
begin
reserve E for  finite non  empty set;
theorem
(for E being  finite non  empty set holds (for ASeq being (SetSequence of E) holds (ASeq is  non-descending implies (ex N being Nat st (for m being Nat holds (N <= m implies ( Union ASeq ) = ( ASeq . m )))))))
proof
let E being  finite non  empty set;
let ASeq being (SetSequence of E);
assume A1: ASeq is  non-descending;
then consider N0 being (Element of ( NAT )) such that A2: (for m being (Element of ( NAT )) holds (N0 <= m implies ( ASeq . N0 ) = ( ASeq . m ))) by CKB29:1;
reconsider N = N0 as Nat;
take N;
let m being Nat;
reconsider M = m as (Element of ( NAT )) by ORDINAL1:def 12;
assume A3: N <= m;
now
let x being set;
assume x in ( Union ASeq );
then consider n being (Element of ( NAT )) such that A4: x in ( ASeq . n ) by PROB_1:12;
per cases ;
suppose A5: n < N;

A6: ( ASeq . N ) c= ( ASeq . M ) by A2,A3;
( ASeq . n ) c= ( ASeq . N ) by A1,A5,PROB_1:def 5;
then ( ASeq . n ) c= ( ASeq . m ) by A6,XBOOLE_1:1;
hence x in ( ASeq . m ) by A4;
end;
suppose N <= n;

then ( ASeq . N ) = ( ASeq . n ) by A2;
then ( ASeq . n ) c= ( ASeq . M ) by A2,A3;
hence x in ( ASeq . m ) by A4;
end;
end;
then A7: ( Union ASeq ) c= ( ASeq . m ) by TARSKI:def 3;
( ASeq . m ) c= ( Union ASeq ) by ABCMIZ_1:1;
hence ( ASeq . m ) = ( Union ASeq ) by A7,XBOOLE_0:def 10;
end;
