begin
theorem
(for Omega being non empty  finite set holds (for f being (Function of Omega,( REAL )) holds f is (Real-Valued-Random-Variable of ( Trivial-SigmaField Omega ))))
proof
let Omega being non empty  finite set;
let f being (Function of Omega,( REAL ));
set Sigma = ( Trivial-SigmaField Omega );
((ex X being (Element of Sigma) st (( dom f ) = X & f is_measurable_on X)) & ( dom f ) = Omega) by CKB19:1,FUNCT_2:def 1;
hence thesis by CKB32:def 1;
end;
