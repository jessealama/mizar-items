begin
reserve n for  natural number;
reserve p for (Point of ( TOP-REAL n ));
reserve r for  real number;
reserve q for (Point of ( TOP-REAL n ));
theorem
Th9: (for r,s being  positive  real number holds ( Tball (p,r) ),( Tball (q,s) ) are_homeomorphic )
proof
let r being  positive  real number;
let s being  positive  real number;
per cases ;
suppose A1: n is  empty;

reconsider n1 = n as (Element of ( NAT )) by ORDINAL1:def 12;
reconsider p1 = p,q1 = q as (Point of ( TOP-REAL n1 ));
set X = { x where x is (Point of ( TOP-REAL n )): |. ( x - p ) .| < r };
set Y = { x where x is (Point of ( TOP-REAL n )): |. ( x - q ) .| < s };
A2: p1 = ( 0. ( TOP-REAL ( 0 ) ) ) by A1;
A3: q1 = ( 0. ( TOP-REAL ( 0 ) ) ) by A1;
(for x being set holds (x in X iff x in Y))
proof
let x being set;
hereby
assume x in X;
then consider x1 being (Point of ( TOP-REAL n )) such that A4: (x = x1 & |. ( x1 - p ) .| < r);
reconsider x1 as (Point of ( TOP-REAL n1 ));
x1 = ( 0. ( TOP-REAL ( 0 ) ) ) by A1;
then |. ( x1 - p1 ) .| = ( 0 ) by A2,TOPRNS_1:28;
hence x in Y by A2,A3,A4;
end;
assume x in Y;
then consider x1 being (Point of ( TOP-REAL n )) such that A5: (x = x1 & |. ( x1 - q ) .| < s);
reconsider x1 as (Point of ( TOP-REAL n1 ));
x1 = ( 0. ( TOP-REAL ( 0 ) ) ) by A1;
then |. ( x1 - q1 ) .| = ( 0 ) by A3,TOPRNS_1:28;
hence x in X by A2,A3,A5;
end;
hence thesis by TARSKI:1;
end;
suppose n is non  empty;

then reconsider n1 = n as non  empty (Element of ( NAT )) by ORDINAL1:def 12;
A6: (for r being  positive  real number holds (for x being (Point of ( TOP-REAL n1 )) holds ( Tunit_ball n1 ),( Tball (x,r) ) are_homeomorphic ))
proof
let r being  positive  real number;
let x being (Point of ( TOP-REAL n1 ));
set U = ( Tunit_ball n );
set C = ( Tball (x,r) );
defpred P[ (Point of U),set ]
 means
(ex w being (Point of ( TOP-REAL n1 )) st (w = $1 & $2 = ( ( r * w ) + x )));
A7: r is Real by XREAL_0:def 1;
A8: (the carrier of C) = ( Ball (x,r) ) by CKB13:1;
A9: (for u being (Point of U) holds (ex y being (Point of C) st P[ u,y ]))
proof
let u being (Point of U);
reconsider v = u as (Point of ( TOP-REAL n1 )) by PRE_TOPC:25;
set y = ( ( r * v ) + x );
A10: |. ( y - x ) .| = |. ( r * v ) .| by EUCLID:48
.= ( ( abs r ) * |. v .| ) by A7,TOPRNS_1:7
.= ( r * |. v .| ) by ABSVALUE:def 1;
( r * |. v .| ) < ( r * 1 ) by CKB14:1,XREAL_1:68;
then reconsider y as (Point of C) by A10,A8,TOPREAL9:7;
take y;
thus thesis;
end;
consider f being (Function of U,C) such that A11: (for x being (Point of U) holds P[ x,( f . x ) ]) from FUNCT_2:sch 3(A9);
(for a being (Point of U) holds (for b being (Point of ( TOP-REAL n1 )) holds (a = b implies ( f . a ) = ( ( r * b ) + x ))))
proof
let a being (Point of U);
let b being (Point of ( TOP-REAL n1 ));
P[ a,( f . a ) ] by A11;
hence thesis;
end;
then (ex f being (Function of U,C) st f is  being_homeomorphism) by CKB16:1;
hence thesis by T_0TOPSP:def 1;
end;
(for x,y being (Point of ( TOP-REAL n1 )) holds ( Tball (x,r) ),( Tball (y,s) ) are_homeomorphic )
proof
let x being (Point of ( TOP-REAL n1 ));
let y being (Point of ( TOP-REAL n1 ));
A12: ( Tunit_ball n ),( Tball (y,s) ) are_homeomorphic  by A6;
( Tball (x,r) ),( Tunit_ball n ) are_homeomorphic  by A6;
hence thesis by A12,BORSUK_3:3;
end;
hence thesis;
end;
end;
