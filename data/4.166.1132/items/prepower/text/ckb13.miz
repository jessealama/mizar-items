constructors FUNCOP_1,REAL_1,SQUARE_1,NAT_1,NAT_D,SEQ_2,SEQM_3,PARTFUN1,VALUED_1,SEQ_4,NEWTON,SEQ_1,XXREAL_2,RELSET_1,BINOP_2,CKB4;
requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve a for  real number;
reserve b for  real number;
reserve n for (Element of ( NAT ));
theorem
Th18: (for n being  natural number holds (((( 0 ) <= a & a < b) & 1 <= n) implies ( a |^ n ) < ( b |^ n )))
proof
let n being  natural number;
assume that
A1: ( 0 ) <= a
and
A2: a < b
and
A3: 1 <= n;
per cases  by A1;
suppose a > ( 0 );

hence thesis by A2,A3,CKB12:1;
end;
suppose A4: a = ( 0 );

reconsider k = n,k1 = 1 as Integer;
reconsider m = ( k - k1 ) as (Element of ( NAT )) by A3,INT_1:5;
( a |^ n ) = ( a |^ ( m + 1 ) )
.= ( ( a |^ m ) * ( a |^ 1 ) ) by NEWTON:8
.= ( ( a |^ m ) * ( ( a GeoSeq ) . ( ( 0 ) + 1 ) ) ) by CKB4:def 1
.= ( ( a |^ m ) * ( ( ( a GeoSeq ) . ( 0 ) ) * ( 0 ) ) ) by A4,CKB5:1
.= ( 0 );
hence thesis by A1,A2,CKB8:1;
end;
end;
