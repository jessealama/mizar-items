begin
reserve x for set;
definition
let s being XFinSequence;
let p being  XFinSequence-yielding Function;
func p +^ s ->  XFinSequence-yielding Function
means
:Def3: (( dom it ) = ( dom p ) & (for x holds (x in ( dom p ) implies ( it . x ) = ( ( p . x ) ^ s ))));
existence
proof
defpred P[ set,set ]
 means
(for x holds (x = $1 implies $2 = ( ( p . x ) ^ s )));
A8: (for x holds (x in ( dom p ) implies (ex y being set st P[ x,y ])))
proof
let x;
assume x in ( dom p );
take ( ( p . x ) ^ s );
thus thesis;
end;
consider f being Function such that A9: (( dom f ) = ( dom p ) & (for x holds (x in ( dom p ) implies P[ x,( f . x ) ]))) from CLASSES1:sch 1(A8);
now
let x;
assume x in ( dom f );
then ( f . x ) = ( ( p . x ) ^ s ) by A9;
hence ( f . x ) is XFinSequence;
end;
then reconsider g = f as  XFinSequence-yielding Function by CKB5:def 1;
take g;
thus thesis by A9;
end;
uniqueness
proof
let f being  XFinSequence-yielding Function,g being  XFinSequence-yielding Function;
assume that
A10: ( dom f ) = ( dom p )
and
A11: (for x holds (x in ( dom p ) implies ( f . x ) = ( ( p . x ) ^ s )))
and
A12: ( dom g ) = ( dom p )
and
A13: (for x holds (x in ( dom p ) implies ( g . x ) = ( ( p . x ) ^ s )));
now
let x;
assume A14: x in ( dom f );
then ( f . x ) = ( ( p . x ) ^ s ) by A10,A11;
hence ( f . x ) = ( g . x ) by A10,A13,A14;
end;
hence thesis by A10,A12,FUNCT_1:2;
end;
end;
