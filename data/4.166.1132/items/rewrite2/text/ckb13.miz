environ
vocabularies NUMBERS,NAT_1,FINSEQ_1,RELAT_1,ARYTM_3,CARD_1,XXREAL_0,ORDINAL4,REWRITE1,TARSKI,SUBSET_1,FUNCT_1,AFINSQ_1,XBOOLE_0,ORDINAL2,RELAT_2,PRELAMB,FINSEQ_5,LANG1,CIRCTRM1,REWRITE2,ORDINAL1,FINSET_1;
notations TARSKI,XBOOLE_0,SUBSET_1,NUMBERS,ORDINAL1,FINSET_1,RELAT_1,DOMAIN_1,NAT_1,FINSEQ_5,FUNCT_1,RELSET_1,XXREAL_0,AFINSQ_1,CATALAN2,RELAT_2,FINSEQ_1,REWRITE1,FLANG_1,LANG1,PARTIT_2,CKB5;
definitions TARSKI,FINSEQ_1,CKB5;
theorems AFINSQ_1,NAT_1,RELAT_1,RELAT_2,XREAL_1,ZFMISC_1,FINSEQ_1,FINSEQ_2,FUNCT_1,REWRITE1,FINSEQ_3,FINSEQ_5,TARSKI,XBOOLE_0,XBOOLE_1,ABCMIZ_0,RELSET_1,PARTIT_2,CKB1,CKB2,CKB3,CKB4,CKB5;
schemes CLASSES1,NAT_1,RELSET_1;
registrations NAT_1,AFINSQ_1,REWRITE1,FINSEQ_1,XXREAL_0,XBOOLE_0,RELAT_1,FUNCT_1,XREAL_0,ORDINAL1,CATALAN2,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12;
constructors NAT_1,FINSEQ_5,REWRITE1,FLANG_1,LANG1,OPOSET_1,XREAL_0,RELSET_1,PARTIT_2,CKB5;
requirements NUMERALS,REAL,BOOLE,SUBSET,ARITHM;
begin
reserve x for set;
reserve R for Relation;
reserve p for (RedSequence of R);
definition
let s being XFinSequence;
let p being  XFinSequence-yielding Function;
func s ^+ p ->  XFinSequence-yielding Function means 
:Def2: (( dom it ) = ( dom p ) & (for x holds (x in ( dom p ) implies ( it . x ) = ( s ^ ( p . x ) ))));
existence
proof
defpred P[ set,set ]
 means
(for x holds (x = $1 implies $2 = ( s ^ ( p . x ) )));
A1: (for x holds (x in ( dom p ) implies (ex y being set st P[ x,y ])))
proof
let x;
assume x in ( dom p );
take ( s ^ ( p . x ) );
thus thesis;
end;
consider f being Function such that A2: (( dom f ) = ( dom p ) & (for x holds (x in ( dom p ) implies P[ x,( f . x ) ]))) from CLASSES1:sch 1(A1);
now
let x;
assume x in ( dom f );
then ( f . x ) = ( s ^ ( p . x ) ) by A2;
hence ( f . x ) is XFinSequence;
end;
then reconsider g = f as  XFinSequence-yielding Function by CKB5:def 1;
take g;
thus thesis by A2;
end;
uniqueness
proof
let f being  XFinSequence-yielding Function;
let g being  XFinSequence-yielding Function;
assume that
A3: ( dom f ) = ( dom p )
and
A4: (for x holds (x in ( dom p ) implies ( f . x ) = ( s ^ ( p . x ) )))
and
A5: ( dom g ) = ( dom p )
and
A6: (for x holds (x in ( dom p ) implies ( g . x ) = ( s ^ ( p . x ) )));
now
let x;
assume A7: x in ( dom f );
then ( f . x ) = ( s ^ ( p . x ) ) by A3,A4;
hence ( f . x ) = ( g . x ) by A3,A6,A7;
end;
hence thesis by A3,A5,FUNCT_1:2;
end;
end;
