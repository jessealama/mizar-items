begin
reserve R for Relation;
reserve E for set;
reserve S for (semi-Thue-system of E);
reserve s for (Element of ( E ^omega ));
reserve t for (Element of ( E ^omega ));
definition
let E;
let S;
func ==>.-relation S -> (Relation of ( E ^omega )) means 
:Def6: ([ s,t ] in it iff s ==>. t,S);
existence
proof
defpred P[ (Element of ( E ^omega )),(Element of ( E ^omega )) ]
 means
$1 ==>. $2,S;
consider R being (Relation of ( E ^omega )) such that A1: (for s,t being (Element of ( E ^omega )) holds ([ s,t ] in R iff P[ s,t ])) from RELSET_1:sch 2;
take R;
thus thesis by A1;
end;
uniqueness
proof
let R1 being (Relation of ( E ^omega ));
let R2 being (Relation of ( E ^omega ));
assume that
A2: (for s,t being (Element of ( E ^omega )) holds ([ s,t ] in R1 iff s ==>. t,S))
and
A3: (for s,t being (Element of ( E ^omega )) holds ([ s,t ] in R2 iff s ==>. t,S));
now
let s being (Element of ( E ^omega ));
let t being (Element of ( E ^omega ));
([ s,t ] in R1 iff s ==>. t,S) by A2;
hence ([ s,t ] in R1 iff [ s,t ] in R2) by A3;
end;
hence thesis by RELSET_1:def 2;
end;
end;
