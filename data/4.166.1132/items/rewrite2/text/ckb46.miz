theorems AFINSQ_1,NAT_1,RELAT_1,RELAT_2,XREAL_1,ZFMISC_1,FINSEQ_1,FINSEQ_2,FUNCT_1,REWRITE1,FINSEQ_3,FINSEQ_5,TARSKI,XBOOLE_0,XBOOLE_1,ABCMIZ_0,RELSET_1,PARTIT_2,CKB1,CKB2,CKB3,CKB4,CKB5,CKB13,CKB14,CKB17,CKB18,CKB19,CKB20,CKB21,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB37,CKB38,CKB39,CKB40,CKB41,CKB42,CKB44,CKB45;
schemes CLASSES1,NAT_1,RELSET_1;
registrations NAT_1,AFINSQ_1,REWRITE1,FINSEQ_1,XXREAL_0,XBOOLE_0,RELAT_1,FUNCT_1,XREAL_0,ORDINAL1,CATALAN2,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB15,CKB16,CKB26,CKB27;
constructors NAT_1,FINSEQ_5,REWRITE1,FLANG_1,LANG1,OPOSET_1,XREAL_0,RELSET_1,PARTIT_2,CKB5,CKB13,CKB14,CKB22,CKB23,CKB24,CKB29,CKB30,CKB43,CKB44;
requirements NUMERALS,REAL,BOOLE,SUBSET,ARITHM;
begin
reserve E for set;
reserve S for (semi-Thue-system of E);
reserve u for (Element of ( E ^omega ));
reserve p for (FinSequence of ( E ^omega ));
theorem
Th23: (p is (RedSequence of ( ==>.-relation S )) implies (( p +^ u ) is (RedSequence of ( ==>.-relation S )) & ( u ^+ p ) is (RedSequence of ( ==>.-relation S ))))
proof
assume A1: p is (RedSequence of ( ==>.-relation S ));
A2:now
let i being (Element of ( NAT ));
assume that
A3: i in ( dom ( p +^ u ) )
and
A4: ( i + 1 ) in ( dom ( p +^ u ) );
A5: ( i + 1 ) in ( dom p ) by A4,CKB14:def 1;
then A6: ( ( p +^ u ) . ( i + 1 ) ) = ( ( p . ( i + 1 ) ) ^ u ) by CKB14:def 1;
A7: i in ( dom p ) by A3,CKB14:def 1;
then [ ( p . i ),( p . ( i + 1 ) ) ] in ( ==>.-relation S ) by A1,A5,REWRITE1:def 2;
then ( p . i ) ==>. ( p . ( i + 1 ) ),S by CKB44:def 1;
then A8: ( ( p . i ) ^ u ) ==>. ( ( p . ( i + 1 ) ) ^ u ),S by CKB33:1;
( ( p +^ u ) . i ) = ( ( p . i ) ^ u ) by A7,CKB14:def 1;
hence [ ( ( p +^ u ) . i ),( ( p +^ u ) . ( i + 1 ) ) ] in ( ==>.-relation S ) by A6,A8,CKB44:def 1;
end;
A9:now
let i being (Element of ( NAT ));
assume that
A10: i in ( dom ( u ^+ p ) )
and
A11: ( i + 1 ) in ( dom ( u ^+ p ) );
A12: ( i + 1 ) in ( dom p ) by A11,CKB13:def 1;
then A13: ( ( u ^+ p ) . ( i + 1 ) ) = ( u ^ ( p . ( i + 1 ) ) ) by CKB13:def 1;
A14: i in ( dom p ) by A10,CKB13:def 1;
then [ ( p . i ),( p . ( i + 1 ) ) ] in ( ==>.-relation S ) by A1,A12,REWRITE1:def 2;
then ( p . i ) ==>. ( p . ( i + 1 ) ),S by CKB44:def 1;
then A15: ( u ^ ( p . i ) ) ==>. ( u ^ ( p . ( i + 1 ) ) ),S by CKB33:1;
( ( u ^+ p ) . i ) = ( u ^ ( p . i ) ) by A14,CKB13:def 1;
hence [ ( ( u ^+ p ) . i ),( ( u ^+ p ) . ( i + 1 ) ) ] in ( ==>.-relation S ) by A13,A15,CKB44:def 1;
end;
( len ( p +^ u ) ) = ( len p ) by CKB17:1;
hence ( p +^ u ) is (RedSequence of ( ==>.-relation S )) by A1,A2,REWRITE1:def 2;
( len ( u ^+ p ) ) = ( len p ) by CKB17:1;
hence thesis by A1,A9,REWRITE1:def 2;
end;
