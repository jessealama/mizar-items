suppose A30: 1 < n;

take h = ( ( f | n ) ^ <* p *> );
thus (((((h is  being_S-Seq & ( h /. 1 ) = p1) & ( h /. ( len h ) ) = p) & ( L~ h ) is_S-P_arc_joining p1,p) & ( L~ h ) c= ( L~ f )) & ( L~ h ) = ( ( L~ ( f | n ) ) \/ ( LSeg (q,p) ) )) by A2,A3,A6,A24,A30,CKB16:1;
end;
end;
hence thesis;
end;
end;
hence thesis;
