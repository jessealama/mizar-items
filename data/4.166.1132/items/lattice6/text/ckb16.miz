begin
theorem
(for L being Lattice holds (L is  noetherian iff ( L .: ) is  co-noetherian))
proof
let L being Lattice;
set R = ( LattPOSet L );
set Ri = ( ( LattPOSet L ) ~ );
A1:now
A2: ( ( LattPOSet L ) ~ ) = ( LattPOSet ( L .: ) ) by LATTICE3:20;
assume ( L .: ) is  co-noetherian;
then ( Ri ~ ) is  well_founded by A2,CKB13:def 1;
then R is  well_founded by LATTICE3:8;
hence L is  noetherian by CKB12:def 1;
end;
now
assume L is  noetherian;
then R is  well_founded by CKB12:def 1;
then A3: ( Ri ~ ) is  well_founded by LATTICE3:8;
( ( LattPOSet L ) ~ ) = ( LattPOSet ( L .: ) ) by LATTICE3:20;
hence ( L .: ) is  co-noetherian by A3,CKB13:def 1;
end;
hence thesis by A1;
end;
