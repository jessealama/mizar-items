theorem
Lm10: (for L being  co-noetherian  complete Lattice holds ( MIRRS L ) is  infimum-dense)
proof
let L being  co-noetherian  complete Lattice;
defpred P[ (Element of ( ( LattPOSet L ) ~ )) ]
 means
(ex D9 being (Subset of ( MIRRS L )) st $1 = ( ( "/\" (D9,L) ) % ));
A1: (for x being (Element of ( ( LattPOSet L ) ~ )) holds ((for y being (Element of ( ( LattPOSet L ) ~ )) holds ((y <> x & [ y,x ] in (the InternalRel of ( ( LattPOSet L ) ~ ))) implies P[ y ])) implies P[ x ]))
proof
let x being (Element of ( ( LattPOSet L ) ~ ));
set X = { d where d is (Element of L): (( % ( ~ x ) ) [= d & d <> ( % ( ~ x ) )) };
A2: (for u being (Element of L) holds (u in X implies (( ( u % ) ~ ) <> x & [ ( ( u % ) ~ ),x ] in (the InternalRel of ( ( LattPOSet L ) ~ )))))
proof
let u being (Element of L);
A3: ( ( ( % ( ~ x ) ) % ) ~ ) = ( ( % ( ~ x ) ) % ) by LATTICE3:def 6
.= ( % ( ~ x ) ) by LATTICE3:def 3
.= ( ~ x ) by LATTICE3:def 4
.= x by LATTICE3:def 7;
assume u in X;
then A4: (ex u9 being (Element of L) st ((u9 = u & ( % ( ~ x ) ) [= u9) & ( % ( ~ x ) ) <> u9));
then ( ( % ( ~ x ) ) % ) <= ( u % ) by LATTICE3:7;
then A5: ( ( u % ) ~ ) <= x by A3,LATTICE3:9;
A6: ( ( u % ) ~ ) = ( u % ) by LATTICE3:def 6
.= u by LATTICE3:def 3;
( % ( ~ x ) ) = ( ~ x ) by LATTICE3:def 4
.= x by LATTICE3:def 7;
hence thesis by A4,A5,A6,ORDERS_2:def 5;
end;
assume A7: (for y being (Element of ( ( LattPOSet L ) ~ )) holds ((y <> x & [ y,x ] in (the InternalRel of ( ( LattPOSet L ) ~ ))) implies P[ y ]));
A8: (for u being (Element of L) holds (u in X implies P[ ( ( u % ) ~ ) ]))
proof
let u being (Element of L);
assume u in X;
then (( ( u % ) ~ ) <> x & [ ( ( u % ) ~ ),x ] in (the InternalRel of ( ( LattPOSet L ) ~ ))) by A2;
hence thesis by A7;
end;
per cases ;
suppose ( % ( ~ x ) ) is  completely-meet-irreducible;

then ( % ( ~ x ) ) in { a where a is (Element of L): a is  completely-meet-irreducible };
then (for y being set holds (y in { ( % ( ~ x ) ) } implies y in ( MIRRS L ))) by TARSKI:def 1;
then A9: { ( % ( ~ x ) ) } is (Subset of ( MIRRS L )) by TARSKI:def 3;
( ( "/\" ({ ( % ( ~ x ) ) },L) ) % ) = ( "/\" ({ ( % ( ~ x ) ) },L) ) by LATTICE3:def 3
.= ( % ( ~ x ) ) by LATTICE3:42
.= ( ~ x ) by LATTICE3:def 4
.= x by LATTICE3:def 7;
hence thesis by A9;
end;
suppose A10: (not ( % ( ~ x ) ) is  completely-meet-irreducible);

set G = { H where H is (Subset of ( MIRRS L )): (ex u being (Element of ( LattPOSet L )) st (( % u ) in X & ( % u ) = ( "/\" (H,L) ))) };
set D9 = ( union G );
A11: ( ( % ( ~ x ) ) *' ) = ( % ( ~ x ) ) by A10,CKB31:def 1;
A12: (for r being (Element of L) holds (r is_less_than D9 implies r [= ( % ( ~ x ) )))
proof
let r being (Element of L);
assume A13: r is_less_than D9;
(for q being (Element of L) holds (q in X implies r [= q))
proof
let q being (Element of L);
assume A14: q in X;
then consider D being (Subset of ( MIRRS L )) such that A15: ( ( q % ) ~ ) = ( ( "/\" (D,L) ) % ) by A8;
A16: ( q % ) = q by LATTICE3:def 3;
(for v being set holds (v in D implies v in D9))
proof
let v being set;
( ( q % ) ~ ) = ( q % ) by LATTICE3:def 6
.= ( % ( q % ) ) by LATTICE3:def 4;
then A17: ( % ( q % ) ) = ( "/\" (D,L) ) by A15,LATTICE3:def 3;
( % ( q % ) ) in X by A14,A16,LATTICE3:def 4;
then A18: D in G by A17;
assume v in D;
hence thesis by A18,TARSKI:def 4;
end;
then D c= D9 by TARSKI:def 3;
then (for p being (Element of L) holds (p in D implies r [= p)) by A13,LATTICE3:def 16;
then A19: r is_less_than D by LATTICE3:def 16;
q = ( ( q % ) ~ ) by A16,LATTICE3:def 6
.= ( "/\" (D,L) ) by A15,LATTICE3:def 3;
hence thesis by A19,LATTICE3:34;
end;
then r is_less_than X by LATTICE3:def 16;
hence thesis by A11,LATTICE3:34;
end;
(for v being set holds (v in D9 implies v in ( MIRRS L )))
proof
let v being set;
assume v in D9;
then consider v9 being set such that A20: v in v9 and A21: v9 in G by TARSKI:def 4;
(ex H being (Subset of ( MIRRS L )) st (H = v9 & (ex u being (Element of ( LattPOSet L )) st (( % u ) in X & ( % u ) = ( "/\" (H,L) ))))) by A21;
hence thesis by A20;
end;
then A22: D9 is (Subset of ( MIRRS L )) by TARSKI:def 3;
A23: ( % ( ~ x ) ) = ( ~ x ) by LATTICE3:def 4
.= x by LATTICE3:def 7;
A24: ( ( "/\" (D9,L) ) % ) = ( "/\" (D9,L) ) by LATTICE3:def 3;
(for q being (Element of L) holds (q in D9 implies ( % ( ~ x ) ) [= q))
proof
let q being (Element of L);
assume q in D9;
then consider h being set such that A25: q in h and A26: h in G by TARSKI:def 4;
(ex h9 being (Subset of ( MIRRS L )) st (h9 = h & (ex u being (Element of ( LattPOSet L )) st (( % u ) in X & ( % u ) = ( "/\" (h9,L) ))))) by A26;
then consider u being (Element of ( LattPOSet L )) such that A27: ( % u ) in X and A28: ( % u ) = ( "/\" (h,L) );
( % u ) is_less_than h by A28,LATTICE3:34;
then A29: ( % u ) [= q by A25,LATTICE3:def 16;
( % ( ~ x ) ) is_less_than X by A11,LATTICE3:34;
then ( % ( ~ x ) ) [= ( % u ) by A27,LATTICE3:def 16;
hence thesis by A29,LATTICES:7;
end;
then ( % ( ~ x ) ) is_less_than D9 by LATTICE3:def 16;
then ( % ( ~ x ) ) = ( "/\" (D9,L) ) by A12,LATTICE3:34;
hence thesis by A22,A23,A24;
end;
end;
A30: ( ( LattPOSet L ) ~ ) is  well_founded by CKB13:def 1;
A31: (for x being (Element of ( ( LattPOSet L ) ~ )) holds P[ x ]) from WELLFND1:sch 3(A1,A30);
(for a being (Element of L) holds (ex D9 being (Subset of ( MIRRS L )) st a = ( "/\" (D9,L) )))
proof
let a being (Element of L);
( ( LattPOSet L ) ~ ) = RelStr (# (the carrier of ( LattPOSet L )),( (the InternalRel of ( LattPOSet L )) ~ ) #) by LATTICE3:def 5;
then reconsider a9 = ( a % ) as (Element of ( ( LattPOSet L ) ~ ));
((ex D9 being (Subset of ( MIRRS L )) st a9 = ( ( "/\" (D9,L) ) % )) & ( a % ) = a) by A31,LATTICE3:def 3;
hence thesis by LATTICE3:def 3;
end;
hence thesis by CKB54:def 1;
end;
