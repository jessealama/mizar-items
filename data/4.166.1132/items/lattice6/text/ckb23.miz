Th3: (for L being  noetherian Lattice holds (for a being (Element of L) holds (for d being (Element of L) holds ((a [= d & a <> d) implies (ex c being (Element of L) st (c [= d & c is-upper-neighbour-of a))))))
proof
let L being  noetherian Lattice;
let a being (Element of L);
