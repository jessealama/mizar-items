begin
theorem
Th19: (for L being  complete  co-noetherian Lattice holds (for a being (Element of L) holds (a <> ( Bottom L ) implies (a is  completely-join-irreducible iff ( a % ) is  join-irreducible))))
proof
let L being  complete  co-noetherian Lattice;
let a being (Element of L);
assume a <> ( Bottom L );
then consider b being (Element of L) such that A1: b is-lower-neighbour-of a by CKB28:1;
A2: a <> b by A1,CKB20:def 1;
now
assume A3: ( a % ) is  join-irreducible;
(for d being (Element of L) holds (d is-lower-neighbour-of a implies d = b))
proof
let d being (Element of L);
A4: ( a % ) = a by LATTICE3:def 3;
A5: (( d % ) = d & ( b % ) = b) by LATTICE3:def 3;
assume A6: d is-lower-neighbour-of a;
then A7: a <> d by CKB20:def 1;
assume d <> b;
then a = ( d "\/" b ) by A1,A6,CKB22:1;
then ( a % ) = ( ( d % ) "\/" ( b % ) ) by A4,CKB41:1;
hence thesis by A2,A3,A7,A4,A5,WAYBEL_6:def 3;
end;
hence a is  completely-join-irreducible by A1,CKB39:1;
end;
hence thesis by CKB43:1;
end;
