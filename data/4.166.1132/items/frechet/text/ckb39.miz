theorem
Th27: (for T being non empty TopSpace holds ((for A being (Subset of T) holds ((for S being (sequence of T) holds ((S is  convergent & ( rng S ) c= A) implies ( Lim S ) c= A)) implies A is  closed)) implies T is  sequential))
proof
let T being non empty TopSpace;
assume A1: (for A being (Subset of T) holds ((for S being (sequence of T) holds ((S is  convergent & ( rng S ) c= A) implies ( Lim S ) c= A)) implies A is  closed));
let A being (Subset of T);
thus (A is  closed implies (for S being (sequence of T) holds ((S is  convergent & ( rng S ) c= A) implies ( Lim S ) c= A))) by CKB38:1;
assume (for S being (sequence of T) holds ((S is  convergent & ( rng S ) c= A) implies ( Lim S ) c= A));
hence thesis by A1;
end;
