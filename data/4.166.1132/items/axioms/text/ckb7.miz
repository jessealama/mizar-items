environ
vocabularies XREAL_0,ORDINAL1,ARYTM_2,SUBSET_1,ARYTM_3,ZFMISC_1,CARD_1,XXREAL_0,NUMBERS,ARYTM_0,FUNCOP_1,XBOOLE_0,TARSKI;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,ORDINAL1,FUNCT_4,ARYTM_2,NUMBERS,ARYTM_0,XCMPLX_0,XREAL_0,XXREAL_0;
definitions TARSKI,XBOOLE_0,XXREAL_0,ARYTM_3;
theorems XBOOLE_0,ARYTM_0,ZFMISC_1,TARSKI,ARYTM_1,ARYTM_2,SUBSET_1,ORDINAL1,XBOOLE_1,XREAL_0,XCMPLX_0,NUMBERS,XXREAL_0,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6;
registrations ORDINAL1,ARYTM_2,NUMBERS,XREAL_0;
constructors FUNCT_4,ARYTM_1,ARYTM_0,XCMPLX_0,XXREAL_0,XREAL_0;
