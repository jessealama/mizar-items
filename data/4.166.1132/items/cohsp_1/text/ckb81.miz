theorem
Th29: (for C1,C2 being Coherence_Space holds (for f,g being  U-continuous (Function of C1,C2) holds (( graph f ) = ( graph g ) implies f = g)))
proof
let C1 being Coherence_Space;
let C2 being Coherence_Space;
let f being  U-continuous (Function of C1,C2);
let g being  U-continuous (Function of C1,C2);
A1: ( dom f ) = C1 by FUNCT_2:def 1;
A2: ( dom g ) = C1 by FUNCT_2:def 1;
A3:now
let x being  finite (Element of C1);
let f being  U-continuous (Function of C1,C2);
let g being  U-continuous (Function of C1,C2);
A4: ( dom f ) = C1 by FUNCT_2:def 1;
assume A5: ( graph f ) = ( graph g );
thus ( f . x ) c= ( g . x )
proof
let z being set;
assume z in ( f . x );
then [ x,z ] in ( graph f ) by A4,CKB77:1;
hence thesis by A5,CKB77:1;
end;

end;
A6:now
let a being (Element of C1);
let f being  U-continuous (Function of C1,C2);
let g being  U-continuous (Function of C1,C2);
A7: ( dom g ) = C1 by FUNCT_2:def 1;
assume A8: ( graph f ) = ( graph g );
thus ( f .: ( Fin a ) ) c= ( g .: ( Fin a ) )
proof
let y being set;
assume y in ( f .: ( Fin a ) );
then consider x being set such that x in ( dom f ) and A9: x in ( Fin a ) and A10: y = ( f . x ) by FUNCT_1:def 6;
(( f . x ) c= ( g . x ) & ( g . x ) c= ( f . x )) by A3,A8,A9;
then ( f . x ) = ( g . x ) by XBOOLE_0:def 10;
hence thesis by A7,A9,A10,FUNCT_1:def 6;
end;

end;
assume A11: ( graph f ) = ( graph g );
now
let a being (Element of C1);
(( f .: ( Fin a ) ) c= ( g .: ( Fin a ) ) & ( g .: ( Fin a ) ) c= ( f .: ( Fin a ) )) by A11,A6;
then A12: ( f .: ( Fin a ) ) = ( g .: ( Fin a ) ) by XBOOLE_0:def 10;
thus ( f . a ) = ( union ( f .: ( Fin a ) ) ) by A1,CKB70:1
.= ( g . a ) by A2,A12,CKB70:1;
end;
hence thesis by FUNCT_2:63;
end;
