constructors FINSUB_1,REAL_1,NAT_1,CARD_3,PBOOLE,BORSUK_1,COH_SP,CKB4,CKB5,CKB16,CKB17,CKB32,CKB35,CKB40,CKB41,CKB43,CKB45,CKB46,CKB47,CKB48;
requirements NUMERALS,BOOLE,SUBSET;
begin
registration
let C1 being Coherence_Space;
let C2 being Coherence_Space;
cluster  union-distributive  cap-distributive for (Function of C1,C2);
existence
proof
reconsider a = ( {} ) as (Element of C2) by COH_SP:1;
take f = ( C1 --> a );
A1: ( dom f ) = C1 by FUNCOP_1:13;
thus f is  union-distributive
proof
let A being (Subset of ( dom f ));
assume ( union A ) in ( dom f );
then A2: ( f . ( union A ) ) = ( {} ) by A1,FUNCOP_1:7;
( f .: A ) c= { ( {} ) }
proof
let x being set;
assume x in ( f .: A );
then (ex y being set st ((y in ( dom f ) & y in A) & x = ( f . y ))) by FUNCT_1:def 6;
then x = ( {} ) by A1,FUNCOP_1:7;
hence thesis by TARSKI:def 1;
end;
then A3: ( union ( f .: A ) ) c= ( union { ( {} ) } ) by ZFMISC_1:77;
( union { ( {} ) } ) = ( {} ) by ZFMISC_1:25;
hence thesis by A2,A3;
end;

let a being set;
let b being set;
assume A4: ( dom f ) includes_lattice_of a,b;
then a in ( dom f ) by CKB44:1;
then A5: ( f . a ) = ( {} ) by A1,FUNCOP_1:7;
( a /\ b ) in ( dom f ) by A4,CKB44:1;
hence thesis by A1,A5,FUNCOP_1:7;
end;
end;
