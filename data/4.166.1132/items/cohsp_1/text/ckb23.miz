begin
theorem
Th8: (for X being non  empty set holds ((for a,b being set holds ((a in X & b in X) implies (ex c being set st (c c= ( a /\ b ) & c in X)))) implies X is  c=filtered))
proof
let X being non  empty set;
assume that
A1: (for a,b being set holds ((a in X & b in X) implies (ex c being set st (c c= ( a /\ b ) & c in X))));
set a = the (Element of X);
defpred P[ set ]
 means
(ex a being set st ((for y being set holds (y in $1 implies a c= y)) & a in X));
let Y being  finite (Subset of X);
A2:now
let x being set;
let B being set;
assume that
A3: x in Y
and
B c= Y;
assume P[ B ];
then consider a being set such that A4: (for y being set holds (y in B implies a c= y)) and A5: a in X;
consider c being set such that A6: c c= ( a /\ x ) and A7: c in X by A1,A3,A5;
A8: (( a /\ x ) c= a & ( a /\ x ) c= x) by XBOOLE_1:17;
thus P[ ( B \/ { x } ) ]
proof
take c;
hereby
let y being set;
assume y in ( B \/ { x } );
then (y in B or y in { x }) by XBOOLE_0:def 3;
then ((a c= y & c c= a) or (y = x & c c= x)) by A4,A6,A8,TARSKI:def 1,XBOOLE_1:1;
hence c c= y by XBOOLE_1:1;
end;
thus thesis by A7;
end;

end;
(for y being set holds (y in ( {} ) implies a c= y));
then A9: P[ ( {} ) ];
A10: Y is  finite;
thus P[ Y ] from FINSET_1:sch 2(A10,A9,A2);
end;
