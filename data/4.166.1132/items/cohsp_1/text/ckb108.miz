requirements NUMERALS,BOOLE,SUBSET;
begin
theorem
Th49: (for C1,C2 being Coherence_Space holds (for a,b being  finite (Element of C1) holds (for y1,y2 being set holds ([ [ a,y1 ],[ b,y2 ] ] in ( Web ( StabCoh (C1,C2) ) ) iff ((((not ( a \/ b ) in C1) & y1 in ( union C2 )) & y2 in ( union C2 )) or ([ y1,y2 ] in ( Web C2 ) & (y1 = y2 implies a = b)))))))
proof
let C1 being Coherence_Space;
let C2 being Coherence_Space;
let a being  finite (Element of C1);
let b being  finite (Element of C1);
let y1 being set;
let y2 being set;
hereby
assume [ [ a,y1 ],[ b,y2 ] ] in ( Web ( StabCoh (C1,C2) ) );
then { [ a,y1 ],[ b,y2 ] } in ( StabCoh (C1,C2) ) by COH_SP:5;
then A1: (ex f being  U-stable (Function of C1,C2) st { [ a,y1 ],[ b,y2 ] } = ( Trace f )) by CKB104:def 1;
A2: ([ a,y1 ] in { [ a,y1 ],[ b,y2 ] } & [ b,y2 ] in { [ a,y1 ],[ b,y2 ] }) by TARSKI:def 2;
assume A3: ((( a \/ b ) in C1 or (not y1 in ( union C2 ))) or (not y2 in ( union C2 )));
then { y1,y2 } in C2 by A1,A2,CKB91:1,ZFMISC_1:87;
hence [ y1,y2 ] in ( Web C2 ) by COH_SP:5;
thus (y1 = y2 implies a = b) by A1,A2,A3,CKB92:1,ZFMISC_1:87;
end;
assume A4: ((((not ( a \/ b ) in C1) & y1 in ( union C2 )) & y2 in ( union C2 )) or ([ y1,y2 ] in ( Web C2 ) & (y1 = y2 implies a = b)));
then A5: y2 in ( union C2 ) by ZFMISC_1:87;
then A6: [ b,y2 ] in [: C1,( union C2 ) :] by ZFMISC_1:87;
A7: y1 in ( union C2 ) by A4,ZFMISC_1:87;
then [ a,y1 ] in [: C1,( union C2 ) :] by ZFMISC_1:87;
then reconsider X = { [ a,y1 ],[ b,y2 ] } as (Subset of [: C1,( union C2 ) :]) by A6,ZFMISC_1:32;
A8:now
let a1 being (Element of C1);
let b1 being (Element of C1);
assume A9: ( a1 \/ b1 ) in C1;
let z1 being set;
let z2 being set;
assume that
A10: [ a1,z1 ] in X
and
A11: [ b1,z2 ] in X;
([ b1,z2 ] = [ a,y1 ] or [ b1,z2 ] = [ b,y2 ]) by A11,TARSKI:def 2;
then A12: ((z2 = y1 & b1 = a) or (b1 = b & z2 = y2)) by ZFMISC_1:27;
([ a1,z1 ] = [ a,y1 ] or [ a1,z1 ] = [ b,y2 ]) by A10,TARSKI:def 2;
then ((z1 = y1 & a1 = a) or (a1 = b & z1 = y2)) by ZFMISC_1:27;
then (({ z1,z2 } = { y1 } or { z1,z2 } in C2) or { z1,z2 } = { y2 }) by A4,A9,A12,COH_SP:5,ENUMSET1:29;
hence { z1,z2 } in C2 by A7,A5,COH_SP:4;
end;
A13:now
let a1 being (Element of C1);
let b1 being (Element of C1);
assume A14: ( a1 \/ b1 ) in C1;
let y being set;
assume that
A15: [ a1,y ] in X
and
A16: [ b1,y ] in X;
([ a1,y ] = [ a,y1 ] or [ a1,y ] = [ b,y2 ]) by A15,TARSKI:def 2;
then A17: ((a1 = a & y = y1) or (a1 = b & y = y2)) by ZFMISC_1:27;
([ b1,y ] = [ a,y1 ] or [ b1,y ] = [ b,y2 ]) by A16,TARSKI:def 2;
hence a1 = b1 by A4,A14,A17,ZFMISC_1:27;
end;
now
let x being set;
assume x in X;
then (x = [ a,y1 ] or x = [ b,y2 ]) by TARSKI:def 2;
hence ( x `1 ) is  finite by MCART_1:7;
end;
then (ex f being  U-stable (Function of C1,C2) st X = ( Trace f )) by A8,A13,CKB96:1;
then X in ( StabCoh (C1,C2) ) by CKB104:def 1;
hence thesis by COH_SP:5;
end;
