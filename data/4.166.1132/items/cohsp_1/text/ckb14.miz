registrations XBOOLE_0,SUBSET_1,RELSET_1,PARTFUN1,FINSET_1,FINSUB_1,XREAL_0,NAT_1,FINSEQ_1,ORDINAL1,CARD_1,RELAT_1,CKB3,CKB9,CKB10,CKB11,CKB12;
constructors FINSUB_1,REAL_1,NAT_1,CARD_3,PBOOLE,BORSUK_1,COH_SP,CKB4,CKB5;
requirements NUMERALS,BOOLE,SUBSET;
begin
scheme MinimalElementwrtIncl { a,A() -> set,P[set] } : (ex a being set st ((a in A() & P[ a ]) & (for b being set holds (((b in A() & P[ b ]) & b c= a) implies b = a))))
provided
A1: (a() in A() & P[ a() ])
and
A2: a() is  finite
proof
reconsider a = a() as  finite set by A2;
defpred p[ set ]
 means
($1 c= a() & P[ $1 ]);
consider X being set such that A3: (for x being set holds (x in X iff (x in A() & p[ x ]))) from XBOOLE_0:sch 1;
A4: (( bool a ) is  finite & X c= ( bool a ))
proof
thus ( bool a ) is  finite;
let x being set;
assume x in X;
then x c= a by A3;
hence thesis;
end;
defpred P[ set,set ]
 means
$1 c= $2;
A5: (for x,y being set holds ((P[ x,y ] & P[ y,x ]) implies x = y)) by XBOOLE_0:def 10;
A6: (for x,y,z being set holds ((P[ x,y ] & P[ y,z ]) implies P[ x,z ])) by XBOOLE_1:1;
reconsider X as  finite set by A4;
A7: X <> ( {} ) by A1,A3;
consider x being set such that A8: (x in X & (for y being set holds ((y in X & y <> x) implies (not P[ y,x ])))) from CARD_2:sch 1(A7,A5,A6);
take x;
thus (x in A() & P[ x ]) by A3,A8;
let b being set;
assume that
A9: (b in A() & P[ b ])
and
A10: b c= x;
x c= a by A3,A8;
then b c= a by A10,XBOOLE_1:1;
then b in X by A3,A9;
hence thesis by A8,A10;
end;
