begin
reserve C1 for Coherence_Space;
reserve C2 for Coherence_Space;
registration
let C1 being Coherence_Space;
let C2 being Coherence_Space;
cluster ( C1 [*] C2 ) ->  subset-closed  binary_complete;
coherence
proof
thus ( C1 [*] C2 ) is  subset-closed
proof
let a being set;
let b being set;
assume a in ( C1 [*] C2 );
then consider a1 being (Element of C1),a2 being (Element of C2) such that A1: a c= [: a1,a2 :] by CKB171:1;
assume b c= a;
then b c= [: a1,a2 :] by A1,XBOOLE_1:1;
hence thesis by CKB171:1;
end;

let A being set;
set A1 = { ( proj1 a ) where a is (Element of ( C1 [*] C2 )): a in A };
set A2 = { ( proj2 a ) where a is (Element of ( C1 [*] C2 )): a in A };
assume A2: (for a,b being set holds ((a in A & b in A) implies ( a \/ b ) in ( C1 [*] C2 )));
now
let a1 being set;
let b1 being set;
assume a1 in A2;
then consider a being (Element of ( C1 [*] C2 )) such that A3: a1 = ( proj2 a ) and A4: a in A;
assume b1 in A2;
then consider b being (Element of ( C1 [*] C2 )) such that A5: b1 = ( proj2 b ) and A6: b in A;
( a \/ b ) in ( C1 [*] C2 ) by A2,A4,A6;
then ( proj2 ( a \/ b ) ) in C2 by CKB173:1;
hence ( a1 \/ b1 ) in C2 by A3,A5,FUNCT_5:4;
end;
then A7: ( union A2 ) in C2 by CKB2:def 1;
A8: ( union A ) c= [: ( union A1 ),( union A2 ) :]
proof
let x being set;
assume x in ( union A );
then consider a being set such that A9: x in a and A10: a in A by TARSKI:def 4;
A11: ( a \/ a ) in ( C1 [*] C2 ) by A2,A10;
then ( proj2 a ) in A2 by A10;
then A12: ( proj2 a ) c= ( union A2 ) by ZFMISC_1:74;
a c= [: ( proj1 a ),( proj2 a ) :] by A11,CKB173:1;
then A13: x in [: ( proj1 a ),( proj2 a ) :] by A9;
( proj1 a ) in A1 by A10,A11;
then ( proj1 a ) c= ( union A1 ) by ZFMISC_1:74;
then [: ( proj1 a ),( proj2 a ) :] c= [: ( union A1 ),( union A2 ) :] by A12,ZFMISC_1:96;
hence thesis by A13;
end;
now
let a1 being set;
let b1 being set;
assume a1 in A1;
then consider a being (Element of ( C1 [*] C2 )) such that A14: a1 = ( proj1 a ) and A15: a in A;
assume b1 in A1;
then consider b being (Element of ( C1 [*] C2 )) such that A16: b1 = ( proj1 b ) and A17: b in A;
( a \/ b ) in ( C1 [*] C2 ) by A2,A15,A17;
then ( proj1 ( a \/ b ) ) in C1 by CKB173:1;
hence ( a1 \/ b1 ) in C1 by A14,A16,FUNCT_5:4;
end;
then ( union A1 ) in C1 by CKB2:def 1;
hence thesis by A7,A8,CKB171:1;
end;
