begin
registration
let C1 being Coherence_Space;
let C2 being Coherence_Space;
cluster ( StabCoh (C1,C2) ) -> non  empty  subset-closed  binary_complete;
coherence
proof
set C = ( StabCoh (C1,C2) );
set f = the  U-stable (Function of C1,C2);
( Trace f ) in C by CKB104:def 1;
hence C is non  empty;
thus C is  subset-closed
proof
let a being set;
let b being set;
assume a in C;
then A1: (ex f being  U-stable (Function of C1,C2) st a = ( Trace f )) by CKB104:def 1;
assume b c= a;
then (ex g being  U-stable (Function of C1,C2) st ( Trace g ) = b) by A1,CKB102:1;
hence thesis by CKB104:def 1;
end;

let A being set;
assume A2: (for a,b being set holds ((a in A & b in A) implies ( a \/ b ) in C));
now
let x being set;
let y being set;
assume (x in A & y in A);
then ( x \/ y ) in C by A2;
hence (ex f being  U-stable (Function of C1,C2) st ( x \/ y ) = ( Trace f )) by CKB104:def 1;
end;
then (ex f being  U-stable (Function of C1,C2) st ( union A ) = ( Trace f )) by CKB103:1;
hence thesis by CKB104:def 1;
end;
end;
