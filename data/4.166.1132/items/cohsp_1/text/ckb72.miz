then ( dom f ) includes_lattice_of c,d by A14,A17,A18,CKB44:1;
then ( f . ( c /\ d ) ) = ( ( f . c ) /\ ( f . d ) ) by A14,CKB48:def 1;
then A20: y in ( f . ( c /\ d ) ) by A12,A16,XBOOLE_0:def 4;
( c /\ d ) is  finite  finite (Subset of b) by A19,XBOOLE_1:1;
then (( c /\ d ) c= d & ( c /\ d ) in A) by A20,XBOOLE_1:17;
hence c c= d by A11,XBOOLE_1:17;
end;
assume that
A21: f is  c=-monotone
and
A22: (for a,y being set holds ((a in ( dom f ) & y in ( f . a )) implies (ex b being set st (((b is  finite & b c= a) & y in ( f . b )) & (for c being set holds ((c c= a & y in ( f . c )) implies b c= c))))));
C is  subset-closed  subset-closed set;
hence ( dom f ) is  multiplicative;
now
let a being set;
let y being set;
assume (a in ( dom f ) & y in ( f . a ));
then (ex b being set st (((b is  finite & b c= a) & y in ( f . b )) & (for c being set holds ((c c= a & y in ( f . c )) implies b c= c)))) by A22;
hence (ex b being set st ((b is  finite & b c= a) & y in ( f . b )));
end;
hence f is  U-continuous by A1,A21,CKB71:1;
thus f is  cap-distributive
proof
let a being set;
