theorem
Lm6: (for C1,C2 being Coherence_Space holds (for X being (Subset of [: ( union C1 ),( union C2 ) :]) holds (((for a,b being set holds ({ a,b } in C1 implies (for y1,y2 being set holds (([ a,y1 ] in X & [ b,y2 ] in X) implies { y1,y2 } in C2)))) & (for a,b being set holds ({ a,b } in C1 implies (for y being set holds (([ a,y ] in X & [ b,y ] in X) implies a = b))))) implies (ex f being  U-linear (Function of C1,C2) st (X = ( LinTrace f ) & (for a being (Element of C1) holds ( f . a ) = ( X .: a )))))))
proof
let C1 being Coherence_Space;
let C2 being Coherence_Space;
let X being (Subset of [: ( union C1 ),( union C2 ) :]);
assume that
A1: (for a,b being set holds ({ a,b } in C1 implies (for y1,y2 being set holds (([ a,y1 ] in X & [ b,y2 ] in X) implies { y1,y2 } in C2))))
and
A2: (for a,b being set holds ({ a,b } in C1 implies (for y being set holds (([ a,y ] in X & [ b,y ] in X) implies a = b))));
deffunc f(set) = ( X .: $1 );
consider f being Function such that A3: (( dom f ) = C1 & (for a being set holds (a in C1 implies ( f . a ) = f(a)))) from FUNCT_1:sch 3;
A4:now
let a being set;
let y being set;
assume that
A5: a in ( dom f )
and
A6: y in ( f . a );
reconsider a9 = a as (Element of C1) by A3,A5;
y in ( X .: a ) by A3,A5,A6;
then consider x being set such that A7: [ x,y ] in X and A8: x in a by RELAT_1:def 13;
take x;
{ x } c= a by A8,ZFMISC_1:31;
then { x } in C1 by A3,A5,CLASSES1:def 1;
then (x in { x } & ( f . { x } ) = ( X .: { x } )) by A3,TARSKI:def 1;
hence (x in a & y in ( f . { x } )) by A7,A8,RELAT_1:def 13;
let c being set;
assume that
A9: c c= a
and
A10: y in ( f . c );
c c= a9 by A9;
then c in ( dom f ) by A3,CLASSES1:def 1;
then y in ( X .: c ) by A3,A10;
then consider z being set such that A11: [ z,y ] in X and A12: z in c by RELAT_1:def 13;
{ x,z } c= a9 by A8,A9,A12,ZFMISC_1:32;
then { x,z } in C1 by CLASSES1:def 1;
hence x in c by A2,A7,A11,A12;
end;
A13: ( rng f ) c= C2
proof
let x being set;
assume x in ( rng f );
then consider a being set such that A14: a in ( dom f ) and A15: x = ( f . a ) by FUNCT_1:def 3;
reconsider a as (Element of C1) by A3,A14;
A16: x = ( X .: a ) by A3,A15;
now
let z being set;
let y being set;
assume z in x;
then consider z1 being set such that A17: [ z1,z ] in X and A18: z1 in a by A16,RELAT_1:def 13;
assume y in x;
then consider y1 being set such that A19: [ y1,y ] in X and A20: y1 in a by A16,RELAT_1:def 13;
{ z1,y1 } in C1 by A18,A20,COH_SP:6;
hence { z,y } in C2 by A1,A17,A19;
end;
hence thesis by COH_SP:6;
end;
f is  c=-monotone
proof
let a being set;
let b being set;
assume that
A21: (a in ( dom f ) & b in ( dom f ))
and
A22: a c= b;
reconsider a,b as (Element of C1) by A3,A21;
A23: ( f . a ) = ( X .: a ) by A3;
( X .: a ) c= ( X .: b ) by A22,RELAT_1:123;
hence thesis by A3,A23;
end;
then reconsider f as  U-linear (Function of C1,C2) by A3,A13,A4,CKB73:1,FUNCT_2:def 1,RELSET_1:4;
take f;
thus X = ( LinTrace f )
proof
let a being set;
let b being set;
hereby
assume A24: [ a,b ] in X;
then a in ( union C1 ) by ZFMISC_1:87;
then consider a9 being set such that A25: a in a9 and A26: a9 in C1 by TARSKI:def 4;
{ a } c= a9 by A25,ZFMISC_1:31;
then reconsider aa = { a } as (Element of C1) by A26,CLASSES1:def 1;
A27: (( f . aa ) = ( X .: aa ) & ( f . ( {} ) ) = ( {} )) by A3,CKB52:1;
a in { a } by TARSKI:def 1;
then b in ( X .: aa ) by A24,RELAT_1:def 13;
hence [ a,b ] in ( LinTrace f ) by A3,A27,CKB112:1;
end;
assume A28: [ a,b ] in ( LinTrace f );
then b in ( f . { a } ) by CKB113:1;
then b in ( X .: { a } ) by A3,A28,CKB113:1;
then (ex x being set st ([ x,b ] in X & x in { a })) by RELAT_1:def 13;
hence thesis by TARSKI:def 1;
end;

thus thesis by A3;
end;
