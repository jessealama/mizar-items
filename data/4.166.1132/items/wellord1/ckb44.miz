begin
reserve a for set;
reserve b for set;
reserve Z for set;
reserve R for Relation;
theorem
Th41: (R is  well-ordering implies (for Z holds ((for a holds ((a in ( field R ) & ( R -Seg a ) c= Z) implies a in Z)) implies ( field R ) c= Z)))
proof
assume A1: R is  well-ordering;
then A2: R is  antisymmetric by CKB11:def 1;
let Z;
assume that
A3: (for a holds ((a in ( field R ) & ( R -Seg a ) c= Z) implies a in Z));
A4:now
let a;
assume that
A5: a in ( field R )
and
A6: (for b holds (([ b,a ] in R & a <> b) implies b in Z));
now
let b;
assume b in ( R -Seg a );
then ([ b,a ] in R & b <> a) by CKB6:1;
hence b in Z by A6;
end;
then ( R -Seg a ) c= Z by TARSKI:def 3;
hence a in Z by A3,A5;
end;
given a such that
A7: (a in ( field R ) & (not a in Z));

( ( field R ) \ Z ) <> ( {} ) by A7,XBOOLE_0:def 5;
then consider a such that A8: a in ( ( field R ) \ Z ) and A9: (for b holds (b in ( ( field R ) \ Z ) implies [ a,b ] in R)) by A1,CKB15:1;
(not a in Z) by A8,XBOOLE_0:def 5;
then consider b such that A10: [ b,a ] in R and A11: b <> a and A12: (not b in Z) by A4,A8;
b in ( field R ) by A10,RELAT_1:15;
then b in ( ( field R ) \ Z ) by A12,XBOOLE_0:def 5;
then [ a,b ] in R by A9;
hence contradiction by A2,A10,A11,CKB3:1;
end;
