begin
reserve a for set;
reserve X for set;
reserve R for Relation;
theorem
Th22: (R is  reflexive implies ( R |_2 X ) is  reflexive)
proof
assume A1: R is  reflexive;
now
let a;
assume A2: a in ( field ( R |_2 X ) );
then a in X by CKB23:1;
then A3: [ a,a ] in [: X,X :] by ZFMISC_1:87;
a in ( field R ) by A2,CKB23:1;
then [ a,a ] in R by A1,CKB1:1;
hence [ a,a ] in ( R |_2 X ) by A3,XBOOLE_0:def 4;
end;
hence thesis by CKB1:1;
end;
