environ
vocabularies RELAT_1,RELAT_2,XBOOLE_0,SUBSET_1,TARSKI,FUNCT_1,ZFMISC_1,WELLORD1;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,RELAT_1,RELAT_2,FUNCT_1,CKB5,CKB8,CKB9,CKB11,CKB12,CKB19;
definitions TARSKI,XBOOLE_0,FUNCT_1,RELAT_1,CKB5,CKB8,CKB9,CKB11,CKB12,CKB19;
theorems TARSKI,XBOOLE_0,FUNCT_1,RELAT_1,RELAT_2,ZFMISC_1,XBOOLE_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB37,CKB38,CKB39,CKB40,CKB41,CKB42,CKB43,CKB44,CKB45;
schemes XBOOLE_0,FUNCT_1;
registrations FUNCT_1,RELAT_1;
constructors TARSKI,SUBSET_1,FUNCT_1,RELAT_2,CKB5,CKB8,CKB9,CKB11,CKB12,CKB19;
requirements SUBSET,BOOLE;
begin
reserve a for set;
reserve b for set;
reserve Z for set;
reserve R for Relation;
reserve F for Function;
theorem
Th43: ((((R is  well-ordering & ( dom F ) = ( field R )) & ( rng F ) c= ( field R )) & (for a holds (for b holds (([ a,b ] in R & a <> b) implies ([ ( F . a ),( F . b ) ] in R & ( F . a ) <> ( F . b )))))) implies (for a holds (a in ( field R ) implies [ a,( F . a ) ] in R)))
proof
assume that
A1: ((R is  well-ordering & ( dom F ) = ( field R )) & ( rng F ) c= ( field R ))
and
A2: (([ a,b ] in R & a <> b) implies ([ ( F . a ),( F . b ) ] in R & ( F . a ) <> ( F . b )));
A3: R is  antisymmetric by A1,CKB11:def 1;
defpred P[ set ]
 means
[ $1,( F . $1 ) ] in R;
consider Z such that A4: (a in Z iff (a in ( field R ) & P[ a ])) from XBOOLE_0:sch 1;
A5: R is  transitive by A1,CKB11:def 1;
now
let a;
assume A6: a in ( field R );
assume A7: ( R -Seg a ) c= Z;
A8:now
let b;
assume A9: b in ( R -Seg a );
then A10: [ b,( F . b ) ] in R by A4,A7;
A11: ([ b,a ] in R & b <> a) by A9,CKB6:1;
then A12: [ ( F . b ),( F . a ) ] in R by A2;
hence [ b,( F . a ) ] in R by A5,A10,CKB2:1;
( F . b ) <> ( F . a ) by A2,A11;
hence b <> ( F . a ) by A3,A10,A12,CKB3:1;
end;
( F . a ) in ( rng F ) by A1,A6,FUNCT_1:def 3;
then [ a,( F . a ) ] in R by A1,A6,A8,CKB45:1;
hence a in Z by A4,A6;
end;
then A13: ( field R ) c= Z by A1,CKB44:1;
let a;
assume a in ( field R );
hence thesis by A4,A13;
end;
