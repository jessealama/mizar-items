begin
reserve a for set;
reserve b for set;
reserve R for Relation;
reserve S for Relation;
reserve F for Function;
theorem
Th45: (F is_isomorphism_of R,S implies (for a holds (for b holds (([ a,b ] in R & a <> b) implies ([ ( F . a ),( F . b ) ] in S & ( F . a ) <> ( F . b ))))))
proof
assume A1: F is_isomorphism_of R,S;
then A2: (( dom F ) = ( field R ) & F is  one-to-one) by CKB47:def 1;
let a;
let b;
assume that
A3: [ a,b ] in R
and
A4: a <> b;
(a in ( field R ) & b in ( field R )) by A1,A3,CKB47:def 1;
hence thesis by A1,A2,A3,A4,CKB47:def 1,FUNCT_1:def 4;
end;
