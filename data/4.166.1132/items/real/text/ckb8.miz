environ
vocabularies XREAL_0,ORDINAL1,XXREAL_0,NUMBERS,ARYTM_2,SUBSET_1,ARYTM_3,ZFMISC_1,CARD_1,XCMPLX_0;
notations TARSKI,ZFMISC_1,SUBSET_1,ARYTM_2,ORDINAL1,NUMBERS,XXREAL_0,XREAL_0;
theorems XBOOLE_0,ZFMISC_1,ARYTM_0,ARYTM_1,XREAL_0,NUMBERS,XXREAL_0,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7;
registrations ARYTM_2,XREAL_0,ORDINAL1,XBOOLE_0;
constructors ARYTM_2,NUMBERS,XXREAL_0,XREAL_0;
requirements SUBSET,BOOLE;
begin
reserve x for  real number;
reserve y for  real number;
theorem
(((x <= y & x is non  zero) & y is non  positive) implies x is  negative)
proof
assume that
A1: x <= y
and
A2: x is non  zero
and
A3: y is non  positive
and
A4: x is non  negative;
x >= ( 0 ) by A4,XXREAL_0:def 7;
then A5: x > ( 0 ) by A2,CKB1:1;
y <= ( 0 ) by A3,XXREAL_0:def 6;
hence thesis by A1,A5,CKB2:1;
end;
