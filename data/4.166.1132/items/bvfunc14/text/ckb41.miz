theorem
Th40: (for A,B,C,D,E,F being set holds (for h being Function holds (for A9,B9,C9,D9,E9,F9 being set holds ((((((((((((((((A <> B & A <> C) & A <> D) & A <> E) & A <> F) & B <> C) & B <> D) & B <> E) & B <> F) & C <> D) & C <> E) & C <> F) & D <> E) & D <> F) & E <> F) & h = ( ( ( ( ( ( B .--> B9 ) +* ( C .--> C9 ) ) +* ( D .--> D9 ) ) +* ( E .--> E9 ) ) +* ( F .--> F9 ) ) +* ( A .--> A9 ) )) implies (((((( h . A ) = A9 & ( h . B ) = B9) & ( h . C ) = C9) & ( h . D ) = D9) & ( h . E ) = E9) & ( h . F ) = F9)))))
proof
let A being set;
let B being set;
let C being set;
let D being set;
let E being set;
let F being set;
let h being Function;
let A9 being set;
let B9 being set;
let C9 being set;
let D9 being set;
let E9 being set;
