begin
reserve Y for non  empty set;
reserve G for (Subset of ( PARTITIONS Y ));
reserve A for (a_partition of Y);
reserve B for (a_partition of Y);
reserve C for (a_partition of Y);
reserve D for (a_partition of Y);
reserve E for (a_partition of Y);
reserve F for (a_partition of Y);
reserve J for (a_partition of Y);
reserve M for (a_partition of Y);
theorem
(for G being (Subset of ( PARTITIONS Y )) holds (for A,B,C,D,E,F,J,M being (a_partition of Y) holds (for z,u being (Element of Y) holds ((((((((((((((((((((((((((((((G is  independent & G = { A,B,C,D,E,F,J,M }) & A <> B) & A <> C) & A <> D) & A <> E) & A <> F) & A <> J) & A <> M) & B <> C) & B <> D) & B <> E) & B <> F) & B <> J) & B <> M) & C <> D) & C <> E) & C <> F) & C <> J) & C <> M) & D <> E) & D <> F) & D <> J) & D <> M) & E <> F) & E <> J) & E <> M) & F <> J) & F <> M) & J <> M) implies ( ( EqClass (u,( ( ( ( ( ( B '/\' C ) '/\' D ) '/\' E ) '/\' F ) '/\' J ) '/\' M )) ) /\ ( EqClass (z,A) ) ) <> ( {} )))))
proof
let G being (Subset of ( PARTITIONS Y ));
let A being (a_partition of Y);
let B being (a_partition of Y);
let C being (a_partition of Y);
let D being (a_partition of Y);
let E being (a_partition of Y);
let F being (a_partition of Y);
let J being (a_partition of Y);
let M being (a_partition of Y);
let z being (Element of Y);
let u being (Element of Y);
assume that
A1: G is  independent
and
A2: G = { A,B,C,D,E,F,J,M }
and
A3: (((((A <> B & A <> C) & A <> D) & A <> E) & A <> F) & A <> J)
and
A4: A <> M
and
A5: ((((((((((((((((((((B <> C & B <> D) & B <> E) & B <> F) & B <> J) & B <> M) & C <> D) & C <> E) & C <> F) & C <> J) & C <> M) & D <> E) & D <> F) & D <> J) & D <> M) & E <> F) & E <> J) & E <> M) & F <> J) & F <> M) & J <> M);
set h = ( ( ( ( ( ( ( ( B .--> ( EqClass (u,B) ) ) +* ( C .--> ( EqClass (u,C) ) ) ) +* ( D .--> ( EqClass (u,D) ) ) ) +* ( E .--> ( EqClass (u,E) ) ) ) +* ( F .--> ( EqClass (u,F) ) ) ) +* ( J .--> ( EqClass (u,J) ) ) ) +* ( M .--> ( EqClass (u,M) ) ) ) +* ( A .--> ( EqClass (z,A) ) ) );
A6: ( h . B ) = ( EqClass (u,B) ) by A3,A5,CKB66:1;
reconsider GG = ( EqClass (u,( ( ( ( ( ( B '/\' C ) '/\' D ) '/\' E ) '/\' F ) '/\' J ) '/\' M )) ) as set;
reconsider I = ( EqClass (z,A) ) as set;
GG = ( ( EqClass (u,( ( ( ( ( B '/\' C ) '/\' D ) '/\' E ) '/\' F ) '/\' J )) ) /\ ( EqClass (u,M) ) ) by CKB1:1;
then GG = ( ( ( EqClass (u,( ( ( ( B '/\' C ) '/\' D ) '/\' E ) '/\' F )) ) /\ ( EqClass (u,J) ) ) /\ ( EqClass (u,M) ) ) by CKB1:1;
then GG = ( ( ( ( EqClass (u,( ( ( B '/\' C ) '/\' D ) '/\' E )) ) /\ ( EqClass (u,F) ) ) /\ ( EqClass (u,J) ) ) /\ ( EqClass (u,M) ) ) by CKB1:1;
then GG = ( ( ( ( ( EqClass (u,( ( B '/\' C ) '/\' D )) ) /\ ( EqClass (u,E) ) ) /\ ( EqClass (u,F) ) ) /\ ( EqClass (u,J) ) ) /\ ( EqClass (u,M) ) ) by CKB1:1;
then GG = ( ( ( ( ( ( EqClass (u,( B '/\' C )) ) /\ ( EqClass (u,D) ) ) /\ ( EqClass (u,E) ) ) /\ ( EqClass (u,F) ) ) /\ ( EqClass (u,J) ) ) /\ ( EqClass (u,M) ) ) by CKB1:1;
then A7: ( GG /\ I ) = ( ( ( ( ( ( ( ( EqClass (u,B) ) /\ ( EqClass (u,C) ) ) /\ ( EqClass (u,D) ) ) /\ ( EqClass (u,E) ) ) /\ ( EqClass (u,F) ) ) /\ ( EqClass (u,J) ) ) /\ ( EqClass (u,M) ) ) /\ ( EqClass (z,A) ) ) by CKB1:1;
A8: ( h . A ) = ( EqClass (z,A) ) by FUNCT_7:94;
A9: ( h . C ) = ( EqClass (u,C) ) by A3,A5,CKB66:1;
A10: ( h . M ) = ( EqClass (u,M) ) by A4,CKB4:1;
A11: ( h . J ) = ( EqClass (u,J) ) by A3,A5,CKB66:1;
A12: ( h . F ) = ( EqClass (u,F) ) by A3,A5,CKB66:1;
A13: ( h . E ) = ( EqClass (u,E) ) by A3,A5,CKB66:1;
A14: ( h . D ) = ( EqClass (u,D) ) by A3,A5,CKB66:1;
A15: ( rng h ) = { ( h . A ),( h . B ),( h . C ),( h . D ),( h . E ),( h . F ),( h . J ),( h . M ) } by CKB68:1;
( rng h ) c= ( bool Y )
proof
let t being set;
assume t in ( rng h );
then (((((((t = ( h . A ) or t = ( h . B )) or t = ( h . C )) or t = ( h . D )) or t = ( h . E )) or t = ( h . F )) or t = ( h . J )) or t = ( h . M )) by A15,ENUMSET1:def 6;
hence thesis by A8,A6,A9,A14,A13,A12,A11,A10;
end;
then reconsider FF = ( rng h ) as (Subset-Family of Y);
A16: ( dom h ) = G by A2,CKB67:1;
then A in ( dom h ) by A2,ENUMSET1:def 6;
then A17: ( h . A ) in ( rng h ) by FUNCT_1:def 3;
then A18: ( Intersect FF ) = ( meet ( rng h ) ) by SETFAM_1:def 9;
(for d being set holds (d in G implies ( h . d ) in d))
proof
let d being set;
assume d in G;
then (((((((d = A or d = B) or d = C) or d = D) or d = E) or d = F) or d = J) or d = M) by A2,ENUMSET1:def 6;
hence thesis by A8,A6,A9,A14,A13,A12,A11,A10;
end;
then ( Intersect FF ) <> ( {} ) by A1,A16,BVFUNC_2:def 5;
then consider m being set such that A19: m in ( Intersect FF ) by XBOOLE_0:def 1;
C in ( dom h ) by A2,A16,ENUMSET1:def 6;
then ( h . C ) in ( rng h ) by FUNCT_1:def 3;
then A20: m in ( EqClass (u,C) ) by A9,A18,A19,SETFAM_1:def 1;
B in ( dom h ) by A2,A16,ENUMSET1:def 6;
then ( h . B ) in ( rng h ) by FUNCT_1:def 3;
then m in ( EqClass (u,B) ) by A6,A18,A19,SETFAM_1:def 1;
then A21: m in ( ( EqClass (u,B) ) /\ ( EqClass (u,C) ) ) by A20,XBOOLE_0:def 4;
D in ( dom h ) by A2,A16,ENUMSET1:def 6;
then ( h . D ) in ( rng h ) by FUNCT_1:def 3;
then m in ( EqClass (u,D) ) by A14,A18,A19,SETFAM_1:def 1;
then A22: m in ( ( ( EqClass (u,B) ) /\ ( EqClass (u,C) ) ) /\ ( EqClass (u,D) ) ) by A21,XBOOLE_0:def 4;
E in ( dom h ) by A2,A16,ENUMSET1:def 6;
then ( h . E ) in ( rng h ) by FUNCT_1:def 3;
then m in ( EqClass (u,E) ) by A13,A18,A19,SETFAM_1:def 1;
then A23: m in ( ( ( ( EqClass (u,B) ) /\ ( EqClass (u,C) ) ) /\ ( EqClass (u,D) ) ) /\ ( EqClass (u,E) ) ) by A22,XBOOLE_0:def 4;
F in ( dom h ) by A2,A16,ENUMSET1:def 6;
then ( h . F ) in ( rng h ) by FUNCT_1:def 3;
then m in ( EqClass (u,F) ) by A12,A18,A19,SETFAM_1:def 1;
then A24: m in ( ( ( ( ( EqClass (u,B) ) /\ ( EqClass (u,C) ) ) /\ ( EqClass (u,D) ) ) /\ ( EqClass (u,E) ) ) /\ ( EqClass (u,F) ) ) by A23,XBOOLE_0:def 4;
J in ( dom h ) by A2,A16,ENUMSET1:def 6;
then ( h . J ) in ( rng h ) by FUNCT_1:def 3;
then m in ( EqClass (u,J) ) by A11,A18,A19,SETFAM_1:def 1;
then A25: m in ( ( ( ( ( ( EqClass (u,B) ) /\ ( EqClass (u,C) ) ) /\ ( EqClass (u,D) ) ) /\ ( EqClass (u,E) ) ) /\ ( EqClass (u,F) ) ) /\ ( EqClass (u,J) ) ) by A24,XBOOLE_0:def 4;
M in ( dom h ) by A2,A16,ENUMSET1:def 6;
then ( h . M ) in ( rng h ) by FUNCT_1:def 3;
then m in ( EqClass (u,M) ) by A10,A18,A19,SETFAM_1:def 1;
then A26: m in ( ( ( ( ( ( ( EqClass (u,B) ) /\ ( EqClass (u,C) ) ) /\ ( EqClass (u,D) ) ) /\ ( EqClass (u,E) ) ) /\ ( EqClass (u,F) ) ) /\ ( EqClass (u,J) ) ) /\ ( EqClass (u,M) ) ) by A25,XBOOLE_0:def 4;
m in ( EqClass (z,A) ) by A8,A17,A18,A19,SETFAM_1:def 1;
hence thesis by A7,A26,XBOOLE_0:def 4;
end;
