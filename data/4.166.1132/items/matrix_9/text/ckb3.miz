registrations XBOOLE_0,SUBSET_1,FUNCT_1,ORDINAL1,PARTFUN1,FUNCT_2,FINSET_1,FINSUB_1,XXREAL_0,XREAL_0,FINSEQ_1,FINSEQ_5,FINSEQ_6,STRUCT_0,GROUP_1,VECTSP_1,MATRIX_2,FVSUM_1,RELSET_1,FINSEQ_2,CARD_1,CKB2;
constructors RELAT_2,BINOP_1,SETWISEO,XXREAL_0,NAT_1,NAT_D,FINSEQ_4,FINSOP_1,FINSEQ_5,ALGSTR_1,GROUP_4,MATRIX_7,RELSET_1;
requirements NUMERALS,REAL,BOOLE,SUBSET,ARITHM;
begin
reserve x for set;
reserve n for Nat;
scheme NonEmptyFiniteX { n() -> (Element of ( NAT )),A() -> non  empty (Element of ( Fin ( Permutations n() ) )),P[set] } : P[ A() ]
provided
A1: (for x being (Element of ( Permutations n() )) holds (x in A() implies P[ { x } ]))
and
A2: (for x being (Element of ( Permutations n() )) holds (for B being non  empty (Element of ( Fin ( Permutations n() ) )) holds ((((x in A() & B c= A()) & (not x in B)) & P[ B ]) implies P[ ( B \/ { x } ) ])))
proof
defpred Q[ set ]
 means
($1 is  empty or P[ $1 ]);
A3: (for x,B being set holds (((x in A() & B c= A()) & Q[ B ]) implies Q[ ( B \/ { x } ) ]))
proof
let x being set;
let B being set;
assume that
A4: x in A()
and
A5: B c= A()
and
A6: Q[ B ];
A7: A() c= ( Permutations n() ) by FINSUB_1:def 5;
then reconsider x as (Element of ( Permutations n() )) by A4;
A8: B c= ( Permutations n() ) by A5,A7,XBOOLE_1:1;
per cases ;
suppose A9: x in B;

then reconsider B as non  empty (Element of ( Fin ( Permutations n() ) )) by A5,A8,FINSUB_1:def 5;
{ x } c= B by A9,ZFMISC_1:31;
hence thesis by A6,XBOOLE_1:12;
end;
suppose A10: (B is non  empty & (not x in B));

then reconsider B as non  empty (Element of ( Fin ( Permutations n() ) )) by A5,A8,FINSUB_1:def 5;
P[ ( B \/ { x } ) ] by A2,A4,A5,A6,A10;
hence thesis;
end;
suppose B is  empty;

then P[ ( B \/ { x } ) ] by A1,A4;
hence thesis;
end;
end;
A11: Q[ ( {} ) ];
A12: A() is  finite;
Q[ A() ] from FINSET_1:sch 2(A12,A11,A3);
hence thesis;
end;
