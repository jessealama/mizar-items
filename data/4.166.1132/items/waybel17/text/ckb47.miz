begin
theorem
Lm10: (for L being  continuous  Scott TopLattice holds (for p being (Element of L) holds (for S being (Subset of L) holds ((S is  open & p in S) implies (ex q being (Element of L) st (q << p & q in S))))))
proof
let L being  continuous  Scott TopLattice;
let p being (Element of L);
let S being (Subset of L);
assume that
A1: S is  open
and
A2: p in S;
A3: S is  inaccessible by A1,WAYBEL11:def 4;
( sup ( waybelow p ) ) = p by WAYBEL_3:def 5;
then ( waybelow p ) meets S by A2,A3,WAYBEL11:def 1;
then ( ( waybelow p ) /\ S ) <> ( {} ) by XBOOLE_0:def 7;
then consider u being set such that A4: u in ( ( waybelow p ) /\ S ) by XBOOLE_0:def 1;
A5: u in ( waybelow p ) by A4,XBOOLE_0:def 4;
reconsider u as (Element of L) by A4;
take u;
thus u << p by A5,WAYBEL_3:7;
thus thesis by A4,XBOOLE_0:def 4;
end;
