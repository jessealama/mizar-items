begin
registration
let S being non  empty RelStr;
let a being (Element of S);
let b being (Element of S);
cluster ( Net-Str (a,b) ) ->  reflexive  transitive  directed  antisymmetric;
coherence
proof
set L = ( Net-Str (a,b) );
thus L is  reflexive
proof
let x being (Element of L);
reconsider x9 = x as (Element of ( NAT )) by CKB18:def 1;
x9 <= x9;
hence thesis by CKB18:def 1;
end;

thus L is  transitive
proof
let x being (Element of L);
let y being (Element of L);
let z being (Element of L);
assume that
A1: x <= y
and
A2: y <= z;
reconsider x9 = x,y9 = y,z9 = z as (Element of ( NAT )) by CKB18:def 1;
A3: x9 <= y9 by A1,CKB18:def 1;
y9 <= z9 by A2,CKB18:def 1;
then x9 <= z9 by A3,XXREAL_0:2;
hence thesis by CKB18:def 1;
end;

( [#] L ) is  directed
proof
let x being (Element of L);
let y being (Element of L);
assume that
x in ( [#] L )
and
y in ( [#] L );
reconsider x9 = x,y9 = y as (Element of ( NAT )) by CKB18:def 1;
set z9 = ( x9 + y9 );
reconsider z = z9 as (Element of L) by CKB18:def 1;
reconsider z as (Element of L);
take z;
A4: x9 <= z9 by NAT_1:11;
y9 <= z9 by NAT_1:11;
hence thesis by A4,CKB18:def 1;
end;
hence L is  directed by WAYBEL_0:def 6;
thus L is  antisymmetric
proof
let x being (Element of L);
let y being (Element of L);
reconsider x9 = x,y9 = y as (Element of ( NAT )) by CKB18:def 1;
assume that
A5: x <= y
and
A6: y <= x;
A7: x9 <= y9 by A5,CKB18:def 1;
y9 <= x9 by A6,CKB18:def 1;
hence thesis by A7,XXREAL_0:1;
end;

end;
