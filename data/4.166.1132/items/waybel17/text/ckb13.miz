theorem
Lm4: (for S,T being  up-complete  Scott non empty  reflexive  transitive  antisymmetric  TopSpace-like TopRelStr holds (for f being (Function of S,T) holds (f is  directed-sups-preserving implies f is  continuous)))
proof
let S being  up-complete  Scott non empty  reflexive  transitive  antisymmetric  TopSpace-like TopRelStr,T being  up-complete  Scott non empty  reflexive  transitive  antisymmetric  TopSpace-like TopRelStr;
let f being (Function of S,T);
assume A1: f is  directed-sups-preserving;
thus f is  continuous
proof
let P1 being (Subset of T);
reconsider P19 = P1 as (Subset of T);
assume P1 is  closed;
then A2: P19 is  directly_closed  lower by WAYBEL11:7;
now
let D being non empty  directed (Subset of S);
assume A3: D c= ( f " P1 );
A4: f preserves_sup_of D by A1,WAYBEL_0:def 37;
 ex_sup_of D,S by WAYBEL_0:75;
then A5: ( sup ( f .: D ) ) = ( f . ( sup D ) ) by A4,WAYBEL_0:def 31;
reconsider fD = ( f .: D ) as  directed (Subset of T) by A1,YELLOW_2:15;
fD c= P1 by A3,EQREL_1:46;
then ( sup fD ) in P19 by A2,WAYBEL11:def 2;
hence ( sup D ) in ( f " P1 ) by A5,FUNCT_2:38;
end;
then ( f " P1 ) is  directly_closed  lower by A1,A2,CKB4:1,WAYBEL11:def 2;
hence thesis by WAYBEL11:7;
end;

