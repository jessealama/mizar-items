environ
vocabularies XBOOLE_0,ROBBINS1,SUBSET_1,ARYTM_3,LATTICES,ROBBINS2;
notations STRUCT_0,LATTICES,ROBBINS1,CKB1;
definitions CKB1;
theorems ROBBINS1,LATTICES,STRUCT_0,CKB1,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11;
registrations LATTICES,ROBBINS1,STRUCT_0,CKB2,CKB3,CKB4;
constructors ROBBINS1,CKB1;
begin
reserve L for  satisfying_DN_1 non  empty ComplLLattStr;
reserve x for (Element of L);
reserve y for (Element of L);
theorem
Th8: (for L being  satisfying_DN_1 non  empty ComplLLattStr holds (for x,y being (Element of L) holds ( ( x + ( ( ( ( x + y ) ` ) + x ) ` ) ) ` ) = ( ( x + y ) ` )))
proof
let L being  satisfying_DN_1 non  empty ComplLLattStr;
let x being (Element of L);
let y being (Element of L);
set X = ( ( x + y ) ` );
set Y = x;
( ( ( ( ( ( X + Y ) ` ) + X ) ` ) + ( ( X + Y ) ` ) ) ` ) = X by CKB11:1;
hence thesis by CKB11:1;
end;
