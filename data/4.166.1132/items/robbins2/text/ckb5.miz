environ
vocabularies XBOOLE_0,ROBBINS1,SUBSET_1,ARYTM_3,LATTICES,ROBBINS2;
notations STRUCT_0,LATTICES,ROBBINS1,CKB1;
definitions CKB1;
theorems ROBBINS1,LATTICES,STRUCT_0,CKB1;
registrations LATTICES,ROBBINS1,STRUCT_0,CKB2,CKB3,CKB4;
constructors ROBBINS1,CKB1;
begin
reserve L for  satisfying_DN_1 non  empty ComplLLattStr;
reserve x for (Element of L);
reserve y for (Element of L);
reserve z for (Element of L);
theorem
Th1: (for L being  satisfying_DN_1 non  empty ComplLLattStr holds (for x,y,z,u,v being (Element of L) holds ( ( ( ( x + y ) ` ) + ( ( ( ( ( ( z + u ) ` ) + x ) ` ) + ( ( ( y ` ) + ( ( y + v ) ` ) ) ` ) ) ` ) ) ` ) = y))
proof
let L being  satisfying_DN_1 non  empty ComplLLattStr;
let x being (Element of L);
let y being (Element of L);
let z being (Element of L);
let u being (Element of L);
let v being (Element of L);
set X = ( ( ( ( z + u ) ` ) + x ) ` );
set Y = ( ( z + ( ( ( x ` ) + ( ( x + u ) ` ) ) ` ) ) ` );
set Z = y;
set U = v;
( ( ( ( ( ( X + Y ) ` ) + Z ) ` ) + ( ( X + ( ( ( Z ` ) + ( ( Z + U ) ` ) ) ` ) ) ` ) ) ` ) = Z by CKB1:def 1;
hence thesis by CKB1:def 1;
end;
