begin
theorem
Th1: (for X,Y,Z being ComplexLinearSpace holds (for f being (LinearOperator of X,Y) holds (for g being (LinearOperator of Y,Z) holds ( g * f ) is (LinearOperator of X,Z))))
proof
let X being ComplexLinearSpace;
let Y being ComplexLinearSpace;
let Z being ComplexLinearSpace;
let f being (LinearOperator of X,Y);
let g being (LinearOperator of Y,Z);
A1:now
let v being (VECTOR of X);
let z being Complex;
thus ( ( g * f ) . ( z * v ) ) = ( g . ( f . ( z * v ) ) ) by FUNCT_2:15
.= ( g . ( z * ( f . v ) ) ) by CLOPBAN1:def 3
.= ( z * ( g . ( f . v ) ) ) by CLOPBAN1:def 3
.= ( z * ( ( g * f ) . v ) ) by FUNCT_2:15;
end;
now
let v being (VECTOR of X);
let w being (VECTOR of X);
thus ( ( g * f ) . ( v + w ) ) = ( g . ( f . ( v + w ) ) ) by FUNCT_2:15
.= ( g . ( ( f . v ) + ( f . w ) ) ) by GRCAT_1:def 8
.= ( ( g . ( f . v ) ) + ( g . ( f . w ) ) ) by GRCAT_1:def 8
.= ( ( ( g * f ) . v ) + ( g . ( f . w ) ) ) by FUNCT_2:15
.= ( ( ( g * f ) . v ) + ( ( g * f ) . w ) ) by FUNCT_2:15;
end;
hence thesis by A1,CLOPBAN1:def 3,GRCAT_1:def 8;
end;
