begin
reserve D for non empty set;
reserve d for (Element of D);
reserve g for (BinOp of D);
reserve k for (Element of ( NAT ));
reserve l for (Element of ( NAT ));
theorem
((g is  associative & (g is  having_a_unity or (k <> ( 0 ) & l <> ( 0 )))) implies ( g "**" ( ( k * l ) |-> d ) ) = ( g "**" ( l |-> ( g "**" ( k |-> d ) ) ) ))
proof
defpred P[ (Element of ( NAT )) ]
 means
(for g holds (for k holds (for d holds ((g is  associative & (g is  having_a_unity or (k <> ( 0 ) & $1 <> ( 0 )))) implies ( g "**" ( ( k * $1 ) |-> d ) ) = ( g "**" ( $1 |-> ( g "**" ( k |-> d ) ) ) )))));
A1: (for l holds (P[ l ] implies P[ ( l + 1 ) ]))
proof
let l;
assume A2: P[ l ];
let g;
let k;
let d;
assume that
A3: g is  associative
and
A4: (g is  having_a_unity or (k <> ( 0 ) & ( l + 1 ) <> ( 0 )));
now
per cases ;
suppose l = ( 0 );

hence thesis by CKB34:1;
end;
suppose A5: l <> ( 0 );

then A6: (k <> ( 0 ) implies ( k * l ) <> ( 0 )) by XCMPLX_1:6;
( g "**" ( ( k * ( l + 1 ) ) |-> d ) ) = ( g "**" ( ( ( k * l ) + ( k * 1 ) ) |-> d ) )
.= ( g . (( g "**" ( ( k * l ) |-> d ) ),( g "**" ( k |-> d ) )) ) by A3,A4,A6,CKB36:1
.= ( g . (( g "**" ( l |-> ( g "**" ( k |-> d ) ) ) ),( g "**" ( k |-> d ) )) ) by A2,A3,A4,A5
.= ( g . (( g "**" ( l |-> ( g "**" ( k |-> d ) ) ) ),( g "**" ( 1 |-> ( g "**" ( k |-> d ) ) ) )) ) by CKB34:1;
hence thesis by A3,A5,CKB36:1;
end;
end;
hence thesis;
end;
A7: P[ ( 0 ) ];
(for l holds P[ l ]) from NAT_1:sch 1(A7,A1);
hence thesis;
end;
