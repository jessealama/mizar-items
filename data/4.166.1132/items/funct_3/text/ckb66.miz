reserve x for set;
theorem
Th68: (for f,g being Function holds (x in ( ( dom f ) /\ ( dom g ) ) implies ( <: f,g :> . x ) = [ ( f . x ),( g . x ) ]))
proof
let f being Function,g being Function;
assume x in ( ( dom f ) /\ ( dom g ) );
then x in ( dom <: f,g :> ) by CKB63:def 1;
hence thesis by CKB63:def 1;
end;
