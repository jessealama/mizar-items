environ
vocabularies XBOOLE_0,TARSKI,RELAT_1,FUNCT_1,ZFMISC_1,SUBSET_1,MCART_1,PARTFUN1,FUNCT_2,FUNCT_3;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,RELAT_1,RELSET_1,FUNCT_1,FUNCT_2,BINOP_1,CKB9,CKB24,CKB25,CKB41,CKB47,CKB48,CKB49,CKB52,CKB53,CKB58,CKB59,CKB60,CKB62,CKB63,CKB77,CKB84;
definitions TARSKI,BINOP_1,CKB9,CKB25,CKB41,CKB52,CKB53,CKB60,CKB63,CKB84;
theorems TARSKI,ZFMISC_1,FUNCT_1,FUNCT_2,RELAT_1,RELSET_1,XBOOLE_0,XBOOLE_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB37,CKB38,CKB39,CKB40,CKB41,CKB42,CKB43,CKB44,CKB45,CKB46,CKB50,CKB51,CKB52,CKB53,CKB54,CKB55,CKB56,CKB57,CKB60,CKB61,CKB63,CKB66,CKB67,CKB68,CKB69,CKB70,CKB71,CKB72,CKB73,CKB74,CKB75,CKB76,CKB78,CKB79,CKB80,CKB81,CKB82,CKB83,CKB84,CKB85,CKB86,CKB87,CKB88,CKB89;
schemes FUNCT_1,CKB6,CKB7;
registrations XBOOLE_0,SUBSET_1,RELAT_1,FUNCT_1,RELSET_1,CKB64,CKB65;
constructors BINOP_1,RELSET_1,CKB9,CKB24,CKB25,CKB41,CKB47,CKB49,CKB52,CKB53,CKB58,CKB59,CKB60,CKB62,CKB63,CKB77,CKB84;
requirements SUBSET,BOOLE;
begin
reserve x for set;
theorem
(for f,g,h,k being Function holds ( [: f,h :] * <: g,k :> ) = <: ( f * g ),( h * k ) :>)
proof
let f being Function;
let g being Function;
let h being Function;
let k being Function;
(x in ( dom ( [: f,h :] * <: g,k :> ) ) iff x in ( dom <: ( f * g ),( h * k ) :> ))
proof
thus (x in ( dom ( [: f,h :] * <: g,k :> ) ) implies x in ( dom <: ( f * g ),( h * k ) :> ))
proof
assume A1: x in ( dom ( [: f,h :] * <: g,k :> ) );
then A2: x in ( dom <: g,k :> ) by FUNCT_1:11;
then A3: x in ( ( dom g ) /\ ( dom k ) ) by CKB63:def 1;
then A4: x in ( dom g ) by XBOOLE_0:def 4;
A5: x in ( dom k ) by A3,XBOOLE_0:def 4;
( <: g,k :> . x ) in ( dom [: f,h :] ) by A1,FUNCT_1:11;
then [ ( g . x ),( k . x ) ] in ( dom [: f,h :] ) by A2,CKB63:def 1;
then A6: [ ( g . x ),( k . x ) ] in [: ( dom f ),( dom h ) :] by CKB84:def 1;
then ( k . x ) in ( dom h ) by ZFMISC_1:87;
then A7: x in ( dom ( h * k ) ) by A5,FUNCT_1:11;
( g . x ) in ( dom f ) by A6,ZFMISC_1:87;
then x in ( dom ( f * g ) ) by A4,FUNCT_1:11;
then x in ( ( dom ( f * g ) ) /\ ( dom ( h * k ) ) ) by A7,XBOOLE_0:def 4;
hence thesis by CKB63:def 1;
end;

assume x in ( dom <: ( f * g ),( h * k ) :> );
then A8: x in ( ( dom ( f * g ) ) /\ ( dom ( h * k ) ) ) by CKB63:def 1;
then A9: x in ( dom ( f * g ) ) by XBOOLE_0:def 4;
A10: x in ( dom ( h * k ) ) by A8,XBOOLE_0:def 4;
then A11: x in ( dom k ) by FUNCT_1:11;
x in ( dom g ) by A9,FUNCT_1:11;
then x in ( ( dom g ) /\ ( dom k ) ) by A11,XBOOLE_0:def 4;
then A12: x in ( dom <: g,k :> ) by CKB63:def 1;
A13: ( k . x ) in ( dom h ) by A10,FUNCT_1:11;
( g . x ) in ( dom f ) by A9,FUNCT_1:11;
then [ ( g . x ),( k . x ) ] in [: ( dom f ),( dom h ) :] by A13,ZFMISC_1:87;
then [ ( g . x ),( k . x ) ] in ( dom [: f,h :] ) by CKB84:def 1;
then ( <: g,k :> . x ) in ( dom [: f,h :] ) by A12,CKB63:def 1;
hence thesis by A12,FUNCT_1:11;
end;
then A14: ( dom ( [: f,h :] * <: g,k :> ) ) = ( dom <: ( f * g ),( h * k ) :> ) by TARSKI:1;
(for x holds (x in ( dom ( [: f,h :] * <: g,k :> ) ) implies ( ( [: f,h :] * <: g,k :> ) . x ) = ( <: ( f * g ),( h * k ) :> . x )))
proof
let x;
assume A15: x in ( dom ( [: f,h :] * <: g,k :> ) );
then A16: x in ( dom <: g,k :> ) by FUNCT_1:11;
then A17: x in ( ( dom g ) /\ ( dom k ) ) by CKB63:def 1;
then A18: x in ( dom g ) by XBOOLE_0:def 4;
( <: g,k :> . x ) in ( dom [: f,h :] ) by A15,FUNCT_1:11;
then [ ( g . x ),( k . x ) ] in ( dom [: f,h :] ) by A16,CKB63:def 1;
then A19: [ ( g . x ),( k . x ) ] in [: ( dom f ),( dom h ) :] by CKB84:def 1;
then A20: ( g . x ) in ( dom f ) by ZFMISC_1:87;
A21: x in ( dom k ) by A17,XBOOLE_0:def 4;
A22: ( k . x ) in ( dom h ) by A19,ZFMISC_1:87;
then A23: x in ( dom ( h * k ) ) by A21,FUNCT_1:11;
x in ( dom ( f * g ) ) by A20,A18,FUNCT_1:11;
then A24: x in ( ( dom ( f * g ) ) /\ ( dom ( h * k ) ) ) by A23,XBOOLE_0:def 4;
thus ( ( [: f,h :] * <: g,k :> ) . x ) = ( [: f,h :] . ( <: g,k :> . x ) ) by A15,FUNCT_1:12
.= ( [: f,h :] . (( g . x ),( k . x )) ) by A16,CKB63:def 1
.= [ ( f . ( g . x ) ),( h . ( k . x ) ) ] by A20,A22,CKB84:def 1
.= [ ( ( f * g ) . x ),( h . ( k . x ) ) ] by A18,FUNCT_1:13
.= [ ( ( f * g ) . x ),( ( h * k ) . x ) ] by A21,FUNCT_1:13
.= ( <: ( f * g ),( h * k ) :> . x ) by A24,CKB66:1;
end;
hence thesis by A14,FUNCT_1:2;
end;
