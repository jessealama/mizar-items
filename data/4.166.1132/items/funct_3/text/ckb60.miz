begin
reserve x for set;
reserve X for set;
definition
let X;
func delta X -> Function
means
:Def7: (( dom it ) = X & (for x holds (x in X implies ( it . x ) = [ x,x ])));
existence
proof
deffunc F(set) = [ $1,$1 ];
(ex f being Function st (( dom f ) = X & (for x holds (x in X implies ( f . x ) = F(x))))) from FUNCT_1:sch 3;
hence thesis;
end;
uniqueness
proof
let f1 being Function,f2 being Function;
assume that
A1: ( dom f1 ) = X
and
A2: (for x holds (x in X implies ( f1 . x ) = [ x,x ]))
and
A3: ( dom f2 ) = X
and
A4: (for x holds (x in X implies ( f2 . x ) = [ x,x ]));
(for x holds (x in X implies ( f1 . x ) = ( f2 . x )))
proof
let x;
assume A5: x in X;
then ( f1 . x ) = [ x,x ] by A2;
hence thesis by A4,A5;
end;
hence thesis by A1,A3,FUNCT_1:2;
end;
end;
