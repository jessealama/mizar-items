begin
reserve x for set;
definition
let f being Function;
let g being Function;
func <:f,g :> -> Function
means
:Def8: (( dom it ) = ( ( dom f ) /\ ( dom g ) ) & (for x holds (x in ( dom it ) implies ( it . x ) = [ ( f . x ),( g . x ) ])));
existence
proof
deffunc F(set) = [ ( f . $1 ),( g . $1 ) ];
(ex fg being Function st (( dom fg ) = ( ( dom f ) /\ ( dom g ) ) & (for x holds (x in ( ( dom f ) /\ ( dom g ) ) implies ( fg . x ) = F(x))))) from FUNCT_1:sch 3;
hence thesis;
end;
uniqueness
proof
let f1 being Function,f2 being Function;
assume that
A1: ( dom f1 ) = ( ( dom f ) /\ ( dom g ) )
and
A2: (for x holds (x in ( dom f1 ) implies ( f1 . x ) = [ ( f . x ),( g . x ) ]))
and
A3: ( dom f2 ) = ( ( dom f ) /\ ( dom g ) )
and
A4: (for x holds (x in ( dom f2 ) implies ( f2 . x ) = [ ( f . x ),( g . x ) ]));
(for x holds (x in ( ( dom f ) /\ ( dom g ) ) implies ( f1 . x ) = ( f2 . x )))
proof
let x;
assume A5: x in ( ( dom f ) /\ ( dom g ) );
then ( f1 . x ) = [ ( f . x ),( g . x ) ] by A1,A2;
hence thesis by A3,A4,A5;
end;
hence thesis by A1,A3,FUNCT_1:2;
end;
end;
