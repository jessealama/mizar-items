begin
reserve q for set;
reserve x for set;
theorem
Th71: (for f,g being Function holds ( rng <: f,g :> ) c= [: ( rng f ),( rng g ) :])
proof
let f being Function,g being Function;
let q;
assume q in ( rng <: f,g :> );
then consider x such that A1: x in ( dom <: f,g :> ) and A2: q = ( <: f,g :> . x ) by FUNCT_1:def 3;
A3: x in ( ( dom f ) /\ ( dom g ) ) by A1,CKB63:def 1;
then x in ( dom f ) by XBOOLE_0:def 4;
then A4: ( f . x ) in ( rng f ) by FUNCT_1:def 3;
x in ( dom g ) by A3,XBOOLE_0:def 4;
then A5: ( g . x ) in ( rng g ) by FUNCT_1:def 3;
q = [ ( f . x ),( g . x ) ] by A1,A2,CKB63:def 1;
hence thesis by A4,A5,ZFMISC_1:87;
end;
