begin
reserve x for set;
reserve X for set;
theorem
Th89: (for f,g being Function holds ((( dom f ) = X & ( dom g ) = X) implies <: f,g :> = ( [: f,g :] * ( delta X ) )))
proof
let f being Function;
let g being Function;
assume that
A1: (( dom f ) = X & ( dom g ) = X);
A2: ( dom ( delta X ) ) = X by CKB60:def 1;
( rng ( delta X ) ) c= [: X,X :] by CKB61:1;
then ( rng ( delta X ) ) c= ( dom [: f,g :] ) by A1,CKB84:def 1;
then A3: ( dom ( [: f,g :] * ( delta X ) ) ) = X by A2,RELAT_1:27;
( ( dom f ) /\ ( dom g ) ) = X by A1;
then A4: ( dom <: f,g :> ) = X by CKB63:def 1;
(for x holds (x in X implies ( <: f,g :> . x ) = ( ( [: f,g :] * ( delta X ) ) . x )))
proof
let x;
assume A5: x in X;
hence ( <: f,g :> . x ) = [ ( f . x ),( g . x ) ] by A4,CKB63:def 1
.= ( [: f,g :] . (x,x) ) by A1,A5,CKB84:def 1
.= ( [: f,g :] . ( ( delta X ) . x ) ) by A5,CKB60:def 1
.= ( ( [: f,g :] * ( delta X ) ) . x ) by A3,A5,FUNCT_1:12;
end;
hence thesis by A4,A3,FUNCT_1:2;
end;
