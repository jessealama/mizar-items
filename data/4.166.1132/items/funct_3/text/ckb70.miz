requirements SUBSET,BOOLE;
begin
reserve x for set;
reserve Y for set;
reserve Z for set;
theorem
Th72: (for f,g being Function holds (((( dom f ) = ( dom g ) & ( rng f ) c= Y) & ( rng g ) c= Z) implies (( ( pr1 (Y,Z) ) * <: f,g :> ) = f & ( ( pr2 (Y,Z) ) * <: f,g :> ) = g)))
proof
let f being Function;
let g being Function;
assume that
A1: ( dom f ) = ( dom g )
and
A2: (( rng f ) c= Y & ( rng g ) c= Z);
A3: [: ( rng f ),( rng g ) :] c= [: Y,Z :] by A2,ZFMISC_1:96;
A4: ( rng <: f,g :> ) c= [: ( rng f ),( rng g ) :] by CKB69:1;
( dom ( pr1 (Y,Z) ) ) = [: Y,Z :] by CKB52:def 1;
then A5: ( dom ( ( pr1 (Y,Z) ) * <: f,g :> ) ) = ( dom <: f,g :> ) by A3,A4,RELAT_1:27,XBOOLE_1:1;
then A6: ( dom ( ( pr1 (Y,Z) ) * <: f,g :> ) ) = ( dom f ) by A1,CKB68:1;
(x in ( dom f ) implies ( ( ( pr1 (Y,Z) ) * <: f,g :> ) . x ) = ( f . x ))
proof
assume A7: x in ( dom f );
then A8: (( f . x ) in ( rng f ) & ( g . x ) in ( rng g )) by A1,FUNCT_1:def 3;
thus ( ( ( pr1 (Y,Z) ) * <: f,g :> ) . x ) = ( ( pr1 (Y,Z) ) . ( <: f,g :> . x ) ) by A6,A7,FUNCT_1:12
.= ( ( pr1 (Y,Z) ) . (( f . x ),( g . x )) ) by A5,A6,A7,CKB63:def 1
.= ( f . x ) by A2,A8,CKB52:def 1;
end;
hence ( ( pr1 (Y,Z) ) * <: f,g :> ) = f by A6,FUNCT_1:2;
( dom ( pr2 (Y,Z) ) ) = [: Y,Z :] by CKB53:def 1;
then A9: ( dom ( ( pr2 (Y,Z) ) * <: f,g :> ) ) = ( dom <: f,g :> ) by A3,A4,RELAT_1:27,XBOOLE_1:1;
then A10: ( dom ( ( pr2 (Y,Z) ) * <: f,g :> ) ) = ( dom g ) by A1,CKB68:1;
(x in ( dom g ) implies ( ( ( pr2 (Y,Z) ) * <: f,g :> ) . x ) = ( g . x ))
proof
assume A11: x in ( dom g );
then A12: (( f . x ) in ( rng f ) & ( g . x ) in ( rng g )) by A1,FUNCT_1:def 3;
thus ( ( ( pr2 (Y,Z) ) * <: f,g :> ) . x ) = ( ( pr2 (Y,Z) ) . ( <: f,g :> . x ) ) by A10,A11,FUNCT_1:12
.= ( ( pr2 (Y,Z) ) . (( f . x ),( g . x )) ) by A9,A10,A11,CKB63:def 1
.= ( g . x ) by A2,A12,CKB53:def 1;
end;
hence thesis by A10,FUNCT_1:2;
end;
