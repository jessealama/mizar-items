environ
vocabularies XBOOLE_0,TARSKI,RELAT_1,FUNCT_1,ZFMISC_1,SUBSET_1,MCART_1,PARTFUN1,FUNCT_2,FUNCT_3;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,RELAT_1,RELSET_1,FUNCT_1,FUNCT_2,BINOP_1,CKB9;
definitions TARSKI,BINOP_1,CKB9;
theorems TARSKI,ZFMISC_1,FUNCT_1,FUNCT_2,RELAT_1,RELSET_1,XBOOLE_0,XBOOLE_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16;
schemes FUNCT_1,CKB6,CKB7;
registrations XBOOLE_0,SUBSET_1,RELAT_1,FUNCT_1,RELSET_1;
constructors BINOP_1,RELSET_1,CKB9;
requirements SUBSET,BOOLE;
begin
reserve x for set;
reserve y for set;
reserve A for set;
reserve X for set;
theorem
Th16: (for f being Function holds (A c= ( bool ( dom f ) ) implies ( f .: ( union A ) ) = ( union ( ( .: f ) .: A ) )))
proof
let f being Function;
assume that
A1: A c= ( bool ( dom f ) );
A2: ( f .: ( union A ) ) c= ( union ( ( .: f ) .: A ) )
proof
let y;
assume y in ( f .: ( union A ) );
then consider x such that x in ( dom f ) and A3: x in ( union A ) and A4: y = ( f . x ) by FUNCT_1:def 6;
consider X such that A5: x in X and A6: X in A by A3,TARSKI:def 4;
X in ( bool ( dom f ) ) by A1,A6;
then X in ( dom ( .: f ) ) by CKB9:def 1;
then A7: ( ( .: f ) . X ) in ( ( .: f ) .: A ) by A6,FUNCT_1:def 6;
y in ( f .: X ) by A1,A4,A5,A6,FUNCT_1:def 6;
then y in ( ( .: f ) . X ) by A1,A6,CKB9:def 1;
hence thesis by A7,TARSKI:def 4;
end;
( union ( ( .: f ) .: A ) ) c= ( f .: ( union A ) ) by CKB16:1;
hence thesis by A2,XBOOLE_0:def 10;
end;
