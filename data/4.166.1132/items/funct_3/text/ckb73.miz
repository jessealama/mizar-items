environ
vocabularies XBOOLE_0,TARSKI,RELAT_1,FUNCT_1,ZFMISC_1,SUBSET_1,MCART_1,PARTFUN1,FUNCT_2,FUNCT_3;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,RELAT_1,RELSET_1,FUNCT_1,FUNCT_2,BINOP_1,CKB9,CKB24,CKB25,CKB41,CKB47,CKB48,CKB49,CKB52,CKB53,CKB58,CKB59,CKB60,CKB62,CKB63;
definitions TARSKI,BINOP_1,CKB9,CKB25,CKB41,CKB52,CKB53,CKB60,CKB63;
theorems TARSKI,ZFMISC_1,FUNCT_1,FUNCT_2,RELAT_1,RELSET_1,XBOOLE_0,XBOOLE_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB37,CKB38,CKB39,CKB40,CKB41,CKB42,CKB43,CKB44,CKB45,CKB46,CKB50,CKB51,CKB52,CKB53,CKB54,CKB55,CKB56,CKB57,CKB60,CKB61,CKB63,CKB66,CKB67,CKB68,CKB69,CKB70,CKB71,CKB72;
schemes FUNCT_1,CKB6,CKB7;
registrations XBOOLE_0,SUBSET_1,RELAT_1,FUNCT_1,RELSET_1,CKB64,CKB65;
constructors BINOP_1,RELSET_1,CKB9,CKB24,CKB25,CKB41,CKB47,CKB49,CKB52,CKB53,CKB58,CKB59,CKB60,CKB62,CKB63;
requirements SUBSET,BOOLE;
begin
reserve x for set;
theorem
(for f,g,h being Function holds <: ( f * h ),( g * h ) :> = ( <: f,g :> * h ))
proof
let f being Function,g being Function,h being Function;
(x in ( dom <: ( f * h ),( g * h ) :> ) iff x in ( dom ( <: f,g :> * h ) ))
proof
thus (x in ( dom <: ( f * h ),( g * h ) :> ) implies x in ( dom ( <: f,g :> * h ) ))
proof
assume x in ( dom <: ( f * h ),( g * h ) :> );
then A1: x in ( ( dom ( f * h ) ) /\ ( dom ( g * h ) ) ) by CKB63:def 1;
then A2: x in ( dom ( f * h ) ) by XBOOLE_0:def 4;
x in ( dom ( g * h ) ) by A1,XBOOLE_0:def 4;
then A3: ( h . x ) in ( dom g ) by FUNCT_1:11;
( h . x ) in ( dom f ) by A2,FUNCT_1:11;
then ( h . x ) in ( ( dom f ) /\ ( dom g ) ) by A3,XBOOLE_0:def 4;
then A4: ( h . x ) in ( dom <: f,g :> ) by CKB63:def 1;
x in ( dom h ) by A2,FUNCT_1:11;
hence thesis by A4,FUNCT_1:11;
end;

assume A5: x in ( dom ( <: f,g :> * h ) );
then A6: x in ( dom h ) by FUNCT_1:11;
( h . x ) in ( dom <: f,g :> ) by A5,FUNCT_1:11;
then A7: ( h . x ) in ( ( dom f ) /\ ( dom g ) ) by CKB63:def 1;
then ( h . x ) in ( dom g ) by XBOOLE_0:def 4;
then A8: x in ( dom ( g * h ) ) by A6,FUNCT_1:11;
( h . x ) in ( dom f ) by A7,XBOOLE_0:def 4;
then x in ( dom ( f * h ) ) by A6,FUNCT_1:11;
then x in ( ( dom ( f * h ) ) /\ ( dom ( g * h ) ) ) by A8,XBOOLE_0:def 4;
hence thesis by CKB63:def 1;
end;
then A9: ( dom <: ( f * h ),( g * h ) :> ) = ( dom ( <: f,g :> * h ) ) by TARSKI:1;
(for x holds (x in ( dom <: ( f * h ),( g * h ) :> ) implies ( <: ( f * h ),( g * h ) :> . x ) = ( ( <: f,g :> * h ) . x )))
proof
let x;
assume A10: x in ( dom <: ( f * h ),( g * h ) :> );
then A11: x in ( ( dom ( f * h ) ) /\ ( dom ( g * h ) ) ) by CKB63:def 1;
then A12: x in ( dom ( f * h ) ) by XBOOLE_0:def 4;
then A13: x in ( dom h ) by FUNCT_1:11;
A14: x in ( dom ( g * h ) ) by A11,XBOOLE_0:def 4;
then A15: ( h . x ) in ( dom g ) by FUNCT_1:11;
( h . x ) in ( dom f ) by A12,FUNCT_1:11;
then A16: ( h . x ) in ( ( dom f ) /\ ( dom g ) ) by A15,XBOOLE_0:def 4;
thus ( <: ( f * h ),( g * h ) :> . x ) = [ ( ( f * h ) . x ),( ( g * h ) . x ) ] by A10,CKB63:def 1
.= [ ( f . ( h . x ) ),( ( g * h ) . x ) ] by A12,FUNCT_1:12
.= [ ( f . ( h . x ) ),( g . ( h . x ) ) ] by A14,FUNCT_1:12
.= ( <: f,g :> . ( h . x ) ) by A16,CKB66:1
.= ( ( <: f,g :> * h ) . x ) by A13,FUNCT_1:13;
end;
hence thesis by A9,FUNCT_1:2;
end;
