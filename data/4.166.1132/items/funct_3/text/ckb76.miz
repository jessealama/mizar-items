theorem
Th78: (for f being (Function of X,Y) holds (for g being (Function of X,Z) holds (((Y = ( {} ) implies X = ( {} )) & (Z = ( {} ) implies X = ( {} ))) implies <: f,g :> is (Function of X,[: Y,Z :]))))
proof
let f being (Function of X,Y);
let g being (Function of X,Z);
assume A1: ((Y = ( {} ) implies X = ( {} )) & (Z = ( {} ) implies X = ( {} )));
per cases ;
suppose A2: ([: Y,Z :] = ( {} ) implies X = ( {} ));

(( rng f ) c= Y & ( rng g ) c= Z) by RELAT_1:def 19;
then A3: [: ( rng f ),( rng g ) :] c= [: Y,Z :] by ZFMISC_1:96;
(( dom f ) = X & ( dom g ) = X) by A1,FUNCT_2:def 1;
then A4: ( dom <: f,g :> ) = X by CKB68:1;
( rng <: f,g :> ) c= [: ( rng f ),( rng g ) :] by CKB69:1;
then ( rng <: f,g :> ) c= [: Y,Z :] by A3,XBOOLE_1:1;
hence thesis by A2,A4,FUNCT_2:def 1,RELSET_1:4;
end;
suppose ([: Y,Z :] = ( {} ) & X <> ( {} ));

hence thesis by A1;
end;
