requirements NUMERALS,SUBSET,BOOLE;
begin
reserve G for Group;
reserve A1 for (Subset of G);
reserve A2 for (Subset of G);
definition
let G;
let N being  normal (Subgroup of G);
func CosOp N -> (BinOp of ( Cosets N ))
means
:Def4: (for W1,W2 being (Element of ( Cosets N )) holds (for A1 holds (for A2 holds ((W1 = A1 & W2 = A2) implies ( it . (W1,W2) ) = ( A1 * A2 )))));
existence
proof
defpred P[ (Element of ( Cosets N )),(Element of ( Cosets N )),set ]
 means
(for A1 holds (for A2 holds (($1 = A1 & $2 = A2) implies $3 = ( A1 * A2 ))));
A1: (for W1,W2 being (Element of ( Cosets N )) holds (ex V being (Element of ( Cosets N )) st P[ W1,W2,V ]))
proof
let W1 being (Element of ( Cosets N )),W2 being (Element of ( Cosets N ));
reconsider A1 = W1,A2 = W2 as (Subset of G) by CKB26:1;
reconsider C = ( A1 * A2 ) as (Element of ( Cosets N )) by CKB27:1;
take C;
thus thesis;
end;
thus (ex B being (BinOp of ( Cosets N )) st (for W1,W2 being (Element of ( Cosets N )) holds P[ W1,W2,( B . (W1,W2) ) ])) from BINOP_1:sch 3(A1);
end;
uniqueness
proof
let o1 being (BinOp of ( Cosets N )),o2 being (BinOp of ( Cosets N ));
assume A2: (for W1,W2 being (Element of ( Cosets N )) holds (for A1 holds (for A2 holds ((W1 = A1 & W2 = A2) implies ( o1 . (W1,W2) ) = ( A1 * A2 )))));
assume A3: (for W1,W2 being (Element of ( Cosets N )) holds (for A1 holds (for A2 holds ((W1 = A1 & W2 = A2) implies ( o2 . (W1,W2) ) = ( A1 * A2 )))));
now
let x being set,y being set;
assume A4: (x in ( Cosets N ) & y in ( Cosets N ));
then reconsider W = x,V = y as (Element of ( Cosets N ));
reconsider A1 = x,A2 = y as (Subset of G) by A4;
( o1 . (W,V) ) = ( A1 * A2 ) by A2;
hence ( o1 . (x,y) ) = ( o2 . (x,y) ) by A3;
end;
hence thesis by BINOP_1:1;
end;
end;
