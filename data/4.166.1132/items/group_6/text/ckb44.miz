vocabularies XBOOLE_0,FUNCT_1,SUBSET_1,GROUP_1,GROUP_2,PRE_TOPC,RLSUB_1,NUMBERS,INT_1,STRUCT_0,RELAT_1,TARSKI,NEWTON,XXREAL_1,GROUP_4,GROUP_5,ALGSTR_0,NATTRA_1,ZFMISC_1,CARD_1,FINSET_1,BINOP_1,QC_LANG1,CQC_SIM1,REALSET1,MSSUBFAM,FUNCOP_1,ARYTM_3,XXREAL_0,COMPLEX1,FUNCT_2,WELLORD1,EQREL_1,GROUP_6;
