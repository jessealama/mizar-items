begin
reserve G for Group;
reserve a for (Element of G);
reserve a1 for (Element of G);
reserve b for (Element of G);
reserve b1 for (Element of G);
reserve x for set;
reserve y for set;
definition
let G;
let N being  normal (Subgroup of G);
func nat_hom N -> (Homomorphism of G,( G ./. N ))
means
:Def9: (for a holds ( it . a ) = ( a * N ));
existence
proof
defpred P[ set,set ]
 means
(ex a st ($1 = a & $2 = ( a * N )));
A1: (for x holds (x in (the carrier of G) implies (ex y st P[ x,y ])))
proof
let x;
assume x in (the carrier of G);
then reconsider a = x as (Element of G);
reconsider y = ( a * N ) as set;
take y;
take a;
thus thesis;
end;
consider f being Function such that A2: ( dom f ) = (the carrier of G) and A3: (for x holds (x in (the carrier of G) implies P[ x,( f . x ) ])) from CLASSES1:sch 1(A1);
( rng f ) c= (the carrier of ( G ./. N ))
proof
let x;
assume x in ( rng f );
then consider y such that A4: y in ( dom f ) and A5: ( f . y ) = x by FUNCT_1:def 3;
consider a such that y = a and A6: ( f . y ) = ( a * N ) by A2,A3,A4;
( a * N ) = ( N * a ) by GROUP_3:117;
then x in ( G ./. N ) by A5,A6,CKB39:1;
hence thesis by STRUCT_0:def 5;
end;
then reconsider f as (Function of G,( G ./. N )) by A2,FUNCT_2:def 1,RELSET_1:4;
now
let a;
let b;
consider a1 such that A7: a = a1 and A8: ( f . a ) = ( a1 * N ) by A3;
consider b1 such that A9: b = b1 and A10: ( f . b ) = ( b1 * N ) by A3;
A11: (ex c being (Element of G) st (c = ( a * b ) & ( f . ( a * b ) ) = ( c * N ))) by A3;
thus ( ( f . a ) * ( f . b ) ) = ( ( @ ( f . a ) ) * ( @ ( f . b ) ) ) by CKB28:def 1
.= ( ( ( a1 * N ) * b1 ) * N ) by A8,A10,GROUP_3:9
.= ( ( a1 * ( N * b1 ) ) * N ) by GROUP_2:106
.= ( ( a1 * ( b1 * N ) ) * N ) by GROUP_3:117
.= ( a1 * ( ( b1 * N ) * N ) ) by GROUP_2:96
.= ( a1 * ( b1 * N ) ) by CKB8:1
.= ( f . ( a * b ) ) by A7,A9,A11,GROUP_2:105;
end;
then reconsider f as (Homomorphism of G,( G ./. N )) by CKB48:def 1;
take f;
let a;
(ex b st (a = b & ( f . a ) = ( b * N ))) by A3;
hence thesis;
end;
uniqueness
proof
let n1 being (Homomorphism of G,( G ./. N )),n2 being (Homomorphism of G,( G ./. N ));
assume that
A12: (for a holds ( n1 . a ) = ( a * N ))
and
A13: (for a holds ( n2 . a ) = ( a * N ));
now
let a;
( n1 . a ) = ( a * N ) by A12;
hence ( n1 . a ) = ( n2 . a ) by A13;
end;
hence thesis by FUNCT_2:63;
end;
end;
