environ
vocabularies REAL_1,FUNCT_1,NUMBERS,DIFF_1,ARYTM_3,CARD_1,NAT_1,RELAT_1,SQUARE_1,ZFMISC_1,ARYTM_1,VALUED_1,VALUED_0,XREAL_0,ORDINAL1,JGRAPH_2,SIN_COS,SIN_COS4;
notations SQUARE_1,ORDINAL1,XCMPLX_0,ZFMISC_1,REAL_1,NAT_1,NUMBERS,FUNCT_1,FUNCT_2,VALUED_1,SEQFUNC,RELAT_1,SIN_COS,DIFF_1,XREAL_0,VALUED_0,FCONT_1,SIN_COS4,FDIFF_9,CKB11,CKB15;
definitions DIFF_1,SIN_COS4,SQUARE_1,SIN_COS,FDIFF_9,CKB11,CKB15;
theorems DIFF_1,XCMPLX_1,SIN_COS4,ZFMISC_1,RFUNCT_1,FCONT_1,FDIFF_8,VALUED_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31;
registrations XREAL_0,RELSET_1,MEMBERED,FUNCT_2,NUMBERS,VALUED_0,VALUED_1;
constructors SQUARE_1,REAL_1,DIFF_1,PARTFUN3,SIN_COS,SIN_COS4,FCONT_1,SEQ_1,FDIFF_9,SEQFUNC,RELSET_1,VALUED_1,CKB11,CKB15;
requirements NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve x0 for Real;
reserve x1 for Real;
reserve x2 for Real;
reserve x3 for Real;
reserve x4 for Real;
reserve x for Real;
reserve a for Real;
reserve b for Real;
reserve c for Real;
reserve f for (Function of ( REAL ),( REAL ));
theorem
(((for x holds ( f . x ) = ( ( ( a * ( x ^2 ) ) + ( b * x ) ) + c )) & x0,x1,x2,x3,x4 are_mutually_different ) implies [! f,x0,x1,x2,x3,x4 !] = ( 0 ))
proof
assume A1: (for x holds ( f . x ) = ( ( ( a * ( x ^2 ) ) + ( b * x ) ) + c ));
assume A2: x0,x1,x2,x3,x4 are_mutually_different ;
then A3: (x1 <> x2 & x1 <> x3) by ZFMISC_1:def 7;
A4: x0 <> x3 by A2,ZFMISC_1:def 7;
A5: x2 <> x3 by A2,ZFMISC_1:def 7;
A6: x3 <> x4 by A2,ZFMISC_1:def 7;
(x1 <> x4 & x2 <> x4) by A2,ZFMISC_1:def 7;
then A7: x1,x2,x3,x4 are_mutually_different  by A3,A5,A6,ZFMISC_1:def 6;
(x0 <> x1 & x0 <> x2) by A2,ZFMISC_1:def 7;
then x0,x1,x2,x3 are_mutually_different  by A4,A3,A5,ZFMISC_1:def 6;
then [! f,x0,x1,x2,x3,x4 !] = ( ( (( 0 ) qua Nat) - [! f,x1,x2,x3,x4 !] ) / ( x0 - x4 ) ) by A1,CKB31:1
.= ( ( (( 0 ) qua Nat) - (( 0 ) qua Nat) ) / ( x0 - x4 ) ) by A1,A7,CKB31:1;
hence thesis;
end;
