environ
vocabularies REAL_1,FUNCT_1,NUMBERS,DIFF_1,ARYTM_3,CARD_1,NAT_1,RELAT_1,SQUARE_1,ZFMISC_1,ARYTM_1,VALUED_1,VALUED_0,XREAL_0,ORDINAL1,JGRAPH_2,SIN_COS,SIN_COS4;
notations SQUARE_1,ORDINAL1,XCMPLX_0,ZFMISC_1,REAL_1,NAT_1,NUMBERS,FUNCT_1,FUNCT_2,VALUED_1,SEQFUNC,RELAT_1,SIN_COS,DIFF_1,XREAL_0,VALUED_0,FCONT_1,SIN_COS4,FDIFF_9,CKB11,CKB15;
definitions DIFF_1,SIN_COS4,SQUARE_1,SIN_COS,FDIFF_9,CKB11,CKB15;
theorems DIFF_1,XCMPLX_1,SIN_COS4,ZFMISC_1,RFUNCT_1,FCONT_1,FDIFF_8,VALUED_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17;
registrations XREAL_0,RELSET_1,MEMBERED,FUNCT_2,NUMBERS,VALUED_0,VALUED_1;
constructors SQUARE_1,REAL_1,DIFF_1,PARTFUN3,SIN_COS,SIN_COS4,FCONT_1,SEQ_1,FDIFF_9,SEQFUNC,RELSET_1,VALUED_1,CKB11,CKB15;
requirements NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve r1 for Real;
reserve r2 for Real;
reserve x0 for Real;
reserve x1 for Real;
reserve x2 for Real;
reserve x3 for Real;
reserve x4 for Real;
reserve x5 for Real;
reserve f1 for (Function of ( REAL ),( REAL ));
reserve f2 for (Function of ( REAL ),( REAL ));
theorem
[! ( ( r1 (#) f1 ) + ( r2 (#) f2 ) ),x0,x1,x2,x3,x4,x5 !] = ( ( r1 * [! f1,x0,x1,x2,x3,x4,x5 !] ) + ( r2 * [! f2,x0,x1,x2,x3,x4,x5 !] ) )
proof
[! ( ( r1 (#) f1 ) + ( r2 (#) f2 ) ),x0,x1,x2,x3,x4,x5 !] = ( [! ( r1 (#) f1 ),x0,x1,x2,x3,x4,x5 !] + [! ( r2 (#) f2 ),x0,x1,x2,x3,x4,x5 !] ) by CKB17:1
.= ( ( r1 * [! f1,x0,x1,x2,x3,x4,x5 !] ) + [! ( r2 (#) f2 ),x0,x1,x2,x3,x4,x5 !] ) by CKB16:1;
hence thesis by CKB16:1;
end;
