theorem
Th12: (( proj1 [: X,Y :] ) c= X & ( proj2 [: X,Y :] ) c= Y)
proof
thus ( proj1 [: X,Y :] ) c= X
proof
let x;
assume x in ( proj1 [: X,Y :] );
then (ex y st [ x,y ] in [: X,Y :]) by RELAT_1:def 4;
hence thesis by ZFMISC_1:87;
end;

let y;
assume y in ( proj2 [: X,Y :] );
