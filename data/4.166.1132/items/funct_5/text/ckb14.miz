begin
reserve x for set;
reserve y for set;
reserve z for set;
reserve t for set;
theorem
(( proj1 { [ x,y ],[ z,t ] } ) = { x,z } & ( proj2 { [ x,y ],[ z,t ] } ) = { y,t })
proof
A1: (( proj2 { [ x,y ] } ) = { y } & ( proj2 { [ z,t ] } ) = { t }) by CKB13:1;
{ [ x,y ],[ z,t ] } = ( { [ x,y ] } \/ { [ z,t ] } ) by ENUMSET1:1;
then A2: (( proj1 { [ x,y ],[ z,t ] } ) = ( ( proj1 { [ x,y ] } ) \/ ( proj1 { [ z,t ] } ) ) & ( proj2 { [ x,y ],[ z,t ] } ) = ( ( proj2 { [ x,y ] } ) \/ ( proj2 { [ z,t ] } ) )) by CKB5:1;
(( proj1 { [ x,y ] } ) = { x } & ( proj1 { [ z,t ] } ) = { z }) by CKB13:1;
hence thesis by A2,A1,ENUMSET1:1;
end;
