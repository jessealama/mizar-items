begin
reserve X for set;
reserve Y for set;
reserve f for Function;
theorem
(( dom f ) = [: X,Y :] implies (( uncurry ( curry f ) ) = f & ( uncurry' ( curry' f ) ) = f))
proof
assume A1: ( dom f ) = [: X,Y :];
A2:now
A3: ( rng ( curry' f ) ) c= ( Funcs (X,( rng f )) ) by A1,CKB40:1;
assume A4: ( dom f ) <> ( {} );
then X <> ( {} ) by A1,ZFMISC_1:90;
then A5: ( curry' ( uncurry' ( curry' f ) ) ) = ( curry' f ) by A3,CKB53:1;
( dom ( curry' f ) ) = Y by A1,A4,CKB29:1;
then A6: ( dom ( uncurry' ( curry' f ) ) ) = [: X,Y :] by A3,CKB31:1;
A7: ( rng ( curry f ) ) c= ( Funcs (Y,( rng f )) ) by A1,CKB40:1;
Y <> ( {} ) by A1,A4,ZFMISC_1:90;
then A8: ( curry ( uncurry ( curry f ) ) ) = ( curry f ) by A7,CKB53:1;
( dom ( curry f ) ) = X by A1,A4,CKB29:1;
then ( dom ( uncurry ( curry f ) ) ) = [: X,Y :] by A7,CKB31:1;
hence thesis by A1,A8,A5,A6,CKB49:1,CKB50:1;
end;
now
assume ( dom f ) = ( {} );
then f = ( {} );
hence thesis by CKB47:1,CKB48:1;
end;
hence thesis by A2;
end;
