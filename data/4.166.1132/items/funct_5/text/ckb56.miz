environ
vocabularies FUNCT_1,RELAT_1,XBOOLE_0,ZFMISC_1,TARSKI,SUBSET_1,MCART_1,FUNCT_2,PARTFUN1,CARD_1,FUNCOP_1,FUNCT_4,BINOP_1,FUNCT_5;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,RELAT_1,FUNCT_1,WELLORD2,FUNCT_2,MCART_1,BINOP_1,FUNCT_3,CARD_1,PARTFUN1,FUNCOP_1,FUNCT_4,CKB20,CKB21,CKB22,CKB23;
definitions TARSKI,FUNCT_1,WELLORD2,XBOOLE_0,BINOP_1,FUNCOP_1,CKB20,CKB21,CKB22,CKB23;
theorems TARSKI,ZFMISC_1,FUNCT_1,WELLORD2,FUNCT_2,MCART_1,FUNCT_3,RELAT_1,CARD_1,PARTFUN1,FUNCOP_1,FUNCT_4,ENUMSET1,XBOOLE_0,XBOOLE_1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB37,CKB38,CKB39,CKB40,CKB41,CKB42,CKB43,CKB44,CKB45,CKB46,CKB47,CKB48,CKB49,CKB50,CKB51,CKB52,CKB53,CKB54,CKB55;
schemes FUNCT_1,PARTFUN1,XBOOLE_0,CKB1;
registrations XBOOLE_0,SUBSET_1,RELAT_1,FUNCT_1,RELSET_1;
constructors PARTFUN1,WELLORD2,BINOP_1,FUNCT_3,FUNCOP_1,FUNCT_4,CARD_1,RELSET_1,CKB20,CKB21,CKB22,CKB23;
requirements SUBSET,BOOLE;
begin
reserve X for set;
reserve Y for set;
reserve x for set;
reserve y for set;
reserve z for set;
reserve t for set;
reserve x1 for set;
reserve x2 for set;
reserve f for Function;
reserve g for Function;
reserve g1 for Function;
theorem
Th58: ((( rng f ) c= ( PFuncs (X,Y) ) & (not ( {} ) in ( rng f ))) implies (( curry ( uncurry f ) ) = f & ( curry' ( uncurry' f ) ) = f))
proof
assume that
A1: ( rng f ) c= ( PFuncs (X,Y) )
and
A2: (not ( {} ) in ( rng f ));
A3: ( dom ( curry ( uncurry f ) ) ) = ( dom f )
proof
( dom ( uncurry f ) ) c= [: ( dom f ),X :] by A1,CKB42:1;
hence ( dom ( curry ( uncurry f ) ) ) c= ( dom f ) by CKB30:1;
let x;
assume A4: x in ( dom f );
then ( f . x ) in ( rng f ) by FUNCT_1:def 3;
then consider g such that A5: ( f . x ) = g and ( dom g ) c= X and ( rng g ) c= Y by A1,PARTFUN1:def 3;
set y = the (Element of ( dom g ));
g <> ( {} ) by A2,A4,A5,FUNCT_1:def 3;
then A6: [ x,y ] in ( dom ( uncurry f ) ) by A4,A5,CKB43:1;
( dom ( curry ( uncurry f ) ) ) = ( proj1 ( dom ( uncurry f ) ) ) by CKB20:def 1;
hence thesis by A6,RELAT_1:def 4;
end;
now
let x;
assume A7: x in ( dom f );
then reconsider h = ( ( curry ( uncurry f ) ) . x ) as Function by A3,CKB35:1;
( f . x ) in ( rng f ) by A7,FUNCT_1:def 3;
then consider g such that A8: ( f . x ) = g and ( dom g ) c= X and ( rng g ) c= Y by A1,PARTFUN1:def 3;
A9: ( dom h ) = ( proj2 ( ( dom ( uncurry f ) ) /\ [: { x },( proj2 ( dom ( uncurry f ) ) ) :] ) ) by A3,A7,CKB36:1;
A10: ( dom h ) = ( dom g )
proof
thus ( dom h ) c= ( dom g )
proof
let z;
assume z in ( dom h );
then consider t such that A11: [ t,z ] in ( ( dom ( uncurry f ) ) /\ [: { x },( proj2 ( dom ( uncurry f ) ) ) :] ) by A9,RELAT_1:def 5;
[ t,z ] in [: { x },( proj2 ( dom ( uncurry f ) ) ) :] by A11,XBOOLE_0:def 4;
then A12: t = x by ZFMISC_1:105;
[ t,z ] in ( dom ( uncurry f ) ) by A11,XBOOLE_0:def 4;
then consider x1,g1,x2 such that A13: [ t,z ] = [ x1,x2 ] and x1 in ( dom f ) and A14: (g1 = ( f . x1 ) & x2 in ( dom g1 )) by CKB21:def 1;
t = x1 by A13,ZFMISC_1:27;
hence thesis by A8,A13,A14,A12,ZFMISC_1:27;
end;

let y;
assume y in ( dom g );
then [ x,y ] in ( dom ( uncurry f ) ) by A7,A8,CKB43:1;
hence thesis by CKB25:1;
end;
now
let y;
assume A15: y in ( dom h );
hence ( h . y ) = ( ( uncurry f ) . (x,y) ) by A3,A7,CKB36:1
.= ( g . y ) by A7,A8,A10,A15,CKB43:1;
end;
hence ( f . x ) = ( ( curry ( uncurry f ) ) . x ) by A8,A10,FUNCT_1:2;
end;
hence A16: ( curry ( uncurry f ) ) = f by A3,FUNCT_1:2;
( dom ( uncurry f ) ) c= [: ( dom f ),X :] by A1,CKB42:1;
hence thesis by A16,FUNCT_4:52;
end;
