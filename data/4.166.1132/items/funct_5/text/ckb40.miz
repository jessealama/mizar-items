reserve g for Function;
theorem
Th42: (( dom f ) = [: X,Y :] implies (( rng ( curry f ) ) c= ( Funcs (Y,( rng f )) ) & ( rng ( curry' f ) ) c= ( Funcs (X,( rng f )) )))
proof
assume A1: ( dom f ) = [: X,Y :];
A2:now
assume A3: [: X,Y :] <> ( {} );
then A4: ( dom ( curry f ) ) = X by A1,CKB29:1;
thus ( rng ( curry f ) ) c= ( Funcs (Y,( rng f )) )
proof
let z;
assume z in ( rng ( curry f ) );
then consider x such that A5: x in ( dom ( curry f ) ) and A6: z = ( ( curry f ) . x ) by FUNCT_1:def 3;
(ex g st (((( ( curry f ) . x ) = g & ( dom g ) = Y) & ( rng g ) c= ( rng f )) & (for y holds (y in Y implies ( g . y ) = ( f . (x,y) ))))) by A1,A3,A4,A5,CKB34:1;
hence thesis by A6,FUNCT_2:def 2;
end;

A7: ( dom ( curry' f ) ) = Y by A1,A3,CKB29:1;
thus ( rng ( curry' f ) ) c= ( Funcs (X,( rng f )) )
proof
let z;
assume z in ( rng ( curry' f ) );
then consider y such that A8: y in ( dom ( curry' f ) ) and A9: z = ( ( curry' f ) . y ) by FUNCT_1:def 3;
(ex g st (((( ( curry' f ) . y ) = g & ( dom g ) = X) & ( rng g ) c= ( rng f )) & (for x holds (x in X implies ( g . x ) = ( f . (x,y) ))))) by A1,A3,A7,A8,CKB37:1;
hence thesis by A9,FUNCT_2:def 2;
end;

end;
now
assume A10: ( dom f ) = ( {} );
then (X = ( {} ) or Y = ( {} )) by A1;
then A11: [: Y,X :] = ( {} ) by ZFMISC_1:90;
( {} ) = ( dom ( curry f ) ) by A10,CKB20:def 1,CKB9:1;
then A12: ( rng ( curry f ) ) = ( {} ) by RELAT_1:42;
(( dom ( ~ f ) ) = [: Y,X :] & ( dom ( curry ( ~ f ) ) ) = ( proj1 ( dom ( ~ f ) ) )) by A1,CKB20:def 1,FUNCT_4:46;
then ( rng ( curry' f ) ) = ( {} ) by A11,CKB9:1,RELAT_1:42;
hence thesis by A12,XBOOLE_1:2;
end;
hence thesis by A1,A2;
end;
