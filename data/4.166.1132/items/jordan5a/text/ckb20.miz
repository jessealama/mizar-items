begin
theorem
Lm6: (for a,b,c being  real number holds (a <= b implies (c in (the carrier of ( Closed-Interval-TSpace (a,b) )) iff (a <= c & c <= b))))
proof
let a being  real number;
let b being  real number;
let c being  real number;
A1: c is Real by XREAL_0:def 1;
assume a <= b;
then A2: (the carrier of ( Closed-Interval-TSpace (a,b) )) = [. a,b .] by TOPMETR:18;
hereby
assume c in (the carrier of ( Closed-Interval-TSpace (a,b) ));
then c in { l where l is Real: (a <= l & l <= b) } by A2,RCOMP_1:def 1;
then (ex c1 being Real st ((c1 = c & a <= c1) & c1 <= b));
hence (a <= c & c <= b);
end;
assume (a <= c & c <= b);
then c in { l where l is Real: (a <= l & l <= b) } by A1;
hence thesis by A2,RCOMP_1:def 1;
end;
