theorem
(for a,b being  real number holds (a <= b implies (( lower_bound [. a,b .] ) = a & ( upper_bound [. a,b .] ) = b)))
proof
let a being  real number,b being  real number;
assume A1: a <= b;
set X = [. a,b .];
a is Real by XREAL_0:def 1;
then A2: a in { l where l is Real: (a <= l & l <= b) } by A1;
A3: (for s being  real number holds (( 0 ) < s implies (ex r being  real number st (r in X & r < ( a + s )))))
proof
let s being  real number;
assume A4: ( 0 ) < s;
A5: a in X by A2,RCOMP_1:def 1;
assume (for r being  real number holds (r in X implies r >= ( a + s )));
hence thesis by A4,A5,XREAL_1:29;
end;
b is Real by XREAL_0:def 1;
then A6: b in { l1 where l1 is Real: (a <= l1 & l1 <= b) } by A1;
A7: (for s being  real number holds (( 0 ) < s implies (ex r being  real number st (r in X & ( b - s ) < r))))
proof
let s being  real number;
assume A8: ( 0 ) < s;
A9: b in X by A6,RCOMP_1:def 1;
assume (for r being  real number holds (r in X implies r <= ( b - s )));
hence thesis by A8,A9,XREAL_1:44;
end;
A10: (for r being  real number holds (r in X implies a <= r))
proof
let r being  real number;
assume r in X;
then r in { l where l is Real: (a <= l & l <= b) } by RCOMP_1:def 1;
then (ex r1 being Real st ((r1 = r & a <= r1) & r1 <= b));
hence thesis;
end;
b is (UpperBound of X)
proof
let r being  ext-real number;
assume r in X;
then r in { l where l is Real: (a <= l & l <= b) } by RCOMP_1:def 1;
then (ex r1 being Real st ((r1 = r & a <= r1) & r1 <= b));
hence thesis;
end;
then A11: X is  bounded_above by XXREAL_2:def 10;
a is (LowerBound of X)
proof
let r being  ext-real number;
assume r in X;
then r in { l where l is Real: (a <= l & l <= b) } by RCOMP_1:def 1;
then (ex r1 being Real st ((r1 = r & a <= r1) & r1 <= b));
hence thesis;
end;
then A12: X is  bounded_below by XXREAL_2:def 9;
A13: (for r being  real number holds (r in X implies b >= r))
proof
let r being  real number;
assume r in X;
then r in { l where l is Real: (a <= l & l <= b) } by RCOMP_1:def 1;
then (ex r1 being Real st ((r1 = r & a <= r1) & r1 <= b));
hence thesis;
end;
a in X by A2,RCOMP_1:def 1;
hence thesis by A12,A11,A10,A3,A13,A7,SEQ_4:def 1,SEQ_4:def 2;
end;
