environ
vocabularies XBOOLE_0,SUBSET_1,FUNCT_2,MARGREL1,FUNCT_1,XBOOLEAN,RELAT_1,TARSKI,BVFUNC_1;
notations TARSKI,XBOOLE_0,SUBSET_1,XBOOLEAN,MARGREL1,RELAT_1,FUNCT_1,FUNCT_2,BVFUNC_1,CKB1,CKB2;
definitions TARSKI,MARGREL1,CKB1,CKB2;
theorems FUNCT_1,FUNCT_2,MARGREL1,BVFUNC_1,BVFUNC_4,BVFUNC_8,BVFUNC25,XBOOLEAN,CKB1,CKB2;
schemes FUNCT_1;
registrations FUNCT_1,FUNCT_2,XBOOLEAN,MARGREL1,XBOOLE_0,CKB3;
constructors BVFUNC_1,RELSET_1,CKB1,CKB2;
requirements SUBSET,BOOLE;
begin
registration
let p being  boolean-valued Function;
let q being  boolean-valued Function;
cluster ( p 'nor' q ) ->  boolean-valued;
coherence
proof
let x being set;
assume x in ( rng ( p 'nor' q ) );
then consider y being set such that A2: (y in ( dom ( p 'nor' q ) ) & x = ( ( p 'nor' q ) . y )) by FUNCT_1:def 3;
x = ( ( p . y ) 'nor' ( q . y ) ) by A2,CKB2:def 1;
then (x = ( FALSE ) or x = ( TRUE )) by XBOOLEAN:def 3;
hence thesis;
end;
end;
