environ
vocabularies XBOOLE_0,SUBSET_1,FUNCT_2,MARGREL1,FUNCT_1,XBOOLEAN,RELAT_1,TARSKI,BVFUNC_1;
notations TARSKI,XBOOLE_0,SUBSET_1,XBOOLEAN,MARGREL1,RELAT_1,FUNCT_1,FUNCT_2,BVFUNC_1,CKB1,CKB2,CKB5;
definitions TARSKI,MARGREL1,CKB1,CKB2,CKB5;
theorems FUNCT_1,FUNCT_2,MARGREL1,BVFUNC_1,BVFUNC_4,BVFUNC_8,BVFUNC25,XBOOLEAN,CKB1,CKB2,CKB5;
schemes FUNCT_1;
registrations FUNCT_1,FUNCT_2,XBOOLEAN,MARGREL1,XBOOLE_0,CKB3,CKB4;
constructors BVFUNC_1,RELSET_1,CKB1,CKB2,CKB5;
requirements SUBSET,BOOLE;
begin
definition
let A being non  empty set;
let p being (Element of ( Funcs (A,( BOOLEAN )) ));
let q being (Element of ( Funcs (A,( BOOLEAN )) ));
redefine func p 'nor' q -> (Element of ( Funcs (A,( BOOLEAN )) )) means 
:Def4: (for x being (Element of A) holds ( it . x ) = ( ( p . x ) 'nor' ( q . x ) ));
coherence
proof
((ex f being Function st ((p = f & ( dom f ) = A) & ( rng f ) c= ( BOOLEAN ))) & (ex f being Function st ((q = f & ( dom f ) = A) & ( rng f ) c= ( BOOLEAN )))) by FUNCT_2:def 2;
then A6: ( dom ( p 'nor' q ) ) = ( A /\ A ) by CKB2:def 1
.= A;
( rng ( p 'nor' q ) ) c= ( BOOLEAN ) by MARGREL1:def 16;
hence thesis by A6,FUNCT_2:def 2;
end;
compatibility
proof
let IT being (Element of ( Funcs (A,( BOOLEAN )) ));
A7: ( dom IT ) = A by FUNCT_2:def 1;
hereby
assume A8: IT = ( p 'nor' q );
let x being (Element of A);
(( dom p ) = A & ( dom q ) = A) by FUNCT_2:def 1;
then ( dom ( p 'nor' q ) ) = ( A /\ A ) by CKB2:def 1
.= A;
hence ( IT . x ) = ( ( p . x ) 'nor' ( q . x ) ) by A8,CKB2:def 1;
end;
A9: ( dom q ) = A by FUNCT_2:def 1;
A10: ( dom IT ) = ( A /\ A ) by FUNCT_2:def 1
.= ( ( dom p ) /\ ( dom q ) ) by A9,FUNCT_2:def 1;
assume (for x being (Element of A) holds ( IT . x ) = ( ( p . x ) 'nor' ( q . x ) ));
then (for x being set holds (x in ( dom IT ) implies ( IT . x ) = ( ( p . x ) 'nor' ( q . x ) ))) by A7;
hence thesis by A10,CKB2:def 1;
end;
end;
