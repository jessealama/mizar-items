environ
vocabularies NUMBERS,SUBSET_1,INT_1,NAT_1,ARYTM_3,INT_2,CARD_1,XXREAL_0,ORDINAL1,SQUARE_1,ABIAN,RELAT_1,ARYTM_1,FINSET_1,FUNCT_1,XBOOLE_0,COMPLEX1,TARSKI,PYTHTRIP;
notations TARSKI,XBOOLE_0,ENUMSET1,SUBSET_1,FINSET_1,NUMBERS,XCMPLX_0,XXREAL_0,INT_1,INT_2,NAT_1,NAT_D,SQUARE_1,ABIAN,PEPIN,DOMAIN_1,RELAT_1,FUNCT_1,LIMFUNC1,ORDINAL1;
definitions INT_1,INT_2,SQUARE_1,RELAT_1,ABIAN;
theorems SQUARE_1,NAT_1,INT_2,WSIERP_1,EULER_2,ABIAN,EULER_1,PEPIN,INT_1,ENUMSET1,FINSET_1,TARSKI,RELAT_1,FUNCT_1,ORDINAL1,ZFMISC_1,XBOOLE_0,XCMPLX_1,NEWTON,XREAL_1,COMPLEX1,XXREAL_0,PREPOWER,NAT_D;
schemes NAT_1;
registrations ORDINAL1,FINSET_1,XXREAL_0,XREAL_0,NAT_1,INT_1,NEWTON,ABIAN,XBOOLE_0;
constructors DOMAIN_1,REAL_1,NAT_1,NAT_D,LIMFUNC1,ABIAN,PEPIN,VALUED_1;
requirements SUBSET,BOOLE,NUMERALS,REAL,ARITHM;
begin
reserve k for (Element of ( NAT ));
reserve m for (Element of ( NAT ));
reserve n for (Element of ( NAT ));
definition
let m being Nat;
let n being Nat;
redefine pred m,n are_relative_prime 
means
:Def1: (for k being Nat holds ((k divides m & k divides n) implies k = 1))
;compatibility
proof
hereby
assume m,n are_relative_prime ;
then A1: ( m gcd n ) = 1 by INT_2:def 3;
let k being Nat;
assume (k divides m & k divides n);
then k divides 1 by A1,NAT_D:def 5;
hence k = 1 by WSIERP_1:15;
end;
assume (for k being Nat holds ((k divides m & k divides n) implies k = 1));
then A2: (for k being Nat holds ((k divides m & k divides n) implies k divides 1));
(1 divides m & 1 divides n) by NAT_D:6;
hence ( m gcd n ) = 1 by A2,NAT_D:def 5;
end;
end;
