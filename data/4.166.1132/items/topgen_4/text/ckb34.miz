begin
reserve T for non  empty TopSpace;
reserve F for (Subset-Family of T);
reserve G for (Subset-Family of T);
theorem
Th17: (for T being set holds (for F being (Subset-Family of T) holds (F is  closed_for_countable_meets  compl-closed iff F is  closed_for_countable_unions  compl-closed)))
proof
let T being set;
let F being (Subset-Family of T);
hereby
assume A1: F is  closed_for_countable_meets  compl-closed;
(for G being  countable (Subset-Family of T) holds (G c= F implies ( union G ) in F))
proof
let G being  countable (Subset-Family of T);
assume A2: G c= F;
per cases ;
suppose G = ( {} );

hence thesis by A1,A2,CKB30:def 1,SETFAM_1:1,ZFMISC_1:2;
end;
suppose G <> ( {} );

then reconsider G9 = G as non  empty (Subset-Family of T);
(( COMPLEMENT G9 ) c= F & ( COMPLEMENT G9 ) is  countable) by A1,A2,CKB2:1,CKB33:1;
then A3: ( meet ( COMPLEMENT G9 ) ) in F by A1,CKB30:def 1;
( ( meet ( COMPLEMENT G9 ) ) ` ) = ( ( ( union G9 ) ` ) ` ) by TOPS_2:6
.= ( union G9 );
hence thesis by A1,A3,PROB_1:def 1;
end;
end;
hence F is  closed_for_countable_unions  compl-closed by A1,CKB24:def 1;
end;
assume A4: F is  closed_for_countable_unions  compl-closed;
(for G being  countable (Subset-Family of T) holds (G c= F implies ( meet G ) in F))
proof
let G being  countable (Subset-Family of T);
assume A5: G c= F;
per cases ;
suppose G = ( {} );

hence thesis by A4,A5,CKB24:def 1,SETFAM_1:1,ZFMISC_1:2;
end;
suppose G <> ( {} );

then reconsider G9 = G as non  empty (Subset-Family of T);
(( COMPLEMENT G9 ) c= F & ( COMPLEMENT G9 ) is  countable) by A4,A5,CKB2:1,CKB33:1;
then A6: ( union ( COMPLEMENT G9 ) ) in F by A4,CKB24:def 1;
( ( union ( COMPLEMENT G9 ) ) ` ) = ( ( ( meet G9 ) ` ) ` ) by TOPS_2:7
.= ( meet G9 );
hence thesis by A4,A6,PROB_1:def 1;
end;
end;
hence thesis by A4,CKB30:def 1;
end;
