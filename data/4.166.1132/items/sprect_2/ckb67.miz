begin
reserve i for (Element of ( NAT ));
theorem
Th64: (for f being (FinSequence of ( TOP-REAL 2 )) holds (for p being (Point of ( TOP-REAL 2 )) holds ((((f is  being_S-Seq & p <> ( f /. 1 )) & (( p `1 ) = ( ( f /. 1 ) `1 ) or ( p `2 ) = ( ( f /. 1 ) `2 ))) & ( ( LSeg (p,( f /. 1 )) ) /\ ( L~ f ) ) = { ( f /. 1 ) }) implies ( <* p *> ^ f ) is S-Sequence_in_R2)))
proof
let f being (FinSequence of ( TOP-REAL 2 ));
let p being (Point of ( TOP-REAL 2 ));
assume that
A1: f is  being_S-Seq
and
A2: p <> ( f /. 1 )
and
A3: (( p `1 ) = ( ( f /. 1 ) `1 ) or ( p `2 ) = ( ( f /. 1 ) `2 ))
and
A4: ( ( LSeg (p,( f /. 1 )) ) /\ ( L~ f ) ) = { ( f /. 1 ) };
reconsider f as S-Sequence_in_R2 by A1;
A5: ( len f ) >= ( 1 + 1 ) by TOPREAL1:def 8;
then A6: ( f /. 1 ) in ( LSeg (f,1) ) by TOPREAL1:21;
set g = ( <* p *> ^ f );
( len g ) = ( ( len <* p *> ) + ( len f ) ) by FINSEQ_1:22;
then ( len g ) >= ( len f ) by NAT_1:11;
then A8: ( len g ) >= 2 by A5,XXREAL_0:2;
now
assume A9: p in ( rng f );
(( rng f ) c= ( L~ f ) & p in ( LSeg (p,( f /. 1 )) )) by A5,RLTOPSP1:68,SPPOL_2:18;
then p in { ( f /. 1 ) } by A4,A9,XBOOLE_0:def 4;
hence contradiction by A2,TARSKI:def 1;
end;
then { p } misses ( rng f ) by ZFMISC_1:50;
then (<* p *> is  one-to-one & ( rng <* p *> ) misses ( rng f )) by FINSEQ_1:39,FINSEQ_3:93;
then A10: g is  one-to-one by FINSEQ_3:91;
( L~ <* p *> ) = ( {} ) by SPPOL_2:12;
then ( ( L~ <* p *> ) /\ ( L~ f ) ) = ( {} );
then A11: ( L~ <* p *> ) misses ( L~ f ) by XBOOLE_0:def 7;
A12: 1 in ( dom f ) by FINSEQ_5:6;
A13:now
let i;
assume that
A14: ( 1 + 1 ) <= i
and
A15: ( i + 1 ) <= ( len f );
A16: 2 in ( dom f ) by A5,FINSEQ_3:25;
now
assume ( f /. 1 ) in ( LSeg (f,i) );
then A17: ( f /. 1 ) in ( ( LSeg (f,1) ) /\ ( LSeg (f,i) ) ) by A6,XBOOLE_0:def 4;
then A18: ( LSeg (f,1) ) meets ( LSeg (f,i) ) by XBOOLE_0:4;
now
per cases  by A14,XXREAL_0:1;
case A19: i = ( 1 + 1 );
then ( ( LSeg (f,1) ) /\ ( LSeg (f,( 1 + 1 )) ) ) = { ( f /. 2 ) } by A15,TOPREAL1:def 6;
hence ( f /. 1 ) = ( f /. 2 ) by A17,A19,TARSKI:def 1;
end;
case i > ( 1 + 1 );
hence contradiction by A18,TOPREAL1:def 7;
end;
end;
then ( f . 1 ) = ( f /. 2 ) by A12,PARTFUN1:def 6
.= ( f . 2 ) by A16,PARTFUN1:def 6;
hence contradiction by A12,A16,FUNCT_1:def 4;
end;
then (not ( f /. 1 ) in ( ( LSeg (f,i) ) /\ ( LSeg (p,( f /. 1 )) ) )) by XBOOLE_0:def 4;
then A20: ( ( LSeg (f,i) ) /\ ( LSeg (p,( f /. 1 )) ) ) <> { ( f /. 1 ) } by TARSKI:def 1;
( ( LSeg (f,i) ) /\ ( LSeg (p,( f /. 1 )) ) ) c= { ( f /. 1 ) } by A4,TOPREAL3:19,XBOOLE_1:26;
then ( ( LSeg (f,i) ) /\ ( LSeg (p,( f /. 1 )) ) ) = ( {} ) by A20,ZFMISC_1:33;
hence ( LSeg (f,i) ) misses ( LSeg (p,( f /. 1 )) ) by XBOOLE_0:def 7;
end;
A21: ( len <* p *> ) = 1 by FINSEQ_1:39;
then A22: (<* p *> is  s.n.c. & ( <* p *> /. ( len <* p *> ) ) = p) by FINSEQ_4:16,SPPOL_2:33;
A23:now
let i;
assume that
1 <= i;
A24: 2 <= ( i + 2 ) by NAT_1:11;
assume ( i + 2 ) <= ( len <* p *> );
hence ( LSeg (<* p *>,i) ) misses ( LSeg (p,( f /. 1 )) ) by A21,A24,XXREAL_0:2;
end;
( ( LSeg (p,( f /. 1 )) ) /\ ( LSeg (f,1) ) ) = { ( f /. 1 ) } by A4,A6,TOPREAL3:19,ZFMISC_1:124;
then g is  unfolded  s.n.c.  special by A3,A22,A11,A23,A13,GOBOARD2:8,SPPOL_2:29,SPPOL_2:36;
hence thesis by A10,A8,TOPREAL1:def 8;
end;
