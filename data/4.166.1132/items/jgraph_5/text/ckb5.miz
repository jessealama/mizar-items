begin
theorem
Th8: (for X being TopStruct holds (for Y,Z being non  empty TopStruct holds (for f being (Function of X,Y) holds (for h being (Function of Y,Z) holds ((h is  being_homeomorphism & f is  continuous) implies ( h * f ) is  continuous)))))
proof
let X being TopStruct;
let Y being non  empty TopStruct;
let Z being non  empty TopStruct;
let f being (Function of X,Y);
let h being (Function of Y,Z);
assume that
A1: h is  being_homeomorphism
and
A2: f is  continuous;
h is  continuous by A1,TOPS_2:def 5;
hence thesis by A2,TOPS_2:46;
end;
