theorem
Th15: (for X,Y being non empty TopSpace holds (for x being (Point of X) holds [: ( X | { x } ),Y :],Y are_homeomorphic ))
proof
let X being non empty TopSpace;
let Y being non empty TopSpace;
let x being (Point of X);
set Z = { x };
(the carrier of [: ( X | Z ),Y :]) = [: (the carrier of ( X | Z )),(the carrier of Y) :] by BORSUK_1:def 2
.= [: Z,(the carrier of Y) :] by PRE_TOPC:8;
then reconsider f = ( pr2 (Z,(the carrier of Y)) ) as (Function of [: ( X | Z ),Y :],Y);
take f;
thus thesis by CKB19:1;
end;
