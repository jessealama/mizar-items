begin
theorem
(for X being non empty TopSpace holds (for Y being  compact non empty TopSpace holds (for G being  open (Subset of [: X,Y :]) holds (for x being set holds ([: { x },(the carrier of Y) :] c= G implies (ex f being (ManySortedSet of (the carrier of Y)) st (for i being set holds (i in (the carrier of Y) implies (ex G1 being (Subset of X) st (ex H1 being (Subset of Y) st ((((( f . i ) = [ G1,H1 ] & [ x,i ] in [: G1,H1 :]) & G1 is  open) & H1 is  open) & [: G1,H1 :] c= G)))))))))))
proof
let X being non empty TopSpace;
let Y being  compact non empty TopSpace;
let G being  open (Subset of [: X,Y :]);
let x being set;
set y = the (Point of Y);
A1: ((the carrier of [: X,Y :]) = [: (the carrier of X),(the carrier of Y) :] & [ x,y ] in [: { x },(the carrier of Y) :]) by BORSUK_1:def 2,ZFMISC_1:105;
defpred P[ set,set ]
 means
(ex G1 being (Subset of X) st (ex H1 being (Subset of Y) st (((($2 = [ G1,H1 ] & [ x,$1 ] in [: G1,H1 :]) & G1 is  open) & H1 is  open) & [: G1,H1 :] c= G)));
assume A2: [: { x },(the carrier of Y) :] c= G;
then [: { x },(the carrier of Y) :] c= (the carrier of [: X,Y :]) by XBOOLE_1:1;
then reconsider x9 = x as (Point of X) by A1,ZFMISC_1:87;
A3: [: { x9 },(the carrier of Y) :] c= ( union ( Base-Appr G ) ) by A2,BORSUK_1:13;
A4:now
let y being set;
A5: x in { x9 } by TARSKI:def 1;
assume y in (the carrier of Y);
then [ x,y ] in [: { x9 },(the carrier of Y) :] by A5,ZFMISC_1:87;
then consider Z being set such that A6: [ x,y ] in Z and A7: Z in ( Base-Appr G ) by A3,TARSKI:def 4;
( Base-Appr G ) = { [: X1,Y1 :] where X1 is (Subset of X),Y1 is (Subset of Y): (([: X1,Y1 :] c= G & X1 is  open) & Y1 is  open) } by BORSUK_1:def 3;
then (ex X1 being (Subset of X) st (ex Y1 being (Subset of Y) st (((Z = [: X1,Y1 :] & [: X1,Y1 :] c= G) & X1 is  open) & Y1 is  open))) by A7;
hence (ex G1 being (Subset of X) st (ex H1 being (Subset of Y) st ((([ x,y ] in [: G1,H1 :] & [: G1,H1 :] c= G) & G1 is  open) & H1 is  open))) by A6;
end;
A8: (for i being set holds (i in (the carrier of Y) implies (ex j being set st P[ i,j ])))
proof
let i being set;
assume i in (the carrier of Y);
then consider G1 being (Subset of X),H1 being (Subset of Y) such that A9: ((([ x,i ] in [: G1,H1 :] & [: G1,H1 :] c= G) & G1 is  open) & H1 is  open) by A4;
(ex G2 being (Subset of X) st (ex H2 being (Subset of Y) st (((([ G1,H1 ] = [ G2,H2 ] & [ x,i ] in [: G2,H2 :]) & G2 is  open) & H2 is  open) & [: G2,H2 :] c= G))) by A9;
hence thesis;
end;
(ex f being (ManySortedSet of (the carrier of Y)) st (for i being set holds (i in (the carrier of Y) implies P[ i,( f . i ) ]))) from PBOOLE:sch 3(A8);
hence thesis;
end;
