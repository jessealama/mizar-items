begin
scheme DomOnSingletons { X() -> non empty set,f() -> Function,F(set) -> set,P[set] } : ( dom f() ) = { o where o is (Element of X()): P[ o ] }
provided
A1: f() = { [ o,F(o) ] where o is (Element of X()): P[ o ] }
proof
set A = { o where o is (Element of X()): P[ o ] };
now
let x being set;
hereby
assume x in A;
then consider o being (Element of X()) such that A2: (x = o & P[ o ]);
take y = F(o);
thus [ x,y ] in f() by A1,A2;
end;
given y being set such that
A3: [ x,y ] in f();

consider o being (Element of X()) such that A4: [ x,y ] = [ o,F(o) ] and A5: P[ o ] by A1,A3;
x = o by A4,ZFMISC_1:27;
hence x in A by A5;
end;
hence thesis by RELAT_1:def 4;
end;
