begin
theorem
Th33: (for C being non empty  transitive AltCatStr holds (for D being non empty  transitive (SubCatStr of C) holds (for p1,p2,p3 being (object of D) holds ((<^ p1,p2 ^> <> ( {} ) & <^ p2,p3 ^> <> ( {} )) implies (for o1,o2,o3 being (object of C) holds (((o1 = p1 & o2 = p2) & o3 = p3) implies (for f being (Morphism of o1,o2) holds (for g being (Morphism of o2,o3) holds (for ff being (Morphism of p1,p2) holds (for gg being (Morphism of p2,p3) holds ((f = ff & g = gg) implies ( g * f ) = ( gg * ff ))))))))))))
proof
let C being non empty  transitive AltCatStr;
let D being non empty  transitive (SubCatStr of C);
let p1 being (object of D),p2 being (object of D),p3 being (object of D);
assume that
A1: (<^ p1,p2 ^> <> ( {} ) & <^ p2,p3 ^> <> ( {} ));
let o1 being (object of C),o2 being (object of C),o3 being (object of C);
assume that
A2: ((o1 = p1 & o2 = p2) & o3 = p3);
let f being (Morphism of o1,o2);
let g being (Morphism of o2,o3);
let ff being (Morphism of p1,p2);
let gg being (Morphism of p2,p3);
assume that
A3: (f = ff & g = gg);
<^ p1,p3 ^> <> ( {} ) by A1,ALTCAT_1:def 2;
then ( dom ( (the Comp of D) . (p1,p2,p3) ) ) = [: <^ p2,p3 ^>,<^ p1,p2 ^> :] by FUNCT_2:def 1;
then A4: [ gg,ff ] in ( dom ( (the Comp of D) . (p1,p2,p3) ) ) by A1,ZFMISC_1:87;
A5: (the Comp of D) cc= (the Comp of C) by CKB48:def 1;
(( (the Comp of D) . (p1,p2,p3) ) = ( (the Comp of D) . [ p1,p2,p3 ] ) & ( (the Comp of C) . (o1,o2,o3) ) = ( (the Comp of C) . [ o1,o2,o3 ] )) by MULTOP_1:def 1;
then A6: ( (the Comp of D) . (p1,p2,p3) ) c= ( (the Comp of C) . (o1,o2,o3) ) by A2,A5,CKB15:def 1;
(<^ o1,o2 ^> <> ( {} ) & <^ o2,o3 ^> <> ( {} )) by A1,A2,CKB71:1,XBOOLE_1:3;
hence ( g * f ) = ( ( (the Comp of C) . (o1,o2,o3) ) . (g,f) ) by ALTCAT_1:def 8
.= ( ( (the Comp of D) . (p1,p2,p3) ) . (gg,ff) ) by A3,A4,A6,GRFUNC_1:2
.= ( gg * ff ) by A1,ALTCAT_1:def 8;
end;
