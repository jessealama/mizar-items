begin
definition
let C being non empty AltCatStr;
redefine attr C is  with_units
means
(for o being (object of C) holds (<^ o,o ^> <> ( {} ) & (ex i being (Morphism of o,o) st (for o9 being (object of C) holds (for m9 being (Morphism of o9,o) holds (for m99 being (Morphism of o,o9) holds ((<^ o9,o ^> <> ( {} ) implies ( i * m9 ) = m9) & (<^ o,o9 ^> <> ( {} ) implies ( m99 * i ) = m99))))))));
