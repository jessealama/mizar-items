proof
hereby
assume A1: C is  with_units;
then reconsider C9 = C as  with_units non empty AltCatStr;
let o being (object of C);
thus <^ o,o ^> <> ( {} ) by A1,ALTCAT_1:18;
reconsider p = o as (object of C9);
reconsider i = ( idm p ) as (Morphism of o,o);
take i;
let o9 being (object of C);
let m9 being (Morphism of o9,o);
let m99 being (Morphism of o,o9);
thus (<^ o9,o ^> <> ( {} ) implies ( i * m9 ) = m9) by ALTCAT_1:20;
thus (<^ o,o9 ^> <> ( {} ) implies ( m99 * i ) = m99) by ALTCAT_1:def 17;
end;
assume A2: (for o being (object of C) holds (<^ o,o ^> <> ( {} ) & (ex i being (Morphism of o,o) st (for o9 being (object of C) holds (for m9 being (Morphism of o9,o) holds (for m99 being (Morphism of o,o9) holds ((<^ o9,o ^> <> ( {} ) implies ( i * m9 ) = m9) & (<^ o,o9 ^> <> ( {} ) implies ( m99 * i ) = m99))))))));
hereby
let j being (Element of C);
