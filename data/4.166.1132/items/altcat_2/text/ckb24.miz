begin
definition
let C being non void non empty CatStr;
func the_hom_sets_of C -> (ManySortedSet of [: (the carrier of C),(the carrier of C) :])
means
:Def3: (for i,j being (Object of C) holds ( it . (i,j) ) = ( Hom (i,j) ));
existence
proof
deffunc H((Object of C),(Object of C)) = ( Hom ($1,$2) );
thus (ex M being (ManySortedSet of [: (the carrier of C),(the carrier of C) :]) st (for i,j being (Object of C) holds ( M . (i,j) ) = H(i,j))) from ALTCAT_1:sch 2;
end;
uniqueness
proof
let M1 being (ManySortedSet of [: (the carrier of C),(the carrier of C) :]),M2 being (ManySortedSet of [: (the carrier of C),(the carrier of C) :]);
assume that
A1: (for i,j being (Object of C) holds ( M1 . (i,j) ) = ( Hom (i,j) ))
and
A2: (for i,j being (Object of C) holds ( M2 . (i,j) ) = ( Hom (i,j) ));
now
let i being (Object of C),j being (Object of C);
thus ( M1 . (i,j) ) = ( Hom (i,j) ) by A1
.= ( M2 . (i,j) ) by A2;
end;
hence thesis by ALTCAT_1:7;
end;
end;
