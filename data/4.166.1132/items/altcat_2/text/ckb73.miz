begin
registration
let C being  associative  transitive non empty AltCatStr;
cluster  transitive ->  associative for non empty non empty (SubCatStr of C);
coherence
proof
let D being non empty (SubCatStr of C);
assume D is  transitive;
then reconsider D as  transitive non empty (SubCatStr of C);
D is  associative
proof
let o1 being (object of D),o2 being (object of D),o3 being (object of D),o4 being (object of D);
A1: (o1 in (the carrier of D) & o2 in (the carrier of D));
A2: (o3 in (the carrier of D) & o4 in (the carrier of D));
(the carrier of D) c= (the carrier of C) by CKB48:def 1;
then reconsider p1 = o1,p2 = o2,p3 = o3,p4 = o4 as (object of C) by A1,A2;
let f being (Morphism of o1,o2);
let g being (Morphism of o2,o3);
let h being (Morphism of o3,o4);
assume that
A3: <^ o1,o2 ^> <> ( {} )
and
A4: <^ o2,o3 ^> <> ( {} )
and
A5: <^ o3,o4 ^> <> ( {} );
A6: <^ o2,o3 ^> c= <^ p2,p3 ^> by CKB71:1;
g in <^ o2,o3 ^> by A4;
then reconsider gg = g as (Morphism of p2,p3) by A6;
A7: <^ o1,o2 ^> c= <^ p1,p2 ^> by CKB71:1;
f in <^ o1,o2 ^> by A3;
then reconsider ff = f as (Morphism of p1,p2) by A7;
A8: (<^ o1,o3 ^> <> ( {} ) & ( g * f ) = ( gg * ff )) by A3,A4,CKB72:1,ALTCAT_1:def 2;
A9: <^ o3,o4 ^> c= <^ p3,p4 ^> by CKB71:1;
h in <^ o3,o4 ^> by A5;
then reconsider hh = h as (Morphism of p3,p4) by A9;
A10: <^ p3,p4 ^> <> ( {} ) by A5,CKB71:1,XBOOLE_1:3;
A11: (<^ p1,p2 ^> <> ( {} ) & <^ p2,p3 ^> <> ( {} )) by A3,A4,CKB71:1,XBOOLE_1:3;
(<^ o2,o4 ^> <> ( {} ) & ( h * g ) = ( hh * gg )) by A4,A5,CKB72:1,ALTCAT_1:def 2;
hence ( ( h * g ) * f ) = ( ( hh * gg ) * ff ) by A3,CKB72:1
.= ( hh * ( gg * ff ) ) by A11,A10,CKB38:def 1
.= ( h * ( g * f ) ) by A5,A8,CKB72:1;
end;
hence thesis;
