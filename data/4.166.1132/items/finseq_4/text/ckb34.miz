begin
reserve p for FinSequence;
reserve x for set;
reserve z for set;
theorem
Th38: (p just_once_values x iff (x in ( rng p ) & { ( x .. p ) } = ( p " { x } )))
proof
thus (p just_once_values x implies (x in ( rng p ) & { ( x .. p ) } = ( p " { x } )))
proof
assume A1: p just_once_values x;
then ( x .. p ) = ( p <- x ) by CKB31:1;
hence thesis by A1,CKB7:1,CKB14:1;
end;

assume that
A2: x in ( rng p )
and
A3: { ( x .. p ) } = ( p " { x } );
A4:now
let z;
assume that
A5: z in ( dom p )
and
A6: z <> ( x .. p )
and
A7: ( p . z ) = x;
( p . z ) in { x } by A7,TARSKI:def 1;
then z in ( p " { x } ) by A5,FUNCT_1:def 7;
hence contradiction by A3,A6,TARSKI:def 1;
end;
(( p . ( x .. p ) ) = x & ( x .. p ) in ( dom p )) by A2,CKB25:1,CKB26:1;
hence thesis by A4,CKB9:1;
end;
