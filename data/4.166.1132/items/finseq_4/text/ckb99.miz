begin
reserve n for Nat;
scheme Sch1 { D() -> non empty set,N() -> Nat,P[set,set] } : (ex f being (FinSequence of D()) st (( len f ) = N() & (for n holds (n in ( Seg N() ) implies P[ n,( f /. n ) ]))))
provided
A1: (for n holds (n in ( Seg N() ) implies (ex d being (Element of D()) st P[ n,d ])))
proof
defpred P1[ set,set ]
 means
(P[ $1,$2 ] & $2 in D());
reconsider X = D() as set;
A2:now
let x being set;
assume A3: x in ( Seg N() );
then reconsider x9 = x as (Element of ( NAT ));
consider d being (Element of D()) such that A4: P[ x9,d ] by A1,A3;
reconsider y = d as set;
take y;
thus (y in X & P1[ x,y ]) by A4;
end;
consider f being Function such that A5: ((( dom f ) = ( Seg N() ) & ( rng f ) c= X) & (for x being set holds (x in ( Seg N() ) implies P1[ x,( f . x ) ]))) from FUNCT_1:sch 5(A2);
reconsider f as FinSequence by A5,FINSEQ_1:def 2;
reconsider f as (FinSequence of D()) by A5,FINSEQ_1:def 4;
take f;
N() in ( NAT ) by ORDINAL1:def 12;
hence ( len f ) = N() by A5,FINSEQ_1:def 3;
let n;
assume A6: n in ( Seg N() );
then P[ n,( f . n ) ] by A5;
hence thesis by A5,A6,PARTFUN1:def 6;
end;
