begin
reserve p for FinSequence;
reserve q for FinSequence;
reserve x for set;
reserve k for Nat;
definition
let p;
let x;
assume A1: x in ( rng p );
func p |-- x -> FinSequence
means
:Def7: (( len it ) = ( ( len p ) - ( x .. p ) ) & (for k holds (k in ( dom it ) implies ( it . k ) = ( p . ( k + ( x .. p ) ) ))));
existence
proof
deffunc F(Nat) = ( p . ( $1 + ( x .. p ) ) );
reconsider n = ( ( len p ) - ( x .. p ) ) as (Element of ( NAT )) by A1,CKB28:1;
consider q such that A2: (( len q ) = n & (for k being Nat holds (k in ( dom q ) implies ( q . k ) = F(k)))) from FINSEQ_1:sch 2;
take q;
thus thesis by A2;
end;
uniqueness
proof
let q being FinSequence,r being FinSequence;
assume that
A3: ( len q ) = ( ( len p ) - ( x .. p ) )
and
A4: (for k holds (k in ( dom q ) implies ( q . k ) = ( p . ( k + ( x .. p ) ) )));
assume that
A5: ( len r ) = ( ( len p ) - ( x .. p ) )
and
A6: (for k holds (k in ( dom r ) implies ( r . k ) = ( p . ( k + ( x .. p ) ) )));
now
let k being Nat;
A7: (( dom q ) = ( Seg ( len q ) ) & ( dom r ) = ( Seg ( len r ) )) by FINSEQ_1:def 3;
assume A8: k in ( dom q );
then ( q . k ) = ( p . ( k + ( x .. p ) ) ) by A4;
hence ( q . k ) = ( r . k ) by A3,A5,A6,A8,A7;
end;
hence thesis by A3,A5,FINSEQ_2:9;
end;
end;
