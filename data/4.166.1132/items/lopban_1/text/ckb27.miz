requirements SUBSET,REAL,BOOLE,NUMERALS,ARITHM;
begin
definition
let X being RealLinearSpace;
let Y being RealLinearSpace;
func LinearOperators (X,Y) -> (Subset of ( RealVectSpace ((the carrier of X),Y) ))
means
:Def7: (for x being set holds (x in it iff x is (LinearOperator of X,Y)));
existence
proof
defpred P[ set ]
 means
$1 is (LinearOperator of X,Y);
consider IT being set such that A1: (for x being set holds (x in IT iff (x in ( Funcs ((the carrier of X),(the carrier of Y)) ) & P[ x ]))) from XBOOLE_0:sch 1;
take IT;
(for x being set holds (x in IT implies x in ( Funcs ((the carrier of X),(the carrier of Y)) ))) by A1;
hence IT is (Subset of ( RealVectSpace ((the carrier of X),Y) )) by TARSKI:def 3;
let x being set;
thus (x in IT implies x is (LinearOperator of X,Y)) by A1;
assume A2: x is (LinearOperator of X,Y);
then x in ( Funcs ((the carrier of X),(the carrier of Y)) ) by FUNCT_2:8;
hence thesis by A1,A2;
end;
uniqueness
proof
let X1 being (Subset of ( RealVectSpace ((the carrier of X),Y) )),X2 being (Subset of ( RealVectSpace ((the carrier of X),Y) ));
assume that
A3: (for x being set holds (x in X1 iff x is (LinearOperator of X,Y)))
and
A4: (for x being set holds (x in X2 iff x is (LinearOperator of X,Y)));
(for x being set holds (x in X2 implies x in X1))
proof
let x being set;
assume x in X2;
then x is (LinearOperator of X,Y) by A4;
hence thesis by A3;
end;
then A5: X2 c= X1 by TARSKI:def 3;
(for x being set holds (x in X1 implies x in X2))
proof
let x being set;
assume x in X1;
then x is (LinearOperator of X,Y) by A3;
hence thesis by A4;
end;
then X1 c= X2 by TARSKI:def 3;
hence thesis by A5,XBOOLE_0:def 10;
end;
end;
