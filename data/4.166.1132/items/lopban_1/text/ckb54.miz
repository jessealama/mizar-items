begin
reserve X for non  empty set;
reserve Y for RealLinearSpace;
reserve f for (Element of ( Funcs (X,(the carrier of Y)) ));
reserve h for (Element of ( Funcs (X,(the carrier of Y)) ));
reserve a for Real;
theorem
Th30: (for X,Y being RealNormSpace holds (for f,h being (VECTOR of ( R_VectorSpace_of_BoundedLinearOperators (X,Y) )) holds (for a being Real holds (h = ( a * f ) iff (for x being (VECTOR of X) holds ( h . x ) = ( a * ( f . x ) ))))))
proof
let X being RealNormSpace;
let Y being RealNormSpace;
let f being (VECTOR of ( R_VectorSpace_of_BoundedLinearOperators (X,Y) ));
let h being (VECTOR of ( R_VectorSpace_of_BoundedLinearOperators (X,Y) ));
let a being Real;
A1: ( R_VectorSpace_of_BoundedLinearOperators (X,Y) ) is (Subspace of ( R_VectorSpace_of_LinearOperators (X,Y) )) by CKB46:1,RSSPACE:11;
then reconsider f1 = f as (VECTOR of ( R_VectorSpace_of_LinearOperators (X,Y) )) by RLSUB_1:10;
reconsider h1 = h as (VECTOR of ( R_VectorSpace_of_LinearOperators (X,Y) )) by A1,RLSUB_1:10;
hereby
assume A2: h = ( a * f );
let x being (Element of X);
h1 = ( a * f1 ) by A1,A2,RLSUB_1:14;
hence ( h . x ) = ( a * ( f . x ) ) by CKB37:1;
end;
assume (for x being (Element of X) holds ( h . x ) = ( a * ( f . x ) ));
then h1 = ( a * f1 ) by CKB37:1;
hence thesis by A1,RLSUB_1:14;
end;
