begin
reserve D for non empty set;
reserve f for (FinSequence of D);
reserve i for Nat;
reserve j for Nat;
theorem
( Swap (( Swap (f,i,j) ),j,i) ) = f
proof
per cases ;
suppose A1: (((1 <= i & i <= ( len f )) & 1 <= j) & j <= ( len f ));

A2: (for k being Nat holds ((1 <= k & k <= ( len f )) implies ( f . k ) = ( ( Swap (( Swap (f,i,j) ),j,i) ) . k )))
proof
A3: i <= ( len ( Swap (f,i,j) ) ) by A1,CKB24:1;
A4: j <= ( len ( Swap (f,i,j) ) ) by A1,CKB24:1;
let k being Nat;
assume that
A5: 1 <= k
and
A6: k <= ( len f );
A7: k <= ( len ( Swap (f,i,j) ) ) by A6,CKB24:1;
now
per cases ;
suppose i = k;

then ( ( Swap (( Swap (f,i,j) ),j,i) ) . k ) = ( ( Swap (f,k,j) ) . j ) by A1,A7,A4,CKB25:1;
hence thesis by A1,A5,A6,CKB25:1;
end;
suppose A8: i <> k;

now
per cases ;
suppose j = k;

then ( ( Swap (( Swap (f,i,j) ),j,i) ) . k ) = ( ( Swap (f,i,k) ) . i ) by A1,A7,A3,CKB25:1;
hence thesis by A1,A5,A6,CKB25:1;
end;
suppose A9: j <> k;

set S = ( Swap (f,i,j) );
( ( Swap (S,j,i) ) . k ) = ( ( Replace (( Replace (S,j,( S /. i )) ),i,( S /. j )) ) . k ) by A1,A4,A3,CKB23:def 1
.= ( ( Replace (S,j,( S /. i )) ) . k ) by A8,FUNCT_7:32
.= ( S . k ) by A9,FUNCT_7:32
.= ( ( Replace (( Replace (f,i,( f /. j )) ),j,( f /. i )) ) . k ) by A1,CKB23:def 1
.= ( ( Replace (f,i,( f /. j )) ) . k ) by A9,FUNCT_7:32;
hence thesis by A8,FUNCT_7:32;
end;
end;
hence thesis;
end;
end;
hence thesis;
end;
( len ( Swap (( Swap (f,i,j) ),j,i) ) ) = ( len ( Swap (f,i,j) ) ) by CKB24:1
.= ( len f ) by CKB24:1;
hence thesis by A2,FINSEQ_1:14;
end;
suppose A10: (not (((1 <= i & i <= ( len f )) & 1 <= j) & j <= ( len f )));

then ( Swap (( Swap (f,i,j) ),j,i) ) = ( Swap (f,j,i) ) by CKB23:def 1;
hence thesis by A10,CKB23:def 1;
end;
end;
