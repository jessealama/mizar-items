theorem
(for i,j being Nat holds ((i < j & i <> ( 0 )) implies (not ( i / j ) is  integer)))
proof
let i being Nat;
let j being Nat;
assume that
A1: i < j
and
A2: i <> ( 0 );
assume ( i / j ) is  integer;
then reconsider r = ( i / j ) as Integer;
r = [\ r /] by INT_1:25
.= ( (i qua Integer) div j ) by INT_1:def 9;
hence thesis by A1,A2,CKB37:1,XCMPLX_1:50;
end;
