begin
reserve a for Int_position;
theorem
(for s being (State of ( SCMPDS )) holds (for iloc being (Element of ( NAT )) holds (for a being Int_position holds ( s . a ) = ( ( s +* ( Start-At (iloc,( SCMPDS )) ) ) . a ))))
proof
let s being (State of ( SCMPDS ));
let iloc being (Element of ( NAT ));
let a being Int_position;
a in (the carrier of ( SCMPDS ));
then a in ( dom s ) by PARTFUN1:def 2;
then A1: (( dom ( Start-At (iloc,( SCMPDS )) ) ) = { ( IC ( SCMPDS ) ) } & a in ( ( dom s ) \/ ( dom ( Start-At (iloc,( SCMPDS )) ) ) )) by FUNCOP_1:13,XBOOLE_0:def 3;
a <> ( IC ( SCMPDS ) ) by SCMPDS_2:43;
then (not a in { ( IC ( SCMPDS ) ) }) by TARSKI:def 1;
hence thesis by A1,FUNCT_4:def 1;
end;
