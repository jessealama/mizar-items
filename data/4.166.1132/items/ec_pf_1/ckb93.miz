P3:now
let n being Nat;
assume P31: n in ( Seg p );
then (1 <= n & n <= p) by FINSEQ_1:1;
then X2: (( 1 - 1 ) <= ( n - 1 ) & ( n - 1 ) <= ( p - 1 )) by XREAL_1:9;
