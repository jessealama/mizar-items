theorem
LMNG: (for p being Prime holds (for n being Nat holds (n in ( Seg p ) implies ( n - 1 ) is (Element of ( GF p )))))
proof
let p being Prime;
let n being Nat;
assume n in ( Seg p );
then (1 <= n & n <= p) by FINSEQ_1:1;
then X2: (( 1 - 1 ) <= ( n - 1 ) & ( n - 1 ) <= ( p - 1 )) by XREAL_1:9;
then X3: ( n - 1 ) is (Element of ( NAT )) by INT_1:3;
( p - 1 ) < ( p - ( 0 ) ) by XREAL_1:15;
then ( n - 1 ) < p by X2,XXREAL_0:2;
hence thesis by X3,NAT_1:44;
end;
