theorem
LmDefREQ3: (for p being Prime holds (for a,b being (Element of ( GF p )) holds (for x being set holds (((p > 3 & ( Disc (a,b,p) ) <> ( 0. ( GF p ) )) & x in ( Class ( R_EllCur (a,b,p) ) )) implies ((ex P being (Element of ( ProjCo ( GF p ) )) st ((P in ( EC_SetProjCo (a,b,p) ) & P = [ ( 0 ),1,( 0 ) ]) & x = ( Class (( R_EllCur (a,b,p) ),P) ))) or (ex P being (Element of ( ProjCo ( GF p ) )) st (ex X,Y being (Element of ( GF p )) st ((P in ( EC_SetProjCo (a,b,p) ) & P = [ X,Y,1 ]) & x = ( Class (( R_EllCur (a,b,p) ),P) )))))))))
proof
let p being Prime;
let a being (Element of ( GF p ));
let b being (Element of ( GF p ));
let x being set;
assume AS1: ((p > 3 & ( Disc (a,b,p) ) <> ( 0. ( GF p ) )) & x in ( Class ( R_EllCur (a,b,p) ) ));
then consider y0 being (Element of ( EC_SetProjCo (a,b,p) )) such that P1: x = ( Class (( R_EllCur (a,b,p) ),y0) ) by EQREL_1:36;
reconsider w = y0 as (Element of ( ProjCo ( GF p ) ));
per cases ;
suppose ( w `3 ) = ( 0 );

then consider y being (Element of ( ProjCo ( GF p ) )) such that D2: ((((y in ( EC_SetProjCo (a,b,p) ) & y _EQ_ w) & ( y `1 ) = ( 0 )) & ( y `2 ) = 1) & ( y `3 ) = ( 0 )) by AS1,CKB80:1;
[ y,w ] in ( R_EllCur (a,b,p) ) by AS1,CKB78:1,D2;
then x = ( Class (( R_EllCur (a,b,p) ),y) ) by P1,D2,EQREL_1:35;
hence thesis by D2,MCART_1:44;
end;
suppose ( w `3 ) <> ( 0 );

then consider y being (Element of ( ProjCo ( GF p ) )) such that D2: ((y in ( EC_SetProjCo (a,b,p) ) & y _EQ_ w) & ( y `3 ) = 1) by AS1,CKB79:1;
set e = ( y `1 );
set f = ( y `2 );
F1: y = [ e,f,1 ] by D2,MCART_1:44;
[ y,w ] in ( R_EllCur (a,b,p) ) by AS1,CKB78:1,D2;
then x = ( Class (( R_EllCur (a,b,p) ),y) ) by P1,D2,EQREL_1:35;
hence thesis by F1,D2;
end;
