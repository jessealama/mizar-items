let f;
assume that
{ ( x. ( 0 ) ),( x. 1 ),( x. 2 ) } misses ( Free H )
and
W,f |= ( All (( x. 3 ),( Ex (( x. ( 0 ) ),( All (( x. 4 ),( H <=> ( ( x. 4 ) '=' ( x. ( 0 ) ) ) )) )) )) );
