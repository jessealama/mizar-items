begin
reserve F for Function;
reserve A for Ordinal;
reserve B for Ordinal;
scheme ALFA9Universe { W() -> Universe,D() -> non empty set,P[set,set] } : (ex F st (( dom F ) = D() & (for d being (Element of D()) holds (ex a being (Ordinal of W()) st ((a = ( F . d ) & P[ d,a ]) & (for b being (Ordinal of W()) holds (P[ d,b ] implies a c= b)))))))
provided
A1: (for d being (Element of D()) holds (ex a being (Ordinal of W()) st P[ d,a ]))
proof
A2: (for d being (Element of D()) holds (ex A st P[ d,A ]))
proof
let d being (Element of D());
consider a being (Ordinal of W()) such that A3: P[ d,a ] by A1;
reconsider a as Ordinal;
take a;
thus thesis by A3;
end;
consider F such that A4: ( dom F ) = D() and A5: (for d being (Element of D()) holds (ex A st ((A = ( F . d ) & P[ d,A ]) & (for B holds (P[ d,B ] implies A c= B))))) from ORDINAL1:sch 6(A2);
take F;
thus ( dom F ) = D() by A4;
let d being (Element of D());
consider A such that A6: (A = ( F . d ) & P[ d,A ]) and A7: (for B holds (P[ d,B ] implies A c= B)) by A5;
consider a being (Ordinal of W()) such that A8: P[ d,a ] by A1;
A c= a by A7,A8;
then reconsider a = A as (Ordinal of W()) by CLASSES1:def 1;
take a;
thus thesis by A6,A7;
end;
