begin
scheme ElementEq { A() -> set,P[set] } : (for X1,X2 being (Element of A()) holds (((for x being set holds (x in X1 iff P[ x ])) & (for x being set holds (x in X2 iff P[ x ]))) implies X1 = X2))
proof
let X1 being (Element of A());
let X2 being (Element of A());
assume that
A1: (for x being set holds (x in X1 iff P[ x ]))
and
A2: (for x being set holds (x in X2 iff P[ x ]));
now
let x being set;
(x in X1 iff P[ x ]) by A1;
hence (x in X1 iff x in X2) by A2;
end;
hence thesis by TARSKI:1;
end;
