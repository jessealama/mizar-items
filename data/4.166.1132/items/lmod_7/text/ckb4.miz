begin
scheme QuaOpEq { A() -> non  empty set,F((Element of A()),(Element of A()),(Element of A()),(Element of A())) -> set } : (for f1,f2 being (QuaOp of A()) holds (((for a,b,c,d being (Element of A()) holds ( f1 . (a,b,c,d) ) = F(a,b,c,d)) & (for a,b,c,d being (Element of A()) holds ( f2 . (a,b,c,d) ) = F(a,b,c,d))) implies f1 = f2))
proof
let f1 being (QuaOp of A());
let f2 being (QuaOp of A());
assume that
A1: (for a,b,c,d being (Element of A()) holds ( f1 . (a,b,c,d) ) = F(a,b,c,d))
and
A2: (for a,b,c,d being (Element of A()) holds ( f2 . (a,b,c,d) ) = F(a,b,c,d));
now
let a being (Element of A());
let b being (Element of A());
let c being (Element of A());
let d being (Element of A());
thus ( f1 . (a,b,c,d) ) = F(a,b,c,d) by A1
.= ( f2 . (a,b,c,d) ) by A2;
end;
hence thesis by MULTOP_1:6;
end;
