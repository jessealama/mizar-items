begin
scheme UnOpEq { A() -> non  empty set,F((Element of A())) -> set } : (for f1,f2 being (UnOp of A()) holds (((for a being (Element of A()) holds ( f1 . a ) = F(a)) & (for a being (Element of A()) holds ( f2 . a ) = F(a))) implies f1 = f2))
proof
let f1 being (UnOp of A());
let f2 being (UnOp of A());
assume that
A1: (for a being (Element of A()) holds ( f1 . a ) = F(a))
and
A2: (for a being (Element of A()) holds ( f2 . a ) = F(a));
now
let a being (Element of A());
thus ( f1 . a ) = F(a) by A1
.= ( f2 . a ) by A2;
end;
hence thesis by FUNCT_2:63;
end;
