begin
reserve X for RealNormSpace;
definition
let X being RealNormSpace;
let A being non  empty  closed_interval (Subset of ( REAL ));
let f being (Function of A,(the carrier of X));
let D being (Division of A);
mode middle_volume of f,D
 -> (FinSequence of X)
means :Def1: (( len it ) = ( len D ) & (for i being Nat holds (i in ( dom D ) implies (ex c being (Point of X) st (c in ( rng ( f | ( divset (D,i) ) ) ) & ( it . i ) = ( ( vol ( divset (D,i) ) ) * c ))))));
correctness
proof
defpred P1[ Nat,set ]
 means
(ex c being (Point of X) st (c in ( rng ( f | ( divset (D,$1) ) ) ) & $2 = ( ( vol ( divset (D,$1) ) ) * c )));
A1: ( Seg ( len D ) ) = ( dom D ) by FINSEQ_1:def 3;
A2: (for k being Nat holds (k in ( Seg ( len D ) ) implies (ex x being (Element of (the carrier of X)) st P1[ k,x ])))
proof
let k being Nat;
assume k in ( Seg ( len D ) );
then A3: k in ( dom D ) by FINSEQ_1:def 3;
( dom f ) = A by FUNCT_2:def 1;
then ( dom ( f | ( divset (D,k) ) ) ) = ( divset (D,k) ) by A3,INTEGRA1:8,RELAT_1:62;
then ( rng ( f | ( divset (D,k) ) ) ) is non  empty by RELAT_1:42;
then consider c being set such that A4: c in ( rng ( f | ( divset (D,k) ) ) ) by XBOOLE_0:def 1;
reconsider c as (Point of X) by A4;
( ( vol ( divset (D,k) ) ) * c ) is (Element of (the carrier of X));
hence thesis by A4;
end;
consider p being (FinSequence of (the carrier of X)) such that A5: (( dom p ) = ( Seg ( len D ) ) & (for k being Nat holds (k in ( Seg ( len D ) ) implies P1[ k,( p . k ) ]))) from FINSEQ_1:sch 5(A2);
( len p ) = ( len D ) by A5,FINSEQ_1:def 3;
hence thesis by A5,A1;
end;
end;
