vocabularies NAT_1,VECTSP_1,MATRIX_1,RELAT_1,SUPINF_2,MESFUNC1,ALGSTR_0,TREES_1,FINSEQ_1,XXREAL_0,CARD_1,QC_LANG1,MATRIX_6,REWRITE1,ARYTM_3,ARYTM_1,RELAT_2,FUNCOP_1,ZFMISC_1,FSM_1,COHSP_1,SUBSET_1,CARD_3,MATRIX_3,FUNCT_1,PARTFUN1,MATRIX_8;
notations TARSKI,ZFMISC_1,PARTFUN1,NUMBERS,FINSEQ_1,STRUCT_0,RLVECT_1,NAT_1,VECTSP_1,MATRIX_1,FUNCT_1,MATRIX_2,MATRIX_3,MATRIX_6,FVSUM_1,XXREAL_0,CKB1,CKB2,CKB3,CKB4;
definitions MATRIX_1,MATRIX_2,MATRIX_3,MATRIX_4,FINSEQ_1,FVSUM_1,ALGSTR_0,CKB1,CKB2,CKB3,CKB4;
theorems ZFMISC_1,RLVECT_1,MATRIX_1,MATRIX_2,MATRIX_4,MATRIX_3,FUNCOP_1,FINSEQ_1,INT_1,MATRIX_6,POLYNOM1,MATRLIN,NAT_1,PARTFUN1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25;
registrations RELSET_1,XREAL_0,INT_1,STRUCT_0,MATRIX_6,ORDINAL1,CKB7,CKB8;
