schemes CLASSES1,FINSEQ_2,PARTFUN1,NAT_1,MATRIX_1;
registrations XBOOLE_0,ORDINAL1,RELSET_1,NUMBERS,XXREAL_0,NAT_1,MEMBERED,FINSEQ_1,FINSEQ_2,STRUCT_0,VECTSP_1,MATRIX_1,MATRLIN,VALUED_0,CARD_1;
constructors XXREAL_0,REAL_1,NAT_1,BINOP_2,NEWTON,FVSUM_1,MATRIX_3,MATRLIN,MATRIXR1,SEQ_1,BINOP_1,RVSUM_1,RELSET_1,CKB1,CKB2;
requirements REAL,NUMERALS,BOOLE,SUBSET,ARITHM;
begin
reserve D for non  empty set;
reserve i for (Element of ( NAT ));
reserve n for Nat;
theorem
Th10: (for D being set holds (for s being FinSequence holds (s is (Matrix of D) iff (ex n st (for i holds (i in ( dom s ) implies (ex p being (FinSequence of D) st (( s . i ) = p & ( len p ) = n))))))))
proof
let D being set;
let s being FinSequence;
thus (s is (Matrix of D) implies (ex n st (for i holds (i in ( dom s ) implies (ex p being (FinSequence of D) st (( s . i ) = p & ( len p ) = n))))))
proof
assume A1: s is (Matrix of D);
then reconsider v = s as (FinSequence of ( D * ));
consider n being Nat such that A2: (for x being set holds (x in ( rng v ) implies (ex t being FinSequence st (t = x & ( len t ) = n)))) by A1,MATRIX_1:def 1;
A3: (for i holds (i in ( dom v ) implies (ex p being (FinSequence of D) st (( v . i ) = p & ( len p ) = n))))
proof
let i;
assume i in ( dom v );
then consider t being FinSequence such that A4: (t = ( v . i ) & ( len t ) = n) by A2,FUNCT_1:3;
take t;
thus thesis by A4;
end;
reconsider n as (Element of ( NAT )) by ORDINAL1:def 12;
take n;
thus thesis by A3;
end;

given n such that
A5: (for i holds (i in ( dom s ) implies (ex p being (FinSequence of D) st (( s . i ) = p & ( len p ) = n))));

A6: (for x being set holds (x in ( rng s ) implies ((ex v being FinSequence st (v = x & ( len v ) = n)) & x in ( D * ))))
proof
let x being set;
assume x in ( rng s );
then consider i being set such that A7: i in ( dom s ) and A8: x = ( s . i ) by FUNCT_1:def 3;
A9: (ex p being (FinSequence of D) st (( s . i ) = p & ( len p ) = n)) by A5,A7;
hence (ex v being FinSequence st (v = x & ( len v ) = n)) by A8;
thus thesis by A8,A9,FINSEQ_1:def 11;
end;
then (for x being set holds (x in ( rng s ) implies x in ( D * )));
then A10: ( rng s ) c= ( D * ) by TARSKI:def 3;
(for x being set holds (x in ( rng s ) implies (ex v being FinSequence st (v = x & ( len v ) = n)))) by A6;
hence thesis by A10,FINSEQ_1:def 4,MATRIX_1:def 1;
end;
