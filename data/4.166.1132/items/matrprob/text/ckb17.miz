constructors XXREAL_0,REAL_1,NAT_1,BINOP_2,NEWTON,FVSUM_1,MATRIX_3,MATRLIN,MATRIXR1,SEQ_1,BINOP_1,RVSUM_1,RELSET_1,CKB1,CKB2;
requirements REAL,NUMERALS,BOOLE,SUBSET,ARITHM;
begin
reserve D for non  empty set;
reserve i for (Element of ( NAT ));
reserve j for (Element of ( NAT ));
theorem
Th15: (for D being non  empty set holds (for M being (Matrix of D) holds ([ i,j ] in ( Indices M ) iff (i in ( dom ( Col (M,j) ) ) & j in ( dom ( Line (M,i) ) )))))
proof
let D being non  empty set;
let M being (Matrix of D);
hereby
assume A1: [ i,j ] in ( Indices M );
then A2: i in ( dom M ) by CKB15:1;
then i in ( Seg ( len M ) ) by FINSEQ_1:def 3;
then i in ( Seg ( len ( Col (M,j) ) ) ) by MATRIX_1:def 8;
hence i in ( dom ( Col (M,j) ) ) by FINSEQ_1:def 3;
j in ( dom ( M . i ) ) by A1,CKB15:1;
hence j in ( dom ( Line (M,i) ) ) by A2,MATRIX_2:16;
end;
assume that
A3: i in ( dom ( Col (M,j) ) )
and
A4: j in ( dom ( Line (M,i) ) );
i in ( Seg ( len ( Col (M,j) ) ) ) by A3,FINSEQ_1:def 3;
then i in ( Seg ( len M ) ) by MATRIX_1:def 8;
then A5: i in ( dom M ) by FINSEQ_1:def 3;
then j in ( dom ( M . i ) ) by A4,MATRIX_2:16;
hence thesis by A5,CKB15:1;
end;
