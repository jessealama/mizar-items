begin
reserve k for (Element of ( NAT ));
definition
let e being (FinSequence of ( ( REAL ) * ));
func Sum e -> (FinSequence of ( REAL ))
means
:Def1: (( len it ) = ( len e ) & (for k holds (k in ( dom it ) implies ( it . k ) = ( Sum ( e . k ) ))));
existence
proof
deffunc f(Nat) = ( Sum ( e . $1 ) );
consider e1 being (FinSequence of ( REAL )) such that A1: (( len e1 ) = ( len e ) & (for k being Nat holds (k in ( dom e1 ) implies ( e1 . k ) = f(k)))) from FINSEQ_2:sch 1;
take e1;
thus thesis by A1;
end;
uniqueness
proof
let e1 being (FinSequence of ( REAL )),e2 being (FinSequence of ( REAL ));
assume that
A2: ( len e1 ) = ( len e )
and
A3: (for k holds (k in ( dom e1 ) implies ( e1 . k ) = ( Sum ( e . k ) )))
and
A4: ( len e2 ) = ( len e )
and
A5: (for k holds (k in ( dom e2 ) implies ( e2 . k ) = ( Sum ( e . k ) )));
(( dom e1 ) = ( dom e2 ) & (for k being Nat holds (k in ( dom e1 ) implies ( e1 . k ) = ( e2 . k ))))
proof
thus A6: ( dom e1 ) = ( Seg ( len e ) ) by A2,FINSEQ_1:def 3
.= ( dom e2 ) by A4,FINSEQ_1:def 3;
let k being Nat;
assume that
A7: k in ( dom e1 );
thus ( e1 . k ) = ( Sum ( e . k ) ) by A3,A7
.= ( e2 . k ) by A5,A6,A7;
end;
hence thesis by FINSEQ_1:13;
end;
end;
