requirements REAL,NUMERALS,BOOLE,SUBSET,ARITHM;
begin
reserve D for non empty set;
theorem
Th1: (for a being (Element of D) holds (for m being non empty Nat holds (for g being (FinSequence of D) holds ((( len g ) = m & (for i being Nat holds (i in ( dom g ) implies ( g . i ) = a))) iff g = ( m |-> a )))))
proof
let a being (Element of D);
let m being non empty Nat;
let g being (FinSequence of D);
hereby
assume that
A1: ( len g ) = m
and
A2: (for i being Nat holds (i in ( dom g ) implies ( g . i ) = a));
(( dom g ) = ( dom ( m |-> a ) ) & (for i being Nat holds (i in ( dom g ) implies ( g . i ) = ( ( m |-> a ) . i ))))
proof
thus ( dom g ) = ( Seg m ) by A1,FINSEQ_1:def 3
.= ( dom ( m |-> a ) ) by FUNCOP_1:13;
let i being Nat;
assume that
A3: i in ( dom g );
A4: i in ( Seg m ) by A1,A3,FINSEQ_1:def 3;
thus ( g . i ) = a by A2,A3
.= ( ( m |-> a ) . i ) by A4,FINSEQ_2:57;
end;
hence g = ( m |-> a ) by FINSEQ_1:13;
end;
assume A5: g = ( m |-> a );
then ( dom g ) = ( Seg m ) by FUNCOP_1:13;
hence thesis by A5,CARD_1:def 7,FINSEQ_2:57;
end;
