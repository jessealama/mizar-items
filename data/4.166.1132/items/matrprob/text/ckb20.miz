requirements REAL,NUMERALS,BOOLE,SUBSET,ARITHM;
begin
reserve D for non  empty set;
reserve i for (Element of ( NAT ));
reserve n for Nat;
reserve m for Nat;
theorem
Th18: (for e1 being (FinSequence of D) holds (( len e1 ) = m implies ( n |-> e1 ) is (Matrix of n,m,D)))
proof
let e1 being (FinSequence of D);
assume that
A1: ( len e1 ) = m;
reconsider e = ( n |-> e1 ) as (FinSequence of ( D * )) by CKB10:1;
A2: ( len e ) = n by CARD_1:def 7;
A3: (for i holds (i in ( dom e ) implies ( len ( e . i ) ) = m))
proof
let i;
assume i in ( dom e );
then i in ( Seg n ) by A2,FINSEQ_1:def 3;
hence thesis by A1,FUNCOP_1:7;
end;
then reconsider e as (Matrix of D) by CKB13:1;
(for p being (FinSequence of D) holds (p in ( rng e ) implies ( len p ) = m))
proof
let p being (FinSequence of D);
assume p in ( rng e );
then (ex i being set st (i in ( dom e ) & p = ( e . i ))) by FUNCT_1:def 3;
hence thesis by A3;
end;
hence thesis by A2,MATRIX_1:def 2;
end;
