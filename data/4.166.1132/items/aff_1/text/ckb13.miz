environ
vocabularies DIRAF,SUBSET_1,ANALOAF,INCSP_1,STRUCT_0,TARSKI,AFF_1;
notations TARSKI,STRUCT_0,ANALOAF,DIRAF,CKB1;
definitions CKB1;
theorems DIRAF,TARSKI,XBOOLE_0,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12;
schemes SUBSET_1;
registrations STRUCT_0;
constructors DIRAF,CKB1;
requirements SUBSET,BOOLE;
begin
reserve AS for AffinSpace;
reserve x for (Element of AS);
reserve y for (Element of AS);
reserve z for (Element of AS);
reserve t for (Element of AS);
reserve u for (Element of AS);
theorem
Th17: ((((x <> y &  LIN x,y,z) &  LIN x,y,t) &  LIN x,y,u) implies  LIN z,t,u)
proof
assume that
A1: x <> y
and
A2:  LIN x,y,z
and
A3:  LIN x,y,t
and
A4:  LIN x,y,u;
A5:now
A6: x,y // x,z by A2,CKB1:def 1;
x,y // x,u by A4,CKB1:def 1;
then x,z // x,u by A1,A6,CKB9:1;
then A7: z,x // z,u by DIRAF:40;
x,y // x,t by A3,CKB1:def 1;
then x,z // x,t by A1,A6,CKB9:1;
then A8: z,x // z,t by DIRAF:40;
assume x <> z;
then z,t // z,u by A8,A7,CKB9:1;
hence thesis by CKB1:def 1;
end;
now
assume A9: x = z;
then A10: z,y // z,u by A4,CKB1:def 1;
z,y // z,t by A3,A9,CKB1:def 1;
then z,t // z,u by A1,A9,A10,CKB9:1;
hence thesis by CKB1:def 1;
end;
hence thesis by A5;
end;
