environ
vocabularies DIRAF,SUBSET_1,ANALOAF,INCSP_1,STRUCT_0,TARSKI,AFF_1;
notations TARSKI,STRUCT_0,ANALOAF,DIRAF,CKB1;
definitions CKB1;
theorems DIRAF,TARSKI,XBOOLE_0,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10;
schemes SUBSET_1;
registrations STRUCT_0;
constructors DIRAF,CKB1;
requirements SUBSET,BOOLE;
begin
reserve AS for AffinSpace;
reserve x for (Element of AS);
reserve y for (Element of AS);
reserve z for (Element of AS);
theorem
Th15: ( LIN x,y,z implies (((( LIN x,z,y &  LIN y,x,z) &  LIN y,z,x) &  LIN z,x,y) &  LIN z,y,x))
proof
assume  LIN x,y,z;
hence ( LIN x,z,y &  LIN y,x,z) by CKB10:1;
hence ( LIN y,z,x &  LIN z,x,y) by CKB10:1;
hence thesis by CKB10:1;
end;
