environ
vocabularies DIRAF,SUBSET_1,ANALOAF,INCSP_1,STRUCT_0,TARSKI,AFF_1;
notations TARSKI,STRUCT_0,ANALOAF,DIRAF,CKB1,CKB20,CKB22,CKB26,CKB33,CKB34,CKB55,CKB57;
definitions CKB1,CKB20,CKB26,CKB33,CKB34;
theorems DIRAF,TARSKI,XBOOLE_0,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB23,CKB24,CKB25,CKB26,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB37,CKB38,CKB39,CKB40,CKB41,CKB42,CKB43,CKB44,CKB45,CKB46,CKB47,CKB48,CKB49,CKB50,CKB51,CKB52,CKB53,CKB54,CKB56,CKB58,CKB59,CKB60,CKB61,CKB62,CKB63,CKB64,CKB65,CKB66,CKB67,CKB68,CKB69,CKB70,CKB71;
schemes SUBSET_1;
registrations STRUCT_0,CKB27;
constructors DIRAF,CKB1,CKB20,CKB22,CKB26,CKB33,CKB34,CKB55,CKB57;
requirements SUBSET,BOOLE;
begin
reserve AS for AffinSpace;
reserve a for (Element of AS);
reserve a9 for (Element of AS);
reserve b for (Element of AS);
reserve b9 for (Element of AS);
reserve o for (Element of AS);
reserve x for (Element of AS);
theorem
(((((((not  LIN o,a,b) &  LIN o,a,a9) &  LIN o,b,b9) &  LIN o,b,x) & a,b // a9,b9) & a,b // a9,x) implies b9 = x)
proof
assume that
A1: (not  LIN o,a,b)
and
A2:  LIN o,a,a9
and
A3:  LIN o,b,b9
and
A4:  LIN o,b,x
and
A5: a,b // a9,b9
and
A6: a,b // a9,x;
set A = ( Line (o,a) );
set C = ( Line (o,b) );
set P = ( Line (a9,b9) );
A7: a9 in P by CKB23:1;
assume A8: b9 <> x;
A9: a9 <> b9
proof
assume A10: a9 = b9;
then a9 = o by A1,A2,A3,CKB70:1;
hence contradiction by A1,A4,A6,A8,A10,CKB71:1;
end;
then A11: P is  being_line by CKB26:def 1;
A12: o <> b by A1,CKB12:1;
then A13: C is  being_line by CKB26:def 1;
A14: b9 in P by CKB23:1;
a <> b by A1,CKB12:1;
then a9,b9 // a9,x by A5,A6,CKB9:1;
then  LIN a9,b9,x by CKB1:def 1;
then A15: x in P by A9,A11,A7,A14,CKB38:1;
A16: b in C by CKB23:1;
A17: o in C by CKB23:1;
then A18: x in C by A4,A12,A13,A16,CKB38:1;
b9 in C by A3,A12,A13,A17,A16,CKB38:1;
then A19: a9 in C by A8,A13,A11,A7,A14,A18,A15,CKB29:1;
A20: o <> a by A1,CKB12:1;
then A21: A is  being_line by CKB26:def 1;
A22: a9 <> o
proof
assume A23: a9 = o;
then b9 = o by A1,A3,A5,CKB71:1;
hence contradiction by A1,A4,A6,A8,A23,CKB71:1;
end;
A24: o in A by CKB23:1;
A25: a in A by CKB23:1;
then a9 in A by A2,A20,A21,A24,CKB38:1;
then b in A by A22,A21,A13,A24,A17,A16,A19,CKB29:1;
hence contradiction by A1,A21,A24,A25,CKB32:1;
end;
