notations TARSKI,XBOOLE_0,SUBSET_1,ORDINAL1,NUMBERS,XXREAL_0,XREAL_0,REAL_1,SQUARE_1,FUNCT_1,RELSET_1,XXREAL_2,STRUCT_0,PRE_TOPC,COMPTS_1,TBSP_1,TOPMETR,METRIC_1,SEQ_4,PCOMPS_1,EUCLID,PSCOMP_1,WEIERSTR;
definitions XBOOLE_0,TARSKI,SEQ_4,EUCLID,STRUCT_0,XXREAL_2;
theorems METRIC_1,TOPMETR,GOBOARD6,PRE_TOPC,FUNCT_2,XBOOLE_0,FUNCT_1,TARSKI,WEIERSTR,SEQ_4,TBSP_1,JORDAN1K,COMPTS_1,PCOMPS_1,XREAL_1,XXREAL_0,XXREAL_2,EUCLID,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18;
registrations SUBSET_1,RELSET_1,NUMBERS,XREAL_0,MEMBERED,STRUCT_0,PRE_TOPC,METRIC_1,PCOMPS_1,EUCLID,TOPMETR,BORSUK_2,WAYBEL_2,TBSP_1,CKB1;
constructors REAL_1,SQUARE_1,SEQ_4,CONNSP_1,COMPTS_1,TBSP_1,EUCLID,WEIERSTR,XXREAL_2,FUNCSDOM,BINOP_2,RVSUM_1,PSCOMP_1;
requirements SUBSET,BOOLE;
begin
theorem
(for M being non  empty MetrSpace holds (for B being (Subset of M) holds (for A being (Subset of ( TopSpaceMetr M )) holds ((A = B & A is  compact) implies B is  bounded))))
proof
let M being non  empty MetrSpace;
let B being (Subset of M);
let A being (Subset of ( TopSpaceMetr M ));
set TA = ( TopSpaceMetr M );
assume that
A1: A = B
and
A2: A is  compact;
A c= (the carrier of ( TA | A )) by PRE_TOPC:8;
then reconsider A2 = A as (Subset of ( TA | A ));
per cases ;
suppose A <> ( {} );

then reconsider A1 = A as non  empty (Subset of M) by TOPMETR:12;
( [#] ( TA | A ) ) = A2 by PRE_TOPC:def 5;
then ( [#] ( TA | A ) ) is  compact by A2,COMPTS_1:2;
then A3: ( TA | A ) is  compact by COMPTS_1:1;
( TopSpaceMetr ( M | A1 ) ) = ( TA | A ) by CKB17:1;
then ( M | A1 ) is  totally_bounded by A3,TBSP_1:9;
then ( M | A1 ) is  bounded by TBSP_1:19;
then A4: ( [#] ( M | A1 ) ) is  bounded;
( [#] ( M | A1 ) ) = (the carrier of ( M | A1 ))
.= A1 by TOPMETR:def 2;
hence thesis by A1,A4,CKB18:1;
end;
suppose A = ( {} );

then A = ( {} M );
hence thesis by A1;
end;
end;
