theorem
Th16: (for A being set holds (for B being non empty set holds (for b being (Element of B) holds (for f being (bifunction of A,B) holds (f = ( [: A,A :] --> [ b,b ] ) implies f is  Covariant  Contravariant)))))
proof
let A being set;
let B being non empty set;
let b being (Element of B);
let f being (bifunction of A,B);
assume that
A1: f = ( [: A,A :] --> [ b,b ] );
reconsider g = ( A --> b ) as (Function of A,B);
thus f is  Covariant
proof
take g;
thus thesis by A1,ALTCAT_2:1;
end;

take g;
( [: A,A :] --> [ b,b ] ) = ( ~ ( [: A,A :] --> [ b,b ] ) ) by CKB10:1;
hence thesis by A1,ALTCAT_2:1;
end;
