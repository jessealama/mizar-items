begin
registration
let A being set;
let B being set;
cluster  Covariant  Contravariant for (bifunction of A,B);
existence
proof
per cases ;
suppose A1: B = ( {} );

then [: B,B :] = ( {} ) by ZFMISC_1:90;
then reconsider f = ( {} ) as (bifunction of A,B) by CKB2:1;
take f;
reconsider g = ( {} ) as (Function of A,B) by A1,CKB2:1;
reconsider h = g as  empty Function;
thus f is  Covariant
proof
take g;
thus f = [: h,h :]
.= [: g,g :];
end;

take g;
thus f = ( ~ [: h,h :] )
.= ( ~ [: g,g :] );
end;
suppose A2: B <> ( {} );

set b = the (Element of B);
set f = ( [: A,A :] --> [ b,b ] );
[ b,b ] in [: B,B :] by A2,ZFMISC_1:87;
then reconsider f as (bifunction of A,B) by FUNCOP_1:45;
take f;
thus thesis by A2,CKB24:1;
end;
