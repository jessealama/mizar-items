begin
theorem
Th20: (for I1 being set holds (for I2,I3 being non empty set holds (for f being (Function of I1,I2) holds (for g being (Function of I2,I3) holds (for A being (ManySortedSet of I1) holds (for B being (ManySortedSet of I2) holds (for C being (ManySortedSet of I3) holds (for F being (MSUnTrans of f,A,B) holds (for G being (MSUnTrans of ( g * f ),( B * f ),C) holds ((for ii being set holds ((ii in I1 & ( ( B * f ) . ii ) = ( {} )) implies (( A . ii ) = ( {} ) or ( ( C * ( g * f ) ) . ii ) = ( {} )))) implies ( G ** (F qua  Function-yielding Function) ) is (MSUnTrans of ( g * f ),A,C)))))))))))
proof
let I1 being set;
let I2 being non empty set,I3 being non empty set;
let f being (Function of I1,I2);
let g being (Function of I2,I3);
let A being (ManySortedSet of I1);
let B being (ManySortedSet of I2);
let C being (ManySortedSet of I3);
let F being (MSUnTrans of f,A,B);
let G being (MSUnTrans of ( g * f ),( B * f ),C);
assume that
A1: (for ii being set holds ((ii in I1 & ( ( B * f ) . ii ) = ( {} )) implies (( A . ii ) = ( {} ) or ( ( C * ( g * f ) ) . ii ) = ( {} ))));
reconsider G as (ManySortedFunction of ( B * f ),( C * ( g * f ) )) by CKB28:def 1;
reconsider F as (ManySortedFunction of A,( B * f )) by CKB28:def 1;
A2: ( dom G ) = I1 by PARTFUN1:def 2;
A3: ( dom F ) = I1 by PARTFUN1:def 2;
A4: ( dom ( G ** F ) ) = ( ( dom G ) /\ ( dom F ) ) by PBOOLE:def 19
.= I1 by A2,A3;
reconsider GF = ( G ** F ) as (ManySortedSet of I1);
GF is (ManySortedFunction of A,( C * ( g * f ) ))
proof
let ii being set;
assume A5: ii in I1;
then reconsider Fi = ( F . ii ) as (Function of ( A . ii ),( ( B * f ) . ii )) by PBOOLE:def 15;
reconsider Gi = ( G . ii ) as (Function of ( ( B * f ) . ii ),( ( C * ( g * f ) ) . ii )) by A5,PBOOLE:def 15;
(( ( B * f ) . ii ) = ( {} ) implies (( A . ii ) = ( {} ) or ( ( C * ( g * f ) ) . ii ) = ( {} ))) by A1,A5;
then ( Gi * Fi ) is (Function of ( A . ii ),( ( C * ( g * f ) ) . ii )) by FUNCT_2:13;
hence thesis by A4,A5,PBOOLE:def 19;
end;
hence thesis by CKB28:def 1;
end;
