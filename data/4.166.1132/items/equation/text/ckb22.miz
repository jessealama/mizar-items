begin
reserve S for non  empty non  void ManySortedSign;
definition
let S being non  void non  empty ManySortedSign;
let A being MSAlgebra over S;
let B being (MSSubAlgebra of A);
func SuperAlgebraSet B means 
:Def2: (for x being set holds (x in it iff (ex C being  strict (MSSubAlgebra of A) st (x = C & B is (MSSubAlgebra of C)))));
existence
proof
defpred P[ set ]
 means
(ex C being  strict (MSSubAlgebra of A) st ($1 = C & B is (MSSubAlgebra of C)));
consider IT being set such that A1: (for x being set holds (x in IT iff (x in ( MSSub A ) & P[ x ]))) from XBOOLE_0:sch 1;
take IT;
let x being set;
thus (x in IT implies (ex C being  strict (MSSubAlgebra of A) st (x = C & B is (MSSubAlgebra of C)))) by A1;
given C being  strict (MSSubAlgebra of A) such that
A2: x = C
and
A3: B is (MSSubAlgebra of C);

x in ( MSSub A ) by A2,MSUALG_2:def 19;
hence thesis by A1,A2,A3;
end;
uniqueness
proof
defpred P[ set ]
 means
(ex C being  strict (MSSubAlgebra of A) st ($1 = C & B is (MSSubAlgebra of C)));
(for X1,X2 being set holds (((for x being set holds (x in X1 iff P[ x ])) & (for x being set holds (x in X2 iff P[ x ]))) implies X1 = X2)) from XBOOLE_0:sch 3;
hence thesis;
end;
end;
