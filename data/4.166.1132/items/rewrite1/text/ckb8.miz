begin
reserve p for FinSequence;
definition
let R being Relation;
mode RedSequence of R
 -> FinSequence
means :Def2: (( len it ) > ( 0 ) & (for i being (Element of ( NAT )) holds ((i in ( dom it ) & ( i + 1 ) in ( dom it )) implies [ ( it . i ),( it . ( i + 1 ) ) ] in R)));
existence
proof
take p = <* ( {} ) *>;
thus ( len p ) > ( 0 );
let i being (Element of ( NAT ));
assume that
A1: i in ( dom p )
and
A2: ( i + 1 ) in ( dom p );
A3: ( dom p ) = { 1 } by FINSEQ_1:2,FINSEQ_1:38;
then i = 1 by A1,TARSKI:def 1;
hence thesis by A3,A2,TARSKI:def 1;
end;
