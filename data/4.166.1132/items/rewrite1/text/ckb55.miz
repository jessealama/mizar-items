begin
theorem
Th43: (for R being Relation holds (for a,b,c being set holds (((R reduces a,b & b,c are_convergent_wrt R) or (a,b are_convergent_wrt R & R reduces c,b)) implies a,c are_convergent_wrt R)))
proof
let R being Relation;
let a being set;
let b being set;
let c being set;
assume A1: ((R reduces a,b & b,c are_convergent_wrt R) or (a,b are_convergent_wrt R & R reduces c,b));
per cases  by A1;
suppose A2: (R reduces a,b & b,c are_convergent_wrt R);

then consider d being set such that A3: R reduces b,d and A4: R reduces c,d by CKB44:def 1;
R reduces a,d by A2,A3,CKB22:1;
hence thesis by A4,CKB44:def 1;
end;
suppose A5: (a,b are_convergent_wrt R & R reduces c,b);

then consider d being set such that A6: R reduces a,d and A7: R reduces b,d by CKB44:def 1;
R reduces c,d by A5,A7,CKB22:1;
hence thesis by A6,CKB44:def 1;
end;
end;
