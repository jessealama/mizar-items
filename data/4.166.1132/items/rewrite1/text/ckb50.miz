begin
theorem
Th38: (for R being Relation holds (for a,b being set holds ((a,b are_convergent_wrt R or a,b are_divergent_wrt R) implies a,b are_convertible_wrt R)))
proof
let R being Relation;
let a being set;
let b being set;
assume A1: (a,b are_convergent_wrt R or a,b are_divergent_wrt R);
per cases  by A1;
suppose a,b are_convergent_wrt R;

then consider c being set such that A2: (R reduces a,c & R reduces b,c) by CKB44:def 1;
(a,c are_convertible_wrt R & c,b are_convertible_wrt R) by A2,CKB32:1;
hence thesis by CKB37:1;
end;
suppose a,b are_divergent_wrt R;

then consider c being set such that A3: (R reduces c,a & R reduces c,b) by CKB45:def 1;
(c,b are_convertible_wrt R & a,c are_convertible_wrt R) by A3,CKB32:1;
hence thesis by CKB37:1;
end;
end;
