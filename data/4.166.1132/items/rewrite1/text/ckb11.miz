begin
Lm1:now
let p being FinSequence;
let x being Nat;
assume x in ( dom p );
then A1: x in ( Seg ( len p ) ) by FINSEQ_1:def 3;
hence (x <= ( len p ) & x >= ( ( 0 ) + 1 )) by FINSEQ_1:1;
thus x > ( 0 ) by A1,FINSEQ_1:1;
end;
theorem
Th8: (for R being Relation holds (for a,b being set holds ([ a,b ] in R implies <* a,b *> is (RedSequence of R))))
proof
let R being Relation;
let a being set;
let b being set;
assume A1: [ a,b ] in R;
set p = <* a,b *>;
thus ( len p ) > ( 0 );
let i being (Element of ( NAT ));
assume that
A2: i in ( dom p )
and
A3: ( i + 1 ) in ( dom p );
( len p ) = ( 1 + 1 ) by FINSEQ_1:44;
then ( i + 1 ) <= ( 1 + 1 ) by A3,Lm1;
then A4: i <= 1 by XREAL_1:6;
i >= 1 by A2,Lm1;
then (( p . 1 ) = a & i = 1) by A4,FINSEQ_1:44,XXREAL_0:1;
hence thesis by A1,FINSEQ_1:44;
end;
