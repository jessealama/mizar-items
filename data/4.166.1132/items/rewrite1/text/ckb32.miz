begin
reserve p for FinSequence;
theorem
Th26: (for R being Relation holds (for a,b being set holds (R reduces a,b implies (a,b are_convertible_wrt R & b,a are_convertible_wrt R))))
proof
let R being Relation;
let a being set;
let b being set;
given p being (RedSequence of R) such that
A1: (( p . 1 ) = a & ( p . ( len p ) ) = b);

p is (RedSequence of ( R \/ ( R ~ ) ))
proof
thus ( len p ) > ( 0 );
let i being (Element of ( NAT ));
assume (i in ( dom p ) & ( i + 1 ) in ( dom p ));
then [ ( p . i ),( p . ( i + 1 ) ) ] in R by CKB8:def 1;
hence thesis by XBOOLE_0:def 3;
end;
then ( R \/ ( R ~ ) ) reduces a,b by A1,CKB15:def 1;
hence a,b are_convertible_wrt R by CKB16:def 1;
hence thesis by CKB31:1;
end;
