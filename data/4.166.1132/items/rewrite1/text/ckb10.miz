begin
theorem
Th7: (for R being Relation holds (for a being set holds <* a *> is (RedSequence of R)))
proof
let R being Relation;
let a being set;
set p = <* a *>;
thus ( len p ) > ( 0 );
let i being (Element of ( NAT ));
assume that
A1: i in ( dom p )
and
A2: ( i + 1 ) in ( dom p );
A3: ( dom p ) = { 1 } by FINSEQ_1:2,FINSEQ_1:38;
then i = 1 by A1,TARSKI:def 1;
hence thesis by A3,A2,TARSKI:def 1;
end;
