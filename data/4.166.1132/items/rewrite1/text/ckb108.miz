begin
Lm1:now
let p being FinSequence;
let x being Nat;
assume x in ( dom p );
then A1: x in ( Seg ( len p ) ) by FINSEQ_1:def 3;
hence (x <= ( len p ) & x >= ( ( 0 ) + 1 )) by FINSEQ_1:1;
thus x > ( 0 ) by A1,FINSEQ_1:1;
end;
Lm2:now
let p being FinSequence;
let x being Nat;
assume ( x + 1 ) in ( dom p );
then ( x + 1 ) <= ( len p ) by Lm1;
hence x < ( len p ) by NAT_1:13;
end;
Lm3:now
let p being FinSequence;
let x being Nat;
assume that
A1: (x <= ( len p ) or x < ( len p ))
and
A2: (x >= 1 or x > ( 0 ));
x >= ( ( 0 ) + 1 ) by A2,NAT_1:13;
then x in ( Seg ( len p ) ) by A1,FINSEQ_1:1;
hence x in ( dom p ) by FINSEQ_1:def 3;
end;
reserve p for FinSequence;
reserve x for set;
reserve y for set;
theorem
Th63: (for R being Relation holds (ex Q being  complete Relation st (( field Q ) c= ( field R ) & (for a,b being set holds (a,b are_convertible_wrt R iff a,b are_convergent_wrt Q)))))
proof
let R being Relation;
per cases ;
suppose A1: R = ( {} );

take E = ( {} );
thus ( field E ) c= ( field R ) by XBOOLE_1:2;
let a being set;
let b being set;
(a,b are_convertible_wrt R iff a = b) by A1,CKB33:1,CKB34:1;
hence thesis by CKB51:1,CKB52:1;
end;
suppose R <> ( {} );

then reconsider R9 = R as non  empty Relation;
set xx = the (Element of R9);
consider x1,x2 being set such that A2: xx = [ x1,x2 ] by RELAT_1:def 1;
defpred P[ set,set ]
 means
$1,$2 are_convertible_wrt R;
A3: (for x,y being set holds (P[ x,y ] implies P[ y,x ])) by CKB31:1;
A4: (for x,y,z being set holds ((P[ x,y ] & P[ y,z ]) implies P[ x,z ])) by CKB37:1;
A5: (for x holds (x in ( field R ) implies P[ x,x ])) by CKB33:1;
consider Q being (Equivalence_Relation of ( field R )) such that A6: (for x holds (for y holds ([ x,y ] in Q iff ((x in ( field R ) & y in ( field R )) & P[ x,y ])))) from EQREL_1:sch 1(A5,A3,A4);
A7: ((for X being set holds (X in ( Class Q ) implies X <> ( {} ))) & (for X,Y being set holds (((X in ( Class Q ) & Y in ( Class Q )) & X <> Y) implies X misses Y))) by EQREL_1:def 4;
x1 in ( field R ) by A2,RELAT_1:15;
then ( Class (Q,x1) ) in ( Class Q ) by EQREL_1:def 3;
then consider X being set such that A8: (for A being set holds (A in ( Class Q ) implies (ex x st ( X /\ A ) = { x }))) by A7,WELLORD2:18;
defpred Z[ set,set ]
 means
(($1 <> $2 & $1,$2 are_convertible_wrt R) & $2 in X);
consider P being Relation such that A9: (for x holds (for y holds ([ x,y ] in P iff ((x in ( field R ) & y in ( field R )) & Z[ x,y ])))) from RELAT_1:sch 1;
A10: P is  locally-confluent
proof
let a being set;
let b being set;
let c being set;
assume that
A11: [ a,b ] in P
and
A12: [ a,c ] in P;
A13: a in ( field R ) by A9,A11;
then ( Class (Q,a) ) in ( Class Q ) by EQREL_1:def 3;
then consider x such that A14: ( X /\ ( Class (Q,a) ) ) = { x } by A8;
(c in ( field R ) & a,c are_convertible_wrt R) by A9,A12;
then [ a,c ] in Q by A6,A13;
then [ c,a ] in Q by EQREL_1:6;
then A15: c in ( Class (Q,a) ) by EQREL_1:19;
c in X by A9,A12;
then c in { x } by A15,A14,XBOOLE_0:def 4;
then A16: c = x by TARSKI:def 1;
(b in ( field R ) & a,b are_convertible_wrt R) by A9,A11;
then [ a,b ] in Q by A6,A13;
then [ b,a ] in Q by EQREL_1:6;
then A17: b in ( Class (Q,a) ) by EQREL_1:19;
take b;
b in X by A9,A11;
then b in { x } by A17,A14,XBOOLE_0:def 4;
then b = x by TARSKI:def 1;
hence thesis by A16,CKB18:1;
end;
A18: (for x holds (for y holds (P reduces x,y implies x,y are_convertible_wrt R)))
proof
let x;
let y;
given p being (RedSequence of P) such that
A19: x = ( p . 1 )
and
A20: y = ( p . ( len p ) );

defpred Z[ (Element of ( NAT )) ]
 means
($1 in ( dom p ) implies x,( p . $1 ) are_convertible_wrt R);
now
let i being (Element of ( NAT ));
assume that
A21: (i in ( dom p ) implies x,( p . i ) are_convertible_wrt R)
and
A22: ( i + 1 ) in ( dom p );
A23: i < ( len p ) by A22,Lm2;
per cases ;
suppose i = ( 0 );

hence x,( p . ( i + 1 ) ) are_convertible_wrt R by A19,CKB33:1;
end;
suppose A24: i > ( 0 );

then i in ( dom p ) by A23,Lm3;
then [ ( p . i ),( p . ( i + 1 ) ) ] in P by A22,CKB8:def 1;
then ( p . i ),( p . ( i + 1 ) ) are_convertible_wrt R by A9;
hence x,( p . ( i + 1 ) ) are_convertible_wrt R by A21,A23,A24,Lm3,CKB37:1;
end;
end;
then A25: (for k being (Element of ( NAT )) holds (Z[ k ] implies Z[ ( k + 1 ) ]));
A26: ( len p ) in ( dom p ) by FINSEQ_5:6;
A27: Z[ ( 0 ) ] by Lm1;
(for i being (Element of ( NAT )) holds Z[ i ]) from NAT_1:sch 1(A27,A25);
hence thesis by A20,A26;
end;
P is  strongly-normalizing
proof
let f being (ManySortedSet of ( NAT ));
per cases ;
suppose (not [ ( f . ( 0 ) ),( f . ( ( 0 ) + 1 ) ) ] in P);

hence thesis;
end;
suppose A28: [ ( f . ( 0 ) ),( f . ( ( 0 ) + 1 ) ) ] in P;

take j = ( ( 0 ) + 1 );
A29: ( f . j ) in X by A9,A28;
assume A30: [ ( f . j ),( f . ( j + 1 ) ) ] in P;
then A31: ( f . j ) in ( field R ) by A9;
then ( Class (Q,( f . j )) ) in ( Class Q ) by EQREL_1:def 3;
then consider x such that A32: ( X /\ ( Class (Q,( f . j )) ) ) = { x } by A8;
(( f . ( j + 1 ) ) in ( field R ) & ( f . j ),( f . ( j + 1 ) ) are_convertible_wrt R) by A9,A30;
then [ ( f . j ),( f . ( j + 1 ) ) ] in Q by A6,A31;
then [ ( f . ( j + 1 ) ),( f . j ) ] in Q by EQREL_1:6;
then A33: ( f . ( j + 1 ) ) in ( Class (Q,( f . j )) ) by EQREL_1:19;
( f . j ) in ( Class (Q,( f . j )) ) by A31,EQREL_1:20;
then ( f . j ) in ( X /\ ( Class (Q,( f . j )) ) ) by A29,XBOOLE_0:def 4;
then A34: ( f . j ) = x by A32,TARSKI:def 1;
( f . ( j + 1 ) ) in X by A9,A30;
then ( f . ( j + 1 ) ) in ( X /\ ( Class (Q,( f . j )) ) ) by A33,XBOOLE_0:def 4;
then ( f . ( j + 1 ) ) = x by A32,TARSKI:def 1;
hence contradiction by A9,A30,A34;
end;
end;
then reconsider P as  strongly-normalizing  locally-confluent Relation by A10;
take P;
thus ( field P ) c= ( field R )
proof
let x;
assume x in ( field P );
then (x in ( dom P ) or x in ( rng P )) by XBOOLE_0:def 3;
then ((ex y st [ x,y ] in P) or (ex y st [ y,x ] in P)) by RELAT_1:def 4,RELAT_1:def 5;
hence thesis by A9;
end;

let a being set;
let b being set;
thus thesis
proof
per cases ;
suppose a = b;

hence thesis by CKB33:1,CKB51:1;
end;
suppose A35: a <> b;

hereby
assume A36: a,b are_convertible_wrt R;
then A37: b in ( field R ) by A35,CKB39:1;
then ( Class (Q,b) ) in ( Class Q ) by EQREL_1:def 3;
then consider x such that A38: ( X /\ ( Class (Q,b) ) ) = { x } by A8;
A39: a in ( field R ) by A35,A36,CKB39:1;
then A40: [ a,b ] in Q by A6,A36,A37;
thus a,b are_convergent_wrt P
proof
take x;
A41: x in { x } by TARSKI:def 1;
then A42: x in X by A38,XBOOLE_0:def 4;
A43: x in ( Class (Q,b) ) by A38,A41,XBOOLE_0:def 4;
then [ x,b ] in Q by EQREL_1:19;
then [ b,x ] in Q by EQREL_1:6;
then b,x are_convertible_wrt R by A6;
then A44: (b = x or [ b,x ] in P) by A9,A37,A38,A41,A42;
a in ( Class (Q,b) ) by A40,EQREL_1:19;
then [ a,x ] in Q by A43,EQREL_1:22;
then a,x are_convertible_wrt R by A6;
then (a = x or [ a,x ] in P) by A9,A39,A38,A41,A42;
hence thesis by A44,CKB18:1,CKB21:1;
end;

end;
given c being set such that
A45: (P reduces a,c & P reduces b,c);

(a,c are_convertible_wrt R & c,b are_convertible_wrt R) by A18,A45,CKB31:1;
hence thesis by CKB37:1;
end;
end;

