begin
reserve p for FinSequence;
theorem
Th11: (for R,Q being Relation holds (R c= Q implies (for p being (RedSequence of R) holds p is (RedSequence of Q))))
proof
let R being Relation;
let Q being Relation;
assume that
A1: R c= Q;
let p being (RedSequence of R);
thus ( len p ) > ( 0 );
let i being (Element of ( NAT ));
assume (i in ( dom p ) & ( i + 1 ) in ( dom p ));
then [ ( p . i ),( p . ( i + 1 ) ) ] in R by CKB8:def 1;
hence thesis by A1;
end;
