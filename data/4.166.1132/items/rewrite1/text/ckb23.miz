begin
Lm1:now
let p being FinSequence;
let x being Nat;
assume x in ( dom p );
then A1: x in ( Seg ( len p ) ) by FINSEQ_1:def 3;
hence (x <= ( len p ) & x >= ( ( 0 ) + 1 )) by FINSEQ_1:1;
thus x > ( 0 ) by A1,FINSEQ_1:1;
end;
Lm2:now
let p being FinSequence;
let x being Nat;
assume ( x + 1 ) in ( dom p );
then ( x + 1 ) <= ( len p ) by Lm1;
hence x < ( len p ) by NAT_1:13;
end;
Lm3:now
let p being FinSequence;
let x being Nat;
assume that
A1: (x <= ( len p ) or x < ( len p ))
and
A2: (x >= 1 or x > ( 0 ));
x >= ( ( 0 ) + 1 ) by A2,NAT_1:13;
then x in ( Seg ( len p ) ) by A1,FINSEQ_1:1;
hence x in ( dom p ) by FINSEQ_1:def 3;
end;
reserve p for FinSequence;
theorem
Th18: (for R being Relation holds (for p being (RedSequence of R) holds (for i,j being (Element of ( NAT )) holds (((i in ( dom p ) & j in ( dom p )) & i <= j) implies R reduces ( p . i ),( p . j )))))
proof
let R being Relation;
let p being (RedSequence of R);
let i being (Element of ( NAT ));
let j being (Element of ( NAT ));
defpred Q[ (Element of ( NAT )) ]
 means
(( i + $1 ) in ( dom p ) implies R reduces ( p . i ),( p . ( i + $1 ) ));
assume that
A1: i in ( dom p )
and
A2: j in ( dom p )
and
A3: i <= j;
consider k being Nat such that A4: j = ( i + k ) by A3,NAT_1:10;
now
A5: i >= 1 by A1,Lm1;
let j being (Element of ( NAT ));
assume that
A6: (( i + j ) in ( dom p ) implies R reduces ( p . i ),( p . ( i + j ) ))
and
A7: ( i + ( j + 1 ) ) in ( dom p );
A8: ( i + ( j + 1 ) ) = ( ( i + j ) + 1 );
then A9: ( i + j ) < ( len p ) by A7,Lm2;
then ( i + j ) in ( dom p ) by A5,Lm3;
then [ ( p . ( i + j ) ),( p . ( i + ( j + 1 ) ) ) ] in R by A7,A8,CKB8:def 1;
then R reduces ( p . ( i + j ) ),( p . ( i + ( j + 1 ) ) ) by CKB21:1;
hence R reduces ( p . i ),( p . ( i + ( j + 1 ) ) ) by A6,A5,A9,Lm3,CKB22:1;
end;
then A10: (for k being (Element of ( NAT )) holds (Q[ k ] implies Q[ ( k + 1 ) ]));
A11: Q[ ( 0 ) ] by CKB18:1;
A12: (for j being (Element of ( NAT )) holds Q[ j ]) from NAT_1:sch 1(A11,A10);
k in ( NAT ) by ORDINAL1:def 12;
hence thesis by A2,A12,A4;
end;
