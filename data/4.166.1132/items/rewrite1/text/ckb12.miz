begin
reserve p for FinSequence;
reserve q for FinSequence;
theorem
Th9: (for R being Relation holds (for p,q being (RedSequence of R) holds (( p . ( len p ) ) = ( q . 1 ) implies ( p $^ q ) is (RedSequence of R))))
proof
let R being Relation;
let p being (RedSequence of R);
let q being (RedSequence of R);
defpred P[ set,set ]
 means
[ $1,$2 ] in R;
set r = ( p $^ q );
consider p1 being FinSequence,x being set such that A1: p = ( p1 ^ <* x *> ) by FINSEQ_1:46;
assume ( p . ( len p ) ) = ( q . 1 );
then A2: ((( len p ) > ( 0 ) & ( len q ) > ( 0 )) & ( p . ( len p ) ) = ( q . 1 ));
r = ( p1 ^ q ) by A1,CKB3:1;
hence ( len r ) > ( 0 );
A3: (for i being (Element of ( NAT )) holds ((i in ( dom q ) & ( i + 1 ) in ( dom q )) implies P[ ( q . i ),( q . ( i + 1 ) ) ])) by CKB8:def 1;
A4: (for i being (Element of ( NAT )) holds ((i in ( dom p ) & ( i + 1 ) in ( dom p )) implies P[ ( p . i ),( p . ( i + 1 ) ) ])) by CKB8:def 1;
(for i being (Element of ( NAT )) holds ((i in ( dom ( p $^ q ) ) & ( i + 1 ) in ( dom ( p $^ q ) )) implies (for x,y being set holds ((x = ( ( p $^ q ) . i ) & y = ( ( p $^ q ) . ( i + 1 ) )) implies P[ x,y ])))) from CKB7:sch 1(A4,A3,A2);
hence thesis;
end;
