begin
Lm1:now
let p being FinSequence;
let x being Nat;
assume x in ( dom p );
then A1: x in ( Seg ( len p ) ) by FINSEQ_1:def 3;
hence (x <= ( len p ) & x >= ( ( 0 ) + 1 )) by FINSEQ_1:1;
thus x > ( 0 ) by A1,FINSEQ_1:1;
end;
Lm2:now
let p being FinSequence;
let x being Nat;
assume ( x + 1 ) in ( dom p );
then ( x + 1 ) <= ( len p ) by Lm1;
hence x < ( len p ) by NAT_1:13;
end;
Lm3:now
let p being FinSequence;
let x being Nat;
assume that
A1: (x <= ( len p ) or x < ( len p ))
and
A2: (x >= 1 or x > ( 0 ));
x >= ( ( 0 ) + 1 ) by A2,NAT_1:13;
then x in ( Seg ( len p ) ) by A1,FINSEQ_1:1;
hence x in ( dom p ) by FINSEQ_1:def 3;
end;
reserve p for FinSequence;
theorem
Th50: (for R being Relation holds (R is  subcommutative implies (for a,b,c being set holds ((R reduces a,b & [ a,c ] in R) implies b,c are_convergent_wrt R))))
proof
let R being Relation;
assume A1: R is  subcommutative;
let a being set;
let b being set;
let c being set;
given p being (RedSequence of R) such that
A2: ( p . 1 ) = a
and
A3: ( p . ( len p ) ) = b;

defpred P[ (Element of ( NAT )) ]
 means
($1 in ( dom p ) implies (ex d being set st (([ ( p . $1 ),d ] in R or ( p . $1 ) = d) & R reduces c,d)));
assume A4: [ a,c ] in R;
now
let i being (Element of ( NAT ));
assume that
A5: (i in ( dom p ) implies P[ i ])
and
A6: ( i + 1 ) in ( dom p );
per cases ;
suppose A7: i = ( 0 );

R reduces c,c by CKB18:1;
hence P[ ( i + 1 ) ] by A2,A4,A7;
end;
suppose A8: i > ( 0 );

A9: i < ( len p ) by A6,Lm2;
then consider d being set such that A10: ([ ( p . i ),d ] in R or ( p . i ) = d) and A11: R reduces c,d by A5,A8,Lm3;
i in ( dom p ) by A8,A9,Lm3;
then A12: [ ( p . i ),( p . ( i + 1 ) ) ] in R by A6,CKB8:def 1;
A13:now
assume [ ( p . i ),d ] in R;
then ( p . ( i + 1 ) ),d are_convergent<=1_wrt R by A1,A12,CKB77:def 1;
then consider e being set such that A14: ([ ( p . ( i + 1 ) ),e ] in R or ( p . ( i + 1 ) ) = e) and A15: ([ d,e ] in R or d = e) by CKB46:def 1;
take e;
thus ([ ( p . ( i + 1 ) ),e ] in R or ( p . ( i + 1 ) ) = e) by A14;
R reduces d,e by A15,CKB18:1,CKB21:1;
hence R reduces c,e by A11,CKB22:1;
end;
now
assume ( p . i ) = d;
then R reduces d,( p . ( i + 1 ) ) by A12,CKB21:1;
hence R reduces c,( p . ( i + 1 ) ) by A11,CKB22:1;
end;
hence P[ ( i + 1 ) ] by A10,A13;
end;
end;
then A16: (for k being (Element of ( NAT )) holds (P[ k ] implies P[ ( k + 1 ) ]));
A17: ( len p ) in ( dom p ) by FINSEQ_5:6;
A18: P[ ( 0 ) ] by Lm1;
(for i being (Element of ( NAT )) holds P[ i ]) from NAT_1:sch 1(A18,A16);
then consider d being set such that A19: (([ b,d ] in R or b = d) & R reduces c,d) by A3,A17;
take d;
thus thesis by A19,CKB18:1,CKB21:1;
end;
