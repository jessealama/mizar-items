constructors REAL_1,RCOMP_1,FCONT_1,ZFMISC_1,TOPS_2,TMAP_1,TOPMETR,PCOMPS_1,TOPREAL1,PSCOMP_1,JORDAN5C,JORDAN6,JORDAN7,SEQ_1,COMPLEX1;
requirements NUMERALS,REAL,BOOLE,SUBSET,ARITHM;
begin
theorem
Th13: (for S,T being non  empty TopSpace holds (for S0 being non  empty (SubSpace of S) holds (for T0 being non  empty (SubSpace of T) holds (for f being (Function of S,T) holds (f is  being_homeomorphism implies (for g being (Function of S0,T0) holds ((g = ( f | S0 ) & g is  onto) implies g is  being_homeomorphism)))))))
proof
let S being non  empty TopSpace;
let T being non  empty TopSpace;
let S0 being non  empty (SubSpace of S);
let T0 being non  empty (SubSpace of T);
let f being (Function of S,T);
assume that
A1: f is  being_homeomorphism;
A2: ( rng f ) = ( [#] T ) by A1,TOPS_2:def 5;
A3: ( f " ) is  continuous by A1,TOPS_2:def 5;
let g being (Function of S0,T0);
assume that
A4: g = ( f | S0 )
and
A5: g is  onto;
A6: g = ( f | (the carrier of S0) ) by A4,TMAP_1:def 4;
then A7: ( f .: (the carrier of S0) ) = ( rng g ) by RELAT_1:115
.= (the carrier of T0) by A5,FUNCT_2:def 3;
thus ( dom g ) = ( [#] S0 ) by FUNCT_2:def 1;
thus ( rng g ) = ( [#] T0 ) by A5,FUNCT_2:def 3;
A9: f is  one-to-one by A1,TOPS_2:def 5;
hence A10: g is  one-to-one by A6,FUNCT_1:52;
B1: f is  onto by A2,FUNCT_2:def 3;
f is  continuous by A1,TOPS_2:def 5;
then g is  continuous  continuous (Function of S0,T) by A4;
hence g is  continuous by CKB8:1;
( g " ) = ( ( (f qua Function) | (the carrier of S0) ) " ) by A5,A6,A10,TOPS_2:def 4
.= ( ( (f qua Function) " ) | ( f .: (the carrier of S0) ) ) by A9,RFUNCT_2:17
.= ( ( f " ) | (the carrier of T0) ) by B1,A9,A7,TOPS_2:def 4
.= ( ( f " ) | T0 ) by TMAP_1:def 4;
then ( g " ) is  continuous  continuous (Function of T0,S) by A3;
hence thesis by CKB8:1;
end;
