environ
vocabularies NUMBERS,TOPREAL2,SUBSET_1,EUCLID,PRE_TOPC,ZFMISC_1,TARSKI,XBOOLE_0,JORDAN6,PSCOMP_1,TOPREAL1,JORDAN3,FUNCT_1,ORDINAL2,CONNSP_2,RCOMP_1,RELAT_1,TOPS_2,FUNCT_2,STRUCT_0,BORSUK_1,CARD_1,REAL_1,ARYTM_3,XXREAL_0,XXREAL_1,PCOMPS_1,XREAL_0,ORDINAL1,JGRAPH_2,PARTFUN1,ARYTM_1,TOPMETR;
notations TARSKI,XBOOLE_0,ENUMSET1,SUBSET_1,ORDINAL1,NUMBERS,XCMPLX_0,XXREAL_0,XREAL_0,REAL_1,RELAT_1,FUNCT_1,RELSET_1,PARTFUN1,FUNCT_2,FCONT_1,ZFMISC_1,STRUCT_0,PRE_TOPC,TOPS_2,COMPTS_1,CONNSP_2,PCOMPS_1,RCOMP_1,TMAP_1,EUCLID,PSCOMP_1,TOPMETR,JORDAN6,TOPREAL1,JORDAN5C,TOPREAL2,JORDAN7;
definitions TARSKI,TOPS_2,TOPREAL1,FUNCT_1,XBOOLE_0,BORSUK_1,STRUCT_0;
theorems PRE_TOPC,TOPMETR,XREAL_0,XBOOLE_0,FUNCT_1,XBOOLE_1,FUNCT_2,TOPREAL5,TREAL_1,BORSUK_1,JORDAN6,TOPREAL1,TOPS_2,TARSKI,RELAT_1,TOPREAL3,JORDAN5B,TOPMETR3,TOPREAL2,ENUMSET1,ZFMISC_1,JORDAN5A,JORDAN5C,FCONT_1,FUNCT_3,CONNSP_2,TMAP_1,RFUNCT_2,RELSET_1,FUNCT_4,JORDAN7,SPRECT_1,XCMPLX_0,XCMPLX_1,XREAL_1,XXREAL_0,XXREAL_1,EUCLID,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21;
schemes FUNCT_2;
registrations XBOOLE_0,FUNCT_1,RELSET_1,FUNCT_2,NUMBERS,XXREAL_0,XREAL_0,MEMBERED,STRUCT_0,PRE_TOPC,BORSUK_1,EUCLID,TOPMETR,TOPREAL2,VALUED_0,FCONT_1,TMAP_1;
constructors REAL_1,RCOMP_1,FCONT_1,ZFMISC_1,TOPS_2,TMAP_1,TOPMETR,PCOMPS_1,TOPREAL1,PSCOMP_1,JORDAN5C,JORDAN6,JORDAN7,SEQ_1,COMPLEX1;
requirements NUMERALS,REAL,BOOLE,SUBSET,ARITHM;
begin
reserve C for Simple_closed_curve;
reserve p1 for (Point of ( TOP-REAL 2 ));
reserve p2 for (Point of ( TOP-REAL 2 ));
theorem
(for p1,p2 being (Point of ( TOP-REAL 2 )) holds (for P being (Subset of ( TOP-REAL 2 )) holds ((((P c= C & P is_an_arc_of p1,p2) & ( W-min C ) in P) & ( E-max C ) in P) implies (( Upper_Arc C ) c= P or ( Lower_Arc C ) c= P))))
proof
let p1 being (Point of ( TOP-REAL 2 ));
let p2 being (Point of ( TOP-REAL 2 ));
let P being (Subset of ( TOP-REAL 2 ));
assume that
A1: P c= C
and
A2: P is_an_arc_of p1,p2
and
A3: ( W-min C ) in P
and
A4: ( E-max C ) in P;
reconsider P9 = P as non  empty (Subset of ( TOP-REAL 2 )) by A3;
A5: ( W-min C ) <> ( E-max C ) by TOPREAL5:19;
per cases  by A2,A3,A4,A5,JORDAN5C:14;
suppose A6:  LE ( W-min C ),( E-max C ),P,p1,p2;

set S = ( Segment (P9,p1,p2,( W-min C ),( E-max C )) );
reconsider S9 = S as non  empty (Subset of ( TOP-REAL 2 )) by A6,CKB5:1;
S c= P by CKB2:1;
then S c= C by A1,XBOOLE_1:1;
then (S9 = ( Lower_Arc C ) or S9 = ( Upper_Arc C )) by A2,A5,A6,CKB15:1,CKB21:1;
hence thesis by CKB2:1;
end;
suppose A7:  LE ( E-max C ),( W-min C ),P,p1,p2;

set S = ( Segment (P9,p1,p2,( E-max C ),( W-min C )) );
reconsider S9 = S as non  empty (Subset of ( TOP-REAL 2 )) by A7,CKB5:1;
S is_an_arc_of ( E-max C ),( W-min C ) by A2,A5,A7,CKB21:1;
then A8: S9 is_an_arc_of ( W-min C ),( E-max C ) by JORDAN5B:14;
S c= P by CKB2:1;
hence thesis by A1,A8,CKB15:1,XBOOLE_1:1;
end;
end;
