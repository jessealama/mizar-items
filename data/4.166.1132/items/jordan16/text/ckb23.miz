environ
vocabularies NUMBERS,TOPREAL2,SUBSET_1,EUCLID,PRE_TOPC,ZFMISC_1,TARSKI,XBOOLE_0,JORDAN6,PSCOMP_1,TOPREAL1,JORDAN3,FUNCT_1,ORDINAL2,CONNSP_2,RCOMP_1,RELAT_1,TOPS_2,FUNCT_2,STRUCT_0,BORSUK_1,CARD_1,REAL_1,ARYTM_3,XXREAL_0,XXREAL_1,PCOMPS_1,XREAL_0,ORDINAL1,JGRAPH_2,PARTFUN1,ARYTM_1,TOPMETR;
notations TARSKI,XBOOLE_0,ENUMSET1,SUBSET_1,ORDINAL1,NUMBERS,XCMPLX_0,XXREAL_0,XREAL_0,REAL_1,RELAT_1,FUNCT_1,RELSET_1,PARTFUN1,FUNCT_2,FCONT_1,ZFMISC_1,STRUCT_0,PRE_TOPC,TOPS_2,COMPTS_1,CONNSP_2,PCOMPS_1,RCOMP_1,TMAP_1,EUCLID,PSCOMP_1,TOPMETR,JORDAN6,TOPREAL1,JORDAN5C,TOPREAL2,JORDAN7;
definitions TARSKI,TOPS_2,TOPREAL1,FUNCT_1,XBOOLE_0,BORSUK_1,STRUCT_0;
theorems PRE_TOPC,TOPMETR,XREAL_0,XBOOLE_0,FUNCT_1,XBOOLE_1,FUNCT_2,TOPREAL5,TREAL_1,BORSUK_1,JORDAN6,TOPREAL1,TOPS_2,TARSKI,RELAT_1,TOPREAL3,JORDAN5B,TOPMETR3,TOPREAL2,ENUMSET1,ZFMISC_1,JORDAN5A,JORDAN5C,FCONT_1,FUNCT_3,CONNSP_2,TMAP_1,RFUNCT_2,RELSET_1,FUNCT_4,JORDAN7,SPRECT_1,XCMPLX_0,XCMPLX_1,XREAL_1,XXREAL_0,XXREAL_1,EUCLID,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22;
schemes FUNCT_2;
registrations XBOOLE_0,FUNCT_1,RELSET_1,FUNCT_2,NUMBERS,XXREAL_0,XREAL_0,MEMBERED,STRUCT_0,PRE_TOPC,BORSUK_1,EUCLID,TOPMETR,TOPREAL2,VALUED_0,FCONT_1,TMAP_1;
constructors REAL_1,RCOMP_1,FCONT_1,ZFMISC_1,TOPS_2,TMAP_1,TOPMETR,PCOMPS_1,TOPREAL1,PSCOMP_1,JORDAN5C,JORDAN6,JORDAN7,SEQ_1,COMPLEX1;
requirements NUMERALS,REAL,BOOLE,SUBSET,ARITHM;
begin
reserve p1 for (Point of ( TOP-REAL 2 ));
reserve p2 for (Point of ( TOP-REAL 2 ));
reserve q1 for (Point of ( TOP-REAL 2 ));
reserve q2 for (Point of ( TOP-REAL 2 ));
theorem
(for P being (Subset of ( TOP-REAL 2 )) holds (for p1,p2,q1,q2 being (Point of ( TOP-REAL 2 )) holds ((((((((P is_an_arc_of p1,p2 & q1 in P) & q2 in P) & q1 <> p1) & q1 <> p2) & q2 <> p1) & q2 <> p2) & q1 <> q2) implies (ex Q being non  empty (Subset of ( TOP-REAL 2 )) st ((Q is_an_arc_of q1,q2 & Q c= P) & Q misses { p1,p2 })))))
proof
let P being (Subset of ( TOP-REAL 2 ));
let p1 being (Point of ( TOP-REAL 2 ));
let p2 being (Point of ( TOP-REAL 2 ));
let q1 being (Point of ( TOP-REAL 2 ));
let q2 being (Point of ( TOP-REAL 2 ));
assume that
A1: P is_an_arc_of p1,p2
and
A2: (q1 in P & q2 in P)
and
A3: q1 <> p1
and
A4: q1 <> p2
and
A5: q2 <> p1
and
A6: q2 <> p2
and
A7: q1 <> q2;
per cases  by A1,A2,A7,JORDAN5C:14;
suppose A8:  LE q1,q2,P,p1,p2;

set S = ( Segment (P,p1,p2,q1,q2) );
S is_an_arc_of q1,q2 by A1,A7,A8,CKB21:1;
then reconsider S as non  empty (Subset of ( TOP-REAL 2 )) by TOPREAL1:1;
take S;
thus S is_an_arc_of q1,q2 by A1,A7,A8,CKB21:1;
thus S c= P by CKB2:1;
now
A9: S = { q where q is (Point of ( TOP-REAL 2 )): ( LE q1,q,P,p1,p2 &  LE q,q2,P,p1,p2) } by JORDAN6:26;
assume A10: S meets { p1,p2 };
per cases  by A10,ZFMISC_1:51;
suppose p1 in S;

then (ex q being (Point of ( TOP-REAL 2 )) st ((q = p1 &  LE q1,q,P,p1,p2) &  LE q,q2,P,p1,p2)) by A9;
hence contradiction by A1,A3,JORDAN6:54;
end;
suppose p2 in S;

then (ex q being (Point of ( TOP-REAL 2 )) st ((q = p2 &  LE q1,q,P,p1,p2) &  LE q,q2,P,p1,p2)) by A9;
hence contradiction by A1,A6,JORDAN6:55;
end;
end;
hence thesis;
end;
suppose A11:  LE q2,q1,P,p1,p2;

set S = ( Segment (P,p1,p2,q2,q1) );
S is_an_arc_of q2,q1 by A1,A7,A11,CKB21:1;
then reconsider S as non  empty (Subset of ( TOP-REAL 2 )) by TOPREAL1:1;
take S;
S is_an_arc_of q2,q1 by A1,A7,A11,CKB21:1;
hence S is_an_arc_of q1,q2 by JORDAN5B:14;
thus S c= P by CKB2:1;
now
A12: S = { q where q is (Point of ( TOP-REAL 2 )): ( LE q2,q,P,p1,p2 &  LE q,q1,P,p1,p2) } by JORDAN6:26;
assume A13: S meets { p1,p2 };
per cases  by A13,ZFMISC_1:51;
suppose p1 in S;

then (ex q being (Point of ( TOP-REAL 2 )) st ((q = p1 &  LE q2,q,P,p1,p2) &  LE q,q1,P,p1,p2)) by A12;
hence contradiction by A1,A5,JORDAN6:54;
end;
suppose p2 in S;

then (ex q being (Point of ( TOP-REAL 2 )) st ((q = p2 &  LE q2,q,P,p1,p2) &  LE q,q1,P,p1,p2)) by A12;
hence contradiction by A1,A4,JORDAN6:55;
end;
end;
hence thesis;
end;
end;
