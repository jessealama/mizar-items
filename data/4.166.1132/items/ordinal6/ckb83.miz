constructors WELLORD1,WELLORD2,CLASSES1,ABIAN,CARD_3,AFINSQ_1,ORDINAL3,ORDINAL5,NAT_1,RELSET_1,CKB1,CKB25,CKB32,CKB41,CKB55,CKB74;
requirements SUBSET,BOOLE,NUMERALS;
begin
reserve b for  ordinal number;
reserve c for  ordinal number;
reserve x for set;
reserve y for set;
reserve f for Ordinal-Sequence;
reserve U for Universe;
definition
let U;
let a being (Ordinal of U);
let b being (Ordinal of U);
redefine func exp (a,b) -> (Ordinal of U);

coherence
proof
per cases  by ORDINAL3:8;
suppose (a = ( 0 ) & b = ( 0 ));

then ( exp (a,b) ) = ( 1-element_of U ) by ORDINAL2:43;
hence thesis;
end;
suppose (a = ( 0 ) & b <> ( 0 ));

hence thesis by ORDINAL4:20;
end;
suppose AA: ( 0 ) in a;

defpred P[ Ordinal ]
 means
($1 in U implies ( exp (a,$1) ) in U);
( exp (a,( 0 )) ) = ( succ ( 0 ) ) by ORDINAL2:43;
then 00: P[ ( {} ) ] by CLASSES2:5;
01: (for b holds (P[ b ] implies P[ ( succ b ) ]))
proof
let b;
assume that
A1: P[ b ]
and
A2: ( succ b ) in U;
b in ( succ b ) by ORDINAL1:6;
then reconsider b as (Ordinal of U) by A2,ORDINAL1:10;
reconsider ab = ( exp (a,b) ) as (Ordinal of U) by A1;
( exp (a,( succ b )) ) = ( a *^ ab ) by ORDINAL2:44;
hence thesis;
end;
02: (for b holds (((b <> ( {} ) & b is  limit_ordinal) & (for c holds (c in b implies P[ c ]))) implies P[ b ]))
proof
let b;
assume that
B1: (b <> ( {} ) & b is  limit_ordinal)
and
B2: (for c holds (c in b implies P[ c ]))
and
B0: b in U;
deffunc F(Ordinal) = ( exp (a,$1) );
consider f such that B3: (( dom f ) = b & (for c holds (c in b implies ( f . c ) = F(c)))) from ORDINAL2:sch 3;
( rng f ) c= ( On U )
proof
let x;
assume x in ( rng f );
then consider y such that C1: (y in b & x = ( f . y )) by B3,FUNCT_1:def 3;
reconsider y as Ordinal by C1;
((P[ y ] & y in U) & x = F(y)) by B0,B2,B3,C1,ORDINAL1:10;
hence thesis by ORDINAL1:def 9;
end;
then reconsider f as (Ordinal-Sequence of b,U) by B3,FUNCT_2:2;
B4: ( exp (a,b) ) = ( lim f ) by B1,B3,ORDINAL2:45;
f is  non-decreasing by AA,B3,ORDINAL5:8;
then ( Union f ) is_limes_of f by B1,B3,ORDINAL5:6;
then ( exp (a,b) ) = ( Union f ) by B4,ORDINAL2:def 10;
hence thesis by B0,CKB75:1;
end;
(for b holds P[ b ]) from ORDINAL2:sch 1(00,01,02);
hence thesis;
end;
end;
end;
