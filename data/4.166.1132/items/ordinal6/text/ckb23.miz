begin
reserve a for  ordinal number;
reserve b for  ordinal number;
reserve c for  ordinal number;
registration
let a;
cluster ( id a ) ->  continuous for Ordinal-Sequence;
coherence
proof
let f being Ordinal-Sequence;
assume that
A1: f = ( id a );
let b;
let c;
assume A2: (((b in ( dom f ) & b <> ( {} )) & b is  limit_ordinal) & c = ( f . b ));
set g = ( f | b );
A3: (( dom f ) = a & ( dom ( id b ) ) = b) by A1,RELAT_1:45;
then A4: c = b by A1,A2,FUNCT_1:18;
b c= a by A2,A3,ORDINAL1:def 2;
then A5: g = ( id b ) by A1,FUNCT_3:1;
per cases ;
case c = ( {} );
hence thesis by A2,A1,A3,FUNCT_1:18;
end;
case c <> ( {} );
let B being Ordinal;
let C being Ordinal;
assume B1: (B in c & c in C);
take D = ( succ B );
thus D in ( dom g ) by A2,A3,A4,A5,B1,ORDINAL1:28;
let E being Ordinal;
assume B3: (D c= E & E in ( dom g ));
then ( g . E ) = E by A3,A5,FUNCT_1:18;
hence (B in ( g . E ) & ( g . E ) in C) by A3,A4,A5,B1,B3,ORDINAL1:10,ORDINAL1:21;
end;
end;
end;
