theorem
Lem4: (for f1,f2 being T-Sequence holds f1 c= ( f1 ^ f2 ))
proof
let f1 being T-Sequence;
let f2 being T-Sequence;
( dom ( f1 ^ f2 ) ) = ( ( dom f1 ) +^ ( dom f2 ) ) by ORDINAL4:def 1;
then A1: ( dom f1 ) c= ( dom ( f1 ^ f2 ) ) by ORDINAL3:24;
(for x holds (x in ( dom f1 ) implies ( f1 . x ) = ( ( f1 ^ f2 ) . x ))) by ORDINAL4:def 1;
hence f1 c= ( f1 ^ f2 ) by A1,GRFUNC_1:2;
end;
