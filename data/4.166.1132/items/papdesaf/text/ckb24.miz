begin
theorem
Lm3: (for V being RealLinearSpace holds (for u,v,y being (VECTOR of V) holds ( ( u - y ) - ( v - y ) ) = ( u - v )))
proof
let V being RealLinearSpace;
let u being (VECTOR of V);
let v being (VECTOR of V);
let y being (VECTOR of V);
thus ( ( u - y ) - ( v - y ) ) = ( u - ( ( v - y ) + y ) ) by RLVECT_1:27
.= ( u - v ) by RLSUB_2:61;
end;
