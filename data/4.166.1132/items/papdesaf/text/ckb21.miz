environ
vocabularies NUMBERS,ANALOAF,DIRAF,ZFMISC_1,SUBSET_1,STRUCT_0,PARSP_1,RLVECT_1,REAL_1,RELAT_1,ARYTM_3,SUPINF_2,CARD_1,ARYTM_1,VECTSP_1,AFF_2,TRANSGEO,CONAFFM,AFF_1,INCSP_1;
notations NUMBERS,STRUCT_0,RLVECT_1,XXREAL_0,REAL_1,ANALOAF,DIRAF,AFF_1,AFF_2,GEOMTRAP,TRANSLAC,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14;
definitions TRANSLAC,RLVECT_1,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14;
theorems RLVECT_1,FUNCSDOM,ANALOAF,DIRAF,AFF_1,AFF_2,PASCH,ANALMETR,GEOMTRAP,RLSUB_2,XCMPLX_0,XCMPLX_1,XREAL_1,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20;
registrations MEMBERED,STRUCT_0,DIRAF,CKB1,CKB2;
constructors XXREAL_0,REAL_1,MEMBERED,AFF_1,AFF_2,TRANSLAC,GEOMTRAP,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14;
requirements NUMERALS,REAL,SUBSET,ARITHM,BOOLE;
begin
theorem
Th17: (for V being RealLinearSpace holds (for OAS being OAffinSpace holds (OAS = ( OASpace V ) implies OAS is  satisfying_DES_1)))
proof
let V being RealLinearSpace;
let OAS being OAffinSpace;
assume that
A1: OAS = ( OASpace V );
(for o,a,b,c,a1,b1,c1 being (Element of OAS) holds (((((((a,o // o,a1 & b,o // o,b1) & c,o // o,c1) & (not  LIN o,a,b)) & (not  LIN o,a,c)) & a,b // b1,a1) & a,c // c1,a1) implies b,c // c1,b1))
proof
let o being (Element of OAS);
let a being (Element of OAS);
let b being (Element of OAS);
let c being (Element of OAS);
let a1 being (Element of OAS);
let b1 being (Element of OAS);
let c1 being (Element of OAS);
assume that
A2: a,o // o,a1
and
A3: b,o // o,b1
and
A4: c,o // o,c1
and
A5: (not  LIN o,a,b)
and
A6: (not  LIN o,a,c)
and
A7: a,b // b1,a1
and
A8: a,c // c1,a1;
reconsider y = o,u = a,v = b,w = c,u1 = a1,v1 = b1,w1 = c1 as (VECTOR of V) by A1,CKB5:1;
A9: o <> a by A5,DIRAF:31;
A10:now
A11: ((not y,u '||' y,v) & (not y,u '||' y,w))
proof
assume (not thesis);
then (((y,u // y,v or y,u // v,y) or y,u // y,w) or y,u // w,y) by GEOMTRAP:def 1;
then (((o,a // o,b or o,a // b,o) or o,a // o,c) or o,a // c,o) by A1,GEOMTRAP:2;
then (o,a '||' o,b or o,a '||' o,c) by DIRAF:def 4;
hence contradiction by A5,A6,DIRAF:def 5;
end;
o,c // c1,o by A4,DIRAF:2;
then y,w // w1,y by A1,GEOMTRAP:2;
then A12: y,w '||' y,w1 by GEOMTRAP:def 1;
o,b // b1,o by A3,DIRAF:2;
then y,v // v1,y by A1,GEOMTRAP:2;
then A13: y,v '||' y,v1 by GEOMTRAP:def 1;
assume A14: o <> a1;
u,y // y,u1 by A1,A2,GEOMTRAP:2;
then consider r being Real such that A15: ( y - u ) = ( r * ( u1 - y ) ) and A16: ( 0 ) < r by A9,A14,CKB20:1;
u,w // w1,u1 by A1,A8,GEOMTRAP:2;
then u,w '||' u1,w1 by GEOMTRAP:def 1;
then A17: w1 = ( u1 + ( ( ( - r ) " ) * ( w - u ) ) ) by A15,A16,A12,A11,CKB19:1;
u,v // v1,u1 by A1,A7,GEOMTRAP:2;
then u,v '||' u1,v1 by GEOMTRAP:def 1;
then v1 = ( u1 + ( ( ( - r ) " ) * ( v - u ) ) ) by A15,A16,A13,A11,CKB19:1;
then A18: ( 1 * ( w1 - v1 ) ) = ( ( u1 + ( ( ( - r ) " ) * ( w - u ) ) ) - ( u1 + ( ( ( - r ) " ) * ( v - u ) ) ) ) by A17,RLVECT_1:def 8
.= ( ( ( ( ( ( - r ) " ) * ( w - u ) ) + u1 ) - u1 ) - ( ( ( - r ) " ) * ( v - u ) ) ) by RLVECT_1:27
.= ( ( ( ( - r ) " ) * ( w - u ) ) - ( ( ( - r ) " ) * ( v - u ) ) ) by RLSUB_2:61
.= ( ( ( - r ) " ) * ( ( w - u ) - ( v - u ) ) ) by RLVECT_1:34
.= ( ( ( - r ) " ) * ( w - ( ( v - u ) + u ) ) ) by RLVECT_1:27
.= ( ( ( - r ) " ) * ( w - v ) ) by RLSUB_2:61
.= ( ( - ( r " ) ) * ( w - v ) ) by XCMPLX_1:222
.= ( ( r " ) * ( - ( w - v ) ) ) by RLVECT_1:24
.= ( ( r " ) * ( v - w ) ) by RLVECT_1:33;
( 0 ) < ( r " ) by A16,XREAL_1:122;
then w,v // v1,w1 by A18,ANALOAF:def 1;
then c,b // b1,c1 by A1,GEOMTRAP:2;
hence thesis by DIRAF:2;
end;
now
assume A19: o = a1;
A20: o = c1
proof
c,o '||' o,c1 by A4,DIRAF:def 4;
then o,c1 '||' o,c by DIRAF:22;
then A21:  LIN o,c1,c by DIRAF:def 5;
A22:  LIN o,c1,o by DIRAF:31;
assume A23: o <> c1;
a,c '||' c1,o by A8,A19,DIRAF:def 4;
then o,c1 '||' c,a by DIRAF:22;
then  LIN o,c1,a by A23,A21,DIRAF:33;
hence contradiction by A6,A23,A21,A22,DIRAF:32;
end;
o = b1
proof
b,o '||' o,b1 by A3,DIRAF:def 4;
then o,b1 '||' o,b by DIRAF:22;
then A24:  LIN o,b1,b by DIRAF:def 5;
A25:  LIN o,b1,o by DIRAF:31;
assume A26: o <> b1;
a,b '||' b1,o by A7,A19,DIRAF:def 4;
then o,b1 '||' b,a by DIRAF:22;
then  LIN o,b1,a by A26,A24,DIRAF:33;
hence contradiction by A5,A26,A24,A25,DIRAF:32;
end;
hence thesis by A20,DIRAF:4;
end;
hence thesis by A10;
end;
hence thesis by CKB14:def 1;
end;
