environ
vocabularies NUMBERS,ANALOAF,DIRAF,ZFMISC_1,SUBSET_1,STRUCT_0,PARSP_1,RLVECT_1,REAL_1,RELAT_1,ARYTM_3,SUPINF_2,CARD_1,ARYTM_1,VECTSP_1,AFF_2,TRANSGEO,CONAFFM,AFF_1,INCSP_1;
notations NUMBERS,STRUCT_0,RLVECT_1,XXREAL_0,REAL_1,ANALOAF,DIRAF,AFF_1,AFF_2,GEOMTRAP,TRANSLAC;
definitions TRANSLAC,RLVECT_1;
theorems RLVECT_1,FUNCSDOM,ANALOAF,DIRAF,AFF_1,AFF_2,PASCH,ANALMETR,GEOMTRAP,RLSUB_2,XCMPLX_0,XCMPLX_1,XREAL_1,CKB3;
registrations MEMBERED,STRUCT_0,DIRAF,CKB1,CKB2;
constructors XXREAL_0,REAL_1,MEMBERED,AFF_1,AFF_2,TRANSLAC,GEOMTRAP;
requirements NUMERALS,REAL,SUBSET,ARITHM,BOOLE;
begin
theorem
Th3: (for OAS being OAffinSpace holds (for a,b,c being (Element of OAS) holds (for a9,b9,c9 being (Element of ( Lambda OAS )) holds (((a = a9 & b = b9) & c = c9) implies ( LIN a,b,c iff  LIN a9,b9,c9)))))
proof
let OAS being OAffinSpace;
let a being (Element of OAS),b being (Element of OAS),c being (Element of OAS);
let a9 being (Element of ( Lambda OAS )),b9 being (Element of ( Lambda OAS )),c9 being (Element of ( Lambda OAS ));
assume that
A1: ((a = a9 & b = b9) & c = c9);
A2:now
assume  LIN a,b,c;
then a,b '||' a,c by DIRAF:def 5;
then a9,b9 // a9,c9 by A1,DIRAF:38;
hence  LIN a9,b9,c9 by AFF_1:def 1;
end;
now
assume  LIN a9,b9,c9;
then a9,b9 // a9,c9 by AFF_1:def 1;
then a,b '||' a,c by A1,DIRAF:38;
hence  LIN a,b,c by DIRAF:def 5;
end;
hence thesis by A2;
end;
