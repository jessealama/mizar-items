environ
vocabularies NUMBERS,ANALOAF,DIRAF,ZFMISC_1,SUBSET_1,STRUCT_0,PARSP_1,RLVECT_1,REAL_1,RELAT_1,ARYTM_3,SUPINF_2,CARD_1,ARYTM_1,VECTSP_1,AFF_2,TRANSGEO,CONAFFM,AFF_1,INCSP_1;
notations NUMBERS,STRUCT_0,RLVECT_1,XXREAL_0,REAL_1,ANALOAF,DIRAF,AFF_1,AFF_2,GEOMTRAP,TRANSLAC,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14;
definitions TRANSLAC,RLVECT_1,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14;
theorems RLVECT_1,FUNCSDOM,ANALOAF,DIRAF,AFF_1,AFF_2,PASCH,ANALMETR,GEOMTRAP,RLSUB_2,XCMPLX_0,XCMPLX_1,XREAL_1,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29;
registrations MEMBERED,STRUCT_0,DIRAF,CKB1,CKB2;
constructors XXREAL_0,REAL_1,MEMBERED,AFF_1,AFF_2,TRANSLAC,GEOMTRAP,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14;
requirements NUMERALS,REAL,SUBSET,ARITHM,BOOLE;
begin
theorem
Th24: (for OAS being OAffinSpace holds ( Lambda OAS ) is  Fanoian)
proof
let OAS being OAffinSpace;
set AS = ( Lambda OAS );
(for a,b,c,d being (Element of AS) holds (((a,b // c,d & a,c // b,d) & a,d // b,c) implies a,b // a,c))
proof
let a being (Element of AS);
let b being (Element of AS);
let c being (Element of AS);
let d being (Element of AS);
assume that
A1: a,b // c,d
and
A2: a,c // b,d
and
A3: a,d // b,c;
reconsider a1 = a,b1 = b,c1 = c,d1 = d as (Element of OAS) by CKB3:1;
set P = ( Line (a,d) );
set Q = ( Line (b,c) );
assume A4: (not a,b // a,c);
then A5: a <> d by A1,AFF_1:4;
then A6: P is  being_line by AFF_1:def 3;
A7: (not  LIN a1,b1,c1)
proof
assume (not thesis);
then a1,b1 '||' a1,c1 by DIRAF:def 5;
hence contradiction by A4,DIRAF:38;
end;
(a1,b1 '||' c1,d1 & a1,c1 '||' b1,d1) by A1,A2,DIRAF:38;
then consider x1 being (Element of OAS) such that A8:  LIN x1,a1,d1 and A9:  LIN x1,b1,c1 by A7,PASCH:25;
reconsider x = x1 as (Element of AS) by CKB3:1;
A10: d in P by AFF_1:15;
x1,a1 '||' x1,d1 by A8,DIRAF:def 5;
then x,a // x,d by DIRAF:38;
then  LIN x,a,d by AFF_1:def 1;
then  LIN a,d,x by AFF_1:6;
then A11: x in P by AFF_1:def 2;
A12: (a in P & b in Q) by AFF_1:15;
x1,b1 '||' x1,c1 by A9,DIRAF:def 5;
then x,b // x,c by DIRAF:38;
then  LIN x,b,c by AFF_1:def 1;
then  LIN b,c,x by AFF_1:6;
then A13: x in Q by AFF_1:def 2;
A14: c in Q by AFF_1:15;
A15: (not  LIN a,b,c) by A4,AFF_1:def 1;
then A16: b <> c by AFF_1:7;
then Q is  being_line by AFF_1:def 3;
then P // Q by A3,A16,A5,A6,A10,A12,A14,AFF_1:38;
then P = Q by A11,A13,AFF_1:45;
hence contradiction by A15,A6,A12,A14,AFF_1:21;
end;
hence thesis by CKB8:def 1;
end;
