environ
vocabularies NUMBERS,ANALOAF,DIRAF,ZFMISC_1,SUBSET_1,STRUCT_0,PARSP_1,RLVECT_1,REAL_1,RELAT_1,ARYTM_3,SUPINF_2,CARD_1,ARYTM_1,VECTSP_1,AFF_2,TRANSGEO,CONAFFM,AFF_1,INCSP_1;
notations NUMBERS,STRUCT_0,RLVECT_1,XXREAL_0,REAL_1,ANALOAF,DIRAF,AFF_1,AFF_2,GEOMTRAP,TRANSLAC;
definitions TRANSLAC,RLVECT_1;
theorems RLVECT_1,FUNCSDOM,ANALOAF,DIRAF,AFF_1,AFF_2,PASCH,ANALMETR,GEOMTRAP,RLSUB_2,XCMPLX_0,XCMPLX_1,XREAL_1,CKB3,CKB4,CKB5;
registrations MEMBERED,STRUCT_0,DIRAF,CKB1,CKB2;
constructors XXREAL_0,REAL_1,MEMBERED,AFF_1,AFF_2,TRANSLAC,GEOMTRAP;
requirements NUMERALS,REAL,SUBSET,ARITHM,BOOLE;
begin
theorem
Th5: (for V being RealLinearSpace holds (for OAS being OAffinSpace holds (OAS = ( OASpace V ) implies (for a,b,c,d being (Element of OAS) holds (for u,v,w,y being (VECTOR of V) holds ((((a = u & b = v) & c = w) & d = y) implies (a,b '||' c,d iff u,v '||' w,y)))))))
proof
let V being RealLinearSpace;
let OAS being OAffinSpace;
assume that
A1: OAS = ( OASpace V );
let a being (Element of OAS),b being (Element of OAS),c being (Element of OAS),d being (Element of OAS);
let u being (VECTOR of V),v being (VECTOR of V),w being (VECTOR of V),y being (VECTOR of V);
assume A2: (((a = u & b = v) & c = w) & d = y);
A3:now
assume u,v '||' w,y;
then (u,v // w,y or u,v // y,w) by GEOMTRAP:def 1;
then (a,b // c,d or a,b // d,c) by A1,A2,GEOMTRAP:2;
hence a,b '||' c,d by DIRAF:def 4;
end;
now
assume a,b '||' c,d;
then (a,b // c,d or a,b // d,c) by DIRAF:def 4;
then (u,v // w,y or u,v // y,w) by A1,A2,GEOMTRAP:2;
hence u,v '||' w,y by GEOMTRAP:def 1;
end;
hence thesis by A3;
end;
