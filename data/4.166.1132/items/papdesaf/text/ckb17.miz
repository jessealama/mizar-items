environ
vocabularies NUMBERS,ANALOAF,DIRAF,ZFMISC_1,SUBSET_1,STRUCT_0,PARSP_1,RLVECT_1,REAL_1,RELAT_1,ARYTM_3,SUPINF_2,CARD_1,ARYTM_1,VECTSP_1,AFF_2,TRANSGEO,CONAFFM,AFF_1,INCSP_1;
notations NUMBERS,STRUCT_0,RLVECT_1,XXREAL_0,REAL_1,ANALOAF,DIRAF,AFF_1,AFF_2,GEOMTRAP,TRANSLAC,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14;
definitions TRANSLAC,RLVECT_1,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14;
theorems RLVECT_1,FUNCSDOM,ANALOAF,DIRAF,AFF_1,AFF_2,PASCH,ANALMETR,GEOMTRAP,RLSUB_2,XCMPLX_0,XCMPLX_1,XREAL_1,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16;
registrations MEMBERED,STRUCT_0,DIRAF,CKB1,CKB2;
constructors XXREAL_0,REAL_1,MEMBERED,AFF_1,AFF_2,TRANSLAC,GEOMTRAP,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14;
requirements NUMERALS,REAL,SUBSET,ARITHM,BOOLE;
begin
theorem
Th13: (for OAS being OAffinSpace holds (for o,a,b,a9,b9 being (Element of OAS) holds (((((not  LIN o,a,b) & o,a // o,a9) &  LIN o,b,b9) & a,b '||' a9,b9) implies (o,b // o,b9 & a,b // a9,b9))))
proof
let OAS being OAffinSpace;
let o being (Element of OAS);
let a being (Element of OAS);
let b being (Element of OAS);
let a9 being (Element of OAS);
let b9 being (Element of OAS);
assume that
A1: (not  LIN o,a,b)
and
A2: o,a // o,a9
and
A3:  LIN o,b,b9
and
A4: a,b '||' a9,b9;
A5: o <> a by A1,DIRAF:31;
consider a2 being (Element of OAS) such that A6:  Mid a,o,a2 and A7: o <> a2 by DIRAF:13;
a,o // o,a2 by A6,DIRAF:def 3;
then consider b2 being (Element of OAS) such that A8: b,o // o,b2 and A9: b,a // a2,b2 by A5,ANALOAF:def 5;
A10: o,b // b2,o by A8,DIRAF:2;
a,o // o,a2 by A6,DIRAF:def 3;
then a2,o // o,a by DIRAF:2;
then A11: a2,o // o,a9 by A2,A5,DIRAF:3;
 LIN a,o,a2 by A6,DIRAF:28;
then A12:  LIN o,a2,a by DIRAF:30;
A13: o <> b2
proof
assume o = b2;
then o,a2 // a,b by A9,DIRAF:2;
then o,a2 '||' a,b by DIRAF:def 4;
then ( LIN o,a2,o &  LIN o,a2,b) by A7,A12,DIRAF:31,DIRAF:33;
hence contradiction by A1,A7,A12,DIRAF:32;
end;
 Mid b,o,b2 by A8,DIRAF:def 3;
then  LIN b,o,b2 by DIRAF:28;
then A14:  LIN b2,o,b by DIRAF:30;
A15: (not  LIN o,a2,b2)
proof
A16:  LIN b2,o,o by DIRAF:31;
A17:  LIN o,a2,o by DIRAF:31;
assume  LIN o,a2,b2;
then  LIN b2,o,a by A7,A12,A17,DIRAF:32;
hence contradiction by A1,A14,A13,A16,DIRAF:32;
end;
a2,b2 // b,a by A9,DIRAF:2;
then A18: a2,b2 '||' a,b by DIRAF:def 4;
b <> a by A1,DIRAF:31;
then A19: a2,b2 '||' a9,b9 by A4,A18,DIRAF:23;
A20: a,b // b2,a2 by A9,DIRAF:2;
 Mid b,o,b2 by A8,DIRAF:def 3;
then  LIN b,o,b2 by DIRAF:28;
then A21:  LIN o,b,b2 by DIRAF:30;
A22:  LIN o,b,o by DIRAF:31;
o <> b by A1,DIRAF:31;
then A23:  LIN o,b2,b9 by A3,A21,A22,DIRAF:32;
then a2,b2 // b9,a9 by A15,A11,A19,CKB16:1;
then A24: b2,a2 // a9,b9 by DIRAF:2;
b2,o // o,b9 by A15,A11,A19,A23,CKB16:1;
hence o,b // o,b9 by A13,A10,DIRAF:3;
a2 <> b2 by A15,DIRAF:31;
hence thesis by A20,A24,DIRAF:3;
end;
