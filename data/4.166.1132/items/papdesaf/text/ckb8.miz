begin
definition
let AS being AffinSpace;
redefine attr AS is  Fanoian
means
:Def5: (for a,b,c,d being (Element of AS) holds (((a,b // c,d & a,c // b,d) & a,d // b,c) implies a,b // a,c));
compatibility
proof
thus (AS is  Fanoian implies (for a,b,c,d being (Element of AS) holds (((a,b // c,d & a,c // b,d) & a,d // b,c) implies a,b // a,c)))
proof
assume A1: (for a,b,c,d being (Element of AS) holds (((a,b // c,d & a,c // b,d) & a,d // b,c) implies  LIN a,b,c));
let a being (Element of AS);
let b being (Element of AS);
let c being (Element of AS);
let d being (Element of AS);
assume ((a,b // c,d & a,c // b,d) & a,d // b,c);
then  LIN a,b,c by A1;
hence thesis by AFF_1:def 1;
end;

assume A2: (for a,b,c,d being (Element of AS) holds (((a,b // c,d & a,c // b,d) & a,d // b,c) implies a,b // a,c));
let a being (Element of AS);
let b being (Element of AS);
let c being (Element of AS);
let d being (Element of AS);
assume ((a,b // c,d & a,c // b,d) & a,d // b,c);
then a,b // a,c by A2;
hence thesis by AFF_1:def 1;
end;
end;
