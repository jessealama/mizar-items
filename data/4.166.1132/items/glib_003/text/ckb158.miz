definition
let G being VGraph;
let v being set;
let x being set;
func G .labelVertex (v,x) -> VGraph equals 
:Def22: ( G .set (( VLabelSelector ),( ( the_VLabel_of G ) +* ( v .--> x ) )) ) if v in ( the_Vertices_of G ) otherwise G;
coherence
proof
set VL = ( ( the_VLabel_of G ) +* ( v .--> x ) );
set G2 = ( G .set (( VLabelSelector ),VL) );
hereby
A1: ( dom VL ) = ( ( dom ( the_VLabel_of G ) ) \/ ( dom ( v .--> x ) ) ) by FUNCT_4:def 1;
assume A2: v in ( the_Vertices_of G );
now
let y being set;
assume A3: y in ( dom VL );
now
per cases  by A1,A3,XBOOLE_0:def 3;
suppose A4: y in ( dom ( the_VLabel_of G ) );

( dom ( the_VLabel_of G ) ) c= ( the_Vertices_of G ) by CKB22:1;
hence y in ( the_Vertices_of G ) by A4;
end;
suppose y in ( dom ( v .--> x ) );

then y in { v } by FUNCOP_1:13;
hence y in ( the_Vertices_of G ) by A2,TARSKI:def 1;
end;
end;
hence y in ( the_Vertices_of G );
end;
then ( dom VL ) c= ( the_Vertices_of G ) by TARSKI:def 3;
then reconsider V1 = VL as (PartFunc of ( the_Vertices_of G ),( rng VL )) by RELSET_1:4;
G2 = ( G .set (( VLabelSelector ),V1) );
hence G2 is VGraph;
end;
thus thesis;
end;
consistency;
end;
