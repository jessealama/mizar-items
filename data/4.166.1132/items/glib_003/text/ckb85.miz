begin
registration
let G being EGraph;
let V being set;
let E being set;
cluster  elabel-inheriting for  [ELabeled]  [ELabeled] (inducedSubgraph of G,V,E);
existence
proof
now
per cases ;
suppose A1: (V is non  empty non  empty (Subset of ( the_Vertices_of G )) & E c= ( G .edgesBetween V ));

set X = the  [ELabeled] (inducedSubgraph of G,V,E);
set EL = ( ( the_ELabel_of G ) | ( the_Edges_of X ) );
reconsider EL9 = EL as (PartFunc of ( dom EL ),( rng EL )) by RELSET_1:4;
reconsider EL9 as (PartFunc of ( the_Edges_of X ),( rng EL )) by RELAT_1:58,RELSET_1:5;
set GG = ( X .set (( ELabelSelector ),EL9) );
A2: GG == X by CKB26:1;
then GG is (Subgraph of X) by GLIB_000:87;
then reconsider GG as (Subgraph of G) by GLIB_000:43;
A3: ( the_Vertices_of GG ) = ( the_Vertices_of X ) by A2,GLIB_000:def 34
.= V by A1,GLIB_000:def 37;
( the_Edges_of GG ) = ( the_Edges_of X ) by A2,GLIB_000:def 34
.= E by A1,GLIB_000:def 37;
then reconsider GG as  [ELabeled] (inducedSubgraph of G,V,E) by A1,A3,GLIB_000:def 37;
take GG;
( the_ELabel_of GG ) = EL by GLIB_000:8
.= ( ( the_ELabel_of G ) | ( the_Edges_of GG ) ) by A2,GLIB_000:def 34;
hence GG is  elabel-inheriting by CKB67:def 1;
end;
suppose A4: (not (V is non  empty non  empty (Subset of ( the_Vertices_of G )) & E c= ( G .edgesBetween V )));

reconsider GG = G as (Subgraph of G) by GLIB_000:40;
reconsider GG as  [ELabeled] (inducedSubgraph of G,V,E) by A4,GLIB_000:def 37;
take GG;
( dom ( the_ELabel_of G ) ) c= ( the_Edges_of G ) by CKB21:1;
then ( the_ELabel_of G ) = ( ( the_ELabel_of G ) | ( the_Edges_of G ) ) by RELAT_1:68;
hence GG is  elabel-inheriting by CKB67:def 1;
end;
end;
hence thesis;
end;
