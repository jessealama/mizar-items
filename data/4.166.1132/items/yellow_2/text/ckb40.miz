begin
reserve L for  complete LATTICE;
theorem
(for L,M being  complete LATTICE holds (for f being (Function of L,M) holds ((f is  sups-preserving or f is  infs-preserving) implies ( Image f ) is  complete  complete LATTICE)))
proof
let L being  complete LATTICE;
let M being  complete LATTICE;
let f being (Function of L,M);
assume that
A1: (f is  sups-preserving or f is  infs-preserving);
per cases  by A1;
suppose f is  sups-preserving;

then ( Image f ) is  sups-inheriting by CKB38:1;
hence thesis by CKB37:1;
end;
suppose f is  infs-preserving;

then ( Image f ) is  infs-inheriting by CKB39:1;
hence thesis by CKB36:1;
end;
end;
