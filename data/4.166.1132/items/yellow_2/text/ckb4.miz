environ
vocabularies XBOOLE_0,ORDERS_2,SUBSET_1,TARSKI,WAYBEL_0,LATTICE3,XXREAL_0,RELAT_2,YELLOW_0,EQREL_1,LATTICES,RELAT_1,WELLORD1,CAT_1,STRUCT_0,FUNCT_1,GROUP_6,SEQM_3,ORDINAL2,BINOP_1,REWRITE1,SETFAM_1,CARD_FIL,YELLOW_1,ZFMISC_1,WELLORD2,YELLOW_2;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,RELAT_1,FUNCT_1,RELSET_1,PARTFUN1,FUNCT_2,SETFAM_1,LATTICE3,WELLORD1,DOMAIN_1,STRUCT_0,ORDERS_2,QUANTAL1,ORDERS_3,YELLOW_0,YELLOW_1,WAYBEL_0;
definitions TARSKI,LATTICE3,QUANTAL1,ORDERS_3,WAYBEL_0,STRUCT_0;
theorems TARSKI,RELAT_1,FUNCT_1,FUNCT_2,SETFAM_1,LATTICE3,WELLORD1,ORDERS_2,ORDERS_3,YELLOW_0,YELLOW_1,WAYBEL_0,XBOOLE_0,XBOOLE_1,CKB1,CKB2,CKB3;
schemes FUNCT_2,DOMAIN_1;
registrations XBOOLE_0,SUBSET_1,RELSET_1,FUNCT_2,STRUCT_0,LATTICE3,YELLOW_0,WAYBEL_0,YELLOW_1;
constructors SETFAM_1,DOMAIN_1,TOLER_1,QUANTAL1,ORDERS_3,WAYBEL_0,YELLOW_1,RELSET_1;
requirements SUBSET,BOOLE;
begin
theorem
(for L being  antisymmetric  transitive  with_infima RelStr holds (for X,Y being set holds (( ex_inf_of X,L &  ex_inf_of Y,L) implies ( ex_inf_of ( X \/ Y ),L & ( "/\" (( X \/ Y ),L) ) = ( ( "/\" (X,L) ) "/\" ( "/\" (Y,L) ) )))))
proof
let L being  antisymmetric  transitive  with_infima RelStr;
let X being set,Y being set;
assume that
A1:  ex_inf_of X,L
and
A2:  ex_inf_of Y,L;
set a = ( ( "/\" (X,L) ) "/\" ( "/\" (Y,L) ) );
A3: ( X \/ Y ) is_>=_than a
proof
let x being (Element of L);
assume A4: x in ( X \/ Y );
per cases  by A4,XBOOLE_0:def 3;
suppose A5: x in X;

X is_>=_than ( "/\" (X,L) ) by A1,YELLOW_0:31;
then A6: x >= ( "/\" (X,L) ) by A5,LATTICE3:def 8;
( "/\" (X,L) ) >= a by YELLOW_0:23;
hence thesis by A6,ORDERS_2:3;
end;
suppose A7: x in Y;

Y is_>=_than ( "/\" (Y,L) ) by A2,YELLOW_0:31;
then A8: x >= ( "/\" (Y,L) ) by A7,LATTICE3:def 8;
( "/\" (Y,L) ) >= a by YELLOW_0:23;
hence thesis by A8,ORDERS_2:3;
end;
end;
(for b being (Element of L) holds (( X \/ Y ) is_>=_than b implies a >= b))
proof
let b being (Element of L);
assume A9: ( X \/ Y ) is_>=_than b;
Y c= ( X \/ Y ) by XBOOLE_1:7;
then Y is_>=_than b by A9,YELLOW_0:9;
then A10: ( "/\" (Y,L) ) >= b by A2,YELLOW_0:31;
X c= ( X \/ Y ) by XBOOLE_1:7;
then X is_>=_than b by A9,YELLOW_0:9;
then ( "/\" (X,L) ) >= b by A1,YELLOW_0:31;
hence thesis by A10,YELLOW_0:23;
end;
hence thesis by A3,YELLOW_0:31;
end;
