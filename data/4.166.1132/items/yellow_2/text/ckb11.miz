begin
theorem
(for L,M being non empty RelStr holds (for f,g being (Function of L,M) holds (f <= g iff (for x being (Element of L) holds ( f . x ) <= ( g . x )))))
proof
let L being non empty RelStr,M being non empty RelStr;
let f being (Function of L,M),g being (Function of L,M);
hereby
assume A1: f <= g;
let x being (Element of L);
(ex m1,m2 being (Element of M) st ((m1 = ( f . x ) & m2 = ( g . x )) & m1 <= m2)) by A1,CKB9:def 1;
hence ( f . x ) <= ( g . x );
end;
assume A2: (for x being (Element of L) holds ( f . x ) <= ( g . x ));
let x being set;
assume x in (the carrier of L);
then reconsider x as (Element of L);
take ( f . x );
take ( g . x );
thus thesis by A2;
end;
