begin
theorem
(for L,M,N being non  empty RelStr holds (for f being (Function of L,M) holds (for g being (Function of M,N) holds ((f is  monotone & g is  monotone) implies ( g * f ) is  monotone))))
proof
let L1 being non  empty RelStr;
let L2 being non  empty RelStr;
let L3 being non  empty RelStr;
let g1 being (Function of L1,L2);
let g2 being (Function of L2,L3);
assume that
A1: g1 is  monotone
and
A2: g2 is  monotone;
let s1 being (Element of L1);
let s2 being (Element of L1);
assume s1 <= s2;
then ( g1 . s1 ) <= ( g1 . s2 ) by A1,ORDERS_3:def 5;
then ( g2 . ( g1 . s1 ) ) <= ( g2 . ( g1 . s2 ) ) by A2,ORDERS_3:def 5;
then ( ( g2 * g1 ) . s1 ) <= ( g2 . ( g1 . s2 ) ) by FUNCT_2:15;
hence thesis by FUNCT_2:15;
end;
