environ
vocabularies XBOOLE_0,ORDERS_2,SUBSET_1,TARSKI,WAYBEL_0,LATTICE3,XXREAL_0,RELAT_2,YELLOW_0,EQREL_1,LATTICES,RELAT_1,WELLORD1,CAT_1,STRUCT_0,FUNCT_1,GROUP_6,SEQM_3,ORDINAL2,BINOP_1,REWRITE1,SETFAM_1,CARD_FIL,YELLOW_1,ZFMISC_1,WELLORD2,YELLOW_2;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,RELAT_1,FUNCT_1,RELSET_1,PARTFUN1,FUNCT_2,SETFAM_1,LATTICE3,WELLORD1,DOMAIN_1,STRUCT_0,ORDERS_2,QUANTAL1,ORDERS_3,YELLOW_0,YELLOW_1,WAYBEL_0,CKB9,CKB10,CKB12;
definitions TARSKI,LATTICE3,QUANTAL1,ORDERS_3,WAYBEL_0,STRUCT_0,CKB9,CKB12;
theorems TARSKI,RELAT_1,FUNCT_1,FUNCT_2,SETFAM_1,LATTICE3,WELLORD1,ORDERS_2,ORDERS_3,YELLOW_0,YELLOW_1,WAYBEL_0,XBOOLE_0,XBOOLE_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB11,CKB12,CKB13,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31;
schemes FUNCT_2,DOMAIN_1;
registrations XBOOLE_0,SUBSET_1,RELSET_1,FUNCT_2,STRUCT_0,LATTICE3,YELLOW_0,WAYBEL_0,YELLOW_1,CKB14,CKB15;
constructors SETFAM_1,DOMAIN_1,TOLER_1,QUANTAL1,ORDERS_3,WAYBEL_0,YELLOW_1,RELSET_1,CKB9,CKB12;
requirements SUBSET,BOOLE;
begin
reserve x for set;
reserve X for set;
reserve L for  complete LATTICE;
theorem
(for L being non  empty RelStr holds ((for A being (Subset of L) holds  ex_sup_of A,L) implies (for X holds ( ex_sup_of X,L & ( "\/" (X,L) ) = ( "\/" (( X /\ (the carrier of L) ),L) )))))
proof
let L being non  empty RelStr;
assume A1: (for A being (Subset of L) holds  ex_sup_of A,L);
let X;
set Y = ( X /\ (the carrier of L) );
set a = ( "\/" (Y,L) );
reconsider Y as (Subset of L) by XBOOLE_1:17;
A2:  ex_sup_of Y,L by A1;
A3: X is_<=_than a
proof
let x being (Element of L);
assume x in X;
then A4: x in Y by XBOOLE_0:def 4;
Y is_<=_than a by A2,YELLOW_0:def 9;
hence thesis by A4,LATTICE3:def 9;
end;
A5: (for b being (Element of L) holds (X is_<=_than b implies a <= b))
proof
let b being (Element of L);
A6: Y c= X by XBOOLE_1:17;
assume X is_<=_than b;
then Y is_<=_than b by A6,YELLOW_0:9;
hence thesis by A2,YELLOW_0:def 9;
end;
 ex_sup_of X,L by A2,YELLOW_0:50;
hence thesis by A3,A5,YELLOW_0:def 9;
end;
