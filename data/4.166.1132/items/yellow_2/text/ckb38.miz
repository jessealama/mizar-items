begin
reserve L for  complete LATTICE;
theorem
Th34: (for M being non empty RelStr holds (for f being (Function of L,M) holds (f is  sups-preserving implies ( Image f ) is  sups-inheriting)))
proof
let M being non empty RelStr;
let f being (Function of L,M);
assume that
A1: f is  sups-preserving;
set S = ( subrelstr ( rng f ) );
(for Y being (Subset of S) holds ( ex_sup_of Y,M implies ( "\/" (Y,M) ) in (the carrier of S)))
proof
let Y being (Subset of S);
assume  ex_sup_of Y,M;
A2: (f preserves_sup_of ( f " Y ) &  ex_sup_of ( f " Y ),L) by A1,WAYBEL_0:def 33,YELLOW_0:17;
Y c= (the carrier of S);
then Y c= ( rng f ) by YELLOW_0:def 15;
then ( "\/" (Y,M) ) = ( sup ( f .: ( f " Y ) ) ) by FUNCT_1:77
.= ( f . ( sup ( f " Y ) ) ) by A2,WAYBEL_0:def 31;
then ( "\/" (Y,M) ) in ( rng f ) by FUNCT_2:4;
hence thesis by YELLOW_0:def 15;
end;
hence thesis by YELLOW_0:def 19;
end;
