begin
theorem
(for L being RelStr holds (for F being (Subset-Family of L) holds ((for X being (Subset of L) holds (X in F implies X is  upper)) implies ( meet F ) is  upper  upper (Subset of L))))
proof
let L being RelStr;
let F being (Subset-Family of L);
reconsider F9 = F as (Subset-Family of L);
assume A1: (for X being (Subset of L) holds (X in F implies X is  upper));
reconsider M = ( meet F9 ) as (Subset of L);
per cases ;
suppose F = ( {} );

then (for x,y being (Element of L) holds ((x in M & x <= y) implies y in M)) by SETFAM_1:def 1;
hence thesis by WAYBEL_0:def 20;
end;
suppose A2: F <> ( {} );

(for x,y being (Element of L) holds ((x in M & x <= y) implies y in M))
proof
let x being (Element of L),y being (Element of L);
assume that
A3: x in M
and
A4: x <= y;
(for Y being set holds (Y in F implies y in Y))
proof
let Y being set;
assume A5: Y in F;
then reconsider X = Y as (Subset of L);
(X is  upper & x in X) by A1,A3,A5,SETFAM_1:def 1;
hence thesis by A4,WAYBEL_0:def 20;
end;
hence thesis by A2,SETFAM_1:def 1;
end;
hence thesis by WAYBEL_0:def 20;
end;
end;
