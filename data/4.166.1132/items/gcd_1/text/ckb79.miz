theorem
(for Amp being (AmpleSet of R) holds (for r1,r2,s1,s2 being (Element of R) holds (((Amp is  multiplicative & r1,r2 are_normalized_wrt Amp) & s1,s2 are_normalized_wrt Amp) implies ( add1 (r1,r2,s1,s2,Amp) ),( add2 (r1,r2,s1,s2,Amp) ) are_normalized_wrt Amp)))
proof
let Amp being (AmpleSet of R);
let r1 being (Element of R);
let r2 being (Element of R);
let s1 being (Element of R);
let s2 being (Element of R);
assume that
A1: Amp is  multiplicative
and
A2: r1,r2 are_normalized_wrt Amp
and
A3: s1,s2 are_normalized_wrt Amp;
A4: r2 <> ( 0. R ) by A2,CKB76:def 1;
r2 in Amp by A2,CKB76:def 1;
then A5: r2 = ( NF (r2,Amp) ) by CKB44:def 1;
s2 in Amp by A3,CKB76:def 1;
then A6: s2 = ( NF (s2,Amp) ) by CKB44:def 1;
A7: ( gcd (r1,r2,Amp) ) = ( 1. R ) by A2,CKB76:def 1;
then A8: r1,r2 are_co-prime  by CKB73:def 1;
A9: ( gcd (s1,s2,Amp) ) = ( 1. R ) by A3,CKB76:def 1;
then A10: s1,s2 are_co-prime  by CKB73:def 1;
A11: s2 <> ( 0. R ) by A3,CKB76:def 1;
now
per cases ;
case A12: r1 = ( 0. R );
then ( add2 (r1,r2,s1,s2,Amp) ) = s2 by A5,A6,A8,A10,CKB78:def 1;
hence thesis by A3,A5,A6,A8,A10,A12,CKB77:def 1;
end;
case A13: s1 = ( 0. R );
then ( add2 (r1,r2,s1,s2,Amp) ) = r2 by A5,A6,A8,A10,CKB78:def 1;
hence thesis by A2,A5,A6,A8,A10,A13,CKB77:def 1;
end;
case A14: ( gcd (r2,s2,Amp) ) = ( 1. R );
then A15: ( add2 (r1,r2,s1,s2,Amp) ) = ( r2 * s2 ) by A5,A6,A8,A10,CKB78:def 1;
( add1 (r1,r2,s1,s2,Amp) ) = ( ( r1 * s2 ) + ( r2 * s1 ) ) by A5,A6,A8,A10,A14,CKB77:def 1;
then A16: ( gcd (( add1 (r1,r2,s1,s2,Amp) ),( add2 (r1,r2,s1,s2,Amp) ),Amp) ) = ( gcd (( ( r1 * ( s2 / ( 1. R ) ) ) + ( s1 * r2 ) ),( r2 * s2 ),Amp) ) by A15,CKB23:1
.= ( gcd (( ( r1 * ( s2 / ( 1. R ) ) ) + ( s1 * ( r2 / ( 1. R ) ) ) ),( r2 * s2 ),Amp) ) by CKB23:1
.= ( gcd (( ( r1 * ( s2 / ( gcd (r2,s2,Amp) ) ) ) + ( s1 * ( r2 / ( gcd (r2,s2,Amp) ) ) ) ),( r2 * ( s2 / ( gcd (r2,s2,Amp) ) ) ),Amp) ) by A14,CKB23:1
.= ( gcd (( ( r1 * ( s2 / ( gcd (r2,s2,Amp) ) ) ) + ( s1 * ( r2 / ( gcd (r2,s2,Amp) ) ) ) ),( gcd (r2,s2,Amp) ),Amp) ) by A4,A7,A9,CKB69:1
.= ( 1. R ) by A14,CKB61:1;
reconsider r2,s2 as (Element of Amp) by A2,A3,CKB76:def 1;
(( r2 * s2 ) in Amp & ( r2 * s2 ) <> ( 0. R )) by A1,A4,A11,CKB47:def 1,VECTSP_2:def 1;
hence thesis by A15,A16,CKB76:def 1;
end;
case A17: ( ( r1 * ( s2 / ( gcd (r2,s2,Amp) ) ) ) + ( s1 * ( r2 / ( gcd (r2,s2,Amp) ) ) ) ) = ( 0. R );
A18: (( 1. R ) in Amp & ( 1. R ) <> ( 0. R )) by CKB41:1;
A19: ( add2 (r1,r2,s1,s2,Amp) ) = ( 1. R ) by A5,A6,A8,A10,A17,CKB78:def 1;
then ( gcd (( add1 (r1,r2,s1,s2,Amp) ),( add2 (r1,r2,s1,s2,Amp) ),Amp) ) = ( 1. R ) by CKB61:1;
hence thesis by A19,A18,CKB76:def 1;
end;
case (((r1 <> ( 0. R ) & s1 <> ( 0. R )) & ( gcd (r2,s2,Amp) ) <> ( 1. R )) & ( ( r1 * ( s2 / ( gcd (r2,s2,Amp) ) ) ) + ( s1 * ( r2 / ( gcd (r2,s2,Amp) ) ) ) ) <> ( 0. R ));
then A20: (( add1 (r1,r2,s1,s2,Amp) ) = ( ( ( r1 * ( s2 / ( gcd (r2,s2,Amp) ) ) ) + ( s1 * ( r2 / ( gcd (r2,s2,Amp) ) ) ) ) / ( gcd (( ( r1 * ( s2 / ( gcd (r2,s2,Amp) ) ) ) + ( s1 * ( r2 / ( gcd (r2,s2,Amp) ) ) ) ),( gcd (r2,s2,Amp) ),Amp) ) ) & ( add2 (r1,r2,s1,s2,Amp) ) = ( ( r2 * ( s2 / ( gcd (r2,s2,Amp) ) ) ) / ( gcd (( ( r1 * ( s2 / ( gcd (r2,s2,Amp) ) ) ) + ( s1 * ( r2 / ( gcd (r2,s2,Amp) ) ) ) ),( gcd (r2,s2,Amp) ),Amp) ) )) by A5,A6,A8,A10,CKB77:def 1,CKB78:def 1;
( gcd (r2,s2,Amp) ) <> ( 0. R ) by A4,CKB62:1;
then A21: ( gcd (( ( r1 * ( s2 / ( gcd (r2,s2,Amp) ) ) ) + ( s1 * ( r2 / ( gcd (r2,s2,Amp) ) ) ) ),( gcd (r2,s2,Amp) ),Amp) ) <> ( 0. R ) by CKB62:1;
( gcd (( ( r1 * ( s2 / ( gcd (r2,s2,Amp) ) ) ) + ( s1 * ( r2 / ( gcd (r2,s2,Amp) ) ) ) ),( r2 * ( s2 / ( gcd (r2,s2,Amp) ) ) ),Amp) ) = ( gcd (( ( r1 * ( s2 / ( gcd (r2,s2,Amp) ) ) ) + ( s1 * ( r2 / ( gcd (r2,s2,Amp) ) ) ) ),( gcd (r2,s2,Amp) ),Amp) ) by A4,A7,A9,CKB69:1;
then A22: ( gcd (( ( ( r1 * ( s2 / ( gcd (r2,s2,Amp) ) ) ) + ( s1 * ( r2 / ( gcd (r2,s2,Amp) ) ) ) ) / ( gcd (( ( r1 * ( s2 / ( gcd (r2,s2,Amp) ) ) ) + ( s1 * ( r2 / ( gcd (r2,s2,Amp) ) ) ) ),( gcd (r2,s2,Amp) ),Amp) ) ),( ( r2 * ( s2 / ( gcd (r2,s2,Amp) ) ) ) / ( gcd (( ( r1 * ( s2 / ( gcd (r2,s2,Amp) ) ) ) + ( s1 * ( r2 / ( gcd (r2,s2,Amp) ) ) ) ),( gcd (r2,s2,Amp) ),Amp) ) ),Amp) ) = ( 1. R ) by A21,CKB67:1;
reconsider r2,s2 as (Element of Amp) by A2,A3,CKB76:def 1;
A23: ( gcd (r2,s2,Amp) ) divides s2 by CKB56:def 1;
reconsider z2 = ( gcd (( ( r1 * ( s2 / ( gcd (r2,s2,Amp) ) ) ) + ( s1 * ( r2 / ( gcd (r2,s2,Amp) ) ) ) ),( gcd (r2,s2,Amp) ),Amp) ) as (Element of Amp) by CKB56:def 1;
A24: ( gcd (r2,s2,Amp) ) <> ( 0. R ) by A4,CKB62:1;
then A25: z2 <> ( 0. R ) by CKB62:1;
( gcd (r2,s2,Amp) ) in Amp by CKB56:def 1;
then reconsider z3 = ( s2 / ( gcd (r2,s2,Amp) ) ) as (Element of Amp) by A1,A23,A24,CKB48:1;
( r2 * z3 ) in Amp by A1,CKB47:def 1;
then reconsider z1 = ( r2 * ( s2 / ( gcd (r2,s2,Amp) ) ) ) as (Element of Amp);
A26: ( r2 * s2 ) <> ( 0. R ) by A4,A11,VECTSP_2:def 1;
A27: ( gcd (r2,s2,Amp) ) divides ( r2 * s2 ) by A23,CKB20:1;
then z1 = ( ( r2 * s2 ) / ( gcd (r2,s2,Amp) ) ) by A23,A24,CKB24:1;
then A28: z1 <> ( 0. R ) by A24,A26,A27,CKB21:1;
(z2 divides ( gcd (r2,s2,Amp) ) & ( gcd (r2,s2,Amp) ) divides r2) by CKB56:def 1;
then A29: z2 divides r2 by CKB15:1;
then z2 divides z1 by CKB20:1;
then A30: ( z1 / z2 ) <> ( 0. R ) by A25,A28,CKB21:1;
( z1 / z2 ) in Amp by A1,A29,A25,CKB20:1,CKB48:1;
hence thesis by A20,A22,A30,CKB76:def 1;
end;
end;
hence thesis;
end;
