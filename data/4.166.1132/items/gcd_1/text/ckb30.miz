theorem
Th17: (for a,b being (Element of R) holds ((a <> ( 0. R ) & ( a * b ) = a) implies b = ( 1. R )))
proof
let A being (Element of R);
let B being (Element of R);
assume that
A1: A <> ( 0. R )
and
A2: ( A * B ) = A;
set A1 = ( A / A );
(A1 = ( 1. R ) & ( ( A * B ) / A ) = ( ( A / A ) * B )) by A1,A2,CKB22:1,CKB24:1;
hence thesis by A2,VECTSP_1:def 8;
end;
