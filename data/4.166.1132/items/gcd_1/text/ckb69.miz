registrations XBOOLE_0,SUBSET_1,MEMBERED,STRUCT_0,VECTSP_1,MONOID_0,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB34,CKB37,CKB50,CKB51,CKB52,CKB53,CKB54;
constructors BINOP_2,VECTSP_2,MONOID_0,CKB8,CKB9,CKB10,CKB11,CKB13,CKB14,CKB33,CKB36,CKB39,CKB40,CKB44,CKB47,CKB49,CKB56;
requirements SUBSET,BOOLE;
begin
reserve R for gcdDomain;
theorem
Th41: (for Amp being (AmpleSet of R) holds (for r1,r2,s1,s2 being (Element of R) holds (((( gcd (r1,r2,Amp) ) = ( 1. R ) & ( gcd (s1,s2,Amp) ) = ( 1. R )) & r2 <> ( 0. R )) implies ( gcd (( ( r1 * ( s2 / ( gcd (r2,s2,Amp) ) ) ) + ( s1 * ( r2 / ( gcd (r2,s2,Amp) ) ) ) ),( r2 * ( s2 / ( gcd (r2,s2,Amp) ) ) ),Amp) ) = ( gcd (( ( r1 * ( s2 / ( gcd (r2,s2,Amp) ) ) ) + ( s1 * ( r2 / ( gcd (r2,s2,Amp) ) ) ) ),( gcd (r2,s2,Amp) ),Amp) ))))
proof
let Amp being (AmpleSet of R);
let r1 being (Element of R);
let r2 being (Element of R);
let s1 being (Element of R);
let s2 being (Element of R);
assume that
A1: ( gcd (r1,r2,Amp) ) = ( 1. R )
and
A2: ( gcd (s1,s2,Amp) ) = ( 1. R )
and
A3: r2 <> ( 0. R );
set d = ( gcd (r2,s2,Amp) );
set r = ( r2 / d );
set s = ( s2 / d );
A4: d <> ( 0. R ) by A3,CKB62:1;
then A5: ( gcd (r,s,Amp) ) = ( 1. R ) by CKB67:1;
A6: d divides s2 by CKB56:def 1;
( gcd (s,s1,Amp) ) = ( 1. R )
proof
( gcd (s,s1,Amp) ) divides s by CKB56:def 1;
then consider e being (Element of R) such that A7: ( ( gcd (s,s1,Amp) ) * e ) = s by CKB8:def 1;
( ( gcd (s,s1,Amp) ) * ( e * d ) ) = ( s * d ) by A7,GROUP_1:def 3
.= s2 by A6,A4,CKB14:def 1;
then A8: ( gcd (s,s1,Amp) ) divides s2 by CKB8:def 1;
A9: (( gcd (s,s1,Amp) ) is (Element of Amp) & ( 1. R ) is (Element of Amp)) by CKB40:def 1,CKB56:def 1;
( ( 1. R ) * ( gcd (s,s1,Amp) ) ) = ( gcd (s,s1,Amp) ) by VECTSP_1:def 8;
then A10: ( 1. R ) divides ( gcd (s,s1,Amp) ) by CKB8:def 1;
( gcd (s,s1,Amp) ) divides s1 by CKB56:def 1;
then ( gcd (s,s1,Amp) ) divides ( gcd (s1,s2,Amp) ) by A8,CKB56:def 1;
then ( gcd (s,s1,Amp) ) is_associated_to ( 1. R ) by A2,A10,CKB11:def 1;
hence thesis by A9,CKB41:1;
end;
then A11: ( gcd (s,( s1 * r ),Amp) ) = ( gcd (s,r,Amp) ) by CKB66:1;
( gcd (( ( r1 * s ) + ( s1 * r ) ),s,Amp) ) = ( gcd (( s1 * r ),s,Amp) ) by CKB68:1;
then A12: ( gcd (( ( r1 * s ) + ( s1 * r ) ),s,Amp) ) = ( gcd (s,( s1 * r ),Amp) ) by CKB58:1
.= ( 1. R ) by A11,A5,CKB58:1;
A13: d divides ( d * r2 ) by CKB19:1;
A14: d divides r2 by CKB56:def 1;
( gcd (r,r1,Amp) ) = ( 1. R )
proof
( gcd (r,r1,Amp) ) divides r by CKB56:def 1;
then consider e being (Element of R) such that A15: ( ( gcd (r,r1,Amp) ) * e ) = r by CKB8:def 1;
( ( gcd (r,r1,Amp) ) * ( e * d ) ) = ( r * d ) by A15,GROUP_1:def 3
.= r2 by A14,A4,CKB14:def 1;
then A16: ( gcd (r,r1,Amp) ) divides r2 by CKB8:def 1;
A17: (( gcd (r,r1,Amp) ) is (Element of Amp) & ( 1. R ) is (Element of Amp)) by CKB40:def 1,CKB56:def 1;
( ( 1. R ) * ( gcd (r,r1,Amp) ) ) = ( gcd (r,r1,Amp) ) by VECTSP_1:def 8;
then A18: ( 1. R ) divides ( gcd (r,r1,Amp) ) by CKB8:def 1;
( gcd (r,r1,Amp) ) divides r1 by CKB56:def 1;
then ( gcd (r,r1,Amp) ) divides ( gcd (r1,r2,Amp) ) by A16,CKB56:def 1;
then ( gcd (r,r1,Amp) ) is_associated_to ( 1. R ) by A1,A18,CKB11:def 1;
hence thesis by A17,CKB41:1;
end;
then A19: ( gcd (r,( r1 * s ),Amp) ) = ( gcd (r,s,Amp) ) by CKB66:1;
A20: ( gcd (( ( r1 * s ) + ( s1 * r ) ),r,Amp) ) = ( gcd (( r1 * s ),r,Amp) ) by CKB68:1;
( gcd (r,s,Amp) ) = ( 1. R ) by A4,CKB67:1;
then A21: ( gcd (( ( r1 * s ) + ( s1 * r ) ),( d * r ),Amp) ) = ( gcd (( ( r1 * s ) + ( s1 * r ) ),d,Amp) ) by A20,A19,CKB58:1,CKB66:1;
( r2 * s ) = ( ( ( 1. R ) * r2 ) * s ) by VECTSP_1:def 8
.= ( ( ( d / d ) * r2 ) * s ) by A4,CKB22:1
.= ( ( ( d * r2 ) / d ) * s ) by A4,A13,CKB24:1
.= ( s * ( d * r ) ) by A14,A4,A13,CKB24:1;
hence thesis by A12,A21,CKB66:1;
end;
