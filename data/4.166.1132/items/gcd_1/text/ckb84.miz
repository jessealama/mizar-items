environ
vocabularies BINOP_1,VECTSP_1,XBOOLE_0,ALGSTR_0,SUBSET_1,MESFUNC1,RELAT_1,LATTICES,ARYTM_3,FUNCSDOM,VECTSP_2,REAL_1,SUPINF_2,CARD_1,STRUCT_0,RLVECT_1,ARYTM_1,GROUP_1,EQREL_1,TARSKI,SETFAM_1,MSSUBFAM,INT_2,GCD_1;
notations TARSKI,XBOOLE_0,SUBSET_1,REAL_1,NUMBERS,STRUCT_0,ALGSTR_0,RLVECT_1,GROUP_1,VECTSP_2,VECTSP_1,FUNCSDOM,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB33,CKB36,CKB39,CKB40,CKB44,CKB47,CKB49,CKB55,CKB56,CKB71,CKB73,CKB74,CKB76,CKB77,CKB78,CKB81,CKB82;
definitions VECTSP_1,VECTSP_2,XBOOLE_0,CKB8,CKB10,CKB11,CKB14,CKB33,CKB36,CKB39,CKB40,CKB44,CKB47,CKB49,CKB56,CKB71,CKB73,CKB76,CKB77,CKB78,CKB81,CKB82;
theorems TARSKI,WELLORD2,SUBSET_1,VECTSP_1,VECTSP_2,RLVECT_1,XBOOLE_0,XCMPLX_1,GROUP_1,STRUCT_0,CKB7,CKB8,CKB10,CKB11,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB35,CKB36,CKB38,CKB39,CKB40,CKB41,CKB42,CKB43,CKB44,CKB45,CKB46,CKB47,CKB48,CKB49,CKB56,CKB57,CKB58,CKB59,CKB60,CKB61,CKB62,CKB63,CKB64,CKB65,CKB66,CKB67,CKB68,CKB69,CKB70,CKB71,CKB72,CKB73,CKB75,CKB76,CKB77,CKB78,CKB79,CKB80,CKB81,CKB82,CKB83;
schemes SUBSET_1;
registrations XBOOLE_0,SUBSET_1,MEMBERED,STRUCT_0,VECTSP_1,MONOID_0,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB34,CKB37,CKB50,CKB51,CKB52,CKB53,CKB54;
constructors BINOP_2,VECTSP_2,MONOID_0,CKB8,CKB9,CKB10,CKB11,CKB13,CKB14,CKB33,CKB36,CKB39,CKB40,CKB44,CKB47,CKB49,CKB56,CKB71,CKB73,CKB74,CKB76,CKB77,CKB78,CKB81,CKB82;
requirements SUBSET,BOOLE;
begin
reserve R for gcdDomain;
theorem
(for Amp being (AmpleSet of R) holds (for r1,r2,s1,s2 being (Element of R) holds ((r1,r2 are_normalized_wrt Amp & s1,s2 are_normalized_wrt Amp) implies ( ( mult1 (r1,r2,s1,s2,Amp) ) * ( r2 * s2 ) ) = ( ( mult2 (r1,r2,s1,s2,Amp) ) * ( r1 * s1 ) ))))
proof
let Amp being (AmpleSet of R);
let r1 being (Element of R);
let r2 being (Element of R);
let s1 being (Element of R);
let s2 being (Element of R);
assume that
A1: r1,r2 are_normalized_wrt Amp
and
A2: s1,s2 are_normalized_wrt Amp;
( gcd (r1,r2,Amp) ) = ( 1. R ) by A1,CKB76:def 1;
then A3: r1,r2 are_co-prime  by CKB73:def 1;
s2 <> ( 0. R ) by A2,CKB76:def 1;
then A4: ( gcd (r1,s2,Amp) ) <> ( 0. R ) by CKB62:1;
r2 in Amp by A1,CKB76:def 1;
then A5: r2 = ( NF (r2,Amp) ) by CKB44:def 1;
( gcd (s1,s2,Amp) ) = ( 1. R ) by A2,CKB76:def 1;
then A6: s1,s2 are_co-prime  by CKB73:def 1;
r2 <> ( 0. R ) by A1,CKB76:def 1;
then A7: ( gcd (s1,r2,Amp) ) <> ( 0. R ) by CKB62:1;
s2 in Amp by A2,CKB76:def 1;
then A8: s2 = ( NF (s2,Amp) ) by CKB44:def 1;
now
per cases ;
case A9: (r1 = ( 0. R ) or s1 = ( 0. R ));
then A10: ( mult1 (r1,r2,s1,s2,Amp) ) = ( 0. R ) by CKB81:def 1;
now
per cases  by A9;
case r1 = ( 0. R );
then ( ( mult2 (r1,r2,s1,s2,Amp) ) * ( r1 * s1 ) ) = ( ( mult2 (r1,r2,s1,s2,Amp) ) * ( 0. R ) ) by VECTSP_1:7
.= ( 0. R ) by VECTSP_1:7;
hence thesis by A10,VECTSP_1:7;
end;
case s1 = ( 0. R );
then ( ( mult2 (r1,r2,s1,s2,Amp) ) * ( r1 * s1 ) ) = ( ( mult2 (r1,r2,s1,s2,Amp) ) * ( 0. R ) ) by VECTSP_1:7
.= ( 0. R ) by VECTSP_1:7;
hence thesis by A10,VECTSP_1:7;
end;
end;
hence thesis;
end;
case A11: (r2 = ( 1. R ) & s2 = ( 1. R ));
then (( mult1 (r1,r2,s1,s2,Amp) ) = ( r1 * s1 ) & ( mult2 (r1,r2,s1,s2,Amp) ) = ( 1. R )) by A3,A6,A5,CKB81:def 1,CKB82:def 1;
hence thesis by A11,VECTSP_1:def 4;
end;
case A12: (s2 <> ( 0. R ) & r2 = ( 1. R ));
( gcd (r1,s2,Amp) ) divides r1 by CKB56:def 1;
then A13: ( gcd (r1,s2,Amp) ) divides ( r1 * s1 ) by CKB20:1;
then A14: ( gcd (r1,s2,Amp) ) divides ( ( r1 * s1 ) * s2 ) by CKB20:1;
A15: ( ( ( r1 * s1 ) / ( gcd (r1,s2,Amp) ) ) * ( r2 * s2 ) ) = ( ( ( r1 * s1 ) / ( gcd (r1,s2,Amp) ) ) * s2 ) by A12,VECTSP_1:def 8
.= ( ( ( r1 * s1 ) * s2 ) / ( gcd (r1,s2,Amp) ) ) by A4,A13,A14,CKB24:1;
A16: ( mult2 (r1,r2,s1,s2,Amp) ) = ( s2 / ( gcd (r1,s2,Amp) ) ) by A3,A6,A5,A8,A12,CKB82:def 1;
A17: ( gcd (r1,s2,Amp) ) divides s2 by CKB56:def 1;
then A18: ( gcd (r1,s2,Amp) ) divides ( s2 * r1 ) by CKB20:1;
then A19: ( gcd (r1,s2,Amp) ) divides ( ( s2 * r1 ) * s1 ) by CKB20:1;
( ( s2 / ( gcd (r1,s2,Amp) ) ) * ( r1 * s1 ) ) = ( ( ( s2 / ( gcd (r1,s2,Amp) ) ) * r1 ) * s1 ) by GROUP_1:def 3
.= ( ( ( s2 * r1 ) / ( gcd (r1,s2,Amp) ) ) * s1 ) by A4,A17,A18,CKB24:1
.= ( ( ( s2 * r1 ) * s1 ) / ( gcd (r1,s2,Amp) ) ) by A4,A18,A19,CKB24:1
.= ( ( ( r1 * s1 ) * s2 ) / ( gcd (r1,s2,Amp) ) ) by GROUP_1:def 3;
hence thesis by A12,A16,A15,CKB81:def 1;
end;
case A20: (r2 <> ( 0. R ) & s2 = ( 1. R ));
( gcd (s1,r2,Amp) ) divides s1 by CKB56:def 1;
then A21: ( gcd (s1,r2,Amp) ) divides ( s1 * r1 ) by CKB20:1;
then A22: ( gcd (s1,r2,Amp) ) divides ( ( s1 * r1 ) * r2 ) by CKB20:1;
A23: ( ( ( r1 * s1 ) / ( gcd (s1,r2,Amp) ) ) * ( r2 * s2 ) ) = ( ( ( r1 * s1 ) / ( gcd (s1,r2,Amp) ) ) * r2 ) by A20,VECTSP_1:def 4
.= ( ( ( r1 * s1 ) * r2 ) / ( gcd (s1,r2,Amp) ) ) by A7,A21,A22,CKB24:1;
A24: ( mult2 (r1,r2,s1,s2,Amp) ) = ( r2 / ( gcd (s1,r2,Amp) ) ) by A3,A6,A5,A8,A20,CKB82:def 1;
A25: ( gcd (s1,r2,Amp) ) divides r2 by CKB56:def 1;
then A26: ( gcd (s1,r2,Amp) ) divides ( r2 * r1 ) by CKB20:1;
then A27: ( gcd (s1,r2,Amp) ) divides ( ( r2 * r1 ) * s1 ) by CKB20:1;
( ( r2 / ( gcd (s1,r2,Amp) ) ) * ( r1 * s1 ) ) = ( ( ( r2 / ( gcd (s1,r2,Amp) ) ) * r1 ) * s1 ) by GROUP_1:def 3
.= ( ( ( r2 * r1 ) / ( gcd (s1,r2,Amp) ) ) * s1 ) by A7,A25,A26,CKB24:1
.= ( ( ( r2 * r1 ) * s1 ) / ( gcd (s1,r2,Amp) ) ) by A7,A26,A27,CKB24:1
.= ( ( ( r1 * s1 ) * r2 ) / ( gcd (s1,r2,Amp) ) ) by GROUP_1:def 3;
hence thesis by A20,A24,A23,CKB81:def 1;
end;
case A28: ((((not (r1 = ( 0. R ) or s1 = ( 0. R ))) & (not (r2 = ( 1. R ) & s2 = ( 1. R )))) & (not (s2 <> ( 0. R ) & r2 = ( 1. R )))) & (not (r2 <> ( 0. R ) & s2 = ( 1. R ))));
A29: (( gcd (s1,r2,Amp) ) divides r2 & ( gcd (r1,s2,Amp) ) divides s2) by CKB56:def 1;
then A30: ( ( gcd (s1,r2,Amp) ) * ( gcd (r1,s2,Amp) ) ) divides ( r2 * s2 ) by CKB16:1;
then A31: ( ( gcd (s1,r2,Amp) ) * ( gcd (r1,s2,Amp) ) ) divides ( ( r2 * s2 ) * r1 ) by CKB20:1;
then A32: ( ( gcd (s1,r2,Amp) ) * ( gcd (r1,s2,Amp) ) ) divides ( ( ( r2 * s2 ) * r1 ) * s1 ) by CKB20:1;
A33: (( gcd (r1,s2,Amp) ) divides r1 & ( gcd (s1,r2,Amp) ) divides s1) by CKB56:def 1;
then A34: ( ( gcd (r1,s2,Amp) ) * ( gcd (s1,r2,Amp) ) ) divides ( r1 * s1 ) by CKB16:1;
then A35: ( ( gcd (r1,s2,Amp) ) * ( gcd (s1,r2,Amp) ) ) divides ( ( r1 * s1 ) * r2 ) by CKB20:1;
then A36: ( ( gcd (r1,s2,Amp) ) * ( gcd (s1,r2,Amp) ) ) divides ( ( ( r1 * s1 ) * r2 ) * s2 ) by CKB20:1;
A37: ( mult2 (r1,r2,s1,s2,Amp) ) = ( ( r2 / ( gcd (s1,r2,Amp) ) ) * ( s2 / ( gcd (r1,s2,Amp) ) ) ) by A3,A6,A5,A8,A28,CKB82:def 1;
A38: ( ( gcd (r1,s2,Amp) ) * ( gcd (s1,r2,Amp) ) ) <> ( 0. R ) by A4,A7,VECTSP_2:def 1;
A39: ( ( ( r2 / ( gcd (s1,r2,Amp) ) ) * ( s2 / ( gcd (r1,s2,Amp) ) ) ) * ( r1 * s1 ) ) = ( ( ( r2 * s2 ) / ( ( gcd (s1,r2,Amp) ) * ( gcd (r1,s2,Amp) ) ) ) * ( r1 * s1 ) ) by A4,A7,A29,CKB27:1
.= ( ( ( ( r2 * s2 ) / ( ( gcd (s1,r2,Amp) ) * ( gcd (r1,s2,Amp) ) ) ) * r1 ) * s1 ) by GROUP_1:def 3
.= ( ( ( ( r2 * s2 ) * r1 ) / ( ( gcd (s1,r2,Amp) ) * ( gcd (r1,s2,Amp) ) ) ) * s1 ) by A38,A30,A31,CKB24:1
.= ( ( ( ( r2 * s2 ) * r1 ) * s1 ) / ( ( gcd (s1,r2,Amp) ) * ( gcd (r1,s2,Amp) ) ) ) by A38,A31,A32,CKB24:1
.= ( ( r1 * ( ( r2 * s2 ) * s1 ) ) / ( ( gcd (s1,r2,Amp) ) * ( gcd (r1,s2,Amp) ) ) ) by GROUP_1:def 3
.= ( ( r1 * ( ( s1 * r2 ) * s2 ) ) / ( ( gcd (s1,r2,Amp) ) * ( gcd (r1,s2,Amp) ) ) ) by GROUP_1:def 3
.= ( ( ( r1 * ( s1 * r2 ) ) * s2 ) / ( ( gcd (s1,r2,Amp) ) * ( gcd (r1,s2,Amp) ) ) ) by GROUP_1:def 3
.= ( ( ( ( r1 * s1 ) * r2 ) * s2 ) / ( ( gcd (s1,r2,Amp) ) * ( gcd (r1,s2,Amp) ) ) ) by GROUP_1:def 3;
( ( ( r1 / ( gcd (r1,s2,Amp) ) ) * ( s1 / ( gcd (s1,r2,Amp) ) ) ) * ( r2 * s2 ) ) = ( ( ( r1 * s1 ) / ( ( gcd (r1,s2,Amp) ) * ( gcd (s1,r2,Amp) ) ) ) * ( r2 * s2 ) ) by A4,A7,A33,CKB27:1
.= ( ( ( ( r1 * s1 ) / ( ( gcd (r1,s2,Amp) ) * ( gcd (s1,r2,Amp) ) ) ) * r2 ) * s2 ) by GROUP_1:def 3
.= ( ( ( ( r1 * s1 ) * r2 ) / ( ( gcd (r1,s2,Amp) ) * ( gcd (s1,r2,Amp) ) ) ) * s2 ) by A34,A35,A38,CKB24:1
.= ( ( ( ( r1 * s1 ) * r2 ) * s2 ) / ( ( gcd (r1,s2,Amp) ) * ( gcd (s1,r2,Amp) ) ) ) by A35,A36,A38,CKB24:1;
hence thesis by A28,A37,A39,CKB81:def 1;
end;
end;
hence thesis;
end;
