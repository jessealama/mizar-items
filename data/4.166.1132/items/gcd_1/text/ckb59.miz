begin
reserve R for gcdDomain;
theorem
Th31: (for Amp being (AmpleSet of R) holds (for a being (Element of R) holds (( gcd (a,( 0. R ),Amp) ) = ( NF (a,Amp) ) & ( gcd (( 0. R ),a,Amp) ) = ( NF (a,Amp) ))))
proof
let Amp being (AmpleSet of R);
let A being (Element of R);
A1: ( NF (A,Amp) ) in Amp by CKB44:def 1;
( ( NF (A,Amp) ) * ( 0. R ) ) = ( 0. R ) by VECTSP_1:6;
then A2: ( NF (A,Amp) ) divides ( 0. R ) by CKB8:def 1;
A3: ( NF (A,Amp) ) is_associated_to A by CKB44:def 1;
A4: (for z being (Element of R) holds ((z divides A & z divides ( 0. R )) implies z divides ( NF (A,Amp) )))
proof
let z being (Element of R);
assume that
A5: z divides A
and
z divides ( 0. R );
A divides ( NF (A,Amp) ) by A3,CKB11:def 1;
hence thesis by A5,CKB15:1;
end;
( NF (A,Amp) ) divides A by A3,CKB11:def 1;
then ( gcd (A,( 0. R ),Amp) ) = ( NF (A,Amp) ) by A2,A4,A1,CKB56:def 1;
hence thesis by CKB58:1;
end;
