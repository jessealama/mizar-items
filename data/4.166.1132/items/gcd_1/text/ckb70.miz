environ
vocabularies BINOP_1,VECTSP_1,XBOOLE_0,ALGSTR_0,SUBSET_1,MESFUNC1,RELAT_1,LATTICES,ARYTM_3,FUNCSDOM,VECTSP_2,REAL_1,SUPINF_2,CARD_1,STRUCT_0,RLVECT_1,ARYTM_1,GROUP_1,EQREL_1,TARSKI,SETFAM_1,MSSUBFAM,INT_2,GCD_1;
notations TARSKI,XBOOLE_0,SUBSET_1,REAL_1,NUMBERS,STRUCT_0,ALGSTR_0,RLVECT_1,GROUP_1,VECTSP_2,VECTSP_1,FUNCSDOM,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB33,CKB36,CKB39,CKB40,CKB44,CKB47,CKB49,CKB55,CKB56;
definitions VECTSP_1,VECTSP_2,XBOOLE_0,CKB8,CKB10,CKB11,CKB14,CKB33,CKB36,CKB39,CKB40,CKB44,CKB47,CKB49,CKB56;
theorems TARSKI,WELLORD2,SUBSET_1,VECTSP_1,VECTSP_2,RLVECT_1,XBOOLE_0,XCMPLX_1,GROUP_1,STRUCT_0,CKB7,CKB8,CKB10,CKB11,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB35,CKB36,CKB38,CKB39,CKB40,CKB41,CKB42,CKB43,CKB44,CKB45,CKB46,CKB47,CKB48,CKB49,CKB56,CKB57,CKB58,CKB59,CKB60,CKB61,CKB62,CKB63,CKB64,CKB65,CKB66,CKB67,CKB68,CKB69;
schemes SUBSET_1;
registrations XBOOLE_0,SUBSET_1,MEMBERED,STRUCT_0,VECTSP_1,MONOID_0,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB34,CKB37,CKB50,CKB51,CKB52,CKB53,CKB54;
constructors BINOP_2,VECTSP_2,MONOID_0,CKB8,CKB9,CKB10,CKB11,CKB13,CKB14,CKB33,CKB36,CKB39,CKB40,CKB44,CKB47,CKB49,CKB56;
requirements SUBSET,BOOLE;
begin
reserve R for gcdDomain;
theorem
Th42: (for Amp being (AmpleSet of R) holds (for r1,r2,s1,s2 being (Element of R) holds ((((( gcd (r1,r2,Amp) ) = ( 1. R ) & ( gcd (s1,s2,Amp) ) = ( 1. R )) & r2 <> ( 0. R )) & s2 <> ( 0. R )) implies ( gcd (( ( r1 / ( gcd (r1,s2,Amp) ) ) * ( s1 / ( gcd (s1,r2,Amp) ) ) ),( ( r2 / ( gcd (s1,r2,Amp) ) ) * ( s2 / ( gcd (r1,s2,Amp) ) ) ),Amp) ) = ( 1. R ))))
proof
let Amp being (AmpleSet of R);
let r1 being (Element of R);
let r2 being (Element of R);
let s1 being (Element of R);
let s2 being (Element of R);
assume that
A1: ( gcd (r1,r2,Amp) ) = ( 1. R )
and
A2: ( gcd (s1,s2,Amp) ) = ( 1. R )
and
A3: r2 <> ( 0. R )
and
A4: s2 <> ( 0. R );
set d1 = ( gcd (r1,s2,Amp) );
A5: d1 <> ( 0. R ) by A4,CKB62:1;
set d2 = ( gcd (s1,r2,Amp) );
set s19 = ( s1 / d2 );
set r29 = ( r2 / d2 );
A6: d2 <> ( 0. R ) by A3,CKB62:1;
then A7: ( gcd (s19,r29,Amp) ) = ( 1. R ) by CKB67:1;
set r19 = ( r1 / d1 );
A8: ( gcd (r19,r29,Amp) ) divides r29 by CKB56:def 1;
d2 divides r2 by CKB56:def 1;
then ( r29 * d2 ) = r2 by A6,CKB14:def 1;
then r29 divides r2 by CKB8:def 1;
then A9: ( gcd (r19,r29,Amp) ) divides r2 by A8,CKB15:1;
A10: (( gcd (r19,r29,Amp) ) is (Element of Amp) & ( 1. R ) is (Element of Amp)) by CKB40:def 1,CKB56:def 1;
d1 divides r1 by CKB56:def 1;
then ( r19 * d1 ) = r1 by A5,CKB14:def 1;
then A11: r19 divides r1 by CKB8:def 1;
( ( 1. R ) * ( gcd (r19,r29,Amp) ) ) = ( gcd (r19,r29,Amp) ) by VECTSP_1:def 8;
then A12: ( 1. R ) divides ( gcd (r19,r29,Amp) ) by CKB8:def 1;
( gcd (r19,r29,Amp) ) divides r19 by CKB56:def 1;
then ( gcd (r19,r29,Amp) ) divides r1 by A11,CKB15:1;
then ( gcd (r19,r29,Amp) ) divides ( gcd (r1,r2,Amp) ) by A9,CKB56:def 1;
then ( gcd (r19,r29,Amp) ) is_associated_to ( 1. R ) by A1,A12,CKB11:def 1;
then ( gcd (r19,r29,Amp) ) = ( 1. R ) by A10,CKB41:1;
then A13: ( gcd (r29,r19,Amp) ) = ( 1. R ) by CKB58:1;
set s29 = ( s2 / d1 );
A14: ( gcd (s19,s29,Amp) ) divides s29 by CKB56:def 1;
d1 divides s2 by CKB56:def 1;
then ( s29 * d1 ) = s2 by A5,CKB14:def 1;
then s29 divides s2 by CKB8:def 1;
then A15: ( gcd (s19,s29,Amp) ) divides s2 by A14,CKB15:1;
A16: (( gcd (s19,s29,Amp) ) is (Element of Amp) & ( 1. R ) is (Element of Amp)) by CKB40:def 1,CKB56:def 1;
d2 divides s1 by CKB56:def 1;
then ( s19 * d2 ) = s1 by A6,CKB14:def 1;
then A17: s19 divides s1 by CKB8:def 1;
( ( 1. R ) * ( gcd (s19,s29,Amp) ) ) = ( gcd (s19,s29,Amp) ) by VECTSP_1:def 8;
then A18: ( 1. R ) divides ( gcd (s19,s29,Amp) ) by CKB8:def 1;
( gcd (s19,s29,Amp) ) divides s19 by CKB56:def 1;
then ( gcd (s19,s29,Amp) ) divides s1 by A17,CKB15:1;
then ( gcd (s19,s29,Amp) ) divides ( gcd (s1,s2,Amp) ) by A15,CKB56:def 1;
then ( gcd (s19,s29,Amp) ) is_associated_to ( 1. R ) by A2,A18,CKB11:def 1;
then A19: ( gcd (s19,s29,Amp) ) = ( 1. R ) by A16,CKB41:1;
A20: ( gcd (s29,r19,Amp) ) = ( gcd (( r1 / d1 ),( s2 / d1 ),Amp) ) by CKB58:1
.= ( 1. R ) by A5,CKB67:1;
( gcd (( r19 * s19 ),r29,Amp) ) = ( gcd (r29,( r19 * s19 ),Amp) ) by CKB58:1
.= ( gcd (r29,s19,Amp) ) by A13,CKB66:1
.= ( 1. R ) by A7,CKB58:1;
then ( gcd (( r19 * s19 ),( r29 * s29 ),Amp) ) = ( gcd (( r19 * s19 ),s29,Amp) ) by CKB66:1
.= ( gcd (s29,( r19 * s19 ),Amp) ) by CKB58:1
.= ( gcd (s29,s19,Amp) ) by A20,CKB66:1
.= ( 1. R ) by A19,CKB58:1;
hence thesis;
end;
