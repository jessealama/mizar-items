begin
reserve R for gcdDomain;
theorem
Th36: (for Amp being (AmpleSet of R) holds (for a,b,c being (Element of R) holds ( gcd (( gcd (a,b,Amp) ),c,Amp) ) = ( gcd (a,( gcd (b,c,Amp) ),Amp) )))
proof
let Amp being (AmpleSet of R);
let A being (Element of R);
let B being (Element of R);
let C being (Element of R);
set D = ( gcd (( gcd (A,B,Amp) ),C,Amp) );
set E = ( gcd (A,( gcd (B,C,Amp) ),Amp) );
A1: D divides C by CKB56:def 1;
A2: E divides A by CKB56:def 1;
A3: E divides ( gcd (B,C,Amp) ) by CKB56:def 1;
then A4: E divides C by CKB57:1;
E divides B by A3,CKB57:1;
then E divides ( gcd (A,B,Amp) ) by A2,CKB56:def 1;
then A5: E divides D by A4,CKB56:def 1;
A6: (D is (Element of Amp) & E is (Element of Amp)) by CKB56:def 1;
A7: D divides ( gcd (A,B,Amp) ) by CKB56:def 1;
then A8: D divides A by CKB57:1;
D divides B by A7,CKB57:1;
then D divides ( gcd (B,C,Amp) ) by A1,CKB56:def 1;
then D divides E by A8,CKB56:def 1;
then D is_associated_to E by A5,CKB11:def 1;
hence thesis by A6,CKB41:1;
end;
