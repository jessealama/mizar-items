begin
reserve R for  domRing-like  commutative Ring;
theorem
Th8: (for a,b being (Element of R) holds ((b divides a & b <> ( 0. R )) implies (( a / b ) = ( 0. R ) iff a = ( 0. R ))))
proof
let a being (Element of R);
let b being (Element of R);
assume that
A1: b divides a
and
A2: b <> ( 0. R );
hereby
assume ( a / b ) = ( 0. R );
then a = ( ( 0. R ) * b ) by A1,A2,CKB14:def 1
.= ( 0. R ) by VECTSP_1:7;
hence a = ( 0. R );
end;
assume a = ( 0. R );
then ( 0. R ) = ( ( a / b ) * b ) by A1,A2,CKB14:def 1;
hence thesis by A2,VECTSP_2:def 1;
end;
