begin
reserve R for gcdDomain;
theorem
Th38: (for Amp being (AmpleSet of R) holds (for a,b,c being (Element of R) holds (( gcd (a,b,Amp) ) = ( 1. R ) implies ( gcd (a,( b * c ),Amp) ) = ( gcd (a,c,Amp) ))))
proof
let Amp being (AmpleSet of R);
let A being (Element of R);
let B being (Element of R);
let C being (Element of R);
assume ( gcd (A,B,Amp) ) = ( 1. R );
then A1: ( C * ( gcd (A,B,Amp) ) ) = C by VECTSP_1:def 4;
( gcd (( A * C ),( B * C ),Amp) ) is_associated_to ( C * ( gcd (A,B,Amp) ) ) by CKB65:1;
then ( gcd (A,C,Amp) ) is_associated_to ( gcd (A,( gcd (( A * C ),( B * C ),Amp) ),Amp) ) by A1,CKB63:1;
then A2: ( gcd (A,C,Amp) ) is_associated_to ( gcd (( gcd (A,( A * C ),Amp) ),( B * C ),Amp) ) by CKB64:1;
A3: ( A * ( gcd (( 1. R ),C,Amp) ) ) = ( A * ( 1. R ) ) by CKB61:1
.= A by VECTSP_1:def 4;
( gcd (( A * ( 1. R ) ),( A * C ),Amp) ) is_associated_to ( A * ( gcd (( 1. R ),C,Amp) ) ) by CKB65:1;
then ( gcd (A,( A * C ),Amp) ) is_associated_to A by A3,VECTSP_1:def 4;
then A4: ( gcd (( gcd (A,( A * C ),Amp) ),( B * C ),Amp) ) is_associated_to ( gcd (A,( B * C ),Amp) ) by CKB63:1;
(( gcd (A,( B * C ),Amp) ) is (Element of Amp) & ( gcd (A,C,Amp) ) is (Element of Amp)) by CKB56:def 1;
hence thesis by A2,A4,CKB17:1,CKB41:1;
end;
