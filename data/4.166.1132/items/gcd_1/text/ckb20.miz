begin
reserve R for  domRing-like  commutative Ring;
reserve c for (Element of R);
theorem
Th7: (for R being  associative non  empty multLoopStr holds (for a,b,c being (Element of R) holds (a divides b implies a divides ( b * c ))))
proof
let R being  associative non  empty multLoopStr;
let a being (Element of R);
let b being (Element of R);
let c being (Element of R);
assume a divides b;
then consider d being (Element of R) such that A1: ( a * d ) = b by CKB8:def 1;
( a * ( d * c ) ) = ( b * c ) by A1,GROUP_1:def 3;
hence thesis by CKB8:def 1;
end;
