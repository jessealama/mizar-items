constructors BINOP_2,VECTSP_2,MONOID_0,CKB8,CKB9,CKB10,CKB11,CKB13,CKB14,CKB33,CKB36,CKB39,CKB40,CKB44,CKB47,CKB49;
requirements SUBSET,BOOLE;
begin
reserve R for  domRing-like  commutative Ring;
definition
let R being  gcd-like  associative  well-unital non  empty multLoopStr;
let Amp being (AmpleSet of R);
let x being (Element of R);
let y being (Element of R);
func gcd (x,y,Amp) -> (Element of R) means 
:Def12: (((it in Amp & it divides x) & it divides y) & (for z being (Element of R) holds ((z divides x & z divides y) implies z divides it)));
existence
proof
consider u being (Element of R) such that A1: (u divides x & u divides y) and A2: (for zz being (Element of R) holds ((zz divides x & zz divides y) implies zz divides u)) by CKB49:def 1;
consider z being (Element of Amp) such that A3: z is_associated_to u by CKB41:1;
A4: (for zz being (Element of R) holds ((zz divides x & zz divides y) implies zz divides z))
proof
let zz being (Element of R);
assume (zz divides x & zz divides y);
then A5: zz divides u by A2;
u divides z by A3,CKB11:def 1;
hence thesis by A5,CKB15:1;
end;
z divides u by A3,CKB11:def 1;
then (z divides x & z divides y) by A1,CKB15:1;
hence thesis by A4;
end;
uniqueness
proof
now
let z1 being (Element of R);
assume that
A6: z1 in Amp
and
A7: ((z1 divides x & z1 divides y) & (for z being (Element of R) holds ((z divides x & z divides y) implies z divides z1)));
let z2 being (Element of R);
assume that
A8: z2 in Amp
and
A9: ((z2 divides x & z2 divides y) & (for z being (Element of R) holds ((z divides x & z divides y) implies z divides z2)));
(z1 divides z2 & z2 divides z1) by A7,A9;
then z1 is_associated_to z2 by CKB11:def 1;
hence z1 = z2 by A6,A8,CKB41:1;
end;
hence thesis;
end;
end;
