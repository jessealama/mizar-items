begin
reserve R for gcdDomain;
theorem
Th35: (for Amp being (AmpleSet of R) holds (for a,b,c being (Element of R) holds (b is_associated_to c implies (( gcd (a,b,Amp) ) is_associated_to ( gcd (a,c,Amp) ) & ( gcd (b,a,Amp) ) is_associated_to ( gcd (c,a,Amp) )))))
proof
let Amp being (AmpleSet of R);
let A being (Element of R);
let B being (Element of R);
let C being (Element of R);
A1: ( gcd (A,B,Amp) ) divides B by CKB56:def 1;
A2: ( gcd (A,B,Amp) ) divides A by CKB56:def 1;
A3: ( gcd (A,C,Amp) ) divides A by CKB56:def 1;
A4: ( gcd (A,C,Amp) ) divides C by CKB56:def 1;
A5: ( gcd (A,B,Amp) ) = ( gcd (B,A,Amp) ) by CKB58:1;
assume A6: B is_associated_to C;
then C divides B by CKB11:def 1;
then ( gcd (A,C,Amp) ) divides B by A4,CKB15:1;
then A7: ( gcd (A,C,Amp) ) divides ( gcd (A,B,Amp) ) by A3,CKB56:def 1;
B divides C by A6,CKB11:def 1;
then ( gcd (A,B,Amp) ) divides C by A1,CKB15:1;
then ( gcd (A,B,Amp) ) divides ( gcd (A,C,Amp) ) by A2,CKB56:def 1;
then ( gcd (A,B,Amp) ) is_associated_to ( gcd (A,C,Amp) ) by A7,CKB11:def 1;
hence thesis by A5,CKB58:1;
end;
