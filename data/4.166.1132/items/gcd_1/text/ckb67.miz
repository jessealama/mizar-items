begin
reserve R for gcdDomain;
theorem
Th39: (for Amp being (AmpleSet of R) holds (for a,b,c being (Element of R) holds ((c = ( gcd (a,b,Amp) ) & c <> ( 0. R )) implies ( gcd (( a / c ),( b / c ),Amp) ) = ( 1. R ))))
proof
let Amp being (AmpleSet of R);
let A being (Element of R);
let B being (Element of R);
let C being (Element of R);
assume that
A1: C = ( gcd (A,B,Amp) )
and
A2: C <> ( 0. R );
set A1 = ( A / C );
C divides A by A1,CKB56:def 1;
then A3: ( A1 * C ) = A by A2,CKB14:def 1;
set B1 = ( B / C );
A4: ( gcd (( A1 * C ),( B1 * C ),Amp) ) is_associated_to ( C * ( gcd (A1,B1,Amp) ) ) by CKB65:1;
A5: (( gcd (A1,B1,Amp) ) is (Element of Amp) & ( 1. R ) is (Element of Amp)) by CKB40:def 1,CKB56:def 1;
C divides B by A1,CKB56:def 1;
then ( gcd (A,B,Amp) ) = ( gcd (( A1 * C ),( B1 * C ),Amp) ) by A2,A3,CKB14:def 1;
then ( C * ( 1. R ) ) is_associated_to ( C * ( gcd (A1,B1,Amp) ) ) by A1,A4,VECTSP_1:def 4;
hence thesis by A2,A5,CKB32:1,CKB41:1;
end;
