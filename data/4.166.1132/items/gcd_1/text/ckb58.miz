begin
reserve R for gcdDomain;
theorem
Th30: (for Amp being (AmpleSet of R) holds (for a,b being (Element of R) holds ( gcd (a,b,Amp) ) = ( gcd (b,a,Amp) )))
proof
let Amp being (AmpleSet of R);
let A being (Element of R);
let B being (Element of R);
set D = ( gcd (A,B,Amp) );
A1: (D divides A & (for z being (Element of R) holds ((z divides B & z divides A) implies z divides D))) by CKB56:def 1;
(D in Amp & D divides B) by CKB56:def 1;
hence thesis by A1,CKB56:def 1;
end;
