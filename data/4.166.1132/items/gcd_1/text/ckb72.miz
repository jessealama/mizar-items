begin
reserve R for gcdDomain;
theorem
Th43: (for Amp,Amp9 being (AmpleSet of R) holds (for x,y being (Element of R) holds (x,y are_canonical_wrt Amp implies x,y are_canonical_wrt Amp9)))
proof
let Amp being (AmpleSet of R);
let Amp9 being (AmpleSet of R);
let x being (Element of R);
let y being (Element of R);
( ( 1. R ) * x ) = x by VECTSP_1:def 8;
then A1: ( 1. R ) divides x by CKB8:def 1;
( ( 1. R ) * y ) = y by VECTSP_1:def 8;
then A2: ( 1. R ) divides y by CKB8:def 1;
assume x,y are_canonical_wrt Amp;
then ( gcd (x,y,Amp) ) = ( 1. R ) by CKB71:def 1;
then A3: (for z being (Element of R) holds ((z divides x & z divides y) implies z divides ( 1. R ))) by CKB56:def 1;
( 1. R ) in Amp9 by CKB40:def 1;
then ( gcd (x,y,Amp9) ) = ( 1. R ) by A3,A1,A2,CKB56:def 1;
hence thesis by CKB71:def 1;
end;
