begin
reserve R for gcdDomain;
theorem
(for Amp being (AmpleSet of R) holds (for r1,r2,s1,s2 being (Element of R) holds (((Amp is  multiplicative & r1,r2 are_normalized_wrt Amp) & s1,s2 are_normalized_wrt Amp) implies ( mult1 (r1,r2,s1,s2,Amp) ),( mult2 (r1,r2,s1,s2,Amp) ) are_normalized_wrt Amp)))
proof
let Amp being (AmpleSet of R);
let r1 being (Element of R);
let r2 being (Element of R);
let s1 being (Element of R);
let s2 being (Element of R);
assume that
A1: Amp is  multiplicative
and
A2: r1,r2 are_normalized_wrt Amp
and
A3: s1,s2 are_normalized_wrt Amp;
A4: ( gcd (r1,r2,Amp) ) = ( 1. R ) by A2,CKB76:def 1;
then A5: r1,r2 are_co-prime  by CKB73:def 1;
s2 in Amp by A3,CKB76:def 1;
then A6: s2 = ( NF (s2,Amp) ) by CKB44:def 1;
r2 in Amp by A2,CKB76:def 1;
then A7: r2 = ( NF (r2,Amp) ) by CKB44:def 1;
A8: r2 <> ( 0. R ) by A2,CKB76:def 1;
then A9: ( gcd (s1,r2,Amp) ) <> ( 0. R ) by CKB62:1;
A10: ( gcd (s1,s2,Amp) ) = ( 1. R ) by A3,CKB76:def 1;
then A11: s1,s2 are_co-prime  by CKB73:def 1;
A12: s2 <> ( 0. R ) by A3,CKB76:def 1;
then A13: ( gcd (r1,s2,Amp) ) <> ( 0. R ) by CKB62:1;
now
per cases ;
case A14: (r1 = ( 0. R ) or s1 = ( 0. R ));
A15: (( 1. R ) in Amp & ( 1. R ) <> ( 0. R )) by CKB41:1;
A16: ( mult2 (r1,r2,s1,s2,Amp) ) = ( 1. R ) by A5,A11,A7,A6,A14,CKB82:def 1;
then ( gcd (( mult1 (r1,r2,s1,s2,Amp) ),( mult2 (r1,r2,s1,s2,Amp) ),Amp) ) = ( 1. R ) by CKB61:1;
hence thesis by A16,A15,CKB76:def 1;
end;
case A17: (r2 = ( 1. R ) & s2 = ( 1. R ));
A18: (( 1. R ) in Amp & ( 1. R ) <> ( 0. R )) by CKB41:1;
A19: ( mult2 (r1,r2,s1,s2,Amp) ) = ( 1. R ) by A5,A11,A7,A17,CKB82:def 1;
then ( gcd (( mult1 (r1,r2,s1,s2,Amp) ),( mult2 (r1,r2,s1,s2,Amp) ),Amp) ) = ( 1. R ) by CKB61:1;
hence thesis by A19,A18,CKB76:def 1;
end;
case A20: (s2 <> ( 0. R ) & r2 = ( 1. R ));
then A21: ( gcd (s1,r2,Amp) ) = ( 1. R ) by CKB61:1;
then ( r2 / ( gcd (s1,r2,Amp) ) ) = ( 1. R ) by A20,CKB22:1;
then A22: ( s2 / ( gcd (r1,s2,Amp) ) ) = ( ( s2 / ( gcd (r1,s2,Amp) ) ) * ( r2 / ( gcd (s1,r2,Amp) ) ) ) by VECTSP_1:def 4;
A23: ( gcd (r1,s2,Amp) ) divides r1 by CKB56:def 1;
then ( gcd (r1,s2,Amp) ) divides ( r1 * s1 ) by CKB20:1;
then A24: ( ( r1 * s1 ) / ( gcd (r1,s2,Amp) ) ) = ( ( r1 / ( gcd (r1,s2,Amp) ) ) * s1 ) by A13,A23,CKB24:1
.= ( ( r1 / ( gcd (r1,s2,Amp) ) ) * ( s1 / ( gcd (s1,r2,Amp) ) ) ) by A21,CKB23:1;
A25: ( mult2 (r1,r2,s1,s2,Amp) ) = ( s2 / ( gcd (r1,s2,Amp) ) ) by A5,A11,A7,A6,A20,CKB82:def 1;
reconsider zz = ( gcd (r1,s2,Amp) ) as (Element of Amp) by CKB56:def 1;
A26: (( gcd (r1,s2,Amp) ) divides s2 & ( gcd (r1,s2,Amp) ) <> ( 0. R )) by A12,CKB56:def 1,CKB62:1;
then A27: ( s2 / ( gcd (r1,s2,Amp) ) ) <> ( 0. R ) by A12,CKB21:1;
( mult1 (r1,r2,s1,s2,Amp) ) = ( ( r1 * s1 ) / ( gcd (r1,s2,Amp) ) ) by A20,CKB81:def 1;
then A28: ( gcd (( mult1 (r1,r2,s1,s2,Amp) ),( mult2 (r1,r2,s1,s2,Amp) ),Amp) ) = ( 1. R ) by A4,A10,A8,A12,A25,A24,A22,CKB70:1;
reconsider s2 as (Element of Amp) by A3,CKB76:def 1;
( s2 / zz ) in Amp by A1,A26,CKB48:1;
hence thesis by A25,A28,A27,CKB76:def 1;
end;
case A29: (r2 <> ( 0. R ) & s2 = ( 1. R ));
then A30: ( gcd (r1,s2,Amp) ) = ( 1. R ) by CKB61:1;
then ( s2 / ( gcd (r1,s2,Amp) ) ) = ( 1. R ) by A29,CKB22:1;
then A31: ( r2 / ( gcd (s1,r2,Amp) ) ) = ( ( r2 / ( gcd (s1,r2,Amp) ) ) * ( s2 / ( gcd (r1,s2,Amp) ) ) ) by VECTSP_1:def 4;
A32: ( gcd (s1,r2,Amp) ) divides s1 by CKB56:def 1;
then ( gcd (s1,r2,Amp) ) divides ( r1 * s1 ) by CKB20:1;
then A33: ( ( r1 * s1 ) / ( gcd (s1,r2,Amp) ) ) = ( r1 * ( s1 / ( gcd (s1,r2,Amp) ) ) ) by A9,A32,CKB24:1
.= ( ( r1 / ( gcd (r1,s2,Amp) ) ) * ( s1 / ( gcd (s1,r2,Amp) ) ) ) by A30,CKB23:1;
A34: ( mult2 (r1,r2,s1,s2,Amp) ) = ( r2 / ( gcd (s1,r2,Amp) ) ) by A5,A11,A7,A6,A29,CKB82:def 1;
reconsider zz = ( gcd (s1,r2,Amp) ) as (Element of Amp) by CKB56:def 1;
A35: (( gcd (s1,r2,Amp) ) divides r2 & ( gcd (s1,r2,Amp) ) <> ( 0. R )) by A8,CKB56:def 1,CKB62:1;
then A36: ( r2 / ( gcd (s1,r2,Amp) ) ) <> ( 0. R ) by A8,CKB21:1;
( mult1 (r1,r2,s1,s2,Amp) ) = ( ( r1 * s1 ) / ( gcd (s1,r2,Amp) ) ) by A29,CKB81:def 1;
then A37: ( gcd (( mult1 (r1,r2,s1,s2,Amp) ),( mult2 (r1,r2,s1,s2,Amp) ),Amp) ) = ( 1. R ) by A4,A10,A8,A12,A34,A33,A31,CKB70:1;
reconsider r2 as (Element of Amp) by A2,CKB76:def 1;
( r2 / zz ) in Amp by A1,A35,CKB48:1;
hence thesis by A34,A37,A36,CKB76:def 1;
end;
case A38: ((((not (r1 = ( 0. R ) or s1 = ( 0. R ))) & (not (r2 = ( 1. R ) & s2 = ( 1. R )))) & (not (s2 <> ( 0. R ) & r2 = ( 1. R )))) & (not (r2 <> ( 0. R ) & s2 = ( 1. R ))));
reconsider z2 = ( gcd (s1,r2,Amp) ) as (Element of Amp) by CKB56:def 1;
reconsider z1 = ( gcd (r1,s2,Amp) ) as (Element of Amp) by CKB56:def 1;
A39: (( gcd (r1,s2,Amp) ) divides s2 & ( gcd (r1,s2,Amp) ) <> ( 0. R )) by A12,CKB56:def 1,CKB62:1;
then A40: ( s2 / ( gcd (r1,s2,Amp) ) ) <> ( 0. R ) by A12,CKB21:1;
A41: ( mult2 (r1,r2,s1,s2,Amp) ) = ( ( r2 / ( gcd (s1,r2,Amp) ) ) * ( s2 / ( gcd (r1,s2,Amp) ) ) ) by A5,A11,A7,A6,A38,CKB82:def 1;
( mult1 (r1,r2,s1,s2,Amp) ) = ( ( r1 / ( gcd (r1,s2,Amp) ) ) * ( s1 / ( gcd (s1,r2,Amp) ) ) ) by A38,CKB81:def 1;
then A42: ( gcd (( mult1 (r1,r2,s1,s2,Amp) ),( mult2 (r1,r2,s1,s2,Amp) ),Amp) ) = ( 1. R ) by A4,A10,A8,A12,A41,CKB70:1;
A43: (( gcd (s1,r2,Amp) ) divides r2 & ( gcd (s1,r2,Amp) ) <> ( 0. R )) by A8,CKB56:def 1,CKB62:1;
then A44: ( r2 / ( gcd (s1,r2,Amp) ) ) <> ( 0. R ) by A8,CKB21:1;
reconsider s2 as (Element of Amp) by A3,CKB76:def 1;
reconsider u = ( s2 / z1 ) as (Element of Amp) by A1,A39,CKB48:1;
reconsider r2 as (Element of Amp) by A2,CKB76:def 1;
reconsider v = ( r2 / z2 ) as (Element of Amp) by A1,A43,CKB48:1;
A45: ( v * u ) <> ( 0. R ) by A40,A44,VECTSP_2:def 1;
( v * u ) in Amp by A1,CKB47:def 1;
hence thesis by A41,A42,A45,CKB76:def 1;
end;
end;
hence thesis;
