constructors BINOP_2,VECTSP_2,MONOID_0,CKB8,CKB9,CKB10,CKB11,CKB13,CKB14,CKB33,CKB36,CKB39,CKB40,CKB44,CKB47;
requirements SUBSET,BOOLE;
begin
reserve R for  domRing-like  commutative Ring;
theorem
Th27: (for Amp being (AmpleSet of R) holds (Amp is  multiplicative implies (for x,y being (Element of Amp) holds ((y divides x & y <> ( 0. R )) implies ( x / y ) in Amp))))
proof
let Amp being (AmpleSet of R);
assume A1: Amp is  multiplicative;
let x being (Element of Amp);
let y being (Element of Amp);
assume that
A2: y divides x
and
A3: y <> ( 0. R );
now
per cases ;
case A4: x <> ( 0. R );
set d = ( x / y );
consider d9 being (Element of Amp) such that A5: d9 is_associated_to d by CKB41:1;
consider u being (Element of R) such that A6: u is  unital and A7: ( d * u ) = d9 by A5,CKB31:1;
x = ( y * d ) by A2,A3,CKB14:def 1;
then A8: ( u * x ) = ( y * d9 ) by A7,GROUP_1:def 3;
A9: x is_associated_to ( u * x )
proof
u divides ( 1. R ) by A6,CKB10:def 1;
then consider e being (Element of R) such that A10: ( u * e ) = ( 1. R ) by CKB8:def 1;
x divides x;
then A11: x divides ( u * x ) by CKB20:1;
( ( u * x ) * e ) = ( ( e * u ) * x ) by GROUP_1:def 3
.= x by A10,VECTSP_1:def 8;
then ( u * x ) divides x by CKB8:def 1;
hence thesis by A11,CKB11:def 1;
end;
A12: ( y * d9 ) in Amp by A1,CKB47:def 1;
( ( 1. R ) * x ) = x by VECTSP_1:def 8
.= ( u * x ) by A8,A12,A9,CKB41:1;
then u = ( 1. R ) by A4,CKB7:1;
then d9 = d by A7,VECTSP_1:def 4;
hence thesis;
end;
case A13: x = ( 0. R );
set d = ( x / y );
x = ( y * d ) by A2,A3,CKB14:def 1;
then A14: d = ( 0. R ) by A3,A13,VECTSP_2:def 1;
( 0. R ) is (Element of Amp) by CKB43:1;
hence thesis by A14;
end;
end;
hence thesis;
end;
