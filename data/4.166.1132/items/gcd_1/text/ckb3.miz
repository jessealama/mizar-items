begin
registration
cluster  commutative  left-distributive ->  distributive for non  empty non  empty doubleLoopStr;
coherence
proof
let R being non  empty doubleLoopStr;
assume that
A3: R is  commutative
and
A4: R is  left-distributive;
let x being (Element of R);
let y being (Element of R);
let z being (Element of R);
thus ( x * ( y + z ) ) = ( ( y + z ) * x ) by A3,GROUP_1:def 12
.= ( ( y * x ) + ( z * x ) ) by A4,VECTSP_1:def 3
.= ( ( x * y ) + ( z * x ) ) by A3,GROUP_1:def 12
.= ( ( x * y ) + ( x * z ) ) by A3,GROUP_1:def 12;
thus thesis by A4,VECTSP_1:def 3;
end;
end;
