begin
theorem
Th10: (for R being RelStr holds (R is  quasi_ordered implies ( <=E R ) partially_orders ( Class ( EqRel R ) )))
proof
let R being RelStr;
set CR = (the carrier of R);
set IR = (the InternalRel of R);
assume A1: R is  quasi_ordered;
then R is  transitive by CKB11:def 1;
then A2: IR is_transitive_in CR by ORDERS_2:def 3;
thus ( <=E R ) is_reflexive_in ( Class ( EqRel R ) )
proof
let x being set;
assume x in ( Class ( EqRel R ) );
then consider a being set such that A3: a in CR and A4: x = ( Class (( EqRel R ),a) ) by EQREL_1:def 3;
R is  reflexive by A1,CKB11:def 1;
then IR is_reflexive_in CR by ORDERS_2:def 2;
then A5: [ a,a ] in IR by A3,RELAT_2:def 1;
reconsider a9 = a as (Element of R) by A3;
a9 <= a9 by A5,ORDERS_2:def 5;
hence thesis by A4,CKB14:def 1;
end;

thus ( <=E R ) is_transitive_in ( Class ( EqRel R ) )
proof
let x being set,y being set,z being set;
assume that
A6: x in ( Class ( EqRel R ) )
and
y in ( Class ( EqRel R ) )
and
z in ( Class ( EqRel R ) )
and
A7: [ x,y ] in ( <=E R )
and
A8: [ y,z ] in ( <=E R );
consider a being (Element of R),b being (Element of R) such that A9: x = ( Class (( EqRel R ),a) ) and A10: y = ( Class (( EqRel R ),b) ) and A11: a <= b by A7,CKB14:def 1;
consider c being (Element of R),d being (Element of R) such that A12: y = ( Class (( EqRel R ),c) ) and A13: z = ( Class (( EqRel R ),d) ) and A14: c <= d by A8,CKB14:def 1;
A15: [ a,b ] in IR by A11,ORDERS_2:def 5;
A16: [ c,d ] in IR by A14,ORDERS_2:def 5;
A17: (ex x1 being set st (x1 in CR & x = ( Class (( EqRel R ),x1) ))) by A6,EQREL_1:def 3;
then b in ( Class (( EqRel R ),c) ) by A10,A12,EQREL_1:23;
then [ b,c ] in ( EqRel R ) by EQREL_1:19;
then [ b,c ] in ( IR /\ ( IR ~ ) ) by A1,CKB12:def 1;
then [ b,c ] in IR by XBOOLE_0:def 4;
then [ a,c ] in IR by A2,A15,A17,RELAT_2:def 8;
then [ a,d ] in IR by A2,A16,A17,RELAT_2:def 8;
then a <= d by ORDERS_2:def 5;
hence thesis by A9,A13,CKB14:def 1;
end;

thus ( <=E R ) is_antisymmetric_in ( Class ( EqRel R ) )
proof
let x being set,y being set;
assume that
A18: x in ( Class ( EqRel R ) )
and
y in ( Class ( EqRel R ) )
and
A19: [ x,y ] in ( <=E R )
and
A20: [ y,x ] in ( <=E R );
consider a being (Element of R),b being (Element of R) such that A21: x = ( Class (( EqRel R ),a) ) and A22: y = ( Class (( EqRel R ),b) ) and A23: a <= b by A19,CKB14:def 1;
consider c being (Element of R),d being (Element of R) such that A24: y = ( Class (( EqRel R ),c) ) and A25: x = ( Class (( EqRel R ),d) ) and A26: c <= d by A20,CKB14:def 1;
A27: [ a,b ] in IR by A23,ORDERS_2:def 5;
A28: [ c,d ] in IR by A26,ORDERS_2:def 5;
A29: (ex x1 being set st (x1 in CR & x = ( Class (( EqRel R ),x1) ))) by A18,EQREL_1:def 3;
then A30: d in ( Class (( EqRel R ),a) ) by A21,A25,EQREL_1:23;
a in ( Class (( EqRel R ),a) ) by A29,EQREL_1:20;
then A31: [ a,d ] in ( EqRel R ) by A30,EQREL_1:22;
A32: c in ( Class (( EqRel R ),b) ) by A22,A24,A29,EQREL_1:23;
b in ( Class (( EqRel R ),b) ) by A29,EQREL_1:20;
then A33: [ b,c ] in ( EqRel R ) by A32,EQREL_1:22;
[ a,d ] in ( IR /\ ( IR ~ ) ) by A1,A31,CKB12:def 1;
then [ a,d ] in ( IR ~ ) by XBOOLE_0:def 4;
then A34: [ d,a ] in IR by RELAT_1:def 7;
[ b,c ] in ( IR /\ ( IR ~ ) ) by A1,A33,CKB12:def 1;
then [ b,c ] in IR by XBOOLE_0:def 4;
then [ b,d ] in IR by A2,A28,A29,RELAT_2:def 8;
then A35: [ b,a ] in IR by A2,A29,A34,RELAT_2:def 8;
[ b,a ] in ( IR ~ ) by A27,RELAT_1:def 7;
then [ b,a ] in ( IR /\ ( IR ~ ) ) by A35,XBOOLE_0:def 4;
then [ b,a ] in ( EqRel R ) by A1,CKB12:def 1;
then b in ( Class (( EqRel R ),a) ) by EQREL_1:19;
hence thesis by A21,A22,EQREL_1:23;
end;

