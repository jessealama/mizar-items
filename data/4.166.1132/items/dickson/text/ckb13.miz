begin
theorem
Th9: (for R being RelStr holds (for x,y being (Element of R) holds (R is  quasi_ordered implies (x in ( Class (( EqRel R ),y) ) iff (x <= y & y <= x)))))
proof
let R being RelStr;
let x being (Element of R);
let y being (Element of R);
assume that
A1: R is  quasi_ordered;
set IR = (the InternalRel of R);
hereby
assume x in ( Class (( EqRel R ),y) );
then [ x,y ] in ( EqRel R ) by EQREL_1:19;
then A2: [ x,y ] in ( IR /\ ( IR ~ ) ) by A1,CKB12:def 1;
then A3: [ x,y ] in IR by XBOOLE_0:def 4;
A4: [ x,y ] in ( IR ~ ) by A2,XBOOLE_0:def 4;
thus x <= y by A3,ORDERS_2:def 5;
[ y,x ] in IR by A4,RELAT_1:def 7;
hence y <= x by ORDERS_2:def 5;
end;
assume that
A5: x <= y
and
A6: y <= x;
A7: [ y,x ] in IR by A6,ORDERS_2:def 5;
A8: [ x,y ] in IR by A5,ORDERS_2:def 5;
[ x,y ] in ( IR ~ ) by A7,RELAT_1:def 7;
then [ x,y ] in ( IR /\ ( IR ~ ) ) by A8,XBOOLE_0:def 4;
then [ x,y ] in ( EqRel R ) by A1,CKB12:def 1;
hence thesis by EQREL_1:19;
end;
