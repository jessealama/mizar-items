requirements BOOLE,SUBSET,NUMERALS;
begin
theorem
Th20: (for R being non  empty RelStr holds (for N being (Subset of R) holds (for x being set holds ((R is  quasi_ordered & x in ( min-classes N )) implies (for y being (Element of ( R \~ )) holds (y in x implies y is_minimal_wrt N,(the InternalRel of ( R \~ ))))))))
proof
let R being non  empty RelStr;
let N being (Subset of R);
let x being set;
assume that
A1: R is  quasi_ordered
and
A2: x in ( min-classes N );
set IR = (the InternalRel of R);
set CR = (the carrier of R);
set IR9 = (the InternalRel of ( R \~ ));
let c being (Element of ( R \~ ));
assume that
A3: c in x;
consider b being (Element of ( R \~ )) such that A4: b is_minimal_wrt N,IR9 and A5: x = ( ( Class (( EqRel R ),b) ) /\ N ) by A2,CKB32:def 1;
c in ( Class (( EqRel R ),b) ) by A3,A5,XBOOLE_0:def 4;
then [ c,b ] in ( EqRel R ) by EQREL_1:19;
then [ c,b ] in ( IR /\ ( IR ~ ) ) by A1,CKB12:def 1;
then A6: [ c,b ] in IR by XBOOLE_0:def 4;
A7:now
assume (ex d being set st ((d in N & d <> c) & [ d,c ] in IR9));
then consider d being set such that A8: d in N and d <> c and A9: [ d,c ] in IR9;
A10: (not [ d,c ] in ( IR ~ )) by A9,XBOOLE_0:def 5;
R is  transitive by A1,CKB11:def 1;
then A11: IR is_transitive_in CR by ORDERS_2:def 3;
then A12: [ d,b ] in IR by A6,A8,A9,RELAT_2:def 8;
now
assume [ d,b ] in ( IR ~ );
then [ b,d ] in IR by RELAT_1:def 7;
then [ c,d ] in IR by A6,A8,A11,RELAT_2:def 8;
hence contradiction by A10,RELAT_1:def 7;
end;
then A13: [ d,b ] in IR9 by A12,XBOOLE_0:def 5;
b <> d by A6,A10,RELAT_1:def 7;
hence contradiction by A4,A8,A13,WAYBEL_4:def 25;
end;
c in N by A3,A5,XBOOLE_0:def 4;
hence thesis by A7,WAYBEL_4:def 25;
end;
