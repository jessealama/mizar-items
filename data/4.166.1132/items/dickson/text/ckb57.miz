begin
theorem
Th39: (for R,S being RelStr holds (((R,S are_isomorphic  & R is  Dickson) & R is  quasi_ordered) implies (S is  quasi_ordered & S is  Dickson)))
proof
let R being RelStr;
let S being RelStr;
assume that
A1: R,S are_isomorphic 
and
A2: R is  Dickson
and
A3: R is  quasi_ordered;
set CRS = (the carrier of S);
set IRS = (the InternalRel of S);
per cases ;
suppose ((not R is  empty) & (not S is  empty));

then reconsider Re = R,Se = S as non  empty RelStr;
consider f being (Function of Re,Se) such that A4: f is  isomorphic by A1,WAYBEL_1:def 8;
A5: f is  one-to-one by A4,WAYBEL_0:66;
A6: ( rng f ) = (the carrier of Se) by A4,WAYBEL_0:66;
A7: Re is  reflexive by A3,CKB11:def 1;
A8: Re is  transitive by A3,CKB11:def 1;
A9: Se is  reflexive by A1,A7,WAYBEL20:15;
Se is  transitive by A1,A8,WAYBEL20:16;
hence A10: S is  quasi_ordered by A9,CKB11:def 1;
now
let t being (sequence of Se);
reconsider fi = ( f " ) as (Function of (the carrier of Se),(the carrier of Re)) by A5,A6,FUNCT_2:25;
deffunc F((Element of ( NAT ))) = ( fi . ( t . $1 ) );
consider sr being (Function of ( NAT ),(the carrier of Re)) such that A11: (for x being (Element of ( NAT )) holds ( sr . x ) = F(x)) from FUNCT_2:sch 4;
reconsider sr as (sequence of Re);
consider i,j being (Element of ( NAT )) such that A12: i < j and A13: ( sr . i ) <= ( sr . j ) by A2,CKB47:1;
take i;
take j;
thus i < j by A12;
A14: ( f . ( sr . i ) ) = ( f . ( ( f " ) . ( t . i ) ) ) by A11
.= ( t . i ) by A5,A6,FUNCT_1:35;
( f . ( sr . j ) ) = ( f . ( ( f " ) . ( t . j ) ) ) by A11
.= ( t . j ) by A5,A6,FUNCT_1:35;
hence ( t . i ) <= ( t . j ) by A4,A13,A14,WAYBEL_0:66;
end;
then (for N being non  empty (Subset of Se) holds (( min-classes N ) is  finite & ( min-classes N ) is non  empty)) by A10,CKB49:1;
hence thesis by A10,CKB50:1;
end;
suppose A15: (not ((not R is  empty) & (not S is  empty)));

A16:now
per cases  by A15;
suppose A17: R is  empty;

(ex f being (Function of R,S) st f is  isomorphic) by A1,WAYBEL_1:def 8;
hence S is  empty by A17,WAYBEL_0:def 38;
end;
suppose S is  empty;

hence S is  empty;
end;
end;
then (for x being set holds (x in CRS implies [ x,x ] in IRS));
then A18: IRS is_reflexive_in CRS by RELAT_2:def 1;
(for x,y,z being set holds (((((x in CRS & y in CRS) & z in CRS) & [ x,y ] in IRS) & [ y,z ] in IRS) implies [ x,z ] in IRS)) by A16;
then A19: IRS is_transitive_in CRS by RELAT_2:def 8;
A20: S is  reflexive by A18,ORDERS_2:def 2;
S is  transitive by A19,ORDERS_2:def 3;
hence S is  quasi_ordered by A20,CKB11:def 1;
thus thesis by A16,CKB55:1;
end;
end;
