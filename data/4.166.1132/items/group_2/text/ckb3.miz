definition
let G;
let A;
func A " -> (Subset of G) equals 
{ ( g " ): g in A };
coherence
proof
set F = { ( g " ): g in A };
F c= (the carrier of G)
proof
let x;
assume x in F;
then (ex g st (x = ( g " ) & g in A));
hence thesis;
end;
hence thesis;
end;
involutiveness
proof
let R being (Subset of G);
let B being (Subset of G);
assume A1: R = { ( g " ): g in B };
thus B c= { ( g " ): g in R }
proof
let a being set;
assume A2: a in B;
then reconsider a as (Element of G);
(( ( a " ) " ) = a & ( a " ) in R) by A1,A2;
hence thesis;
end;

let a being set;
assume a in { ( g " ): g in R };
then consider g such that A3: a = ( g " ) and A4: g in R;
(ex h st (g = ( h " ) & h in B)) by A1,A4;
hence thesis by A3;
end;
end;
