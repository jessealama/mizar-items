environ
vocabularies NUMBERS,SUBSET_1,REAL_1,SEQ_1,PARTFUN1,FDIFF_1,RELAT_1,CARD_3,FUNCT_1,FINSEQ_1,FINSEQ_2,PDIFF_1,RCOMP_1,TARSKI,ARYTM_1,ARYTM_3,XREAL_0,ORDINAL1,CARD_1,XXREAL_1,VALUED_0,SEQ_2,ORDINAL2,XXREAL_0,NAT_1,FUNCT_2,VALUED_1,XBOOLE_0,COMPLEX1,FCONT_1,PDIFF_2;
notations TARSKI,SUBSET_1,FUNCT_1,RELSET_1,PARTFUN1,FUNCT_2,XCMPLX_0,XREAL_0,COMPLEX1,ORDINAL1,NUMBERS,REAL_1,NAT_1,VALUED_0,VALUED_1,SEQ_1,SEQ_2,FINSEQ_1,FINSEQ_2,RCOMP_1,EUCLID,FDIFF_1,XXREAL_0,FCONT_1,PDIFF_1,CKB3,CKB18,CKB19;
definitions TARSKI,EUCLID,RCOMP_1,PDIFF_1,CKB3,CKB18,CKB19;
theorems TARSKI,XBOOLE_0,XBOOLE_1,XREAL_1,RCOMP_1,SEQ_4,SEQ_1,SEQ_2,FINSEQ_1,FINSEQ_2,RFUNCT_2,RELAT_1,FUNCT_1,FUNCT_2,PARTFUN1,FDIFF_1,VALUED_1,SEQM_3,RFUNCT_1,XCMPLX_0,XCMPLX_1,PDIFF_1,NAT_1,VALUED_0,ORDINAL1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20;
schemes SEQ_1;
registrations RELSET_1,XREAL_0,MEMBERED,FDIFF_1,FUNCT_2,NAT_1,NUMBERS,XBOOLE_0,XXREAL_0,FINSEQ_1,VALUED_0,VALUED_1,ORDINAL1,SEQ_4;
constructors REAL_1,SEQ_2,PARTFUN1,RFUNCT_2,COMPLEX1,RCOMP_1,FDIFF_1,SEQ_1,NAT_1,FCONT_1,PDIFF_1,BHSP_3,VFUNCT_1,SEQM_3,RELSET_1,BINOP_2,RVSUM_1,CKB3,CKB18,CKB19;
requirements SUBSET,REAL,BOOLE,NUMERALS,ARITHM;
begin
reserve y for Real;
reserve y0 for Real;
reserve z for (Element of ( REAL 2 ));
reserve z0 for (Element of ( REAL 2 ));
reserve Z for (Subset of ( REAL 2 ));
reserve f for (PartFunc of ( REAL 2 ),( REAL ));
reserve R for REST;
reserve L for LINEAR;
theorem
(f is_partial_differentiable`2_on Z implies (Z c= ( dom f ) & (for z holds (z in Z implies f is_partial_differentiable_in z,2))))
proof
set g = ( f | Z );
assume A1: f is_partial_differentiable`2_on Z;
hence Z c= ( dom f ) by CKB19:def 1;
let z0 being (Element of ( REAL 2 ));
assume z0 in Z;
then g is_partial_differentiable_in z0,2 by A1,CKB19:def 1;
then consider x0,y0 being Real such that A2: z0 = <* x0,y0 *> and A3: (ex N being (Neighbourhood of y0) st (N c= ( dom ( SVF1 (2,g,z0) ) ) & (ex L st (ex R st (for y holds (y in N implies ( ( ( SVF1 (2,g,z0) ) . y ) - ( ( SVF1 (2,g,z0) ) . y0 ) ) = ( ( L . ( y - y0 ) ) + ( R . ( y - y0 ) ) ))))))) by CKB11:1;
consider N being (Neighbourhood of y0) such that A4: N c= ( dom ( SVF1 (2,g,z0) ) ) and A5: (ex L st (ex R st (for y holds (y in N implies ( ( ( SVF1 (2,g,z0) ) . y ) - ( ( SVF1 (2,g,z0) ) . y0 ) ) = ( ( L . ( y - y0 ) ) + ( R . ( y - y0 ) ) ))))) by A3;
consider L,R such that A6: (for y holds (y in N implies ( ( ( SVF1 (2,g,z0) ) . y ) - ( ( SVF1 (2,g,z0) ) . y0 ) ) = ( ( L . ( y - y0 ) ) + ( R . ( y - y0 ) ) ))) by A5;
A7: (for y holds (y in N implies ( ( ( SVF1 (2,f,z0) ) . y ) - ( ( SVF1 (2,f,z0) ) . y0 ) ) = ( ( L . ( y - y0 ) ) + ( R . ( y - y0 ) ) )))
proof
let y;
A8: (for y holds (y in ( dom ( SVF1 (2,g,z0) ) ) implies ( ( SVF1 (2,g,z0) ) . y ) = ( ( SVF1 (2,f,z0) ) . y )))
proof
let y;
assume A9: y in ( dom ( SVF1 (2,g,z0) ) );
then A10: y in ( dom ( reproj (2,z0) ) ) by FUNCT_1:11;
A11: ( ( reproj (2,z0) ) . y ) in ( dom ( f | Z ) ) by A9,FUNCT_1:11;
( ( SVF1 (2,g,z0) ) . y ) = ( ( f | Z ) . ( ( reproj (2,z0) ) . y ) ) by A9,FUNCT_1:12
.= ( f . ( ( reproj (2,z0) ) . y ) ) by A11,FUNCT_1:47
.= ( ( SVF1 (2,f,z0) ) . y ) by A10,FUNCT_1:13;
hence thesis;
end;
A12: y0 in N by RCOMP_1:16;
assume A13: y in N;
then ( ( L . ( y - y0 ) ) + ( R . ( y - y0 ) ) ) = ( ( ( SVF1 (2,g,z0) ) . y ) - ( ( SVF1 (2,g,z0) ) . y0 ) ) by A6
.= ( ( ( SVF1 (2,f,z0) ) . y ) - ( ( SVF1 (2,g,z0) ) . y0 ) ) by A4,A13,A8
.= ( ( ( SVF1 (2,f,z0) ) . y ) - ( ( SVF1 (2,f,z0) ) . y0 ) ) by A4,A8,A12;
hence thesis;
end;
(for y being Real holds (y in ( dom ( SVF1 (2,g,z0) ) ) implies y in ( dom ( SVF1 (2,f,z0) ) )))
proof
let y;
( dom ( f | Z ) ) = ( ( dom f ) /\ Z ) by RELAT_1:61;
then A14: ( dom ( f | Z ) ) c= ( dom f ) by XBOOLE_1:17;
assume y in ( dom ( SVF1 (2,g,z0) ) );
then (y in ( dom ( reproj (2,z0) ) ) & ( ( reproj (2,z0) ) . y ) in ( dom ( f | Z ) )) by FUNCT_1:11;
hence thesis by A14,FUNCT_1:11;
end;
then (for y being set holds (y in ( dom ( SVF1 (2,g,z0) ) ) implies y in ( dom ( SVF1 (2,f,z0) ) )));
then ( dom ( SVF1 (2,g,z0) ) ) c= ( dom ( SVF1 (2,f,z0) ) ) by TARSKI:def 3;
then N c= ( dom ( SVF1 (2,f,z0) ) ) by A4,XBOOLE_1:1;
hence thesis by A2,A7,CKB11:1;
end;
