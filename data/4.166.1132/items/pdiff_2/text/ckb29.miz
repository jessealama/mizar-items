theorem
(for z0 being (Element of ( REAL 2 )) holds (f is_partial_differentiable_in z0,2 implies ( SVF1 (2,f,z0) ) is_continuous_in ( ( proj (2,2) ) . z0 )))
proof
let z0 being (Element of ( REAL 2 ));
