environ
vocabularies NUMBERS,SUBSET_1,REAL_1,SEQ_1,PARTFUN1,FDIFF_1,RELAT_1,CARD_3,FUNCT_1,FINSEQ_1,FINSEQ_2,PDIFF_1,RCOMP_1,TARSKI,ARYTM_1,ARYTM_3,XREAL_0,ORDINAL1,CARD_1,XXREAL_1,VALUED_0,SEQ_2,ORDINAL2,XXREAL_0,NAT_1,FUNCT_2,VALUED_1,XBOOLE_0,COMPLEX1,FCONT_1,PDIFF_2;
notations TARSKI,SUBSET_1,FUNCT_1,RELSET_1,PARTFUN1,FUNCT_2,XCMPLX_0,XREAL_0,COMPLEX1,ORDINAL1,NUMBERS,REAL_1,NAT_1,VALUED_0,VALUED_1,SEQ_1,SEQ_2,FINSEQ_1,FINSEQ_2,RCOMP_1,EUCLID,FDIFF_1,XXREAL_0,FCONT_1,PDIFF_1,CKB3,CKB18,CKB19;
definitions TARSKI,EUCLID,RCOMP_1,PDIFF_1,CKB3,CKB18,CKB19;
theorems TARSKI,XBOOLE_0,XBOOLE_1,XREAL_1,RCOMP_1,SEQ_4,SEQ_1,SEQ_2,FINSEQ_1,FINSEQ_2,RFUNCT_2,RELAT_1,FUNCT_1,FUNCT_2,PARTFUN1,FDIFF_1,VALUED_1,SEQM_3,RFUNCT_1,XCMPLX_0,XCMPLX_1,PDIFF_1,NAT_1,VALUED_0,ORDINAL1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21;
schemes SEQ_1;
registrations RELSET_1,XREAL_0,MEMBERED,FDIFF_1,FUNCT_2,NAT_1,NUMBERS,XBOOLE_0,XXREAL_0,FINSEQ_1,VALUED_0,VALUED_1,ORDINAL1,SEQ_4;
constructors REAL_1,SEQ_2,PARTFUN1,RFUNCT_2,COMPLEX1,RCOMP_1,FDIFF_1,SEQ_1,NAT_1,FCONT_1,PDIFF_1,BHSP_3,VFUNCT_1,SEQM_3,RELSET_1,BINOP_2,RVSUM_1,CKB3,CKB18,CKB19;
requirements SUBSET,REAL,BOOLE,NUMERALS,ARITHM;
begin
definition
let f being (PartFunc of ( REAL 2 ),( REAL ));
let Z being set;
assume A1: f is_partial_differentiable`1_on Z;
func f `partial1| Z -> (PartFunc of ( REAL 2 ),( REAL ))
means
(( dom it ) = Z & (for z being (Element of ( REAL 2 )) holds (z in Z implies ( it . z ) = ( partdiff (f,z,1) ))));
existence
proof
deffunc F((Element of ( REAL 2 ))) = ( partdiff (f,$1,1) );
defpred P[ (Element of ( REAL 2 )) ]
 means
$1 in Z;
consider F being (PartFunc of ( REAL 2 ),( REAL )) such that A2: ((for z being (Element of ( REAL 2 )) holds (z in ( dom F ) iff P[ z ])) & (for z being (Element of ( REAL 2 )) holds (z in ( dom F ) implies ( F . z ) = F(z)))) from SEQ_1:sch 3;
take F;
now
Z c= ( dom f ) by A1,CKB18:def 1;
then A3: Z is (Subset of ( REAL 2 )) by XBOOLE_1:1;
let y being set;
assume y in Z;
hence y in ( dom F ) by A2,A3;
end;
then A4: Z c= ( dom F ) by TARSKI:def 3;
(for y being set holds (y in ( dom F ) implies y in Z)) by A2;
then ( dom F ) c= Z by TARSKI:def 3;
hence ( dom F ) = Z by A4,XBOOLE_0:def 10;
let z being (Element of ( REAL 2 ));
assume z in Z;
then z in ( dom F ) by A2;
hence thesis by A2;
end;
uniqueness
proof
let F being (PartFunc of ( REAL 2 ),( REAL )),G being (PartFunc of ( REAL 2 ),( REAL ));
assume that
A5: ( dom F ) = Z
and
A6: (for z being (Element of ( REAL 2 )) holds (z in Z implies ( F . z ) = ( partdiff (f,z,1) )))
and
A7: ( dom G ) = Z
and
A8: (for z being (Element of ( REAL 2 )) holds (z in Z implies ( G . z ) = ( partdiff (f,z,1) )));
now
let z being (Element of ( REAL 2 ));
assume A9: z in ( dom F );
then ( F . z ) = ( partdiff (f,z,1) ) by A5,A6;
hence ( F . z ) = ( G . z ) by A5,A8,A9;
end;
hence thesis by A5,A7,PARTFUN1:5;
end;
end;
