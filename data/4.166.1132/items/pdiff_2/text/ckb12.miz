schemes SEQ_1;
registrations RELSET_1,XREAL_0,MEMBERED,FDIFF_1,FUNCT_2,NAT_1,NUMBERS,XBOOLE_0,XXREAL_0,FINSEQ_1,VALUED_0,VALUED_1,ORDINAL1,SEQ_4;
constructors REAL_1,SEQ_2,PARTFUN1,RFUNCT_2,COMPLEX1,RCOMP_1,FDIFF_1,SEQ_1,NAT_1,FCONT_1,PDIFF_1,BHSP_3,VFUNCT_1,SEQM_3,RELSET_1,BINOP_2,RVSUM_1,CKB3;
requirements SUBSET,REAL,BOOLE,NUMERALS,ARITHM;
begin
reserve x for Real;
reserve x0 for Real;
reserve x1 for Real;
reserve y0 for Real;
reserve y1 for Real;
reserve r for Real;
reserve z for (Element of ( REAL 2 ));
reserve f for (PartFunc of ( REAL 2 ),( REAL ));
reserve R for REST;
reserve L for LINEAR;
theorem
Lm1: ((z = <* x0,y0 *> & f is_partial_differentiable_in z,1) implies (r = ( diff (( SVF1 (1,f,z) ),x0) ) iff (ex x0,y0 being Real st (z = <* x0,y0 *> & (ex N being (Neighbourhood of x0) st (N c= ( dom ( SVF1 (1,f,z) ) ) & (ex L st (ex R st (r = ( L . 1 ) & (for x holds (x in N implies ( ( ( SVF1 (1,f,z) ) . x ) - ( ( SVF1 (1,f,z) ) . x0 ) ) = ( ( L . ( x - x0 ) ) + ( R . ( x - x0 ) ) ))))))))))))
proof
set F1 = ( SVF1 (1,f,z) );
assume that
A1: z = <* x0,y0 *>
and
A2: f is_partial_differentiable_in z,1;
A3: F1 is_differentiable_in x0 by A1,A2,CKB4:1;
hereby
assume A4: r = ( diff (F1,x0) );
F1 is_differentiable_in x0 by A1,A2,CKB4:1;
then (ex N being (Neighbourhood of x0) st (N c= ( dom F1 ) & (ex L st (ex R st (r = ( L . 1 ) & (for x holds (x in N implies ( ( F1 . x ) - ( F1 . x0 ) ) = ( ( L . ( x - x0 ) ) + ( R . ( x - x0 ) ) )))))))) by A4,FDIFF_1:def 5;
hence (ex x0,y0 being Real st (z = <* x0,y0 *> & (ex N being (Neighbourhood of x0) st (N c= ( dom ( SVF1 (1,f,z) ) ) & (ex L st (ex R st (r = ( L . 1 ) & (for x holds (x in N implies ( ( ( SVF1 (1,f,z) ) . x ) - ( ( SVF1 (1,f,z) ) . x0 ) ) = ( ( L . ( x - x0 ) ) + ( R . ( x - x0 ) ) )))))))))) by A1;
end;
given x1 being Real,y1 being Real such that
A5: z = <* x1,y1 *>
and
A6: (ex N being (Neighbourhood of x1) st (N c= ( dom ( SVF1 (1,f,z) ) ) & (ex L st (ex R st (r = ( L . 1 ) & (for x holds (x in N implies ( ( ( SVF1 (1,f,z) ) . x ) - ( ( SVF1 (1,f,z) ) . x1 ) ) = ( ( L . ( x - x1 ) ) + ( R . ( x - x1 ) ) ))))))));

x1 = x0 by A1,A5,FINSEQ_1:77;
hence thesis by A6,A3,FDIFF_1:def 5;
end;
