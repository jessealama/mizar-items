definitions TARSKI,EUCLID,RCOMP_1,PDIFF_1,CKB3;
theorems TARSKI,XBOOLE_0,XBOOLE_1,XREAL_1,RCOMP_1,SEQ_4,SEQ_1,SEQ_2,FINSEQ_1,FINSEQ_2,RFUNCT_2,RELAT_1,FUNCT_1,FUNCT_2,PARTFUN1,FDIFF_1,VALUED_1,SEQM_3,RFUNCT_1,XCMPLX_0,XCMPLX_1,PDIFF_1,NAT_1,VALUED_0,ORDINAL1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7;
schemes SEQ_1;
registrations RELSET_1,XREAL_0,MEMBERED,FDIFF_1,FUNCT_2,NAT_1,NUMBERS,XBOOLE_0,XXREAL_0,FINSEQ_1,VALUED_0,VALUED_1,ORDINAL1,SEQ_4;
constructors REAL_1,SEQ_2,PARTFUN1,RFUNCT_2,COMPLEX1,RCOMP_1,FDIFF_1,SEQ_1,NAT_1,FCONT_1,PDIFF_1,BHSP_3,VFUNCT_1,SEQM_3,RELSET_1,BINOP_2,RVSUM_1,CKB3;
requirements SUBSET,REAL,BOOLE,NUMERALS,ARITHM;
begin
reserve x for Real;
reserve x0 for Real;
reserve x1 for Real;
reserve y0 for Real;
reserve y1 for Real;
reserve z for (Element of ( REAL 2 ));
reserve f for (PartFunc of ( REAL 2 ),( REAL ));
reserve R for REST;
reserve L for LINEAR;
theorem
((z = <* x0,y0 *> & f is_partial_differentiable_in z,1) implies (ex N being (Neighbourhood of x0) st (N c= ( dom ( SVF1 (1,f,z) ) ) & (ex L st (ex R st (for x holds (x in N implies ( ( ( SVF1 (1,f,z) ) . x ) - ( ( SVF1 (1,f,z) ) . x0 ) ) = ( ( L . ( x - x0 ) ) + ( R . ( x - x0 ) ) ))))))))
proof
assume that
A1: z = <* x0,y0 *>
and
A2: f is_partial_differentiable_in z,1;
(ex x1 st (ex y1 st (z = <* x1,y1 *> & ( SVF1 (1,f,z) ) is_differentiable_in x1))) by A2,CKB6:1;
then ( SVF1 (1,f,z) ) is_differentiable_in x0 by A1,FINSEQ_1:77;
hence thesis by FDIFF_1:def 4;
end;
