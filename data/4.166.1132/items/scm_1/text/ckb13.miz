theorem
Th23: (for k,n being (Element of ( NAT )) holds (for s being (State of ( SCM )) holds (for il being (Element of ( NAT )) holds ((( IC ( Comput (F,s,k) ) ) = n & ( F . n ) = ( SCM-goto il )) implies (( IC ( Comput (F,s,( k + 1 )) ) ) = il & (for d being Data-Location holds ( ( Comput (F,s,( k + 1 )) ) . d ) = ( ( Comput (F,s,k) ) . d )))))))
proof
let k being (Element of ( NAT ));
let n being (Element of ( NAT ));
let s being (State of ( SCM ));
let il being (Element of ( NAT ));
assume A1: (( IC ( Comput (F,s,k) ) ) = n & ( F . n ) = ( SCM-goto il ));
set csk1 = ( Comput (F,s,( k + 1 )) );
set csk = ( Comput (F,s,k) );
A2: ( dom F ) = ( NAT ) by PARTFUN1:def 2;
A3: csk1 = ( Exec (( CurInstr (F,csk) ),csk) ) by CKB7:1
.= ( Exec (( SCM-goto il ),csk) ) by A1,A2,PARTFUN1:def 6;
hence ( IC csk1 ) = il by AMI_3:7;
thus thesis by A3,AMI_3:7;
end;
