begin
reserve F for  total ( NAT ) -defined (the Instructions of ( SCM )) -valued Function;
theorem
Th25: (for k,n being (Element of ( NAT )) holds (for s being (State of ( SCM )) holds (for a being Data-Location holds (for il being (Element of ( NAT )) holds ((( IC ( Comput (F,s,k) ) ) = n & ( F . n ) = ( a >0_goto il )) implies (((( ( Comput (F,s,k) ) . a ) > ( 0 ) implies ( IC ( Comput (F,s,( k + 1 )) ) ) = il) & (( ( Comput (F,s,k) ) . a ) <= ( 0 ) implies ( IC ( Comput (F,s,( k + 1 )) ) ) = ( n + 1 ))) & (for d being Data-Location holds ( ( Comput (F,s,( k + 1 )) ) . d ) = ( ( Comput (F,s,k) ) . d ))))))))
proof
let k being (Element of ( NAT ));
let n being (Element of ( NAT ));
let s being (State of ( SCM ));
let a being Data-Location;
let il being (Element of ( NAT ));
assume that
A1: ( IC ( Comput (F,s,k) ) ) = n
and
A2: ( F . n ) = ( a >0_goto il );
set csk1 = ( Comput (F,s,( k + 1 )) );
set csk = ( Comput (F,s,k) );
A3: ( dom F ) = ( NAT ) by PARTFUN1:def 2;
A4: csk1 = ( Exec (( CurInstr (F,csk) ),csk) ) by CKB7:1
.= ( Exec (( a >0_goto il ),csk) ) by A1,A2,A3,PARTFUN1:def 6;
hence (( csk . a ) > ( 0 ) implies ( IC csk1 ) = il) by AMI_3:9;
hereby
assume ( csk . a ) <= ( 0 );
hence ( IC csk1 ) = ( succ n ) by A1,A4,AMI_3:9
.= ( n + 1 );
end;
thus thesis by A4,AMI_3:9;
