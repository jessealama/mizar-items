begin
definition
let G being _Graph;
let v being (Vertex of G);
func G .reachableDFrom v -> non  empty (Subset of ( the_Vertices_of G )) means 
:Def6: (for x being set holds (x in it iff (ex W being (DWalk of G) st W is_Walk_from v,x)));
existence
proof
set W = ( G .walkOf v );
defpred P[ set ]
 means
(ex W being  directed (Walk of G) st W is_Walk_from v,$1);
consider IT being (Subset of ( the_Vertices_of G )) such that A1: (for x being set holds (x in IT iff (x in ( the_Vertices_of G ) & P[ x ]))) from SUBSET_1:sch 1;
W is_Walk_from v,v by GLIB_001:13;
then reconsider IT as non  empty (Subset of ( the_Vertices_of G )) by A1;
take IT;
let x being set;
thus (x in IT implies (ex W being  directed (Walk of G) st W is_Walk_from v,x)) by A1;
given W being  directed (Walk of G) such that
A2: W is_Walk_from v,x;

x is (Vertex of G) by A2,GLIB_001:18;
hence thesis by A1,A2;
end;
uniqueness
proof
let IT1 being non  empty (Subset of ( the_Vertices_of G ));
let IT2 being non  empty (Subset of ( the_Vertices_of G ));
assume that
A3: (for x being set holds (x in IT1 iff (ex W being  directed (Walk of G) st W is_Walk_from v,x)))
and
A4: (for x being set holds (x in IT2 iff (ex W being  directed (Walk of G) st W is_Walk_from v,x)));
now
let x being set;
hereby
assume x in IT1;
then (ex W being  directed (Walk of G) st W is_Walk_from v,x) by A3;
hence x in IT2 by A4;
end;
assume x in IT2;
then (ex W being  directed (Walk of G) st W is_Walk_from v,x) by A4;
hence x in IT1 by A3;
end;
hence thesis by TARSKI:1;
end;
end;
