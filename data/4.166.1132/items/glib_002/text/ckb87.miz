begin
theorem
(for G1 being _Graph holds (for G2 being (Subgraph of G1) holds (for v1 being (Vertex of G1) holds (for v2 being (Vertex of G2) holds (v1 = v2 implies ( G2 .reachableDFrom v2 ) c= ( G1 .reachableDFrom v1 ))))))
proof
let G1 being _Graph;
let G2 being (Subgraph of G1);
let v1 being (Vertex of G1);
let v2 being (Vertex of G2);
assume A1: v1 = v2;
now
let v being set;
assume v in ( G2 .reachableDFrom v2 );
then consider W being (DWalk of G2) such that A2: W is_Walk_from v2,v by CKB17:def 1;
reconsider W as (DWalk of G1) by GLIB_001:175;
W is_Walk_from v1,v by A1,A2,GLIB_001:19;
hence v in ( G1 .reachableDFrom v1 ) by CKB17:def 1;
end;
hence thesis by TARSKI:def 3;
end;
