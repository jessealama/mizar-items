constructors DOMAIN_1,CARD_FIL,GLIB_001,VALUED_1,XXREAL_2,WELLORD2,RELSET_1,CKB2,CKB3,CKB4,CKB11,CKB16,CKB17,CKB22,CKB27,CKB29,CKB30,CKB31;
requirements ARITHM,BOOLE,NUMERALS,REAL,SUBSET;
begin
theorem
Lm9: (for G1,G2 being _Graph holds (for v1 being (Vertex of G1) holds (for v2 being (Vertex of G2) holds ((G1 == G2 & v1 = v2) implies ( G1 .reachableFrom v1 ) = ( G2 .reachableFrom v2 )))))
proof
let G1 being _Graph;
let G2 being _Graph;
let v1 being (Vertex of G1);
let v2 being (Vertex of G2);
assume that
A1: G1 == G2
and
A2: v1 = v2;
now
let x being set;
hereby
assume x in ( G1 .reachableFrom v1 );
then consider W being (Walk of G1) such that A3: W is_Walk_from v2,x by A2,CKB16:def 1;
reconsider W2 = W as (Walk of G2) by A1,GLIB_001:179;
W2 is_Walk_from v2,x by A3,GLIB_001:19;
hence x in ( G2 .reachableFrom v2 ) by CKB16:def 1;
end;
assume x in ( G2 .reachableFrom v2 );
then consider W being (Walk of G2) such that A4: W is_Walk_from v1,x by A2,CKB16:def 1;
reconsider W2 = W as (Walk of G1) by A1,GLIB_001:179;
W2 is_Walk_from v1,x by A4,GLIB_001:19;
hence x in ( G1 .reachableFrom v1 ) by CKB16:def 1;
end;
hence thesis by TARSKI:1;
end;
