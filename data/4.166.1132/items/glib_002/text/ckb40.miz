begin
theorem
Lm12: (for G1,G2 being _Graph holds (G1 == G2 implies ( G1 .componentSet() ) = ( G2 .componentSet() )))
proof
let G1 being _Graph,G2 being _Graph;
assume A1: G1 == G2;
now
let x being set;
hereby
assume x in ( G1 .componentSet() );
then consider v1 being (Vertex of G1) such that A2: x = ( G1 .reachableFrom v1 ) by CKB27:def 1;
reconsider v2 = v1 as (Vertex of G2) by A1,GLIB_000:def 34;
x = ( G2 .reachableFrom v2 ) by A1,A2,CKB37:1;
hence x in ( G2 .componentSet() ) by CKB27:def 1;
end;
assume x in ( G2 .componentSet() );
then consider v2 being (Vertex of G2) such that A3: x = ( G2 .reachableFrom v2 ) by CKB27:def 1;
reconsider v1 = v2 as (Vertex of G1) by A1,GLIB_000:def 34;
x = ( G1 .reachableFrom v1 ) by A1,A3,CKB37:1;
hence x in ( G1 .componentSet() ) by CKB27:def 1;
end;
hence thesis by TARSKI:1;
end;
