begin
registration
cluster  trivial ->  connected for _Graph;
coherence
proof
let G being _Graph;
assume G is  trivial;
then consider x being (Vertex of G) such that A1: ( the_Vertices_of G ) = { x } by GLIB_000:22;
now
set W = ( G .walkOf x );
let u being (Vertex of G),v being (Vertex of G);
(u = x & v = x) by A1,TARSKI:def 1;
then W is_Walk_from u,v by GLIB_001:13;
hence (ex W being (Walk of G) st W is_Walk_from u,v);
end;
hence thesis by CKB2:def 1;
end;
end;
