begin
theorem
Lm3: (for G being _Graph holds (for v1,v2 being (Vertex of G) holds (v1 in ( G .reachableFrom v2 ) implies ( G .reachableFrom v1 ) = ( G .reachableFrom v2 ))))
proof
let G being _Graph;
let v1 being (Vertex of G);
let v2 being (Vertex of G);
assume v1 in ( G .reachableFrom v2 );
then consider WA being (Walk of G) such that A1: WA is_Walk_from v2,v1 by CKB16:def 1;
A2: ( WA .reverse() ) is_Walk_from v1,v2 by A1,GLIB_001:23;
now
let x being set;
hereby
assume x in ( G .reachableFrom v1 );
then consider WB being (Walk of G) such that A3: WB is_Walk_from v1,x by CKB16:def 1;
( WA .append WB ) is_Walk_from v2,x by A1,A3,GLIB_001:31;
hence x in ( G .reachableFrom v2 ) by CKB16:def 1;
end;
assume x in ( G .reachableFrom v2 );
then consider WB being (Walk of G) such that A4: WB is_Walk_from v2,x by CKB16:def 1;
( ( WA .reverse() ) .append WB ) is_Walk_from v1,x by A2,A4,GLIB_001:31;
hence x in ( G .reachableFrom v1 ) by CKB16:def 1;
end;
hence thesis by TARSKI:1;
end;
