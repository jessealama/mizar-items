begin
theorem
Lm11: (for G being _Graph holds (G is  connected iff ( G .componentSet() ) = { ( the_Vertices_of G ) }))
proof
let G being _Graph;
hereby
assume A1: G is  connected;
now
set v = the (Vertex of G);
let x being set;
hereby
assume x in ( G .componentSet() );
then (ex v being (Vertex of G) st x = ( G .reachableFrom v )) by CKB27:def 1;
then x = ( the_Vertices_of G ) by A1,CKB36:1;
hence x in { ( the_Vertices_of G ) } by TARSKI:def 1;
end;
assume x in { ( the_Vertices_of G ) };
then A2: x = ( the_Vertices_of G ) by TARSKI:def 1;
( G .reachableFrom v ) = ( the_Vertices_of G ) by A1,CKB36:1;
hence x in ( G .componentSet() ) by A2,CKB27:def 1;
end;
hence ( G .componentSet() ) = { ( the_Vertices_of G ) } by TARSKI:1;
end;
assume ( G .componentSet() ) = { ( the_Vertices_of G ) };
then ( the_Vertices_of G ) in ( G .componentSet() ) by TARSKI:def 1;
then (ex v being (Vertex of G) st ( G .reachableFrom v ) = ( the_Vertices_of G )) by CKB27:def 1;
hence thesis by CKB35:1;
end;
