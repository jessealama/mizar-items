begin
registration
cluster  trivial ->  arithmetic for LATTICE;
coherence
proof
let L being LATTICE;
assume Z: L is  trivial;
reconsider L9 = L as 1 -element LATTICE by Z;
XX: (for x,y being (Element of L9) holds (x << y implies (ex k being (Element of L9) st ((k in (the carrier of ( CompactSublatt L9 )) & x <= k) & k <= y))))
proof
let x being (Element of L9),y being (Element of L9);
assume A1: x << y;
take k = x;
x = y by STRUCT_0:def 10;
then k is  compact by A1,WAYBEL_3:def 2;
hence k in (the carrier of ( CompactSublatt L9 )) by CKB1:def 1;
thus thesis by STRUCT_0:def 10;
end;
A2: L9 is  algebraic by CKB13:1,XX;
(for z,v being (Element of L9) holds (((z in (the carrier of ( CompactSublatt L9 )) & v in (the carrier of ( CompactSublatt L9 ))) &  ex_inf_of { z,v },L9) implies ( inf { z,v } ) in (the carrier of ( CompactSublatt L9 )))) by STRUCT_0:def 10;
then ( CompactSublatt L9 ) is  meet-inheriting by YELLOW_0:def 16;
hence thesis by A2,CKB17:def 1;
end;
end;
