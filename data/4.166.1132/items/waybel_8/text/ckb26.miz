theorem
Th13: (for L1,L2 being RelStr holds ((( the RelStr of L1 ) = ( the RelStr of L2 ) & L1 is  transitive) implies L2 is  transitive))
proof
let L1 being RelStr;
let L2 being RelStr;
assume that
A1: ( the RelStr of L1 ) = ( the RelStr of L2 )
and
A2: L1 is  transitive;
now
let x being (Element of L2);
let y being (Element of L2);
let z being (Element of L2);
reconsider x9 = x,y9 = y,z9 = z as (Element of L1) by A1;
assume (x <= y & y <= z);
then (x9 <= y9 & y9 <= z9) by A1,YELLOW_0:1;
then x9 <= z9 by A2,YELLOW_0:def 2;
hence x <= z by A1,YELLOW_0:1;
end;
hence thesis by YELLOW_0:def 2;
end;
