begin
registration
let X being set;
let x being (Element of ( BoolePoset X ));
cluster ( compactbelow x ) ->  lower  directed;
coherence
proof
now
let a being set;
let b being set;
assume that
A1: a c= b
and
A2: b in ( compactbelow x );
b in { y where y is  finite (Subset of x): (not contradiction) } by A2,CKB52:1;
then (ex b1 being  finite (Subset of x) st b = b1);
then a is  finite  finite (Subset of x) by A1,XBOOLE_1:1;
then a in { y where y is  finite (Subset of x): (not contradiction) };
hence a in ( compactbelow x ) by CKB52:1;
end;
hence A3: ( compactbelow x ) is  lower by WAYBEL_7:6;
now
let a being set;
let b being set;
assume that
A4: a in ( compactbelow x )
and
A5: b in ( compactbelow x );
b in { y where y is  finite (Subset of x): (not contradiction) } by A5,CKB52:1;
then A6: (ex b1 being  finite (Subset of x) st b = b1);
a in { y where y is  finite (Subset of x): (not contradiction) } by A4,CKB52:1;
then (ex a1 being  finite (Subset of x) st a = a1);
then ( a \/ b ) is  finite  finite (Subset of x) by A6,XBOOLE_1:8;
then ( a \/ b ) in { y where y is  finite (Subset of x): (not contradiction) };
hence ( a \/ b ) in ( compactbelow x ) by CKB52:1;
end;
hence thesis by A3,WAYBEL_7:8;
end;
end;
