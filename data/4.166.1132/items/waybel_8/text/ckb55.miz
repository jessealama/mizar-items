begin
theorem
Th33: (for X being set holds ( BoolePoset X ) is  algebraic)
proof
let X being set;
now
let x being (Element of ( BoolePoset X ));
A1:now
let a being (Element of ( BoolePoset X ));
assume A2: a is_>=_than ( compactbelow x );
now
let t being set;
assume A3: t in x;
A4: x c= X by CKB49:1;
now
let k being set;
assume k in { t };
then k = t by TARSKI:def 1;
hence k in X by A3,A4;
end;
then { t } c= X by TARSKI:def 3;
then reconsider t1 = { t } as (Element of ( BoolePoset X )) by CKB49:1;
(for k being set holds (k in { t } implies k in x)) by A3,TARSKI:def 1;
then { t } is  finite  finite (Subset of x) by TARSKI:def 3;
then { t } in { y where y is  finite (Subset of x): (not contradiction) };
then { t } in ( compactbelow x ) by CKB52:1;
then t1 <= a by A2,LATTICE3:def 9;
then (t in { t } & { t } c= a) by TARSKI:def 1,YELLOW_1:2;
hence t in a;
end;
then x c= a by TARSKI:def 3;
hence x <= a by YELLOW_1:2;
end;
(for b being (Element of ( BoolePoset X )) holds (b in ( compactbelow x ) implies b <= x)) by CKB7:1;
then x is_>=_than ( compactbelow x ) by LATTICE3:def 9;
hence x = ( sup ( compactbelow x ) ) by A1,YELLOW_0:32;
end;
then ((for x being (Element of ( BoolePoset X )) holds ( compactbelow x ) is non empty  directed) & ( BoolePoset X ) is  satisfying_axiom_K) by CKB11:def 1;
hence thesis by CKB12:def 1;
end;
