requirements SUBSET,BOOLE,NUMERALS;
begin
theorem
(for L being  complete LATTICE holds (for x,y,k being (Element of L) holds (((x <= k & k <= y) & k in (the carrier of ( CompactSublatt L ))) implies x << y)))
proof
let L being  complete LATTICE;
let x being (Element of L);
let y being (Element of L);
let k being (Element of L);
assume that
A1: (x <= k & k <= y)
and
A2: k in (the carrier of ( CompactSublatt L ));
k is  compact by A2,CKB1:def 1;
then k << k by WAYBEL_3:def 2;
hence thesis by A1,WAYBEL_3:2;
end;
