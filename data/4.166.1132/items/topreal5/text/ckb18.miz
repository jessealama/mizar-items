theorem
Th23: (for C being  compact non empty (Subset of ( TOP-REAL 2 )) holds (C is  being_simple_closed_curve implies ( E-bound C ) > ( W-bound C )))
proof
let C being  compact non empty (Subset of ( TOP-REAL 2 ));
assume A1: C is  being_simple_closed_curve;
now
assume A2: ( E-bound C ) <= ( W-bound C );
(for p holds (p in C implies ( p `1 ) = ( W-bound C )))
proof
let p;
assume p in C;
then A3: (( W-bound C ) <= ( p `1 ) & ( p `1 ) <= ( E-bound C )) by PSCOMP_1:24;
then ( W-bound C ) <= ( E-bound C ) by XXREAL_0:2;
then ( W-bound C ) = ( E-bound C ) by A2,XXREAL_0:1;
hence thesis by A3,XXREAL_0:1;
end;
hence contradiction by A1,CKB16:1;
end;
hence thesis;
end;
