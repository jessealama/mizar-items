begin
reserve x for set;
reserve y for set;
reserve z for set;
scheme FuncExCD3 { C,D,E() -> non empty set,P,Q,R[set,set],F,G,H(set,set) -> (Element of E()) } : (ex f being (Function of [: C(),D() :],E()) st ((for c being (Element of C()) holds (for d being (Element of D()) holds ([ c,d ] in ( dom f ) iff ((P[ c,d ] or Q[ c,d ]) or R[ c,d ])))) & (for c being (Element of C()) holds (for d being (Element of D()) holds ([ c,d ] in ( dom f ) implies (((P[ c,d ] implies ( f . [ c,d ] ) = F(c,d)) & (Q[ c,d ] implies ( f . [ c,d ] ) = G(c,d))) & (R[ c,d ] implies ( f . [ c,d ] ) = H(c,d))))))))
provided
A1: (for c being (Element of C()) holds (for d being (Element of D()) holds (((P[ c,d ] implies (not Q[ c,d ])) & (P[ c,d ] implies (not R[ c,d ]))) & (Q[ c,d ] implies (not R[ c,d ])))))
and
A2: (for c being (Element of C()) holds (for d being (Element of D()) holds ((P[ c,d ] or Q[ c,d ]) or R[ c,d ])))
proof
A3: (for c being (Element of C()) holds (for d being (Element of D()) holds (((P[ c,d ] implies (not Q[ c,d ])) & (P[ c,d ] implies (not R[ c,d ]))) & (Q[ c,d ] implies (not R[ c,d ]))))) by A1;
consider f being (PartFunc of [: C(),D() :],E()) such that A4: ((for c being (Element of C()) holds (for b being (Element of D()) holds ([ c,b ] in ( dom f ) iff ((P[ c,b ] or Q[ c,b ]) or R[ c,b ])))) & (for a being (Element of C()) holds (for b being (Element of D()) holds ([ a,b ] in ( dom f ) implies (((P[ a,b ] implies ( f . [ a,b ] ) = F(a,b)) & (Q[ a,b ] implies ( f . [ a,b ] ) = G(a,b))) & (R[ a,b ] implies ( f . [ a,b ] ) = H(a,b))))))) from CKB20:sch 1(A3);
consider v being Function such that A5: v = f;
( dom f ) = [: C(),D() :]
proof
thus ( dom f ) c= [: C(),D() :];
let x;
assume x in [: C(),D() :];
then consider y,z such that A6: y in C() and A7: z in D() and A8: x = [ y,z ] by ZFMISC_1:def 2;
reconsider z as (Element of D()) by A7;
reconsider y as (Element of C()) by A6;
((P[ y,z ] or Q[ y,z ]) or R[ y,z ]) by A2;
hence thesis by A4,A8;
end;
then reconsider v as (Function of [: C(),D() :],E()) by A5,FUNCT_2:def 1;
take v;
thus thesis by A4,A5;
end;
