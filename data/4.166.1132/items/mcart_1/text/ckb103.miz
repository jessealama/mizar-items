begin
reserve X1 for set;
reserve X2 for set;
reserve X3 for set;
reserve X4 for set;
reserve x for (Element of [: X1,X2,X3,X4 :]);
definition
let f being Function;
func pr2 f -> Function means 
(( dom it ) = ( dom f ) & (for x being set holds (x in ( dom f ) implies ( it . x ) = ( ( f . x ) `2 ))));
existence
proof
deffunc F(set) = ( ( f . $1 ) `2 );
(ex g being Function st (( dom g ) = ( dom f ) & (for x being set holds (x in ( dom f ) implies ( g . x ) = F(x))))) from FUNCT_1:sch 3;
hence thesis;
end;
uniqueness
proof
let p1 being Function;
let p2 being Function;
assume that
A6: ( dom p1 ) = ( dom f )
and
A7: (for x being set holds (x in ( dom f ) implies ( p1 . x ) = ( ( f . x ) `2 )))
and
A8: ( dom p2 ) = ( dom f )
and
A9: (for x being set holds (x in ( dom f ) implies ( p2 . x ) = ( ( f . x ) `2 )));
now
let x being set;
assume A10: x in ( dom f );
then ( p1 . x ) = ( ( f . x ) `2 ) by A7;
hence ( p1 . x ) = ( p2 . x ) by A9,A10;
end;
hence thesis by A6,A8,FUNCT_1:2;
end;
end;
