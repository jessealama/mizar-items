theorem
((for z holds (z in Z iff (ex x1 st (ex x2 st (ex x3 st (ex x4 st ((((x1 in X1 & x2 in X2) & x3 in X3) & x4 in X4) & z = [ x1,x2,x3,x4 ]))))))) implies Z = [: X1,X2,X3,X4 :])
proof
assume A1: (for z holds (z in Z iff (ex x1 st (ex x2 st (ex x3 st (ex x4 st ((((x1 in X1 & x2 in X2) & x3 in X3) & x4 in X4) & z = [ x1,x2,x3,x4 ])))))));
now
let z;
thus (z in Z implies z in [: [: X1,X2,X3 :],X4 :])
proof
assume z in Z;
then consider x1,x2,x3,x4 such that A2: ((x1 in X1 & x2 in X2) & x3 in X3) and A3: (x4 in X4 & z = [ x1,x2,x3,x4 ]) by A1;
[ x1,x2,x3 ] in [: X1,X2,X3 :] by A2,CKB83:1;
hence thesis by A3,ZFMISC_1:def 2;
end;

assume z in [: [: X1,X2,X3 :],X4 :];
then consider x123,x4 being set such that A4: x123 in [: X1,X2,X3 :] and A5: x4 in X4 and A6: z = [ x123,x4 ] by ZFMISC_1:def 2;
consider x1,x2,x3 such that A7: ((x1 in X1 & x2 in X2) & x3 in X3) and A8: x123 = [ x1,x2,x3 ] by A4,CKB82:1;
z = [ x1,x2,x3,x4 ] by A6,A8;
hence z in Z by A1,A5,A7;
end;
then Z = [: [: X1,X2,X3 :],X4 :] by TARSKI:1;
hence thesis by ZFMISC_1:def 4;
end;
