constructors TARSKI,ENUMSET1,SUBSET_1,FUNCT_1,CKB7,CKB8;
requirements SUBSET,BOOLE;
begin
reserve x for set;
reserve y1 for set;
reserve y2 for set;
reserve z for set;
theorem
(z in [: { x },{ y1,y2 } :] implies (( z `1 ) = x & (( z `2 ) = y1 or ( z `2 ) = y2)))
proof
assume z in [: { x },{ y1,y2 } :];
then (( z `1 ) in { x } & ( z `2 ) in { y1,y2 }) by CKB12:1;
hence thesis by TARSKI:def 1,TARSKI:def 2;
end;
