environ
vocabularies XBOOLE_0,SUBSET_1,TARSKI,ZFMISC_1,RELAT_1,FUNCT_1,MCART_1;
notations TARSKI,XBOOLE_0,ENUMSET1,ZFMISC_1,SUBSET_1,RELAT_1,FUNCT_1;
definitions TARSKI,XBOOLE_0,RELAT_1;
theorems TARSKI,XBOOLE_0,ZFMISC_1,ENUMSET1,ORDINAL1,XBOOLE_1,FUNCT_1,RELAT_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6;
schemes XBOOLE_0,FUNCT_1;
registrations XBOOLE_0,RELAT_1;
constructors TARSKI,ENUMSET1,SUBSET_1,FUNCT_1;
requirements SUBSET,BOOLE;
begin
reserve x for set;
reserve x1 for set;
reserve x2 for set;
reserve y1 for set;
reserve y2 for set;
reserve z1 for set;
reserve z2 for set;
definition
let x;
given x1 being set,x2 being set such that
A1: x = [ x1,x2 ];

func x `1
means
:Def1: (x = [ y1,y2 ] implies it = y1);
existence
proof
take x1;
thus thesis by A1,ZFMISC_1:27;
end;
uniqueness
proof
let z1;
let z2;
assume that
A2: (for y1 holds (for y2 holds (x = [ y1,y2 ] implies z1 = y1)))
and
A3: (for y1 holds (for y2 holds (x = [ y1,y2 ] implies z2 = y1)));
z1 = x1 by A1,A2;
hence thesis by A1,A3;
end;
end;
