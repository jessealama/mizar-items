begin
reserve x for set;
reserve x1 for set;
reserve x2 for set;
reserve y1 for set;
reserve y2 for set;
reserve z1 for set;
reserve z2 for set;
definition
let x;
given x1 being set,x2 being set such that
A1: x = [ x1,x2 ];

func x `2 means 
:Def2: (x = [ y1,y2 ] implies it = y2);
existence
proof
take x2;
thus thesis by A1,ZFMISC_1:27;
end;
uniqueness
proof
let z1;
let z2;
assume that
A4: (for y1 holds (for y2 holds (x = [ y1,y2 ] implies z1 = y2)))
and
A5: (for y1 holds (for y2 holds (x = [ y1,y2 ] implies z2 = y2)));
z1 = x2 by A1,A4;
hence thesis by A1,A5;
end;
end;
