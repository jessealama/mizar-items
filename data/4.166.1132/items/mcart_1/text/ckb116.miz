reserve x for set;
scheme BiFuncEx { A() -> set,B() -> set,C() -> set,P[set,set,set] } : (ex f,g being Function st ((( dom f ) = A() & ( dom g ) = A()) & (for x holds (x in A() implies P[ x,( f . x ),( g . x ) ]))))
provided
A1: (x in A() implies (ex y st (ex z st ((y in B() & z in C()) & P[ x,y,z ]))))
proof
defpred H[ set,set ]
 means
(for y holds (for z holds ((( $2 `1 ) = y & ( $2 `2 ) = z) implies P[ $1,y,z ])));
A2: (x in A() implies (ex p being set st (p in [: B(),C() :] & H[ x,p ])))
proof
assume x in A();
then consider y,z such that A3: (y in B() & z in C()) and A4: P[ x,y,z ] by A1;
take p = [ y,z ];
thus p in [: B(),C() :] by A3,ZFMISC_1:87;
thus (for y holds (for z holds ((( p `1 ) = y & ( p `2 ) = z) implies P[ x,y,z ])))
proof
let x1 being set,x2 being set;
assume that
A5: ( p `1 ) = x1
and
A6: ( p `2 ) = x2;
x1 = y by A5,CKB9:1;
hence thesis by A4,A6,CKB9:1;
end;

end;
consider h being Function such that (( dom h ) = A() & ( rng h ) c= [: B(),C() :]) and A7: (for x holds (x in A() implies H[ x,( h . x ) ])) from FUNCT_1:sch 5(A2);
deffunc g(set) = ( ( h . $1 ) `2 );
deffunc f(set) = ( ( h . $1 ) `1 );
consider f being Function such that A8: ( dom f ) = A() and A9: (for x holds (x in A() implies ( f . x ) = f(x))) from FUNCT_1:sch 3;
consider g being Function such that A10: ( dom g ) = A() and A11: (for x holds (x in A() implies ( g . x ) = g(x))) from FUNCT_1:sch 3;
take f;
take g;
thus (( dom f ) = A() & ( dom g ) = A()) by A8,A10;
thus (for x holds (x in A() implies P[ x,( f . x ),( g . x ) ]))
proof
let x;
assume A12: x in A();
then (( f . x ) = ( ( h . x ) `1 ) & ( g . x ) = ( ( h . x ) `2 )) by A9,A11;
hence thesis by A7,A12;
end;

end;
