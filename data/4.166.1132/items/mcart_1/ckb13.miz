begin
reserve x for set;
reserve y for set;
reserve z for set;
reserve X for set;
reserve Y for set;
theorem
((((ex x st (ex y st z = [ x,y ])) & ( z `1 ) in X) & ( z `2 ) in Y) implies z in [: X,Y :])
proof
assume (ex x st (ex y st z = [ x,y ]));
then [ ( z `1 ),( z `2 ) ] = z by CKB10:1;
hence thesis by ZFMISC_1:def 2;
end;
