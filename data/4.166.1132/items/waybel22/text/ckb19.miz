begin
reserve X for set;
reserve x for (Element of ( BoolePoset X ));
reserve z for (Element of X);
theorem
(for L being  continuous  complete non empty Poset holds (for f being (Function of ( FixedUltraFilters X ),(the carrier of L)) holds ( f -extension_to_hom ) is  monotone))
proof
let L being  continuous  complete non empty Poset;
let f being (Function of ( FixedUltraFilters X ),(the carrier of L));
let F1 being (Element of ( InclPoset ( Filt ( BoolePoset X ) ) )),F2 being (Element of ( InclPoset ( Filt ( BoolePoset X ) ) ));
set F = ( f -extension_to_hom );
set F1s = { ( "/\" ({ ( f . ( uparrow x ) ): (ex z st (x = { z } & z in Y)) },L) ) where Y is (Subset of X): Y in F1 };
set F2s = { ( "/\" ({ ( f . ( uparrow x ) ): (ex z st (x = { z } & z in Y)) },L) ) where Y is (Subset of X): Y in F2 };
A1: ( ex_sup_of F1s,L &  ex_sup_of F2s,L) by YELLOW_0:17;
A2: ( F . F1 ) = ( "\/" ({ ( "/\" ({ ( f . ( uparrow x ) ): (ex z st (x = { z } & z in Y)) },L) ) where Y is (Subset of X): Y in F1 },L) ) by CKB18:def 1;
assume F1 <= F2;
then A3: F1 c= F2 by YELLOW_1:3;
A4: F1s c= F2s
proof
let s being set;
assume s in F1s;
then (ex Y being (Subset of X) st (s = ( "/\" ({ ( f . ( uparrow x ) ): (ex z st (x = { z } & z in Y)) },L) ) & Y in F1));
hence thesis by A3;
end;
A5: ( F . F2 ) = ( "\/" ({ ( "/\" ({ ( f . ( uparrow x ) ): (ex z st (x = { z } & z in Y)) },L) ) where Y is (Subset of X): Y in F2 },L) ) by CKB18:def 1;
let FF1 being (Element of L),FF2 being (Element of L);
assume (FF1 = ( F . F1 ) & FF2 = ( F . F2 ));
hence FF1 <= FF2 by A2,A5,A1,A4,YELLOW_0:34;
end;
