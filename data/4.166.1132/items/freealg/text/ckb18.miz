begin
reserve x for set;
reserve y for set;
definition
let f being non  empty (FinSequence of ( NAT ));
let X being set;
func REL (f,X) -> (Relation of ( ( dom f ) \/ X ),( ( ( dom f ) \/ X ) * )) means 
:Def8: (for a being (Element of ( ( dom f ) \/ X )) holds (for b being (Element of ( ( ( dom f ) \/ X ) * )) holds ([ a,b ] in it iff (a in ( dom f ) & ( f . a ) = ( len b )))));
existence
proof
set A = ( ( dom f ) \/ X );
defpred P[ (Element of A),(Element of ( A * )) ]
 means
($1 in ( dom f ) & ( f . $1 ) = ( len $2 ));
consider R being (Relation of A,( A * )) such that A1: (for x being (Element of A) holds (for y being (Element of ( A * )) holds ([ x,y ] in R iff P[ x,y ]))) from RELSET_1:sch 2;
take R;
let a being (Element of A);
let b being (Element of ( A * ));
thus thesis by A1;
end;
uniqueness
proof
set A = ( ( dom f ) \/ X );
let R being (Relation of A,( A * ));
let T being (Relation of A,( A * ));
assume that
A2: (for a being (Element of A) holds (for b being (Element of ( A * )) holds ([ a,b ] in R iff (a in ( dom f ) & ( f . a ) = ( len b )))))
and
A3: (for a being (Element of A) holds (for b being (Element of ( A * )) holds ([ a,b ] in T iff (a in ( dom f ) & ( f . a ) = ( len b )))));
(for x holds (for y holds ([ x,y ] in R iff [ x,y ] in T)))
proof
let x;
let y;
thus ([ x,y ] in R implies [ x,y ] in T)
proof
assume A4: [ x,y ] in R;
then reconsider x1 = x as (Element of A) by ZFMISC_1:87;
reconsider y1 = y as (Element of ( A * )) by A4,ZFMISC_1:87;
[ x1,y1 ] in R by A4;
then A5: x1 in ( dom f ) by A2;
( f . x1 ) = ( len y1 ) by A2,A4;
hence thesis by A3,A5;
end;

assume A6: [ x,y ] in T;
then reconsider x1 = x as (Element of A) by ZFMISC_1:87;
reconsider y1 = y as (Element of ( A * )) by A6,ZFMISC_1:87;
[ x1,y1 ] in T by A6;
then A7: x1 in ( dom f ) by A3;
( f . x1 ) = ( len y1 ) by A3,A6;
hence thesis by A2,A7;
end;
hence thesis by RELAT_1:def 2;
end;
end;
