begin
theorem
(for T being non  empty TopSpace holds (for x,y being (Point of T) holds (for B1 being (Basis of x) holds (for B2 being (Basis of y) holds (for U being set holds ((x in U & U in B2) implies (ex V being  open (Subset of T) st (V in B1 & V c= U))))))))
proof
let T being non  empty TopSpace;
let x being (Point of T);
let y being (Point of T);
let B1 being (Basis of x);
let B2 being (Basis of y);
let U being set;
assume that
A1: x in U
and
A2: U in B2;
U is  open  open (Subset of T) by A2,YELLOW_8:12;
then consider V being (Subset of T) such that A3: V in B1 and A4: V c= U by A1,YELLOW_8:13;
V is  open by A3,YELLOW_8:12;
hence thesis by A3,A4;
end;
