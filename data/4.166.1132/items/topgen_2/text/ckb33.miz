environ
vocabularies XBOOLE_0,PRE_TOPC,RLVECT_3,SUBSET_1,TARSKI,SETFAM_1,RCOMP_1,YELLOW_8,PBOOLE,FUNCT_1,CARD_3,ZFMISC_1,STRUCT_0,RELAT_1,CARD_1,ORDINAL1,FRECHET,WAYBEL23,FINSET_1,EQREL_1,NATTRA_1,CARD_2,CANTOR_1,TOPS_1,FINSUB_1,FINSEQ_1,PCOMPS_1,YELLOW_9,TOPGEN_2;
notations TARSKI,XBOOLE_0,SUBSET_1,DOMAIN_1,SETFAM_1,RELAT_1,FUNCT_1,RELSET_1,FUNCT_2,CARD_1,FINSEQ_1,CARD_3,EQREL_1,ORDINAL1,FINSET_1,FINSUB_1,CARD_2,STRUCT_0,PRE_TOPC,TOPS_1,TOPS_2,TDLAT_3,PCOMPS_1,CANTOR_1,FRECHET,YELLOW_8,YELLOW_9,WAYBEL23,CKB3,CKB5,CKB9,CKB10,CKB11,CKB18,CKB19,CKB32;
definitions TARSKI,XBOOLE_0,FUNCT_1,CARD_3,STRUCT_0,PRE_TOPC,FRECHET,YELLOW_8,YELLOW_9,SUBSET_1,CKB3,CKB5,CKB9,CKB18,CKB32;
theorems XBOOLE_1,ZFMISC_1,SETFAM_1,CANTOR_1,CARD_5,YELLOW_8,YELLOW_9,TOPS_1,CARD_1,CARD_4,CARD_LAR,XBOOLE_0,TSEP_1,PRE_TOPC,TARSKI,TOPS_2,FUNCT_1,CARD_3,FINSET_1,TDLAT_3,CARD_2,EQREL_1,FUNCT_2,WELLORD2,WAYBEL23,SUBSET_1,BORSUK_5,FINSEQ_1,FINSUB_1,WAYBEL19,PCOMPS_1,MSSUBFAM,ENUMSET1,PARTFUN1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB12,CKB13,CKB14,CKB15,CKB17,CKB18,CKB22,CKB23,CKB24,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32;
schemes ORDINAL1,PBOOLE,FUNCT_1,FUNCT_2;
registrations SUBSET_1,RELAT_1,FINSET_1,CARD_1,EQREL_1,CARD_5,CARD_FIL,STRUCT_0,PRE_TOPC,TOPS_1,TDLAT_3,YELLOW12,ORDINAL1,COMPTS_1,RELSET_1,CKB16,CKB20,CKB21,CKB25;
constructors SETFAM_1,DOMAIN_1,FINSUB_1,CARD_2,CARD_3,TOPS_1,TOPS_2,PCOMPS_1,TDLAT_3,CANTOR_1,YELLOW_8,YELLOW_9,FRECHET,WAYBEL23,RELSET_1,FINSEQ_1,FINSEQ_2,CKB3,CKB5,CKB9,CKB10,CKB11,CKB18,CKB32;
requirements BOOLE,SUBSET,NUMERALS;
begin
reserve a for set;
reserve b for set;
registration
let X being set;
let x0 being set;
cluster ( DiscrWithInfin (X,x0) ) ->  TopSpace-like;
coherence
proof
set O1 = { U where U is (Subset of X): (not x0 in U) };
set O2 = { ( F ` ) where F is (Subset of X): F is  finite };
set O = ( O1 \/ O2 );
set T = ( DiscrWithInfin (X,x0) );
A1: (the carrier of T) = X by CKB32:def 1;
A2: (the topology of T) = O by CKB32:def 1;
( ( {} X ) ` ) = X;
then X in O2;
hence (the carrier of T) in (the topology of T) by A1,A2,XBOOLE_0:def 3;
hereby
let a being (Subset-Family of T);
assume that
A3: a c= (the topology of T);
per cases ;
suppose (not a c= O1);

then consider b such that A4: b in a and A5: (not b in O1) by TARSKI:def 3;
b in O2 by A2,A3,A4,A5,XBOOLE_0:def 3;
then A6: (ex F being (Subset of X) st (b = ( F ` ) & F is  finite));
A7: ( ( ( union a ) ` ) ` ) = ( union a );
b c= ( union a ) by A4,ZFMISC_1:74;
then ( ( union a ) ` ) is  finite by A1,A6,FINSET_1:1,SUBSET_1:17;
then ( union a ) in O2 by A1,A7;
hence ( union a ) in (the topology of T) by A2,XBOOLE_0:def 3;
end;
suppose A8: a c= O1;

now
assume x0 in ( union a );
then consider b such that A9: x0 in b and A10: b in a by TARSKI:def 4;
b in O1 by A8,A10;
then (ex U being (Subset of X) st (b = U & (not x0 in U)));
hence contradiction by A9;
end;
then ( union a ) in O1 by A1;
hence ( union a ) in (the topology of T) by A2,XBOOLE_0:def 3;
end;
end;
let a being (Subset of T);
let b being (Subset of T);
assume that
A11: a in (the topology of T)
and
A12: b in (the topology of T);
per cases  by A2,A11,A12,XBOOLE_0:def 3;
suppose A13: (a in O2 & b in O2);

then consider F2 being (Subset of X) such that A14: b = ( F2 ` ) and A15: F2 is  finite;
consider F1 being (Subset of X) such that A16: a = ( F1 ` ) and A17: F1 is  finite by A13;
( a /\ b ) = ( ( F1 \/ F2 ) ` ) by A16,A14,XBOOLE_1:53;
then ( a /\ b ) in O2 by A17,A15;
hence thesis by A2,XBOOLE_0:def 3;
end;
suppose (a in O1 or b in O1);

then ((ex U being (Subset of X) st (a = U & (not x0 in U))) or (ex U being (Subset of X) st (b = U & (not x0 in U))));
then (not x0 in ( a /\ b )) by XBOOLE_0:def 4;
then ( a /\ b ) in O1 by A1;
hence thesis by A2,XBOOLE_0:def 3;
end;
end;
end;
