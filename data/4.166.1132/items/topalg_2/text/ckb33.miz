begin
reserve a for  real number;
theorem
Th14: (for T being non  empty  interval (SubSpace of ( R^1 )) holds (for a being (Point of T) holds (for C being (Loop of a) holds (the carrier of ( pi_1 (T,a) )) = { ( Class (( EqRel (T,a) ),C) ) })))
proof
let T being non  empty  interval (SubSpace of ( R^1 ));
let a being (Point of T);
let C being (Loop of a);
set E = ( EqRel (T,a) );
hereby
let x being set;
assume x in (the carrier of ( pi_1 (T,a) ));
then consider P being (Loop of a) such that A1: x = ( Class (E,P) ) by TOPALG_1:47;
P,C are_homotopic  by CKB31:1;
then x = ( Class (E,C) ) by A1,TOPALG_1:46;
hence x in { ( Class (E,C) ) } by TARSKI:def 1;
end;
let x being set;
assume x in { ( Class (E,C) ) };
then A2: x = ( Class (E,C) ) by TARSKI:def 1;
C in ( Loops a ) by TOPALG_1:def 1;
then x in ( Class E ) by A2,EQREL_1:def 3;
hence thesis by TOPALG_1:def 5;
end;
