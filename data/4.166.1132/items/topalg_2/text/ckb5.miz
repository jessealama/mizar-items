begin
theorem
(for X being non  empty TopSpace holds (for Y being non  empty (SubSpace of X) holds (for x1,x2 being (Point of X) holds (for y1,y2 being (Point of Y) holds (for f being (Path of y1,y2) holds (((x1 = y1 & x2 = y2) & y1,y2 are_connected ) implies f is (Path of x1,x2)))))))
proof
let X being non  empty TopSpace;
let Y being non  empty (SubSpace of X);
let x1 being (Point of X);
let x2 being (Point of X);
let y1 being (Point of Y);
let y2 being (Point of Y);
let f being (Path of y1,y2);
assume that
A1: (x1 = y1 & x2 = y2)
and
A2: y1,y2 are_connected ;
(the carrier of Y) is (Subset of X) by TSEP_1:1;
then reconsider g = f as (Function of ( I[01] ),X) by FUNCT_2:7;
f is  continuous by A2,BORSUK_2:def 2;
then A3: g is  continuous by PRE_TOPC:26;
A4: (( g . ( 0 ) ) = x1 & ( g . 1 ) = x2) by A1,A2,BORSUK_2:def 2;
then x1,x2 are_connected  by A3,BORSUK_2:def 1;
hence thesis by A4,A3,BORSUK_2:def 2;
end;
