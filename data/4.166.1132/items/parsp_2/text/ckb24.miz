environ
vocabularies RLVECT_1,ALGSTR_0,XBOOLE_0,SUBSET_1,ARYTM_1,SUPINF_2,ARYTM_3,VECTSP_1,PARSP_1,ZFMISC_1,STRUCT_0,MCART_1,RELAT_1,GROUP_1,MESFUNC1,PARSP_2;
notations ZFMISC_1,SUBSET_1,MCART_1,DOMAIN_1,STRUCT_0,ALGSTR_0,RLVECT_1,GROUP_1,VECTSP_1,PARSP_1,CKB19,CKB21,CKB23;
definitions CKB19,CKB23;
theorems MCART_1,VECTSP_1,PARSP_1,RLVECT_1,GROUP_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB22,CKB23;
registrations XBOOLE_0,STRUCT_0,VECTSP_1,PARSP_1,CKB20;
constructors DOMAIN_1,PARSP_1,CKB19,CKB23;
begin
reserve FdSp for FanodesSp;
reserve a for (Element of FdSp);
reserve b for (Element of FdSp);
reserve c for (Element of FdSp);
theorem
Th15: (a,b,c is_collinear  implies ((((a,c,b is_collinear  & c,b,a is_collinear ) & b,a,c is_collinear ) & b,c,a is_collinear ) & c,a,b is_collinear ))
proof
assume a,b,c is_collinear ;
then A1: a,b '||' a,c by CKB23:def 1;
then A2: (b,a '||' b,c & b,c '||' b,a) by PARSP_1:24;
A3: c,a '||' c,b by A1,PARSP_1:24;
(a,c '||' a,b & c,b '||' c,a) by A1,PARSP_1:24;
hence thesis by A2,A3,CKB23:def 1;
end;
