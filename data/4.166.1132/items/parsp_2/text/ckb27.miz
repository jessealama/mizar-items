environ
vocabularies RLVECT_1,ALGSTR_0,XBOOLE_0,SUBSET_1,ARYTM_1,SUPINF_2,ARYTM_3,VECTSP_1,PARSP_1,ZFMISC_1,STRUCT_0,MCART_1,RELAT_1,GROUP_1,MESFUNC1,PARSP_2;
notations ZFMISC_1,SUBSET_1,MCART_1,DOMAIN_1,STRUCT_0,ALGSTR_0,RLVECT_1,GROUP_1,VECTSP_1,PARSP_1,CKB19,CKB21,CKB23;
definitions CKB19,CKB23;
theorems MCART_1,VECTSP_1,PARSP_1,RLVECT_1,GROUP_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB22,CKB23,CKB24,CKB25,CKB26;
registrations XBOOLE_0,STRUCT_0,VECTSP_1,PARSP_1,CKB20;
constructors DOMAIN_1,PARSP_1,CKB19,CKB23;
begin
reserve FdSp for FanodesSp;
reserve a for (Element of FdSp);
reserve b for (Element of FdSp);
reserve p for (Element of FdSp);
reserve q for (Element of FdSp);
reserve r for (Element of FdSp);
theorem
Th19: ((((a <> b & a,b,p is_collinear ) & a,b,q is_collinear ) & a,b,r is_collinear ) implies p,q,r is_collinear )
proof
assume that
A1: a <> b
and
A2: a,b,p is_collinear 
and
A3: a,b,q is_collinear 
and
A4: a,b,r is_collinear ;
A5: a,b '||' a,p by A2,CKB23:def 1;
a,b '||' a,r by A4,CKB23:def 1;
then A6: a,b '||' p,r by A5,PARSP_1:35;
a,b '||' a,q by A3,CKB23:def 1;
then a,b '||' p,q by A5,PARSP_1:35;
then p,q '||' p,r by A1,A6,PARSP_1:def 12;
hence thesis by CKB23:def 1;
end;
