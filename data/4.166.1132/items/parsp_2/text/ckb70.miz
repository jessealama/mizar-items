begin
reserve FdSp for FanodesSp;
reserve a for (Element of FdSp);
reserve b for (Element of FdSp);
reserve c for (Element of FdSp);
reserve d for (Element of FdSp);
reserve p for (Element of FdSp);
reserve q for (Element of FdSp);
theorem
(ex d st a,b congr c,d)
proof
A1:now
assume a = b;
then a,b congr c,c by CKB60:def 1;
hence thesis;
end;
A2:now
assume that
A3: a <> b
and
A4: a,b,c is_collinear ;
consider p,q such that A5:  parallelogram a,b,p,q by A3,CKB58:1;
(not p,q,c is_collinear ) by A4,A5,CKB44:1;
then consider d such that A6:  parallelogram p,q,c,d by CKB49:1;
 parallelogram p,q,a,b by A5,CKB48:1;
then a,b congr c,d by A6,CKB60:def 1;
hence thesis;
end;
now
assume that
a <> b
and
A7: (not a,b,c is_collinear );
(ex d st  parallelogram a,b,c,d) by A7,CKB49:1;
hence thesis by CKB67:1;
end;
hence thesis by A1,A2;
end;
