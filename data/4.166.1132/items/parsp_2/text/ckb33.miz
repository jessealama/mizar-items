environ
vocabularies RLVECT_1,ALGSTR_0,XBOOLE_0,SUBSET_1,ARYTM_1,SUPINF_2,ARYTM_3,VECTSP_1,PARSP_1,ZFMISC_1,STRUCT_0,MCART_1,RELAT_1,GROUP_1,MESFUNC1,PARSP_2;
notations ZFMISC_1,SUBSET_1,MCART_1,DOMAIN_1,STRUCT_0,ALGSTR_0,RLVECT_1,GROUP_1,VECTSP_1,PARSP_1,CKB19,CKB21,CKB23;
definitions CKB19,CKB23;
theorems MCART_1,VECTSP_1,PARSP_1,RLVECT_1,GROUP_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32;
registrations XBOOLE_0,STRUCT_0,VECTSP_1,PARSP_1,CKB20;
constructors DOMAIN_1,PARSP_1,CKB19,CKB23;
begin
reserve FdSp for FanodesSp;
reserve a for (Element of FdSp);
reserve b for (Element of FdSp);
reserve p for (Element of FdSp);
reserve q for (Element of FdSp);
reserve o for (Element of FdSp);
theorem
(((((o <> a & o <> b) & o,a,b is_collinear ) & o,a,p is_collinear ) & o,b,q is_collinear ) implies a,b '||' p,q)
proof
assume that
A1: o <> a
and
A2: o <> b
and
A3: o,a,b is_collinear 
and
A4: o,a,p is_collinear 
and
A5: o,b,q is_collinear ;
A6:now
A7: o,a '||' o,b by A3,CKB23:def 1;
o,a '||' o,p by A4,CKB23:def 1;
then A8: o,b '||' o,p by A1,A7,PARSP_1:def 12;
o,b '||' o,q by A5,CKB23:def 1;
then o,p '||' o,q by A2,A8,PARSP_1:def 12;
then A9: o,p '||' p,q by PARSP_1:24;
o,b '||' a,b by A7,PARSP_1:24;
then A10: a,b '||' o,p by A2,A8,PARSP_1:def 12;
assume o <> p;
hence thesis by A10,A9,PARSP_1:26;
end;
now
assume A11: o = p;
then p,a '||' p,b by A3,CKB23:def 1;
then A12: a,b '||' p,b by PARSP_1:24;
p,b '||' p,q by A5,A11,CKB23:def 1;
hence thesis by A2,A11,A12,PARSP_1:26;
end;
hence thesis by A6;
end;
