begin
theorem
Th21: (for a,b being non  empty Nat holds ((for p being (Element of ( NAT )) holds (p is  prime implies ( p |-count a ) = ( p |-count b ))) implies a = b))
proof
let a being non  empty Nat;
let b being non  empty Nat;
assume A1: (for p being (Element of ( NAT )) holds (p is  prime implies ( p |-count a ) = ( p |-count b )));
then (for p being (Element of ( NAT )) holds (p is  prime implies ( p |-count a ) <= ( p |-count b )));
then consider c being (Element of ( NAT )) such that A2: b = ( a * c ) by CKB21:1;
(for p being (Element of ( NAT )) holds (p is  prime implies ( p |-count b ) <= ( p |-count a ))) by A1;
then consider d being (Element of ( NAT )) such that A3: a = ( b * d ) by CKB21:1;
a = ( a * ( c * d ) ) by A2,A3;
then c = 1 by NAT_1:15,XCMPLX_1:7;
hence thesis by A2;
end;
