begin
theorem
Lm1: (for p being Nat holds ((not p is  prime) iff (p <= 1 or (ex n being (Element of ( NAT )) st (((n divides p & 1 < n) & ( n * n ) <= p) & n is  prime)))))
proof
let p being Nat;
A1:now
assume A2: (not p is  prime);
assume (not p <= 1);
then (ex k being (Element of ( NAT )) st ((k divides p & 1 < k) & k < p)) by A2,CKB12:1;
then consider k being (Element of ( NAT )) such that A3: (k divides p & 1 < k) and A4: ( k * k ) <= p by CKB8:1;
consider n being (Element of ( NAT )) such that A5: n divides p and A6: n <= k and A7: n is  prime by A3,CKB13:1;
take n;
thus n divides p by A5;
thus 1 < n by A7,CKB12:1;
( n * n ) <= ( k * k ) by A6,XREAL_1:66;
hence ( n * n ) <= p by A4,XXREAL_0:2;
thus n is  prime by A7;
end;
now
assume A8: (p <= 1 or (ex n being (Element of ( NAT )) st ((n divides p & 1 < n) & ( n * n ) <= p)));
per cases  by A8;
suppose p <= 1;

hence (not p is  prime) by CKB12:1;
end;
suppose (ex n being (Element of ( NAT )) st ((n divides p & 1 < n) & ( n * n ) <= p));

then consider n being (Element of ( NAT )) such that A9: n divides p and A10: 1 < n and A11: ( n * n ) <= p;
n < p by A10,A11,CKB2:1;
hence (not p is  prime) by A9,A10,CKB12:1;
end;
end;
hence thesis by A1;
end;
