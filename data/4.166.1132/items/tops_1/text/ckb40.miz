begin
reserve TS for TopSpace;
reserve x for set;
reserve P for (Subset of TS);
reserve Q for (Subset of TS);
theorem
(P is  open iff (for x holds (x in P iff (ex Q st ((Q is  open & Q c= P) & x in Q)))))
proof
thus (P is  open implies (for x holds (x in P iff (ex Q st ((Q is  open & Q c= P) & x in Q)))));
assume A1: (for x holds (x in P iff (ex Q st ((Q is  open & Q c= P) & x in Q))));
now
let x;
(x in P iff (ex Q st ((Q is  open & Q c= P) & x in Q))) by A1;
hence (x in P iff x in ( Int P )) by CKB37:1;
end;
hence thesis by TARSKI:1;
end;
