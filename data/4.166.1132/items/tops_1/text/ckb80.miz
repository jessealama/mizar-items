environ
vocabularies STRUCT_0,SUBSET_1,PRE_TOPC,XBOOLE_0,RCOMP_1,TARSKI,TOPS_1;
notations TARSKI,XBOOLE_0,SUBSET_1,STRUCT_0,PRE_TOPC,CKB23,CKB43,CKB66,CKB73;
definitions TARSKI,XBOOLE_0,PRE_TOPC,SUBSET_1,STRUCT_0,CKB23,CKB43,CKB66,CKB73;
theorems PRE_TOPC,TARSKI,SUBSET_1,XBOOLE_0,XBOOLE_1,CKB1,CKB2,CKB4,CKB6,CKB9,CKB10,CKB11,CKB14,CKB15,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB28,CKB29,CKB30,CKB31,CKB37,CKB38,CKB39,CKB40,CKB41,CKB42,CKB43,CKB45,CKB46,CKB47,CKB48,CKB49,CKB50,CKB51,CKB52,CKB53,CKB54,CKB55,CKB56,CKB57,CKB58,CKB59,CKB60,CKB61,CKB62,CKB63,CKB64,CKB65,CKB66,CKB69,CKB70,CKB71,CKB72,CKB73,CKB75,CKB78,CKB79;
registrations XBOOLE_0,SUBSET_1,STRUCT_0,PRE_TOPC,CKB3,CKB5,CKB7,CKB8,CKB12,CKB13,CKB16,CKB17,CKB27,CKB32,CKB33,CKB34,CKB35,CKB36,CKB44,CKB67,CKB68,CKB74,CKB76,CKB77;
constructors SETFAM_1,PRE_TOPC,CKB23,CKB43,CKB66,CKB73;
requirements BOOLE,SUBSET;
begin
reserve TS for TopSpace;
reserve P for (Subset of TS);
reserve Q for (Subset of TS);
theorem
(P is  closed implies (P is  boundary iff (for Q holds ((Q <> ( {} ) & Q is  open) implies (ex G being (Subset of TS) st (((G c= Q & G <> ( {} )) & G is  open) & P misses G))))))
proof
assume A1: P is  closed;
hereby
assume P is  boundary;
then A2: ( P ` ) is  dense by CKB73:def 1;
A3: P misses ( P ` ) by XBOOLE_1:79;
let Q;
assume that
A4: Q <> ( {} )
and
A5: Q is  open;
( P /\ ( ( P ` ) /\ Q ) ) = ( ( P /\ ( P ` ) ) /\ Q ) by XBOOLE_1:16
.= ( ( {} TS ) /\ Q ) by A3,XBOOLE_0:def 7
.= ( {} );
then A6: P misses ( ( P ` ) /\ Q ) by XBOOLE_0:def 7;
( P ` ) meets Q by A2,A4,A5,CKB70:1;
then ( ( P ` ) /\ Q ) <> ( {} ) by XBOOLE_0:def 7;
hence (ex G being (Subset of TS) st (((G c= Q & G <> ( {} )) & G is  open) & P misses G)) by A1,A5,A6,XBOOLE_1:17;
end;
assume A7: (for Q holds ((Q <> ( {} ) & Q is  open) implies (ex G being (Subset of TS) st (((G c= Q & G <> ( {} )) & G is  open) & P misses G))));
now
let Q;
assume that
A8: Q <> ( {} )
and
A9: Q is  open;
consider G being (Subset of TS) such that A10: G c= Q and A11: G <> ( {} ) and G is  open and A12: P misses G by A7,A8,A9;
G misses ( ( P ` ) ` ) by A12;
then G c= ( P ` ) by SUBSET_1:24;
hence ( P ` ) meets Q by A10,A11,XBOOLE_1:67;
end;
then ( P ` ) is  dense by CKB70:1;
hence thesis by CKB73:def 1;
end;
