begin
reserve TS for TopSpace;
reserve P for (Subset of TS);
reserve Q for (Subset of TS);
theorem
Th80: (P is  dense iff (for Q holds ((Q <> ( {} ) & Q is  open) implies P meets Q)))
proof
hereby
assume P is  dense;
then A1: ( Cl P ) = ( [#] TS ) by CKB66:def 1;
let Q;
assume that
A2: Q <> ( {} )
and
A3: Q is  open;
set x = the (Element of Q);
x in Q by A2;
then A4: TS is non empty;
x in ( Cl P ) by A2,A1,TARSKI:def 3;
hence P meets Q by A2,A3,A4,CKB20:1;
end;
assume A5: (for Q holds ((Q <> ( {} ) & Q is  open) implies P meets Q));
( [#] TS ) c= ( Cl P )
proof
let x being set;
A6: (for C being (Subset of TS) holds ((C is  open & x in C) implies P meets C)) by A5;
assume A7: x in ( [#] TS );
then TS is non empty;
hence thesis by A7,A6,CKB20:1;
end;
then ( Cl P ) = ( [#] TS ) by XBOOLE_0:def 10;
hence thesis by CKB66:def 1;
end;
