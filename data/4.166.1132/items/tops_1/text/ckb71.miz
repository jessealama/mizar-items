begin
reserve TS for TopSpace;
reserve x for set;
reserve P for (Subset of TS);
reserve Q for (Subset of TS);
theorem
Th81: (P is  dense implies (for Q holds (Q is  open implies ( Cl Q ) = ( Cl ( Q /\ P ) ))))
proof
assume A1: P is  dense;
let Q;
assume A2: Q is  open;
thus ( Cl Q ) c= ( Cl ( Q /\ P ) )
proof
let x being set;
assume A3: x in ( Cl Q );
then A4: TS is non  empty;
now
let Q1 being (Subset of TS);
assume A5: Q1 is  open;
assume x in Q1;
then Q meets Q1 by A3,A4,A5,CKB20:1;
then ( Q /\ Q1 ) <> ( {} ) by XBOOLE_0:def 7;
then P meets ( Q /\ Q1 ) by A1,A2,A5,CKB70:1;
then A6: ( P /\ ( Q /\ Q1 ) ) <> ( {} ) by XBOOLE_0:def 7;
( P /\ ( Q /\ Q1 ) ) = ( ( Q /\ P ) /\ Q1 ) by XBOOLE_1:16;
hence ( Q /\ P ) meets Q1 by A6,XBOOLE_0:def 7;
end;
hence thesis by A3,A4,CKB20:1;
end;

thus thesis by PRE_TOPC:19,XBOOLE_1:17;
end;
