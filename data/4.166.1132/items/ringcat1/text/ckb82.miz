begin
reserve UN for Universe;
theorem
Lm11: (for b being (Object of ( RingCat UN )) holds (((( dom ( id b ) ) = b & ( cod ( id b ) ) = b) & (for f being (Morphism of ( RingCat UN )) holds (( cod f ) = b implies ( ( id b ) * f ) = f))) & (for g being (Morphism of ( RingCat UN )) holds (( dom g ) = b implies ( g * ( id b ) ) = g))))
proof
set C = ( RingCat UN );
set V = ( RingObjects UN );
set X = ( Morphs V );
let b being (Object of C);
reconsider b9 = b as (Element of V);
reconsider b99 = b9 as Ring;
A1: ( id b ) = ( ID b9 ) by CKB78:1;
hence A2: ( dom ( id b ) ) = ( dom ( ID b99 ) ) by CKB68:def 1
.= b;
thus A3: ( cod ( id b ) ) = ( cod ( ID b99 ) ) by A1,CKB69:def 1
.= b;
thus (for f being (Morphism of C) holds (( cod f ) = b implies ( ( id b ) * f ) = f))
proof
let f being (Morphism of C);
assume that
A4: ( cod f ) = b;
reconsider f1 = f as  strict (Element of X) by CKB77:1;
reconsider f9 = f1 as  strict RingMorphism;
A5: ( cod f9 ) = b99 by A4,CKB69:def 1;
thus ( ( id b ) * f ) = ( ( ID b99 ) * f9 ) by A1,A2,A4,CKB79:1
.= f by A5,CKB38:1;
end;

thus (for g being (Morphism of C) holds (( dom g ) = b implies ( g * ( id b ) ) = g))
proof
let f being (Morphism of C);
assume that
A6: ( dom f ) = b;
reconsider f1 = f as  strict (Element of X) by CKB77:1;
reconsider f9 = f1 as  strict RingMorphism;
A7: ( dom f9 ) = b99 by A6,CKB68:def 1;
thus ( f * ( id b ) ) = ( f9 * ( ID b99 ) ) by A1,A3,A6,CKB79:1
.= f by A7,CKB38:1;
end;

end;
