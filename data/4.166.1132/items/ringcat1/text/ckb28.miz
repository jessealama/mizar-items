reserve F for RingMorphism;
theorem
Lm8: (for F being (Morphism of G,H) holds (G <= H implies (ex f being (Function of G,H) st F = RingMorphismStr (# G,H,f #))))
proof
let F being (Morphism of G,H);
assume G <= H;
then consider f being (Function of G,H) such that A1: F = RingMorphismStr (# G,H,f #) and f is  linear by CKB27:1;
take f;
thus thesis by A1;
end;
