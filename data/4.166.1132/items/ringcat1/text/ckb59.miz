begin
reserve x for set;
reserve y for set;
reserve UN for Universe;
definition
let UN;
func RingObjects UN -> set
means
:Def17: (for y holds (y in it iff (ex x st (x in UN &  GO x,y))));
existence
proof
defpred P[ set,set ]
 means
 GO $1,$2;
A1: (for x,y1,y2 being set holds ((P[ x,y1 ] & P[ x,y2 ]) implies y1 = y2)) by CKB57:1;
consider Y being set such that A2: (for y holds (y in Y iff (ex x st (x in UN & P[ x,y ])))) from TARSKI:sch 1(A1);
take Y;
thus thesis by A2;
end;
uniqueness
proof
let Y1 being set,Y2 being set;
assume that
A3: (for y holds (y in Y1 iff (ex x st (x in UN &  GO x,y))))
and
A4: (for y holds (y in Y2 iff (ex x st (x in UN &  GO x,y))));
now
let y;
(y in Y1 iff (ex x st (x in UN &  GO x,y))) by A3;
hence (y in Y1 iff y in Y2) by A4;
end;
hence thesis by TARSKI:1;
end;
end;
