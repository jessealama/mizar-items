begin
reserve f for RingMorphismStr;
reserve G1 for Ring;
reserve G2 for Ring;
reserve G3 for Ring;
reserve G4 for Ring;
theorem
Th12: (for f being (Morphism of G1,G2) holds (for g being (Morphism of G2,G3) holds (for h being (Morphism of G3,G4) holds (((G1 <= G2 & G2 <= G3) & G3 <= G4) implies ( h * ( g * f ) ) = ( ( h * g ) * f )))))
proof
let f being (Morphism of G1,G2);
let g being (Morphism of G2,G3);
let h being (Morphism of G3,G4);
assume that
A1: G1 <= G2
and
A2: G2 <= G3
and
A3: G3 <= G4;
consider f0 being (Function of G1,G2) such that A4: f = RingMorphismStr (# G1,G2,f0 #) by A1,CKB28:1;
consider h0 being (Function of G3,G4) such that A5: h = RingMorphismStr (# G3,G4,h0 #) by A3,CKB28:1;
consider g0 being (Function of G2,G3) such that A6: g = RingMorphismStr (# G2,G3,g0 #) by A2,CKB28:1;
A7: ( cod g ) = G3 by A6;
A8: ( dom h ) = G3 by A5;
then A9: ( h * g ) = RingMorphismStr (# G2,G4,( h0 * g0 ) #) by A6,A5,A7,CKB30:def 1;
A10: ( dom g ) = G2 by A6;
then A11: ( dom ( h * g ) ) = G2 by A7,A8,CKB35:1;
A12: ( cod f ) = G2 by A4;
then A13: ( cod ( g * f ) ) = G3 by A10,A7,CKB35:1;
( g * f ) = RingMorphismStr (# G1,G3,( g0 * f0 ) #) by A4,A6,A12,A10,CKB30:def 1;
then ( h * ( g * f ) ) = RingMorphismStr (# G1,G4,( h0 * ( g0 * f0 ) ) #) by A5,A8,A13,CKB30:def 1
.= RingMorphismStr (# G1,G4,( ( h0 * g0 ) * f0 ) #) by RELAT_1:36
.= ( ( h * g ) * f ) by A4,A12,A9,A11,CKB30:def 1;
hence thesis;
end;
