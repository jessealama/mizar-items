begin
reserve G for Ring;
theorem
Th14: (((( dom ( ID G ) ) = G & ( cod ( ID G ) ) = G) & (for f being  strict RingMorphism holds (( cod f ) = G implies ( ( ID G ) * f ) = f))) & (for g being  strict RingMorphism holds (( dom g ) = G implies ( g * ( ID G ) ) = g)))
proof
set i = ( ID G );
thus A1: (( dom i ) = G & ( cod i ) = G);
thus (for f being  strict RingMorphism holds (( cod f ) = G implies ( i * f ) = f))
proof
let f being  strict RingMorphism;
assume that
A2: ( cod f ) = G;
set H = ( dom f );
reconsider f9 = f as (Morphism of H,G) by A2,CKB26:1;
consider m being (Function of H,G) such that A3: f9 = RingMorphismStr (# H,G,m #) by A2;
( ( id G ) * m ) = m by FUNCT_2:17;
hence thesis by A1,A2,A3,CKB30:def 1;
end;

thus (for g being  strict RingMorphism holds (( dom g ) = G implies ( g * ( ID G ) ) = g))
proof
let f being  strict RingMorphism;
assume that
A4: ( dom f ) = G;
set H = ( cod f );
reconsider f9 = f as (Morphism of G,H) by A4,CKB26:1;
consider m being (Function of G,H) such that A5: f9 = RingMorphismStr (# G,H,m #) by A4;
( m * ( id G ) ) = m by FUNCT_2:17;
hence thesis by A1,A4,A5,CKB30:def 1;
end;

