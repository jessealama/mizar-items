let f being (PartFunc of ( REAL ),( REAL ));
let A being non  empty  closed_interval (Subset of ( REAL ));
let a;
let b;
consider a1,b1 being Real such that A1: a1 <= b1 and A2: A = [. a1,b1 .] by MEASURE5:14;
