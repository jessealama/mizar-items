constructors SETFAM_1,WELLORD1,WELLORD2,DOMAIN_1,XXREAL_0,NAT_1,INT_1,FINSEQ_3,CQC_SIM1,SUBSTUT2,CALCUL_1,CALCUL_2,RELSET_1,CKB4,CKB5,CKB7;
requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve Z for (Subset of ( CQC-WFF ));
reserve k for (Element of ( NAT ));
reserve m for (Element of ( NAT ));
reserve n for (Element of ( NAT ));
reserve p for (Element of ( CQC-WFF ));
reserve f1 for (FinSequence of ( CQC-WFF ));
reserve f2 for (FinSequence of ( CQC-WFF ));
theorem
(for f being (Function of ( NAT ),( bool ( CQC-WFF ) )) holds ((for n holds (for m holds (((m in ( dom f ) & n in ( dom f )) & n < m) implies (( f . n ) is  Consistent & ( f . n ) c= ( f . m ))))) implies ( union ( rng f ) ) is  Consistent))
proof
let f being (Function of ( NAT ),( bool ( CQC-WFF ) ));
assume A1: (for n holds (for m holds (((m in ( dom f ) & n in ( dom f )) & n < m) implies (( f . n ) is  Consistent & ( f . n ) c= ( f . m )))));
now
A2: (for n holds (n in ( dom f ) implies ( f . n ) is  Consistent))
proof
let n;
assume that
A3: n in ( dom f );
( n + 1 ) in ( NAT );
then (n < ( n + 1 ) & ( n + 1 ) in ( dom f )) by FUNCT_2:def 1,NAT_1:13;
hence thesis by A1,A3;
end;
assume (not ( union ( rng f ) ) is  Consistent);
then consider Z such that A4: (Z c= ( union ( rng f ) ) & Z is  finite) and A5: Z is  Inconsistent by CKB12:1;
(for n holds (for m holds (((m in ( dom f ) & n in ( dom f )) & n < m) implies ( f . n ) c= ( f . m )))) by A1;
then consider k such that A6: Z c= ( f . k ) by A4,CKB3:1;
reconsider Y = ( f . k ) as (Subset of ( CQC-WFF ));
consider p such that A7: Z |- p and A8: Z |- ( 'not' p ) by A5,CKB5:def 1;
consider f1 such that A9: ( rng f1 ) c= Z and A10:  |- ( f1 ^ <* p *> ) by A7,CKB4:def 1;
consider f2 such that A11: ( rng f2 ) c= Z and A12:  |- ( f2 ^ <* ( 'not' p ) *> ) by A8,CKB4:def 1;
( rng ( f1 ^ f2 ) ) = ( ( rng f1 ) \/ ( rng f2 ) ) by FINSEQ_1:31;
then ( rng ( f1 ^ f2 ) ) c= Z by A9,A11,XBOOLE_1:8;
then A13: ( rng ( f1 ^ f2 ) ) c= Y by A6,XBOOLE_1:1;
 |- ( ( f1 ^ f2 ) ^ <* ( 'not' p ) *> ) by A12,CALCUL_2:20;
then A14: Y |- ( 'not' p ) by A13,CKB4:def 1;
 |- ( ( f1 ^ f2 ) ^ <* p *> ) by A10,CKB10:1;
then Y |- p by A13,CKB4:def 1;
then A15: (not Y is  Consistent) by A14,CKB5:def 1;
k in ( NAT );
then k in ( dom f ) by FUNCT_2:def 1;
hence contradiction by A15,A2;
end;
hence thesis;
end;
