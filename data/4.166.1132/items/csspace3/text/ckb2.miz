begin
theorem
Th1: (for c being Complex holds (for seq being Complex_Sequence holds (for rseq being Real_Sequence holds ((seq is  convergent & (for i being (Element of ( NAT )) holds ( rseq . i ) = |. ( ( seq . i ) - c ) .|)) implies (rseq is  convergent & ( lim rseq ) = |. ( ( lim seq ) - c ) .|)))))
proof
let c being Complex;
let seq being Complex_Sequence;
let rseq being Real_Sequence;
assume that
A1: seq is  convergent
and
A2: (for i being (Element of ( NAT )) holds ( rseq . i ) = |. ( ( seq . i ) - c ) .|);
reconsider cseq = ( ( NAT ) --> c ) as Complex_Sequence;
A3: (for n being (Element of ( NAT )) holds ( cseq . n ) = c) by FUNCOP_1:7;
then A4: cseq is  convergent by COMSEQ_2:9;
then reconsider seq9 = ( seq - cseq ) as  convergent Complex_Sequence by A1;
( seq - cseq ) is  convergent by A1,A4;
then A5: ( lim |. ( seq - cseq ) .| ) = |. ( lim ( seq - cseq ) ) .| by COMSEQ_2:11
.= |. ( ( lim seq ) - ( lim cseq ) ) .| by A1,A4,COMSEQ_2:26
.= |. ( ( lim seq ) - c ) .| by A3,COMSEQ_2:10;
now
let i being (Element of ( NAT ));
thus ( rseq . i ) = |. ( ( seq . i ) - c ) .| by A2
.= |. ( ( seq . i ) - ( cseq . i ) ) .| by FUNCOP_1:7
.= |. ( ( seq . i ) + ( - ( cseq . i ) ) ) .|
.= |. ( ( seq . i ) + ( ( - cseq ) . i ) ) .| by VALUED_1:8
.= |. ( ( seq - cseq ) . i ) .| by VALUED_1:1
.= ( |. ( seq - cseq ) .| . i ) by VALUED_1:18;
end;
then A6: (for x being set holds (x in ( NAT ) implies ( rseq . x ) = ( |. ( seq - cseq ) .| . x )));
|. seq9 .| is  convergent;
hence thesis by A6,A5,FUNCT_2:12;
end;
