constructors PARTFUN1,BINOP_1,FUNCOP_1,XXREAL_0,REAL_1,NAT_1,COMSEQ_2,COMSEQ_3,REALSET1,CSSPACE,SEQ_1,SEQ_2,SERIES_1,VALUED_1,RELSET_1,CKB1;
requirements SUBSET,REAL,BOOLE,NUMERALS,ARITHM;
begin
registration
cluster ( the_set_of_l1ComplexSequences ) ->  linearly-closed;
coherence
proof
set W = ( the_set_of_l1ComplexSequences );
A1: (for v,u being (VECTOR of ( Linear_Space_of_ComplexSequences )) holds ((v in ( the_set_of_l1ComplexSequences ) & u in ( the_set_of_l1ComplexSequences )) implies ( v + u ) in ( the_set_of_l1ComplexSequences )))
proof
let v being (VECTOR of ( Linear_Space_of_ComplexSequences ));
let u being (VECTOR of ( Linear_Space_of_ComplexSequences ));
assume that
A2: v in W
and
A3: u in W;
( seq_id ( v + u ) ) is  absolutely_summable
proof
set r = |. ( seq_id ( v + u ) ) .|;
set q = |. ( seq_id u ) .|;
set p = |. ( seq_id v ) .|;
A4: (for n being (Element of ( NAT )) holds ( 0 ) <= ( r . n ))
proof
let n being (Element of ( NAT ));
( r . n ) = |. ( ( seq_id ( v + u ) ) . n ) .| by VALUED_1:18;
hence thesis by COMPLEX1:46;
end;
A5: (for n being (Element of ( NAT )) holds ( r . n ) <= ( ( p + q ) . n ))
proof
let n being (Element of ( NAT ));
A6: ( |. ( ( seq_id v ) . n ) .| + |. ( ( seq_id u ) . n ) .| ) = ( ( |. ( seq_id v ) .| . n ) + |. ( ( seq_id u ) . n ) .| ) by VALUED_1:18
.= ( ( |. ( seq_id v ) .| . n ) + ( |. ( seq_id u ) .| . n ) ) by VALUED_1:18
.= ( ( p + q ) . n ) by SEQ_1:7;
( r . n ) = |. ( ( seq_id ( v + u ) ) . n ) .| by VALUED_1:18
.= |. ( ( seq_id ( ( seq_id v ) + ( seq_id u ) ) ) . n ) .| by CSSPACE:2,CSSPACE:def 7
.= |. ( ( ( seq_id v ) + ( seq_id u ) ) . n ) .| by CSSPACE:1
.= |. ( ( ( seq_id v ) . n ) + ( ( seq_id u ) . n ) ) .| by VALUED_1:1;
hence thesis by A6,COMPLEX1:56;
end;
( seq_id u ) is  absolutely_summable by A3,CKB1:def 1;
then A7: q is  summable by COMSEQ_3:def 9;
( seq_id v ) is  absolutely_summable by A2,CKB1:def 1;
then p is  summable by COMSEQ_3:def 9;
then ( p + q ) is  summable by A7,SERIES_1:7;
then r is  summable by A4,A5,SERIES_1:20;
hence thesis by COMSEQ_3:def 9;
end;
hence thesis by CKB1:def 1,CSSPACE:def 7;
end;
(for a being Complex holds (for v being (VECTOR of ( Linear_Space_of_ComplexSequences )) holds (v in W implies ( a * v ) in W)))
proof
let a being Complex;
let v being (VECTOR of ( Linear_Space_of_ComplexSequences ));
assume that
A8: v in W;
( seq_id ( a * v ) ) is  absolutely_summable
proof
set r1 = ( |. a .| (#) |. ( seq_id v ) .| );
set q1 = |. ( seq_id ( a * v ) ) .|;
A9: (for n being (Element of ( NAT )) holds ( 0 ) <= ( q1 . n ))
proof
let n being (Element of ( NAT ));
( q1 . n ) = |. ( ( seq_id ( a * v ) ) . n ) .| by VALUED_1:18;
hence thesis by COMPLEX1:46;
end;
A10: (for n being (Element of ( NAT )) holds ( q1 . n ) <= ( r1 . n ))
proof
let n being (Element of ( NAT ));
( q1 . n ) = |. ( ( seq_id ( a * v ) ) . n ) .| by VALUED_1:18
.= |. ( ( seq_id ( a (#) ( seq_id v ) ) ) . n ) .| by CSSPACE:3,CSSPACE:def 7
.= |. ( ( a (#) ( seq_id v ) ) . n ) .| by CSSPACE:1
.= ( |. ( a (#) ( seq_id v ) ) .| . n ) by VALUED_1:18;
hence thesis by COMSEQ_1:50;
end;
( seq_id v ) is  absolutely_summable by A8,CKB1:def 1;
then |. ( seq_id v ) .| is  summable by COMSEQ_3:def 9;
then r1 is  summable by SERIES_1:10;
then q1 is  summable by A9,A10,SERIES_1:20;
hence thesis by COMSEQ_3:def 9;
end;
hence thesis by CKB1:def 1,CSSPACE:def 7;
end;
hence thesis by A1,CLVECT_1:def 7;
end;
end;
