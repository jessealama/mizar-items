begin
reserve i for Nat;
reserve j for Nat;
reserve x for set;
reserve D for non  empty set;
theorem
Th19: (for x being (Element of D) holds <* <* x *> *> = ( <* <* x *> *> @ ))
proof
let x being (Element of D);
set P = <* <* x *> *>;
set R = ( <* <* x *> *> @ );
A1: ( len P ) = 1 by FINSEQ_1:40;
then A2: ( width P ) = ( len <* x *> ) by MATRIX_1:20
.= 1 by FINSEQ_1:40;
then A3: ( len R ) = 1 by MATRIX_2:10;
A4:now
let i;
let j;
assume A5: [ i,j ] in ( Indices P );
then A6: [ i,j ] in [: ( dom P ),( Seg 1 ) :] by A2,MATRIX_1:def 4;
then i in ( dom P ) by ZFMISC_1:87;
then i in ( Seg 1 ) by A1,FINSEQ_1:def 3;
then A7: i = 1 by FINSEQ_1:2,TARSKI:def 1;
j in ( Seg 1 ) by A6,ZFMISC_1:87;
then j = 1 by FINSEQ_1:2,TARSKI:def 1;
hence ( P * (i,j) ) = ( R * (i,j) ) by A5,A7,MATRIX_1:def 6;
end;
( width R ) = 1 by A1,A2,MATRIX_2:10;
hence thesis by A1,A2,A3,A4,MATRIX_1:21;
end;
