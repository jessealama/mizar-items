begin
reserve k for Nat;
reserve i for Nat;
reserve m for Nat;
reserve n for Nat;
reserve K for Field;
reserve V for (VectSp of K);
theorem
Th31: (for M1 being (Matrix of n,k,(the carrier of V)) holds (for M2 being (Matrix of m,k,(the carrier of V)) holds ( Sum ( M1 ^ M2 ) ) = ( ( Sum M1 ) ^ ( Sum M2 ) )))
proof
let M1 being (Matrix of n,k,(the carrier of V));
let M2 being (Matrix of m,k,(the carrier of V));
A1: ( dom ( Sum ( M1 ^ M2 ) ) ) = ( Seg ( len ( Sum ( M1 ^ M2 ) ) ) ) by FINSEQ_1:def 3;
A2:now
let i being Nat;
assume A3: i in ( dom ( Sum ( M1 ^ M2 ) ) );
then i in ( Seg ( len ( M1 ^ M2 ) ) ) by A1,CKB21:def 1;
then A4: i in ( dom ( M1 ^ M2 ) ) by FINSEQ_1:def 3;
now
per cases  by A4,FINSEQ_1:25;
suppose A5: i in ( dom M1 );

( len M1 ) = ( len ( Sum M1 ) ) by CKB21:def 1;
then A6: ( dom M1 ) = ( dom ( Sum M1 ) ) by FINSEQ_3:29;
thus ( ( Sum ( M1 ^ M2 ) ) . i ) = ( ( Sum ( M1 ^ M2 ) ) /. i ) by A3,PARTFUN1:def 6
.= ( Sum ( ( M1 ^ M2 ) /. i ) ) by A3,CKB21:def 1
.= ( Sum ( M1 /. i ) ) by A5,FINSEQ_4:68
.= ( ( Sum M1 ) /. i ) by A5,A6,CKB21:def 1
.= ( ( Sum M1 ) . i ) by A5,A6,PARTFUN1:def 6
.= ( ( ( Sum M1 ) ^ ( Sum M2 ) ) . i ) by A5,A6,FINSEQ_1:def 7;
end;
suppose A7: (ex n being Nat st (n in ( dom M2 ) & i = ( ( len M1 ) + n )));

A8: ( len M1 ) = ( len ( Sum M1 ) ) by CKB21:def 1;
( len M2 ) = ( len ( Sum M2 ) ) by CKB21:def 1;
then A9: ( dom M2 ) = ( dom ( Sum M2 ) ) by FINSEQ_3:29;
consider n being Nat such that A10: n in ( dom M2 ) and A11: i = ( ( len M1 ) + n ) by A7;
thus ( ( Sum ( M1 ^ M2 ) ) . i ) = ( ( Sum ( M1 ^ M2 ) ) /. i ) by A3,PARTFUN1:def 6
.= ( Sum ( ( M1 ^ M2 ) /. i ) ) by A3,CKB21:def 1
.= ( Sum ( M2 /. n ) ) by A10,A11,FINSEQ_4:69
.= ( ( Sum M2 ) /. n ) by A10,A9,CKB21:def 1
.= ( ( Sum M2 ) . n ) by A10,A9,PARTFUN1:def 6
.= ( ( ( Sum M1 ) ^ ( Sum M2 ) ) . i ) by A10,A11,A8,A9,FINSEQ_1:def 7;
end;
end;
hence ( ( Sum ( M1 ^ M2 ) ) . i ) = ( ( ( Sum M1 ) ^ ( Sum M2 ) ) . i );
end;
( len ( Sum ( M1 ^ M2 ) ) ) = ( len ( M1 ^ M2 ) ) by CKB21:def 1
.= ( ( len M1 ) + ( len M2 ) ) by FINSEQ_1:22
.= ( ( len ( Sum M1 ) ) + ( len M2 ) ) by CKB21:def 1
.= ( ( len ( Sum M1 ) ) + ( len ( Sum M2 ) ) ) by CKB21:def 1
.= ( len ( ( Sum M1 ) ^ ( Sum M2 ) ) ) by FINSEQ_1:22;
hence thesis by A2,FINSEQ_2:9;
end;
