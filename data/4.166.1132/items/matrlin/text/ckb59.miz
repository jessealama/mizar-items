.= ( width ( AutMt (f1,b1,b2) ) ) by A8,CKB56:1;
end;
end;
then A9: ( width ( AutMt (( f1 + f2 ),b1,b2) ) ) = ( width ( ( AutMt (f1,b1,b2) ) + ( AutMt (f2,b1,b2) ) ) ) by MATRIX_3:def 3;
( len ( AutMt (f1,b1,b2) ) ) = ( len b1 ) by CKB53:def 1
.= ( len ( AutMt (f2,b1,b2) ) ) by CKB53:def 1;
then A10: ( dom ( AutMt (f1,b1,b2) ) ) = ( dom ( AutMt (f2,b1,b2) ) ) by FINSEQ_3:29;
A11: (for i holds (for j holds ([ i,j ] in ( Indices ( AutMt (( f1 + f2 ),b1,b2) ) ) implies ( ( AutMt (( f1 + f2 ),b1,b2) ) * (i,j) ) = ( ( ( AutMt (f1,b1,b2) ) + ( AutMt (f2,b1,b2) ) ) * (i,j) ))))
proof
let i;
let j;
assume A12: [ i,j ] in ( Indices ( AutMt (( f1 + f2 ),b1,b2) ) );
then A13: [ i,j ] in [: ( dom ( AutMt (( f1 + f2 ),b1,b2) ) ),( Seg ( width ( AutMt (( f1 + f2 ),b1,b2) ) ) ) :] by MATRIX_1:def 4;
then A14: [ i,j ] in ( Indices ( AutMt (f1,b1,b2) ) ) by A2,A6,MATRIX_1:def 4;
A15: [ i,j ] in ( Indices ( AutMt (f2,b1,b2) ) ) by A10,A3,A2,A6,A13,MATRIX_1:def 4;
( ( AutMt (( f1 + f2 ),b1,b2) ) * (i,j) ) = ( ( ( AutMt (f1,b1,b2) ) * (i,j) ) + ( ( AutMt (f2,b1,b2) ) * (i,j) ) )
proof
consider KL3 being (Linear_Combination of V2) such that A16: (( f2 . ( b1 /. i ) ) = ( Sum KL3 ) & ( Carrier KL3 ) c= ( rng b2 )) and A17: (for t holds ((1 <= t & t <= ( len ( ( f2 . ( b1 /. i ) ) |-- b2 ) )) implies ( ( ( f2 . ( b1 /. i ) ) |-- b2 ) /. t ) = ( KL3 . ( b2 /. t ) ))) by CKB46:def 1;
consider KL2 being (Linear_Combination of V2) such that A18: (( f1 . ( b1 /. i ) ) = ( Sum KL2 ) & ( Carrier KL2 ) c= ( rng b2 )) and A19: (for t holds ((1 <= t & t <= ( len ( ( f1 . ( b1 /. i ) ) |-- b2 ) )) implies ( ( ( f1 . ( b1 /. i ) ) |-- b2 ) /. t ) = ( KL2 . ( b2 /. t ) ))) by CKB46:def 1;
A20: i in ( dom ( AutMt (( f1 + f2 ),b1,b2) ) ) by A13,ZFMISC_1:87;
then A21: i in ( dom b1 ) by CKB54:1;
reconsider b4 = ( rng b2 ) as (Basis of V2) by CKB13:def 1;
consider p1 being (FinSequence of K) such that A22: p1 = ( ( AutMt (( f1 + f2 ),b1,b2) ) . i ) and A23: ( ( AutMt (( f1 + f2 ),b1,b2) ) * (i,j) ) = ( p1 . j ) by A12,MATRIX_1:def 5;
consider KL1 being (Linear_Combination of V2) such that A24: (( ( f1 + f2 ) . ( b1 /. i ) ) = ( Sum KL1 ) & ( Carrier KL1 ) c= ( rng b2 )) and A25: (for t holds ((1 <= t & t <= ( len ( ( ( f1 + f2 ) . ( b1 /. i ) ) |-- b2 ) )) implies ( ( ( ( f1 + f2 ) . ( b1 /. i ) ) |-- b2 ) /. t ) = ( KL1 . ( b2 /. t ) ))) by CKB46:def 1;
(b4 is  linearly-independent & ( ( f1 + f2 ) . ( b1 /. i ) ) = ( ( f1 . ( b1 /. i ) ) + ( f2 . ( b1 /. i ) ) )) by CKB14:def 1,VECTSP_7:def 3;
then A26: ( KL1 . ( b2 /. j ) ) = ( ( KL2 + KL3 ) . ( b2 /. j ) ) by A24,A18,A16,CKB8:1
.= ( ( KL2 . ( b2 /. j ) ) + ( KL3 . ( b2 /. j ) ) ) by VECTSP_6:22;
A27: p1 = ( ( AutMt (( f1 + f2 ),b1,b2) ) /. i ) by A22,A20,PARTFUN1:def 6
.= ( ( ( f1 + f2 ) . ( b1 /. i ) ) |-- b2 ) by A21,CKB53:def 1;
consider p3 being (FinSequence of K) such that A28: p3 = ( ( AutMt (f2,b1,b2) ) . i ) and A29: ( ( AutMt (f2,b1,b2) ) * (i,j) ) = ( p3 . j ) by A15,MATRIX_1:def 5;
consider p2 being (FinSequence of K) such that A30: p2 = ( ( AutMt (f1,b1,b2) ) . i ) and A31: ( ( AutMt (f1,b1,b2) ) * (i,j) ) = ( p2 . j ) by A14,MATRIX_1:def 5;
A32: j in ( Seg ( width ( AutMt (( f1 + f2 ),b1,b2) ) ) ) by A13,ZFMISC_1:87;
