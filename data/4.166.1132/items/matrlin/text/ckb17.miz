begin
reserve k for Nat;
reserve K for Field;
reserve a for (Element of K);
reserve V1 for  finite-dimensional (VectSp of K);
reserve v for (Element of V1);
reserve F for (FinSequence of V1);
theorem
Th14: (for a being (Element of V1) holds (for F being (FinSequence of K) holds (for G being (FinSequence of V1) holds ((( len F ) = ( len G ) & (for k holds (k in ( dom F ) implies ( G . k ) = ( ( F /. k ) * a )))) implies ( Sum G ) = ( ( Sum F ) * a )))))
proof
let a being (Element of V1);
let F being (FinSequence of K);
let G being (FinSequence of V1);
assume that
A1: ( len F ) = ( len G )
and
A2: (for k holds (k in ( dom F ) implies ( G . k ) = ( ( F /. k ) * a )));
now
let k;
let v being (Element of K);
assume that
A3: k in ( dom G )
and
A4: v = ( F . k );
A5: k in ( dom F ) by A1,A3,FINSEQ_3:29;
then v = ( F /. k ) by A4,PARTFUN1:def 6;
hence ( G . k ) = ( v * a ) by A2,A5;
end;
hence thesis by A1,CKB16:1;
end;
