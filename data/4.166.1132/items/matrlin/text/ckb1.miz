begin
reserve k for Nat;
reserve n for Nat;
reserve x for set;
reserve D for non  empty set;
definition
let D being non  empty set;
let k;
let M being (Matrix of D);
redefine func Del (M,k) -> (Matrix of D);

coherence
proof
(ex n st (for x holds (x in ( rng ( Del (M,k) ) ) implies (ex p being (FinSequence of D) st (x = p & ( len p ) = n)))))
proof
consider n such that A1: (for x holds (x in ( rng M ) implies (ex p being (FinSequence of D) st (x = p & ( len p ) = n)))) by MATRIX_1:9;
take n;
let x;
assume that
A2: x in ( rng ( Del (M,k) ) );
( Del (M,k) ) is (FinSequence of ( D * )) by FINSEQ_3:105;
then ( rng ( Del (M,k) ) ) c= ( D * ) by FINSEQ_1:def 4;
then reconsider p = x as (FinSequence of D) by A2,FINSEQ_1:def 11;
take p;
( rng ( Del (M,k) ) ) c= ( rng M ) by FINSEQ_3:106;
then (ex p1 being (FinSequence of D) st (x = p1 & ( len p1 ) = n)) by A1,A2;
hence thesis;
end;
hence thesis by MATRIX_1:9;
end;
end;
