begin
reserve n for (Element of ( NAT ));
reserve k for (Element of ( NAT ));
reserve k1 for (Element of ( NAT ));
reserve r for  real number;
reserve r1 for  real number;
reserve s for  real number;
reserve seq for Real_Sequence;
theorem
Th35: (seq is  non-increasing implies (for n holds (for R being (Subset of ( REAL )) holds (R = { ( seq . k ): n <= k } implies ( upper_bound R ) = ( seq . n )))))
proof
assume A1: seq is  non-increasing;
let n;
A2:now
let r;
assume r in { ( seq . k ): n <= k };
then consider r1 being  real number such that A3: (r = r1 & r1 in { ( seq . k ): n <= k });
consider k1 such that A4: r1 = ( seq . k1 ) and A5: n <= k1 by A3;
consider k being Nat such that A6: ( n + k ) = k1 by A5,NAT_1:10;
k in ( NAT ) by ORDINAL1:def 12;
hence r <= ( seq . n ) by A1,A3,A4,A6,SEQM_3:7;
end;
let R being (Subset of ( REAL ));
A7:now
let s;
A8: ( seq . n ) in { ( seq . k ): n <= k };
assume ( 0 ) < s;
hence (ex r st (r in { ( seq . k ): n <= k } & ( ( seq . n ) - s ) < r)) by A8,XREAL_1:44;
end;
assume A9: R = { ( seq . k ): n <= k };
then A10: R <> ( {} ) by SETLIM_1:1;
R is  bounded_above by A1,A9,CKB36:1,LIMFUNC1:1;
hence thesis by A9,A10,A2,A7,SEQ_4:def 1;
end;
