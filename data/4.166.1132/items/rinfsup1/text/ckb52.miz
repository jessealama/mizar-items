reserve seq for Real_Sequence;
theorem
Th47: (seq is  bounded_above implies ((for m holds (n <= m implies ( seq . m ) <= r)) iff ( ( superior_realsequence seq ) . n ) <= r))
proof
assume A1: seq is  bounded_above;
thus ((for m holds (n <= m implies ( seq . m ) <= r)) implies ( ( superior_realsequence seq ) . n ) <= r)
proof
assume (for m holds (n <= m implies ( seq . m ) <= r));
then (for k holds ( seq . ( n + k ) ) <= r) by NAT_1:11;
hence thesis by A1,CKB51:1;
end;

assume A2: ( ( superior_realsequence seq ) . n ) <= r;
now
let m;
assume n <= m;
then consider k being Nat such that A3: m = ( n + k ) by NAT_1:10;
k in ( NAT ) by ORDINAL1:def 12;
hence ( seq . m ) <= r by A1,A2,A3,CKB51:1;
end;
hence thesis;
end;
