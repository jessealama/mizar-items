func inferior_realsequence seq -> Real_Sequence
means
:Def4: (for n holds (for Y being (Subset of ( REAL )) holds (Y = { ( seq . k ): n <= k } implies ( it . n ) = ( lower_bound Y ))));
existence
proof
defpred P[ (Element of ( NAT )),(Element of ( REAL )) ]
 means
(for Y being (Subset of ( REAL )) holds (Y = { ( seq . k ): $1 <= k } implies $2 = ( lower_bound Y )));
A1: (for n being (Element of ( NAT )) holds (ex y being (Element of ( REAL )) st P[ n,y ]))
proof
let n being (Element of ( NAT ));
reconsider Y = { ( seq . k ): n <= k } as (Subset of ( REAL )) by CKB34:1;
reconsider y = ( lower_bound Y ) as (Element of ( REAL ));
(for Y being (Subset of ( REAL )) holds (Y = { ( seq . k ): n <= k } implies y = ( lower_bound Y )));
hence thesis;
end;
(ex f being (Function of ( NAT ),( REAL )) st (for n being (Element of ( NAT )) holds P[ n,( f . n ) ])) from FUNCT_2:sch 3(A1);
hence thesis;
end;
uniqueness
proof
let seq1 being Real_Sequence,seq2 being Real_Sequence;
assume that
A1: (for n holds (for Y being (Subset of ( REAL )) holds (Y = { ( seq . k ): n <= k } implies ( seq1 . n ) = ( lower_bound Y ))))
and
A2: (for m holds (for Y being (Subset of ( REAL )) holds (Y = { ( seq . k ): m <= k } implies ( seq2 . m ) = ( lower_bound Y ))));
A3: (for y holds (y in ( NAT ) implies ( seq1 . y ) = ( seq2 . y )))
proof
let y;
assume y in ( NAT );
then reconsider y as (Element of ( NAT ));
reconsider Y = { ( seq . k ): y <= k } as (Subset of ( REAL )) by CKB34:1;
( seq1 . y ) = ( lower_bound Y ) by A1;
hence thesis by A2;
end;
(( NAT ) = ( dom seq1 ) & ( NAT ) = ( dom seq2 )) by FUNCT_2:def 1;
hence thesis by A3,FUNCT_1:2;
end;
end;
