begin
reserve n for (Element of ( NAT ));
reserve m for (Element of ( NAT ));
reserve r for  real number;
reserve s for  real number;
reserve p for  real number;
reserve seq for Real_Sequence;
theorem
Th90: ((seq is  bounded & ( lim_sup seq ) = ( lim_inf seq )) iff seq is  convergent)
proof
thus ((seq is  bounded & ( lim_sup seq ) = ( lim_inf seq )) implies seq is  convergent)
proof
reconsider r = ( lower_bound ( superior_realsequence seq ) ) as  real number;
assume that
A1: seq is  bounded
and
A2: ( lim_sup seq ) = ( lim_inf seq );
A3: ( inferior_realsequence seq ) is  bounded by A1,CKB63:1;
A4: ( inferior_realsequence seq ) is  non-decreasing by A1,CKB57:1;
A5: ( superior_realsequence seq ) is  non-increasing by A1,CKB58:1;
A6: ( superior_realsequence seq ) is  bounded by A1,CKB63:1;
(for s holds (( 0 ) < s implies (ex n st (for m holds (n <= m implies ( abs ( ( seq . m ) - r ) ) < s)))))
proof
let s;
assume that
A7: ( 0 ) < s;
consider k2 being (Element of ( NAT )) such that A8: ( ( superior_realsequence seq ) . k2 ) < ( r + s ) by A6,A7,CKB11:1;
consider k1 being (Element of ( NAT )) such that A9: ( r - s ) < ( ( inferior_realsequence seq ) . k1 ) by A2,A3,A7,CKB10:1;
reconsider k = ( max (k1,k2) ) as (Element of ( NAT ));
k2 <= k by XXREAL_0:25;
then A10: ( ( superior_realsequence seq ) . k ) <= ( ( superior_realsequence seq ) . k2 ) by A5,SEQM_3:8;
k1 <= k by XXREAL_0:25;
then A11: ( ( inferior_realsequence seq ) . k1 ) <= ( ( inferior_realsequence seq ) . k ) by A4,SEQM_3:6;
(ex n st (for m holds (n <= m implies ( abs ( ( seq . m ) - r ) ) < s)))
proof
take k;
let m;
assume k <= m;
then consider k3 being Nat such that A12: m = ( k + k3 ) by NAT_1:10;
A13: k3 in ( NAT ) by ORDINAL1:def 12;
then ( seq . m ) <= ( ( superior_realsequence seq ) . k ) by A1,A12,CKB48:1;
then ( seq . m ) <= ( ( superior_realsequence seq ) . k2 ) by A10,XXREAL_0:2;
then A14: ( seq . m ) < ( r + s ) by A8,XXREAL_0:2;
( ( inferior_realsequence seq ) . k ) <= ( seq . m ) by A1,A12,A13,CKB47:1;
then ( ( inferior_realsequence seq ) . k1 ) <= ( seq . m ) by A11,XXREAL_0:2;
then ( r - s ) < ( seq . m ) by A9,XXREAL_0:2;
hence thesis by A14,CKB2:1;
end;
hence thesis;
end;
hence thesis by SEQ_2:def 6;
end;

assume A15: seq is  convergent;
then consider r such that A16: (for p holds (( 0 ) < p implies (ex n st (for m holds (n <= m implies ( abs ( ( seq . m ) - r ) ) < p))))) by SEQ_2:def 6;
A17: seq is  bounded by A15;
A18: (for p holds (( 0 ) < p implies (ex n st (( r - p ) <= ( ( inferior_realsequence seq ) . n ) & ( ( superior_realsequence seq ) . n ) <= ( r + p )))))
proof
let p;
assume ( 0 ) < p;
then consider n such that A19: (for m holds (n <= m implies ( abs ( ( seq . m ) - r ) ) < p)) by A16;
A20: (for m holds (n <= m implies (( r - p ) <= ( seq . m ) & ( seq . m ) <= ( r + p ))))
proof
let m;
assume n <= m;
then ( abs ( ( seq . m ) - r ) ) < p by A19;
hence thesis by CKB2:1;
end;
then (for m holds (n <= m implies ( seq . m ) <= ( r + p )));
then A21: ( ( superior_realsequence seq ) . n ) <= ( r + p ) by A17,CKB52:1;
(for m holds (n <= m implies ( r - p ) <= ( seq . m ))) by A20;
then ( r - p ) <= ( ( inferior_realsequence seq ) . n ) by A17,CKB50:1;
hence thesis by A21;
end;
A22: (( superior_realsequence seq ) is  bounded & ( inferior_realsequence seq ) is  bounded) by A17,CKB63:1;
A23: (for p holds (( 0 ) < p implies (( r - p ) <= ( upper_bound ( inferior_realsequence seq ) ) & ( lower_bound ( superior_realsequence seq ) ) <= ( r + p ))))
proof
let p;
assume ( 0 ) < p;
then consider n such that A24: (( r - p ) <= ( ( inferior_realsequence seq ) . n ) & ( ( superior_realsequence seq ) . n ) <= ( r + p )) by A18;
(( ( inferior_realsequence seq ) . n ) <= ( upper_bound ( inferior_realsequence seq ) ) & ( lower_bound ( superior_realsequence seq ) ) <= ( ( superior_realsequence seq ) . n )) by A22,CKB10:1,CKB11:1;
hence thesis by A24,XXREAL_0:2;
end;
reconsider r as Real by XREAL_0:def 1;
A25: (for p holds (( 0 ) < p implies ( lower_bound ( superior_realsequence seq ) ) <= ( r + p ))) by A23;
then A26: ( lower_bound ( superior_realsequence seq ) ) <= r by XREAL_1:41;
(for p holds (( 0 ) < p implies r <= ( ( upper_bound ( inferior_realsequence seq ) ) + p )))
proof
let p;
assume ( 0 ) < p;
then ( r - p ) <= ( upper_bound ( inferior_realsequence seq ) ) by A23;
hence thesis by XREAL_1:20;
end;
then A27: r <= ( upper_bound ( inferior_realsequence seq ) ) by XREAL_1:41;
A28: ( upper_bound ( inferior_realsequence seq ) ) <= ( lower_bound ( superior_realsequence seq ) ) by A15,CKB62:1;
( lower_bound ( superior_realsequence seq ) ) <= r by A25,XREAL_1:41;
then ( upper_bound ( inferior_realsequence seq ) ) <= r by A28,XXREAL_0:2;
then r = ( upper_bound ( inferior_realsequence seq ) ) by A27,XXREAL_0:1;
hence thesis by A15,A28,A26,XXREAL_0:1;
end;
