constructors REAL_1,NAT_1,COMPLEX1,SEQM_3,LIMFUNC1,PARTFUN3,PARTFUN1,SEQ_1,XXREAL_2,SEQ_4,RELSET_1,BINOP_2,SEQ_2,MEASURE6,MEMBER_1,SQUARE_1,CKB3,CKB4;
requirements REAL,NUMERALS,BOOLE,SUBSET,ARITHM;
begin
reserve n for (Element of ( NAT ));
reserve r for  real number;
reserve t for  real number;
reserve seq for Real_Sequence;
theorem
Th6: (seq is  bounded_below iff ( rng seq ) is  bounded_below)
proof
A1: (seq is  bounded_below implies ( rng seq ) is  bounded_below)
proof
assume seq is  bounded_below;
then consider t such that A2: (for n holds t < ( seq . n )) by SEQ_2:def 4;
t is (LowerBound of ( rng seq ))
proof
let r being  ext-real number;
assume r in ( rng seq );
then (ex n being set st (n in ( dom seq ) & ( seq . n ) = r)) by FUNCT_1:def 3;
hence t <= r by A2;
end;
hence thesis by XXREAL_2:def 9;
end;
(( rng seq ) is  bounded_below implies seq is  bounded_below)
proof
assume ( rng seq ) is  bounded_below;
then consider t such that A3: t is (LowerBound of ( rng seq )) by XXREAL_2:def 9;
A4: (for r holds (r in ( rng seq ) implies t <= r)) by A3,XXREAL_2:def 2;
now
let n;
t <= ( seq . n ) by A4,FUNCT_2:4;
hence ( t - 1 ) < ( seq . n ) by CKB1:1;
end;
hence thesis by SEQ_2:def 4;
end;
hence thesis by A1;
end;
