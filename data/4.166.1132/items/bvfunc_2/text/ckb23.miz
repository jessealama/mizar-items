requirements SUBSET,BOOLE;
begin
reserve Y for non empty set;
reserve G for (Subset of ( PARTITIONS Y ));
theorem
(for a being (Element of ( Funcs (Y,( BOOLEAN )) )) holds (for PA being (a_partition of Y) holds ( Ex (( O_el Y ),PA,G) ) = ( O_el Y )))
proof
let a being (Element of ( Funcs (Y,( BOOLEAN )) ));
let PA being (a_partition of Y);
(for z being (Element of Y) holds ( ( Ex (( O_el Y ),PA,G) ) . z ) = ( FALSE ))
proof
let z being (Element of Y);
(for x being (Element of Y) holds (x in ( EqClass (z,( CompF (PA,G) )) ) implies ( ( O_el Y ) . x ) <> ( TRUE ))) by BVFUNC_1:def 10;
hence thesis by BVFUNC_1:def 17;
end;
hence thesis by BVFUNC_1:def 10;
