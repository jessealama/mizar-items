environ
vocabularies XBOOLE_0,SUBSET_1,PARTIT1,EQREL_1,TARSKI,ZFMISC_1,FUNCT_1,SETFAM_1,RELAT_1,PARTFUN1,FUNCT_2,MARGREL1,BVFUNC_1,XBOOLEAN,BVFUNC_2;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,XBOOLEAN,MARGREL1,RELAT_1,FUNCT_1,SETFAM_1,RELSET_1,PARTFUN1,FUNCT_2,EQREL_1,PARTIT1,BVFUNC_1,CKB1,CKB2,CKB4,CKB5,CKB7,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17;
definitions TARSKI,BVFUNC_1,XBOOLE_0,XBOOLEAN,CKB4,CKB5,CKB7,CKB10,CKB11,CKB12,CKB14,CKB15,CKB16,CKB17;
theorems TARSKI,FUNCT_1,SETFAM_1,EQREL_1,ZFMISC_1,FUNCT_2,SUBSET_1,MARGREL1,BINARITH,PARTIT1,BVFUNC_1,RELAT_1,XBOOLE_0,XBOOLE_1,XBOOLEAN,PARTFUN1,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB14,CKB15,CKB16,CKB17;
schemes DOMAIN_1,PARTFUN2,XBOOLE_0;
registrations XBOOLE_0,SUBSET_1,RELSET_1,XBOOLEAN,EQREL_1,MARGREL1,PARTIT1;
constructors SETFAM_1,PARTIT1,BVFUNC_1,RELSET_1,CKB1,CKB2,CKB4,CKB5,CKB7,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17;
requirements SUBSET,BOOLE;
begin
reserve Y for non empty set;
reserve G for (Subset of ( PARTITIONS Y ));
theorem
(for a being (Element of ( Funcs (Y,( BOOLEAN )) )) holds (for PA being (a_partition of Y) holds ( All (a,PA,G) ) is_dependent_of ( CompF (PA,G) )))
proof
let a being (Element of ( Funcs (Y,( BOOLEAN )) ));
let PA being (a_partition of Y);
let F being set;
assume A1: F in ( CompF (PA,G) );
thus (for x1,x2 being set holds ((x1 in F & x2 in F) implies ( ( All (a,PA,G) ) . x1 ) = ( ( All (a,PA,G) ) . x2 )))
proof
let x1 being set,x2 being set;
assume A2: (x1 in F & x2 in F);
then reconsider x1,x2 as (Element of Y) by A1;
A3: x2 in ( EqClass (x2,( CompF (PA,G) )) ) by EQREL_1:def 6;
(F = ( EqClass (x2,( CompF (PA,G) )) ) or F misses ( EqClass (x2,( CompF (PA,G) )) )) by A1,EQREL_1:def 4;
then A4: ( EqClass (x1,( CompF (PA,G) )) ) = ( EqClass (x2,( CompF (PA,G) )) ) by A2,A3,EQREL_1:def 6,XBOOLE_0:3;
per cases ;
suppose A5: ((for x being (Element of Y) holds (x in ( EqClass (x1,( CompF (PA,G) )) ) implies ( a . x ) = ( TRUE ))) & (for x being (Element of Y) holds (x in ( EqClass (x2,( CompF (PA,G) )) ) implies ( a . x ) = ( TRUE ))));

then ( ( All (a,PA,G) ) . x2 ) = ( TRUE ) by BVFUNC_1:def 16;
hence thesis by A5,BVFUNC_1:def 16;
end;
suppose ((for x being (Element of Y) holds (x in ( EqClass (x1,( CompF (PA,G) )) ) implies ( a . x ) = ( TRUE ))) & (not (for x being (Element of Y) holds (x in ( EqClass (x2,( CompF (PA,G) )) ) implies ( a . x ) = ( TRUE )))));

hence thesis by A4;
end;
suppose ((not (for x being (Element of Y) holds (x in ( EqClass (x1,( CompF (PA,G) )) ) implies ( a . x ) = ( TRUE )))) & (for x being (Element of Y) holds (x in ( EqClass (x2,( CompF (PA,G) )) ) implies ( a . x ) = ( TRUE ))));

hence thesis by A4;
end;
suppose A6: ((not (for x being (Element of Y) holds (x in ( EqClass (x1,( CompF (PA,G) )) ) implies ( a . x ) = ( TRUE )))) & (not (for x being (Element of Y) holds (x in ( EqClass (x2,( CompF (PA,G) )) ) implies ( a . x ) = ( TRUE )))));

then ( ( All (a,PA,G) ) . x2 ) = ( FALSE ) by BVFUNC_1:def 16;
hence thesis by A6,BVFUNC_1:def 16;
end;
end;

end;
