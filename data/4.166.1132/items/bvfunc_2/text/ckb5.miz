begin
reserve Y for non empty set;
definition
let Y;
let G being (Subset of ( PARTITIONS Y ));
let b being set;
pred b is_upper_min_depend_of G
means
:Def2: ((for d being (a_partition of Y) holds (d in G implies b is_a_dependent_set_of d)) & (for e being set holds ((e c= b & (for d being (a_partition of Y) holds (d in G implies e is_a_dependent_set_of d))) implies e = b)))
;end;
