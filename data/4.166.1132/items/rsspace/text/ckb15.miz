let v being (VECTOR of RLSStruct (# ( the_set_of_RealSequences ),( Zeroseq ),( l_add ),( l_mult ) #));
(for s being set holds (s in ( NAT ) implies ( ( ( a + b ) (#) ( seq_id v ) ) . s ) = ( ( ( a (#) ( seq_id v ) ) + ( b (#) ( seq_id v ) ) ) . s )))
proof
let s being set;
