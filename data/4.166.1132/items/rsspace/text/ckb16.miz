theorem
Th11: (for a,b being Real holds (for v being (VECTOR of RLSStruct (# ( the_set_of_RealSequences ),( Zeroseq ),( l_add ),( l_mult ) #)) holds ( ( a * b ) * v ) = ( a * ( b * v ) )))
proof
let a being Real;
let b being Real;
let v being (VECTOR of RLSStruct (# ( the_set_of_RealSequences ),( Zeroseq ),( l_add ),( l_mult ) #));
( ( a * b ) * v ) = ( ( a * b ) (#) ( seq_id v ) ) by CKB9:1
.= ( a (#) ( b (#) ( seq_id v ) ) ) by SEQ_1:23
.= ( a (#) ( seq_id ( b (#) ( seq_id v ) ) ) ) by CKB7:1
.= ( a (#) ( seq_id ( b * v ) ) ) by CKB9:1;
hence thesis by CKB9:1;
end;
