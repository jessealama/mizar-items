requirements NUMERALS,REAL,SUBSET,BOOLE,ARITHM;
begin
definition
let L being non empty doubleLoopStr;
let F being (Subset of L);
assume A1: F is non empty;
func F -LeftIdeal -> (LeftIdeal of L)
means
:Def16: (F c= it & (for I being (LeftIdeal of L) holds (F c= I implies it c= I)));
existence
proof
set Id = { I where I is (Subset of L): (F c= I & I is (LeftIdeal of L)) };
set I = ( meet Id );
(the carrier of L) is (LeftIdeal of L) by CKB30:1;
then A18: (the carrier of L) in Id;
A19:now
let X being set;
assume X in Id;
then (ex X9 being (Subset of L) st ((X9 = X & F c= X9) & X9 is (LeftIdeal of L)));
hence F c= X;
end;
then F c= I by A18,SETFAM_1:5;
then reconsider I as non empty (Subset of L) by A1,A18,SETFAM_1:3;
A20: I is  add-closed
proof
let x being (Element of L),y being (Element of L);
assume A21: (x in I & y in I);
now
let X being set;
assume A22: X in Id;
then consider X9 being (Subset of L) such that A23: X9 = X and F c= X9 and A24: X9 is (LeftIdeal of L);
(x in X & y in X) by A21,A22,SETFAM_1:def 1;
then ( x + y ) in X9 by A23,A24,CKB4:def 1;
hence { ( x + y ) } c= X by A23,ZFMISC_1:31;
end;
then { ( x + y ) } c= I by A18,SETFAM_1:5;
hence thesis by ZFMISC_1:31;
end;
I is  left-ideal
proof
let p being (Element of L),x being (Element of L);
assume A25: x in I;
now
let X being set;
assume A26: X in Id;
then consider X9 being (Subset of L) such that A27: X9 = X and F c= X9 and A28: X9 is (LeftIdeal of L);
x in X by A25,A26,SETFAM_1:def 1;
then ( p * x ) in X9 by A27,A28,CKB5:def 1;
hence { ( p * x ) } c= X by A27,ZFMISC_1:31;
end;
then { ( p * x ) } c= I by A18,SETFAM_1:5;
hence thesis by ZFMISC_1:31;
end;
then reconsider I as (LeftIdeal of L) by A20;
take I;
now
let X being (LeftIdeal of L);
assume F c= X;
then X in Id;
hence I c= X by SETFAM_1:3;
end;
hence thesis by A18,A19,SETFAM_1:5;
end;
uniqueness
proof
let X being (LeftIdeal of L),Y being (LeftIdeal of L);
assume (((F c= X & (for I being (LeftIdeal of L) holds (F c= I implies X c= I))) & F c= Y) & (for I being (LeftIdeal of L) holds (F c= I implies Y c= I)));
then (X c= Y & Y c= X);
hence thesis by XBOOLE_0:def 10;
end;
end;
