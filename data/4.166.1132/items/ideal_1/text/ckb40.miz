begin
definition
let R being non empty addLoopStr;
let I being  add-closed non empty (Subset of R);
func add| (I,R) -> (BinOp of I)
equals
( (the addF of R) || I );
coherence
proof
reconsider f = ( (the addF of R) || I ) as (Function of [: I,I :],(the carrier of R)) by FUNCT_2:32;
A1: ( dom f ) = [: I,I :] by FUNCT_2:def 1;
(for x being set holds (x in [: I,I :] implies ( f . x ) in I))
proof
let x being set;
assume A2: x in [: I,I :];
then consider u being set,v being set such that A3: (u in I & v in I) and A4: x = [ u,v ] by ZFMISC_1:def 2;
reconsider u,v as (Element of R) by A3;
reconsider u,v as (Element of R);
( f . x ) = ( u + v ) by A1,A2,A4,FUNCT_1:47;
hence thesis by A3,CKB4:def 1;
end;
hence thesis by A1,FUNCT_2:3;
end;
