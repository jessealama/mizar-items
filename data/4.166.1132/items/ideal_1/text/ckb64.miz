theorem
(for R being non empty multLoopStr holds (for A being non empty (Subset of R) holds (for a being (Element of R) holds (for F being (LeftLinearCombination of A) holds ( F * a ) is (LinearCombination of A)))))
proof
let R being non empty multLoopStr;
let A being non empty (Subset of R);
let a being (Element of R);
let F being (LeftLinearCombination of A);
let i being set;
reconsider c = a as (Element of R);
assume i in ( dom ( F * a ) );
then A1: i in ( dom F ) by POLYNOM1:def 2;
then consider u being (Element of R),b being (Element of A) such that A2: ( F /. i ) = ( u * b ) by CKB53:def 1;
take u;
take c;
take b;
thus thesis by A1,A2,POLYNOM1:def 2;
end;
