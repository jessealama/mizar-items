environ
vocabularies BCIALG_1,XBOOLE_0,SUBSET_1,CARD_FIL,XXREAL_0,SUPINF_2,CHORD,TARSKI,RCOMP_1,BINOP_1,STRUCT_0,WAYBEL15,BCIIDEAL;
notations TARSKI,XBOOLE_0,SUBSET_1,STRUCT_0,BCIALG_1,BCIALG_2,BCIALG_3,CKB5;
definitions BCIALG_1,TARSKI,XBOOLE_0,CKB5;
theorems BCIALG_1,TARSKI,XBOOLE_0,BCIALG_2,BCIALG_3,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8;
registrations STRUCT_0,BCIALG_1,BCIALG_2,BCIALG_3;
constructors BCIALG_2,BCIALG_3,CKB5;
