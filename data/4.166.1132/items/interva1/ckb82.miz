begin
definition
let U being non empty set;
func IntervalSets U -> non empty set
means
:Def11: (for x being set holds (x in it iff x is non empty non empty (IntervalSet of U)));
existence
proof
defpred P[ set ]
 means
$1 is non empty non empty (IntervalSet of U);
(ex X being set st (for x being set holds (x in X iff (x in ( bool ( bool U ) ) & P[ x ])))) from XBOOLE_0:sch 1;
then consider X being set such that A1: (for x being set holds (x in X iff (x in ( bool ( bool U ) ) & P[ x ])));
set x = the (Element of U);
reconsider E = { x } as (Subset of U) by ZFMISC_1:31;
{ E } is non empty non empty (IntervalSet of U) by CKB12:1;
then reconsider X as non empty set by A1;
take X;
thus thesis by A1;
end;
uniqueness
proof
let S1 being non empty set,S2 being non empty set;
assume that
A2: (for x being set holds (x in S1 iff x is non empty non empty (IntervalSet of U)))
and
A3: (for x being set holds (x in S2 iff x is non empty non empty (IntervalSet of U)));
(for x being set holds (x in S1 iff x in S2))
proof
let x being set;
thus (x in S1 implies x in S2)
proof
assume x in S1;
then x is non empty non empty (IntervalSet of U) by A2;
hence thesis by A3;
end;

assume x in S2;
then x is non empty non empty (IntervalSet of U) by A3;
hence thesis by A2;
end;
hence thesis by TARSKI:1;
end;
