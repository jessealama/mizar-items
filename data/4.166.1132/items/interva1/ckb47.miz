begin
reserve U for non empty set;
theorem
Th27: (for A,B being (Subset of U) holds (for F being  ordered non empty (Subset-Family of U) holds (F = ( Inter (A,B) ) implies (( min F ) = A & ( max F ) = B))))
proof
let A being (Subset of U),B being (Subset of U);
let F being  ordered non empty (Subset-Family of U);
assume A1: F = ( Inter (A,B) );
then (A is (Element of F) & (for Y being set holds (Y in F implies A c= Y))) by CKB3:1,CKB2:1;
then A2: A = ( min F ) by CKB45:1;
(B is (Element of F) & (for Y being set holds (Y in F implies Y c= B))) by CKB3:1,A1,CKB2:1;
hence thesis by A2,CKB46:1;
end;
