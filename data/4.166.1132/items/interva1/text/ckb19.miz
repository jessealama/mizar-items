begin
definition
let U being non empty set;
let A being non empty (IntervalSet of U);
let B being non empty (IntervalSet of U);
func A _\/_ B -> (IntervalSet of U)
equals
( UNION (A,B) );
coherence
proof
set IT = ( UNION (A,B) );
consider A1 being (Subset of U),A2 being (Subset of U) such that A3: (A1 c= A2 & A = ( Inter (A1,A2) )) by CKB15:1;
consider B1 being (Subset of U),B2 being (Subset of U) such that A4: (B1 c= B2 & B = ( Inter (B1,B2) )) by CKB15:1;
set LAB = ( A1 \/ B1 );
set UAB = ( A2 \/ B2 );
IT = ( Inter (LAB,UAB) ) by CKB17:1,A3,A4;
hence thesis;
end;
end;
