begin
registration
let X being set;
cluster non empty  ordered for (Subset-Family of X);
existence
proof
reconsider F = { X } as (Subset-Family of X) by ZFMISC_1:68;
take F;
(ex A,B being set st ((A in F & B in F) & (for Y being set holds (Y in F iff (A c= Y & Y c= B)))))
proof
take X;
take X;
thus (X in F & X in F) by TARSKI:def 1;
let Y being set;
thus (Y in F implies (X c= Y & Y c= X)) by TARSKI:def 1;
assume (X c= Y & Y c= X);
then X = Y by XBOOLE_0:def 10;
hence thesis by TARSKI:def 1;
end;
hence thesis by CKB37:def 1;
end;
