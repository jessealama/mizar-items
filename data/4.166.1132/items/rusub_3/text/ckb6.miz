begin
theorem
(for V being RealUnitarySpace holds (for A being (Subset of V) holds (( Lin A ) = ( (0). V ) implies (A = ( {} ) or A = { ( 0. V ) }))))
proof
let V being RealUnitarySpace;
let A being (Subset of V);
assume that
A1: ( Lin A ) = ( (0). V )
and
A2: A <> ( {} );
thus A c= { ( 0. V ) }
proof
let x being set;
assume x in A;
then x in ( Lin A ) by CKB3:1;
then x = ( 0. V ) by A1,CKB4:1;
hence thesis by TARSKI:def 1;
end;

set y = the (Element of A);
let x being set;
assume x in { ( 0. V ) };
then A3: x = ( 0. V ) by TARSKI:def 1;
(y in A & y in ( Lin A )) by A2,CKB3:1;
hence thesis by A1,A3,CKB4:1;
end;
