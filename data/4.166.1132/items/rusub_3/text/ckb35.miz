begin
theorem
(for V being RealUnitarySpace holds (for W being (Subspace of V) holds (for A being (Subset of V) holds (A c= (the carrier of W) implies ( Lin A ) is (Subspace of W)))))
proof
let V being RealUnitarySpace;
let W being (Subspace of V);
let A being (Subset of V);
assume A1: A c= (the carrier of W);
now
let w being set;
assume w in (the carrier of ( Lin A ));
then w in ( Lin A ) by STRUCT_0:def 5;
then consider L being (Linear_Combination of A) such that A2: w = ( Sum L ) by CKB2:1;
( Carrier L ) c= A by RLVECT_2:def 6;
then (ex K being (Linear_Combination of W) st (( Carrier K ) = ( Carrier L ) & ( Sum L ) = ( Sum K ))) by A1,CKB27:1,XBOOLE_1:1;
hence w in (the carrier of W) by A2;
end;
then (the carrier of ( Lin A )) c= (the carrier of W) by TARSKI:def 3;
hence thesis by RUSUB_1:22;
end;
