begin
theorem
Lm2: (for X being set holds (for A being (Subset of X) holds (for O being (Order of X) holds (O is_connected_in X implies O is_connected_in A))))
proof
let X being set;
let A being (Subset of X);
let O being (Order of X);
assume O is_connected_in X;
then (for x,y being set holds (((x in A & y in A) & x <> y) implies ([ x,y ] in O or [ y,x ] in O))) by RELAT_2:def 6;
hence thesis by RELAT_2:def 6;
end;
