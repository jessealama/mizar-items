begin
theorem
Th15: (for n being Ordinal holds (for b being (bag of n) holds ( RelIncl n ) linearly_orders ( support b )))
proof
let n being Ordinal;
let b being (bag of n);
set R = ( RelIncl n );
set s = ( support b );
(for x,y being set holds (((x in s & y in s) & x <> y) implies ([ x,y ] in R or [ y,x ] in R)))
proof
let x being set,y being set;
assume that
A1: x in s
and
A2: y in s
and
x <> y;
assume A3: (not [ x,y ] in R);
reconsider x,y as Ordinal by A1,A2;
y c= x by A1,A2,A3,WELLORD2:def 1;
hence thesis by A1,A2,WELLORD2:def 1;
end;
then A4: R is_connected_in s by RELAT_2:def 6;
A5: R is_antisymmetric_in s by CKB1:1;
A6: R is_transitive_in s by CKB1:1;
R is_reflexive_in s by CKB1:1;
hence thesis by A4,A5,A6,ORDERS_1:def 8;
end;
