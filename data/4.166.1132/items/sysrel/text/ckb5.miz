environ
vocabularies RELAT_1,XBOOLE_0,ZFMISC_1,TARSKI,SUBSET_1,SYSREL;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,RELAT_1;
definitions TARSKI,XBOOLE_0,RELAT_1;
theorems ZFMISC_1,RELAT_1,TARSKI,XBOOLE_0,XBOOLE_1,CKB1,CKB2,CKB3,CKB4;
registrations XBOOLE_0,RELAT_1;
constructors TARSKI,SUBSET_1,RELAT_1;
requirements BOOLE,SUBSET;
begin
reserve z for set;
reserve t for set;
reserve X for set;
reserve Y for set;
reserve R for Relation;
theorem
(R c= [: X,Y :] implies ( R ~ ) c= [: Y,X :])
proof
assume A1: R c= [: X,Y :];
let z;
let t;
assume [ z,t ] in ( R ~ );
then [ t,z ] in R by RELAT_1:def 7;
then (t in X & z in Y) by A1,ZFMISC_1:87;
hence thesis by ZFMISC_1:87;
end;
