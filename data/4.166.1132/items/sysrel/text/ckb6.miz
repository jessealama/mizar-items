environ
vocabularies RELAT_1,XBOOLE_0,ZFMISC_1,TARSKI,SUBSET_1,SYSREL;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,RELAT_1;
definitions TARSKI,XBOOLE_0,RELAT_1;
theorems ZFMISC_1,RELAT_1,TARSKI,XBOOLE_0,XBOOLE_1,CKB1,CKB2,CKB3,CKB4,CKB5;
registrations XBOOLE_0,RELAT_1;
constructors TARSKI,SUBSET_1,RELAT_1;
requirements BOOLE,SUBSET;
begin
reserve x for set;
reserve y for set;
reserve X for set;
reserve Y for set;
theorem
( [: X,Y :] ~ ) = [: Y,X :]
proof
let x;
let y;
thus ([ x,y ] in ( [: X,Y :] ~ ) implies [ x,y ] in [: Y,X :])
proof
assume [ x,y ] in ( [: X,Y :] ~ );
then [ y,x ] in [: X,Y :] by RELAT_1:def 7;
then (y in X & x in Y) by ZFMISC_1:87;
hence thesis by ZFMISC_1:87;
end;

assume [ x,y ] in [: Y,X :];
then (y in X & x in Y) by ZFMISC_1:87;
then [ y,x ] in [: X,Y :] by ZFMISC_1:87;
hence thesis by RELAT_1:def 7;
end;
