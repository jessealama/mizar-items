environ
vocabularies RELAT_1,XBOOLE_0,ZFMISC_1,TARSKI,SUBSET_1,SYSREL;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,RELAT_1;
definitions TARSKI,XBOOLE_0,RELAT_1;
theorems ZFMISC_1,RELAT_1,TARSKI,XBOOLE_0,XBOOLE_1,CKB1,CKB2;
registrations XBOOLE_0,RELAT_1;
constructors TARSKI,SUBSET_1,RELAT_1;
requirements BOOLE,SUBSET;
begin
reserve X for set;
reserve Y for set;
reserve R for Relation;
theorem
(X misses Y implies ( dom ( R /\ [: X,Y :] ) ) misses ( rng ( R /\ [: X,Y :] ) ))
proof
assume A1: ( X /\ Y ) = ( {} );
( dom ( R /\ [: X,Y :] ) ) c= X by CKB2:1;
then A2: ( ( dom ( R /\ [: X,Y :] ) ) /\ ( rng ( R /\ [: X,Y :] ) ) ) c= ( X /\ ( rng ( R /\ [: X,Y :] ) ) ) by XBOOLE_1:26;
( X /\ ( rng ( R /\ [: X,Y :] ) ) ) c= ( X /\ Y ) by CKB2:1,XBOOLE_1:26;
hence ( ( dom ( R /\ [: X,Y :] ) ) /\ ( rng ( R /\ [: X,Y :] ) ) ) = ( {} ) by A1,A2,XBOOLE_1:1,XBOOLE_1:3;
end;
