environ
vocabularies RELAT_1,XBOOLE_0,ZFMISC_1,TARSKI,SUBSET_1,SYSREL;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,RELAT_1;
definitions TARSKI,XBOOLE_0,RELAT_1;
theorems ZFMISC_1,RELAT_1,TARSKI,XBOOLE_0,XBOOLE_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18;
registrations XBOOLE_0,RELAT_1;
constructors TARSKI,SUBSET_1,RELAT_1;
requirements BOOLE,SUBSET;
begin
reserve x for set;
reserve y for set;
reserve z for set;
reserve R for Relation;
theorem
(R c= ( id ( dom R ) ) implies R = ( id ( dom R ) ))
proof
assume A1: R c= ( id ( dom R ) );
let x;
let y;
thus ([ x,y ] in R implies [ x,y ] in ( id ( dom R ) )) by A1;
assume A2: [ x,y ] in ( id ( dom R ) );
then x in ( dom R ) by RELAT_1:def 10;
then A3: (ex z st [ x,z ] in R) by RELAT_1:def 4;
x = y by A2,RELAT_1:def 10;
hence thesis by A1,A3,RELAT_1:def 10;
end;
