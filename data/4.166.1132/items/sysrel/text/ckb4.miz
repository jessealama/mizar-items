environ
vocabularies RELAT_1,XBOOLE_0,ZFMISC_1,TARSKI,SUBSET_1,SYSREL;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,RELAT_1;
definitions TARSKI,XBOOLE_0,RELAT_1;
theorems ZFMISC_1,RELAT_1,TARSKI,XBOOLE_0,XBOOLE_1,CKB1,CKB2,CKB3;
registrations XBOOLE_0,RELAT_1;
constructors TARSKI,SUBSET_1,RELAT_1;
requirements BOOLE,SUBSET;
begin
reserve X for set;
reserve Y for set;
reserve R for Relation;
theorem
Th15: (R c= [: X,Y :] implies (( dom R ) c= X & ( rng R ) c= Y))
proof
assume R c= [: X,Y :];
then ( R /\ [: X,Y :] ) = R by XBOOLE_1:28;
hence thesis by CKB2:1;
end;
