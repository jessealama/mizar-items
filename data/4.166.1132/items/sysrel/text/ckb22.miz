environ
vocabularies RELAT_1,XBOOLE_0,ZFMISC_1,TARSKI,SUBSET_1,SYSREL;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,RELAT_1;
definitions TARSKI,XBOOLE_0,RELAT_1;
theorems ZFMISC_1,RELAT_1,TARSKI,XBOOLE_0,XBOOLE_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21;
registrations XBOOLE_0,RELAT_1;
constructors TARSKI,SUBSET_1,RELAT_1;
requirements BOOLE,SUBSET;
begin
reserve x for set;
reserve y for set;
reserve X for set;
reserve R for Relation;
theorem
(R c= [: X,X :] implies (( R \ ( id ( dom R ) ) ) = ( R \ ( id X ) ) & ( R \ ( id ( rng R ) ) ) = ( R \ ( id X ) )))
proof
A1: ( R \ ( id ( dom R ) ) ) c= ( R \ ( id X ) )
proof
let x;
let y;
assume A2: [ x,y ] in ( R \ ( id ( dom R ) ) );
then A3: (not [ x,y ] in ( id ( dom R ) )) by XBOOLE_0:def 5;
(not [ x,y ] in ( id X ))
proof
assume [ x,y ] in ( id X );
then A4: x = y by RELAT_1:def 10;
x in ( dom R ) by A2,RELAT_1:6;
hence contradiction by A3,A4,RELAT_1:def 10;
end;
hence thesis by A2,XBOOLE_0:def 5;
end;
A5: ( R \ ( id ( rng R ) ) ) c= ( R \ ( id X ) )
proof
let x;
let y;
assume A6: [ x,y ] in ( R \ ( id ( rng R ) ) );
then A7: (not [ x,y ] in ( id ( rng R ) )) by XBOOLE_0:def 5;
(not [ x,y ] in ( id X ))
proof
assume [ x,y ] in ( id X );
then A8: x = y by RELAT_1:def 10;
y in ( rng R ) by A6,RELAT_1:6;
hence contradiction by A7,A8,RELAT_1:def 10;
end;
hence thesis by A6,XBOOLE_0:def 5;
end;
assume A9: R c= [: X,X :];
then ( id ( dom R ) ) c= ( id X ) by CKB4:1,CKB17:1;
then ( R \ ( id X ) ) c= ( R \ ( id ( dom R ) ) ) by XBOOLE_1:34;
hence ( R \ ( id ( dom R ) ) ) = ( R \ ( id X ) ) by A1,XBOOLE_0:def 10;
( id ( rng R ) ) c= ( id X ) by A9,CKB4:1,CKB17:1;
then ( R \ ( id X ) ) c= ( R \ ( id ( rng R ) ) ) by XBOOLE_1:34;
hence thesis by A5,XBOOLE_0:def 10;
end;
