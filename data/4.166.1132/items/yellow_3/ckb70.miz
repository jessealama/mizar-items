requirements SUBSET,BOOLE;
begin
theorem
(for S1,S2 being  antisymmetric non empty RelStr holds (for D1 being non empty (Subset of S1) holds (for D2 being non empty (Subset of S2) holds (for x being (Element of S1) holds (for y being (Element of S2) holds (((for c being (Element of S1) holds (c is_>=_than D1 implies x <= c)) & (for d being (Element of S2) holds (d is_>=_than D2 implies y <= d))) implies (for b being (Element of [: S1,S2 :]) holds (b is_>=_than [: D1,D2 :] implies [ x,y ] <= b))))))))
proof
let S1 being  antisymmetric non empty RelStr,S2 being  antisymmetric non empty RelStr;
let D1 being non empty (Subset of S1);
let D2 being non empty (Subset of S2);
let x being (Element of S1);
let y being (Element of S2);
assume that
A1: (for c being (Element of S1) holds (c is_>=_than D1 implies x <= c))
and
A2: (for d being (Element of S2) holds (d is_>=_than D2 implies y <= d));
let b being (Element of [: S1,S2 :]);
assume that
A3: b is_>=_than [: D1,D2 :];
(the carrier of [: S1,S2 :]) = [: (the carrier of S1),(the carrier of S2) :] by CKB21:def 1;
then (ex c,d being set st ((c in (the carrier of S1) & d in (the carrier of S2)) & b = [ c,d ])) by ZFMISC_1:def 2;
then A4: b = [ ( b `1 ),( b `2 ) ] by MCART_1:8;
then ( b `2 ) is_>=_than D2 by A3,CKB62:1;
then A5: y <= ( b `2 ) by A2;
( b `1 ) is_>=_than D1 by A3,A4,CKB62:1;
then x <= ( b `1 ) by A1;
hence thesis by A4,A5,CKB28:1;
end;
