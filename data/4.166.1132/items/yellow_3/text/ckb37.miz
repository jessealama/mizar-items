theorem
(for X,Y being RelStr holds ([: X,Y :] is non  empty implies (X is non  empty & Y is non  empty)))
proof
let X being RelStr;
let Y being RelStr;
assume [: X,Y :] is non  empty;
then A1: (ex x being set st x in (the carrier of [: X,Y :])) by XBOOLE_0:def 1;
(the carrier of [: X,Y :]) = [: (the carrier of X),(the carrier of Y) :] by CKB21:def 1;
hence thesis by A1,MCART_1:10;
end;
