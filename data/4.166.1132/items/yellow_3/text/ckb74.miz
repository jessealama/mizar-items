environ
vocabularies XBOOLE_0,SUBSET_1,TARSKI,ORDERS_2,WAYBEL_0,XXREAL_0,ZFMISC_1,RELAT_1,MCART_1,LATTICE3,RELAT_2,LATTICES,YELLOW_0,EQREL_1,REWRITE1,ORDINAL2,FUNCT_1,STRUCT_0,YELLOW_3;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,RELAT_1,RELAT_2,RELSET_1,MCART_1,DOMAIN_1,FUNCT_2,BINOP_1,STRUCT_0,ORDERS_2,LATTICE3,YELLOW_0,WAYBEL_0,CKB18,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB43,CKB47,CKB50,CKB53,CKB56;
definitions LATTICE3,RELAT_2,TARSKI,WAYBEL_0,ORDERS_2,CKB18,CKB21,CKB56;
theorems FUNCT_1,FUNCT_2,FUNCT_5,LATTICE3,MCART_1,ORDERS_2,RELAT_1,RELAT_2,RELSET_1,TARSKI,WAYBEL_0,YELLOW_0,YELLOW_2,ZFMISC_1,XBOOLE_0,BINOP_1,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB18,CKB19,CKB21,CKB28,CKB29,CKB30,CKB37,CKB38,CKB39,CKB40,CKB41,CKB42,CKB44,CKB45,CKB46,CKB48,CKB49,CKB51,CKB52,CKB54,CKB55,CKB56,CKB62,CKB63,CKB64,CKB65,CKB66,CKB67,CKB68,CKB69,CKB70,CKB71,CKB72,CKB73;
schemes FUNCT_7,RELAT_1,CKB1,CKB14,CKB15,CKB16,CKB17;
registrations XBOOLE_0,SUBSET_1,RELSET_1,STRUCT_0,LATTICE3,YELLOW_0,ORDERS_2,WAYBEL_0,RELAT_1,CKB2,CKB3,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB57,CKB58,CKB59,CKB60,CKB61;
constructors DOMAIN_1,LATTICE3,ORDERS_3,WAYBEL_0,RELSET_1,CKB18,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB43,CKB47,CKB50,CKB53,CKB56;
requirements SUBSET,BOOLE;
begin
theorem
Th41: (for S1,S2 being  antisymmetric non empty RelStr holds (for D being (Subset of [: S1,S2 :]) holds (( ex_sup_of ( proj1 D ),S1 &  ex_sup_of ( proj2 D ),S2) iff  ex_sup_of D,[: S1,S2 :])))
proof
let S1 being  antisymmetric non empty RelStr,S2 being  antisymmetric non empty RelStr;
let D being (Subset of [: S1,S2 :]);
A1: (the carrier of [: S1,S2 :]) = [: (the carrier of S1),(the carrier of S2) :] by CKB21:def 1;
then A2: D c= [: ( proj1 D ),( proj2 D ) :] by CKB4:1;
hereby
assume that
A3:  ex_sup_of ( proj1 D ),S1
and
A4:  ex_sup_of ( proj2 D ),S2;
(ex a being (Element of [: S1,S2 :]) st (D is_<=_than a & (for b being (Element of [: S1,S2 :]) holds (D is_<=_than b implies a <= b))))
proof
consider x2 being (Element of S2) such that A5: ( proj2 D ) is_<=_than x2 and A6: (for b being (Element of S2) holds (( proj2 D ) is_<=_than b implies x2 <= b)) by A4,YELLOW_0:15;
consider x1 being (Element of S1) such that A7: ( proj1 D ) is_<=_than x1 and A8: (for b being (Element of S1) holds (( proj1 D ) is_<=_than b implies x1 <= b)) by A3,YELLOW_0:15;
take a = [ x1,x2 ];
thus D is_<=_than a
proof
let q being (Element of [: S1,S2 :]);
assume q in D;
then consider q1 being set,q2 being set such that A9: q1 in ( proj1 D ) and A10: q2 in ( proj2 D ) and A11: q = [ q1,q2 ] by A2,ZFMISC_1:def 2;
reconsider q2 as (Element of S2) by A10;
reconsider q1 as (Element of S1) by A9;
(q1 <= x1 & q2 <= x2) by A7,A5,A9,A10,LATTICE3:def 9;
hence thesis by A11,CKB28:1;
end;

let b being (Element of [: S1,S2 :]);
assume that
A12: D is_<=_than b;
A13: b = [ ( b `1 ),( b `2 ) ] by A1,MCART_1:21;
then ( proj2 D ) is_<=_than ( b `2 ) by A12,CKB64:1;
then A14: x2 <= ( b `2 ) by A6;
( proj1 D ) is_<=_than ( b `1 ) by A12,A13,CKB64:1;
then x1 <= ( b `1 ) by A8;
hence thesis by A13,A14,CKB28:1;
end;
hence  ex_sup_of D,[: S1,S2 :] by YELLOW_0:15;
end;
assume  ex_sup_of D,[: S1,S2 :];
then consider x being (Element of [: S1,S2 :]) such that A15: D is_<=_than x and A16: (for b being (Element of [: S1,S2 :]) holds (D is_<=_than b implies x <= b)) by YELLOW_0:15;
A17: x = [ ( x `1 ),( x `2 ) ] by A1,MCART_1:21;
then A18: ( proj1 D ) is_<=_than ( x `1 ) by A15,CKB64:1;
A19: ( proj2 D ) is_<=_than ( x `2 ) by A15,A17,CKB64:1;
(for b being (Element of S1) holds (( proj1 D ) is_<=_than b implies ( x `1 ) <= b))
proof
let b being (Element of S1);
assume ( proj1 D ) is_<=_than b;
then D is_<=_than [ b,( x `2 ) ] by A19,CKB64:1;
then x <= [ b,( x `2 ) ] by A16;
hence thesis by A17,CKB28:1;
end;
hence  ex_sup_of ( proj1 D ),S1 by A18,YELLOW_0:15;
(for b being (Element of S2) holds (( proj2 D ) is_<=_than b implies ( x `2 ) <= b))
proof
let b being (Element of S2);
assume ( proj2 D ) is_<=_than b;
then D is_<=_than [ ( x `1 ),b ] by A18,CKB64:1;
then x <= [ ( x `1 ),b ] by A16;
hence thesis by A17,CKB28:1;
end;
hence thesis by A19,YELLOW_0:15;
end;
