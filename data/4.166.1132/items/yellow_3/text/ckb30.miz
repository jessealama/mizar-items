begin
theorem
(for A,B being RelStr holds (for C being non empty RelStr holds (for f,g being (Function of [: A,B :],C) holds ((for x being (Element of A) holds (for y being (Element of B) holds ( f . (x,y) ) = ( g . (x,y) ))) implies f = g))))
proof
let A being RelStr,B being RelStr;
let C being non empty RelStr;
let f being (Function of [: A,B :],C),g being (Function of [: A,B :],C);
assume (for x being (Element of A) holds (for y being (Element of B) holds ( f . (x,y) ) = ( g . (x,y) )));
then A1: (for x,y being set holds ((x in (the carrier of A) & y in (the carrier of B)) implies ( f . (x,y) ) = ( g . (x,y) )));
(the carrier of [: A,B :]) = [: (the carrier of A),(the carrier of B) :] by CKB21:def 1;
hence thesis by A1,BINOP_1:1;
end;
