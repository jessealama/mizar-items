registrations XBOOLE_0,SUBSET_1,RELSET_1,STRUCT_0,LATTICE3,YELLOW_0,ORDERS_2,WAYBEL_0,RELAT_1,CKB2,CKB3,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB57,CKB58,CKB59,CKB60,CKB61;
constructors DOMAIN_1,LATTICE3,ORDERS_3,WAYBEL_0,RELSET_1,CKB18,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB43,CKB47,CKB50,CKB53,CKB56;
requirements SUBSET,BOOLE;
begin
theorem
Th40: (for S1,S2 being  antisymmetric non  empty RelStr holds (for D1 being non  empty (Subset of S1) holds (for D2 being non  empty (Subset of S2) holds (( ex_inf_of D1,S1 &  ex_inf_of D2,S2) iff  ex_inf_of [: D1,D2 :],[: S1,S2 :]))))
proof
let S1 being  antisymmetric non  empty RelStr;
let S2 being  antisymmetric non  empty RelStr;
let D1 being non  empty (Subset of S1);
let D2 being non  empty (Subset of S2);
hereby
assume that
A1:  ex_inf_of D1,S1
and
A2:  ex_inf_of D2,S2;
consider a2 being (Element of S2) such that A3: D2 is_>=_than a2 and A4: (for b being (Element of S2) holds (D2 is_>=_than b implies a2 >= b)) by A2,YELLOW_0:16;
consider a1 being (Element of S1) such that A5: D1 is_>=_than a1 and A6: (for b being (Element of S1) holds (D1 is_>=_than b implies a1 >= b)) by A1,YELLOW_0:16;
(ex a being (Element of [: S1,S2 :]) st ([: D1,D2 :] is_>=_than a & (for b being (Element of [: S1,S2 :]) holds ([: D1,D2 :] is_>=_than b implies a >= b))))
proof
take a = [ a1,a2 ];
thus [: D1,D2 :] is_>=_than a by A5,A3,CKB66:1;
let b being (Element of [: S1,S2 :]);
assume that
A7: [: D1,D2 :] is_>=_than b;
(the carrier of [: S1,S2 :]) = [: (the carrier of S1),(the carrier of S2) :] by CKB21:def 1;
then A8: b = [ ( b `1 ),( b `2 ) ] by MCART_1:21;
then D2 is_>=_than ( b `2 ) by A7,CKB65:1;
then A9: a2 >= ( b `2 ) by A4;
D1 is_>=_than ( b `1 ) by A7,A8,CKB65:1;
then a1 >= ( b `1 ) by A6;
hence thesis by A8,A9,CKB28:1;
end;
hence  ex_inf_of [: D1,D2 :],[: S1,S2 :] by YELLOW_0:16;
end;
assume  ex_inf_of [: D1,D2 :],[: S1,S2 :];
then consider x being (Element of [: S1,S2 :]) such that A10: [: D1,D2 :] is_>=_than x and A11: (for b being (Element of [: S1,S2 :]) holds ([: D1,D2 :] is_>=_than b implies x >= b)) by YELLOW_0:16;
(the carrier of [: S1,S2 :]) = [: (the carrier of S1),(the carrier of S2) :] by CKB21:def 1;
then A12: x = [ ( x `1 ),( x `2 ) ] by MCART_1:21;
then A13: D1 is_>=_than ( x `1 ) by A10,CKB65:1;
A14: D2 is_>=_than ( x `2 ) by A10,A12,CKB65:1;
(for b being (Element of S1) holds (D1 is_>=_than b implies ( x `1 ) >= b))
proof
let b being (Element of S1);
assume D1 is_>=_than b;
then x >= [ b,( x `2 ) ] by A11,A14,CKB66:1;
hence thesis by A12,CKB28:1;
end;
hence  ex_inf_of D1,S1 by A13,YELLOW_0:16;
(for b being (Element of S2) holds (D2 is_>=_than b implies ( x `2 ) >= b))
proof
let b being (Element of S2);
assume D2 is_>=_than b;
then x >= [ ( x `1 ),b ] by A11,A13,CKB66:1;
hence thesis by A12,CKB28:1;
end;
hence thesis by A14,YELLOW_0:16;
end;
