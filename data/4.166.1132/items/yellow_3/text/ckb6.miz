environ
vocabularies XBOOLE_0,SUBSET_1,TARSKI,ORDERS_2,WAYBEL_0,XXREAL_0,ZFMISC_1,RELAT_1,MCART_1,LATTICE3,RELAT_2,LATTICES,YELLOW_0,EQREL_1,REWRITE1,ORDINAL2,FUNCT_1,STRUCT_0,YELLOW_3;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,RELAT_1,RELAT_2,RELSET_1,MCART_1,DOMAIN_1,FUNCT_2,BINOP_1,STRUCT_0,ORDERS_2,LATTICE3,YELLOW_0,WAYBEL_0;
definitions LATTICE3,RELAT_2,TARSKI,WAYBEL_0,ORDERS_2;
theorems FUNCT_1,FUNCT_2,FUNCT_5,LATTICE3,MCART_1,ORDERS_2,RELAT_1,RELAT_2,RELSET_1,TARSKI,WAYBEL_0,YELLOW_0,YELLOW_2,ZFMISC_1,XBOOLE_0,BINOP_1,CKB4,CKB5;
schemes FUNCT_7,RELAT_1,CKB1;
registrations XBOOLE_0,SUBSET_1,RELSET_1,STRUCT_0,LATTICE3,YELLOW_0,ORDERS_2,WAYBEL_0,RELAT_1,CKB2,CKB3;
constructors DOMAIN_1,LATTICE3,ORDERS_3,WAYBEL_0,RELSET_1;
requirements SUBSET,BOOLE;
begin
theorem
(for L being  with_infima  transitive  antisymmetric RelStr holds (for a,b,c,d being (Element of L) holds ((a <= c & b <= d) implies ( a "/\" b ) <= ( c "/\" d ))))
proof
let L being  with_infima  transitive  antisymmetric RelStr;
let a being (Element of L),b being (Element of L),c being (Element of L),d being (Element of L);
assume that
A1: a <= c
and
A2: b <= d;
A3:  ex_inf_of { a,b },L by YELLOW_0:21;
then ( a "/\" b ) <= b by YELLOW_0:19;
then A4: ( a "/\" b ) <= d by A2,ORDERS_2:3;
( a "/\" b ) <= a by A3,YELLOW_0:19;
then ((ex x being (Element of L) st ((c >= x & d >= x) & (for z being (Element of L) holds ((c >= z & d >= z) implies x >= z)))) & ( a "/\" b ) <= c) by A1,LATTICE3:def 11,ORDERS_2:3;
hence thesis by A4,LATTICE3:def 14;
end;
