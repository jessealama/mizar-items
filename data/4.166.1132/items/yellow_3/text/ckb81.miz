schemes FUNCT_7,RELAT_1,CKB1,CKB14,CKB15,CKB16,CKB17;
registrations XBOOLE_0,SUBSET_1,RELSET_1,STRUCT_0,LATTICE3,YELLOW_0,ORDERS_2,WAYBEL_0,RELAT_1,CKB2,CKB3,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB57,CKB58,CKB59,CKB60,CKB61,CKB78;
constructors DOMAIN_1,LATTICE3,ORDERS_3,WAYBEL_0,RELSET_1,CKB18,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB43,CKB47,CKB50,CKB53,CKB56;
requirements SUBSET,BOOLE;
begin
theorem
(for L1,L2 being  antisymmetric non empty RelStr holds (for D being non empty (Subset of [: L1,L2 :]) holds (([: L1,L2 :] is  complete or  ex_inf_of D,[: L1,L2 :]) implies ( inf D ) = [ ( inf ( proj1 D ) ),( inf ( proj2 D ) ) ])))
proof
let L1 being  antisymmetric non empty RelStr,L2 being  antisymmetric non empty RelStr;
let D being non empty (Subset of [: L1,L2 :]);
reconsider C1 = (the carrier of L1),C2 = (the carrier of L2) as non empty set;
(the carrier of [: L1,L2 :]) = [: C1,C2 :] by CKB21:def 1;
then consider d1 being set,d2 being set such that A1: d1 in C1 and A2: d2 in C2 and A3: ( inf D ) = [ d1,d2 ] by ZFMISC_1:def 2;
reconsider d1 as (Element of L1) by A1;
reconsider D9 = D as non empty (Subset of [: C1,C2 :]) by CKB21:def 1;
( proj1 D9 ) is non empty;
then reconsider D1 = ( proj1 D ) as non empty (Subset of L1);
( proj2 D9 ) is non empty;
then reconsider D2 = ( proj2 D ) as non empty (Subset of L2);
A4: D9 c= [: D1,D2 :] by CKB4:1;
reconsider d2 as (Element of L2) by A2;
assume ([: L1,L2 :] is  complete or  ex_inf_of D,[: L1,L2 :]);
then A5:  ex_inf_of D,[: L1,L2 :] by YELLOW_0:17;
then A6:  ex_inf_of D2,L2 by CKB75:1;
A7:  ex_inf_of D1,L1 by A5,CKB75:1;
then  ex_inf_of [: D1,D2 :],[: L1,L2 :] by A6,CKB73:1;
then ( inf D ) >= ( inf [: D1,D2 :] ) by A5,A4,YELLOW_0:35;
then A8: ( inf D ) >= [ ( inf ( proj1 D ) ),( inf ( proj2 D ) ) ] by A7,A6,CKB77:1;
D2 is_>=_than d2
proof
let b being (Element of L2);
assume b in D2;
then consider x being set such that A9: [ x,b ] in D by RELAT_1:def 5;
reconsider x as (Element of D1) by A9,FUNCT_5:2;
reconsider x as (Element of L1);
D is_>=_than [ d1,d2 ] by A5,A3,YELLOW_0:def 10;
then [ x,b ] >= [ d1,d2 ] by A9,LATTICE3:def 8;
hence thesis by CKB28:1;
end;
then A10: ( inf D2 ) >= d2 by A6,YELLOW_0:def 10;
D1 is_>=_than d1
proof
let b being (Element of L1);
assume b in D1;
then consider x being set such that A11: [ b,x ] in D by RELAT_1:def 4;
reconsider x as (Element of D2) by A11,FUNCT_5:2;
reconsider x as (Element of L2);
D is_>=_than [ d1,d2 ] by A5,A3,YELLOW_0:def 10;
then [ b,x ] >= [ d1,d2 ] by A11,LATTICE3:def 8;
hence thesis by CKB28:1;
end;
then ( inf D1 ) >= d1 by A7,YELLOW_0:def 10;
then [ ( inf D1 ),( inf D2 ) ] >= ( inf D ) by A3,A10,CKB28:1;
hence thesis by A8,ORDERS_2:2;
end;
