begin
theorem
Th36: (for S1,S2 being  antisymmetric non  empty RelStr holds (for D1 being (Subset of S1) holds (for D2 being (Subset of S2) holds (for x being (Element of S1) holds (for y being (Element of S2) holds ((( ex_inf_of D1,S1 &  ex_inf_of D2,S2) & (for b being (Element of [: S1,S2 :]) holds (b is_<=_than [: D1,D2 :] implies [ x,y ] >= b))) implies ((for c being (Element of S1) holds (c is_<=_than D1 implies x >= c)) & (for d being (Element of S2) holds (d is_<=_than D2 implies y >= d)))))))))
proof
let S1 being  antisymmetric non  empty RelStr;
let S2 being  antisymmetric non  empty RelStr;
let D1 being (Subset of S1);
let D2 being (Subset of S2);
let x being (Element of S1);
let y being (Element of S2);
assume that
A1:  ex_inf_of D1,S1
and
A2:  ex_inf_of D2,S2
and
A3: (for b being (Element of [: S1,S2 :]) holds (b is_<=_than [: D1,D2 :] implies [ x,y ] >= b));
thus (for c being (Element of S1) holds (c is_<=_than D1 implies x >= c))
proof
A4: ( inf D2 ) is_<=_than D2 by A2,YELLOW_0:31;
let b being (Element of S1);
assume b is_<=_than D1;
then [ x,y ] >= [ b,( inf D2 ) ] by A3,A4,CKB66:1;
hence thesis by CKB28:1;
end;

A5: ( inf D1 ) is_<=_than D1 by A1,YELLOW_0:31;
let b being (Element of S2);
assume b is_<=_than D2;
then [ x,y ] >= [ ( inf D1 ),b ] by A3,A5,CKB66:1;
hence thesis by CKB28:1;
end;
