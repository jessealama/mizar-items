begin
theorem
(for L being  complete  reflexive  antisymmetric non  empty RelStr holds (for D being (Subset of L) holds (for x being (Element of L) holds (x in D implies ( ( inf D ) "\/" x ) = x))))
proof
let L being  complete  reflexive  antisymmetric non  empty RelStr;
let D being (Subset of L);
let x being (Element of L);
assume that
A1: x in D;
D is_>=_than ( inf D ) by YELLOW_0:33;
then ( inf D ) <= x by A1,LATTICE3:def 8;
hence thesis by YELLOW_0:24;
end;
