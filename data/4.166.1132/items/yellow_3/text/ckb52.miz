begin
theorem
(for S1,S2 being non empty  reflexive RelStr holds (for D being non empty  upper (Subset of [: S1,S2 :]) holds (( proj1 D ) is  upper & ( proj2 D ) is  upper)))
proof
let S1 being non empty  reflexive RelStr,S2 being non empty  reflexive RelStr;
let D being non empty  upper (Subset of [: S1,S2 :]);
set D1 = ( proj1 D );
set D2 = ( proj2 D );
reconsider d1 = D1 as non empty (Subset of S1) by CKB45:1;
(the carrier of [: S1,S2 :]) = [: (the carrier of S1),(the carrier of S2) :] by CKB21:def 1;
then A1: D c= [: D1,D2 :] by CKB4:1;
thus D1 is  upper
proof
reconsider d2 = D2 as non empty (Subset of S2) by CKB45:1;
let x being (Element of S1),y being (Element of S1);
assume that
A2: x in D1
and
A3: x <= y;
consider q1 being set such that A4: [ x,q1 ] in D by A2,RELAT_1:def 4;
reconsider q1 as (Element of d2) by A1,A4,ZFMISC_1:87;
q1 <= q1;
then [ x,q1 ] <= [ y,q1 ] by A3,CKB28:1;
then [ y,q1 ] in D by A4,WAYBEL_0:def 20;
hence thesis by A1,ZFMISC_1:87;
end;

let x being (Element of S2),y being (Element of S2);
assume that
A5: x in D2
and
A6: x <= y;
consider q1 being set such that A7: [ q1,x ] in D by A5,RELAT_1:def 5;
reconsider q1 as (Element of d1) by A1,A7,ZFMISC_1:87;
q1 <= q1;
then [ q1,x ] <= [ q1,y ] by A6,CKB28:1;
then [ q1,y ] in D by A7,WAYBEL_0:def 20;
hence thesis by A1,ZFMISC_1:87;
end;
