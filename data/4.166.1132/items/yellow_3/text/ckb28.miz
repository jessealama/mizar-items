begin
theorem
Th11: (for S1,S2 being non  empty RelStr holds (for a,c being (Element of S1) holds (for b,d being (Element of S2) holds ((a <= c & b <= d) iff [ a,b ] <= [ c,d ]))))
proof
let S1 being non  empty RelStr;
let S2 being non  empty RelStr;
let a being (Element of S1);
let c being (Element of S1);
let b being (Element of S2);
let d being (Element of S2);
set I1 = (the InternalRel of S1);
set I2 = (the InternalRel of S2);
set x = [ [ a,b ],[ c,d ] ];
A1: (( ( x `1 ) `1 ) = a & ( ( x `2 ) `1 ) = c) by CKB5:1;
A2: (( ( x `1 ) `2 ) = b & ( ( x `2 ) `2 ) = d) by CKB5:1;
A3: (( x `1 ) = [ a,b ] & ( x `2 ) = [ c,d ]) by MCART_1:7;
thus ((a <= c & b <= d) implies [ a,b ] <= [ c,d ])
proof
assume (a <= c & b <= d);
then ([ ( ( x `1 ) `1 ),( ( x `2 ) `1 ) ] in I1 & [ ( ( x `1 ) `2 ),( ( x `2 ) `2 ) ] in I2) by A1,A2,ORDERS_2:def 5;
then x in [" I1,I2 "] by A3,CKB19:1;
hence [ [ a,b ],[ c,d ] ] in (the InternalRel of [: S1,S2 :]) by CKB21:def 1;
end;

assume [ a,b ] <= [ c,d ];
then x in (the InternalRel of [: S1,S2 :]) by ORDERS_2:def 5;
then x in [" I1,I2 "] by CKB21:def 1;
hence ([ a,c ] in (the InternalRel of S1) & [ b,d ] in (the InternalRel of S2)) by A1,A2,CKB19:1;
end;
