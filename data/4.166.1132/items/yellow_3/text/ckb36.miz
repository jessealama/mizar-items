environ
vocabularies XBOOLE_0,SUBSET_1,TARSKI,ORDERS_2,WAYBEL_0,XXREAL_0,ZFMISC_1,RELAT_1,MCART_1,LATTICE3,RELAT_2,LATTICES,YELLOW_0,EQREL_1,REWRITE1,ORDINAL2,FUNCT_1,STRUCT_0,YELLOW_3;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,RELAT_1,RELAT_2,RELSET_1,MCART_1,DOMAIN_1,FUNCT_2,BINOP_1,STRUCT_0,ORDERS_2,LATTICE3,YELLOW_0,WAYBEL_0,CKB18,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27;
definitions LATTICE3,RELAT_2,TARSKI,WAYBEL_0,ORDERS_2,CKB18,CKB21;
theorems FUNCT_1,FUNCT_2,FUNCT_5,LATTICE3,MCART_1,ORDERS_2,RELAT_1,RELAT_2,RELSET_1,TARSKI,WAYBEL_0,YELLOW_0,YELLOW_2,ZFMISC_1,XBOOLE_0,BINOP_1,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB18,CKB19,CKB21,CKB28,CKB29,CKB30;
schemes FUNCT_7,RELAT_1,CKB1,CKB14,CKB15,CKB16,CKB17;
registrations XBOOLE_0,SUBSET_1,RELSET_1,STRUCT_0,LATTICE3,YELLOW_0,ORDERS_2,WAYBEL_0,RELAT_1,CKB2,CKB3,CKB31,CKB32,CKB33,CKB34,CKB35;
constructors DOMAIN_1,LATTICE3,ORDERS_3,WAYBEL_0,RELSET_1,CKB18,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27;
requirements SUBSET,BOOLE;
begin
registration
let X being  with_infima RelStr;
let Y being  with_infima RelStr;
cluster [: X,Y :] ->  with_infima;
coherence
proof
set IT = [: X,Y :];
let x being (Element of IT);
let y being (Element of IT);
consider zx being (Element of X) such that A1: (( x `1 ) >= zx & ( y `1 ) >= zx) and A2: (for z9 being (Element of X) holds ((( x `1 ) >= z9 & ( y `1 ) >= z9) implies zx >= z9)) by LATTICE3:def 11;
consider zy being (Element of Y) such that A3: (( x `2 ) >= zy & ( y `2 ) >= zy) and A4: (for z9 being (Element of Y) holds ((( x `2 ) >= z9 & ( y `2 ) >= z9) implies zy >= z9)) by LATTICE3:def 11;
A5: (the carrier of [: X,Y :]) = [: (the carrier of X),(the carrier of Y) :] by CKB21:def 1;
then A6: ((ex a,b being set st ((a in (the carrier of X) & b in (the carrier of Y)) & x = [ a,b ])) & (ex c,d being set st ((c in (the carrier of X) & d in (the carrier of Y)) & y = [ c,d ]))) by ZFMISC_1:def 2;
take z = [ zx,zy ];
([ ( x `1 ),( x `2 ) ] >= [ zx,zy ] & [ ( y `1 ),( y `2 ) ] >= [ zx,zy ]) by A1,A3,CKB28:1;
hence (x >= z & y >= z) by A6,MCART_1:8;
let z9 being (Element of IT);
A7: (ex a,b being set st ((a in (the carrier of X) & b in (the carrier of Y)) & z9 = [ a,b ])) by A5,ZFMISC_1:def 2;
assume A8: (x >= z9 & y >= z9);
then (( x `2 ) >= ( z9 `2 ) & ( y `2 ) >= ( z9 `2 )) by CKB29:1;
then A9: zy >= ( z9 `2 ) by A4;
(( x `1 ) >= ( z9 `1 ) & ( y `1 ) >= ( z9 `1 )) by A8,CKB29:1;
then zx >= ( z9 `1 ) by A2;
then [ zx,zy ] >= [ ( z9 `1 ),( z9 `2 ) ] by A9,CKB28:1;
hence thesis by A7,MCART_1:8;
end;
end;
