begin
theorem
(for L being RelStr holds (for X,Y being (Subset of L) holds (X c= Y implies ( downarrow X ) c= ( downarrow Y ))))
proof
let L being RelStr;
let X being (Subset of L);
let Y being (Subset of L);
assume that
A1: X c= Y;
let q being set;
assume A2: q in ( downarrow X );
then reconsider x = q as (Element of L);
(ex z being (Element of L) st (x <= z & z in X)) by A2,WAYBEL_0:def 15;
hence thesis by A1,WAYBEL_0:def 15;
end;
