begin
theorem
Th44: (for S1,S2 being  antisymmetric non  empty RelStr holds (for D1 being non  empty (Subset of S1) holds (for D2 being non  empty (Subset of S2) holds (( ex_inf_of D1,S1 &  ex_inf_of D2,S2) implies ( inf [: D1,D2 :] ) = [ ( inf D1 ),( inf D2 ) ]))))
proof
let S1 being  antisymmetric non  empty RelStr;
let S2 being  antisymmetric non  empty RelStr;
let D1 being non  empty (Subset of S1);
let D2 being non  empty (Subset of S2);
assume that
A1: ( ex_inf_of D1,S1 &  ex_inf_of D2,S2);
set s = ( inf [: D1,D2 :] );
s is (Element of [: (the carrier of S1),(the carrier of S2) :]) by CKB21:def 1;
then consider s1,s2 being set such that A2: s1 in (the carrier of S1) and A3: s2 in (the carrier of S2) and A4: s = [ s1,s2 ] by ZFMISC_1:def 2;
reconsider s2 as (Element of S2) by A3;
reconsider s1 as (Element of S1) by A2;
A5:  ex_inf_of [: D1,D2 :],[: S1,S2 :] by A1,CKB73:1;
then A6: [ s1,s2 ] is_<=_than [: D1,D2 :] by A4,YELLOW_0:31;
then A7: s1 is_<=_than D1 by CKB65:1;
A8: (for b being (Element of [: S1,S2 :]) holds (b is_<=_than [: D1,D2 :] implies [ s1,s2 ] >= b)) by A4,A5,YELLOW_0:31;
then (for b being (Element of S1) holds (b is_<=_than D1 implies s1 >= b)) by A1,CKB69:1;
then A9: s1 = ( inf D1 ) by A7,YELLOW_0:31;
A10: s2 is_<=_than D2 by A6,CKB65:1;
(for b being (Element of S2) holds (b is_<=_than D2 implies s2 >= b)) by A1,A8,CKB69:1;
hence thesis by A4,A9,A10,YELLOW_0:31;
end;
