requirements SUBSET,BOOLE;
begin
theorem
Th31: (for S1,S2 being non empty RelStr holds (for D being (Subset of [: S1,S2 :]) holds (for x being (Element of S1) holds (for y being (Element of S2) holds ([ x,y ] is_>=_than D iff (x is_>=_than ( proj1 D ) & y is_>=_than ( proj2 D )))))))
proof
let S1 being non empty RelStr,S2 being non empty RelStr;
let D being (Subset of [: S1,S2 :]);
let x being (Element of S1);
let y being (Element of S2);
set D1 = ( proj1 D );
set D2 = ( proj2 D );
hereby
assume A1: [ x,y ] is_>=_than D;
thus x is_>=_than D1
proof
let q being (Element of S1);
assume q in D1;
then consider z being set such that A2: [ q,z ] in D by RELAT_1:def 4;
reconsider d2 = D2 as non empty (Subset of S2) by A2,FUNCT_5:2;
reconsider z as (Element of d2) by A2,FUNCT_5:2;
[ x,y ] >= [ q,z ] by A1,A2,LATTICE3:def 9;
hence thesis by CKB28:1;
end;

thus y is_>=_than D2
proof
let q being (Element of S2);
assume q in D2;
then consider z being set such that A3: [ z,q ] in D by RELAT_1:def 5;
reconsider d1 = D1 as non empty (Subset of S1) by A3,FUNCT_5:2;
reconsider z as (Element of d1) by A3,FUNCT_5:2;
[ x,y ] >= [ z,q ] by A1,A3,LATTICE3:def 9;
hence thesis by CKB28:1;
end;

end;
assume (x is_>=_than ( proj1 D ) & y is_>=_than ( proj2 D ));
then A4: [ x,y ] is_>=_than [: D1,D2 :] by CKB63:1;
(the carrier of [: S1,S2 :]) = [: (the carrier of S1),(the carrier of S2) :] by CKB21:def 1;
then D c= [: D1,D2 :] by CKB4:1;
hence thesis by A4,YELLOW_0:9;
end;
