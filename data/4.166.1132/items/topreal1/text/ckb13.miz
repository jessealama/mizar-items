begin
reserve p for (Point of ( TOP-REAL 2 ));
reserve T for TopSpace;
theorem
Lm2: (for T being TopSpace holds (T is  T_2 iff ( the TopStruct of T ) is  T_2))
proof
let T being TopSpace;
thus (T is  T_2 implies ( the TopStruct of T ) is  T_2)
proof
assume A1: T is  T_2;
let p being (Point of ( the TopStruct of T ));
let q being (Point of ( the TopStruct of T ));
assume that
A2: p <> q;
reconsider pp = p,qq = q as (Point of T);
consider G1,G2 being (Subset of T) such that A3: G1 is  open and A4: G2 is  open and A5: pp in G1 and A6: qq in G2 and A7: G1 misses G2 by A1,A2,PRE_TOPC:def 10;
reconsider H1 = G1,H2 = G2 as (Subset of ( the TopStruct of T ));
take H1;
take H2;
thus (H1 is  open & H2 is  open) by A3,A4,PRE_TOPC:30;
thus ((p in H1 & q in H2) & H1 misses H2) by A5,A6,A7;
end;

assume A8: ( the TopStruct of T ) is  T_2;
let p being (Point of T);
let q being (Point of T);
assume that
A9: p <> q;
reconsider pp = p,qq = q as (Point of ( the TopStruct of T ));
consider G1,G2 being (Subset of ( the TopStruct of T )) such that A10: G1 is  open and A11: G2 is  open and A12: pp in G1 and A13: qq in G2 and A14: G1 misses G2 by A8,A9,PRE_TOPC:def 10;
reconsider H1 = G1,H2 = G2 as (Subset of T);
take H1;
take H2;
thus (H1 is  open & H2 is  open) by A10,A11,PRE_TOPC:30;
thus ((p in H1 & q in H2) & H1 misses H2) by A12,A13,A14;
end;
