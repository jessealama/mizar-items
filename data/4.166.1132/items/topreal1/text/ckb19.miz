requirements REAL,NUMERALS,BOOLE,SUBSET,ARITHM;
begin
reserve n for Nat;
theorem
(for p1,p2,q1 being (Point of ( TOP-REAL n )) holds (((p1 <> p2 or p2 <> q1) & ( ( LSeg (p1,p2) ) /\ ( LSeg (p2,q1) ) ) = { p2 }) implies ( ( LSeg (p1,p2) ) \/ ( LSeg (p2,q1) ) ) is_an_arc_of p1,q1))
proof
let p1 being (Point of ( TOP-REAL n )),p2 being (Point of ( TOP-REAL n )),q1 being (Point of ( TOP-REAL n ));
assume that
A1: (p1 <> p2 or p2 <> q1)
and
A2: ( ( LSeg (p1,p2) ) /\ ( LSeg (p2,q1) ) ) = { p2 };
per cases  by A1;
suppose p1 <> p2;

hence thesis by A2,CKB15:1,CKB17:1;
end;
suppose p2 <> q1;

hence thesis by A2,CKB15:1,CKB18:1;
end;
end;
