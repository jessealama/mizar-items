begin
scheme TRSubsetUniq { n() -> Nat,P[set] } : (for A,B being (Subset of ( TOP-REAL n() )) holds (((for p being (Point of ( TOP-REAL n() )) holds (p in A iff P[ p ])) & (for p being (Point of ( TOP-REAL n() )) holds (p in B iff P[ p ]))) implies A = B))
proof
let A being (Subset of ( TOP-REAL n() )),B being (Subset of ( TOP-REAL n() ));
assume that
A1: (for p being (Point of ( TOP-REAL n() )) holds (p in A iff P[ p ]))
and
A2: (for p being (Point of ( TOP-REAL n() )) holds (p in B iff P[ p ]));
hereby
let x being set;
assume A3: x in A;
then P[ x ] by A1;
hence x in B by A2,A3;
end;
let x being set;
assume A4: x in B;
then P[ x ] by A2;
hence thesis by A1,A4;
end;
