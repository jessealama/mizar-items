begin
theorem
Th11: (for R,S,T being non empty  reflexive RelStr holds (for f being (Function of [: R,S :],T) holds (for a being (Element of R) holds (for b being (Element of S) holds (f is  antitone implies (( Proj (f,a) ) is  antitone & ( Proj (f,b) ) is  antitone))))))
proof
let R being non empty  reflexive RelStr,S being non empty  reflexive RelStr,T being non empty  reflexive RelStr;
let f being (Function of [: R,S :],T);
let a being (Element of R);
let b being (Element of S);
reconsider a9 = a as (Element of R);
set g = ( Proj (f,b) );
set h = ( Proj (f,a) );
assume A1: f is  antitone;
A2:now
reconsider b9 = b as (Element of S);
let x being (Element of R),y being (Element of R);
A3: b9 <= b9;
A4: (( g . x ) = ( f . (x,b) ) & ( g . y ) = ( f . (y,b) )) by CKB14:1;
assume x <= y;
then [ x,b9 ] <= [ y,b9 ] by A3,YELLOW_3:11;
hence ( g . x ) >= ( g . y ) by A1,A4,WAYBEL_0:def 5;
end;
now
let x being (Element of S),y being (Element of S);
A5: a9 <= a9;
A6: (( h . x ) = ( f . (a,x) ) & ( h . y ) = ( f . (a,y) )) by CKB12:1;
assume x <= y;
then [ a9,x ] <= [ a9,y ] by A5,YELLOW_3:11;
hence ( h . x ) >= ( h . y ) by A1,A6,WAYBEL_0:def 5;
end;
hence thesis by A2,WAYBEL_9:def 1;
end;
