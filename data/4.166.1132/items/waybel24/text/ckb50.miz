theorem
Lm1: (for S being non empty RelStr holds (for D being non empty (Subset of S) holds D = { i where i is (Element of S): i in D }))
proof
let S being non empty RelStr;
let D being non empty (Subset of S);
thus D c= { i where i is (Element of S): i in D }
proof
let a being set;
assume a in D;
hence thesis;
end;

thus { i where i is (Element of S): i in D } c= D
proof
let a being set;
assume a in { i where i is (Element of S): i in D };
then (ex j being (Element of S) st (j = a & j in D));
hence thesis;
end;

end;
