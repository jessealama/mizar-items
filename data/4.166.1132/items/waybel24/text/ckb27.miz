begin
theorem
(for R,S,T being LATTICE holds (for f being (Function of [: R,S :],T) holds (for a being (Element of R) holds (for b being (Element of S) holds (f is  directed-sups-preserving implies (( Proj (f,a) ) is  directed-sups-preserving & ( Proj (f,b) ) is  directed-sups-preserving))))))
proof
let R being LATTICE,S being LATTICE,T being LATTICE;
let f being (Function of [: R,S :],T);
let a being (Element of R);
let b being (Element of S);
assume A1: f is  directed-sups-preserving;
A2: (for X being (Subset of S) holds (X is non empty  directed implies ( Proj (f,a) ) preserves_sup_of X))
proof
reconsider Y9 = { a } as non empty  directed (Subset of R) by WAYBEL_0:5;
let X being (Subset of S);
assume X is non empty  directed;
then reconsider X9 = X as non empty  directed (Subset of S);
( Proj (f,a) ) preserves_sup_of X
proof
A3: ( sup Y9 ) = a by YELLOW_0:39;
A4: f preserves_sup_of [: Y9,X9 :] by A1,WAYBEL_0:def 37;
A5: ( ( Proj (f,a) ) .: X ) = ( f .: [: Y9,X9 :] ) by CKB26:1;
A6:  ex_sup_of Y9,R by YELLOW_0:38;
assume A7:  ex_sup_of X,S;
then A8:  ex_sup_of [: Y9,X9 :],[: R,S :] by A6,YELLOW_3:39;
( sup ( ( Proj (f,a) ) .: X ) ) = ( sup ( f .: [: Y9,X9 :] ) ) by CKB26:1
.= ( f . ( sup [: Y9,X9 :] ) ) by A8,A4,WAYBEL_0:def 31
.= ( f . (( sup Y9 ),( sup X9 )) ) by A7,A6,YELLOW_3:43
.= ( ( Proj (f,a) ) . ( sup X ) ) by A3,CKB12:1;
hence thesis by A8,A4,A5,WAYBEL_0:def 31;
end;
hence thesis;
end;
(for X being (Subset of R) holds (X is non empty  directed implies ( Proj (f,b) ) preserves_sup_of X))
proof
reconsider Y9 = { b } as non empty  directed (Subset of S) by WAYBEL_0:5;
let X being (Subset of R);
assume X is non empty  directed;
then reconsider X9 = X as non empty  directed (Subset of R);
( Proj (f,b) ) preserves_sup_of X
proof
A9: ( sup Y9 ) = b by YELLOW_0:39;
A10: f preserves_sup_of [: X9,Y9 :] by A1,WAYBEL_0:def 37;
A11: ( ( Proj (f,b) ) .: X ) = ( f .: [: X9,Y9 :] ) by CKB25:1;
A12:  ex_sup_of Y9,S by YELLOW_0:38;
assume A13:  ex_sup_of X,R;
then A14:  ex_sup_of [: X9,Y9 :],[: R,S :] by A12,YELLOW_3:39;
( sup ( ( Proj (f,b) ) .: X ) ) = ( sup ( f .: [: X9,Y9 :] ) ) by CKB25:1
.= ( f . ( sup [: X9,Y9 :] ) ) by A14,A10,WAYBEL_0:def 31
.= ( f . (( sup X9 ),( sup Y9 )) ) by A13,A12,YELLOW_3:43
.= ( ( Proj (f,b) ) . ( sup X ) ) by A9,CKB14:1;
hence thesis by A14,A10,A11,WAYBEL_0:def 31;
end;
hence thesis;
end;
hence thesis by A2,WAYBEL_0:def 37;
end;
