begin
theorem
(for X,Y,x,y being set holds (( X --> x ) tolerates ( Y --> y ) iff (x = y or X misses Y)))
proof
let X being set,Y being set,x being set,y being set;
set f = ( X --> x );
set g = ( Y --> y );
A1: ( dom g ) = Y by CKB19:1;
A2: ( dom f ) = X by CKB19:1;
thus (f tolerates g implies (x = y or X misses Y))
proof
assume that
A3: (for z being set holds (z in ( ( dom f ) /\ ( dom g ) ) implies ( f . z ) = ( g . z )))
and
A4: x <> y;
assume X meets Y;
then consider z being set such that A5: z in X and A6: z in Y by XBOOLE_0:3;
A7: ( f . z ) = x by A5,CKB10:1;
A8: ( g . z ) = y by A6,CKB10:1;
z in ( X /\ Y ) by A5,A6,XBOOLE_0:def 4;
hence thesis by A2,A1,A3,A4,A7,A8;
end;

assume A9: (x = y or X misses Y);
let z being set;
assume A10: z in ( ( dom f ) /\ ( dom g ) );
then A11: z in Y;
A12: z in X by A2,A10,XBOOLE_0:def 4;
then ( f . z ) = x by CKB10:1;
hence thesis by A9,A12,A11,CKB10:1,XBOOLE_0:3;
end;
