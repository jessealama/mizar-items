theorem
(for X,Y,Z being non empty set holds (for f being (Function of X,[: Y,Z :]) holds ( rng ( f ~ ) ) = ( ( rng f ) ~ )))
proof
let X being non empty set,Y being non empty set,Z being non empty set;
let f being (Function of X,[: Y,Z :]);
let x being set,y being set;
thus ([ x,y ] in ( rng ( f ~ ) ) implies [ x,y ] in ( ( rng f ) ~ ))
proof
assume [ x,y ] in ( rng ( f ~ ) );
then consider z being set such that A1: z in ( dom ( f ~ ) ) and A2: [ x,y ] = ( ( f ~ ) . z ) by FUNCT_1:def 3;
A3: z in ( dom f ) by A1,CKB2:def 1;
( f . z ) = ( ( ( f ~ ) ~ ) . z )
.= [ y,x ] by A1,A2,CKB2:def 1;
then [ y,x ] in ( rng f ) by A3,FUNCT_1:def 3;
hence thesis by RELAT_1:def 7;
end;

assume [ x,y ] in ( ( rng f ) ~ );
then [ y,x ] in ( rng f ) by RELAT_1:def 7;
then consider z being set such that A4: (z in ( dom f ) & [ y,x ] = ( f . z )) by FUNCT_1:def 3;
(z in ( dom ( f ~ ) ) & ( ( f ~ ) . z ) = [ x,y ]) by A4,CKB2:def 1;
hence thesis by FUNCT_1:def 3;
end;
