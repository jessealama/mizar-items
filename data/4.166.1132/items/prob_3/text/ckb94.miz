theorem
Th78: (for Z being (Field_Subset of Omega) holds ( monotoneclass Z ) is (Field_Subset of Omega))
proof
let Z being (Field_Subset of Omega);
A1: Z c= ( monotoneclass Z ) by CKB93:def 1;
then reconsider Z1 = ( monotoneclass Z ) as non  empty (Subset-Family of Omega);
A2: (for y holds (for Y holds (Y = { x where x is (Element of Z1): ((( y \ x ) in Z1 & ( x \ y ) in Z1) & ( x /\ y ) in Z1) } implies (for z holds (z in Y implies (((z in Z1 & ( y \ z ) in Z1) & ( z \ y ) in Z1) & ( z /\ y ) in Z1))))))
proof
let y;
let Y;
assume A3: Y = { x where x is (Element of Z1): ((( y \ x ) in Z1 & ( x \ y ) in Z1) & ( x /\ y ) in Z1) };
thus (for z holds (z in Y implies (((z in Z1 & ( y \ z ) in Z1) & ( z \ y ) in Z1) & ( z /\ y ) in Z1)))
proof
let z;
assume z in Y;
then (ex z1 being (Element of Z1) st (((z = z1 & ( y \ z1 ) in Z1) & ( z1 \ y ) in Z1) & ( z1 /\ y ) in Z1)) by A3;
hence thesis;
end;

end;
A4: (for y being (Element of Z1) holds (for Y holds (Y = { x where x is (Element of Z1): ((( y \ x ) in Z1 & ( x \ y ) in Z1) & ( x /\ y ) in Z1) } implies Y is (MonotoneClass of Omega))))
proof
let y being (Element of Z1);
let Y;
assume A5: Y = { x where x is (Element of Z1): ((( y \ x ) in Z1 & ( x \ y ) in Z1) & ( x /\ y ) in Z1) };
A6: (for A1 being (SetSequence of Omega) holds ((A1 is  monotone & ( rng A1 ) c= Y) implies ( lim A1 ) in Y))
proof
let A1 being (SetSequence of Omega);
assume that
A7: A1 is  monotone
and
A8: ( rng A1 ) c= Y;
A9: A1 is  convergent by A7,SETLIM_1:65;
(for n holds ( A1 . n ) in Z1)
proof
let n;
( A1 . n ) in ( rng A1 ) by NAT_1:51;
hence thesis by A2,A5,A8;
end;
then ( rng A1 ) c= Z1 by NAT_1:52;
then A10: ( lim A1 ) is (Element of Z1) by A7,CKB90:1;
(for n holds ( ( A1 (\) y ) . n ) in Z1)
proof
let n being Nat;
( A1 . n ) in ( rng A1 ) by NAT_1:51;
then (n in ( NAT ) & ( ( A1 . n ) \ y ) in Z1) by A2,A5,A8,ORDINAL1:def 12;
hence thesis by SETLIM_2:def 8;
end;
then A11: ( rng ( A1 (\) y ) ) c= Z1 by NAT_1:52;
(for n holds ( ( y (/\) A1 ) . n ) in Z1)
proof
let n;
( A1 . n ) in ( rng A1 ) by NAT_1:51;
then (n in ( NAT ) & ( y /\ ( A1 . n ) ) in Z1) by A2,A5,A8,ORDINAL1:def 12;
hence thesis by SETLIM_2:def 5;
end;
then A12: ( rng ( y (/\) A1 ) ) c= Z1 by NAT_1:52;
( y (/\) A1 ) is  monotone by A7,SETLIM_2:23;
then ( lim ( y (/\) A1 ) ) in Z1 by A12,CKB90:1;
then A13: ( y /\ ( lim A1 ) ) in Z1 by A9,SETLIM_2:92;
(for n holds ( ( y (\) A1 ) . n ) in Z1)
proof
let n;
( A1 . n ) in ( rng A1 ) by NAT_1:51;
then (n in ( NAT ) & ( y \ ( A1 . n ) ) in Z1) by A2,A5,A8,ORDINAL1:def 12;
hence thesis by SETLIM_2:def 7;
end;
then A14: ( rng ( y (\) A1 ) ) c= Z1 by NAT_1:52;
( y (\) A1 ) is  monotone by A7,SETLIM_2:29;
then ( lim ( y (\) A1 ) ) in Z1 by A14,CKB90:1;
then A15: ( y \ ( lim A1 ) ) in Z1 by A9,SETLIM_2:94;
( A1 (\) y ) is  monotone by A7,SETLIM_2:32;
then ( lim ( A1 (\) y ) ) in Z1 by A11,CKB90:1;
then ( ( lim A1 ) \ y ) in Z1 by A9,SETLIM_2:95;
hence thesis by A5,A10,A15,A13;
end;
(z in Y implies z in Z1) by A2,A5;
then Y c= Z1 by TARSKI:def 3;
hence thesis by A6,CKB90:1,XBOOLE_1:1;
end;
A16: (for y being (Element of Z) holds (for Y holds (Y = { x where x is (Element of Z1): ((( y \ x ) in Z1 & ( x \ y ) in Z1) & ( x /\ y ) in Z1) } implies Z1 c= Y)))
proof
let y being (Element of Z);
let Y;
assume A17: Y = { x where x is (Element of Z1): ((( y \ x ) in Z1 & ( x \ y ) in Z1) & ( x /\ y ) in Z1) };
A18: Z c= Y
proof
let z;
assume A19: z in Z;
then A20: ( z /\ y ) in Z by FINSUB_1:def 2;
(( z \ y ) in Z & ( y \ z ) in Z) by A19,PROB_1:6;
hence thesis by A1,A17,A19,A20;
end;
y in Z;
then Y is (MonotoneClass of Omega) by A1,A4,A17;
hence thesis by A18,CKB93:def 1;
end;
A21: (for y being (Element of Z1) holds (for Y holds (Y = { x where x is (Element of Z1): ((( y \ x ) in Z1 & ( x \ y ) in Z1) & ( x /\ y ) in Z1) } implies Z1 c= Y)))
proof
let y being (Element of Z1);
let Y;
assume A22: Y = { x where x is (Element of Z1): ((( y \ x ) in Z1 & ( x \ y ) in Z1) & ( x /\ y ) in Z1) };
A23: Z c= Y
proof
let z;
set Y1 = { x where x is (Element of Z1): ((( z \ x ) in Z1 & ( x \ z ) in Z1) & ( x /\ z ) in Z1) };
assume A24: z in Z;
then A25: Z1 c= Y1 by A16;
A26: y in Z1;
then A27: ( z /\ y ) in Z1 by A2,A25;
(( z \ y ) in Z1 & ( y \ z ) in Z1) by A2,A25,A26;
hence thesis by A1,A22,A24,A27;
end;
Y is (MonotoneClass of Omega) by A4,A22;
hence thesis by A23,CKB93:def 1;
end;
A28: (for y being (Subset of Omega) holds (y in Z1 implies ( y ` ) in Z1))
proof
Omega in Z by PROB_1:5;
then A29: Omega in Z1 by A1;
let y being (Subset of Omega);
assume that
A30: y in Z1;
set Y = { x where x is (Element of Z1): ((( y \ x ) in Z1 & ( x \ y ) in Z1) & ( x /\ y ) in Z1) };
Z1 c= Y by A21,A30;
then ( Omega \ y ) in Z1 by A2,A29;
hence thesis by SUBSET_1:def 4;
end;
now
let y;
let z;
assume that
A31: y in Z1
and
A32: z in Z1;
set Y = { x where x is (Element of Z1): ((( y \ x ) in Z1 & ( x \ y ) in Z1) & ( x /\ y ) in Z1) };
Z1 c= Y by A21,A31;
hence ( y /\ z ) in Z1 by A2,A32;
end;
hence thesis by A28,FINSUB_1:def 2,PROB_1:def 1;
end;
