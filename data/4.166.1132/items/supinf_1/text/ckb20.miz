begin
theorem
Th117: (for F being (bool_DOMAIN of ( ExtREAL )) holds (for S being non  empty  ext-real-membered set holds (S = ( union F ) implies ( inf S ) is (LowerBound of ( INF F )))))
proof
let F being (bool_DOMAIN of ( ExtREAL ));
let S being non  empty  ext-real-membered set;
assume A1: S = ( union F );
(for x being  ext-real number holds (x in ( INF F ) implies ( inf S ) <= x))
proof
let x being  ext-real number;
assume x in ( INF F );
then consider A being non  empty  ext-real-membered set such that A2: A in F and A3: x = ( inf A ) by CKB18:def 1;
A c= S
proof
let a being  ext-real number;
assume a in A;
hence thesis by A1,A2,TARSKI:def 4;
end;
hence thesis by A3,XXREAL_2:60;
end;
hence thesis by XXREAL_2:def 2;
end;
