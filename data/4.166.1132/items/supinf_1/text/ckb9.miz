environ
vocabularies SUBSET_1,NUMBERS,XXREAL_0,MEMBERED,XXREAL_2,ORDINAL1,XBOOLE_0,TARSKI,ORDINAL2,SETFAM_1,SUPINF_1;
notations XBOOLE_0,SUBSET_1,SETFAM_1,DOMAIN_1,ORDINAL1,NUMBERS,MEMBERED,XXREAL_0,XXREAL_2,CKB1,CKB2,CKB3,CKB4,CKB7;
definitions TARSKI,XXREAL_0,MEMBERED,SUBSET_1,SETFAM_1,XXREAL_2,CKB4,CKB7;
theorems TARSKI,XXREAL_0,SETFAM_1,XXREAL_2,CKB4,CKB6,CKB7;
schemes MEMBERED;
registrations XBOOLE_0,SUBSET_1,NUMBERS,XXREAL_0,XREAL_0,MEMBERED,CKB5,CKB8;
constructors NUMBERS,XXREAL_0,XREAL_0,MEMBERED,SETFAM_1,DOMAIN_1,XXREAL_2,CKB2,CKB3,CKB4,CKB7;
requirements SUBSET,BOOLE;
begin
theorem
(for X,Y being  ext-real-membered set holds (X c= Y implies (for x being  ext-real number holds (x in ( SetMinorant Y ) implies x in ( SetMinorant X )))))
proof
let X being  ext-real-membered set;
let Y being  ext-real-membered set;
assume A1: X c= Y;
let x being  ext-real number;
assume x in ( SetMinorant Y );
then x is (LowerBound of Y) by CKB7:def 1;
then x is (LowerBound of X) by A1,XXREAL_2:5;
hence thesis by CKB7:def 1;
end;
