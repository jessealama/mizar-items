theorem
Th8: (for p being (Element of ( Funcs (( Valuations_in A ),( BOOLEAN )) )) holds (( ( FOR_ALL (x,p) ) . v ) = ( TRUE ) iff (for v1 holds ((for y holds (x <> y implies ( v1 . y ) = ( v . y ))) implies ( p . v1 ) = ( TRUE )))))
proof
let p being (Element of ( Funcs (( Valuations_in A ),( BOOLEAN )) ));
A1:now
assume ( ( FOR_ALL (x,p) ) . v ) = ( TRUE );
then ( ALL { ( p . v99 ) where v99 is (Element of ( Valuations_in A )): (for y holds (x <> y implies ( v99 . y ) = ( v . y ))) } ) = ( TRUE ) by CKB5:def 1;
then A2: (not ( FALSE ) in { ( p . v99 ) where v99 is (Element of ( Valuations_in A )): (for y holds (x <> y implies ( v99 . y ) = ( v . y ))) }) by MARGREL1:17;
thus (for v1 holds ((for y holds (x <> y implies ( v1 . y ) = ( v . y ))) implies ( p . v1 ) = ( TRUE )))
proof
let v1;
assume (for y holds (x <> y implies ( v1 . y ) = ( v . y )));
then (not ( p . v1 ) = ( FALSE )) by A2;
hence thesis by XBOOLEAN:def 3;
end;

end;
now
assume (for v1 holds ((for y holds (x <> y implies ( v1 . y ) = ( v . y ))) implies ( p . v1 ) = ( TRUE )));
then (not (ex v1 st (( p . v1 ) = ( FALSE ) & (for y holds (x <> y implies ( v1 . y ) = ( v . y ))))));
then (not ( FALSE ) in { ( p . v99 ) where v99 is (Element of ( Valuations_in A )): (for y holds (x <> y implies ( v99 . y ) = ( v . y ))) });
then ( ALL { ( p . v99 ) where v99 is (Element of ( Valuations_in A )): (for y holds (x <> y implies ( v99 . y ) = ( v . y ))) } ) = ( TRUE ) by MARGREL1:17;
hence ( ( FOR_ALL (x,p) ) . v ) = ( TRUE ) by CKB5:def 1;
end;
hence thesis by A1;
