begin
definition
let X being set;
let f being (Function of [: X,X :],( REAL ));
let x being (Element of X);
func dist (f,x) -> (Function of X,( REAL ))
means
:Def2: (for y being (Element of X) holds ( it . y ) = ( f . (x,y) ));
existence
proof
deffunc D(set) = ( f . (x,$1) );
A1: (for y being set holds (y in X implies D(y) in ( REAL )))
proof
let y being set;
assume y in X;
( f . [ x,y ] ) in ( REAL );
hence thesis;
end;
consider DIST being (Function of X,( REAL )) such that A2: (for y being set holds (y in X implies ( DIST . y ) = D(y))) from FUNCT_2:sch 2(A1);
now
per cases ;
suppose A3: X is  empty;

let y being (Element of X);
(not [ x,y ] in ( dom f )) by A3;
then A4: ( f . [ x,y ] ) = ( {} ) by FUNCT_1:def 2;
(not y in ( dom DIST )) by A3;
hence ( f . (x,y) ) = ( DIST . y ) by A4,FUNCT_1:def 2;
end;
suppose X is non empty;

hence (for y being (Element of X) holds ( DIST . y ) = ( f . (x,y) )) by A2;
end;
end;
hence thesis;
end;
uniqueness
proof
let D1 being (Function of X,( REAL )),D2 being (Function of X,( REAL ));
assume that
A5: (for y being (Element of X) holds ( D1 . y ) = ( f . (x,y) ))
and
A6: (for y being (Element of X) holds ( D2 . y ) = ( f . (x,y) ));
now
let y being (Element of X);
( D1 . y ) = ( f . (x,y) ) by A5;
hence ( D1 . y ) = ( D2 . y ) by A6;
end;
hence thesis by FUNCT_2:63;
end;
end;
