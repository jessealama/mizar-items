begin
theorem
(for TS being non empty TopSpace holds ((TS is  T_2 & TS is  compact) implies TS is  regular))
proof
let TS being non empty TopSpace;
assume that
A1: TS is  T_2
and
A2: TS is  compact;
let p being (Point of TS);
let F being (Subset of TS);
assume that
A3: F <> ( {} )
and
A4: F is  closed
and
A5: p in ( F ` );
thus thesis by A1,A2,A3,A4,A5,CKB12:1,CKB14:1;
end;
