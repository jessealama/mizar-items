begin
theorem
Th32: (for T being non empty TopSpace holds (for F being set holds (F is  open  open (Subset-Family of T) iff F is  open  open (Subset-Family of ( the TopStruct of T )))))
proof
let T being non empty TopSpace;
let F being set;
thus (F is  open  open (Subset-Family of T) implies F is  open  open (Subset-Family of ( the TopStruct of T )))
proof
assume A1: F is  open  open (Subset-Family of T);
then reconsider F as (Subset-Family of ( the TopStruct of T ));
F is  open
proof
let P being (Subset of ( the TopStruct of T ));
assume P in F;
then P is  open  open (Subset of T) by A1,TOPS_2:def 1;
hence thesis by PRE_TOPC:30;
end;
hence thesis;
end;

assume A2: F is  open  open (Subset-Family of ( the TopStruct of T ));
then reconsider F as (Subset-Family of T);
F is  open
proof
let P being (Subset of T);
assume P in F;
then P is  open  open (Subset of ( the TopStruct of T )) by A2,TOPS_2:def 1;
hence thesis by PRE_TOPC:30;
end;
hence thesis;
end;
