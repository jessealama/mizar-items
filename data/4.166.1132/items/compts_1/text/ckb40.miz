environ
vocabularies PRE_TOPC,SUBSET_1,RCOMP_1,SETFAM_1,TARSKI,FINSET_1,XBOOLE_0,CARD_5,RELAT_1,ZFMISC_1,FUNCT_1,STRUCT_0,ORDINAL2,TOPS_2,FUNCT_4,COMPTS_1;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,RELAT_1,FUNCT_1,RELSET_1,FUNCT_3,SETFAM_1,FINSET_1,DOMAIN_1,FUNCT_4,STRUCT_0,PRE_TOPC,TOPS_2,CKB1,CKB2,CKB3,CKB4,CKB5,CKB24;
definitions TARSKI,TOPS_2,XBOOLE_0,SUBSET_1,STRUCT_0,PRE_TOPC,CKB1,CKB2,CKB3,CKB5,CKB24;
theorems TARSKI,SETFAM_1,FUNCT_1,FUNCT_3,ZFMISC_1,ORDERS_1,PRE_TOPC,TOPS_1,TOPS_2,RELAT_1,XBOOLE_0,XBOOLE_1,SUBSET_1,FUNCT_2,STRUCT_0,FUNCT_4,RELSET_1,FINSET_1,CKB1,CKB2,CKB3,CKB5,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB33,CKB38,CKB39;
schemes MCART_1;
registrations XBOOLE_0,SUBSET_1,RELSET_1,FINSET_1,STRUCT_0,PRE_TOPC,TOPS_1,CKB6,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB34,CKB35,CKB36,CKB37;
constructors SETFAM_1,DOMAIN_1,FUNCT_3,FINSET_1,TOPS_2,FUNCT_4,RELSET_1,CKB1,CKB5,CKB24;
requirements SUBSET,BOOLE;
begin
reserve S for non empty TopSpace;
theorem
(for T being non empty TopSpace holds (for T1,T2 being (SubSpace of T) holds (for p1,p2 being (Point of T) holds (for f being (Function of T1,S) holds (for g being (Function of T2,S) holds (((((((((( ( [#] T1 ) \/ ( [#] T2 ) ) = ( [#] T ) & ( ( [#] T1 ) /\ ( [#] T2 ) ) = { p1,p2 }) & T1 is  compact) & T2 is  compact) & T is  T_2) & f is  continuous) & g is  continuous) & ( f . p1 ) = ( g . p1 )) & ( f . p2 ) = ( g . p2 )) implies ( f +* g ) is  continuous  continuous (Function of T,S)))))))
proof
let T being non empty TopSpace;
let T1 being (SubSpace of T),T2 being (SubSpace of T);
let p1 being (Point of T),p2 being (Point of T);
let f being (Function of T1,S);
let g being (Function of T2,S);
assume that
A1: ( ( [#] T1 ) \/ ( [#] T2 ) ) = ( [#] T )
and
A2: ( ( [#] T1 ) /\ ( [#] T2 ) ) = { p1,p2 }
and
A3: T1 is  compact
and
A4: T2 is  compact
and
A5: T is  T_2
and
A6: f is  continuous
and
A7: g is  continuous
and
A8: ( f . p1 ) = ( g . p1 )
and
A9: ( f . p2 ) = ( g . p2 );
set h = ( f +* g );
A10: ( dom g ) = ( [#] T2 ) by FUNCT_2:def 1;
( rng h ) c= ( ( rng f ) \/ ( rng g ) ) by FUNCT_4:17;
then A11: ( rng h ) c= (the carrier of S) by XBOOLE_1:1;
A12: ( dom f ) = ( [#] T1 ) by FUNCT_2:def 1;
then ( dom h ) = (the carrier of T) by A1,A10,FUNCT_4:def 1;
then reconsider h as (Function of T,S) by A11,FUNCT_2:def 1,RELSET_1:4;
(for P being (Subset of S) holds (P is  closed implies ( h " P ) is  closed))
proof
let P being (Subset of S);
( [#] T1 ) c= ( [#] T ) by A1,XBOOLE_1:7;
then reconsider P1 = ( f " P ) as (Subset of T) by XBOOLE_1:1;
( [#] T2 ) c= ( [#] T ) by A1,XBOOLE_1:7;
then reconsider P2 = ( g " P ) as (Subset of T) by XBOOLE_1:1;
A13: ( dom h ) = ( ( dom f ) \/ ( dom g ) ) by FUNCT_4:def 1;
A14:now
let x being set;
thus (x in ( ( h " P ) /\ ( [#] T2 ) ) implies x in ( g " P ))
proof
assume A15: x in ( ( h " P ) /\ ( [#] T2 ) );
then x in ( h " P ) by XBOOLE_0:def 4;
then A16: ( h . x ) in P by FUNCT_1:def 7;
( g . x ) = ( h . x ) by A10,A15,FUNCT_4:13;
hence thesis by A10,A15,A16,FUNCT_1:def 7;
end;

assume A17: x in ( g " P );
then A18: x in ( dom g ) by FUNCT_1:def 7;
( g . x ) in P by A17,FUNCT_1:def 7;
then A19: ( h . x ) in P by A18,FUNCT_4:13;
x in ( dom h ) by A13,A18,XBOOLE_0:def 3;
then x in ( h " P ) by A19,FUNCT_1:def 7;
hence x in ( ( h " P ) /\ ( [#] T2 ) ) by A17,XBOOLE_0:def 4;
end;
A20: (for x being set holds (x in ( [#] T1 ) implies ( h . x ) = ( f . x )))
proof
let x being set;
assume that
A21: x in ( [#] T1 );
now
per cases ;
suppose A22: x in ( [#] T2 );

then A23: x in ( ( [#] T1 ) /\ ( [#] T2 ) ) by A21,XBOOLE_0:def 4;
now
per cases  by A2,A23,TARSKI:def 2;
suppose x = p1;

hence thesis by A8,A10,A22,FUNCT_4:13;
end;
suppose x = p2;

hence thesis by A9,A10,A22,FUNCT_4:13;
end;
end;
hence thesis;
end;
suppose (not x in ( [#] T2 ));

hence thesis by A10,FUNCT_4:11;
end;
end;
hence thesis;
end;
now
let x being set;
thus (x in ( ( h " P ) /\ ( [#] T1 ) ) implies x in ( f " P ))
proof
assume A24: x in ( ( h " P ) /\ ( [#] T1 ) );
then x in ( h " P ) by XBOOLE_0:def 4;
then A25: ( h . x ) in P by FUNCT_1:def 7;
( f . x ) = ( h . x ) by A20,A24;
hence thesis by A12,A24,A25,FUNCT_1:def 7;
end;

assume A26: x in ( f " P );
then x in ( dom f ) by FUNCT_1:def 7;
then A27: x in ( dom h ) by A13,XBOOLE_0:def 3;
( f . x ) in P by A26,FUNCT_1:def 7;
then ( h . x ) in P by A20,A26;
then x in ( h " P ) by A27,FUNCT_1:def 7;
hence x in ( ( h " P ) /\ ( [#] T1 ) ) by A26,XBOOLE_0:def 4;
end;
then A28: ( ( h " P ) /\ ( [#] T1 ) ) = ( f " P ) by TARSKI:1;
assume A29: P is  closed;
then ( f " P ) is  closed by A6,PRE_TOPC:def 6;
then ( f " P ) is  compact by A3,CKB14:1;
then A30: P1 is  compact by CKB38:1;
( g " P ) is  closed by A7,A29,PRE_TOPC:def 6;
then ( g " P ) is  compact by A4,CKB14:1;
then A31: P2 is  compact by CKB38:1;
( h " P ) = ( ( h " P ) /\ ( ( [#] T1 ) \/ ( [#] T2 ) ) ) by A12,A10,A13,RELAT_1:132,XBOOLE_1:28
.= ( ( ( h " P ) /\ ( [#] T1 ) ) \/ ( ( h " P ) /\ ( [#] T2 ) ) ) by XBOOLE_1:23;
then ( h " P ) = ( ( f " P ) \/ ( g " P ) ) by A28,A14,TARSKI:1;
hence thesis by A5,A30,A31;
end;
hence thesis by PRE_TOPC:def 6;
end;
