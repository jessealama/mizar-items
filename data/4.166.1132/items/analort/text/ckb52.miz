environ
vocabularies NUMBERS,RLVECT_1,XBOOLE_0,SUBSET_1,ARYTM_3,REAL_1,RELAT_1,ARYTM_1,ANALMETR,SUPINF_2,CARD_1,MCART_1,ANALOAF,SYMSP_1,ZFMISC_1,STRUCT_0,ANALORT;
notations TARSKI,ZFMISC_1,XXREAL_0,REAL_1,RELSET_1,NUMBERS,STRUCT_0,ALGSTR_0,RLVECT_1,ANALOAF,ANALMETR,GEOMTRAP,CKB8,CKB17,CKB25,CKB26;
definitions STRUCT_0,RLVECT_1,CKB8,CKB17,CKB25,CKB26;
theorems GEOMTRAP,RELAT_1,RLVECT_1,ANALOAF,ANALMETR,ZFMISC_1,XCMPLX_0,XCMPLX_1,XREAL_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB37,CKB38,CKB39,CKB40,CKB41,CKB42,CKB43,CKB44,CKB45,CKB46,CKB47,CKB48,CKB49,CKB50,CKB51;
schemes RELSET_1;
registrations RELSET_1,MEMBERED,STRUCT_0,TDGROUP;
constructors DOMAIN_1,XXREAL_0,REAL_1,MEMBERED,TDGROUP,ANALMETR,GEOMTRAP,CKB8,CKB17,CKB25,CKB26;
requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve V for RealLinearSpace;
reserve u for (VECTOR of V);
reserve u1 for (VECTOR of V);
reserve u2 for (VECTOR of V);
reserve v for (VECTOR of V);
reserve v2 for (VECTOR of V);
reserve w for (VECTOR of V);
reserve x for (VECTOR of V);
reserve y for (VECTOR of V);
theorem
Th41: ( Gen x,y implies (for u holds (for v holds (for w holds (ex u1 st (w <> u1 & u,v,w,u1 are_COrtm_wrt x,y))))))
proof
assume A1:  Gen x,y;
let u;
let v;
let w;
A2:now
assume A3: u = v;
take u1 = ( w + x );
( Ortm (x,y,w) ),( Ortm (x,y,u1) ) // u,v by A3,ANALOAF:9;
then A4: w,u1,u,v are_COrtm_wrt x,y by CKB26:def 1;
w <> u1
proof
assume w = u1;
then x = ( 0. V ) by RLVECT_1:9;
hence contradiction by A1,CKB4:1;
end;
hence thesis by A1,A4,CKB30:1;
end;
now
assume A5: u <> v;
consider u2 such that A6: ( Ortm (x,y,u2) ) = u by A1,CKB16:1;
consider v2 such that A7: ( Ortm (x,y,v2) ) = v by A1,CKB16:1;
take u1 = ( ( v2 + w ) - u2 );
u2,v2 // w,u1 by ANALOAF:16;
then w,u1 // u2,v2 by ANALOAF:12;
then ( Ortm (x,y,w) ),( Ortm (x,y,u1) ) // ( Ortm (x,y,u2) ),( Ortm (x,y,v2) ) by A1,CKB28:1;
then A8: w,u1,u,v are_COrtm_wrt x,y by A6,A7,CKB26:def 1;
w <> u1
proof
assume w = u1;
then w = ( w + ( v2 - u2 ) ) by RLVECT_1:def 3;
then ( v2 - u2 ) = ( 0. V ) by RLVECT_1:9;
hence contradiction by A5,A6,A7,RLVECT_1:21;
end;
hence thesis by A1,A8,CKB30:1;
end;
hence thesis by A2;
end;
