environ
vocabularies NUMBERS,RLVECT_1,XBOOLE_0,SUBSET_1,ARYTM_3,REAL_1,RELAT_1,ARYTM_1,ANALMETR,SUPINF_2,CARD_1,MCART_1,ANALOAF,SYMSP_1,ZFMISC_1,STRUCT_0,ANALORT;
notations TARSKI,ZFMISC_1,XXREAL_0,REAL_1,RELSET_1,NUMBERS,STRUCT_0,ALGSTR_0,RLVECT_1,ANALOAF,ANALMETR,GEOMTRAP,CKB8,CKB17,CKB25,CKB26;
definitions STRUCT_0,RLVECT_1,CKB8,CKB17,CKB25,CKB26;
theorems GEOMTRAP,RELAT_1,RLVECT_1,ANALOAF,ANALMETR,ZFMISC_1,XCMPLX_0,XCMPLX_1,XREAL_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB37,CKB38,CKB39,CKB40,CKB41,CKB42,CKB43,CKB44,CKB45,CKB46,CKB47,CKB48,CKB49,CKB50,CKB51,CKB52,CKB53,CKB54,CKB55,CKB56,CKB57,CKB58;
schemes RELSET_1;
registrations RELSET_1,MEMBERED,STRUCT_0,TDGROUP;
constructors DOMAIN_1,XXREAL_0,REAL_1,MEMBERED,TDGROUP,ANALMETR,GEOMTRAP,CKB8,CKB17,CKB25,CKB26;
requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve V for RealLinearSpace;
reserve u for (VECTOR of V);
reserve u1 for (VECTOR of V);
reserve u2 for (VECTOR of V);
reserve v for (VECTOR of V);
reserve v1 for (VECTOR of V);
reserve w for (VECTOR of V);
reserve w1 for (VECTOR of V);
reserve x for (VECTOR of V);
reserve y for (VECTOR of V);
reserve a for Real;
reserve b for Real;
theorem
( Gen x,y implies (for v holds (for w holds (for u1 holds (for v1 holds (for w1 holds ((((not v,v1,w,u1 are_COrte_wrt x,y) & (not v,v1,u1,w are_COrte_wrt x,y)) & u1,w1,u1,w are_COrte_wrt x,y) implies (ex u2 st ((v,v1,v,u2 are_COrte_wrt x,y or v,v1,u2,v are_COrte_wrt x,y) & (u1,w1,u1,u2 are_COrte_wrt x,y or u1,w1,u2,u1 are_COrte_wrt x,y))))))))))
proof
assume A1:  Gen x,y;
let v;
let w;
let u1;
let v1;
let w1;
consider u such that A2: v <> u and A3: v,v1,v,u are_COrte_wrt x,y by A1,CKB51:1;
assume that
A4: (not v,v1,w,u1 are_COrte_wrt x,y)
and
A5: (not v,v1,u1,w are_COrte_wrt x,y)
and
A6: u1,w1,u1,w are_COrte_wrt x,y;
A7: (not ( Orte (x,y,v) ),( Orte (x,y,v1) ) // w,u1) by A4,CKB25:def 1;
A8: ( Orte (x,y,v) ),( Orte (x,y,v1) ) // v,u by A3,CKB25:def 1;
A9: ( Orte (x,y,u1) ),( Orte (x,y,w1) ) // u1,w by A6,CKB25:def 1;
A10: (not ( Orte (x,y,v) ),( Orte (x,y,v1) ) // u1,w) by A5,CKB25:def 1;
A11: v,u // ( Orte (x,y,v) ),( Orte (x,y,v1) ) by A8,ANALOAF:12;
A12: u1,w // ( Orte (x,y,u1) ),( Orte (x,y,w1) ) by A9,ANALOAF:12;
A13: u1 <> w by A7,ANALOAF:9;
A14: (not v,u // u1,w) by A2,A10,A11,ANALOAF:11;
A15: (not v,u // w,u1) by A2,A7,A11,ANALOAF:11;
( Gen x,y implies (ex u st (ex v st (for w holds (ex a st (ex b st ( ( a * u ) + ( b * v ) ) = w))))))
proof
assume A16:  Gen x,y;
take x;
take y;
thus thesis by A16,ANALMETR:def 1;
end;
then consider u2 such that A17: (v,u // v,u2 or v,u // u2,v) and A18: (u1,w // u1,u2 or u1,w // u2,u1) by A1,A14,A15,ANALOAF:21;
(( Orte (x,y,v) ),( Orte (x,y,v1) ) // v,u2 or ( Orte (x,y,v) ),( Orte (x,y,v1) ) // u2,v) by A2,A11,A17,ANALOAF:11;
then A19: (v,v1,v,u2 are_COrte_wrt x,y or v,v1,u2,v are_COrte_wrt x,y) by CKB25:def 1;
(( Orte (x,y,u1) ),( Orte (x,y,w1) ) // u1,u2 or ( Orte (x,y,u1) ),( Orte (x,y,w1) ) // u2,u1) by A12,A13,A18,ANALOAF:11;
then (u1,w1,u1,u2 are_COrte_wrt x,y or u1,w1,u2,u1 are_COrte_wrt x,y) by CKB25:def 1;
hence thesis by A19;
end;
