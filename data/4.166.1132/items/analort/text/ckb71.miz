environ
vocabularies NUMBERS,RLVECT_1,XBOOLE_0,SUBSET_1,ARYTM_3,REAL_1,RELAT_1,ARYTM_1,ANALMETR,SUPINF_2,CARD_1,MCART_1,ANALOAF,SYMSP_1,ZFMISC_1,STRUCT_0,ANALORT;
notations TARSKI,ZFMISC_1,XXREAL_0,REAL_1,RELSET_1,NUMBERS,STRUCT_0,ALGSTR_0,RLVECT_1,ANALOAF,ANALMETR,GEOMTRAP,CKB8,CKB17,CKB25,CKB26,CKB63,CKB64,CKB65,CKB67;
definitions STRUCT_0,RLVECT_1,CKB8,CKB17,CKB25,CKB26,CKB63,CKB64,CKB65,CKB67;
theorems GEOMTRAP,RELAT_1,RLVECT_1,ANALOAF,ANALMETR,ZFMISC_1,XCMPLX_0,XCMPLX_1,XREAL_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB37,CKB38,CKB39,CKB40,CKB41,CKB42,CKB43,CKB44,CKB45,CKB46,CKB47,CKB48,CKB49,CKB50,CKB51,CKB52,CKB53,CKB54,CKB55,CKB56,CKB57,CKB58,CKB59,CKB60,CKB61,CKB62,CKB63,CKB64,CKB65,CKB67,CKB69,CKB70;
schemes RELSET_1;
registrations RELSET_1,MEMBERED,STRUCT_0,TDGROUP,CKB66,CKB68;
constructors DOMAIN_1,XXREAL_0,REAL_1,MEMBERED,TDGROUP,ANALMETR,GEOMTRAP,CKB8,CKB17,CKB25,CKB26,CKB63,CKB64,CKB65,CKB67;
requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve V for RealLinearSpace;
reserve u for (VECTOR of V);
reserve u1 for (VECTOR of V);
reserve v for (VECTOR of V);
reserve v1 for (VECTOR of V);
reserve x for (VECTOR of V);
reserve y for (VECTOR of V);
reserve p for (Element of ( CESpace (V,x,y) ));
reserve q for (Element of ( CESpace (V,x,y) ));
reserve r for (Element of ( CESpace (V,x,y) ));
reserve s for (Element of ( CESpace (V,x,y) ));
theorem
((((u = p & v = q) & u1 = r) & v1 = s) implies (p,q // r,s iff u,v,u1,v1 are_COrte_wrt x,y))
proof
assume that
A1: u = p
and
A2: v = q
and
A3: u1 = r
and
A4: v1 = s;
A5: (p,q // r,s implies u,v,u1,v1 are_COrte_wrt x,y)
proof
assume p,q // r,s;
then [ [ p,q ],[ r,s ] ] in (the CONGR of ( CESpace (V,x,y) )) by ANALOAF:def 2;
then consider u19,u29,v19,v29 being (VECTOR of V) such that A6: [ u,v ] = [ u19,u29 ] and A7: [ u1,v1 ] = [ v19,v29 ] and A8: u19,u29,v19,v29 are_COrte_wrt x,y by A1,A2,A3,A4,CKB63:def 1;
A9: u = u19 by A6,ZFMISC_1:27;
A10: v = u29 by A6,ZFMISC_1:27;
u1 = v19 by A7,ZFMISC_1:27;
hence thesis by A7,A8,A9,A10,ZFMISC_1:27;
end;
(u,v,u1,v1 are_COrte_wrt x,y implies p,q // r,s)
proof
assume u,v,u1,v1 are_COrte_wrt x,y;
then [ [ u,v ],[ u1,v1 ] ] in (the CONGR of AffinStruct (# (the carrier of V),( CORTE (V,x,y) ) #)) by CKB63:def 1;
hence thesis by A1,A2,A3,A4,ANALOAF:def 2;
end;
hence thesis by A5;
end;
