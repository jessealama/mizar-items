environ
vocabularies NUMBERS,RLVECT_1,XBOOLE_0,SUBSET_1,ARYTM_3,REAL_1,RELAT_1,ARYTM_1,ANALMETR,SUPINF_2,CARD_1,MCART_1,ANALOAF,SYMSP_1,ZFMISC_1,STRUCT_0,ANALORT;
notations TARSKI,ZFMISC_1,XXREAL_0,REAL_1,RELSET_1,NUMBERS,STRUCT_0,ALGSTR_0,RLVECT_1,ANALOAF,ANALMETR,GEOMTRAP,CKB8,CKB17,CKB25,CKB26;
definitions STRUCT_0,RLVECT_1,CKB8,CKB17,CKB25,CKB26;
theorems GEOMTRAP,RELAT_1,RLVECT_1,ANALOAF,ANALMETR,ZFMISC_1,XCMPLX_0,XCMPLX_1,XREAL_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB37,CKB38;
schemes RELSET_1;
registrations RELSET_1,MEMBERED,STRUCT_0,TDGROUP;
constructors DOMAIN_1,XXREAL_0,REAL_1,MEMBERED,TDGROUP,ANALMETR,GEOMTRAP,CKB8,CKB17,CKB25,CKB26;
requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve V for RealLinearSpace;
reserve u for (VECTOR of V);
reserve u1 for (VECTOR of V);
reserve u2 for (VECTOR of V);
reserve v for (VECTOR of V);
reserve v1 for (VECTOR of V);
reserve v2 for (VECTOR of V);
reserve x for (VECTOR of V);
reserve y for (VECTOR of V);
theorem
( Gen x,y implies (u,v // u1,v1 iff (ex u2 st (ex v2 st ((u2 <> v2 & u2,v2,u,v are_COrtm_wrt x,y) & u2,v2,u1,v1 are_COrtm_wrt x,y)))))
proof
assume A1:  Gen x,y;
A2: (u,v // u1,v1 implies (ex u2 st (ex v2 st ((u2 <> v2 & u2,v2,u,v are_COrtm_wrt x,y) & u2,v2,u1,v1 are_COrtm_wrt x,y))))
proof
assume A3: u,v // u1,v1;
A4:now
assume that
A5: u = v
and
A6: u1 = v1;
take u2 = ( 0. V );
take v2 = x;
A7: ( Ortm (x,y,u2) ),( Ortm (x,y,v2) ) // u,v by A5,ANALOAF:9;
( Ortm (x,y,u2) ),( Ortm (x,y,v2) ) // u1,v1 by A6,ANALOAF:9;
then A8: u2,v2,u1,v1 are_COrtm_wrt x,y by CKB26:def 1;
A9: u2,v2,u,v are_COrtm_wrt x,y by A7,CKB26:def 1;
u2 <> v2 by A1,CKB4:1;
hence thesis by A8,A9;
end;
A10:now
assume A11: u <> v;
consider u2 such that A12: ( Ortm (x,y,u2) ) = u by A1,CKB16:1;
consider v2 such that A13: ( Ortm (x,y,v2) ) = v by A1,CKB16:1;
( Ortm (x,y,u2) ),( Ortm (x,y,v2) ) // u,v by A12,A13,ANALOAF:8;
then A14: u2,v2,u,v are_COrtm_wrt x,y by CKB26:def 1;
u2,v2,u1,v1 are_COrtm_wrt x,y by A3,A12,A13,CKB26:def 1;
hence thesis by A11,A12,A13,A14;
end;
now
assume A15: u1 <> v1;
consider u2 such that A16: ( Ortm (x,y,u2) ) = u1 by A1,CKB16:1;
consider v2 such that A17: ( Ortm (x,y,v2) ) = v1 by A1,CKB16:1;
( Ortm (x,y,u2) ),( Ortm (x,y,v2) ) // u1,v1 by A16,A17,ANALOAF:8;
then A18: u2,v2,u1,v1 are_COrtm_wrt x,y by CKB26:def 1;
( Ortm (x,y,u2) ),( Ortm (x,y,v2) ) // u,v by A3,A16,A17,ANALOAF:12;
then u2,v2,u,v are_COrtm_wrt x,y by CKB26:def 1;
hence thesis by A15,A16,A17,A18;
end;
hence thesis by A4,A10;
end;
((ex u2 st (ex v2 st ((u2 <> v2 & u2,v2,u,v are_COrtm_wrt x,y) & u2,v2,u1,v1 are_COrtm_wrt x,y))) implies u,v // u1,v1)
proof
given u2,v2 such that
A19: u2 <> v2
and
A20: u2,v2,u,v are_COrtm_wrt x,y
and
A21: u2,v2,u1,v1 are_COrtm_wrt x,y;

A22: ( Ortm (x,y,u2) ),( Ortm (x,y,v2) ) // u,v by A20,CKB26:def 1;
( Ortm (x,y,u2) ),( Ortm (x,y,v2) ) // u1,v1 by A21,CKB26:def 1;
hence thesis by A1,A19,A22,CKB14:1,ANALOAF:11;
end;
hence thesis by A2;
end;
