environ
vocabularies NUMBERS,RLVECT_1,XBOOLE_0,SUBSET_1,ARYTM_3,REAL_1,RELAT_1,ARYTM_1,ANALMETR,SUPINF_2,CARD_1,MCART_1,ANALOAF,SYMSP_1,ZFMISC_1,STRUCT_0,ANALORT;
notations TARSKI,ZFMISC_1,XXREAL_0,REAL_1,RELSET_1,NUMBERS,STRUCT_0,ALGSTR_0,RLVECT_1,ANALOAF,ANALMETR,GEOMTRAP,CKB8,CKB17,CKB25,CKB26;
definitions STRUCT_0,RLVECT_1,CKB8,CKB17,CKB25,CKB26;
theorems GEOMTRAP,RELAT_1,RLVECT_1,ANALOAF,ANALMETR,ZFMISC_1,XCMPLX_0,XCMPLX_1,XREAL_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB37,CKB38,CKB39,CKB40,CKB41,CKB42;
schemes RELSET_1;
registrations RELSET_1,MEMBERED,STRUCT_0,TDGROUP;
constructors DOMAIN_1,XXREAL_0,REAL_1,MEMBERED,TDGROUP,ANALMETR,GEOMTRAP,CKB8,CKB17,CKB25,CKB26;
requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve V for RealLinearSpace;
reserve u for (VECTOR of V);
reserve u1 for (VECTOR of V);
reserve v for (VECTOR of V);
reserve v1 for (VECTOR of V);
reserve w for (VECTOR of V);
reserve x for (VECTOR of V);
reserve y for (VECTOR of V);
theorem
((( Gen x,y & u,v,u1,v1 are_COrte_wrt x,y) & u,v,u1,w are_COrte_wrt x,y) implies (u,v,v1,w are_COrte_wrt x,y or u,v,w,v1 are_COrte_wrt x,y))
proof
assume that
A1:  Gen x,y
and
A2: u,v,u1,v1 are_COrte_wrt x,y
and
A3: u,v,u1,w are_COrte_wrt x,y;
A4: ( Orte (x,y,u) ),( Orte (x,y,v) ) // u1,v1 by A2,CKB25:def 1;
A5: ( Orte (x,y,u) ),( Orte (x,y,v) ) // u1,w by A3,CKB25:def 1;
now
assume A6: u <> v;
now
assume that
A7: u1 <> v1
and
A8: u1 <> w;
A9: u1,v1 // u1,w by A1,A4,A5,A6,CKB22:1,ANALOAF:11;
A10:now
assume A11: u1,v1 // v1,w;
u1,v1 // ( Orte (x,y,u) ),( Orte (x,y,v) ) by A4,ANALOAF:12;
then ( Orte (x,y,u) ),( Orte (x,y,v) ) // v1,w by A7,A11,ANALOAF:11;
hence thesis by CKB25:def 1;
end;
now
assume A12: u1,w // w,v1;
u1,w // ( Orte (x,y,u) ),( Orte (x,y,v) ) by A5,ANALOAF:12;
then ( Orte (x,y,u) ),( Orte (x,y,v) ) // w,v1 by A8,A12,ANALOAF:11;
hence thesis by CKB25:def 1;
end;
hence thesis by A9,A10,ANALOAF:14;
end;
hence thesis by A2,A3;
end;
hence thesis by CKB31:1;
end;
