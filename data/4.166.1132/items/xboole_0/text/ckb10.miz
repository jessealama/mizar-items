constructors TARSKI,CKB2,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9;
begin
reserve X for set;
reserve Y for set;
reserve x for set;
definition
let X being set;
let Y being set;
pred X c< Y
means
:Def8: (X c= Y & X <> Y)
;irreflexivity;
asymmetry
proof
let X;
let Y;
assume A1: X c= Y;
assume X <> Y;
assume Y c= X;
then (for x holds (x in X iff x in Y)) by A1,TARSKI:def 3;
hence thesis by TARSKI:1;
end;
end;
