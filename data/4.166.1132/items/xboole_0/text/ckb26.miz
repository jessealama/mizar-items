environ
vocabularies TARSKI,XBOOLE_0;
notations TARSKI,CKB2,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13;
definitions CKB2,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12;
theorems TARSKI,CKB2,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB14,CKB15,CKB22,CKB23,CKB24,CKB25;
schemes TARSKI,CKB1;
registrations CKB3,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21;
constructors TARSKI,CKB2,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11;
begin
reserve x for set;
scheme Extensionality { X,Y() -> set,P[set] } : X() = Y()
provided
A1: (for x holds (x in X() iff P[ x ]))
and
A2: (for x holds (x in Y() iff P[ x ]))
proof
A3: (x in Y() implies x in X())
proof
assume x in Y();
then P[ x ] by A2;
hence thesis by A1;
end;
(x in X() implies x in Y())
proof
assume x in X();
then P[ x ] by A1;
hence thesis by A2;
end;
hence thesis by A3,TARSKI:1;
end;
