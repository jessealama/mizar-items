environ
vocabularies TARSKI,XBOOLE_0;
notations TARSKI,CKB2,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13;
definitions CKB2,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12;
theorems TARSKI,CKB2,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB14,CKB15,CKB22,CKB23;
schemes TARSKI,CKB1;
registrations CKB3,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21;
constructors TARSKI,CKB2,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11;
begin
reserve X for set;
reserve Y for set;
reserve x for set;
theorem
(X meets Y iff (ex x st x in ( X /\ Y )))
proof
hereby
assume X meets Y;
then ( X /\ Y ) <> ( {} ) by CKB9:def 1;
then (not ( X /\ Y ) is  empty) by CKB22:1;
then consider x such that A1: x in ( X /\ Y ) by CKB2:def 1;
take x;
thus x in ( X /\ Y ) by A1;
end;
assume (ex x st x in ( X /\ Y ));
then ( X /\ Y ) <> ( {} ) by CKB2:def 1;
hence thesis by CKB9:def 1;
end;
