environ
vocabularies TARSKI,XBOOLE_0;
notations TARSKI,CKB2,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13;
definitions CKB2,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12;
theorems TARSKI,CKB2,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB14;
schemes TARSKI,CKB1;
registrations CKB3;
constructors TARSKI,CKB2,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11;
begin
reserve X for set;
reserve Y for set;
reserve Z for set;
reserve x for set;
theorem
((for x holds ((not x in X) iff (x in Y iff x in Z))) implies X = ( Y \+\ Z ))
proof
assume A1: ((not x in X) iff (x in Y iff x in Z));
now
let x;
(x in X iff ((x in Y & (not x in Z)) or (x in Z & (not x in Y)))) by A1;
then (x in X iff (x in ( Y \ Z ) or x in ( Z \ Y ))) by CKB7:def 1;
hence (x in X iff x in ( Y \+\ Z )) by CKB5:def 1;
end;
hence thesis by TARSKI:1;
end;
