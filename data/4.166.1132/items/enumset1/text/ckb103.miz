begin
reserve x for set;
reserve y for set;
theorem
(for x,y,z being set holds ((x <> y & x <> z) implies ( { x,y,z } \ { x } ) = { y,z }))
proof
let x being set;
let y being set;
let z being set;
assume that
A1: (x <> y & x <> z);
hereby
let a being set;
assume A2: a in ( { x,y,z } \ { x } );
then a in { x,y,z } by XBOOLE_0:def 5;
then A3: ((a = x or a = y) or a = z) by CKB2:def 1;
(not a in { x }) by A2,XBOOLE_0:def 5;
hence a in { y,z } by A3,TARSKI:def 1,TARSKI:def 2;
end;
let a being set;
assume a in { y,z };
then A4: (a = y or a = z) by TARSKI:def 2;
then A5: (not a in { x }) by A1,TARSKI:def 1;
a in { x,y,z } by A4,CKB2:def 1;
hence thesis by A5,XBOOLE_0:def 5;
end;
