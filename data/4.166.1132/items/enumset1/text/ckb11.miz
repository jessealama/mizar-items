environ
vocabularies TARSKI,XBOOLE_0;
notations TARSKI,XBOOLE_0,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9;
definitions TARSKI,XBOOLE_0,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9;
theorems TARSKI,XBOOLE_0,XBOOLE_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10;
schemes XBOOLE_0;
constructors TARSKI,XBOOLE_0,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9;
begin
reserve x for set;
reserve x1 for set;
reserve x2 for set;
reserve x3 for set;
theorem
Th42: { x1,x2,x3 } = ( { x1 } \/ { x2,x3 } )
proof
now
let x;
(x in { x1,x2,x3 } iff ((x = x1 or x = x2) or x = x3)) by CKB2:def 1;
then (x in { x1,x2,x3 } iff (x in { x1 } or x in { x2,x3 })) by TARSKI:def 1,TARSKI:def 2;
hence (x in { x1,x2,x3 } iff x in ( { x1 } \/ { x2,x3 } )) by XBOOLE_0:def 3;
end;
hence thesis by TARSKI:1;
end;
