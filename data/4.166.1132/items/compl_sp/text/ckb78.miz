theorem
Th41: (for M being  symmetric  triangle  Reflexive non empty MetrStruct holds (for a being (Point of M) holds (for S being (sequence of ( WellSpace (a,X) )) holds (S is  Cauchy implies ((for Xa being (Point of ( WellSpace (a,X) )) holds (Xa = [ X,a ] implies (for r holds (r > ( 0 ) implies (ex n st (for m holds (m >= n implies ( dist (( S . m ),Xa) ) < r))))))) or (ex n st (ex Y st (for m holds (m >= n implies (ex p being (Point of M) st ( S . m ) = [ Y,p ]))))))))))
proof
let M being  symmetric  triangle  Reflexive non empty MetrStruct;
let a being (Point of M);
set W = ( WellSpace (a,X) );
reconsider Xa = [ X,a ] as (Point of W) by CKB67:1;
let S being (sequence of W);
assume that
A1: S is  Cauchy;
per cases ;
suppose (for r holds (r > ( 0 ) implies (ex n st (for m holds (m >= n implies ( dist (( S . m ),Xa) ) < r)))));

hence thesis;
end;
suppose (ex r st (r > ( 0 ) & (for n holds (ex m st (m >= n & ( dist (( S . m ),Xa) ) >= r)))));

then consider r being Real such that A2: r > ( 0 ) and A3: (for n holds (ex m st (m >= n & ( dist (( S . m ),Xa) ) >= r)));
consider p being (Element of ( NAT )) such that A4: (for n,m being (Element of ( NAT )) holds ((n >= p & m >= p) implies ( dist (( S . n ),( S . m )) ) < r)) by A1,A2,TBSP_1:def 4;
consider p9 being  natural number such that A5: p9 >= p and A6: ( dist (( S . p9 ),Xa) ) >= r by A3;
consider Y being set,y being (Point of M) such that A7: ( S . p9 ) = [ Y,y ] and ((Y in X & y <> a) or (Y = X & y = a)) by CKB67:1;
(ex n st (ex Y st (for m holds (m >= n implies (ex p being (Point of M) st ( S . m ) = [ Y,p ])))))
proof
take p9;
take Y;
let m;
assume that
A8: m >= p9;
consider Z being set,z being (Point of M) such that A9: ( S . m ) = [ Z,z ] and ((Z in X & z <> a) or (Z = X & z = a)) by CKB67:1;
Y = Z
proof
A10: p9 in ( NAT ) by ORDINAL1:def 12;
A11: ( dist (a,z) ) >= ( 0 ) by METRIC_1:5;
A12: ( dist (a,a) ) = ( 0 ) by METRIC_1:1;
(X = Y or X <> Y);
then (( dist (( S . p9 ),Xa) ) = ( dist (y,a) ) or ( dist (( S . p9 ),Xa) ) = ( ( dist (y,a) ) + ( 0 ) )) by A7,A12,CKB68:def 1;
then A13: ( ( dist (y,a) ) + ( dist (a,z) ) ) >= ( r + ( 0 ) ) by A6,A11,XREAL_1:7;
assume Y <> Z;
then A14: ( dist (( S . p9 ),( S . m )) ) >= r by A7,A9,A13,CKB68:def 1;
A15: m in ( NAT ) by ORDINAL1:def 12;
m >= p by A5,A8,XXREAL_0:2;
hence thesis by A4,A5,A15,A10,A14;
end;
hence thesis by A9;
end;
hence thesis;
end;
