environ
vocabularies POSET_1,ORDERS_1,RELAT_1,RELAT_2,XBOOLE_0,FUNCT_1,FUNCT_2,ORDINAL2,FUNCOP_1,SEQM_3,LATTICES,LATTICE3,YELLOW_0,WAYBEL_0,ABIAN,TARSKI,CARD_1,FUNCT_7,NAT_1,SUBSET_1,ORDERS_2,XXREAL_0,STRUCT_0,ARYTM_3,TREES_2,EQREL_1;
notations TARSKI,XBOOLE_0,SUBSET_1,RELAT_1,RELAT_2,FUNCT_1,ORDERS_1,RELSET_1,PARTFUN1,FUNCT_2,STRUCT_0,ORDERS_2,ORDERS_3,NUMBERS,NAT_1,FUNCT_7,XXREAL_0,XCMPLX_0,YELLOW_2,LATTICE3,YELLOW_0,WAYBEL_0,ABIAN,CKB6;
definitions RELAT_1,LATTICE3,STRUCT_0,CKB6;
theorems RELAT_1,RELSET_1,RELAT_2,ORDERS_2,TARSKI,FUNCT_1,FUNCT_2,XBOOLE_0,XBOOLE_1,ORDERS_1,ORDERS_3,FUNCOP_1,FUNCT_7,NAT_1,LATTICE3,YELLOW_0,ABIAN,YELLOW_2,WAYBEL_0,CKB1,CKB2,CKB3,CKB5,CKB6;
schemes NAT_1,RELSET_1,FUNCT_2;
registrations XBOOLE_0,ORDINAL1,FUNCT_1,PARTFUN1,STRUCT_0,ORDERS_2,XREAL_0,NAT_1,RELAT_1,WAYBEL10,YELLOW_0,WAYBEL24,FUNCT_7,RELSET_1,CKB4;
constructors ORDERS_3,NAT_1,DOMAIN_1,XXREAL_0,ABIAN,LATTICE3,YELLOW_2,RELSET_1,CKB6;
requirements NUMERALS,BOOLE,SUBSET,ARITHM;
begin
theorem
Th1: (for P1,P2 being non empty Poset holds (for K being non empty (Chain of P1) holds (for h being  monotone (Function of P1,P2) holds ( h .: K ) is non empty non empty (Chain of P2))))
proof
let P1 being non empty Poset,P2 being non empty Poset;
let K being non empty (Chain of P1);
let h being  monotone (Function of P1,P2);
set R = (the InternalRel of P2);
set K2 = ( h .: K );
(for x,y being set holds (((x in K2 & y in K2) & x <> y) implies ([ x,y ] in R or [ y,x ] in R)))
proof
let x being set,y being set;
assume A1: ((x in K2 & y in K2) & x <> y);
then reconsider x,y as (Element of P2);
consider a being set such that A2: ((a in ( dom h ) & a in K) & x = ( h . a )) by A1,FUNCT_1:def 6;
consider b being set such that A3: ((b in ( dom h ) & b in K) & y = ( h . b )) by A1,FUNCT_1:def 6;
reconsider a,b as (Element of P1) by A2,A3;
(a <= b or b <= a) by A2,A3,ORDERS_2:11;
then (x <= y or y <= x) by A2,A3,ORDERS_3:def 5;
hence thesis by ORDERS_2:def 5;
end;
then A4: R is_connected_in K2 by RELAT_2:def 6;
(for x being set holds (x in K2 implies [ x,x ] in R))
proof
let x being set;
assume x in K2;
then reconsider x as (Element of P2);
x <= x;
hence thesis by ORDERS_2:def 5;
end;
then R is_reflexive_in K2 by RELAT_2:def 1;
then R is_strongly_connected_in K2 by A4,ORDERS_1:7;
then reconsider K2 as (Chain of P2) by ORDERS_2:def 7;
consider a being set such that A5: a in K by XBOOLE_0:7;
a in (the carrier of P1) by A5;
then a in ( dom h ) by FUNCT_2:def 1;
then ( h . a ) in K2 by A5,FUNCT_1:def 6;
hence thesis;
end;
