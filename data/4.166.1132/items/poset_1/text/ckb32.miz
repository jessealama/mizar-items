registrations XBOOLE_0,ORDINAL1,FUNCT_1,PARTFUN1,STRUCT_0,ORDERS_2,XREAL_0,NAT_1,RELAT_1,WAYBEL10,YELLOW_0,WAYBEL24,FUNCT_7,RELSET_1,CKB4,CKB8,CKB9,CKB27,CKB28;
constructors ORDERS_3,NAT_1,DOMAIN_1,XXREAL_0,ABIAN,LATTICE3,YELLOW_2,RELSET_1,CKB6,CKB12,CKB21,CKB24;
requirements NUMERALS,BOOLE,SUBSET,ARITHM;
begin
reserve P for  strict  chain-complete non  empty Poset;
reserve p for (Element of P);
reserve p1 for (Element of P);
reserve p2 for (Element of P);
reserve g for  monotone (Function of P,P);
definition
let P;
let g being  monotone (Function of P,P);
assume A1: g is  continuous;
func least_fix_point g -> (Element of P) means 
:Def5: (it is_a_fixpoint_of g & (for p holds (p is_a_fixpoint_of g implies it <= p)));
existence
proof
set IT = ( sup ( iter_min g ) );
take IT;
thus thesis by A1,CKB30:1,CKB31:1;
end;
uniqueness
proof
let p1;
let p2;
assume ((p1 is_a_fixpoint_of g & (for p holds (p is_a_fixpoint_of g implies p1 <= p))) & (p2 is_a_fixpoint_of g & (for p holds (p is_a_fixpoint_of g implies p2 <= p))));
then (p1 <= p2 & p2 <= p1);
hence thesis by ORDERS_2:2;
end;
end;
