definition
let P;
func fix_func P -> (Function of ( ConPoset (P,P) ),P)
means
:Def12: (for g being (Element of ( ConPoset (P,P) )) holds (for h being  continuous (Function of P,P) holds (g = h implies ( it . g ) = ( least_fix_point h ))));
existence
proof
set X = (the carrier of ( ConPoset (P,P) ));
set Y = (the carrier of P);
defpred U[ set,set ]
 means
(for g being (Element of ( ConPoset (P,P) )) holds (for h being  continuous (Function of P,P) holds ((g = h & g = $1) implies $2 = ( least_fix_point h ))));
A1: (for x holds (x in X implies (ex y st (y in Y & U[ x,y ]))))
proof
let x;
assume x in X;
then reconsider x as (Element of ( ConPoset (P,P) ));
reconsider h = x as  continuous (Function of P,P) by CKB63:1;
take ( least_fix_point h );
thus thesis;
end;
consider IT being (Function of X,Y) such that A2: (for x holds (x in X implies U[ x,( IT . x ) ])) from FUNCT_2:sch 1(A1);
(for g being (Element of ( ConPoset (P,P) )) holds (for h being  continuous (Function of P,P) holds (g = h implies ( IT . g ) = ( least_fix_point h )))) by A2;
hence thesis;
end;
uniqueness
proof
let h1 being (Function of ( ConPoset (P,P) ),P),h2 being (Function of ( ConPoset (P,P) ),P);
assume that
A3: ((for g being (Element of ( ConPoset (P,P) )) holds (for h being  continuous (Function of P,P) holds (g = h implies ( h1 . g ) = ( least_fix_point h )))) & (for g being (Element of ( ConPoset (P,P) )) holds (for h being  continuous (Function of P,P) holds (g = h implies ( h2 . g ) = ( least_fix_point h )))));
set X = (the carrier of ( ConPoset (P,P) ));
(for x holds (x in X implies ( h1 . x ) = ( h2 . x )))
proof
let x;
assume x in X;
then reconsider g = x as (Element of ( ConPoset (P,P) ));
reconsider h = g as  continuous (Function of P,P) by CKB63:1;
( h1 . x ) = ( least_fix_point h ) by A3
.= ( h2 . x ) by A3;
hence thesis;
end;
hence thesis by FUNCT_2:12;
end;
