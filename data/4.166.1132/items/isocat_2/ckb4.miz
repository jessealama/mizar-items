begin
theorem
Th3: (for x being set holds (for A being non empty set holds (for f,g being (Function of { x },A) holds (( f . x ) = ( g . x ) implies f = g))))
proof
let x being set;
let A being non empty set;
let f being (Function of { x },A),g being (Function of { x },A);
assume that
A1: ( f . x ) = ( g . x );
now
let y being set;
assume y in { x };
then y = x by TARSKI:def 1;
hence ( f . y ) = ( g . y ) by A1;
end;
hence thesis by FUNCT_2:12;
end;
