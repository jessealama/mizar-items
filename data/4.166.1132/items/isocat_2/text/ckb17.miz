begin
reserve A for Category;
reserve B for Category;
theorem
Th16: (for a1,a2 being (Object of A) holds (for b1,b2 being (Object of B) holds (for f being (Morphism of A) holds (for g being (Morphism of B) holds ((f in ( Hom (a1,a2) ) & g in ( Hom (b1,b2) )) implies [ f,g ] in ( Hom ([ a1,b1 ],[ a2,b2 ]) ))))))
proof
let a1 being (Object of A),a2 being (Object of A);
let b1 being (Object of B),b2 being (Object of B);
let f being (Morphism of A);
let g being (Morphism of B);
assume (f in ( Hom (a1,a2) ) & g in ( Hom (b1,b2) ));
then [ f,g ] in [: ( Hom (a1,a2) ),( Hom (b1,b2) ) :] by ZFMISC_1:87;
hence thesis by CAT_2:32;
end;
