environ
vocabularies XBOOLE_0,FUNCT_1,FUNCT_2,FUNCT_5,ZFMISC_1,RELAT_1,TARSKI,SUBSET_1,MCART_1,CAT_1,GRAPH_1,NATTRA_1,STRUCT_0,CAT_2,FINSEQ_2,PZFMISC1,FUNCOP_1,PARTFUN1,BORSUK_1,ISOCAT_2;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,RELAT_1,FUNCT_1,RELSET_1,PARTFUN1,FUNCT_2,FUNCOP_1,BINOP_1,FUNCT_3,FUNCT_5,STRUCT_0,GRAPH_1,CAT_1,CAT_2,NATTRA_1,ISOCAT_1,CKB1,CKB10,CKB16,CKB19,CKB24,CKB30,CKB36,CKB40,CKB41,CKB42,CKB45,CKB46,CKB53;
definitions TARSKI,FUNCT_1,CAT_1,NATTRA_1,ISOCAT_1,XBOOLE_0,BINOP_1,GRAPH_1,CKB10,CKB16,CKB19,CKB24,CKB30,CKB36,CKB41,CKB42,CKB45,CKB46,CKB53;
theorems FUNCT_2,CAT_1,TARSKI,ZFMISC_1,FUNCT_1,DOMAIN_1,CAT_2,FUNCT_3,FUNCOP_1,NATTRA_1,ISOCAT_1,FUNCT_5,RELSET_1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB38,CKB39,CKB41,CKB42,CKB43,CKB44,CKB45,CKB46,CKB47,CKB48,CKB49,CKB50,CKB51,CKB52,CKB53,CKB54;
schemes FUNCT_2;
registrations XBOOLE_0,SUBSET_1,RELSET_1,FUNCT_2,STRUCT_0,CKB37;
constructors PARTFUN1,DOMAIN_1,ISOCAT_1,FUNCOP_1,RELSET_1,CKB1,CKB10,CKB16,CKB19,CKB24,CKB30,CKB36,CKB40,CKB41,CKB42,CKB45,CKB46,CKB53;
requirements SUBSET,BOOLE;
begin
reserve A for Category;
reserve B for Category;
reserve C for Category;
reserve F1 for (Functor of A,B);
theorem
Lm4: (for F1,G1 being (Functor of A,B) holds (for F2,G2 being (Functor of A,C) holds ((F1 is_naturally_transformable_to G1 & F2 is_naturally_transformable_to G2) implies (for t1 being (natural_transformation of F1,G1) holds (for t2 being (natural_transformation of F2,G2) holds (for a,b being (Object of A) holds (( Hom (a,b) ) <> ( {} ) implies (for f being (Morphism of a,b) holds ( ( <: t1,t2 :> . b ) * ( <: F1,F2 :> . f ) ) = ( ( <: G1,G2 :> . f ) * ( <: t1,t2 :> . a ) )))))))))
proof
let F1 being (Functor of A,B);
let G1 being (Functor of A,B);
let F2 being (Functor of A,C);
let G2 being (Functor of A,C);
assume that
A1: F1 is_naturally_transformable_to G1
and
A2: F2 is_naturally_transformable_to G2;
let t1 being (natural_transformation of F1,G1);
let t2 being (natural_transformation of F2,G2);
let a being (Object of A);
let b being (Object of A);
assume that
A3: ( Hom (a,b) ) <> ( {} );
A4: ( Hom (( F1 . a ),( F1 . b )) ) <> ( {} ) by A3,CAT_1:84;
let f being (Morphism of a,b);
A5: (( ( t1 . b ) * ( F1 . f ) ) = ( ( G1 . f ) * ( t1 . a ) ) & ( ( t2 . b ) * ( F2 . f ) ) = ( ( G2 . f ) * ( t2 . a ) )) by A1,A2,A3,NATTRA_1:def 8;
A6: ( <: G1,G2 :> . f ) = [ ( G1 . f ),( G2 . f ) ] by A3,CKB49:1;
A7: ( <: F1,F2 :> . f ) = [ ( F1 . f ),( F2 . f ) ] by A3,CKB49:1;
A8: F2 is_transformable_to G2 by A2,NATTRA_1:def 7;
then A9: ( Hom (( F2 . b ),( G2 . b )) ) <> ( {} ) by NATTRA_1:def 2;
A10: F1 is_transformable_to G1 by A1,NATTRA_1:def 7;
then A11: ( <: t1,t2 :> . b ) = [ ( t1 . b ),( t2 . b ) ] by A8,CKB54:1;
A12: ( <: t1,t2 :> . a ) = [ ( t1 . a ),( t2 . a ) ] by A10,A8,CKB54:1;
A13: ( Hom (( G2 . a ),( G2 . b )) ) <> ( {} ) by A3,CAT_1:84;
A14: <: F1,F2 :> is_transformable_to <: G1,G2 :> by A10,A8,CKB52:1;
then A15: ( Hom (( <: F1,F2 :> . a ),( <: G1,G2 :> . a )) ) <> ( {} ) by NATTRA_1:def 2;
A16: ( Hom (( <: G1,G2 :> . a ),( <: G1,G2 :> . b )) ) <> ( {} ) by A3,CAT_1:84;
A17: ( Hom (( G1 . a ),( G1 . b )) ) <> ( {} ) by A3,CAT_1:84;
A18: ( Hom (( F2 . a ),( F2 . b )) ) <> ( {} ) by A3,CAT_1:84;
then A19: ( ( t2 . b ) * ( F2 . f ) ) = ( ( t2 . b ) * (( F2 . f ) qua (Morphism of C)) ) by A9,CAT_1:def 10;
A20: ( Hom (( F2 . a ),( G2 . a )) ) <> ( {} ) by A8,NATTRA_1:def 2;
then A21: ( ( G2 . f ) * ( t2 . a ) ) = ( ( G2 . f ) * (( t2 . a ) qua (Morphism of C)) ) by A13,CAT_1:def 10;
A22: ( cod ( t2 . a ) ) = ( G2 . a ) by A20,CAT_1:5
.= ( dom ( G2 . f ) ) by A13,CAT_1:5;
A23: ( Hom (( F1 . b ),( G1 . b )) ) <> ( {} ) by A10,NATTRA_1:def 2;
then A24: ( dom ( t1 . b ) ) = ( F1 . b ) by CAT_1:5
.= ( cod ( F1 . f ) ) by A4,CAT_1:5;
A25: ( Hom (( F1 . a ),( G1 . a )) ) <> ( {} ) by A10,NATTRA_1:def 2;
then A26: ( ( G1 . f ) * ( t1 . a ) ) = ( ( G1 . f ) * (( t1 . a ) qua (Morphism of B)) ) by A17,CAT_1:def 10;
A27: ( cod ( t1 . a ) ) = ( G1 . a ) by A25,CAT_1:5
.= ( dom ( G1 . f ) ) by A17,CAT_1:5;
A28: ( dom ( t2 . b ) ) = ( F2 . b ) by A9,CAT_1:5
.= ( cod ( F2 . f ) ) by A18,CAT_1:5;
A29: ( ( t1 . b ) * ( F1 . f ) ) = ( ( t1 . b ) * (( F1 . f ) qua (Morphism of B)) ) by A23,A4,CAT_1:def 10;
A30: ( Hom (( <: F1,F2 :> . b ),( <: G1,G2 :> . b )) ) <> ( {} ) by A14,NATTRA_1:def 2;
( Hom (( <: F1,F2 :> . a ),( <: F1,F2 :> . b )) ) <> ( {} ) by A3,CAT_1:84;
hence ( ( <: t1,t2 :> . b ) * ( <: F1,F2 :> . f ) ) = ( ( <: t1,t2 :> . b ) * (( <: F1,F2 :> . f ) qua (Morphism of [: B,C :])) ) by A30,CAT_1:def 10
.= [ ( ( G1 . f ) * ( t1 . a ) ),( ( G2 . f ) * ( t2 . a ) ) ] by A5,A11,A7,A24,A28,A29,A19,CAT_2:29
.= ( ( <: G1,G2 :> . f ) * (( <: t1,t2 :> . a ) qua (Morphism of [: B,C :])) ) by A26,A21,A27,A22,A6,A12,CAT_2:29
.= ( ( <: G1,G2 :> . f ) * ( <: t1,t2 :> . a ) ) by A15,A16,CAT_1:def 10;
end;
