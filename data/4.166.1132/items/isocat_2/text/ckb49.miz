theorem
Th42: (for F being (Functor of A,B) holds (for G being (Functor of A,C) holds (for a,b being (Object of A) holds (( Hom (a,b) ) <> ( {} ) implies (for f being (Morphism of a,b) holds ( <: F,G :> . f ) = [ ( F . f ),( G . f ) ])))))
proof
let F being (Functor of A,B);
let G being (Functor of A,C);
let a being (Object of A);
let b being (Object of A);
assume that
A1: ( Hom (a,b) ) <> ( {} );
let f being (Morphism of a,b);
reconsider g = f as (Morphism of A);
thus ( <: F,G :> . f ) = ( <: F,G :> . g ) by A1,NATTRA_1:def 1
.= [ ( F . g ),( G . g ) ] by FUNCT_3:59
.= [ ( F . g ),( G . f ) ] by A1,NATTRA_1:def 1
.= [ ( F . f ),( G . f ) ] by A1,NATTRA_1:def 1;
end;
