begin
reserve A for Category;
reserve B for Category;
reserve C for Category;
reserve F for (Functor of A,B);
theorem
Th12: (for F being (Functor of [: A,B :],C) holds (for a being (Object of A) holds (for b being (Object of B) holds ( ( F ?- a ) . b ) = ( F . [ a,b ] ))))
proof
let F being (Functor of [: A,B :],C);
let a being (Object of A);
let b being (Object of B);
thus ( ( F ?- a ) . b ) = ( ( Obj F ) . (a,b) ) by CAT_2:37
.= ( F . [ a,b ] );
end;
