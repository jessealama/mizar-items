begin
reserve A for Category;
reserve B for Category;
reserve C for Category;
reserve F for (Functor of A,B);
theorem
Th36: (for F being (Functor of A,B) holds (for G being (Functor of A,C) holds (( Pr1 <: F,G :> ) = F & ( Pr2 <: F,G :> ) = G)))
proof
let F being (Functor of A,B);
let G being (Functor of A,C);
thus ( Pr1 <: F,G :> ) = ( ( pr1 ((the carrier' of B),(the carrier' of C)) ) * <: F,G :> ) by CAT_2:def 10
.= F by FUNCT_3:62;
thus ( Pr2 <: F,G :> ) = ( ( pr2 ((the carrier' of B),(the carrier' of C)) ) * <: F,G :> ) by CAT_2:def 11
.= G by FUNCT_3:62;
end;
