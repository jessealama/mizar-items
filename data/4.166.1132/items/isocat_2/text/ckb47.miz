requirements SUBSET,BOOLE;
begin
reserve A for Category;
reserve B for Category;
reserve C for Category;
reserve F for (Functor of A,B);
reserve F1 for (Functor of A,B);
reserve t for (natural_transformation of F,F1);
theorem
Th39: (for F1,F2,G1,G2 being (Functor of A,[: B,C :]) holds ((F1 is_naturally_transformable_to F2 & G1 is_naturally_transformable_to G2) implies (for s being (natural_transformation of F1,F2) holds (for t being (natural_transformation of G1,G2) holds ((( Pr1 s ) = ( Pr1 t ) & ( Pr2 s ) = ( Pr2 t )) implies s = t)))))
proof
let F1 being (Functor of A,[: B,C :]);
let F2 being (Functor of A,[: B,C :]);
let G1 being (Functor of A,[: B,C :]);
let G2 being (Functor of A,[: B,C :]);
assume that
A1: F1 is_naturally_transformable_to F2
and
A2: G1 is_naturally_transformable_to G2;
let s being (natural_transformation of F1,F2);
let t being (natural_transformation of G1,G2);
assume that
A3: ( Pr1 s ) = ( Pr1 t )
and
A4: ( Pr2 s ) = ( Pr2 t );
reconsider S = s,T = t as (Function of (the carrier of A),[: (the carrier' of B),(the carrier' of C) :]) by CAT_2:23;
A5: ( ( pr2 ((the carrier' of B),(the carrier' of C)) ) * S ) = ( ( pr2 (B,C) ) * S ) by CAT_2:def 11
.= ( Pr2 s ) by A1,CKB39:1
.= ( ( pr2 (B,C) ) * T ) by A2,A4,CKB39:1
.= ( ( pr2 ((the carrier' of B),(the carrier' of C)) ) * T ) by CAT_2:def 11;
( ( pr1 ((the carrier' of B),(the carrier' of C)) ) * S ) = ( ( pr1 (B,C) ) * S ) by CAT_2:def 10
.= ( Pr1 s ) by A1,CKB39:1
.= ( ( pr1 (B,C) ) * T ) by A2,A3,CKB39:1
.= ( ( pr1 ((the carrier' of B),(the carrier' of C)) ) * T ) by CAT_2:def 10;
hence thesis by A5,CKB5:1;
end;
