begin
theorem
(for X being set holds (for f,g being Function holds ((X c= ( dom f ) & f c= g) implies ( f | X ) = ( g | X ))))
proof
let X being set;
let f being Function,g being Function;
assume that
A1: X c= ( dom f );
assume f c= g;
hence ( f | X ) = ( ( g | ( dom f ) ) | X ) by CKB27:1
.= ( g | ( ( dom f ) /\ X ) ) by RELAT_1:71
.= ( g | X ) by A1,XBOOLE_1:28;
end;
