constructors REAL_1,FDIFF_1,VFUNCT_1,NDIFF_1,SEQ_1,RELSET_1,RVSUM_1,BINOP_2,INTEGR15,NAT_D,MEASURE6,NFCONT_3,CKB3,CKB6,CKB15,CKB16,CKB17,CKB21;
requirements SUBSET,REAL,BOOLE,NUMERALS,ARITHM;
begin
reserve F for non  trivial RealNormSpace;
reserve x for Real;
reserve x0 for Real;
reserve r for Real;
reserve p for Real;
reserve n for (Element of ( NAT ));
reserve Z for  open (Subset of ( REAL ));
reserve f for (PartFunc of ( REAL ),(the carrier of F));
reserve h for  convergent_to_0 Real_Sequence;
reserve L for (LINEAR of F);
theorem
Th21: (for r,p being (Point of F) holds (for Z holds (for f holds ((Z c= ( dom f ) & (for x holds (x in Z implies ( f /. x ) = ( ( x * r ) + p )))) implies (f is_differentiable_on Z & (for x holds (x in Z implies ( ( f `| Z ) . x ) = r)))))))
proof
let r being (Point of F);
let p being (Point of F);
let Z;
let f;
set R = ( ( REAL ) --> ( 0. F ) );
defpred P[ set ]
 means
$1 in ( REAL );
A1: ( dom R ) = ( REAL ) by FUNCOP_1:13;
now
let h;
now
let n being Nat;
A2: ( rng h ) c= ( dom R ) by A1;
A3: n in ( NAT ) by ORDINAL1:def 12;
hence ( ( ( h " ) (#) ( R /* h ) ) . n ) = ( ( ( h " ) . n ) * ( ( R /* h ) . n ) ) by NDIFF_1:def 2
.= ( ( ( h " ) . n ) * ( R /. ( h . n ) ) ) by A3,A2,FUNCT_2:108
.= ( 0. F ) by FUNCOP_1:7,RLVECT_1:10;
end;
then (( ( h " ) (#) ( R /* h ) ) is  constant & ( ( ( h " ) (#) ( R /* h ) ) . ( 0 ) ) = ( 0. F )) by VALUED_0:def 18;
hence (( ( h " ) (#) ( R /* h ) ) is  convergent & ( lim ( ( h " ) (#) ( R /* h ) ) ) = ( 0. F )) by NDIFF_1:18;
end;
then reconsider R as (REST of F) by CKB3:def 1;
assume that
A4: Z c= ( dom f )
and
A5: (for x holds (x in Z implies ( f /. x ) = ( ( x * r ) + p )));
deffunc G(Real) = ( $1 * r );
consider L being (PartFunc of ( REAL ),(the carrier of F)) such that A6: ((for x holds (x in ( dom L ) iff P[ x ])) & (for x holds (x in ( dom L ) implies ( L . x ) = G(x)))) from SEQ_1:sch 3;
( dom L ) = ( REAL ) by A6,FDIFF_1:1;
then A7: L is  total by PARTFUN1:def 2;
A8:now
let x;
x in ( dom L ) by A6;
hence ( L . x ) = ( x * r ) by A6;
end;
then reconsider L as (LINEAR of F) by A7,CKB6:def 1;
A9:now
let x0;
assume A10: x0 in Z;
then consider N being (Neighbourhood of x0) such that A11: N c= Z by RCOMP_1:18;
A12: (for x holds (x in N implies ( ( f /. x ) - ( f /. x0 ) ) = ( ( L . ( x - x0 ) ) + ( R /. ( x - x0 ) ) )))
proof
let x being Real;
A13: ( R /. ( x - x0 ) ) = ( R . ( x - x0 ) ) by A1,PARTFUN1:def 6
.= ( 0. F ) by FUNCOP_1:7;
assume x in N;
hence ( ( f /. x ) - ( f /. x0 ) ) = ( ( ( x * r ) + p ) - ( f /. x0 ) ) by A5,A11
.= ( ( ( x * r ) + p ) - ( ( x0 * r ) + p ) ) by A5,A10
.= ( ( ( ( x * r ) + p ) - ( x0 * r ) ) - p ) by RLVECT_1:27
.= ( ( p + ( ( x * r ) - ( x0 * r ) ) ) - p ) by RLVECT_1:28
.= ( ( ( x * r ) - ( x0 * r ) ) + ( p - p ) ) by RLVECT_1:28
.= ( ( ( x * r ) - ( x0 * r ) ) + ( 0. F ) ) by RLVECT_1:15
.= ( ( ( x - x0 ) * r ) + ( 0. F ) ) by RLVECT_1:35
.= ( ( L . ( x - x0 ) ) + ( R /. ( x - x0 ) ) ) by A13,A8;
end;
N c= ( dom f ) by A4,A11,XBOOLE_1:1;
hence f is_differentiable_in x0 by A12,CKB15:def 1;
end;
hence A14: f is_differentiable_on Z by A4,CKB19:1;
let x0;
assume A15: x0 in Z;
then consider N being (Neighbourhood of x0) such that A16: N c= Z by RCOMP_1:18;
A17: (for x holds (x in N implies ( ( f /. x ) - ( f /. x0 ) ) = ( ( L . ( x - x0 ) ) + ( R /. ( x - x0 ) ) )))
proof
let x;
A18: ( R /. ( x - x0 ) ) = ( R . ( x - x0 ) ) by A1,PARTFUN1:def 6
.= ( 0. F ) by FUNCOP_1:7;
assume x in N;
hence ( ( f /. x ) - ( f /. x0 ) ) = ( ( ( x * r ) + p ) - ( f /. x0 ) ) by A5,A16
.= ( ( ( x * r ) + p ) - ( ( x0 * r ) + p ) ) by A5,A15
.= ( ( ( ( x * r ) + p ) - ( x0 * r ) ) - p ) by RLVECT_1:27
.= ( ( p + ( ( x * r ) - ( x0 * r ) ) ) - p ) by RLVECT_1:28
.= ( ( ( x * r ) - ( x0 * r ) ) + ( p - p ) ) by RLVECT_1:28
.= ( ( ( x * r ) - ( x0 * r ) ) + ( 0. F ) ) by RLVECT_1:15
.= ( ( ( x - x0 ) * r ) + ( 0. F ) ) by RLVECT_1:35
.= ( ( L . ( x - x0 ) ) + ( R /. ( x - x0 ) ) ) by A18,A8;
end;
A19: N c= ( dom f ) by A4,A16,XBOOLE_1:1;
A20: f is_differentiable_in x0 by A9,A15;
thus ( ( f `| Z ) . x0 ) = ( diff (f,x0) ) by A14,A15,CKB21:def 1
.= ( L . 1 ) by A20,A19,A17,CKB16:def 1
.= ( 1 * r ) by A8
.= r by RLVECT_1:def 8;
end;
