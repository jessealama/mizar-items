definitions TARSKI,STRUCT_0,XBOOLE_0,SUBSET_1,XBOOLEAN,FIN_TOPO,CKB6,CKB7,CKB11,CKB15,CKB19,CKB26,CKB27,CKB28,CKB29,CKB31,CKB33,CKB35,CKB37;
theorems TARSKI,SUBSET_1,FIN_TOPO,FUNCT_2,XBOOLE_0,XBOOLE_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB37,CKB38,CKB39,CKB40,CKB41,CKB42,CKB43,CKB44,CKB45,CKB46,CKB47;
schemes DOMAIN_1,FUNCT_2;
registrations XBOOLE_0,SUBSET_1,RELSET_1,STRUCT_0,PRE_TOPC,FIN_TOPO,CKB24,CKB25;
constructors DOMAIN_1,MARGREL1,PRE_TOPC,FIN_TOPO,RELSET_1,CKB6,CKB7,CKB11,CKB15,CKB19,CKB24,CKB26,CKB27,CKB28,CKB29,CKB31,CKB33,CKB35,CKB37;
requirements SUBSET,BOOLE;
begin
reserve FMT for non  empty FMT_Space_Str;
reserve W for (Subset of FMT);
theorem
(for FMT being non  empty FMT_Space_Str holds (FMT is  Fo_filled implies ((for A,B being (Subset of FMT) holds ( ( A \/ B ) ^Fodelta ) = ( ( A ^Fodelta ) \/ ( B ^Fodelta ) )) implies (for x being (Element of FMT) holds (for V1,V2 being (Subset of FMT) holds ((V1 in ( U_FMT x ) & V2 in ( U_FMT x )) implies (ex W being (Subset of FMT) st (W in ( U_FMT x ) & W c= ( V1 /\ V2 )))))))))
proof
let FMT being non  empty FMT_Space_Str;
assume A1: FMT is  Fo_filled;
((ex x being (Element of FMT) st (ex V1,V2 being (Subset of FMT) st ((V1 in ( U_FMT x ) & V2 in ( U_FMT x )) & (for W being (Subset of FMT) holds (W in ( U_FMT x ) implies (not W c= ( V1 /\ V2 ))))))) implies (ex A,B being (Subset of FMT) st ( ( A \/ B ) ^Fodelta ) <> ( ( A ^Fodelta ) \/ ( B ^Fodelta ) )))
proof
given x0 being (Element of FMT),V1 being (Subset of FMT),V2 being (Subset of FMT) such that
A2: V1 in ( U_FMT x0 )
and
A3: V2 in ( U_FMT x0 )
and
A4: (for W being (Subset of FMT) holds (W in ( U_FMT x0 ) implies (not W c= ( V1 /\ V2 ))));

take ( V1 ` );
take ( V2 ` );
A5: (not x0 in ( ( V2 ` ) ^Fodelta ))
proof
assume x0 in ( ( V2 ` ) ^Fodelta );
then V2 meets ( V2 ` ) by A3,CKB30:1;
hence contradiction by XBOOLE_1:79;
end;
(for W being (Subset of FMT) holds (W in ( U_FMT x0 ) implies (W meets ( ( V1 ` ) \/ ( V2 ` ) ) & W meets ( ( ( V1 ` ) \/ ( V2 ` ) ) ` ))))
proof
let W being (Subset of FMT);
assume A6: W in ( U_FMT x0 );
then A7: (not W c= ( V1 /\ V2 )) by A4;
A8: W meets ( ( V1 /\ V2 ) ` )
proof
assume ( W /\ ( ( V1 /\ V2 ) ` ) ) = ( {} );
then ( W \ ( V1 /\ V2 ) ) = ( {} ) by SUBSET_1:13;
hence contradiction by A7,XBOOLE_1:37;
end;
(x0 in V1 & x0 in V2) by A1,A2,A3,CKB28:def 1;
then A9: x0 in ( V1 /\ V2 ) by XBOOLE_0:def 4;
x0 in W by A1,A6,CKB28:def 1;
then ( W /\ ( ( ( V1 /\ V2 ) ` ) ` ) ) <> ( {} ) by A9,XBOOLE_0:def 4;
then W meets ( ( ( V1 /\ V2 ) ` ) ` ) by XBOOLE_0:def 7;
hence thesis by A8,XBOOLE_1:54;
end;
then A10: x0 in ( ( ( V1 ` ) \/ ( V2 ` ) ) ^Fodelta );
(not x0 in ( ( V1 ` ) ^Fodelta ))
proof
assume x0 in ( ( V1 ` ) ^Fodelta );
then V1 meets ( V1 ` ) by A2,CKB30:1;
hence contradiction by XBOOLE_1:79;
end;
hence thesis by A10,A5,XBOOLE_0:def 3;
end;
hence thesis;
end;
