Th26: (for FMT being non  empty FMT_Space_Str holds (for A,B being (Subset of FMT) holds (A c= B implies ( A ^Foi ) c= ( B ^Foi ))))
proof
let FMT being non  empty FMT_Space_Str;
let A being (Subset of FMT);
let B being (Subset of FMT);
assume A1: A c= B;
