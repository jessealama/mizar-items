environ
vocabularies XBOOLE_0,ORDERS_2,SUBSET_1,TARSKI,FIN_TOPO,RCOMP_1,MARGREL1,XBOOLEAN,STRUCT_0,FUNCT_1,ZFMISC_1,PRE_TOPC,SETFAM_1,FINTOPO2;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,DOMAIN_1,STRUCT_0,FUNCT_1,FUNCT_2,FIN_TOPO,ORDERS_2,PRE_TOPC,MARGREL1,CKB6,CKB7,CKB11,CKB15,CKB19,CKB24,CKB26,CKB27,CKB28,CKB29,CKB31,CKB33,CKB35,CKB37;
definitions TARSKI,STRUCT_0,XBOOLE_0,SUBSET_1,XBOOLEAN,FIN_TOPO,CKB6,CKB7,CKB11,CKB15,CKB19,CKB26,CKB27,CKB28,CKB29,CKB31,CKB33,CKB35,CKB37;
theorems TARSKI,SUBSET_1,FIN_TOPO,FUNCT_2,XBOOLE_0,XBOOLE_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB37,CKB38,CKB39,CKB40,CKB41,CKB42,CKB43,CKB44,CKB45;
schemes DOMAIN_1,FUNCT_2;
registrations XBOOLE_0,SUBSET_1,RELSET_1,STRUCT_0,PRE_TOPC,FIN_TOPO,CKB24,CKB25;
constructors DOMAIN_1,MARGREL1,PRE_TOPC,FIN_TOPO,RELSET_1,CKB6,CKB7,CKB11,CKB15,CKB19,CKB24,CKB26,CKB27,CKB28,CKB29,CKB31,CKB33,CKB35,CKB37;
requirements SUBSET,BOOLE;
begin
reserve FMT for non  empty FMT_Space_Str;
reserve x for (Element of FMT);
reserve A for (Subset of FMT);
reserve B for (Subset of FMT);
reserve W for (Subset of FMT);
theorem
(for FMT being non  empty FMT_Space_Str holds ((for x being (Element of FMT) holds (for V1,V2 being (Subset of FMT) holds ((V1 in ( U_FMT x ) & V2 in ( U_FMT x )) implies (ex W being (Subset of FMT) st (W in ( U_FMT x ) & W c= ( V1 /\ V2 )))))) iff (for A,B being (Subset of FMT) holds ( ( A ^Foi ) /\ ( B ^Foi ) ) = ( ( A /\ B ) ^Foi ))))
proof
let FMT being non  empty FMT_Space_Str;
thus ((for x being (Element of FMT) holds (for V1,V2 being (Subset of FMT) holds ((V1 in ( U_FMT x ) & V2 in ( U_FMT x )) implies (ex W being (Subset of FMT) st (W in ( U_FMT x ) & W c= ( V1 /\ V2 )))))) implies (for A,B being (Subset of FMT) holds ( ( A ^Foi ) /\ ( B ^Foi ) ) = ( ( A /\ B ) ^Foi )))
proof
assume A1: (for x being (Element of FMT) holds (for V1,V2 being (Subset of FMT) holds ((V1 in ( U_FMT x ) & V2 in ( U_FMT x )) implies (ex W being (Subset of FMT) st (W in ( U_FMT x ) & W c= ( V1 /\ V2 ))))));
let A being (Subset of FMT);
let B being (Subset of FMT);
(for x being (Element of FMT) holds (x in ( ( A ^Foi ) /\ ( B ^Foi ) ) iff x in ( ( A /\ B ) ^Foi )))
proof
let x being (Element of FMT);
A2: (x in ( ( A ^Foi ) /\ ( B ^Foi ) ) implies x in ( ( A /\ B ) ^Foi ))
proof
assume A3: x in ( ( A ^Foi ) /\ ( B ^Foi ) );
then x in ( B ^Foi ) by XBOOLE_0:def 4;
then A4: (ex W2 being (Subset of FMT) st (W2 in ( U_FMT x ) & W2 c= B)) by CKB34:1;
x in ( A ^Foi ) by A3,XBOOLE_0:def 4;
then (ex W1 being (Subset of FMT) st (W1 in ( U_FMT x ) & W1 c= A)) by CKB34:1;
then consider W1,W2 being (Subset of FMT) such that A5: (W1 in ( U_FMT x ) & W2 in ( U_FMT x )) and A6: W1 c= A and A7: W2 c= B by A4;
consider W being (Subset of FMT) such that A8: W in ( U_FMT x ) and A9: W c= ( W1 /\ W2 ) by A1,A5;
( W1 /\ W2 ) c= W2 by XBOOLE_1:17;
then W c= W2 by A9,XBOOLE_1:1;
then A10: W c= B by A7,XBOOLE_1:1;
( W1 /\ W2 ) c= W1 by XBOOLE_1:17;
then W c= W1 by A9,XBOOLE_1:1;
then W c= A by A6,XBOOLE_1:1;
then W c= ( A /\ B ) by A10,XBOOLE_1:19;
hence thesis by A8;
end;
(x in ( ( A /\ B ) ^Foi ) implies x in ( ( A ^Foi ) /\ ( B ^Foi ) ))
proof
assume A11: x in ( ( A /\ B ) ^Foi );
( ( A /\ B ) ^Foi ) c= ( ( A ^Foi ) /\ ( B ^Foi ) ) by CKB44:1;
hence thesis by A11;
end;
hence thesis by A2;
end;
hence ( ( A ^Foi ) /\ ( B ^Foi ) ) = ( ( A /\ B ) ^Foi ) by SUBSET_1:3;
end;

((ex x being (Element of FMT) st (ex V1,V2 being (Subset of FMT) st ((V1 in ( U_FMT x ) & V2 in ( U_FMT x )) & (for W being (Subset of FMT) holds (W in ( U_FMT x ) implies (not W c= ( V1 /\ V2 ))))))) implies (ex A,B being (Subset of FMT) st ( ( A ^Foi ) /\ ( B ^Foi ) ) <> ( ( A /\ B ) ^Foi )))
proof
given x0 being (Element of FMT),V1 being (Subset of FMT),V2 being (Subset of FMT) such that
A12: (V1 in ( U_FMT x0 ) & V2 in ( U_FMT x0 ))
and
A13: (for W being (Subset of FMT) holds (W in ( U_FMT x0 ) implies (not W c= ( V1 /\ V2 ))));

take V1;
take V2;
(x0 in ( V1 ^Foi ) & x0 in ( V2 ^Foi )) by A12;
then x0 in ( ( V1 ^Foi ) /\ ( V2 ^Foi ) ) by XBOOLE_0:def 4;
hence thesis by A13,CKB34:1;
end;
hence ((for A,B being (Subset of FMT) holds ( ( A ^Foi ) /\ ( B ^Foi ) ) = ( ( A /\ B ) ^Foi )) implies (for x being (Element of FMT) holds (for V1,V2 being (Subset of FMT) holds ((V1 in ( U_FMT x ) & V2 in ( U_FMT x )) implies (ex W being (Subset of FMT) st (W in ( U_FMT x ) & W c= ( V1 /\ V2 )))))));
end;
