environ
vocabularies XBOOLE_0,ORDERS_2,SUBSET_1,TARSKI,FIN_TOPO,RCOMP_1,MARGREL1,XBOOLEAN,STRUCT_0,FUNCT_1,ZFMISC_1,PRE_TOPC,SETFAM_1,FINTOPO2;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,DOMAIN_1,STRUCT_0,FUNCT_1,FUNCT_2,FIN_TOPO,ORDERS_2,PRE_TOPC,MARGREL1,CKB6,CKB7,CKB11,CKB15,CKB19,CKB24,CKB26,CKB27,CKB28,CKB29,CKB31,CKB33,CKB35,CKB37;
definitions TARSKI,STRUCT_0,XBOOLE_0,SUBSET_1,XBOOLEAN,FIN_TOPO,CKB6,CKB7,CKB11,CKB15,CKB19,CKB26,CKB27,CKB28,CKB29,CKB31,CKB33,CKB35,CKB37;
theorems TARSKI,SUBSET_1,FIN_TOPO,FUNCT_2,XBOOLE_0,XBOOLE_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB37,CKB38,CKB39,CKB40,CKB41,CKB42,CKB43,CKB44;
schemes DOMAIN_1,FUNCT_2;
registrations XBOOLE_0,SUBSET_1,RELSET_1,STRUCT_0,PRE_TOPC,FIN_TOPO,CKB24,CKB25;
constructors DOMAIN_1,MARGREL1,PRE_TOPC,FIN_TOPO,RELSET_1,CKB6,CKB7,CKB11,CKB15,CKB19,CKB24,CKB26,CKB27,CKB28,CKB29,CKB31,CKB33,CKB35,CKB37;
requirements SUBSET,BOOLE;
begin
theorem
(for FMT being non empty FMT_Space_Str holds ((for x being (Element of FMT) holds (for V1,V2 being (Subset of FMT) holds ((V1 in ( U_FMT x ) & V2 in ( U_FMT x )) implies (ex W being (Subset of FMT) st (W in ( U_FMT x ) & W c= ( V1 /\ V2 )))))) iff (for A,B being (Subset of FMT) holds ( ( A \/ B ) ^Fob ) = ( ( A ^Fob ) \/ ( B ^Fob ) ))))
proof
let FMT being non empty FMT_Space_Str;
A1: ((for x being (Element of FMT) holds (for V1,V2 being (Subset of FMT) holds ((V1 in ( U_FMT x ) & V2 in ( U_FMT x )) implies (ex W being (Subset of FMT) st (W in ( U_FMT x ) & W c= ( V1 /\ V2 )))))) implies (for A,B being (Subset of FMT) holds ( ( A \/ B ) ^Fob ) = ( ( A ^Fob ) \/ ( B ^Fob ) )))
proof
assume A2: (for x being (Element of FMT) holds (for V1,V2 being (Subset of FMT) holds ((V1 in ( U_FMT x ) & V2 in ( U_FMT x )) implies (ex W being (Subset of FMT) st (W in ( U_FMT x ) & W c= ( V1 /\ V2 ))))));
let A being (Subset of FMT),B being (Subset of FMT);
(for x being (Element of FMT) holds (x in ( ( A \/ B ) ^Fob ) iff x in ( ( A ^Fob ) \/ ( B ^Fob ) )))
proof
let x being (Element of FMT);
A3: (x in ( ( A \/ B ) ^Fob ) implies x in ( ( A ^Fob ) \/ ( B ^Fob ) ))
proof
assume A4: x in ( ( A \/ B ) ^Fob );
A5: (for W1 being (Subset of FMT) holds (W1 in ( U_FMT x ) implies (W1 meets A or W1 meets B)))
proof
let W1 being (Subset of FMT);
assume W1 in ( U_FMT x );
then W1 meets ( A \/ B ) by A4,CKB32:1;
then ( W1 /\ ( A \/ B ) ) <> ( {} ) by XBOOLE_0:def 7;
then ( ( W1 /\ A ) \/ ( W1 /\ B ) ) <> ( {} ) by XBOOLE_1:23;
then (( W1 /\ A ) <> ( {} ) or ( W1 /\ B ) <> ( {} ));
hence thesis by XBOOLE_0:def 7;
end;
(for V1,V2 being (Subset of FMT) holds ((V1 in ( U_FMT x ) & V2 in ( U_FMT x )) implies (V1 meets A or V2 meets B)))
proof
let V1 being (Subset of FMT),V2 being (Subset of FMT);
assume (V1 in ( U_FMT x ) & V2 in ( U_FMT x ));
then consider W being (Subset of FMT) such that A6: W in ( U_FMT x ) and A7: W c= ( V1 /\ V2 ) by A2;
( V1 /\ V2 ) c= V1 by XBOOLE_1:17;
then W c= V1 by A7,XBOOLE_1:1;
then A8: ( W /\ A ) c= ( V1 /\ A ) by XBOOLE_1:26;
( V1 /\ V2 ) c= V2 by XBOOLE_1:17;
then W c= V2 by A7,XBOOLE_1:1;
then A9: ( W /\ B ) c= ( V2 /\ B ) by XBOOLE_1:26;
(W meets A or W meets B) by A5,A6;
then (( W /\ A ) <> ( {} ) or ( W /\ B ) <> ( {} )) by XBOOLE_0:def 7;
then (ex z1,z2 being (Element of FMT) st (z1 in ( W /\ A ) or z2 in ( W /\ B ))) by SUBSET_1:4;
hence thesis by A8,A9,XBOOLE_0:def 7;
end;
then ((for V1 being (Subset of FMT) holds (V1 in ( U_FMT x ) implies V1 meets A)) or (for V2 being (Subset of FMT) holds (V2 in ( U_FMT x ) implies V2 meets B)));
then (x in ( A ^Fob ) or x in ( B ^Fob ));
hence thesis by XBOOLE_0:def 3;
end;
(x in ( ( A ^Fob ) \/ ( B ^Fob ) ) implies x in ( ( A \/ B ) ^Fob ))
proof
assume A10: x in ( ( A ^Fob ) \/ ( B ^Fob ) );
( ( A ^Fob ) \/ ( B ^Fob ) ) c= ( ( A \/ B ) ^Fob ) by CKB41:1;
hence thesis by A10;
end;
hence thesis by A3;
end;
hence thesis by SUBSET_1:3;
end;
((ex x being (Element of FMT) st (ex V1,V2 being (Subset of FMT) st ((V1 in ( U_FMT x ) & V2 in ( U_FMT x )) & (for W being (Subset of FMT) holds (W in ( U_FMT x ) implies (not W c= ( V1 /\ V2 ))))))) implies (ex A,B being (Subset of FMT) st ( ( A \/ B ) ^Fob ) <> ( ( A ^Fob ) \/ ( B ^Fob ) )))
proof
given x0 being (Element of FMT),V1 being (Subset of FMT),V2 being (Subset of FMT) such that
A11: V1 in ( U_FMT x0 )
and
A12: V2 in ( U_FMT x0 )
and
A13: (for W being (Subset of FMT) holds (W in ( U_FMT x0 ) implies (not W c= ( V1 /\ V2 ))));

A14: (not x0 in ( ( V2 ` ) ^Fob ))
proof
A15: V2 misses ( V2 ` ) by XBOOLE_1:79;
assume x0 in ( ( V2 ` ) ^Fob );
hence contradiction by A12,A15,CKB32:1;
end;
take ( V1 ` );
take ( V2 ` );
(for W being (Subset of FMT) holds (W in ( U_FMT x0 ) implies W meets ( ( V1 ` ) \/ ( V2 ` ) )))
proof
let W being (Subset of FMT);
assume W in ( U_FMT x0 );
then A16: (not W c= ( V1 /\ V2 )) by A13;
( W /\ ( ( V1 /\ V2 ) ` ) ) <> ( {} )
proof
assume ( W /\ ( ( V1 /\ V2 ) ` ) ) = ( {} );
then ( W \ ( V1 /\ V2 ) ) = ( {} ) by SUBSET_1:13;
hence contradiction by A16,XBOOLE_1:37;
end;
hence ( W /\ ( ( V1 ` ) \/ ( V2 ` ) ) ) <> ( {} ) by XBOOLE_1:54;
end;
then A17: x0 in ( ( ( V1 ` ) \/ ( V2 ` ) ) ^Fob );
(not x0 in ( ( V1 ` ) ^Fob ))
proof
A18: V1 misses ( V1 ` ) by XBOOLE_1:79;
assume x0 in ( ( V1 ` ) ^Fob );
hence contradiction by A11,A18,CKB32:1;
end;
hence thesis by A17,A14,XBOOLE_0:def 3;
end;
hence thesis by A1;
end;
