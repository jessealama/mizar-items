theorem
Th25: (for FMT being non empty FMT_Space_Str holds (for A,B being (Subset of FMT) holds (A c= B implies ( A ^Fob ) c= ( B ^Fob ))))
proof
let FMT being non empty FMT_Space_Str;
let A being (Subset of FMT),B being (Subset of FMT);
assume A1: A c= B;
let x being set;
assume A2: x in ( A ^Fob );
then reconsider y = x as (Element of FMT);
(for W being (Subset of FMT) holds (W in ( U_FMT y ) implies W meets B))
proof
let W being (Subset of FMT);
assume W in ( U_FMT y );
then W meets A by A2,CKB32:1;
then (ex z being set st (z in W & z in A)) by XBOOLE_0:3;
hence ( W /\ B ) <> ( {} ) by A1,XBOOLE_0:def 4;
end;
hence thesis;
end;
