environ
vocabularies XBOOLE_0,ORDERS_2,SUBSET_1,TARSKI,FIN_TOPO,RCOMP_1,MARGREL1,XBOOLEAN,STRUCT_0,FUNCT_1,ZFMISC_1,PRE_TOPC,SETFAM_1,FINTOPO2;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,DOMAIN_1,STRUCT_0,FUNCT_1,FUNCT_2,FIN_TOPO,ORDERS_2,PRE_TOPC,MARGREL1,CKB6,CKB7,CKB11,CKB15,CKB19;
definitions TARSKI,STRUCT_0,XBOOLE_0,SUBSET_1,XBOOLEAN,FIN_TOPO,CKB6,CKB7,CKB11,CKB15,CKB19;
theorems TARSKI,SUBSET_1,FIN_TOPO,FUNCT_2,XBOOLE_0,XBOOLE_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21;
schemes DOMAIN_1,FUNCT_2;
registrations XBOOLE_0,SUBSET_1,RELSET_1,STRUCT_0,PRE_TOPC,FIN_TOPO;
constructors DOMAIN_1,MARGREL1,PRE_TOPC,FIN_TOPO,RELSET_1,CKB6,CKB7,CKB11,CKB15,CKB19;
requirements SUBSET,BOOLE;
begin
reserve FT for non empty RelStr;
theorem
(for x being (Element of FT) holds (for A being (Subset of FT) holds (x in ( A ^n ) iff (( P_A (x,A) ) = ( TRUE ) & (ex y being (Element of FT) st (( P_1 (x,y,A) ) = ( TRUE ) & ( P_e (x,y) ) = ( FALSE )))))))
proof
let x being (Element of FT);
let A being (Subset of FT);
A1: (x in ( A ^n ) implies (( P_A (x,A) ) = ( TRUE ) & (ex y being (Element of FT) st (( P_1 (x,y,A) ) = ( TRUE ) & ( P_e (x,y) ) = ( FALSE )))))
proof
assume A2: x in ( A ^n );
then ( ( U_FT x ) \ { x } ) meets A by FIN_TOPO:10;
then ( ( ( U_FT x ) \ { x } ) /\ A ) <> ( {} ) by XBOOLE_0:def 7;
then consider y being (Element of FT) such that A3: y in ( ( ( U_FT x ) \ { x } ) /\ A ) by SUBSET_1:4;
A4: y in ( ( U_FT x ) \ { x } ) by A3,XBOOLE_0:def 4;
then A5: y in ( U_FT x ) by XBOOLE_0:def 5;
(not y in { x }) by A4,XBOOLE_0:def 5;
then (not x = y) by TARSKI:def 1;
then A6: ( P_e (x,y) ) = ( FALSE ) by CKB19:def 1;
y in A by A3,XBOOLE_0:def 4;
then A7: ( P_1 (x,y,A) ) = ( TRUE ) by A5,CKB6:def 1;
x in A by A2,FIN_TOPO:10;
hence thesis by A6,A7,CKB15:def 1;
end;
((( P_A (x,A) ) = ( TRUE ) & (ex y being (Element of FT) st (( P_1 (x,y,A) ) = ( TRUE ) & ( P_e (x,y) ) = ( FALSE )))) implies x in ( A ^n ))
proof
assume that
A8: ( P_A (x,A) ) = ( TRUE )
and
A9: (ex y being (Element of FT) st (( P_1 (x,y,A) ) = ( TRUE ) & ( P_e (x,y) ) = ( FALSE )));
consider y being (Element of FT) such that A10: ( P_1 (x,y,A) ) = ( TRUE ) and A11: ( P_e (x,y) ) = ( FALSE ) by A9;
x <> y by A11,CKB19:def 1;
then A12: (not y in { x }) by TARSKI:def 1;
y in ( U_FT x ) by A10,CKB6:def 1;
then A13: y in ( ( U_FT x ) \ { x } ) by A12,XBOOLE_0:def 5;
y in A by A10,CKB6:def 1;
then A14: ( ( U_FT x ) \ { x } ) meets A by A13,XBOOLE_0:3;
x in A by A8,CKB15:def 1;
hence thesis by A14,FIN_TOPO:10;
end;
hence thesis by A1;
end;
