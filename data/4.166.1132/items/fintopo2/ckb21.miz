environ
vocabularies XBOOLE_0,ORDERS_2,SUBSET_1,TARSKI,FIN_TOPO,RCOMP_1,MARGREL1,XBOOLEAN,STRUCT_0,FUNCT_1,ZFMISC_1,PRE_TOPC,SETFAM_1,FINTOPO2;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,DOMAIN_1,STRUCT_0,FUNCT_1,FUNCT_2,FIN_TOPO,ORDERS_2,PRE_TOPC,MARGREL1,CKB6,CKB7,CKB11,CKB15,CKB19;
definitions TARSKI,STRUCT_0,XBOOLE_0,SUBSET_1,XBOOLEAN,FIN_TOPO,CKB6,CKB7,CKB11,CKB15,CKB19;
theorems TARSKI,SUBSET_1,FIN_TOPO,FUNCT_2,XBOOLE_0,XBOOLE_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20;
schemes DOMAIN_1,FUNCT_2;
registrations XBOOLE_0,SUBSET_1,RELSET_1,STRUCT_0,PRE_TOPC,FIN_TOPO;
constructors DOMAIN_1,MARGREL1,PRE_TOPC,FIN_TOPO,RELSET_1,CKB6,CKB7,CKB11,CKB15,CKB19;
requirements SUBSET,BOOLE;
begin
reserve FT for non empty RelStr;
theorem
(for x being (Element of FT) holds (for A being (Subset of FT) holds (x in ( A ^s ) iff (( P_A (x,A) ) = ( TRUE ) & (not (ex y being (Element of FT) st (( P_1 (x,y,A) ) = ( TRUE ) & ( P_e (x,y) ) = ( FALSE ))))))))
proof
let x being (Element of FT);
let A being (Subset of FT);
A1: (x in ( A ^s ) implies (( P_A (x,A) ) = ( TRUE ) & (not (ex y being (Element of FT) st (( P_1 (x,y,A) ) = ( TRUE ) & ( P_e (x,y) ) = ( FALSE ))))))
proof
assume A2: x in ( A ^s );
then ( ( U_FT x ) \ { x } ) misses A by FIN_TOPO:9;
then A3: ( ( ( U_FT x ) \ { x } ) /\ A ) = ( {} ) by XBOOLE_0:def 7;
A4: (not (ex y being (Element of FT) st (( P_1 (x,y,A) ) = ( TRUE ) & ( P_e (x,y) ) = ( FALSE ))))
proof
given y being (Element of FT) such that
A5: ( P_1 (x,y,A) ) = ( TRUE )
and
A6: ( P_e (x,y) ) = ( FALSE );

(not x = y) by A6,CKB19:def 1;
then A7: (not y in { x }) by TARSKI:def 1;
y in ( U_FT x ) by A5,CKB6:def 1;
then A8: y in ( ( U_FT x ) \ { x } ) by A7,XBOOLE_0:def 5;
y in A by A5,CKB6:def 1;
hence contradiction by A3,A8,XBOOLE_0:def 4;
end;
x in A by A2,FIN_TOPO:9;
hence thesis by A4,CKB15:def 1;
end;
((( P_A (x,A) ) = ( TRUE ) & (not (ex y being (Element of FT) st (( P_1 (x,y,A) ) = ( TRUE ) & ( P_e (x,y) ) = ( FALSE ))))) implies x in ( A ^s ))
proof
assume that
A9: ( P_A (x,A) ) = ( TRUE )
and
A10: (not (ex y being (Element of FT) st (( P_1 (x,y,A) ) = ( TRUE ) & ( P_e (x,y) ) = ( FALSE ))));
(for y being (Element of FT) holds (not y in ( ( ( U_FT x ) \ { x } ) /\ A )))
proof
let y being (Element of FT);
(not (( P_1 (x,y,A) ) = ( TRUE ) & ( P_e (x,y) ) = ( FALSE ))) by A10;
then (not ((y in ( U_FT x ) & (not x = y)) & y in A)) by CKB6:def 1,CKB19:def 1;
then (not ((y in ( U_FT x ) & (not y in { x })) & y in A)) by TARSKI:def 1;
then (not (y in ( ( U_FT x ) \ { x } ) & y in A)) by XBOOLE_0:def 5;
hence thesis by XBOOLE_0:def 4;
end;
then ( ( ( U_FT x ) \ { x } ) /\ A ) = ( {} ) by SUBSET_1:4;
then A11: ( ( U_FT x ) \ { x } ) misses A by XBOOLE_0:def 7;
x in A by A9,CKB15:def 1;
hence thesis by A11;
end;
hence thesis by A1;
end;
