begin
reserve r for  real number;
reserve s for  real number;
theorem
Th9: (for a,b being  real number holds ((r in [. a,b .] & s in [. a,b .]) implies ( ( r + s ) / 2 ) in [. a,b .]))
proof
let a being  real number;
let b being  real number;
assume that
A1: r in [. a,b .]
and
A2: s in [. a,b .];
reconsider a,b,r,s as Real by XREAL_0:def 1;
A3: s <= b by A2,XXREAL_1:1;
r <= b by A1,XXREAL_1:1;
then ( r + s ) <= ( b + b ) by A3,XREAL_1:7;
then A4: ( ( r + s ) / 2 ) <= ( ( b + b ) / 2 ) by XREAL_1:72;
A5: a <= s by A2,XXREAL_1:1;
a <= r by A1,XXREAL_1:1;
then ( a + a ) <= ( r + s ) by A5,XREAL_1:7;
then ( ( a + a ) / 2 ) <= ( ( r + s ) / 2 ) by XREAL_1:72;
hence thesis by A4;
end;
