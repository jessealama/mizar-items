environ
vocabularies ANPROJ_2,SUBSET_1,PARSP_2,AFF_2,INCSP_1;
notations STRUCT_0,COLLSP,ANPROJ_2;
theorems ANPROJ_2,COLLSP,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19;
registrations ANPROJ_2,PROJDES1;
constructors ANPROJ_2;
begin
reserve PCPP for  Pappian CollProjectivePlane;
reserve a1 for (Element of PCPP);
reserve a2 for (Element of PCPP);
reserve a3 for (Element of PCPP);
reserve b1 for (Element of PCPP);
reserve b2 for (Element of PCPP);
reserve b3 for (Element of PCPP);
reserve c1 for (Element of PCPP);
reserve c2 for (Element of PCPP);
reserve c3 for (Element of PCPP);
reserve o for (Element of PCPP);
theorem
Th21: ((((((((((((((((((o <> b1 & a1 <> b1) & o <> b2) & a2 <> b2) & o <> b3) & a3 <> b3) & (not o,a1,a2 is_collinear )) & (not o,a1,a3 is_collinear )) & (not o,a2,a3 is_collinear )) & a1,a2,c3 is_collinear ) & b1,b2,c3 is_collinear ) & a2,a3,c1 is_collinear ) & b2,b3,c1 is_collinear ) & a1,a3,c2 is_collinear ) & b1,b3,c2 is_collinear ) & o,a1,b1 is_collinear ) & o,a2,b2 is_collinear ) & o,a3,b3 is_collinear ) implies c1,c2,c3 is_collinear )
proof
assume that
A1: (((((((o <> b1 & a1 <> b1) & o <> b2) & a2 <> b2) & o <> b3) & a3 <> b3) & (not o,a1,a2 is_collinear )) & (not o,a1,a3 is_collinear ))
and
A2: (not o,a2,a3 is_collinear )
and
A3: (a1,a2,c3 is_collinear  & b1,b2,c3 is_collinear )
and
A4: a2,a3,c1 is_collinear 
and
A5: b2,b3,c1 is_collinear 
and
A6: ((((a1,a3,c2 is_collinear  & b1,b3,c2 is_collinear ) & o,a1,b1 is_collinear ) & o,a2,b2 is_collinear ) & o,a3,b3 is_collinear );
A7: o <> c1 by A2,A4,CKB1:1;
A8: b3,b2,c1 is_collinear  by A5,CKB1:1;
A9: ((not o,a3,a2 is_collinear ) & a3,a2,c1 is_collinear ) by A2,A4,CKB1:1;
now
assume o,c1,c3 is_collinear ;
then A10: c1,c3,o is_collinear  by CKB1:1;
assume (not thesis);
then A11: (not c1,c3,c2 is_collinear ) by CKB1:1;
then (not c1,o,c2 is_collinear ) by A7,A10,CKB6:1;
then (not o,c1,c2 is_collinear ) by CKB1:1;
hence contradiction by A1,A3,A6,A9,A8,A11,CKB19:1;
end;
hence thesis by A1,A2,A3,A4,A5,A6,CKB19:1;
end;
