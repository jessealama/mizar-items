environ
vocabularies ANPROJ_2,SUBSET_1,PARSP_2,AFF_2,INCSP_1;
notations STRUCT_0,COLLSP,ANPROJ_2;
theorems ANPROJ_2,COLLSP,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20;
registrations ANPROJ_2,PROJDES1;
constructors ANPROJ_2;
begin
reserve PCPP for  Pappian CollProjectivePlane;
registration
cluster  Pappian ->  Desarguesian for CollProjectiveSpace;
coherence
proof
let PCPP being CollProjectiveSpace;
assume that
A1: PCPP is  Pappian;
A2:now
assume (not (ex p,p1,q,q1 being (Element of PCPP) st (not (ex r being (Element of PCPP) st (p,p1,r is_collinear  & q,q1,r is_collinear )))));
then PCPP is  Pappian  Pappian CollProjectivePlane by A1,ANPROJ_2:def 14;
then (for o,p1,p2,p3,q1,q2,q3,r1,r2,r3 being (Element of PCPP) holds ((((((((((((((((((o <> q1 & p1 <> q1) & o <> q2) & p2 <> q2) & o <> q3) & p3 <> q3) & (not o,p1,p2 is_collinear )) & (not o,p1,p3 is_collinear )) & (not o,p2,p3 is_collinear )) & p1,p2,r3 is_collinear ) & q1,q2,r3 is_collinear ) & p2,p3,r1 is_collinear ) & q2,q3,r1 is_collinear ) & p1,p3,r2 is_collinear ) & q1,q3,r2 is_collinear ) & o,p1,q1 is_collinear ) & o,p2,q2 is_collinear ) & o,p3,q3 is_collinear ) implies r1,r2,r3 is_collinear )) by CKB20:1;
hence thesis by ANPROJ_2:def 12;
end;
now
assume (ex p,p1,q,q1 being (Element of PCPP) st (not (ex r being (Element of PCPP) st (p,p1,r is_collinear  & q,q1,r is_collinear ))));
then PCPP is  up-3-dimensional  up-3-dimensional CollProjectiveSpace by ANPROJ_2:def 14;
hence thesis;
end;
hence thesis by A2;
end;
end;
