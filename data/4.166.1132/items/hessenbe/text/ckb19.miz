environ
vocabularies ANPROJ_2,SUBSET_1,PARSP_2,AFF_2,INCSP_1;
notations STRUCT_0,COLLSP,ANPROJ_2;
theorems ANPROJ_2,COLLSP,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18;
registrations ANPROJ_2,PROJDES1;
constructors ANPROJ_2;
begin
reserve PCPP for  Pappian CollProjectivePlane;
reserve a1 for (Element of PCPP);
reserve a2 for (Element of PCPP);
reserve a3 for (Element of PCPP);
reserve b1 for (Element of PCPP);
reserve b2 for (Element of PCPP);
reserve b3 for (Element of PCPP);
reserve c1 for (Element of PCPP);
reserve c2 for (Element of PCPP);
reserve c3 for (Element of PCPP);
reserve o for (Element of PCPP);
reserve x for (Element of PCPP);
reserve y for (Element of PCPP);
theorem
Lm3: (((((((((((((((((((o <> b1 & a1 <> b1) & o <> b2) & a2 <> b2) & o <> b3) & a3 <> b3) & (not o,a1,a2 is_collinear )) & (not o,a1,a3 is_collinear )) & (not o,a2,a3 is_collinear )) & (not o,c1,c3 is_collinear )) & a1,a2,c3 is_collinear ) & b1,b2,c3 is_collinear ) & a2,a3,c1 is_collinear ) & b2,b3,c1 is_collinear ) & a1,a3,c2 is_collinear ) & b1,b3,c2 is_collinear ) & o,a1,b1 is_collinear ) & o,a2,b2 is_collinear ) & o,a3,b3 is_collinear ) implies c1,c2,c3 is_collinear )
proof
assume that
A1: o <> b1
and
A2: a1 <> b1
and
A3: o <> b2
and
A4: a2 <> b2
and
A5: o <> b3
and
A6: a3 <> b3
and
A7: (not o,a1,a2 is_collinear )
and
A8: (not o,a1,a3 is_collinear )
and
A9: (not o,a2,a3 is_collinear )
and
A10: (not o,c1,c3 is_collinear )
and
A11: (a1,a2,c3 is_collinear  & b1,b2,c3 is_collinear )
and
A12: a2,a3,c1 is_collinear 
and
A13: b2,b3,c1 is_collinear 
and
A14: (a1,a3,c2 is_collinear  & b1,b3,c2 is_collinear )
and
A15: o,a1,b1 is_collinear 
and
A16: o,a2,b2 is_collinear 
and
A17: o,a3,b3 is_collinear ;
A18: o <> a2 by A7,ANPROJ_2:def 7;
A19: a1 <> a3 by A8,ANPROJ_2:def 7;
A20: (o <> a1 & o <> a3) by A8,ANPROJ_2:def 7;
now
assume that
(not a1,a2,a3 is_collinear )
and
(not b1,b2,b3 is_collinear );
consider x such that A21: a1,a3,x is_collinear  and A22: o,a2,x is_collinear  by ANPROJ_2:def 14;
consider y such that A23: b1,b3,y is_collinear  and A24: o,a2,y is_collinear  by ANPROJ_2:def 14;
assume A25: (not thesis);
A26:now
((not o,a3,a2 is_collinear ) & b3,b2,c1 is_collinear ) by A9,A13,CKB1:1;
then A27: a2 <> c1 by A4,A5,A16,A17,CKB8:1;
(not a2,a3,o is_collinear ) by A9,CKB1:1;
then (not a2,c1,o is_collinear ) by A12,A27,CKB6:1;
then A28: (not o,a2,c1 is_collinear ) by CKB1:1;
A29: b1 <> b3 by A1,A8,A15,A17,CKB7:1;
((not a1,o,a3 is_collinear ) & a1,o,b1 is_collinear ) by A8,A15,CKB1:1;
then (not a1,b1,a3 is_collinear ) by A2,CKB6:1;
then A30: (not a1,a3,b1 is_collinear ) by CKB1:1;
assume that
A31: c1,c3,x is_collinear 
and
A32: c1,c3,y is_collinear ;
(c1 <> c3 & o <> a2) by A7,A10,ANPROJ_2:def 7;
then b1,b3,x is_collinear  by A22,A23,A24,A31,A32,A28,CKB14:1;
then c1,c3,c2 is_collinear  by A14,A19,A21,A31,A29,A30,CKB14:1;
hence contradiction by A25,CKB1:1;
end;
now
A33:now
A34: (not o,b1,b3 is_collinear ) by A1,A5,A8,A15,A17,CKB15:1;
A35: (o,b2,a2 is_collinear  & o,b3,a3 is_collinear ) by A16,A17,CKB1:1;
A36: (o,b2,y is_collinear  & o,b1,a1 is_collinear ) by A15,A16,A18,A24,CKB1:1,CKB2:1;
assume A37: (not c1,c3,y is_collinear );
((not o,b1,b2 is_collinear ) & (not o,b2,b3 is_collinear )) by A1,A3,A5,A7,A9,A15,A16,A17,CKB15:1;
hence contradiction by A2,A4,A6,A10,A11,A12,A13,A14,A18,A20,A23,A25,A37,A36,A35,A34,CKB18:1;
end;
assume ((not c1,c3,x is_collinear ) or (not c1,c3,y is_collinear ));
hence thesis by A1,A2,A3,A4,A5,A6,A7,A8,A9,A10,A11,A12,A13,A14,A15,A16,A17,A21,A22,A33,CKB18:1;
end;
hence thesis by A26;
end;
hence thesis by A1,A3,A7,A8,A9,A11,A12,A13,A14,A15,A16,A17,CKB17:1;
end;
