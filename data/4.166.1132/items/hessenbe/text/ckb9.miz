environ
vocabularies ANPROJ_2,SUBSET_1,PARSP_2,AFF_2,INCSP_1;
notations STRUCT_0,COLLSP,ANPROJ_2;
theorems ANPROJ_2,COLLSP,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8;
registrations ANPROJ_2,PROJDES1;
constructors ANPROJ_2;
begin
reserve PCPP for CollProjectiveSpace;
reserve a1 for (Element of PCPP);
reserve a2 for (Element of PCPP);
reserve a3 for (Element of PCPP);
reserve c1 for (Element of PCPP);
reserve c3 for (Element of PCPP);
reserve x for (Element of PCPP);
theorem
Th12: (((((((((not a1,a2,a3 is_collinear ) & a1,a2,c3 is_collinear ) & a2,a3,c1 is_collinear ) & c1,c3,x is_collinear ) & c3 <> a1) & c3 <> a2) & c1 <> a2) & c1 <> a3) implies (a1 <> x & a3 <> x))
proof
assume that
A1: (not a1,a2,a3 is_collinear )
and
A2: a1,a2,c3 is_collinear 
and
A3: a2,a3,c1 is_collinear 
and
A4: c1,c3,x is_collinear 
and
A5: c3 <> a1
and
A6: c3 <> a2
and
A7: c1 <> a2
and
A8: c1 <> a3;
A9: a3 <> x
proof
assume (not thesis);
then A10: a3,c1,c3 is_collinear  by A4,CKB1:1;
a3,c1,a2 is_collinear  by A3,CKB1:1;
then a3,a2,c3 is_collinear  by A8,A10,CKB2:1;
then A11: a2,c3,a3 is_collinear  by CKB1:1;
a2,c3,a1 is_collinear  by A2,CKB1:1;
then a2,a1,a3 is_collinear  by A6,A11,CKB2:1;
hence contradiction by A1,CKB1:1;
end;
a1 <> x
proof
assume (not thesis);
then A12: a1,c3,c1 is_collinear  by A4,CKB1:1;
a1,c3,a2 is_collinear  by A2,CKB1:1;
then a1,c1,a2 is_collinear  by A5,A12,CKB2:1;
then A13: a2,c1,a1 is_collinear  by CKB1:1;
a2,c1,a3 is_collinear  by A3,CKB1:1;
then a2,a1,a3 is_collinear  by A7,A13,CKB2:1;
hence contradiction by A1,CKB1:1;
end;
hence thesis by A9;
end;
