reserve j for Nat;
reserve x for set;
reserve X for set;
reserve Y for set;
theorem
Lm31: ((X <> ( {} ) & X c= ( Seg n )) implies (ex k st (((1 <= k & k <= n) & k in X) & (for i holds ((1 <= i & i < k) implies (not i in X))))))
proof
defpred P[ Nat ]
 means
(for X holds ((X <> ( {} ) & X c= ( Seg $1 )) implies (ex k st (((1 <= k & k <= $1) & k in X) & (for i holds ((1 <= i & i < k) implies (not i in X)))))));
A1: (for m holds (P[ m ] implies P[ ( m + 1 ) ]))
proof
let m;
assume that
A2: P[ m ];
set m1 = ( m + 1 );
A3: (((Y <> ( {} ) & Y c= ( Seg m1 )) & (not m1 in Y)) implies Y c= ( Seg m ))
proof
assume that
Y <> ( {} )
and
A4: Y c= ( Seg m1 )
and
A5: (not m1 in Y);
Y c= ( Seg m )
proof
let x;
assume A6: x in Y;
then x in ( Seg m1 ) by A4;
then x in { j where j is (Element of ( NAT )): (1 <= j & j <= m1) } by FINSEQ_1:def 1;
then consider j being (Element of ( NAT )) such that A7: x = j and A8: 1 <= j and A9: j <= m1;
j < m1 by A5,A6,A7,A9,XXREAL_0:1;
then j <= m by NAT_1:13;
hence thesis by A7,A8,FINSEQ_1:1;
end;
hence thesis;
end;
(for X holds ((X <> ( {} ) & X c= ( Seg m1 )) implies (ex k st (((1 <= k & k <= m1) & k in X) & (for i holds ((1 <= i & i < k) implies (not i in X)))))))
proof
let X;
assume that
A10: X <> ( {} )
and
A11: X c= ( Seg m1 );
now
per cases ;
suppose (not m1 in X);

then X c= ( Seg m ) by A3,A10,A11;
then consider k such that A12: 1 <= k and A13: k <= m and A14: (k in X & (for i holds ((1 <= i & i < k) implies (not i in X)))) by A2,A10;
m <= m1 by NAT_1:11;
then k <= m1 by A13,XXREAL_0:2;
hence thesis by A12,A14;
end;
suppose A15: m1 in X;

set X1 = ( X \ { m1 } );
m1 in { m1 } by TARSKI:def 1;
then A16: (not m1 in X1) by XBOOLE_0:def 5;
A17: X1 c= ( Seg m1 ) by A11,XBOOLE_1:1;
now
per cases ;
suppose A18: X1 <> ( {} );

then X1 c= ( Seg m ) by A3,A16,A17;
then consider k such that A19: 1 <= k and A20: k <= m and A21: k in X1 and A22: (for i holds ((1 <= i & i < k) implies (not i in X1))) by A2,A18;
m <= m1 by NAT_1:11;
then A23: k <= m1 by A20,XXREAL_0:2;
(for i holds ((1 <= i & i < k) implies (not i in X)))
proof
let i;
assume (1 <= i & i < k);
then ((not i in { m1 }) & (not i in X1)) by A22,A23,TARSKI:def 1;
hence thesis by XBOOLE_0:def 5;
end;
hence thesis by A19,A21,A23;
end;
suppose X1 = ( {} );

then X c= { m1 } by XBOOLE_1:37;
then (1 <= m1 & (for i holds ((1 <= i & i < m1) implies (not i in X)))) by NAT_1:11,TARSKI:def 1;
hence thesis by A15;
end;
end;
hence thesis;
end;
end;
hence thesis;
end;
hence thesis;
end;
A24: P[ ( 0 ) ];
(for m holds P[ m ]) from NAT_1:sch 2(A24,A1);
hence thesis;
end;
