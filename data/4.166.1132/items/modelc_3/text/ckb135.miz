begin
reserve G for LTL-formula;
reserve v for LTL-formula;
reserve N for  strict LTLnode over v;
reserve w for (Element of ( Inf_seq ( AtomicFamily ) ));
reserve U for (Choice_Function of ( BOOL ( Subformulae v ) ));
theorem
Th59: ((w |= ( * N ) & N is non  elementary) implies (w |= ( * ( chosen_succ (w,v,U,N) ) ) & ( chosen_succ (w,v,U,N) ) is_succ_of N))
proof
assume that
A1: w |= ( * N )
and
A2: N is non  elementary;
set H = ( chosen_formula (U,N) );
set SN = ( chosen_succ (w,v,U,N) );
set H2 = ( the_right_argument_of H );
A3: H in (the LTLnew of N) by A2,CKB133:1;
now
per cases ;
suppose A4: (((not H is  Until) & w |= ( * ( SuccNode1 (H,N) ) )) or (H is  Until & w |/= H2));

then A5: SN = ( SuccNode1 (H,N) ) by CKB134:def 1;
A6: w |= ( * SN )
proof
now
per cases  by A4;
suppose ((not H is  Until) & w |= ( * ( SuccNode1 (H,N) ) ));

hence thesis by CKB134:def 1;
end;
suppose A7: (H is  Until & w |/= H2);

set N2 = ( SuccNode2 (H,N) );
A8: (w |= ( * SN ) or w |= ( * N2 )) by A1,A3,A5,A7,CKB45:1;
now
(the LTLold of N2) = ( (the LTLold of N) \/ { H } ) by A3,CKB21:def 1;
then A9: (H2 in (the LTLold of N) implies H2 in (the LTLold of N2)) by XBOOLE_0:def 3;
( LTLNew2 H ) = { H2 } by A7,CKB17:def 1;
then H2 in ( LTLNew2 H ) by TARSKI:def 1;
then A10: ((not H2 in (the LTLold of N)) implies H2 in ( ( LTLNew2 H ) \ (the LTLold of N) )) by XBOOLE_0:def 5;
assume A11: (not w |= ( * SN ));
(the LTLnew of N2) = ( ( (the LTLnew of N) \ { H } ) \/ ( ( LTLNew2 H ) \ (the LTLold of N) ) ) by A3,CKB21:def 1;
then ((not H2 in (the LTLold of N)) implies H2 in (the LTLnew of N2)) by A10,XBOOLE_0:def 3;
then H2 in ( * N2 ) by A9,CKB1:1;
hence contradiction by A7,A8,A11,MODELC_2:def 64;
end;
hence thesis;
end;
end;
hence thesis;
end;
SN is_succ1_of N by A3,A5,CKB23:def 1;
hence thesis by A6,CKB25:def 1;
end;
suppose A12: (not (((not H is  Until) & w |= ( * ( SuccNode1 (H,N) ) )) or (H is  Until & w |/= H2)));

set N1 = ( SuccNode1 (H,N) );
A13: SN = ( SuccNode2 (H,N) ) by A12,CKB134:def 1;
(w |= ( * SN ) & SN is_succ_of N)
proof
now
per cases  by A12;
suppose A14: (H is  Until & w |= H2);

set NN = (the LTLnew of N);
set NO = (the LTLold of N);
set SNN = (the LTLnew of SN);
( LTLNew2 H ) = { H2 } by A14,CKB17:def 1;
then A15: SNN = ( ( NN \ { H } ) \/ ( { H2 } \ NO ) ) by A3,A13,CKB21:def 1;
( NN \ { H } ) c= NN by XBOOLE_1:36;
then A16: SNN c= ( NN \/ { H2 } ) by A15,XBOOLE_1:13;
set NX = (the LTLnext of N);
set SNX = (the LTLnext of SN);
set SNO = (the LTLold of SN);
(SNO = ( NO \/ { H } ) & { H } c= NN) by A3,A13,CKB21:def 1,ZFMISC_1:31;
then A17: SNO c= ( NO \/ NN ) by XBOOLE_1:9;
A18: SNX = NX by A3,A13,CKB21:def 1;
A19: (G in ( * SN ) implies w |= G)
proof
assume A20: G in ( * SN );
now
per cases  by A20,CKB1:1;
suppose G in SNO;

then G in ( * N ) by A17,XBOOLE_0:def 3;
hence thesis by A1,MODELC_2:def 64;
end;
suppose G in SNN;

then (G in NN or G in { H2 }) by A16,XBOOLE_0:def 3;
then (G in ( * N ) or G = H2) by CKB1:1,TARSKI:def 1;
hence thesis by A1,A14,MODELC_2:def 64;
end;
suppose G in ( 'X' ( CastLTL SNX ) );

then G in ( * N ) by A18,CKB1:1;
hence thesis by A1,MODELC_2:def 64;
end;
end;
hence thesis;
end;
SN is_succ2_of N by A3,A13,A14,CKB24:def 1;
hence thesis by A19,CKB25:def 1,MODELC_2:def 64;
end;
suppose A21: (not w |= ( * N1 ));

now
per cases  by MODELC_2:2;
suppose (((H is  atomic or H is  negative) or H is  conjunctive) or H is  next);

hence thesis by A1,A3,A21,CKB41:1,CKB42:1;
end;
suppose A22: ((H is  disjunctive or H is  Until) or H is  Release);

then SN is_succ2_of N by A3,A13,CKB24:def 1;
hence thesis by A1,A3,A13,A21,A22,CKB25:def 1,CKB44:1,CKB45:1,CKB46:1;
end;
end;
hence thesis;
end;
end;
hence thesis;
end;
hence thesis;
end;
end;
hence thesis;
end;
