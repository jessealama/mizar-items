theorem
Lm6: ((((o <> a & o <> b) &  LIN o,a,b) & ((((not  LIN o,a,x) &  LIN o,x,y) & a,x // b,y) or ( LIN o,a,x & (ex p st (ex p9 st (((((not  LIN o,a,p) &  LIN o,p,p9) & a,p // b,p9) & p,x // p9,y) &  LIN o,a,y)))))) implies (((o <> b & o <> a) &  LIN o,b,a) & ((((not  LIN o,b,y) &  LIN o,y,x) & b,y // a,x) or ( LIN o,b,y & (ex p st (ex p9 st (((((not  LIN o,b,p) &  LIN o,p,p9) & b,p // a,p9) & p,y // p9,x) &  LIN o,b,x)))))))
proof
