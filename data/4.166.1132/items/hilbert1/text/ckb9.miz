environ
vocabularies FINSEQ_1,CARD_1,ORDINAL4,SUBSET_1,NUMBERS,ARYTM_3,TARSKI,RELAT_1,XBOOLE_0,FUNCT_1,QC_LANG1,XBOOLEAN,HILBERT1;
notations TARSKI,XBOOLE_0,SUBSET_1,NUMBERS,NAT_1,FUNCT_1,FINSEQ_1,CKB1,CKB2,CKB3,CKB4,CKB5;
definitions TARSKI,CKB1,CKB2,CKB3,CKB4,CKB5;
theorems TARSKI,FINSEQ_1,XBOOLE_0,XBOOLE_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6;
schemes XBOOLE_0;
registrations SUBSET_1,ORDINAL1,FUNCT_1,FINSEQ_1,CKB7,CKB8;
constructors NAT_1,FINSEQ_1,CKB1,CKB2,CKB3,CKB4,CKB5;
requirements NUMERALS,BOOLE,SUBSET;
begin
definition
func HP-WFF -> set means 
:Def6: (it is  HP-closed & (for D being set holds (D is  HP-closed implies it c= D)));
existence
proof
A1: <* ( 0 ) *> in ( ( NAT ) * ) by FINSEQ_1:def 11;
defpred P[ set ]
 means
(for D being set holds (D is  HP-closed implies $1 in D));
consider D0 being set such that A2: (for x being set holds (x in D0 iff (x in ( ( NAT ) * ) & P[ x ]))) from XBOOLE_0:sch 1;
A3: (for D being set holds (D is  HP-closed implies <* ( 0 ) *> in D)) by CKB1:def 1;
then reconsider D0 as non  empty set by A2,A1;
take D0;
A4: D0 c= ( ( NAT ) * )
proof
let x being set;
thus thesis by A2;
end;
(for p,q being FinSequence holds ((p in D0 & q in D0) implies ( ( <* 2 *> ^ p ) ^ q ) in D0))
proof
let p being FinSequence;
let q being FinSequence;
assume that
A5: p in D0
and
A6: q in D0;
A7: q in ( ( NAT ) * ) by A2,A6;
A8: (for D being set holds (D is  HP-closed implies ( ( <* 2 *> ^ p ) ^ q ) in D))
proof
let D being set;
assume A9: D is  HP-closed;
then A10: q in D by A2,A6;
p in D by A2,A5,A9;
hence thesis by A9,A10,CKB3:def 1;
end;
p in ( ( NAT ) * ) by A2,A5;
then reconsider p9 = p,q9 = q as (FinSequence of ( NAT )) by A7,FINSEQ_1:def 11;
( ( <* 2 *> ^ p9 ) ^ q9 ) in ( ( NAT ) * ) by FINSEQ_1:def 11;
hence thesis by A2,A8;
end;
then A11: D0 is  with_conjunction by CKB3:def 1;
(for p,q being FinSequence holds ((p in D0 & q in D0) implies ( ( <* 1 *> ^ p ) ^ q ) in D0))
proof
let p being FinSequence;
let q being FinSequence;
assume that
A12: p in D0
and
A13: q in D0;
A14: q in ( ( NAT ) * ) by A2,A13;
A15: (for D being set holds (D is  HP-closed implies ( ( <* 1 *> ^ p ) ^ q ) in D))
proof
let D being set;
assume A16: D is  HP-closed;
then A17: q in D by A2,A13;
p in D by A2,A12,A16;
hence thesis by A16,A17,CKB2:def 1;
end;
p in ( ( NAT ) * ) by A2,A12;
then reconsider p9 = p,q9 = q as (FinSequence of ( NAT )) by A14,FINSEQ_1:def 11;
( ( <* 1 *> ^ p9 ) ^ q9 ) in ( ( NAT ) * ) by FINSEQ_1:def 11;
hence thesis by A2,A15;
end;
then A18: D0 is  with_implication by CKB2:def 1;
(for n being (Element of ( NAT )) holds <* ( 3 + n ) *> in D0)
proof
let n being (Element of ( NAT ));
set p = ( 3 + n );
reconsider h = <* p *> as (FinSequence of ( NAT ));
A19: h in ( ( NAT ) * ) by FINSEQ_1:def 11;
(for D being set holds (D is  HP-closed implies <* p *> in D)) by CKB4:def 1;
hence thesis by A2,A19;
end;
then A20: D0 is  with_propositional_variables by CKB4:def 1;
<* ( 0 ) *> in D0 by A2,A1,A3;
then D0 is  with_VERUM by CKB1:def 1;
hence D0 is  HP-closed by A4,A20,A18,A11;
let D being set;
assume that
A21: D is  HP-closed;
let x being set;
assume x in D0;
hence thesis by A2,A21;
end;
uniqueness
proof
let D1 being set;
let D2 being set;
assume that
A22: D1 is  HP-closed
and
A23: (for D being set holds (D is  HP-closed implies D1 c= D))
and
A24: D2 is  HP-closed
and
A25: (for D being set holds (D is  HP-closed implies D2 c= D));
A26: D2 c= D1 by A22,A25;
D1 c= D2 by A23,A24;
hence thesis by A26,XBOOLE_0:def 10;
end;
end;
