begin
reserve x for set;
reserve Y for set;
reserve f for Function;
reserve g for Function;
defpred P[ set ]
 means
$1 is Function;
reserve A for set;
reserve B for set;
definition
let S being  functional set;
func product" S -> Function means 
:Def13: ((for x being set holds (x in ( dom it ) iff (for f being Function holds (f in S implies x in ( dom f ))))) & (for i being set holds (i in ( dom it ) implies ( it . i ) = ( pi (S,i) )))) if S is non  empty otherwise it = ( {} );
existence
proof
thus (S is non  empty implies (ex g being Function st ((for x being set holds (x in ( dom g ) iff (for f being Function holds (f in S implies x in ( dom f ))))) & (for i being set holds (i in ( dom g ) implies ( g . i ) = ( pi (S,i) ))))))
proof
assume S is non  empty;
then reconsider S1 = S as non  empty  functional set;
set D = { ( dom f ) where f is (Element of S1): (not contradiction) };
defpred P[ set,set ]
 means
$2 = ( pi (S,$1) );
A1: (for e being set holds (e in ( meet D ) implies (ex u being set st P[ e,u ])));
consider g being Function such that A2: ( dom g ) = ( meet D ) and A3: (for e being set holds (e in ( meet D ) implies P[ e,( g . e ) ])) from CLASSES1:sch 1(A1);
take g;
hereby
let x being set;
hereby
assume A4: x in ( dom g );
let f being Function;
assume f in S;
then ( dom f ) in D;
hence x in ( dom f ) by A2,A4,SETFAM_1:def 1;
end;
assume A5: (for f being Function holds (f in S implies x in ( dom f )));
set f = the (Element of S1);
A6: ( dom f ) in D;
(for Y being set holds (Y in D implies x in Y))
proof
let Y being set;
assume Y in D;
then (ex f being (Element of S1) st (Y = ( dom f ) & (not contradiction)));
hence thesis by A5;
end;
hence x in ( dom g ) by A2,A6,SETFAM_1:def 1;
end;
thus thesis by A2,A3;
end;

thus thesis;
end;
uniqueness
proof
let A being Function;
let B being Function;
thus (((((S is non  empty & (for x being set holds (x in ( dom A ) iff (for f being Function holds (f in S implies x in ( dom f )))))) & (for i being set holds (i in ( dom A ) implies ( A . i ) = ( pi (S,i) )))) & (for x being set holds (x in ( dom B ) iff (for f being Function holds (f in S implies x in ( dom f )))))) & (for i being set holds (i in ( dom B ) implies ( B . i ) = ( pi (S,i) )))) implies A = B)
proof
defpred P[ set ]
 means
(for f being Function holds (f in S implies $1 in ( dom f )));
assume that
S is non  empty
and
A7: (for x being set holds (x in ( dom A ) iff P[ x ]))
and
A8: (for i being set holds (i in ( dom A ) implies ( A . i ) = ( pi (S,i) )))
and
A9: (for x being set holds (x in ( dom B ) iff P[ x ]))
and
A10: (for i being set holds (i in ( dom B ) implies ( B . i ) = ( pi (S,i) )));
A11: ( dom A ) = ( dom B ) from XBOOLE_0:sch 2(A7,A9);
now
let i being set;
assume A12: i in ( dom A );
hence ( A . i ) = ( pi (S,i) ) by A8
.= ( B . i ) by A10,A11,A12;
end;
hence thesis by A11,FUNCT_1:2;
end;

thus thesis;
end;
consistency;
end;
