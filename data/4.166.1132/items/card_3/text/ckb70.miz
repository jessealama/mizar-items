reserve A for set;
registration
let f being Function;
cluster ( sproduct f ) ->  functional non  empty;
coherence
proof
defpred P[ set ]
 means
(ex g st (($1 = g & ( dom g ) c= ( dom f )) & (for x holds (x in ( dom g ) implies ( g . x ) in ( f . x )))));
consider A being set such that A1: (x in A iff (x in ( PFuncs (( dom f ),( union ( rng f ) )) ) & P[ x ])) from XBOOLE_0:sch 1;
( {} ) is (PartFunc of ( dom f ),( union ( rng f ) )) by RELSET_1:12;
then A2: ( {} ) in ( PFuncs (( dom f ),( union ( rng f ) )) ) by PARTFUN1:45;
A3: ( dom ( {} ) ) c= ( dom f ) by XBOOLE_1:2;
A4: (for x holds (x in ( dom ( {} ) ) implies ( ( {} ) . x ) in ( f . x )));
now
let x being set;
assume x in A;
then (ex g st ((x = g & ( dom g ) c= ( dom f )) & (for x holds (x in ( dom g ) implies ( g . x ) in ( f . x ))))) by A1;
hence x is Function;
end;
then reconsider A as  functional non  empty set by A1,A2,A3,A4,FUNCT_1:def 13;
now
let x;
thus (x in A implies (ex g st ((x = g & ( dom g ) c= ( dom f )) & (for x holds (x in ( dom g ) implies ( g . x ) in ( f . x )))))) by A1;
given g such that
A5: x = g
and
A6: ( dom g ) c= ( dom f )
and
A7: (for x holds (x in ( dom g ) implies ( g . x ) in ( f . x )));

( rng g ) c= ( union ( rng f ) )
proof
let y being set;
assume y in ( rng g );
then consider z being set such that A8: z in ( dom g ) and A9: y = ( g . z ) by FUNCT_1:def 3;
A10: ( g . z ) in ( f . z ) by A7,A8;
( f . z ) in ( rng f ) by A6,A8,FUNCT_1:def 3;
hence thesis by A9,A10,TARSKI:def 4;
end;
then x in ( PFuncs (( dom f ),( union ( rng f ) )) ) by A5,A6,PARTFUN1:def 3;
hence x in A by A1,A5,A6,A7;
end;
hence thesis by CKB69:def 1;
end;
