constructors SETFAM_1,RELAT_2,WELLORD1,PARTFUN1,WELLORD2,FUNCOP_1,FUNCT_4,CLASSES1,ORDINAL2,ORDINAL3,FINSET_1,CARD_1,FINSUB_1,RELSET_1,FUNCT_1,PBOOLE,CKB1;
requirements NUMERALS,BOOLE,SUBSET;
begin
reserve x for set;
reserve f for Function;
definition
let f;
func Card f -> Cardinal-Function means 
:Def2: (( dom it ) = ( dom f ) & (for x holds (x in ( dom f ) implies ( it . x ) = ( card ( f . x ) ))));
existence
proof
deffunc f(set) = ( card ( f . $1 ) );
thus (ex g being Cardinal-Function st (( dom g ) = ( dom f ) & (for x holds (x in ( dom f ) implies ( g . x ) = f(x))))) from CKB8:sch 1;
end;
uniqueness
proof
let a1 being Cardinal-Function;
let a2 being Cardinal-Function;
assume that
A1: ( dom a1 ) = ( dom f )
and
A2: (for x holds (x in ( dom f ) implies ( a1 . x ) = ( card ( f . x ) )))
and
A3: ( dom a2 ) = ( dom f )
and
A4: (for x holds (x in ( dom f ) implies ( a2 . x ) = ( card ( f . x ) )));
now
let x;
assume A5: x in ( dom f );
then ( a1 . x ) = ( card ( f . x ) ) by A2;
hence ( a1 . x ) = ( a2 . x ) by A4,A5;
end;
hence thesis by A1,A3,FUNCT_1:2;
end;
end;
