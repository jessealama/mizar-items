begin
reserve R for non empty Poset;
theorem
Th2: (for F being (ManySortedFunction of (the carrier of R)) holds (F is  order-sorted implies (for s1,s2 being (Element of R) holds (s1 <= s2 implies (( dom ( F . s1 ) ) c= ( dom ( F . s2 ) ) & ( F . s1 ) c= ( F . s2 ))))))
proof
let F being (ManySortedFunction of (the carrier of R));
assume that
A1: F is  order-sorted;
let s1 being (Element of R),s2 being (Element of R);
assume that
A2: s1 <= s2;
thus ( dom ( F . s1 ) ) c= ( dom ( F . s2 ) )
proof
let x being set;
assume x in ( dom ( F . s1 ) );
hence thesis by A1,A2,CKB1:def 1;
end;

(for a,b being set holds ([ a,b ] in ( F . s1 ) implies [ a,b ] in ( F . s2 )))
proof
let y being set,z being set;
assume that
A3: [ y,z ] in ( F . s1 );
y in ( dom ( F . s1 ) ) by A3,RELAT_1:def 4;
then A4: (y in ( dom ( F . s2 ) ) & ( ( F . s1 ) . y ) = ( ( F . s2 ) . y )) by A1,A2,CKB1:def 1;
( ( F . s1 ) . y ) = z by A3,FUNCT_1:1;
hence thesis by A4,FUNCT_1:1;
end;
hence thesis by RELAT_1:def 3;
end;
