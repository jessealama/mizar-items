environ
vocabularies XBOOLE_0,ORDERS_2,OSALG_1,PBOOLE,STRUCT_0,SUBSET_1,XXREAL_0,RELAT_1,FUNCT_1,TARSKI,FINSEQ_1,CARD_3,MEMBER_1,MSUALG_3,MSUALG_1,GROUP_6,MARGREL1,NAT_1,PARTFUN1,SEQM_3,OSALG_2,MSUALG_2,UNIALG_2,OSALG_3;
notations TARSKI,XBOOLE_0,SUBSET_1,NAT_1,RELAT_1,FUNCT_1,RELSET_1,PARTFUN1,FUNCT_2,FINSEQ_1,FINSEQ_2,CARD_3,ORDERS_2,PBOOLE,STRUCT_0,MSUALG_1,MSUALG_2,OSALG_1,OSALG_2,MSUALG_3,CKB1,CKB10,CKB13,CKB14;
definitions TARSKI,OSALG_1,CKB1,CKB10;
theorems XBOOLE_1,FUNCT_1,FUNCT_2,FINSEQ_1,PBOOLE,CARD_3,MSUALG_1,ZFMISC_1,MSUALG_9,FINSEQ_3,MSUALG_2,TARSKI,RELAT_1,OSALG_1,OSALG_2,MSUALG_3,MSAFREE3,ORDINAL1,PARTFUN1,FINSEQ_2,CKB1,CKB2,CKB3,CKB4,CKB5,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB15,CKB16;
registrations XBOOLE_0,FUNCT_1,RELSET_1,FUNCOP_1,FINSEQ_1,PBOOLE,STRUCT_0,MSUALG_1,MSUALG_3,ORDERS_3,MSUALG_9,OSALG_1,CKB6;
constructors MSUALG_3,ORDERS_3,OSALG_2,CARD_3,RELSET_1,CKB1,CKB10,CKB13,CKB14;
requirements BOOLE,SUBSET;
begin
reserve S1 for OrderSortedSign;
theorem
Th13: (for U1,U2 being  non-empty (OSAlgebra of S1) holds (for F being (ManySortedFunction of U1,U2) holds (F is  order-sorted implies (for o1,o2 being (OperSymbol of S1) holds (o1 <= o2 implies (for x being (Element of ( Args (o1,U1) )) holds (for x1 being (Element of ( Args (o2,U1) )) holds (x = x1 implies ( F # x ) = ( F # x1 )))))))))
proof
let U1 being  non-empty (OSAlgebra of S1),U2 being  non-empty (OSAlgebra of S1);
let F being (ManySortedFunction of U1,U2);
assume that
A1: F is  order-sorted;
let o1 being (OperSymbol of S1),o2 being (OperSymbol of S1);
assume that
A2: o1 <= o2;
let x being (Element of ( Args (o1,U1) ));
let x1 being (Element of ( Args (o2,U1) ));
assume that
A3: x = x1;
A4: ( dom x ) = ( dom ( the_arity_of o1 ) ) by MSUALG_3:6;
A5: (for n being set holds (n in ( dom x ) implies ( ( F # x ) . n ) = ( ( F # x1 ) . n )))
proof
let n1 being set;
assume that
A6: n1 in ( dom x );
reconsider n2 = n1 as Nat by A6,ORDINAL1:def 12;
reconsider pi1 = ( ( the_arity_of o1 ) /. n2 ),pi2 = ( ( the_arity_of o2 ) /. n2 ) as (Element of S1);
A7: ( ( the_arity_of o1 ) /. n2 ) = ( ( the_arity_of o1 ) . n2 ) by A4,A6,PARTFUN1:def 6;
A8: ( the_arity_of o1 ) <= ( the_arity_of o2 ) by A2,OSALG_1:def 20;
then ( len ( the_arity_of o1 ) ) = ( len ( the_arity_of o2 ) ) by OSALG_1:def 6;
then ( dom ( the_arity_of o1 ) ) = ( dom ( the_arity_of o2 ) ) by FINSEQ_3:29;
then ( ( the_arity_of o2 ) /. n2 ) = ( ( the_arity_of o2 ) . n2 ) by A4,A6,PARTFUN1:def 6;
then A9: pi1 <= pi2 by A4,A6,A8,A7,OSALG_1:def 6;
( rng ( the_arity_of o1 ) ) c= (the carrier of S1);
then ( rng ( the_arity_of o1 ) ) c= ( dom (the Sorts of U1) ) by PARTFUN1:def 2;
then A10: n2 in ( dom ( (the Sorts of U1) * ( the_arity_of o1 ) ) ) by A4,A6,RELAT_1:27;
( dom ( F . pi1 ) ) = ( (the Sorts of U1) . pi1 ) by FUNCT_2:def 1
.= ( (the Sorts of U1) . ( ( the_arity_of o1 ) . n2 ) ) by A4,A6,PARTFUN1:def 6
.= ( ( (the Sorts of U1) * ( the_arity_of o1 ) ) . n2 ) by A4,A6,FUNCT_1:13;
then A11: ( x1 . n2 ) in ( dom ( F . pi1 ) ) by A3,A10,MSUALG_3:6;
( ( F # x ) . n2 ) = ( ( F . ( ( the_arity_of o1 ) /. n2 ) ) . ( x1 . n2 ) ) by A3,A6,MSUALG_3:def 6
.= ( ( F . pi2 ) . ( x1 . n2 ) ) by A1,A11,A9,CKB1:def 1
.= ( ( F # x1 ) . n2 ) by A3,A6,MSUALG_3:def 6;
hence thesis;
end;
( dom x1 ) = ( dom ( the_arity_of o2 ) ) by MSUALG_3:6;
then A12: ( dom ( F # x1 ) ) = ( dom x1 ) by MSUALG_3:6;
( dom ( F # x ) ) = ( dom x ) by A4,MSUALG_3:6;
hence thesis by A3,A12,A5,FUNCT_1:2;
end;
