begin
reserve R for non empty Poset;
theorem
Th8: (for A being (OrderSortedSet of R) holds (for F being (ManySortedFunction of (the carrier of R)) holds (F is  order-sorted implies ( F .:.: A ) is (OrderSortedSet of R))))
proof
let A being (OrderSortedSet of R);
let F being (ManySortedFunction of (the carrier of R));
assume that
A1: F is  order-sorted;
set C = ( F .:.: A );
reconsider C1 = C as (ManySortedSet of R);
C1 is  order-sorted
proof
let s1 being (Element of R),s2 being (Element of R);
assume s1 <= s2;
then A2: (( A . s1 ) c= ( A . s2 ) & ( F . s1 ) c= ( F . s2 )) by A1,CKB2:1,OSALG_1:def 16;
(( C1 . s1 ) = ( ( F . s1 ) .: ( A . s1 ) ) & ( C1 . s2 ) = ( ( F . s2 ) .: ( A . s2 ) )) by PBOOLE:def 20;
hence thesis by A2,RELAT_1:125;
end;
hence thesis;
end;
