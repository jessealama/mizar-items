notations TARSKI,XBOOLE_0,SUBSET_1,NAT_1,RELAT_1,FUNCT_1,RELSET_1,PARTFUN1,FUNCT_2,FINSEQ_1,FINSEQ_2,CARD_3,ORDERS_2,PBOOLE,STRUCT_0,MSUALG_1,MSUALG_2,OSALG_1,OSALG_2,MSUALG_3,CKB1;
definitions TARSKI,OSALG_1,CKB1;
theorems XBOOLE_1,FUNCT_1,FUNCT_2,FINSEQ_1,PBOOLE,CARD_3,MSUALG_1,ZFMISC_1,MSUALG_9,FINSEQ_3,MSUALG_2,TARSKI,RELAT_1,OSALG_1,OSALG_2,MSUALG_3,MSAFREE3,ORDINAL1,PARTFUN1,FINSEQ_2,CKB1,CKB2,CKB3,CKB4,CKB5;
registrations XBOOLE_0,FUNCT_1,RELSET_1,FUNCOP_1,FINSEQ_1,PBOOLE,STRUCT_0,MSUALG_1,MSUALG_3,ORDERS_3,MSUALG_9,OSALG_1,CKB6;
constructors MSUALG_3,ORDERS_3,OSALG_2,CARD_3,RELSET_1,CKB1;
requirements BOOLE,SUBSET;
begin
reserve R for non empty Poset;
theorem
Th6: (for A being (OrderSortedSet of R) holds (for B,C being  non-empty (OrderSortedSet of R) holds (for F being (ManySortedFunction of A,B) holds (for G being (ManySortedFunction of B,C) holds ((F is  order-sorted & G is  order-sorted) implies ( G ** F ) is  order-sorted)))))
proof
let A being (OrderSortedSet of R);
let B being  non-empty (OrderSortedSet of R),C being  non-empty (OrderSortedSet of R);
let F being (ManySortedFunction of A,B);
let G being (ManySortedFunction of B,C);
assume that
A1: F is  order-sorted
and
A2: G is  order-sorted;
(for s1,s2 being (Element of R) holds (s1 <= s2 implies (for a1 being set holds (a1 in ( A . s1 ) implies ( ( ( G ** F ) . s1 ) . a1 ) = ( ( ( G ** F ) . s2 ) . a1 )))))
proof
let s1 being (Element of R),s2 being (Element of R);
assume that
A3: s1 <= s2;
A4: ( A . s1 ) c= ( A . s2 ) by A3,OSALG_1:def 16;
let a1 being set;
assume that
A5: a1 in ( A . s1 );
A6: ( ( F . s1 ) . a1 ) = ( ( F . s2 ) . a1 ) by A1,A3,A5,CKB3:1;
( ( F . s1 ) . a1 ) in ( B . s1 ) by A5,FUNCT_2:5;
then A7: ( ( G . s1 ) . ( ( F . s2 ) . a1 ) ) = ( ( G . s2 ) . ( ( F . s2 ) . a1 ) ) by A2,A3,A6,CKB3:1;
( ( ( G ** F ) . s1 ) . a1 ) = ( ( ( G . s1 ) * ( F . s1 ) ) . a1 ) by MSUALG_3:2
.= ( ( G . s1 ) . ( ( F . s2 ) . a1 ) ) by A5,A6,FUNCT_2:15
.= ( ( ( G . s2 ) * ( F . s2 ) ) . a1 ) by A5,A4,A7,FUNCT_2:15
.= ( ( ( G ** F ) . s2 ) . a1 ) by MSUALG_3:2;
hence thesis;
end;
hence thesis by CKB3:1;
end;
