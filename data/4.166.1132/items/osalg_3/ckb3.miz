environ
vocabularies XBOOLE_0,ORDERS_2,OSALG_1,PBOOLE,STRUCT_0,SUBSET_1,XXREAL_0,RELAT_1,FUNCT_1,TARSKI,FINSEQ_1,CARD_3,MEMBER_1,MSUALG_3,MSUALG_1,GROUP_6,MARGREL1,NAT_1,PARTFUN1,SEQM_3,OSALG_2,MSUALG_2,UNIALG_2,OSALG_3;
notations TARSKI,XBOOLE_0,SUBSET_1,NAT_1,RELAT_1,FUNCT_1,RELSET_1,PARTFUN1,FUNCT_2,FINSEQ_1,FINSEQ_2,CARD_3,ORDERS_2,PBOOLE,STRUCT_0,MSUALG_1,MSUALG_2,OSALG_1,OSALG_2,MSUALG_3,CKB1;
definitions TARSKI,OSALG_1,CKB1;
theorems XBOOLE_1,FUNCT_1,FUNCT_2,FINSEQ_1,PBOOLE,CARD_3,MSUALG_1,ZFMISC_1,MSUALG_9,FINSEQ_3,MSUALG_2,TARSKI,RELAT_1,OSALG_1,OSALG_2,MSUALG_3,MSAFREE3,ORDINAL1,PARTFUN1,FINSEQ_2,CKB1,CKB2;
registrations XBOOLE_0,FUNCT_1,RELSET_1,FUNCOP_1,FINSEQ_1,PBOOLE,STRUCT_0,MSUALG_1,MSUALG_3,ORDERS_3,MSUALG_9,OSALG_1;
constructors MSUALG_3,ORDERS_3,OSALG_2,CARD_3,RELSET_1,CKB1;
requirements BOOLE,SUBSET;
begin
reserve R for non empty Poset;
theorem
Th3: (for A being (OrderSortedSet of R) holds (for B being  non-empty (OrderSortedSet of R) holds (for F being (ManySortedFunction of A,B) holds (F is  order-sorted iff (for s1,s2 being (Element of R) holds (s1 <= s2 implies (for a1 being set holds (a1 in ( A . s1 ) implies ( ( F . s1 ) . a1 ) = ( ( F . s2 ) . a1 )))))))))
proof
let A being (OrderSortedSet of R);
let B being  non-empty (OrderSortedSet of R);
let F being (ManySortedFunction of A,B);
hereby
assume A1: F is  order-sorted;
let s1 being (Element of R),s2 being (Element of R);
assume that
A2: s1 <= s2;
let a1 being set;
assume a1 in ( A . s1 );
then a1 in ( dom ( F . s1 ) ) by FUNCT_2:def 1;
hence ( ( F . s1 ) . a1 ) = ( ( F . s2 ) . a1 ) by A1,A2,CKB1:def 1;
end;
assume A3: (for s1,s2 being (Element of R) holds (s1 <= s2 implies (for a1 being set holds (a1 in ( A . s1 ) implies ( ( F . s1 ) . a1 ) = ( ( F . s2 ) . a1 )))));
let s1 being (Element of R),s2 being (Element of R);
assume that
A4: s1 <= s2;
A5: (( dom ( F . s1 ) ) = ( A . s1 ) & ( dom ( F . s2 ) ) = ( A . s2 )) by FUNCT_2:def 1;
let a1 being set;
assume that
A6: a1 in ( dom ( F . s1 ) );
( A . s1 ) c= ( A . s2 ) by A4,OSALG_1:def 16;
hence a1 in ( dom ( F . s2 ) ) by A6,A5;
thus ( ( F . s1 ) . a1 ) = ( ( F . s2 ) . a1 ) by A3,A4,A6;
end;
