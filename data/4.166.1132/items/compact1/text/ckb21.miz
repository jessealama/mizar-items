environ
vocabularies PRE_TOPC,SETFAM_1,RCOMP_1,SUBSET_1,XBOOLE_0,WAYBEL_3,YELLOW13,CONNSP_2,TARSKI,ZFMISC_1,TOPS_1,CARD_5,COMPTS_1,NATTRA_1,RELAT_1,FUNCT_3,FUNCT_1,QUOFIELD,TOPS_2,ORDINAL2,STRUCT_0,ORDINAL1,FINSET_1,COMPACT1;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,RELAT_1,FUNCT_1,RELSET_1,SETFAM_1,ORDINAL1,FINSET_1,FINSUB_1,DOMAIN_1,STRUCT_0,PRE_TOPC,TOPS_1,TOPS_2,TDLAT_3,TSEP_1,TEX_3,COMPTS_1,CONNSP_2,WAYBEL_3,YELLOW_9,YELLOW13,CKB1,CKB2,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11;
definitions TARSKI,XBOOLE_0,ORDINAL1,STRUCT_0,PRE_TOPC,SUBSET_1,TOPS_1,TOPS_2,COMPTS_1,WAYBEL_3,YELLOW13,SETFAM_1,CKB1,CKB2,CKB8,CKB9,CKB10,CKB11;
theorems XBOOLE_1,ZFMISC_1,YELLOW_8,YELLOW_9,XBOOLE_0,RELAT_1,CONNSP_2,TEX_3,TEX_4,PRE_TOPC,TOPS_1,TOPS_2,TSEP_1,TARSKI,FUNCT_1,FINSET_1,TDLAT_3,FUNCT_2,SUBSET_1,COMPTS_1,WAYBEL_3,TOPMETR,YELLOW13,SETFAM_1,CKB1,CKB2,CKB8,CKB9,CKB10,CKB11;
schemes FUNCT_1;
registrations XBOOLE_0,SUBSET_1,FINSET_1,STRUCT_0,FINSUB_1,RELAT_1,TOPS_1,BORSUK_2,FUNCT_1,PRE_TOPC,TEX_1,YELLOW13,COMPTS_1,RELSET_1,CKB3,CKB4,CKB5,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20;
constructors FINSOP_1,SETFAM_1,DOMAIN_1,TOPS_1,TOPS_2,YELLOW_9,YELLOW13,TDLAT_3,CONNSP_2,T_0TOPSP,WAYBEL_3,TSEP_1,TEX_3,COMPTS_1,RELSET_1,CKB1,CKB2,CKB9,CKB10,CKB11;
requirements BOOLE,SUBSET;
begin
registration
let X being  locally-compact non empty TopSpace;
let C being  closed non empty (Subset of X);
cluster ( X | C ) ->  locally-compact;
coherence
proof
let x being (Point of ( X | C ));
reconsider xx = x as (Point of X) by PRE_TOPC:25;
consider K being (a_neighborhood of xx) such that A1: K is  compact by CKB11:def 1;
A2: ( [#] ( X | C ) ) = C by PRE_TOPC:8;
then reconsider KC = ( K /\ C ) as (Subset of ( X | C )) by XBOOLE_1:17;
reconsider KC as (a_neighborhood of x) by A2,TOPMETR:5;
take KC;
A3: xx in ( Int K ) by CONNSP_2:def 1;
A4: ( [#] ( X | K ) ) = K by PRE_TOPC:8;
then reconsider KK = ( K /\ C ) as (Subset of ( X | K )) by XBOOLE_1:17;
A5: KK is  closed by A4,PRE_TOPC:13;
A6: ( Int K ) c= K by TOPS_1:16;
then A7: x in KC by A2,A3,XBOOLE_0:def 4;
( X | K ) is  compact by A1,A6,A3,COMPTS_1:3;
then KK is  compact by A5,COMPTS_1:8;
then ( ( X | K ) | KK ) is  compact by A7,COMPTS_1:3;
then ( X | ( K /\ C ) ) is  compact by PRE_TOPC:7,XBOOLE_1:17;
then ( ( X | C ) | KC ) is  compact by PRE_TOPC:7,XBOOLE_1:17;
hence thesis by A7,COMPTS_1:3;
end;
end;
