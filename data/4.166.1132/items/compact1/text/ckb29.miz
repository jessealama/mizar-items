begin
definition
let X being TopStruct;
func One-Point_Compactification X ->  strict TopStruct
means
:Def9: ((the carrier of it) = ( succ ( [#] X ) ) & (the topology of it) = ( (the topology of X) \/ { ( U \/ { ( [#] X ) } ) where U is (Subset of X): (U is  open & ( U ` ) is  compact) } ));
existence
proof
set T = ( succ ( [#] X ) );
set D = { ( U \/ { ( [#] X ) } ) where U is (Subset of X): (U is  open & ( U ` ) is  compact) };
set O = ( (the topology of X) \/ D );
O c= ( bool T )
proof
let a being set;
A1:now
assume a in D;
then consider U being (Subset of X) such that A2: a = ( U \/ { ( [#] X ) } ) and U is  open and ( U ` ) is  compact;
thus a c= T
proof
let A being set;
assume A in a;
then (A in U or A in { ( [#] X ) }) by A2,XBOOLE_0:def 3;
hence thesis by XBOOLE_0:def 3;
end;

end;
assume a in O;
then (a in (the topology of X) or a in D) by XBOOLE_0:def 3;
then a is (Subset of T) by A1,XBOOLE_1:10;
hence thesis;
end;
then reconsider O as (Subset-Family of T);
set Y = TopStruct (# T,O #);
Y is non empty;
hence thesis;
end;
uniqueness;
end;
