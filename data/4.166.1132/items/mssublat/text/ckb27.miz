theorem
(for A being Universal_Algebra holds (for B being  strict  non-empty (MSSubAlgebra of ( MSAlg A )) holds ((the carrier of ( MSSign A )) = { ( 0 ) } implies ( 1-Alg B ) is (SubAlgebra of A))))
proof
let A being Universal_Algebra;
let B being  strict  non-empty (MSSubAlgebra of ( MSAlg A ));
