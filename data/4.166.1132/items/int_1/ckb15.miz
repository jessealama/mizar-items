begin
reserve k for (Element of ( NAT ));
reserve l for (Element of ( NAT ));
registration
let i1 being Integer;
let i2 being Integer;
cluster ( i1 * i2 ) ->  integer;
coherence
proof
consider l such that A15: (i2 = l or i2 = ( - l )) by CKB10:1;
consider k such that A16: (i1 = k or i1 = ( - k )) by CKB10:1;
A17:now
assume that
A18: i1 = ( - k )
and
A19: i2 = ( - l );
( i1 * i2 ) = ( k * l ) by A18,A19;
hence ( i1 * i2 ) is Integer;
end;
now
assume ((i1 = ( - k ) & i2 = l) or (i1 = k & i2 = ( - l )));
then ( i1 * i2 ) = ( - ( k * l ) );
hence ( i1 * i2 ) is Integer by CKB9:1;
end;
hence thesis by A16,A15,A17;
end;
end;
