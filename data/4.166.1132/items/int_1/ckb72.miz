begin
reserve r for  real number;
theorem
([\ r /] = [/ r \] iff (not ( [\ r /] + 1 ) = [/ r \]))
proof
set Diff = ( [/ r \] + ( - [\ r /] ) );
A1:now
assume (not r is Integer);
then [\ r /] < [/ r \] by CKB66:1;
then ( [\ r /] + ( - [\ r /] ) ) < Diff by XREAL_1:6;
then A2: 1 <= Diff by CKB23:1;
( r - 1 ) < [\ r /] by CKB54:def 1;
then A3: ( - [\ r /] ) < ( - ( r - 1 ) ) by XREAL_1:24;
[/ r \] < ( r + 1 ) by CKB60:def 1;
then Diff < ( ( r + 1 ) + ( - ( r - 1 ) ) ) by A3,XREAL_1:8;
then ( ( Diff + 1 ) + ( - 1 ) ) <= ( ( 1 + 1 ) + ( - 1 ) ) by CKB24:1;
then ( [\ r /] + 1 ) = ( [\ r /] + Diff ) by A2,XXREAL_0:1;
hence (( [\ r /] + 1 ) = [/ r \] & [\ r /] <> [/ r \]);
end;
now
assume r is Integer;
then [\ r /] = [/ r \] by CKB65:1;
hence ([\ r /] = [/ r \] & ( [\ r /] + 1 ) <> [/ r \]);
end;
hence thesis by A1;
end;
