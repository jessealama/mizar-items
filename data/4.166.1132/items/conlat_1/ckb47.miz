begin
theorem
Lm1: (for C being FormalContext holds (for CS being ConceptStr over C holds (( ( ObjectDerivation C ) . (the Extent of CS) ) = (the Intent of CS) implies CS is non empty)))
proof
let C being FormalContext;
let CS being ConceptStr over C;
assume A1: ( ( ObjectDerivation C ) . (the Extent of CS) ) = (the Intent of CS);
now
per cases ;
case (the Extent of CS) is  empty;
then (the Intent of CS) = (the carrier' of C) by A1,CKB38:1;
hence thesis by CKB41:def 1;
end;
case (the Extent of CS) is non empty;
hence thesis by CKB41:def 1;
end;
end;
hence thesis;
end;
