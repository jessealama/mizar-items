begin
theorem
Lm7: (for seq being Complex_Sequence holds (seq is  absolutely_summable implies |. ( Sum seq ) .| <= ( Sum |. seq .| )))
proof
let seq being Complex_Sequence;
A1: (for k being (Element of ( NAT )) holds ( |. ( Partial_Sums seq ) .| . k ) <= ( ( Partial_Sums |. seq .| ) . k ))
proof
let k being (Element of ( NAT ));
|. ( ( Partial_Sums seq ) . k ) .| <= ( ( Partial_Sums |. seq .| ) . k ) by COMSEQ_3:30;
hence thesis by VALUED_1:18;
end;
assume A2: seq is  absolutely_summable;
then reconsider Pseq = ( Partial_Sums seq ) as  convergent Complex_Sequence;
A3: |. ( Sum seq ) .| = |. ( lim ( Partial_Sums seq ) ) .| by COMSEQ_3:def 7
.= ( lim |. ( Partial_Sums seq ) .| ) by A2,COMSEQ_2:11;
(|. Pseq .| is  convergent & ( Partial_Sums |. seq .| ) is  convergent) by A2,SERIES_1:def 2;
then ( lim |. ( Partial_Sums seq ) .| ) <= ( lim ( Partial_Sums |. seq .| ) ) by A1,SEQ_2:18;
hence thesis by A3,SERIES_1:def 3;
end;
