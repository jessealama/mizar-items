begin
reserve L1 for Lattice;
reserve L2 for Lattice;
reserve a1 for (Element of L1);
reserve a2 for (Element of L2);
reserve DL for  distributive Lattice;
reserve f for (Homomorphism of DL,L2);
reserve 0L for  lower-bounded Lattice;
theorem
Th19: (for f being (Homomorphism of 0L,L2) holds (f is  onto implies (L2 is  lower-bounded & f preserves_bottom )))
proof
let f being (Homomorphism of 0L,L2);
set r = ( f . ( Bottom 0L ) );
assume A1: f is  onto;
A2:now
let a2 being (Element of L2);
consider a1 being (Element of 0L) such that A3: ( f . a1 ) = a2 by A1,CKB8:1;
thus ( r "/\" a2 ) = ( f . ( ( Bottom 0L ) "/\" a1 ) ) by A3,CKB5:def 1
.= r by LATTICES:15;
hence ( a2 "/\" r ) = r;
end;
thus L2 is  lower-bounded
proof
take r;
thus thesis by A2;
end;

then ( Bottom L2 ) = r by A2,LATTICES:def 16;
hence thesis by CKB12:def 1;
end;
