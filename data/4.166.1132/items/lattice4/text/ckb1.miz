requirements BOOLE,SUBSET;
begin
reserve X for set;
reserve X1 for set;
reserve Y for set;
reserve Z for set;
theorem
(for X holds ((X <> ( {} ) & (for Z holds (((Z <> ( {} ) & Z c= X) & Z is  c=-linear) implies (ex Y st (Y in X & (for X1 holds (X1 in Z implies X1 c= Y))))))) implies (ex Y st (Y in X & (for Z holds ((Z in X & Z <> Y) implies (not Y c= Z)))))))
proof
let X;
assume that
A1: X <> ( {} )
and
A2: (for Z holds (((Z <> ( {} ) & Z c= X) & Z is  c=-linear) implies (ex Y st (Y in X & (for X1 holds (X1 in Z implies X1 c= Y))))));
(for Z holds ((Z c= X & Z is  c=-linear) implies (ex Y st (Y in X & (for X1 holds (X1 in Z implies X1 c= Y))))))
proof
let Z;
assume that
A3: (Z c= X & Z is  c=-linear);
per cases ;
suppose A4: Z = ( {} );

set Y = the (Element of X);
(for X1 holds (X1 in Z implies X1 c= Y)) by A4;
hence thesis by A1;
end;
suppose Z <> ( {} );

hence thesis by A2,A3;
end;
end;
hence thesis by A1,ORDERS_1:65;
end;
