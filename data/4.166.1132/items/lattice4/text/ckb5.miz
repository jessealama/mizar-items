begin
reserve L1 for Lattice;
reserve L2 for Lattice;
reserve a1 for (Element of L1);
reserve b1 for (Element of L1);
definition
let L1;
let L2;
mode Homomorphism of L1,L2
 -> (Function of (the carrier of L1),(the carrier of L2))
means :Def1: (( it . ( a1 "\/" b1 ) ) = ( ( it . a1 ) "\/" ( it . b1 ) ) & ( it . ( a1 "/\" b1 ) ) = ( ( it . a1 ) "/\" ( it . b1 ) ));
existence
proof
set b = the (Element of L2);
defpred P[ set,set ]
 means
(for a1 holds ($1 = a1 implies $2 = b));
A1:now
let x being (Element of L1);
take b;
thus P[ x,b ];
end;
consider f being (Function of (the carrier of L1),(the carrier of L2)) such that A2: (for x being (Element of L1) holds P[ x,( f . x ) ]) from FUNCT_2:sch 3(A1);
take f;
now
let a1;
let b1;
thus ( f . ( a1 "\/" b1 ) ) = b by A2
.= ( b "\/" b ) by LATTICES:1
.= ( ( f . a1 ) "\/" b ) by A2
.= ( ( f . a1 ) "\/" ( f . b1 ) ) by A2;
thus ( f . ( a1 "/\" b1 ) ) = b by A2
.= ( b "/\" b ) by LATTICES:2
.= ( ( f . a1 ) "/\" b ) by A2
.= ( ( f . a1 ) "/\" ( f . b1 ) ) by A2;
end;
hence thesis;
end;
end;
