begin
definition
let D being non empty set;
let M being (Matrix of D);
redefine attr M is  empty-yielding
means
:Def5: (( 0 ) = ( len M ) or ( 0 ) = ( width M ));
compatibility
proof
consider n being Nat such that A1: (for x being set holds (x in ( rng M ) implies (ex s being FinSequence st (s = x & ( len s ) = n)))) by MATRIX_1:def 1;
hereby
set s = the (Element of ( rng M ));
assume A2: M is  empty-yielding;
assume A3: ( 0 ) <> ( len M );
then A4: M <> ( {} );
then (ex p being FinSequence st (p = s & ( len p ) = n)) by A1;
then reconsider s as FinSequence;
s = ( 0 ) by A2,A4,RELAT_1:149;
hence ( 0 ) = ( width M ) by A3,A4,CARD_1:27,MATRIX_1:def 3;
end;
assume A5: (( 0 ) = ( len M ) or ( 0 ) = ( width M ));
now
let s being set;
assume A6: s in ( rng M );
then M <> ( {} );
then ( len M ) > ( 0 );
then consider p being FinSequence such that A7: p in ( rng M ) and A8: ( len p ) = ( 0 ) by A5,MATRIX_1:def 3;
(ex q being FinSequence st (q = p & ( len q ) = n)) by A1,A7;
then (ex q being FinSequence st (q = s & ( len q ) = ( 0 ))) by A1,A6,A8;
hence s = ( {} );
end;
hence thesis by RELAT_1:149;
end;
end;
