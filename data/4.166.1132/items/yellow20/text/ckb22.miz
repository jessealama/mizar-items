theorem
(for A,B being AltCatStr holds (A,B have_the_same_composition  implies ( Intersect (A,B) ) = ( Intersect (B,A) )))
proof
let A being AltCatStr,B being AltCatStr;
set AB = ( Intersect (A,B) );
assume A1: A,B have_the_same_composition ;
then A2: (the Comp of AB) = ( Intersect ((the Comp of A),(the Comp of B)) ) by CKB21:def 1;
((the carrier of AB) = ( (the carrier of A) /\ (the carrier of B) ) & (the Arrows of AB) = ( Intersect ((the Arrows of A),(the Arrows of B)) )) by A1,CKB21:def 1;
hence thesis by A1,A2,CKB21:def 1;
end;
