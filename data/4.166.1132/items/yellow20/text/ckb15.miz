begin
theorem
(for I being set holds (for A,B being (ManySortedSet of I) holds ( Intersect (A,B) ) = ( A /\ B )))
proof
let I being set;
let A being (ManySortedSet of I);
let B being (ManySortedSet of I);
A1: (( dom A ) = I & ( dom B ) = I) by PARTFUN1:def 2;
then ( dom ( Intersect (A,B) ) ) = ( I /\ I ) by CKB14:def 1;
then reconsider AB = ( Intersect (A,B) ) as (ManySortedSet of I) by PARTFUN1:def 2,RELAT_1:def 18;
( I /\ I ) = I;
then (for i being set holds (i in I implies ( AB . i ) = ( ( A . i ) /\ ( B . i ) ))) by A1,CKB14:def 1;
hence thesis by PBOOLE:def 5;
end;
