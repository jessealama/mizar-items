theorem
Th30: (for A,B being category holds (for C being non  empty (subcategory of A) holds (for F being  covariant (Functor of A,B) holds (for a,b being (object of A) holds (for c,d being (object of C) holds (((c = a & d = b) & <^ c,d ^> <> ( {} )) implies (for f being (Morphism of a,b) holds (for g being (Morphism of c,d) holds (g = f implies ( ( F | C ) . g ) = ( F . f ))))))))))
proof
let A being category;
let B being category;
let C being non  empty (subcategory of A);
let F being  covariant (Functor of A,B);
let a being (object of A);
let b being (object of A);
let c being (object of C);
let d being (object of C);
assume that
A1: (c = a & d = b)
and
A2: <^ c,d ^> <> ( {} );
let f being (Morphism of a,b);
let g being (Morphism of c,d);
assume g = f;
then A3: ( ( incl C ) . g ) = f by A2,CKB34:1;
(( ( incl C ) . c ) = a & ( ( incl C ) . d ) = b) by A1,FUNCTOR0:27;
hence thesis by A2,A3,FUNCTOR3:6;
end;
