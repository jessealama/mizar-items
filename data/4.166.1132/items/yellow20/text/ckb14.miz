begin
reserve x for set;
definition
let f being Function;
let g being Function;
func Intersect (f,g) -> Function means 
:Def2: (( dom it ) = ( ( dom f ) /\ ( dom g ) ) & (for x being set holds (x in ( ( dom f ) /\ ( dom g ) ) implies ( it . x ) = ( ( f . x ) /\ ( g . x ) ))));
existence
proof
deffunc F(set) = ( ( f . $1 ) /\ ( g . $1 ) );
thus (ex h being Function st (( dom h ) = ( ( dom f ) /\ ( dom g ) ) & (for x being set holds (x in ( ( dom f ) /\ ( dom g ) ) implies ( h . x ) = F(x))))) from FUNCT_1:sch 3;
end;
uniqueness
proof
let f1 being Function;
let f2 being Function;
assume that
A1: ( dom f1 ) = ( ( dom f ) /\ ( dom g ) )
and
A2: (for x being set holds (x in ( ( dom f ) /\ ( dom g ) ) implies ( f1 . x ) = ( ( f . x ) /\ ( g . x ) )))
and
A3: ( dom f2 ) = ( ( dom f ) /\ ( dom g ) )
and
A4: (for x being set holds (x in ( ( dom f ) /\ ( dom g ) ) implies ( f2 . x ) = ( ( f . x ) /\ ( g . x ) )));
now
let x being set;
assume A5: x in ( ( dom f ) /\ ( dom g ) );
then ( f1 . x ) = ( ( f . x ) /\ ( g . x ) ) by A2;
hence ( f1 . x ) = ( f2 . x ) by A4,A5;
end;
hence thesis by A1,A3,FUNCT_1:2;
end;
commutativity;
end;
