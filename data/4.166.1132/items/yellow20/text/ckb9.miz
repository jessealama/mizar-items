theorem
(for A,B being category holds (for F being  contravariant (Functor of A,B) holds (F is  bijective implies (for G being  contravariant (Functor of B,A) holds (((for a being (object of A) holds ( G . ( F . a ) ) = a) & (for a,b being (object of A) holds (<^ a,b ^> <> ( {} ) implies (for f being (Morphism of a,b) holds ( G . ( F . f ) ) = f)))) implies ( the FunctorStr of G ) = ( F " ))))))
proof
let A being category,B being category;
let F being  contravariant (Functor of A,B);
assume that
A1: F is  bijective;
let G being  contravariant (Functor of B,A);
assume that
A2: (for b being (object of A) holds ( G . ( F . b ) ) = b)
and
A3: (for a,b being (object of A) holds (<^ a,b ^> <> ( {} ) implies (for f being (Morphism of a,b) holds ( G . ( F . f ) ) = f)));
A4:now
let a being (object of A),b being (object of A);
assume that
A5: <^ a,b ^> <> ( {} );
let f being (Morphism of a,b);
thus ( ( G * F ) . f ) = ( G . ( F . f ) ) by A5,FUNCTOR3:7
.= f by A3,A5
.= ( ( id A ) . f ) by A5,FUNCTOR0:31;
end;
now
let b being (object of A);
thus ( ( G * F ) . b ) = ( G . ( F . b ) ) by FUNCTOR0:33
.= b by A2
.= ( ( id A ) . b ) by FUNCTOR0:29;
end;
hence thesis by A1,A4,CKB5:1,YELLOW18:1;
