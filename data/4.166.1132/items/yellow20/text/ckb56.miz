theorem
(for A,B being category holds (for F being  contravariant (Functor of A,B) holds (F is  bijective implies A,B are_anti-isomorphic_under F)))
proof
let A being category;
let B being category;
let F being  contravariant (Functor of A,B);
assume that
A1: F is  bijective;
((the Arrows of A) = (the Arrows of A) & (the Arrows of B) = (the Arrows of B));
hence (A is (subcategory of A) & B is (subcategory of B)) by ALTCAT_2:20,ALTCAT_4:35;
take F;
thus (F is  bijective & (for a9 being (object of A) holds (for a being (object of A) holds (a9 = a implies ( F . a9 ) = ( F . a ))))) by A1;
let b9 being (object of A);
let c9 being (object of A);
let b being (object of A);
let c being (object of A);
assume A2: ((<^ b9,c9 ^> <> ( {} ) & b9 = b) & c9 = c);
then <^ ( F . c ),( F . b ) ^> <> ( {} ) by FUNCTOR0:def 19;
hence thesis by A2,FUNCTOR0:def 16;
end;
