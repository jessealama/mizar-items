theorem
(for A,B being category holds (for F being  covariant (Functor of A,B) holds (A,B are_isomorphic_under F implies F is  bijective)))
proof
let A being category;
let B being category;
let F being  covariant (Functor of A,B);
assume that
A is (subcategory of A)
and
B is (subcategory of B);
given G being  covariant (Functor of A,B) such that
A1: G is  bijective
and
A2: (for a9 being (object of A) holds (for a being (object of A) holds (a9 = a implies ( G . a9 ) = ( F . a ))))
and
A3: (for b9,c9 being (object of A) holds (for b,c being (object of A) holds (((<^ b9,c9 ^> <> ( {} ) & b9 = b) & c9 = c) implies (for f9 being (Morphism of b9,c9) holds (for f being (Morphism of b,c) holds (f9 = f implies ( G . f9 ) = ( ( Morph-Map (F,b,c) ) . f )))))));

G is  injective  surjective by A1,FUNCTOR0:def 35;
then A4: G is  one-to-one  faithful  full  onto by FUNCTOR0:def 33,FUNCTOR0:def 34;
A5:now
let a being (object of A);
let b being (object of A);
assume that
A6: <^ a,b ^> <> ( {} );
let f being (Morphism of a,b);
<^ ( F . a ),( F . b ) ^> <> ( {} ) by A6,FUNCTOR0:def 18;
hence ( F . f ) = ( ( Morph-Map (F,a,b) ) . f ) by A6,FUNCTOR0:def 15
.= ( G . f ) by A3,A6;
end;
(for a being (object of A) holds ( F . a ) = ( G . a )) by A2;
then ( the FunctorStr of F ) = ( the FunctorStr of G ) by A5,YELLOW18:1;
hence ((((the ObjectMap of F) is  one-to-one & (the MorphMap of F) is  "1-1") & (ex f being (ManySortedFunction of (the Arrows of A),( (the Arrows of B) * (the ObjectMap of F) )) st (f = (the MorphMap of F) & f is  "onto"))) & (the ObjectMap of F) is  onto) by A4,FUNCTOR0:def 6,FUNCTOR0:def 7,FUNCTOR0:def 30,FUNCTOR0:def 32;
end;
