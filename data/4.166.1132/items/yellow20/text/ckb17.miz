begin
theorem
Th15: (for I,J being set holds (for A being (ManySortedSet of I) holds (for B being Function holds (for C being (ManySortedSet of J) holds (C = ( Intersect (A,B) ) implies C cc= A)))))
proof
let I being set;
let J being set;
let A being (ManySortedSet of I);
let B being Function;
let C being (ManySortedSet of J);
assume that
A1: C = ( Intersect (A,B) );
A2: ( dom A ) = I by PARTFUN1:def 2;
( dom C ) = J by PARTFUN1:def 2;
then A3: J = ( I /\ ( dom B ) ) by A1,A2,CKB14:def 1;
hence J c= I by XBOOLE_1:17;
let i being set;
assume i in J;
then ( C . i ) = ( ( A . i ) /\ ( B . i ) ) by A1,A2,A3,CKB14:def 1;
hence thesis by XBOOLE_1:17;
end;
