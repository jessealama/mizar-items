begin
theorem
Th24: (for A,B being AltCatStr holds (A,B have_the_same_composition  implies (for a1,a2 being (object of A) holds (for b1,b2 being (object of B) holds (for o1,o2 being (object of ( Intersect (A,B) )) holds ((((((o1 = a1 & o1 = b1) & o2 = a2) & o2 = b2) & <^ a1,a2 ^> <> ( {} )) & <^ b1,b2 ^> <> ( {} )) implies (for f being (Morphism of a1,a2) holds (for g being (Morphism of b1,b2) holds (f = g implies f in <^ o1,o2 ^>)))))))))
proof
let A being AltCatStr,B being AltCatStr;
assume that
A1: A,B have_the_same_composition ;
let a1 being (object of A),a2 being (object of A);
let b1 being (object of B),b2 being (object of B);
let o1 being (object of ( Intersect (A,B) )),o2 being (object of ( Intersect (A,B) ));
assume (((o1 = a1 & o1 = b1) & o2 = a2) & o2 = b2);
then <^ o1,o2 ^> = ( <^ a1,a2 ^> /\ <^ b1,b2 ^> ) by A1,CKB24:1;
hence thesis by XBOOLE_0:def 4;
end;
