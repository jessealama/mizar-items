(for f being S-Sequence_in_R2 holds (for g being (FinSequence of ( TOP-REAL 2 )) holds (((((g is  unfolded  s.n.c.  one-to-one & ( ( L~ f ) /\ ( L~ g ) ) = { ( f /. 1 ) }) & ( f /. 1 ) = ( g /. ( len g ) )) & (for i being (Element of ( NAT )) holds ((1 <= i & ( i + 2 ) <= ( len f )) implies ( ( LSeg (f,i) ) /\ ( LSeg (( f /. ( len f ) ),( g /. 1 )) ) ) = ( {} )))) & (for i being (Element of ( NAT )) holds ((2 <= i & ( i + 1 ) <= ( len g )) implies ( ( LSeg (g,i) ) /\ ( LSeg (( f /. ( len f ) ),( g /. 1 )) ) ) = ( {} )))) implies ( f ^ g ) is  s.c.c.)))
proof
let f being S-Sequence_in_R2;
let g being (FinSequence of ( TOP-REAL 2 ));
assume that
A1: g is  unfolded  s.n.c.  one-to-one
and
A2: ( ( L~ f ) /\ ( L~ g ) ) = { ( f /. 1 ) }
and
A3: ( f /. 1 ) = ( g /. ( len g ) )
and
A4: (for i being (Element of ( NAT )) holds ((1 <= i & ( i + 2 ) <= ( len f )) implies ( ( LSeg (f,i) ) /\ ( LSeg (( f /. ( len f ) ),( g /. 1 )) ) ) = ( {} )))
and
A5: (for i being (Element of ( NAT )) holds ((2 <= i & ( i + 1 ) <= ( len g )) implies ( ( LSeg (g,i) ) /\ ( LSeg (( f /. ( len f ) ),( g /. 1 )) ) ) = ( {} )));
let i being (Element of ( NAT )),j being (Element of ( NAT ));
assume that
A6: ( i + 1 ) < j
and
A7: ((i > 1 & j < ( len ( f ^ g ) )) or ( j + 1 ) < ( len ( f ^ g ) ));
A8: ( j + 1 ) <= ( len ( f ^ g ) ) by A7,NAT_1:13;
A9:now
let i being (Element of ( NAT ));
assume that
A10: 2 <= i
and
A11: ( i + 1 ) <= ( len g );
( ( LSeg (g,i) ) /\ ( LSeg (( f /. ( len f ) ),( g /. 1 )) ) ) = ( {} ) by A5,A10,A11;
hence ( LSeg (g,i) ) misses ( LSeg (( f /. ( len f ) ),( g /. 1 )) ) by XBOOLE_0:def 7;
end;
A12:now
let i being (Element of ( NAT ));
assume that
A13: 1 <= i
and
A14: ( i + 2 ) <= ( len f );
( ( LSeg (f,i) ) /\ ( LSeg (( f /. ( len f ) ),( g /. 1 )) ) ) = ( {} ) by A4,A13,A14;
hence ( LSeg (f,i) ) misses ( LSeg (( f /. ( len f ) ),( g /. 1 )) ) by XBOOLE_0:def 7;
