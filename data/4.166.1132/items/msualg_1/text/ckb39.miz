environ
vocabularies STRUCT_0,FUNCT_1,RELAT_1,XBOOLE_0,FUNCOP_1,SUBSET_1,MARGREL1,PBOOLE,CARD_1,NAT_1,UNIALG_1,FUNCT_2,PARTFUN1,FINSEQ_2,TARSKI,FINSEQ_1,NUMBERS,ZFMISC_1,CARD_3,MSUALG_1;
notations TARSKI,XBOOLE_0,SUBSET_1,CARD_1,NUMBERS,NAT_1,RELAT_1,FUNCT_1,FUNCT_2,FINSEQ_1,STRUCT_0,PARTFUN1,FINSEQ_2,CARD_3,FUNCOP_1,PBOOLE,MARGREL1,UNIALG_1,CKB1,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB16,CKB17,CKB18,CKB24,CKB27,CKB29,CKB30,CKB31,CKB33,CKB38;
definitions TARSKI,FINSEQ_1,FUNCT_1,PBOOLE,STRUCT_0,XBOOLE_0,UNIALG_1,FUNCOP_1,MARGREL1,CKB6,CKB7,CKB10,CKB16,CKB17,CKB18,CKB24,CKB27,CKB29,CKB30,CKB31,CKB33,CKB38;
theorems TARSKI,FUNCT_1,PARTFUN1,FINSEQ_1,FUNCOP_1,UNIALG_1,PBOOLE,FUNCT_2,CARD_3,FINSEQ_3,FINSEQ_2,RELAT_1,RELSET_1,STRUCT_0,ORDINAL1,MARGREL1,CARD_1,CKB6,CKB7,CKB10,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB27,CKB29,CKB30,CKB31,CKB33,CKB35,CKB36,CKB37,CKB38;
registrations XBOOLE_0,FUNCT_1,ORDINAL1,RELSET_1,FUNCOP_1,FINSEQ_2,RELAT_1,PBOOLE,STRUCT_0,UNIALG_1,FUNCT_2,CARD_1,ZFMISC_1,MARGREL1,CKB1,CKB2,CKB3,CKB8,CKB9,CKB11,CKB12,CKB13,CKB14,CKB15,CKB26,CKB28,CKB32,CKB34;
constructors CARD_3,PBOOLE,REALSET2,UNIALG_1,RELSET_1,MARGREL1,CKB1,CKB6,CKB7,CKB8,CKB9,CKB10,CKB16,CKB17,CKB18,CKB24,CKB27,CKB29,CKB30,CKB31,CKB33,CKB38;
requirements BOOLE,SUBSET,NUMERALS;
begin
reserve n for Nat;
reserve MS for  segmental non  void 1 -element ManySortedSign;
reserve A for  non-empty MSAlgebra over MS;
reserve h for (PartFunc of ( ( the_sort_of A ) * ),( the_sort_of A ));
reserve x for (FinSequence of ( the_sort_of A ));
reserve y for (FinSequence of ( the_sort_of A ));
definition
let MS;
let A;
func 1-Alg A ->  non-empty  strict Universal_Algebra equals 
UAStr (# ( the_sort_of A ),( the_charact_of A ) #);
coherence
proof
A1: ( the_charact_of A ) is  quasi_total
proof
let n;
let h;
assume that
A2: n in ( dom ( the_charact_of A ) )
and
A3: h = ( ( the_charact_of A ) . n );
reconsider o = n as (OperSymbol of MS) by A2,PARTFUN1:def 2;
let x;
let y;
assume that
A4: ( len x ) = ( len y )
and
A5: x in ( dom h );
A6: ( ( ( (the Sorts of A) # ) * (the Arity of MS) ) . o ) = ( Args (o,A) );
h is (Function of ( ( ( (the Sorts of A) # ) * (the Arity of MS) ) . o ),( ( (the Sorts of A) * (the ResultSort of MS) ) . o )) by A3,PBOOLE:def 15;
then A7: ( dom h ) = ( ( ( (the Sorts of A) # ) * (the Arity of MS) ) . o ) by FUNCT_2:def 1
.= ( ( len ( the_arity_of o ) ) -tuples_on ( the_sort_of A ) ) by A6,CKB35:1;
then ( len y ) = ( len ( the_arity_of o ) ) by A4,A5,CARD_1:def 7;
then y is (Element of ( dom h )) by A7,FINSEQ_2:92;
hence thesis by A5;
end;
A8: ( the_charact_of A ) is  homogeneous
proof
let n;
let h;
assume that
A9: n in ( dom ( the_charact_of A ) )
and
A10: h = ( ( the_charact_of A ) . n );
reconsider o = n as (OperSymbol of MS) by A9,PARTFUN1:def 2;
thus ( dom h ) is  with_common_domain
proof
let x being FinSequence;
let y being FinSequence;
assume that
A11: x in ( dom h )
and
A12: y in ( dom h );
A13: ( ( ( (the Sorts of A) # ) * (the Arity of MS) ) . o ) = ( Args (o,A) );
h is (Function of ( ( ( (the Sorts of A) # ) * (the Arity of MS) ) . o ),( ( (the Sorts of A) * (the ResultSort of MS) ) . o )) by A10,PBOOLE:def 15;
then A14: ( dom h ) = ( ( ( (the Sorts of A) # ) * (the Arity of MS) ) . o ) by FUNCT_2:def 1
.= ( ( len ( the_arity_of o ) ) -tuples_on ( the_sort_of A ) ) by A13,CKB35:1;
hence ( len x ) = ( len ( the_arity_of o ) ) by A11,CARD_1:def 7
.= ( len y ) by A12,A14,CARD_1:def 7;
end;

end;
( the_charact_of A ) is  non-empty
proof
let n being set;
assume n in ( dom ( the_charact_of A ) );
then reconsider o = n as (OperSymbol of MS) by PARTFUN1:def 2;
set h = ( ( the_charact_of A ) . n );
h = ( Den (o,A) );
hence thesis;
end;
hence thesis by A1,A8,UNIALG_1:def 1,UNIALG_1:def 2,UNIALG_1:def 3;
end;
correctness;
end;
