begin
reserve S for non  empty ManySortedSign;
theorem
Th10: (for S being non  empty ManySortedSign holds (S is  trivial implies (for A being MSAlgebra over S holds (for c1,c2 being (Component of (the Sorts of A)) holds c1 = c2))))
proof
let S being non  empty ManySortedSign;
assume that
A1: S is  trivial;
let A being MSAlgebra over S;
let c1 being (Component of (the Sorts of A));
let c2 being (Component of (the Sorts of A));
((ex i1 being set st (i1 in (the carrier of S) & c1 = ( (the Sorts of A) . i1 ))) & (ex i2 being set st (i2 in (the carrier of S) & c2 = ( (the Sorts of A) . i2 )))) by PBOOLE:138;
hence thesis by A1,STRUCT_0:def 10;
end;
