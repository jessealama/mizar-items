notations ORDINAL1,SUBSET_1,NUMBERS,XREAL_0,REAL_1,RCOMP_1,STRUCT_0,RELAT_1,FUNCT_1,TOPREAL1,TOPREAL2,PRE_TOPC,TOPS_2,BORSUK_1,EUCLID,JORDAN5C,PSCOMP_1,JORDAN6,XXREAL_0,CKB9;
