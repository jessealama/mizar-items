environ
vocabularies NUMBERS,TOPREAL2,PRE_TOPC,EUCLID,SUBSET_1,TOPREAL1,FUNCT_1,BORSUK_1,RELAT_1,REAL_1,TOPS_2,CARD_1,XXREAL_0,STRUCT_0,XXREAL_1,JORDAN3,PSCOMP_1,JORDAN6,ARYTM_3,XREAL_0,ORDINAL1,XBOOLE_0,JORDAN17;
notations ORDINAL1,SUBSET_1,NUMBERS,XREAL_0,REAL_1,RCOMP_1,STRUCT_0,RELAT_1,FUNCT_1,TOPREAL1,TOPREAL2,PRE_TOPC,TOPS_2,BORSUK_1,EUCLID,JORDAN5C,PSCOMP_1,JORDAN6,XXREAL_0,CKB9;
definitions TOPREAL1,STRUCT_0,CKB9;
theorems XBOOLE_0,JORDAN5C,JORDAN6,JORDAN7,TOPS_2,FUNCT_1,BORSUK_1,PRE_TOPC,TOPREAL5,XREAL_0,SPRECT_1,JORDAN16,XREAL_1,XXREAL_0,XXREAL_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16;
registrations RELSET_1,XREAL_0,MEMBERED,TOPREAL2,EUCLID;
constructors REAL_1,RCOMP_1,TOPS_2,COMPTS_1,TOPMETR,TOPREAL1,PSCOMP_1,JORDAN5C,JORDAN6,CKB9;
requirements NUMERALS,REAL,SUBSET,BOOLE;
begin
reserve P for Simple_closed_curve;
reserve a for (Point of ( TOP-REAL 2 ));
reserve b for (Point of ( TOP-REAL 2 ));
reserve c for (Point of ( TOP-REAL 2 ));
reserve d for (Point of ( TOP-REAL 2 ));
reserve e for (Point of ( TOP-REAL 2 ));
theorem
((b <> c & a,b,c,d are_in_this_order_on P) implies (ex e st ((e <> b & e <> c) & b,e,c,d are_in_this_order_on P)))
proof
assume that
A1: b <> c
and
A2: ((((( LE a,b,P &  LE b,c,P) &  LE c,d,P) or (( LE b,c,P &  LE c,d,P) &  LE d,a,P)) or (( LE c,d,P &  LE d,a,P) &  LE a,b,P)) or (( LE d,a,P &  LE a,b,P) &  LE b,c,P));
per cases  by A2;
suppose A3: (( LE a,b,P &  LE b,c,P) &  LE c,d,P);

then consider e such that A4: (((e <> b & e <> c) &  LE b,e,P) &  LE e,c,P) by A1,CKB8:1;
take e;
thus thesis by A3,A4,CKB9:def 1;
end;
suppose A5: (( LE b,c,P &  LE c,d,P) &  LE d,a,P);

then consider e such that A6: (((e <> b & e <> c) &  LE b,e,P) &  LE e,c,P) by A1,CKB8:1;
take e;
thus thesis by A5,A6,CKB9:def 1;
end;
suppose that A7:  LE c,d,P
and
A8:  LE d,a,P
and
A9:  LE a,b,P;
thus thesis
proof
A10:  LE d,b,P by A8,A9,JORDAN6:58;
per cases ;
suppose A11: c = ( W-min P );

b in P by A9,JORDAN7:5;
then consider e such that A12: e <> b and A13:  LE b,e,P by CKB7:1;
take e;
thus e <> b by A12;
thus e <> c by A1,A11,A13,JORDAN7:2;
thus thesis by A7,A10,A13,CKB9:def 1;
end;
suppose A14: c <> ( W-min P );

take e = ( W-min P );
c in P by A7,JORDAN7:5;
then A15:  LE e,c,P by JORDAN7:3;
now
 LE c,a,P by A7,A8,JORDAN6:58;
then A16:  LE c,b,P by A9,JORDAN6:58;
assume e = b;
hence contradiction by A1,A15,A16,JORDAN6:57;
end;
hence e <> b;
thus e <> c by A14;
thus thesis by A7,A10,A15,CKB9:def 1;
end;
end;

end;
suppose that A17: ( LE d,a,P &  LE a,b,P)
and
A18:  LE b,c,P;
consider e such that A19: (e <> b & e <> c) and A20: ( LE b,e,P &  LE e,c,P) by A1,A18,CKB8:1;
take e;
thus (e <> b & e <> c) by A19;
 LE d,b,P by A17,JORDAN6:58;
hence thesis by A20,CKB9:def 1;
end;
end;
