environ
vocabularies NUMBERS,NAT_1,XREAL_0,ORDINAL1,SUBSET_1,FINSEQ_2,PRE_TOPC,EUCLID,RVSUM_1,FUNCT_1,CARD_1,FINSEQ_1,RELAT_1,VALUED_0,ARYTM_3,COMPLEX1,SQUARE_1,ARYTM_1,CARD_3,XXREAL_0,TARSKI,FUNCT_3,SUPINF_2;
notations TARSKI,SUBSET_1,RELAT_1,FUNCT_1,ORDINAL1,NUMBERS,XCMPLX_0,XREAL_0,COMPLEX1,NAT_1,FUNCOP_1,FINSEQ_1,FINSEQ_2,VALUED_0,QUIN_1,STRUCT_0,PRE_TOPC,RVSUM_1,BINOP_2,RLVECT_1,EUCLID,SQUARE_1,XXREAL_0;
definitions SQUARE_1,FINSEQ_2,RVSUM_1,VALUED_1,ALGSTR_0;
theorems EUCLID,RVSUM_1,FINSEQ_2,FINSEQ_1,FINSEQ_3,SQUARE_1,QUIN_1,ABSVALUE,TOPRNS_1,JORDAN2C,COMPLEX1,XREAL_1,XXREAL_0,FUNCOP_1,XCMPLX_1,FINSEQOP,ORDINAL1,FUNCT_1,CARD_1,CKB1,CKB2;
registrations FUNCT_1,RELSET_1,FUNCOP_1,NUMBERS,XREAL_0,SQUARE_1,BINOP_2,MEMBERED,FINSEQ_2,RVSUM_1,MONOID_0,EUCLID,VALUED_0,CARD_1,FINSEQ_1;
constructors REAL_1,SQUARE_1,BINOP_2,COMPLEX1,QUIN_1,FINSEQOP,MONOID_0,TOPRNS_1,SEQ_1,RELSET_1;
requirements REAL,SUBSET,NUMERALS,ARITHM,BOOLE;
begin
reserve n for Nat;
theorem
Th10: (for y1,y2 being  real-valued FinSequence holds (for x1,x2 being (Element of ( REAL n )) holds ((x1 = y1 & x2 = y2) implies |( y1,y2 )| = ( ( 1 / 4 ) * ( ( |. ( x1 + x2 ) .| ^2 ) - ( |. ( x1 - x2 ) .| ^2 ) ) ))))
proof
let y1 being  real-valued FinSequence;
let y2 being  real-valued FinSequence;
let x1 being (Element of ( REAL n ));
let x2 being (Element of ( REAL n ));
assume A1: (x1 = y1 & x2 = y2);
reconsider w1 = x1,w2 = x2 as (Element of ( n -tuples_on ( REAL ) )) by EUCLID:def 1;
set v1 = ( sqr ( x1 + x2 ) );
set v2 = ( sqr ( x1 - x2 ) );
set z1 = ( x1 + x2 );
set z2 = ( x1 - x2 );
A2: ( ( 1 / 4 ) * ( ( |. ( x1 + x2 ) .| ^2 ) - ( |. ( x1 - x2 ) .| ^2 ) ) ) = ( ( 1 / 4 ) * ( ( ( sqrt ( Sum ( sqr z1 ) ) ) ^2 ) - ( |. z2 .| ^2 ) ) ) by EUCLID:def 5
.= ( ( 1 / 4 ) * ( ( ( sqrt ( Sum ( sqr z1 ) ) ) ^2 ) - ( ( sqrt ( Sum ( sqr z2 ) ) ) ^2 ) ) ) by EUCLID:def 5;
( Sum ( sqr ( x1 + x2 ) ) ) >= ( 0 ) by RVSUM_1:86;
then A3: ( ( sqrt ( Sum ( sqr ( x1 + x2 ) ) ) ) ^2 ) = ( Sum ( sqr ( x1 + x2 ) ) ) by SQUARE_1:def 2;
A4: ( ( Sum ( sqr ( x1 + x2 ) ) ) - ( Sum ( sqr ( x1 - x2 ) ) ) ) = ( Sum ( v1 - v2 ) ) by RVSUM_1:90;
( Sum ( sqr ( x1 - x2 ) ) ) >= ( 0 ) by RVSUM_1:86;
then A5: ( ( sqrt ( Sum ( sqr ( x1 - x2 ) ) ) ) ^2 ) = ( Sum ( sqr ( x1 - x2 ) ) ) by SQUARE_1:def 2;
A6: ( ( ( 2 * ( mlt (w1,w2) ) ) + ( sqr w2 ) ) + ( 2 * ( mlt (w1,w2) ) ) ) = ( ( 2 * ( mlt (w1,w2) ) ) + ( ( 2 * ( mlt (w1,w2) ) ) + ( sqr w2 ) ) )
.= ( ( ( 2 * ( mlt (w1,w2) ) ) + ( 2 * ( mlt (w1,w2) ) ) ) + ( sqr w2 ) ) by FINSEQOP:28;
A7: ( ( sqr w1 ) + ( ( 2 * ( mlt (w1,w2) ) ) + ( sqr w2 ) ) ) = ( ( ( 2 * ( mlt (w1,w2) ) ) + ( sqr w2 ) ) + ( sqr w1 ) );
( v1 - v2 ) = ( ( ( ( sqr w1 ) + ( 2 * ( mlt (w1,w2) ) ) ) + ( sqr w2 ) ) - ( sqr ( w1 - w2 ) ) ) by RVSUM_1:68
.= ( ( ( ( sqr w1 ) + ( 2 * ( mlt (w1,w2) ) ) ) + ( sqr w2 ) ) - ( ( ( sqr w1 ) - ( 2 * ( mlt (w1,w2) ) ) ) + ( sqr w2 ) ) ) by RVSUM_1:69
.= ( ( ( ( 2 * ( mlt (w1,w2) ) ) + ( sqr w2 ) ) + ( sqr w1 ) ) - ( ( ( sqr w1 ) - ( 2 * ( mlt (w1,w2) ) ) ) + ( sqr w2 ) ) ) by A7,FINSEQOP:28
.= ( ( ( ( ( 2 * ( mlt (w1,w2) ) ) + ( sqr w2 ) ) + ( sqr w1 ) ) - ( ( sqr w1 ) - ( 2 * ( mlt (w1,w2) ) ) ) ) - ( sqr w2 ) ) by RVSUM_1:39
.= ( ( ( ( ( ( 2 * ( mlt (w1,w2) ) ) + ( sqr w2 ) ) + ( sqr w1 ) ) - ( sqr w1 ) ) + ( 2 * ( mlt (w1,w2) ) ) ) - ( sqr w2 ) ) by RVSUM_1:41
.= ( ( ( ( 2 * ( mlt (w1,w2) ) ) + ( sqr w2 ) ) + ( 2 * ( mlt (w1,w2) ) ) ) - ( sqr w2 ) ) by RVSUM_1:42
.= ( ( 2 * ( mlt (w1,w2) ) ) + ( 2 * ( mlt (w1,w2) ) ) ) by A6,RVSUM_1:42
.= ( ( 2 + 2 ) * ( mlt (w1,w2) ) ) by RVSUM_1:50
.= ( 4 * ( mlt (w1,w2) ) );
then ( ( 1 / 4 ) * ( ( |. ( x1 + x2 ) .| ^2 ) - ( |. ( x1 - x2 ) .| ^2 ) ) ) = ( ( 1 / 4 ) * ( 4 * ( Sum ( mlt (w1,w2) ) ) ) ) by A2,A3,A5,A4,RVSUM_1:87
.= ( Sum ( mlt (w1,w2) ) );
hence thesis by A1;
end;
