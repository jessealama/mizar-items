constructors PARTFUN1,XXREAL_0,REAL_1,NAT_1,SEQ_1,COMSEQ_2,CFUNCT_1,COMSEQ_3,RECDEF_1,SEQM_3,VALUED_1,RELSET_1,CKB1;
requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve n1 for (Element of ( NAT ));
reserve m for (Element of ( NAT ));
reserve seq for Complex_Sequence;
reserve seq1 for Complex_Sequence;
reserve p for Real;
reserve Nseq for  increasing (sequence of ( NAT ));
theorem
Th40: ((seq1 is (subsequence of seq) & seq is  convergent) implies ( lim seq1 ) = ( lim seq ))
proof
assume that
A1: seq1 is (subsequence of seq)
and
A2: seq is  convergent;
consider Nseq such that A3: seq1 = ( seq * Nseq ) by A1,VALUED_0:def 17;
A4:now
let p;
assume ( 0 ) < p;
then consider n1 such that A5: (for m holds (n1 <= m implies |. ( ( seq . m ) - ( lim seq ) ) .| < p)) by A2,COMSEQ_2:def 5;
take n = n1;
let m;
assume that
A6: n <= m;
m <= ( Nseq . m ) by SEQM_3:14;
then A7: n <= ( Nseq . m ) by A6,XXREAL_0:2;
( seq1 . m ) = ( seq . ( Nseq . m ) ) by A3,FUNCT_2:15;
hence |. ( ( seq1 . m ) - ( lim seq ) ) .| < p by A5,A7;
end;
seq1 is  convergent by A1,A2,CKB18:1;
hence thesis by A4,COMSEQ_2:def 5;
end;
