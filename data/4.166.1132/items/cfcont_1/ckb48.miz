theorem
Th67: (for g holds (for X holds (for f holds (f is_continuous_on X implies ( g (#) f ) is_continuous_on X))))
proof
let g;
let X;
let f;
assume A1: f is_continuous_on X;
then A2: X c= ( dom f ) by CKB41:1;
then A3: X c= ( dom ( g (#) f ) ) by CFUNCT_1:4;
now
let s1;
assume that
A4: ( rng s1 ) c= X
and
A5: s1 is  convergent
and
A6: ( lim s1 ) in X;
A7: ( f /* s1 ) is  convergent by A1,A4,A5,A6,CKB41:1;
then A8: ( g (#) ( f /* s1 ) ) is  convergent by COMSEQ_2:17;
( f /. ( lim s1 ) ) = ( lim ( f /* s1 ) ) by A1,A4,A5,A6,CKB41:1;
then ( ( g (#) f ) /. ( lim s1 ) ) = ( g * ( lim ( f /* s1 ) ) ) by A3,A6,CFUNCT_1:4
.= ( lim ( g (#) ( f /* s1 ) ) ) by A7,COMSEQ_2:18
.= ( lim ( ( g (#) f ) /* s1 ) ) by A2,A4,CKB9:1,XBOOLE_1:1;
hence (( ( g (#) f ) /* s1 ) is  convergent & ( ( g (#) f ) /. ( lim s1 ) ) = ( lim ( ( g (#) f ) /* s1 ) )) by A2,A4,A8,CKB9:1,XBOOLE_1:1;
end;
hence thesis by A3,CKB41:1;
end;
