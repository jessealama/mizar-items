begin
reserve X for set;
reserve s1 for Complex_Sequence;
reserve f1 for (PartFunc of ( COMPLEX ),( COMPLEX ));
reserve f2 for (PartFunc of ( COMPLEX ),( COMPLEX ));
theorem
Th65: (for X holds (for f1 holds (for f2 holds ((f1 is_continuous_on X & f2 is_continuous_on X) implies ((( f1 + f2 ) is_continuous_on X & ( f1 - f2 ) is_continuous_on X) & ( f1 (#) f2 ) is_continuous_on X)))))
proof
let X;
let f1;
let f2;
assume A1: (f1 is_continuous_on X & f2 is_continuous_on X);
then (X c= ( dom f1 ) & X c= ( dom f2 )) by CKB41:1;
then A2: X c= ( ( dom f1 ) /\ ( dom f2 ) ) by XBOOLE_1:19;
then A3: X c= ( dom ( f1 + f2 ) ) by CFUNCT_1:1;
now
let s1;
assume that
A4: ( rng s1 ) c= X
and
A5: s1 is  convergent
and
A6: ( lim s1 ) in X;
A7: (( f1 /* s1 ) is  convergent & ( f2 /* s1 ) is  convergent) by A1,A4,A5,A6,CKB41:1;
then A8: ( ( f1 /* s1 ) + ( f2 /* s1 ) ) is  convergent by COMSEQ_2:13;
A9: ( rng s1 ) c= ( ( dom f1 ) /\ ( dom f2 ) ) by A2,A4,XBOOLE_1:1;
(( f1 /. ( lim s1 ) ) = ( lim ( f1 /* s1 ) ) & ( f2 /. ( lim s1 ) ) = ( lim ( f2 /* s1 ) )) by A1,A4,A5,A6,CKB41:1;
then ( ( f1 + f2 ) /. ( lim s1 ) ) = ( ( lim ( f1 /* s1 ) ) + ( lim ( f2 /* s1 ) ) ) by A3,A6,CFUNCT_1:1
.= ( lim ( ( f1 /* s1 ) + ( f2 /* s1 ) ) ) by A7,COMSEQ_2:14
.= ( lim ( ( f1 + f2 ) /* s1 ) ) by A9,CKB8:1;
hence (( ( f1 + f2 ) /* s1 ) is  convergent & ( ( f1 + f2 ) /. ( lim s1 ) ) = ( lim ( ( f1 + f2 ) /* s1 ) )) by A9,A8,CKB8:1;
end;
hence ( f1 + f2 ) is_continuous_on X by A3,CKB41:1;
A10: X c= ( dom ( f1 - f2 ) ) by A2,CFUNCT_1:2;
now
let s1;
assume that
A11: ( rng s1 ) c= X
and
A12: s1 is  convergent
and
A13: ( lim s1 ) in X;
A14: (( f1 /* s1 ) is  convergent & ( f2 /* s1 ) is  convergent) by A1,A11,A12,A13,CKB41:1;
then A15: ( ( f1 /* s1 ) - ( f2 /* s1 ) ) is  convergent by COMSEQ_2:25;
A16: ( rng s1 ) c= ( ( dom f1 ) /\ ( dom f2 ) ) by A2,A11,XBOOLE_1:1;
(( f1 /. ( lim s1 ) ) = ( lim ( f1 /* s1 ) ) & ( f2 /. ( lim s1 ) ) = ( lim ( f2 /* s1 ) )) by A1,A11,A12,A13,CKB41:1;
then ( ( f1 - f2 ) /. ( lim s1 ) ) = ( ( lim ( f1 /* s1 ) ) - ( lim ( f2 /* s1 ) ) ) by A10,A13,CFUNCT_1:2
.= ( lim ( ( f1 /* s1 ) - ( f2 /* s1 ) ) ) by A14,COMSEQ_2:26
.= ( lim ( ( f1 - f2 ) /* s1 ) ) by A16,CKB8:1;
hence (( ( f1 - f2 ) /* s1 ) is  convergent & ( ( f1 - f2 ) /. ( lim s1 ) ) = ( lim ( ( f1 - f2 ) /* s1 ) )) by A16,A15,CKB8:1;
end;
hence ( f1 - f2 ) is_continuous_on X by A10,CKB41:1;
A17: X c= ( dom ( f1 (#) f2 ) ) by A2,CFUNCT_1:3;
now
let s1;
assume that
A18: ( rng s1 ) c= X
and
A19: s1 is  convergent
and
A20: ( lim s1 ) in X;
A21: (( f1 /* s1 ) is  convergent & ( f2 /* s1 ) is  convergent) by A1,A18,A19,A20,CKB41:1;
then A22: ( ( f1 /* s1 ) (#) ( f2 /* s1 ) ) is  convergent by COMSEQ_2:29;
A23: ( rng s1 ) c= ( ( dom f1 ) /\ ( dom f2 ) ) by A2,A18,XBOOLE_1:1;
(( f1 /. ( lim s1 ) ) = ( lim ( f1 /* s1 ) ) & ( f2 /. ( lim s1 ) ) = ( lim ( f2 /* s1 ) )) by A1,A18,A19,A20,CKB41:1;
then ( ( f1 (#) f2 ) /. ( lim s1 ) ) = ( ( lim ( f1 /* s1 ) ) * ( lim ( f2 /* s1 ) ) ) by A17,A20,CFUNCT_1:3
.= ( lim ( ( f1 /* s1 ) (#) ( f2 /* s1 ) ) ) by A21,COMSEQ_2:30
.= ( lim ( ( f1 (#) f2 ) /* s1 ) ) by A23,CKB8:1;
hence (( ( f1 (#) f2 ) /* s1 ) is  convergent & ( ( f1 (#) f2 ) /. ( lim s1 ) ) = ( lim ( ( f1 (#) f2 ) /* s1 ) )) by A23,A22,CKB8:1;
end;
hence thesis by A17,CKB41:1;
end;
