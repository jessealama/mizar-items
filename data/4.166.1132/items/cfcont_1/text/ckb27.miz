theorem
Th48: (seq is  constant implies seq is  convergent)
proof
assume seq is  constant;
then consider t such that A1: (for n being Nat holds ( seq . n ) = t) by VALUED_0:def 18;
take g = t;
let p;
assume that
A2: ( 0 ) < p;
take n = ( 0 );
let m;
assume that
n <= m;
|. ( ( seq . m ) - g ) .| = |. ( t - g ) .| by A1
.= ( 0 ) by COMPLEX1:44;
hence |. ( ( seq . m ) - g ) .| < p by A2;
end;
