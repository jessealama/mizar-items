theorem
(x0 in ( dom f ) implies f is_continuous_on { x0 })
proof
assume A1: x0 in ( dom f );
thus { x0 } c= ( dom f )
proof
let x;
assume x in { x0 };
hence thesis by A1,TARSKI:def 1;
end;

let t;
assume that
A2: t in { x0 };
thus ( f | { x0 } ) is_continuous_in t
proof
t in ( dom f ) by A1,A2,TARSKI:def 1;
then t in ( ( dom f ) /\ { x0 } ) by A2,XBOOLE_0:def 4;
