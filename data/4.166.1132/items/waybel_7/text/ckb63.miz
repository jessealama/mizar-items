registrations XBOOLE_0,SUBSET_1,FINSET_1,STRUCT_0,PRE_TOPC,LATTICE3,YELLOW_0,WAYBEL_0,YELLOW_1,YELLOW_2,WAYBEL_1,YELLOW_3,WAYBEL_3,WAYBEL_4,YELLOW_7,WAYBEL_6,RELAT_1,CKB4,CKB5,CKB7,CKB8,CKB18,CKB22,CKB31,CKB41,CKB57;
constructors SETFAM_1,DOMAIN_1,TOPS_2,TEX_2,CANTOR_1,WAYBEL_1,YELLOW_3,YELLOW_4,WAYBEL_3,WAYBEL_4,WAYBEL_6,CKB16,CKB20,CKB30,CKB39,CKB40,CKB49,CKB56;
requirements SUBSET,BOOLE;
begin
Lm3:now
let L being  continuous  lower-bounded LATTICE;
let p being (Element of L);
assume that
A1: ( L -waybelow ) is  multiplicative
and
A2: (for a,b being (Element of L) holds (( a "/\" b ) << p implies (a <= p or b <= p)))
and
A3: (not p is  prime);
consider x,y being (Element of L) such that A4: ( x "/\" y ) <= p and A5: (not x <= p) and A6: (not y <= p) by A3,WAYBEL_6:def 6;
A7: (for a being (Element of L) holds ( waybelow a ) is non  empty  directed);
then consider u being (Element of L) such that A8: u << x and A9: (not u <= p) by A5,WAYBEL_3:24;
consider v being (Element of L) such that A10: v << y and A11: (not v <= p) by A6,A7,WAYBEL_3:24;
A12: [ v,y ] in ( L -waybelow ) by A10,WAYBEL_4:def 1;
[ u,x ] in ( L -waybelow ) by A8,WAYBEL_4:def 1;
then [ ( u "/\" v ),( x "/\" y ) ] in ( L -waybelow ) by A1,A12,CKB59:1;
then ( u "/\" v ) << ( x "/\" y ) by WAYBEL_4:def 1;
then ( u "/\" v ) << p by A4,WAYBEL_3:2;
hence contradiction by A2,A9,A11;
end;
theorem
(for L being  continuous  lower-bounded LATTICE holds (( L -waybelow ) is  multiplicative implies (for p being (Element of L) holds (p is  pseudoprime implies p is  prime))))
proof
let L being  continuous  lower-bounded LATTICE;
assume that
A1: ( L -waybelow ) is  multiplicative;
let p being (Element of L);
assume p is  pseudoprime;
then (for a,b being (Element of L) holds (( a "/\" b ) << p implies (a <= p or b <= p))) by A1,CKB62:1;
hence thesis by A1,Lm3;
end;
