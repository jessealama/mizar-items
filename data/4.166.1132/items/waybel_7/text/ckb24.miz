theorem
Th20: (for L being LATTICE holds (for x being set holds (x is  prime  prime (Ideal of L) iff x is  prime  prime (Filter of ( L opp )))))
proof
let L being LATTICE;
let x being set;
hereby
assume x is  prime  prime (Ideal of L);
then reconsider I = x as  prime (Ideal of L);
reconsider F = I as (Filter of ( L opp )) by YELLOW_7:26,YELLOW_7:28;
F is  prime
proof
let x being (Element of ( L opp ));
let y being (Element of ( L opp ));
A1: ( x "\/" y ) = ( ( ~ x ) "/\" ( ~ y ) ) by YELLOW_7:22;
(( ~ x ) = x & ( ~ y ) = y) by LATTICE3:def 7;
hence thesis by A1,CKB16:def 1;
end;
hence x is  prime  prime (Filter of ( L opp ));
end;
assume x is  prime  prime (Filter of ( L opp ));
then reconsider I = x as  prime (Filter of ( L opp ));
reconsider F = I as (Ideal of L) by YELLOW_7:26,YELLOW_7:28;
F is  prime
proof
let x being (Element of L);
let y being (Element of L);
A2: ( x "/\" y ) = ( ( x ~ ) "\/" ( y ~ ) ) by YELLOW_7:21;
(( x ~ ) = x & ( y ~ ) = y) by LATTICE3:def 6;
hence thesis by A2,CKB20:def 1;
end;
hence thesis;
end;
