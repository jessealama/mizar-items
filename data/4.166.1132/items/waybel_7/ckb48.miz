begin
theorem
Th37: (for L being LATTICE holds (for p being (Element of L) holds (p is  prime implies ( downarrow p ) is  prime)))
proof
let L being LATTICE;
let p being (Element of L);
assume that
A1: (for x,y being (Element of L) holds (( x "/\" y ) <= p implies (x <= p or y <= p)));
let x being (Element of L),y being (Element of L);
assume ( x "/\" y ) in ( downarrow p );
then ( x "/\" y ) <= p by WAYBEL_0:17;
then (x <= p or y <= p) by A1;
hence thesis by WAYBEL_0:17;
end;
