begin
theorem
Th3: (for L being  finite LATTICE holds (for C being (Chain of L) holds (for x,y being (Element of L) holds ((x in C & y in C) implies (x < y iff ( height x ) < ( height y ))))))
proof
let L being  finite LATTICE;
let C being (Chain of L);
let x being (Element of L);
let y being (Element of L);
assume A1: (x in C & y in C);
(( height x ) < ( height y ) implies x < y)
proof
assume A2: ( height x ) < ( height y );
per cases  by A1,ORDERS_2:11;
suppose x <= y;

hence thesis by A2,ORDERS_2:def 6;
end;
suppose y <= x;

then (x = y or y < x) by ORDERS_2:def 6;
hence thesis by A2,CKB6:1;
end;
end;
hence (x < y iff ( height x ) < ( height y )) by CKB6:1;
end;
