begin
definition
let L being LATTICE;
let x being (Element of L);
let y being (Element of L);
assume that
A1: x <= y;
mode Chain of x,y
 -> non empty (Chain of L)
means :Def2: ((x in it & y in it) & (for z being (Element of L) holds (z in it implies (x <= z & z <= y))));
existence
proof
reconsider A = { x,y } as non empty (Chain of L) by A1,ORDERS_2:9;
A2: (for z being (Element of L) holds (z in A implies (x <= z & z <= y)))
proof
let z being (Element of L);
assume A3: z in A;
per cases  by A3,TARSKI:def 2;
suppose z = x;

hence thesis by A1;
end;
suppose z = y;

hence thesis by A1;
end;
end;
(y in A & x in A) by TARSKI:def 2;
hence thesis by A2;
end;
end;
