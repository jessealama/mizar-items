begin
reserve A for Ordinal;
reserve B for Ordinal;
reserve x for set;
reserve y for set;
reserve z for set;
reserve W for set;
reserve f for Function;
reserve f for Function;
reserve L for T-Sequence;
reserve F for Cardinal-Function;
defpred PQ[ Ordinal ]
 means
((W is  Tarski & $1 in ( On W )) implies (( card ( Rank $1 ) ) in ( card W ) & ( Rank $1 ) in W));
Lm2: PQ[ ( {} ) ] by CKB12:1,CARD_1:27,CLASSES1:29,ORDINAL1:def 9;
Lm3: (PQ[ A ] implies PQ[ ( succ A ) ])
proof
assume A1: PQ[ A ];
A2: A in ( succ A ) by ORDINAL1:6;
let W;
assume that
A3: W is  Tarski
and
A4: ( succ A ) in ( On W );
( card W ) = ( On W ) by A3,CKB12:1;
then A in ( On W ) by A4,A2,ORDINAL1:10;
then ( Rank A ) in W by A1,A3;
then A5: ( bool ( Rank A ) ) in W by A3,CLASSES1:def 2;
( Rank ( succ A ) ) = ( bool ( Rank A ) ) by CLASSES1:30;
hence thesis by A3,A5,CKB4:1;
end;
Lm4: (((A <> ( {} ) & A is  limit_ordinal) & (for B holds (B in A implies PQ[ B ]))) implies PQ[ A ])
proof
deffunc f(Ordinal) = ( Rank $1 );
assume that
A1: A <> ( {} )
and
A2: A is  limit_ordinal
and
A3: (for B holds (B in A implies PQ[ B ]));
let W;
assume that
A4: W is  Tarski
and
A5: A in ( On W );
consider L such that A6: (( dom L ) = A & (for B holds (B in A implies ( L . B ) = f(B)))) from ORDINAL2:sch 2;
deffunc g(set) = ( card ( bool ( L . $1 ) ) );
consider F such that A7: (( dom F ) = A & (for x holds (x in A implies ( F . x ) = g(x)))) from CARD_3:sch 1;
A8: ( product F ) c= ( Funcs (A,W) )
proof
let x;
assume x in ( product F );
then consider f such that A9: x = f and A10: ( dom f ) = ( dom F ) and A11: (for x holds (x in ( dom F ) implies ( f . x ) in ( F . x ))) by CARD_3:def 5;
( rng f ) c= W
proof
A12: A in W by A5,ORDINAL1:def 9;
let z;
assume z in ( rng f );
then consider y such that A13: y in ( dom f ) and A14: z = ( f . y ) by FUNCT_1:def 3;
reconsider y as Ordinal by A7,A10,A13;
A15: ( f . y ) in ( F . y ) by A10,A11,A13;
y c= A by A7,A10,A13,ORDINAL1:def 2;
then y in W by A4,A12,CLASSES1:def 1;
then A16: y in ( On W ) by ORDINAL1:def 9;
( L . y ) = ( Rank y ) by A6,A7,A10,A13;
then ( L . y ) in W by A3,A4,A7,A10,A13,A16;
then ( bool ( L . y ) ) in W by A4,CLASSES1:def 2;
then A17: ( card ( bool ( L . y ) ) ) in W by A4,CKB14:1;
( F . y ) = ( card ( bool ( L . y ) ) ) by A7,A10,A13;
then ( F . y ) c= W by A4,A17,CKB8:1;
hence thesis by A14,A15;
end;
hence thesis by A7,A9,A10,FUNCT_2:def 2;
end;
A18: ( Product F ) = ( card ( product F ) ) by CARD_3:def 8;
A19: A in W by A5,ORDINAL1:def 9;
then A c= W by A4,CKB8:1;
then ( Funcs (A,W) ) c= W by A4,A19,CKB25:1;
then ( product F ) c= W by A8,XBOOLE_1:1;
then A20: ( Product F ) c= ( card W ) by A18,CARD_1:11;
A21: (for x holds (x in ( dom ( Card L ) ) implies ( ( Card L ) . x ) in ( F . x )))
proof
let x;
A22: ( card ( L . x ) ) in ( card ( bool ( L . x ) ) ) by CARD_1:14;
assume x in ( dom ( Card L ) );
then A23: x in ( dom L ) by CARD_3:def 2;
then ( F . x ) = ( card ( bool ( L . x ) ) ) by A6,A7;
hence thesis by A23,A22,CARD_3:def 2;
end;
( dom ( Card L ) ) = ( dom L ) by CARD_3:def 2;
then A24: ( Sum ( Card L ) ) in ( Product F ) by A6,A7,A21,CARD_3:41;
A25: ( Rank A ) c= W
proof
let x;
assume x in ( Rank A );
then consider B such that A26: B in A and A27: x in ( Rank B ) by A1,A2,CLASSES1:31;
B c= A by A26,ORDINAL1:def 2;
then B in W by A4,A19,CLASSES1:def 1;
then B in ( On W ) by ORDINAL1:def 9;
then ( Rank B ) c= W by A3,A4,A26,CKB10:1;
hence thesis by A27;
end;
A28: ( Rank A ) = ( Union L ) by A2,A6,CKB27:1;
hence ( card ( Rank A ) ) in ( card W ) by A24,A20,CARD_3:39,ORDINAL1:12;
( card ( Rank A ) ) in ( Product F ) by A28,A24,CARD_3:39,ORDINAL1:12;
hence thesis by A4,A20,A25,CLASSES1:1;
end;
theorem
Th26: ((W is  Tarski & A in ( On W )) implies (( card ( Rank A ) ) in ( card W ) & ( Rank A ) in W))
proof
PQ[ B ] from ORDINAL2:sch 1(Lm2,Lm3,Lm4);
hence thesis;
end;
