begin
reserve FT for non  empty RelStr;
reserve x for (Element of FT);
definition
let IT being non  empty RelStr;
redefine attr IT is  filled
means
:Def4: (for x being (Element of IT) holds x in ( U_FT x ));
compatibility
proof
thus (IT is  filled implies (for x being (Element of IT) holds x in ( U_FT x )))
proof
assume A1: IT is  filled;
let x being (Element of IT);
x <= x by A1,ORDERS_2:1;
then [ x,x ] in (the InternalRel of IT) by ORDERS_2:def 5;
hence thesis by EQREL_1:18;
end;

assume A2: (for x being (Element of IT) holds x in ( U_FT x ));
let x being set;
assume x in (the carrier of IT);
then reconsider x as (Element of IT);
x in ( U_FT x ) by A2;
hence thesis by EQREL_1:18;
end;
end;
