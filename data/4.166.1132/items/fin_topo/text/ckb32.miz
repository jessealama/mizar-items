environ
vocabularies NUMBERS,FINSEQ_1,ZFMISC_1,SUBSET_1,XXREAL_0,ARYTM_3,PARTFUN1,TARSKI,NAT_1,CARD_1,XBOOLE_0,FINSET_1,FUNCT_1,RELAT_1,ORDINAL1,COMPLEX1,ARYTM_1,STRUCT_0,ORDERS_2,EQREL_1,FUNCOP_1,FUNCT_2,RELAT_2,SETFAM_1,RCOMP_1,FIN_TOPO;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,ORDINAL1,CARD_1,NUMBERS,XCMPLX_0,FINSET_1,SETFAM_1,RELAT_1,EQREL_1,FUNCT_1,PARTFUN1,FUNCT_2,FUNCOP_1,DOMAIN_1,RELSET_1,FINSEQ_1,INT_2,NAT_1,XXREAL_0,STRUCT_0,ORDERS_2,CKB5,CKB6,CKB7,CKB8,CKB10,CKB11,CKB16,CKB18,CKB19,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26;
definitions TARSKI,STRUCT_0,ORDINAL1,XBOOLE_0,SUBSET_1,RELAT_2,ORDERS_2,RELAT_1,FUNCOP_1,SETFAM_1,CKB5,CKB7,CKB8,CKB11,CKB16,CKB18,CKB19,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26;
theorems NAT_1,TARSKI,FUNCOP_1,SUBSET_1,ZFMISC_1,FINSET_1,INT_1,CARD_1,FINSEQ_4,FUNCT_1,FUNCT_2,FINSEQ_1,ABSVALUE,RELSET_1,XBOOLE_0,XBOOLE_1,MEASURE2,XREAL_1,XXREAL_0,ORDINAL1,RELAT_1,EQREL_1,ORDERS_2,PARTFUN1,CKB1,CKB2,CKB5,CKB7,CKB8,CKB11,CKB12,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31;
schemes NAT_1,FINSEQ_2,DOMAIN_1,CKB3;
registrations XBOOLE_0,SUBSET_1,ORDINAL1,RELSET_1,FUNCOP_1,XXREAL_0,XREAL_0,NAT_1,INT_1,STRUCT_0,FINSET_1,FINSEQ_1,CKB4,CKB9,CKB13,CKB14;
constructors SETFAM_1,DOMAIN_1,FUNCOP_1,XXREAL_0,REAL_1,NAT_1,INT_2,EQREL_1,FINSEQ_4,ORDERS_2,SEQ_1,RELSET_1,CKB5,CKB6,CKB7,CKB8,CKB16,CKB18,CKB19,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26;
requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve FT for non  empty RelStr;
reserve x for (Element of FT);
reserve y for (Element of FT);
reserve z for (Element of FT);
reserve A for (Subset of FT);
theorem
(FT is  symmetric iff (for A holds ( A ^b ) = ( A ^f )))
proof
thus (FT is  symmetric implies (for A holds ( A ^b ) = ( A ^f )))
proof
assume A1: FT is  symmetric;
let A being (Subset of FT);
thus ( A ^b ) c= ( A ^f )
proof
let x being set;
assume A2: x in ( A ^b );
then reconsider y = x as (Element of FT);
( U_FT y ) meets A by A2,CKB28:1;
then consider z being set such that A3: z in ( ( U_FT y ) /\ A ) by XBOOLE_0:4;
reconsider z as (Element of FT) by A3;
z in ( U_FT y ) by A3,XBOOLE_0:def 4;
then A4: y in ( U_FT z ) by A1,CKB26:def 1;
z in A by A3,XBOOLE_0:def 4;
hence thesis by A4;
end;

let x being set;
assume A5: x in ( A ^f );
then reconsider y = x as (Element of FT);
consider z such that A6: z in A and A7: y in ( U_FT z ) by A5,CKB31:1;
z in ( U_FT y ) by A1,A7,CKB26:def 1;
then ( U_FT y ) meets A by A6,XBOOLE_0:3;
hence thesis;
end;

assume A8: (for A being (Subset of FT) holds ( A ^b ) = ( A ^f ));
let x being (Element of FT);
let y being (Element of FT);
assume y in ( U_FT x );
then ( U_FT x ) meets { y } by ZFMISC_1:48;
then x in ( { y } ^b );
then x in ( { y } ^f ) by A8;
then (ex z st (z in { y } & x in ( U_FT z ))) by CKB31:1;
hence thesis by TARSKI:def 1;
end;
