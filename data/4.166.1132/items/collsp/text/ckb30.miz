begin
reserve CLSP for  proper CollSp;
reserve a for (Point of CLSP);
reserve b for (Point of CLSP);
reserve P for (LINE of CLSP);
theorem
Lm9: (for x being set holds (x in P implies x is (Point of CLSP)))
proof
let x being set;
consider a,b such that a <> b and A1: P = ( Line (a,b) ) by CKB25:def 1;
assume x in P;
then (ex r being (Point of CLSP) st (r = x & a,b,r is_collinear )) by A1;
hence thesis;
end;
