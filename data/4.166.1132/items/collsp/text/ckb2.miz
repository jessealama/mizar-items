environ
vocabularies TARSKI,ZFMISC_1,XBOOLE_0,SUBSET_1,STRUCT_0,PRE_TOPC,PARSP_2,RELAT_2,INCSP_1,NUMBERS,COLLSP;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,NUMBERS,DOMAIN_1,STRUCT_0,PRE_TOPC,CKB1;
definitions TARSKI,STRUCT_0,CKB1;
theorems TARSKI,MCART_1,ENUMSET1,XBOOLE_0,CKB1;
registrations XBOOLE_0,ORDINAL1,STRUCT_0;
constructors DOMAIN_1,NUMBERS,STRUCT_0,CKB1;
requirements NUMERALS,SUBSET,BOOLE;
begin
reserve X for set;
theorem
Th2: (X = ( {} ) or (ex a being set st ({ a } = X or (ex a,b being set st ((a <> b & a in X) & b in X)))))
proof
now
set p = the (Element of X);
assume (X <> ( {} ) & (not (ex a,b being set st ((a <> b & a in X) & b in X))));
then (for z being set holds (z in X iff z = p));
then X = { p } by TARSKI:def 1;
hence (ex a being set st { a } = X);
end;
hence thesis;
end;
