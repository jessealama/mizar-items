environ
vocabularies TARSKI,ZFMISC_1,XBOOLE_0,SUBSET_1,STRUCT_0,PRE_TOPC,PARSP_2,RELAT_2,INCSP_1,NUMBERS,COLLSP;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,NUMBERS,DOMAIN_1,STRUCT_0,PRE_TOPC,CKB1,CKB3,CKB5,CKB7,CKB8,CKB10,CKB19,CKB22,CKB25;
definitions TARSKI,STRUCT_0,CKB1,CKB5,CKB7,CKB8,CKB19,CKB22,CKB25;
theorems TARSKI,MCART_1,ENUMSET1,XBOOLE_0,CKB1,CKB2,CKB5,CKB6,CKB7,CKB8,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB24,CKB25;
registrations XBOOLE_0,ORDINAL1,STRUCT_0,CKB3,CKB4,CKB9,CKB23;
constructors DOMAIN_1,NUMBERS,STRUCT_0,CKB1,CKB3,CKB5,CKB7,CKB8,CKB19,CKB22,CKB25;
requirements NUMERALS,SUBSET,BOOLE;
begin
reserve CLSP for  proper CollSp;
reserve a for (Point of CLSP);
reserve b for (Point of CLSP);
reserve p for (Point of CLSP);
theorem
(a = b implies ( Line (a,b) ) = (the carrier of CLSP))
proof
assume A1: a = b;
(for x being set holds (x in ( Line (a,b) ) iff x in (the carrier of CLSP)))
proof
let x being set;
thus (x in ( Line (a,b) ) implies x in (the carrier of CLSP))
proof
assume x in ( Line (a,b) );
then (ex p st (x = p & a,b,p is_collinear ));
then reconsider x as (Point of CLSP);
x is (Element of CLSP);
hence thesis;
end;

thus (x in (the carrier of CLSP) implies x in ( Line (a,b) ))
proof
assume x in (the carrier of CLSP);
then reconsider x as (Point of CLSP);
a,b,x is_collinear  by A1,CKB11:1;
hence thesis;
end;

end;
hence thesis by TARSKI:1;
end;
