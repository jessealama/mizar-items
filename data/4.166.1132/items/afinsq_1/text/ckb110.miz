begin
reserve x for set;
reserve y for set;
reserve p for XFinSequence;
reserve q for XFinSequence;
reserve i for Nat;
reserve m for Nat;
reserve D for non  empty set;
definition
let D being non  empty set;
let p being (XFinSequence of D);
assume that
A1: ( p . ( 0 ) ) is Nat
and
A2: ( p . ( 0 ) ) in ( len p );
func XFS2FS* p -> (FinSequence of D) means 
:Def12: (for m being Nat holds (m = ( p . ( 0 ) ) implies (( len it ) = m & (for i holds ((1 <= i & i <= m) implies ( it . i ) = ( p . i ))))));
existence
proof
reconsider m0 = ( p . ( 0 ) ) as (Element of ( NAT )) by A1,ORDINAL1:def 12;
deffunc F(set) = ( p . $1 );
(ex q being FinSequence st (( len q ) = m0 & (for k being Nat holds (k in ( dom q ) implies ( q . k ) = F(k))))) from FINSEQ_1:sch 2;
then consider q being FinSequence such that A3: ( len q ) = m0 and A4: (for k being Nat holds (k in ( dom q ) implies ( q . k ) = F(k)));
( rng q ) c= D
proof
A5: m0 < ( len p ) by A2,NAT_1:44;
let y being set;
assume y in ( rng q );
then consider x being set such that A6: x in ( dom q ) and A7: y = ( q . x ) by FUNCT_1:def 3;
reconsider k0 = x as (Element of ( NAT )) by A6;
k0 in ( Seg m0 ) by A3,A6,FINSEQ_1:def 3;
then k0 <= m0 by FINSEQ_1:1;
then k0 < ( len p ) by A5,XXREAL_0:2;
then A8: k0 in ( dom p ) by NAT_1:44;
y = ( p . k0 ) by A4,A6,A7;
then (( rng p ) c= D & y in ( rng p )) by A8,FUNCT_1:def 3,RELAT_1:def 19;
hence thesis;
end;
then reconsider q0 = q as (FinSequence of D) by FINSEQ_1:def 4;
A9: ( dom q ) = ( Seg m0 ) by A3,FINSEQ_1:def 3;
(for m being Nat holds (m = ( p . ( 0 ) ) implies (( len q0 ) = m & (for i holds ((1 <= i & i <= m) implies ( q0 . i ) = ( p . i ))))))
proof
let m being Nat;
A10: (for i holds ((1 <= i & i <= m0) implies ( q0 . i ) = ( p . i )))
proof
let i;
assume (1 <= i & i <= m0);
then i in ( Seg m0 ) by FINSEQ_1:1;
hence thesis by A4,A9;
end;
assume m = ( p . ( 0 ) );
hence thesis by A3,A10;
end;
hence thesis;
end;
uniqueness
proof
reconsider m2 = ( p . ( 0 ) ) as Nat by A1;
let g1 being (FinSequence of D);
let g2 being (FinSequence of D);
assume that
A11: (for m holds (m = ( p . ( 0 ) ) implies (( len g1 ) = m & (for i holds ((1 <= i & i <= m) implies ( g1 . i ) = ( p . i ))))))
and
A12: (for m holds (m = ( p . ( 0 ) ) implies (( len g2 ) = m & (for i holds ((1 <= i & i <= m) implies ( g2 . i ) = ( p . i ))))));
A13: ( len g1 ) = m2 by A11;
A14: (for i being Nat holds ((1 <= i & i <= ( len g1 )) implies ( g1 . i ) = ( g2 . i )))
proof
let i being Nat;
assume A15: (1 <= i & i <= ( len g1 ));
then ( g1 . i ) = ( p . i ) by A11,A13;
hence thesis by A12,A13,A15;
end;
( len g2 ) = m2 by A12;
hence thesis by A11,A14,FINSEQ_1:14;
end;
