constructors WELLORD2,FUNCT_4,XXREAL_0,ORDINAL4,FUNCT_7,ORDINAL3,VALUED_1,ENUMSET1,NAT_D,XXREAL_2,BINOP_1,FINSOP_1,RELSET_1,CKB10,CKB11,CKB31,CKB33;
requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve x for set;
reserve y for set;
reserve z for set;
reserve f for Function;
definition
let x;
redefine func <%x %> -> Function means 
:Def5: (( dom it ) = 1 & ( it . ( 0 ) ) = x);
coherence;
compatibility
proof
let f being Function;
thus (f = <% x %> implies (( dom f ) = 1 & ( f . ( 0 ) ) = x)) by CARD_1:49,FUNCOP_1:13,FUNCOP_1:72;
assume that
A1: ( dom f ) = 1
and
A2: ( f . ( 0 ) ) = x;
reconsider g = { [ ( 0 ),( f . ( 0 ) ) ] } as Function;
([ y,z ] in f iff [ y,z ] in g)
proof
hereby
assume A3: [ y,z ] in f;
then y in { ( 0 ) } by A1,CARD_1:49,RELAT_1:def 4;
then A4: y = ( 0 ) by TARSKI:def 1;
A5: ( rng f ) = { ( f . ( 0 ) ) } by A1,CARD_1:49,FUNCT_1:4;
z in ( rng f ) by A3,RELAT_1:def 5;
then z = ( f . ( 0 ) ) by A5,TARSKI:def 1;
hence [ y,z ] in g by A4,TARSKI:def 1;
end;
assume [ y,z ] in g;
then A6: (y = ( 0 ) & z = ( f . ( 0 ) )) by CKB52:1;
( 0 ) in ( dom f ) by A1,CARD_1:49,TARSKI:def 1;
hence thesis by A6,FUNCT_1:def 2;
end;
then f = { [ ( 0 ),( f . ( 0 ) ) ] } by RELAT_1:def 2;
hence thesis by A2,FUNCT_4:82;
end;
end;
