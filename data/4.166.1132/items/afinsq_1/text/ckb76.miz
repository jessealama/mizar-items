reserve x for set;
reserve p for XFinSequence;
reserve q for XFinSequence;
reserve r for XFinSequence;
reserve s for XFinSequence;
reserve t for XFinSequence;
theorem
(for p,q,r,s being XFinSequence holds ((( p ^ q ) = ( r ^ s ) & ( len p ) <= ( len r )) implies (ex t being XFinSequence st ( p ^ t ) = r)))
proof
defpred P[ XFinSequence ]
 means
(for p holds (for q holds (for s holds ((( p ^ q ) = ( $1 ^ s ) & ( len p ) <= ( len $1 )) implies (ex t being XFinSequence st ( p ^ t ) = $1)))));
A1: (for r holds (for x holds (P[ r ] implies P[ ( r ^ <% x %> ) ])))
proof
let r;
let x;
assume A2: (for p holds (for q holds (for s holds ((( p ^ q ) = ( r ^ s ) & ( len p ) <= ( len r )) implies (ex t st ( p ^ t ) = r)))));
let p;
let q;
let s;
assume that
A3: ( p ^ q ) = ( ( r ^ <% x %> ) ^ s )
and
A4: ( len p ) <= ( len ( r ^ <% x %> ) );
A5:now
assume ( len p ) <> ( len ( r ^ <% x %> ) );
then ( len p ) <> ( ( len r ) + ( len <% x %> ) ) by CKB35:def 1;
then A6: ( len p ) <> ( ( len r ) + 1 ) by CKB64:1;
( len p ) <= ( ( len r ) + ( len <% x %> ) ) by A4,CKB35:def 1;
then A7: ( len p ) <= ( ( len r ) + 1 ) by CKB64:1;
( p ^ q ) = ( r ^ ( <% x %> ^ s ) ) by A3,CKB47:1;
then consider t being XFinSequence such that A8: ( p ^ t ) = r by A2,A6,A7,NAT_1:8;
( p ^ ( t ^ <% x %> ) ) = ( r ^ <% x %> ) by A8,CKB47:1;
hence thesis;
end;
now
assume A9: ( len p ) = ( len ( r ^ <% x %> ) );
A10: (for k holds (k in ( dom p ) implies ( p . k ) = ( ( r ^ <% x %> ) . k )))
proof
let k;
assume A11: k in ( dom p );
hence ( p . k ) = ( ( ( r ^ <% x %> ) ^ s ) . k ) by A3,CKB35:def 1
.= ( ( r ^ <% x %> ) . k ) by A9,A11,CKB35:def 1;
end;
( p ^ ( {} ) ) = p by CKB49:1
.= ( r ^ <% x %> ) by A9,A10,CKB16:1;
hence thesis;
end;
hence thesis by A5;
end;
A12: P[ ( {} ) ]
proof
let p;
let q;
let s;
assume that
( p ^ q ) = ( ( {} ) ^ s )
and
A13: ( len p ) <= ( len ( {} ) );
take ( {} );
thus ( p ^ ( {} ) ) = p by CKB49:1
.= ( {} ) by A13;
end;
(for r holds P[ r ]) from CKB75:sch 1(A12,A1);
hence thesis;
end;
