environ
vocabularies XBOOLE_0,TARSKI,SUBSET_1,ZFMISC_1,SETFAM_1;
notations TARSKI,XBOOLE_0,ENUMSET1,ZFMISC_1,SUBSET_1,CKB1,CKB13,CKB14;
definitions TARSKI,XBOOLE_0,SUBSET_1,CKB1,CKB13,CKB14;
theorems TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,ENUMSET1,XBOOLE_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21;
schemes XBOOLE_0,SUBSET_1;
registrations XBOOLE_0,SUBSET_1;
constructors TARSKI,ENUMSET1,SUBSET_1,CKB1,CKB13,CKB14;
requirements SUBSET,BOOLE;
begin
reserve X for set;
reserve Y for set;
reserve Z for set;
reserve Z1 for set;
reserve Z2 for set;
reserve SFX for set;
theorem
(SFX is_finer_than { X,Y } implies (for Z holds (Z in SFX implies (Z c= X or Z c= Y))))
proof
assume A1: (for Z1 holds (Z1 in SFX implies (ex Z2 st (Z2 in { X,Y } & Z1 c= Z2))));
let Z;
assume Z in SFX;
then consider Z2 such that A2: Z2 in { X,Y } and A3: Z c= Z2 by A1;
{ X,Y } = ( { X } \/ { Y } ) by ENUMSET1:1;
then (Z2 in { X } or Z2 in { Y }) by A2,XBOOLE_0:def 3;
hence thesis by A3,TARSKI:def 1;
end;
