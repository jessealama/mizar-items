environ
vocabularies XBOOLE_0,TARSKI,SUBSET_1,ZFMISC_1,SETFAM_1;
notations TARSKI,XBOOLE_0,ENUMSET1,ZFMISC_1,SUBSET_1,CKB1;
definitions TARSKI,XBOOLE_0,SUBSET_1,CKB1;
theorems TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,ENUMSET1,XBOOLE_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10;
schemes XBOOLE_0,SUBSET_1;
registrations XBOOLE_0,SUBSET_1;
constructors TARSKI,ENUMSET1,SUBSET_1,CKB1;
requirements SUBSET,BOOLE;
begin
reserve Z for set;
reserve x for set;
reserve y for set;
theorem
( meet { x } ) = x
proof
A1: x c= ( meet { x } )
proof
let y;
assume y in x;
then (for Z holds (Z in { x } implies y in Z)) by TARSKI:def 1;
hence thesis by CKB1:def 1;
end;
x in { x } by TARSKI:def 1;
then ( meet { x } ) c= x by CKB4:1;
hence thesis by A1,XBOOLE_0:def 10;
end;
