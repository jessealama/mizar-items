begin
theorem
(for X being set holds (for H,J being (Subset-Family of X) holds (H c= J implies ( Intersect J ) c= ( Intersect H ))))
proof
let X being set;
let H being (Subset-Family of X),J being (Subset-Family of X);
assume that
A1: H c= J;
let x being set;
assume A2: x in ( Intersect J );
then (for Y being set holds (Y in H implies x in Y)) by A1,CKB68:1;
hence thesis by A2,CKB68:1;
end;
