begin
theorem
Th58: (for X,x being set holds (for R being (Subset-Family of X) holds (x in X implies (x in ( Intersect R ) iff (for Y being set holds (Y in R implies x in Y))))))
proof
let X being set,x being set;
let R being (Subset-Family of X);
assume A1: x in X;
hereby
assume A2: x in ( Intersect R );
let Y being set;
assume A3: Y in R;
then ( Intersect R ) = ( meet R ) by CKB67:def 1;
hence x in Y by A2,A3,CKB1:def 1;
end;
assume A4: (for Y being set holds (Y in R implies x in Y));
per cases ;
suppose A5: R <> ( {} );

then x in ( meet R ) by A4,CKB1:def 1;
hence thesis by A5,CKB67:def 1;
end;
suppose R = ( {} );

hence thesis by A1,CKB67:def 1;
end;
end;
