environ
vocabularies XBOOLE_0,TARSKI,SUBSET_1,ZFMISC_1,SETFAM_1;
notations TARSKI,XBOOLE_0,ENUMSET1,ZFMISC_1,SUBSET_1,CKB1;
definitions TARSKI,XBOOLE_0,SUBSET_1,CKB1;
theorems TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,ENUMSET1,XBOOLE_1,CKB1,CKB2,CKB3,CKB4,CKB5;
schemes XBOOLE_0,SUBSET_1;
registrations XBOOLE_0,SUBSET_1;
constructors TARSKI,ENUMSET1,SUBSET_1,CKB1;
requirements SUBSET,BOOLE;
begin
reserve X for set;
reserve Y for set;
reserve Z for set;
reserve Z1 for set;
reserve x for set;
theorem
((X <> ( {} ) & (for Z1 holds (Z1 in X implies Z c= Z1))) implies Z c= ( meet X ))
proof
assume that
A1: X <> ( {} )
and
A2: (for Z1 holds (Z1 in X implies Z c= Z1));
thus Z c= ( meet X )
proof
let x;
assume that
A3: x in Z;
(for Y holds (Y in X implies x in Y))
proof
let Y;
assume Y in X;
then Z c= Y by A2;
hence thesis by A3;
end;
hence thesis by A1,CKB1:def 1;
end;

end;
