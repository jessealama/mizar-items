environ
vocabularies XBOOLE_0,TARSKI,SUBSET_1,ZFMISC_1,SETFAM_1;
notations TARSKI,XBOOLE_0,ENUMSET1,ZFMISC_1,SUBSET_1,CKB1;
definitions TARSKI,XBOOLE_0,SUBSET_1,CKB1;
theorems TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,ENUMSET1,XBOOLE_1,CKB1,CKB2;
schemes XBOOLE_0,SUBSET_1;
registrations XBOOLE_0,SUBSET_1;
constructors TARSKI,ENUMSET1,SUBSET_1,CKB1;
requirements SUBSET,BOOLE;
begin
reserve X for set;
reserve x for set;
theorem
Th3: ( meet X ) c= ( union X )
proof
A1:now
assume A2: X <> ( {} );
now
set y = the (Element of X);
let x;
assume x in ( meet X );
then x in y by A2,CKB1:def 1;
hence x in ( union X ) by A2,TARSKI:def 4;
end;
hence thesis by TARSKI:def 3;
end;
now
assume X = ( {} );
then ( meet X ) = ( {} ) by CKB1:def 1;
hence thesis by XBOOLE_1:2;
end;
hence thesis by A1;
end;
