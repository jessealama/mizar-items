environ
vocabularies XBOOLE_0,TARSKI,SUBSET_1,ZFMISC_1,SETFAM_1;
notations TARSKI,XBOOLE_0,ENUMSET1,ZFMISC_1,SUBSET_1,CKB1;
definitions TARSKI,XBOOLE_0,SUBSET_1,CKB1;
theorems TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,ENUMSET1,XBOOLE_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6;
schemes XBOOLE_0,SUBSET_1;
registrations XBOOLE_0,SUBSET_1;
constructors TARSKI,ENUMSET1,SUBSET_1,CKB1;
requirements SUBSET,BOOLE;
begin
reserve X for set;
reserve Y for set;
reserve Z for set;
reserve x for set;
theorem
Th7: ((X <> ( {} ) & X c= Y) implies ( meet Y ) c= ( meet X ))
proof
assume that
A1: X <> ( {} )
and
A2: X c= Y;
let x;
assume x in ( meet Y );
then (for Z holds (Z in X implies x in Z)) by A2,CKB1:def 1;
hence thesis by A1,CKB1:def 1;
end;
