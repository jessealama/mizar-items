environ
vocabularies XBOOLE_0,TARSKI,SUBSET_1,ZFMISC_1,SETFAM_1;
notations TARSKI,XBOOLE_0,ENUMSET1,ZFMISC_1,SUBSET_1,CKB1,CKB13,CKB14;
definitions TARSKI,XBOOLE_0,SUBSET_1,CKB1,CKB13,CKB14;
theorems TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,ENUMSET1,XBOOLE_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20;
schemes XBOOLE_0,SUBSET_1;
registrations XBOOLE_0,SUBSET_1;
constructors TARSKI,ENUMSET1,SUBSET_1,CKB1,CKB13,CKB14;
requirements SUBSET,BOOLE;
begin
reserve X for set;
reserve Y for set;
reserve Z for set;
reserve SFX for set;
theorem
(SFX is_finer_than { Y } implies (for X holds (X in SFX implies X c= Y)))
proof
assume A1: (for X holds (X in SFX implies (ex Z st (Z in { Y } & X c= Z))));
let X;
assume X in SFX;
then (ex Z st (Z in { Y } & X c= Z)) by A1;
hence thesis by TARSKI:def 1;
end;
