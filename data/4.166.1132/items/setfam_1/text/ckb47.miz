begin
theorem
(for X being set holds (for F,G being (Subset-Family of X) holds (( COMPLEMENT F ) c= G iff F c= ( COMPLEMENT G ))))
proof
let X being set;
let F being (Subset-Family of X),G being (Subset-Family of X);
hereby
assume ( COMPLEMENT F ) c= G;
then ( COMPLEMENT F ) c= ( COMPLEMENT ( COMPLEMENT G ) );
hence F c= ( COMPLEMENT G ) by CKB46:1;
end;
assume F c= ( COMPLEMENT G );
then ( COMPLEMENT ( COMPLEMENT F ) ) c= ( COMPLEMENT G );
hence thesis by CKB46:1;
end;
