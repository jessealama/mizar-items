begin
theorem
Th67: (for G being _Graph holds (for S being non empty (Subset of ( the_Vertices_of G )) holds (for H being (inducedSubgraph of G,S) holds (for u being (Vertex of G) holds ((u in S & ( G .AdjacentSet { u } ) c= S) implies (for v being (Vertex of H) holds (u = v implies (u is  simplicial iff v is  simplicial))))))))
proof
let G being _Graph;
let S being non empty (Subset of ( the_Vertices_of G ));
let G2 being (inducedSubgraph of G,S);
let u being (Vertex of G);
assume that
A1: u in S
and
A2: ( G .AdjacentSet { u } ) c= S;
let v being (Vertex of G2);
assume that
A3: u = v;
A4: (( G .AdjacentSet { u } ) = ( {} ) iff ( G2 .AdjacentSet { v } ) = ( {} )) by A1,A2,A3,CKB65:1;
per cases ;
suppose ( G .AdjacentSet { u } ) = ( {} );

hence thesis by A4,CKB76:def 1;
end;
suppose A5: ( G .AdjacentSet { u } ) <> ( {} );

hereby
set Ga = the (AdjGraph of G,{ u });
assume u is  simplicial;
then A6: Ga is  complete by A5,CKB76:def 1;
thus v is  simplicial
proof
assume ( G2 .AdjacentSet { v } ) <> ( {} );
let Ha being (AdjGraph of G2,{ v });
thus thesis by A1,A2,A3,A5,A6,CKB68:1,CKB74:1;
end;

end;
set Ha = the (AdjGraph of G2,{ v });
assume A7: v is  simplicial;
( G2 .AdjacentSet { v } ) <> ( {} ) by A1,A2,A3,A5,CKB65:1;
then A8: Ha is  complete by A7,CKB76:def 1;
thus u is  simplicial
proof
assume ( G .AdjacentSet { u } ) <> ( {} );
let Ga being (AdjGraph of G,{ u });
Ga == Ha by A1,A2,A3,A5,CKB68:1;
hence thesis by A8,CKB74:1;
end;

end;
end;
