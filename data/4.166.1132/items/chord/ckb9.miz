theorem
Th8: (for n being  odd  natural number holds (n <= 6 implies ((n = 1 or n = 3) or n = 5)))
proof
let n being  odd Nat;
assume that
A1: n <= 6;
n <> ( 2 * 3 );
then n < ( 5 + 1 ) by A1,XXREAL_0:1;
then A2: n <= 5 by NAT_1:13;
per cases  by A2,XXREAL_0:1;
suppose n = 5;

hence thesis;
end;
suppose n < ( 4 + 1 );

then n <= 4 by NAT_1:13;
hence thesis by CKB8:1;
end;
end;
