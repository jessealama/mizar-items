begin
theorem
Th58: (for G being _Graph holds (for S being non empty (Subset of ( the_Vertices_of G )) holds (for H being (inducedSubgraph of G,S) holds (for u being (Vertex of G) holds ((u in S & ( G .AdjacentSet { u } ) c= S) implies (for v being (Vertex of H) holds (u = v implies ( G .AdjacentSet { u } ) = ( H .AdjacentSet { v } ))))))))
proof
let G being _Graph;
let S being non empty (Subset of ( the_Vertices_of G ));
let G2 being (inducedSubgraph of G,S);
let u being (Vertex of G);
assume that
A1: u in S
and
A2: ( G .AdjacentSet { u } ) c= S;
let v being (Vertex of G2);
assume that
A3: u = v;
now
let x being set;
hereby
assume A4: x in ( G .AdjacentSet { u } );
then reconsider y = x as (Vertex of G);
reconsider w = x as (Vertex of G2) by A2,A4,GLIB_000:def 37;
y,u are_adjacent  by A4,CKB59:1;
then consider e being set such that A5: e Joins y,u,G by CKB49:def 1;
e Joins y,u,G2 by A1,A2,A4,A5,CKB22:1;
then A6: w,v are_adjacent  by A3,CKB49:def 1;
w <> v by A3,A4,CKB59:1;
hence x in ( G2 .AdjacentSet { v } ) by A6,CKB59:1;
end;
assume A7: x in ( G2 .AdjacentSet { v } );
then reconsider y = x as (Vertex of G2);
x in ( the_Vertices_of G2 ) by A7;
then reconsider w = x as (Vertex of G);
y,v are_adjacent  by A7,CKB59:1;
then consider e being set such that A8: e Joins y,v,G2 by CKB49:def 1;
e Joins y,v,G by A8,GLIB_000:72;
then A9: w,u are_adjacent  by A3,CKB49:def 1;
w <> u by A3,A7,CKB59:1;
hence x in ( G .AdjacentSet { u } ) by A9,CKB59:1;
end;
hence thesis by TARSKI:1;
end;
