theorem
Th34: (for G being _Graph holds (for W being (Walk of G) holds (for S being (Subwalk of W) holds ((( S .first() ) = ( W .first() ) & ( S .edgeSeq() ) = ( W .edgeSeq() )) implies S = W))))
proof
let G being _Graph;
let W being (Walk of G);
let S being (Subwalk of W);
assume that
A1: ( S .first() ) = ( W .first() )
and
A2: ( S .edgeSeq() ) = ( W .edgeSeq() );
defpred P[ Nat ]
 means
($1 in ( dom S ) implies ( S . $1 ) = ( W . $1 ));
( len S ) = ( ( 2 * ( len ( W .edgeSeq() ) ) ) + 1 ) by A2,GLIB_001:def 15;
then A3: ( len S ) = ( len W ) by GLIB_001:def 15;
A4:now
let k being Nat;
assume that
A5: (for n being Nat holds (n < k implies P[ n ]));
A6: k in ( NAT ) by ORDINAL1:def 12;
per cases ;
suppose A7: k in ( dom S );

then A8: 1 <= k by FINSEQ_3:25;
A9: k <= ( len S ) by A7,FINSEQ_3:25;
per cases ;
suppose A10: k is  even;

then ( S . k ) = ( ( S .edgeSeq() ) . ( k div 2 ) ) by A6,A8,A9,GLIB_001:77;
hence P[ k ] by A2,A3,A8,A9,A10,GLIB_001:77;
end;
suppose k is  odd;

then reconsider kk = k as  odd Nat;
per cases  by A8,XXREAL_0:1;
suppose k = 1;

hence P[ k ] by A1;
end;
suppose k > ( ( 2 * ( 0 ) ) + 1 );

then A11: ( 1 + 2 ) <= kk by CKB5:1;
then A12: ( 3 + ( - 2 ) ) <= ( k + ( - 2 ) ) by XREAL_1:7;
A13: ( 0 ) <= 2;
( 3 + ( - 1 ) ) <= ( k + ( - 1 ) ) by A11,XREAL_1:7;
then reconsider k1 = ( k - 1 ) as (Element of ( NAT )) by A13,INT_1:3;
k1 < k by XREAL_1:44;
then A14: k1 <= ( len S ) by A9,XXREAL_0:2;
( 3 + ( - 1 ) ) <= ( k + ( - 1 ) ) by A11,XREAL_1:7;
then 1 <= k1 by XXREAL_0:2;
then k1 in ( dom S ) by A14,FINSEQ_3:25;
then A15: ( S . k1 ) = ( W . k1 ) by A5,XREAL_1:44;
( 3 + ( - 2 ) ) <= ( k + ( - 2 ) ) by A11,XREAL_1:7;
then reconsider k2 = ( kk - ( 2 * 1 ) ) as  odd (Element of ( NAT )) by INT_1:3;
A16: k2 < k by XREAL_1:44;
then k2 < ( len S ) by A9,XXREAL_0:2;
then A17: ( S . ( k2 + 1 ) ) Joins ( S . k2 ),( S . ( k2 + 2 ) ),G by GLIB_001:def 3;
k2 <= ( len S ) by A9,A16,XXREAL_0:2;
then k2 in ( dom S ) by A12,FINSEQ_3:25;
then A18: ( S . k2 ) = ( W . k2 ) by A5,XREAL_1:44;
k2 < ( len W ) by A3,A9,A16,XXREAL_0:2;
then ( W . ( k2 + 1 ) ) Joins ( W . k2 ),( W . ( k2 + 2 ) ),G by GLIB_001:def 3;
then ((( S . k2 ) = ( S . k2 ) & ( S . k ) = ( W . k )) or (( S . k2 ) = ( W . k ) & ( S . k ) = ( S . k2 ))) by A15,A18,A17,GLIB_000:15;
hence P[ k ];
end;
end;
end;
suppose (not k in ( dom S ));

hence P[ k ];
end;
end;
A19: (for n being Nat holds P[ n ]) from NAT_1:sch 4(A4);
now
let k being Nat;
assume that
A20: 1 <= k
and
A21: k <= ( len S );
k in ( dom S ) by A20,A21,FINSEQ_3:25;
hence ( S . k ) = ( W . k ) by A19;
end;
hence thesis by A3,FINSEQ_1:14;
end;
