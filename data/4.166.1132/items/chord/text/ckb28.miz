theorem
Th25: (for G being _Graph holds (for P being (Path of G) holds (for m,n being  odd  natural number holds (((m <= ( len P ) & n <= ( len P )) & ( P . m ) = ( P . n )) implies ((m = n or (m = 1 & n = ( len P ))) or (m = ( len P ) & n = 1))))))
proof
let G being _Graph;
let P being (Path of G);
let m being  odd Nat,n being  odd Nat;
assume that
A1: m <= ( len P )
and
A2: n <= ( len P )
and
A3: ( P . m ) = ( P . n );
A4: m in ( NAT ) by ORDINAL1:def 12;
A5: n in ( NAT ) by ORDINAL1:def 12;
((m = n or m < n) or n < m) by XXREAL_0:1;
hence thesis by A1,A2,A3,A4,A5,GLIB_001:def 28;
end;
