begin
theorem
Th76: (for G being _Graph holds (for a,b being (Vertex of G) holds ((a <> b & (not a,b are_adjacent )) implies (for S being (VertexSeparator of a,b) holds (for G2 being (removeVertices of G,S) holds (for a2,b2 being (Vertex of G2) holds ((a2 = a & b2 = b) implies ( ( G2 .reachableFrom a2 ) /\ ( G2 .reachableFrom b2 ) ) = ( {} ))))))))
proof
let G being _Graph;
let a being (Vertex of G),b being (Vertex of G);
assume that
A1: a <> b
and
A2: (not a,b are_adjacent );
let S being (VertexSeparator of a,b);
let G2 being (removeVertices of G,S);
let a2 being (Vertex of G2),b2 being (Vertex of G2);
assume that
A3: a2 = a
and
A4: b2 = b;
set A = ( G2 .reachableFrom a2 );
set B = ( G2 .reachableFrom b2 );
now
let x being set;
assume that
A5: x in ( A /\ B );
x in A by A5,XBOOLE_0:def 4;
then consider W1 being (Walk of G2) such that A6: W1 is_Walk_from a2,x by GLIB_002:def 5;
x in B by A5,XBOOLE_0:def 4;
then consider rW2 being (Walk of G2) such that A7: rW2 is_Walk_from b2,x by GLIB_002:def 5;
set W2 = ( rW2 .reverse() );
set W = ( W1 .append W2 );
W2 is_Walk_from x,b2 by A7,GLIB_001:23;
then W is_Walk_from a2,b2 by A6,GLIB_001:31;
hence contradiction by A1,A2,A3,A4,CKB83:def 1;
end;
hence thesis by XBOOLE_0:def 1;
end;
