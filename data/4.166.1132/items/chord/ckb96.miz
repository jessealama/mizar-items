begin
theorem
(for G being _Graph holds (for a,b being (Vertex of G) holds ((a <> b & (not a,b are_adjacent )) implies (for S being (VertexSeparator of a,b) holds (S is  minimal implies (for x being (Vertex of G) holds (x in S implies (ex W being (Walk of G) st (W is_Walk_from a,b & x in ( W .vertices() ))))))))))
proof
let G being _Graph;
let a being (Vertex of G),b being (Vertex of G);
assume that
A1: a <> b
and
A2: (not a,b are_adjacent );
let S being (VertexSeparator of a,b);
assume that
A3: S is  minimal;
let x being (Vertex of G);
assume that
A4: x in S;
set T = ( S \ { x } );
A5: T c= S by XBOOLE_1:36;
then A6: (not b in T) by A1,A2,CKB83:def 1;
assume A7: (not (ex W being (Walk of G) st (W is_Walk_from a,b & x in ( W .vertices() ))));
A8:now
let W being (Walk of G);
assume that
A9: W is_Walk_from a,b;
consider y being (Vertex of G) such that A10: y in S and A11: y in ( W .vertices() ) by A1,A2,A9,CKB85:1;
take y;
y <> x by A7,A9,A11;
then (not y in { x }) by TARSKI:def 1;
hence y in T by A10,XBOOLE_0:def 5;
thus y in ( W .vertices() ) by A11;
end;
x in { x } by TARSKI:def 1;
then A12: T <> S by A4,XBOOLE_0:def 5;
(not a in T) by A1,A2,A5,CKB83:def 1;
then T is (VertexSeparator of a,b) by A1,A2,A6,A8,CKB85:1;
hence contradiction by A3,A12,A5,CKB92:def 1;
end;
