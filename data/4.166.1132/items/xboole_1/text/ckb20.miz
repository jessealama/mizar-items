environ
vocabularies TARSKI,XBOOLE_0;
notations TARSKI,XBOOLE_0;
definitions TARSKI,XBOOLE_0;
theorems TARSKI,XBOOLE_0,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19;
registrations XBOOLE_0;
constructors TARSKI,XBOOLE_0;
requirements BOOLE;
begin
reserve X for set;
reserve Y for set;
reserve Z for set;
reserve V for set;
theorem
(((X c= Y & X c= Z) & (for V holds ((V c= Y & V c= Z) implies V c= X))) implies X = ( Y /\ Z ))
proof
assume that
A1: (X c= Y & X c= Z)
and
A2: ((V c= Y & V c= Z) implies V c= X);
thus X c= ( Y /\ Z ) by A1,CKB19:1;
((( Y /\ Z ) c= Y & ( Y /\ Z ) c= Z) implies ( Y /\ Z ) c= X) by A2;
hence thesis by CKB17:1;
end;
