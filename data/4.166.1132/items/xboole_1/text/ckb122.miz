theorem
(for P,G,C being set holds (C c= G implies ( P \ C ) = ( ( P \ G ) \/ ( P /\ ( G \ C ) ) )))
proof
let P being set,G being set,C being set;
assume C c= G;
then A1: ( P \ G ) c= ( P \ C ) by CKB35:1;
thus ( P \ C ) c= ( ( P \ G ) \/ ( P /\ ( G \ C ) ) )
proof
let x being set;
assume x in ( P \ C );
then ((x in P & (not x in G)) or ((x in P & x in G) & (not x in C))) by XBOOLE_0:def 5;
then (x in ( P \ G ) or (x in P & x in ( G \ C ))) by XBOOLE_0:def 5;
then (x in ( P \ G ) or x in ( P /\ ( G \ C ) )) by XBOOLE_0:def 4;
hence thesis by XBOOLE_0:def 3;
end;

(( P /\ ( G \ C ) ) = ( ( P /\ G ) \ C ) & ( ( P /\ G ) \ C ) c= ( P \ C )) by CKB17:1,CKB34:1,CKB51:1;
hence thesis by A1,CKB8:1;
end;
