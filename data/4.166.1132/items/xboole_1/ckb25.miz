environ
vocabularies TARSKI,XBOOLE_0;
notations TARSKI,XBOOLE_0;
definitions TARSKI,XBOOLE_0;
theorems TARSKI,XBOOLE_0,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24;
registrations XBOOLE_0;
constructors TARSKI,XBOOLE_0;
requirements BOOLE;
begin
reserve X for set;
reserve Y for set;
reserve Z for set;
theorem
( ( ( X /\ Y ) \/ ( Y /\ Z ) ) \/ ( Z /\ X ) ) = ( ( ( X \/ Y ) /\ ( Y \/ Z ) ) /\ ( Z \/ X ) )
proof
thus ( ( ( X /\ Y ) \/ ( Y /\ Z ) ) \/ ( Z /\ X ) ) = ( ( ( ( X /\ Y ) \/ ( Y /\ Z ) ) \/ Z ) /\ ( ( ( X /\ Y ) \/ ( Y /\ Z ) ) \/ X ) ) by CKB24:1
.= ( ( ( X /\ Y ) \/ ( ( Y /\ Z ) \/ Z ) ) /\ ( ( ( X /\ Y ) \/ ( Y /\ Z ) ) \/ X ) ) by CKB4:1
.= ( ( ( X /\ Y ) \/ Z ) /\ ( ( ( X /\ Y ) \/ ( Y /\ Z ) ) \/ X ) ) by CKB22:1
.= ( ( ( X /\ Y ) \/ Z ) /\ ( ( ( X /\ Y ) \/ X ) \/ ( Y /\ Z ) ) ) by CKB4:1
.= ( ( ( X /\ Y ) \/ Z ) /\ ( X \/ ( Y /\ Z ) ) ) by CKB22:1
.= ( ( ( X \/ Z ) /\ ( Y \/ Z ) ) /\ ( X \/ ( Y /\ Z ) ) ) by CKB24:1
.= ( ( ( X \/ Z ) /\ ( Y \/ Z ) ) /\ ( ( X \/ Y ) /\ ( X \/ Z ) ) ) by CKB24:1
.= ( ( X \/ Y ) /\ ( ( ( Y \/ Z ) /\ ( X \/ Z ) ) /\ ( X \/ Z ) ) ) by CKB16:1
.= ( ( X \/ Y ) /\ ( ( Y \/ Z ) /\ ( ( X \/ Z ) /\ ( X \/ Z ) ) ) ) by CKB16:1
.= ( ( ( X \/ Y ) /\ ( Y \/ Z ) ) /\ ( Z \/ X ) ) by CKB16:1;
end;
