begin
reserve n for Nat;
reserve m for Nat;
reserve f for n -element  real-valued FinSequence;
reserve M1 for (Matrix of n,m,( F_Real ));
reserve M2 for (Matrix of n,m,( F_Real ));
theorem
( ( Mx2Tran ( M1 + M2 ) ) . f ) = ( ( ( Mx2Tran M1 ) . f ) + ( ( Mx2Tran M2 ) . f ) )
proof
set T12 = ( Mx2Tran ( M1 + M2 ) );
set T2 = ( Mx2Tran M2 );
set T1 = ( Mx2Tran M1 );
per cases ;
suppose A1: n <> ( 0 );

per cases ;
suppose A2: m = ( 0 );

then ( T12 . f ) = ( 0.REAL ( 0 ) );
hence thesis by A2;
end;
suppose A3: m > ( 0 );

set L = ( LineVec2Mx ( @ f ) );
( len f ) = n by CARD_1:def 7;
then A4: ( width L ) = n by MATRIX13:1;
A5: (( len M2 ) = n & ( width M2 ) = m) by A1,MATRIX13:1;
then A6: ( width ( L * M2 ) ) = m by A4,MATRIX_3:def 4;
A7: 1 <= m by A3,NAT_1:14;
A8: ( len M1 ) = n by A1,MATRIX13:1;
A9: ( width M1 ) = m by A1,MATRIX13:1;
then A10: ( width ( L * M1 ) ) = m by A4,A8,MATRIX_3:def 4;
A11: ( len L ) = 1 by MATRIX13:1;
then ( len ( L * M1 ) ) = 1 by A4,A8,MATRIX_3:def 4;
then A12: [ 1,1 ] in ( Indices ( L * M1 ) ) by A10,A7,MATRIX_1:36;
(( @ ( T1 . f ) ) = ( Line (( L * M1 ),1) ) & ( @ ( T2 . f ) ) = ( Line (( L * M2 ),1) )) by A1,CKB31:def 1;
hence ( ( T1 . f ) + ( T2 . f ) ) = ( ( Line (( L * M1 ),1) ) + ( Line (( L * M2 ),1) ) ) by RVSUM_1:def 4
.= ( Line (( ( L * M1 ) + ( L * M2 ) ),1) ) by A10,A6,A12,MATRIX_4:59
.= ( Line (( L * ( M1 + M2 ) ),1) ) by A1,A11,A4,A8,A9,A5,MATRIX_4:62
.= ( T12 . f ) by A1,CKB31:def 1;
end;
end;
suppose A13: n = ( 0 );

A14: ( 0. ( TOP-REAL m ) ) = ( 0* m ) by EUCLID:70
.= ( m |-> ( 0 ) );
then A15: ( T2 . f ) = ( m |-> ( 0 ) ) by A13,CKB31:def 1;
thus ( T12 . f ) = ( m |-> ( ( 0 ) + ( 0 ) ) ) by A13,A14,CKB31:def 1
.= ( ( m |-> ( 0 ) ) + ( m |-> ( 0 ) ) ) by RVSUM_1:14
.= ( ( T1 . f ) + ( T2 . f ) ) by A13,A14,A15,CKB31:def 1;
end;
end;
