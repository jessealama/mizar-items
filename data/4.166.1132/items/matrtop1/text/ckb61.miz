begin
reserve n for Nat;
reserve m for Nat;
reserve i for Nat;
reserve f for n -element  real-valued FinSequence;
reserve M for (Matrix of n,m,( F_Real ));
theorem
Lm4: (for f being n -element  real-valued FinSequence holds (ex L being m -element (FinSequence of ( REAL )) st (|. ( ( Mx2Tran M ) . f ) .| <= ( ( Sum L ) * |. f .| ) & (for i being Nat holds (i in ( dom L ) implies ( L . i ) = |. ( @ ( Col (M,i) ) ) .|)))))
proof
let f being n -element  real-valued FinSequence;
set Lf = ( LineVec2Mx ( @ f ) );
set LfM = ( Lf * M );
set LM = ( Line (LfM,1) );
deffunc L(Nat) = |. ( @ ( Col (M,$1) ) ) .|;
consider L being FinSequence such that A1: (( len L ) = m & (for k being Nat holds (k in ( dom L ) implies ( L . k ) = L(k)))) from FINSEQ_1:sch 2;
( rng L ) c= ( REAL )
proof
let y being set;
assume y in ( rng L );
then consider x being set such that A2: x in ( dom L ) and A3: ( L . x ) = y by FUNCT_1:def 3;
reconsider x as Nat by A2;
( L . x ) = L(x) by A1,A2;
hence thesis by A3;
end;
then L is (FinSequence of ( REAL )) by FINSEQ_1:def 4;
then reconsider L1 = L as (Element of ( m -tuples_on ( REAL ) )) by A1,FINSEQ_2:92;
take L1;
per cases ;
suppose A4: n <> ( 0 );

then A5: ( len M ) = n by MATRIX13:1;
( ( Mx2Tran M ) . f ) = LM by A4,CKB31:def 1;
then A6: ( len LM ) = m by CARD_1:def 7;
A7: |. ( ( Mx2Tran M ) . f ) .| = ( sqrt ( Sum ( sqr ( @ LM ) ) ) ) by A4,CKB31:def 1;
reconsider LM as (Element of ( m -tuples_on ( REAL ) )) by A6,FINSEQ_2:92;
( len ( abs LM ) ) = m by CARD_1:def 7;
then reconsider aLM = ( abs LM ) as (Element of ( m -tuples_on ( REAL ) )) by FINSEQ_2:92;
A8: ( len f ) = n by CARD_1:def 7;
then A9: ( width Lf ) = n by MATRIX13:1;
( width M ) = m by A4,MATRIX13:1;
then A10: ( width LfM ) = m by A5,A9,MATRIX_3:def 4;
( len Lf ) = 1 by MATRIX13:1;
then A11: ( len LfM ) = 1 by A5,A9,MATRIX_3:def 4;
now
let i being Nat;
A12: ( len ( Col (M,i) ) ) = n by A5,MATRIX_1:def 8;
then A13: ( abs |( f,( @ ( Col (M,i) ) ) )| ) <= ( |. f .| * L(i) ) by A8,EUCLID_2:15;
assume A14: i in ( Seg m );
then A15: (1 <= i & i <= m) by FINSEQ_1:1;
then A16: [ 1,i ] in ( Indices LfM ) by A11,A10,MATRIX_1:36;
i in ( dom L ) by A1,A15,FINSEQ_3:25;
then A17: ( L . i ) = L(i) by A1;
( LM . i ) = ( LfM * (1,i) ) by A10,A14,MATRIX_1:def 7
.= ( ( Line (Lf,1) ) "*" ( Col (M,i) ) ) by A5,A9,A16,MATRIX_3:def 4
.= ( Sum ( mlt (( @ f ),( Col (M,i) )) ) ) by MATRIX15:25
.= ( Sum ( mlt (f,( @ ( Col (M,i) ) )) ) ) by A8,A12,MATRPROB:35,MATRPROB:36
.= |( f,( @ ( Col (M,i) ) ) )| by RVSUM_1:def 16;
then ( aLM . i ) <= ( |. f .| * L(i) ) by A13,VALUED_1:18;
hence ( aLM . i ) <= ( ( |. f .| * L1 ) . i ) by A17,RVSUM_1:44;
end;
then A18: ( Sum aLM ) <= ( Sum ( |. f .| * L1 ) ) by RVSUM_1:82;
( sqr LM ) = ( LM (#) LM ) by VALUED_1:def 8;
then ( sqrt ( Sum ( sqr LM ) ) ) <= ( Sum ( sqrt ( sqr LM ) ) ) by CKB12:1;
then (( Sum ( |. f .| * L1 ) ) = ( |. f .| * ( Sum L1 ) ) & ( sqrt ( Sum ( sqr LM ) ) ) <= ( Sum aLM )) by CKB11:1,RVSUM_1:87;
hence thesis by A7,A1,A18,XXREAL_0:2;
end;
suppose A19: n = ( 0 );

now
let i being Nat;
assume i in ( dom L1 );
then ( L1 . i ) = |. ( @ ( Col (M,i) ) ) .| by A1;
hence ( 0 ) <= ( L1 . i );
end;
then A20: ( Sum L1 ) >= ( 0 ) by RVSUM_1:84;
|. ( ( Mx2Tran M ) . f ) .| = |. ( 0. ( TOP-REAL m ) ) .| by A19,CKB31:def 1
.= ( 0 ) by EUCLID_2:39;
hence thesis by A1,A20;
end;
end;
