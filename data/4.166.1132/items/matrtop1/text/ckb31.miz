begin
reserve n for Nat;
reserve m for Nat;
reserve F for Function;
reserve f for n -element  real-valued FinSequence;
reserve M for (Matrix of n,m,( F_Real ));
definition
let n;
let m;
let M;
func Mx2Tran M -> (Function of ( TOP-REAL n ),( TOP-REAL m )) means 
:Def3: ( it . f ) = ( Line (( ( LineVec2Mx ( @ f ) ) * M ),1) ) if n <> ( 0 ) otherwise ( it . f ) = ( 0. ( TOP-REAL m ) );
existence
proof
set Tm = ( TOP-REAL m );
set Tn = ( TOP-REAL n );
A1:now
assume A2: n <> ( 0 );
defpred P[ (Element of Tn),(Element of Tm) ]
 means
$2 = ( Line (( ( LineVec2Mx ( @ $1 ) ) * M ),1) );
A3: (for x being (Element of Tn) holds (ex y being (Element of Tm) st P[ x,y ]))
proof
let x being (Element of Tn);
set L = ( LineVec2Mx ( @ x ) );
set LL = ( Line (( L * M ),1) );
( len x ) = n by CARD_1:def 7;
then A4: ( width L ) = n by MATRIX13:1;
(( len M ) = n & ( width M ) = m) by A2,MATRIX13:1;
then ( width ( L * M ) ) = m by A4,MATRIX_3:def 4;
then LL in ( REAL m );
then reconsider LL as (Element of Tm) by EUCLID:22;
P[ x,LL ];
hence thesis;
end;
consider F being (Function of Tn,Tm) such that A5: (for x being (Element of Tn) holds P[ x,( F . x ) ]) from FUNCT_2:sch 3(A3);
take F;
let f;
(( @ f ) is (FinSequence of ( REAL )) & ( len f ) = n) by CARD_1:def 7;
then f is (Element of ( n -tuples_on ( REAL ) )) by FINSEQ_2:92;
then f in ( REAL n );
then f is (Element of Tn) by EUCLID:22;
hence ( F . f ) = ( Line (( ( LineVec2Mx ( @ f ) ) * M ),1) ) by A5;
end;
now
assume n = ( 0 );
defpred P[ (Element of Tn),(Element of Tm) ]
 means
$2 = ( 0. Tm );
A6: (for x being (Element of Tn) holds (ex y being (Element of Tm) st P[ x,y ]));
consider F being (Function of Tn,Tm) such that A7: (for x being (Element of Tn) holds P[ x,( F . x ) ]) from FUNCT_2:sch 3(A6);
take F;
let f;
(( @ f ) is (FinSequence of ( REAL )) & ( len f ) = n) by CARD_1:def 7;
then f is (Element of ( n -tuples_on ( REAL ) )) by FINSEQ_2:92;
then f in ( REAL n );
then f is (Element of Tn) by EUCLID:22;
hence ( F . f ) = ( 0. Tm ) by A7;
end;
hence thesis by A1;
end;
uniqueness
proof
let F1 being (Function of ( TOP-REAL n ),( TOP-REAL m ));
let F2 being (Function of ( TOP-REAL n ),( TOP-REAL m ));
A8:now
assume n <> ( 0 );
assume A9: ( F1 . f ) = ( Line (( ( LineVec2Mx ( @ f ) ) * M ),1) );
assume A10: ( F2 . f ) = ( Line (( ( LineVec2Mx ( @ f ) ) * M ),1) );
now
let x being (Element of ( TOP-REAL n ));
thus ( F1 . x ) = ( Line (( ( LineVec2Mx ( @ x ) ) * M ),1) ) by A9
.= ( F2 . x ) by A10;
end;
hence F1 = F2 by FUNCT_2:63;
end;
now
assume n = ( 0 );
assume A11: ( F1 . f ) = ( 0. ( TOP-REAL m ) );
assume A12: ( F2 . f ) = ( 0. ( TOP-REAL m ) );
now
let x being (Element of ( TOP-REAL n ));
thus ( F1 . x ) = ( 0. ( TOP-REAL m ) ) by A11
.= ( F2 . x ) by A12;
end;
hence F1 = F2 by FUNCT_2:63;
end;
hence thesis by A8;
end;
correctness;
end;
