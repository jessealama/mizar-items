constructors BINARITH,CONNSP_2,FINSOP_1,FVSUM_1,LAPLACE,MATRIX_6,MATRIX11,MATRIX13,MATRIX15,MATRLIN2,MONOID_0,PARTFUN3,PNPROC_1,RANKNULL,RELSET_1,RUSUB_5,SEQ_1,SETWISEO,SQUARE_1,TOPS_1,TOPS_2,VECTSP_9,VECTSP10,WELLORD2,SEQ_4,EUCLID,CKB6,CKB7,CKB31;
requirements ARITHM,NUMERALS,REAL,BOOLE,SUBSET;
begin
reserve n for Nat;
reserve m for Nat;
reserve M for (Matrix of n,m,( F_Real ));
theorem
Th39: (( Mx2Tran M ) is  one-to-one iff ( the_rank_of M ) = n)
proof
set MM = ( Mx2Tran M );
per cases ;
suppose A1: (n = ( 0 ) iff m = ( 0 ));

set nV = ( n -VectSp_over ( F_Real ) );
set mV = ( m -VectSp_over ( F_Real ) );
reconsider Bn = ( MX2FinS ( 1. (( F_Real ),n) ) ) as (OrdBasis of nV) by MATRLIN2:45;
reconsider Bm = ( MX2FinS ( 1. (( F_Real ),m) ) ) as (OrdBasis of mV) by MATRLIN2:45;
A2: ( len Bn ) = n by CKB36:1;
then reconsider M1 = M as (Matrix of ( len Bn ),( len Bm ),( F_Real )) by CKB36:1;
A3: ( len M1 ) = n by A1,MATRIX13:1;
A4: ( len Bm ) = m by CKB36:1;
set T = ( Mx2Tran (M1,Bn,Bm) );
A5: (T is  one-to-one iff ( ker T ) = ( (0). nV )) by MATRLIN2:43;
A6: ( width M1 ) = m by A1,MATRIX13:1;
per cases ;
suppose A7: m = ( 0 );

A8: (the carrier of ( TOP-REAL ( 0 ) )) = { ( 0. ( TOP-REAL ( 0 ) ) ) } by EUCLID:22,JORDAN2C:105;
then ( rng MM ) c= { ( 0. ( TOP-REAL ( 0 ) ) ) } by A7,RELAT_1:def 19;
then ( rng MM ) = { ( 0. ( TOP-REAL ( 0 ) ) ) } by ZFMISC_1:33;
then (( card { ( 0. ( TOP-REAL ( 0 ) ) ) } ) = ( card { ( 0. ( TOP-REAL ( 0 ) ) ) } ) & MM is  onto) by A7,A8,FUNCT_2:def 3;
hence thesis by A1,A3,A7,A8,MATRIX13:74,STIRL2_1:60;
end;
suppose A9: m > ( 0 );

then reconsider SS = ( Space_of_Solutions_of ( M1 @ ) ) as (Subspace of nV) by A3,A6,MATRIX_2:10;
A10: ( width ( M1 @ ) ) = n by A3,A6,A9,MATRIX_2:10;
hereby
assume A11: ( Mx2Tran M ) is  one-to-one;
( [#] SS ) c= { ( 0. nV ) }
proof
let x being set;
assume A12: x in ( [#] SS );
then reconsider v = x as (Vector of nV) by VECTSP_4:10;
v = ( v |-- Bn ) by A2,MATRLIN2:46;
then ( v |-- Bn ) in SS by A12,STRUCT_0:def 5;
then v in ( ker T ) by A1,A2,A4,A9,MATRLIN2:41;
then v = ( 0. nV ) by A5,A11,CKB37:1,VECTSP_4:35;
hence thesis by TARSKI:def 1;
end;
then ( [#] SS ) = { ( 0. nV ) } by ZFMISC_1:33;
then SS = ( (0). nV ) by VECTSP_4:def 3;
then ( dim SS ) = ( 0 ) by RANKNULL:16;
then ( 0 ) = ( n - ( the_rank_of ( M1 @ ) ) ) by A1,A9,A10,MATRIX15:68;
hence ( the_rank_of M ) = n by MATRIX13:84;
end;
A13: ( the_rank_of ( M1 @ ) ) = ( the_rank_of M ) by MATRIX13:84;
assume ( the_rank_of M ) = n;
then ( dim SS ) = ( n - n ) by A1,A9,A10,A13,MATRIX15:68;
then A14: SS is  trivial by MATRLIN2:42;
( [#] ( ker T ) ) c= { ( 0. nV ) }
proof
let x being set;
assume A15: x in ( [#] ( ker T ) );
then reconsider v = x as (Vector of nV) by VECTSP_4:10;
v in ( ker T ) by A15,STRUCT_0:def 5;
then ( v |-- Bn ) in SS by A1,A2,A4,A9,MATRLIN2:41;
then v in SS by A2,MATRLIN2:46;
then v in (the carrier of SS) by STRUCT_0:def 5;
then v = ( 0. SS ) by A14,STRUCT_0:def 18;
then v = ( 0. nV ) by VECTSP_4:11;
hence thesis by TARSKI:def 1;
end;
then ( [#] ( ker T ) ) = { ( 0. nV ) } by ZFMISC_1:33;
hence thesis by A5,CKB37:1,VECTSP_4:def 3;
end;
end;
suppose A16: (n = ( 0 ) & m <> ( 0 ));

A17:now
let x1 being set;
let x2 being set;
assume that
A18: ((x1 in ( dom MM ) & x2 in ( dom MM )) & ( MM . x1 ) = ( MM . x2 ));
A19: ( dom MM ) = ( [#] ( TOP-REAL ( 0 ) ) ) by A16,FUNCT_2:def 1
.= { ( 0. ( TOP-REAL ( 0 ) ) ) } by EUCLID:22,JORDAN2C:105;
hence x1 = ( 0. ( TOP-REAL ( 0 ) ) ) by A18,TARSKI:def 1
.= x2 by A18,A19,TARSKI:def 1;
end;
( len M ) = n by MATRIX_1:def 2;
hence thesis by A17,A16,FUNCT_1:def 4,MATRIX13:74;
end;
suppose A20: (n <> ( 0 ) & m = ( 0 ));

reconsider x1 = ( n |-> 1 ) as (Point of ( TOP-REAL n )) by CKB30:1;
reconsider x2 = ( n |-> 2 ) as (Point of ( TOP-REAL n )) by CKB30:1;
A21: ( dom MM ) = ( [#] ( TOP-REAL n ) ) by FUNCT_2:def 1;
A22: ( MM . x1 ) = ( 0. ( TOP-REAL ( 0 ) ) ) by A20
.= ( MM . x2 ) by A20;
A23: x1 <> x2
proof
assume A24: x1 = x2;
A25: x1 = ( ( Seg n ) --> 1 ) by FINSEQ_2:def 2
.= [: ( Seg n ),{ 1 } :] by FUNCOP_1:def 2;
x2 = ( ( Seg n ) --> 2 ) by FINSEQ_2:def 2
.= [: ( Seg n ),{ 2 } :] by FUNCOP_1:def 2;
then { 1 } = { 2 } by A24,A25,A20,ZFMISC_1:110;
then 2 in { 1 } by TARSKI:def 1;
hence contradiction by TARSKI:def 1;
end;
( width M ) = m by A20,MATRIX13:1;
hence thesis by A23,A20,A22,A21,FUNCT_1:def 4,MATRIX13:74;
end;
end;
