environ
vocabularies ANALMETR,SUBSET_1,SYMSP_1,ANALOAF,AFF_1,DIRAF,INCSP_1,AFF_2,CONAFFM,STRUCT_0,CONMETR;
notations SUBSET_1,STRUCT_0,ANALOAF,AFF_1,ANALMETR,AFF_2,CONAFFM,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8;
definitions AFF_2,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8;
theorems AFF_1,AFF_2,ANALMETR,TRANSLAC,CONAFFM,DIRAF,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8;
registrations ANALMETR;
constructors AFF_1,AFF_2,CONAFFM,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8;
requirements SUBSET;
begin
reserve X for OrtAfPl;
reserve a for (Element of X);
reserve b for (Element of X);
reserve c for (Element of X);
theorem
Th1: (ex a st (ex b st (ex c st ((( LIN a,b,c & a <> b) & b <> c) & c <> a))))
proof
consider a being (Element of X),p being (Element of X) such that A1: a <> p by ANALMETR:39;
consider b such that A2: a,p _|_ p,b and A3: p <> b by ANALMETR:39;
reconsider a9 = a,b9 = b,p9 = p as (Element of ( Af X )) by ANALMETR:35;
consider c such that A4: p,c _|_ a,b and A5:  LIN a,b,c by ANALMETR:69;
take a;
take b;
take c;
thus  LIN a,b,c by A5;
thus a <> b
proof
assume (not thesis);
then a,p _|_ a,p by A2,ANALMETR:61;
hence contradiction by A1,ANALMETR:39;
end;

thus b <> c
proof
assume (not thesis);
then a,p // a,b by A2,A3,A4,ANALMETR:63;
then  LIN a,p,b by ANALMETR:def 10;
then  LIN a9,p9,b9 by ANALMETR:40;
then  LIN p9,a9,b9 by AFF_1:6;
then  LIN p,a,b by ANALMETR:40;
then p,a // p,b by ANALMETR:def 10;
then a,p _|_ p,a by A2,A3,ANALMETR:62;
then a,p _|_ a,p by ANALMETR:61;
hence contradiction by A1,ANALMETR:39;
end;

assume (not thesis);
then a,p _|_ a,b by A4,ANALMETR:61;
then p,b // a,b by A1,A2,ANALMETR:63;
then b,p // b,a by ANALMETR:59;
then  LIN b,p,a by ANALMETR:def 10;
then  LIN b9,p9,a9 by ANALMETR:40;
then  LIN p9,a9,b9 by AFF_1:6;
then  LIN p,a,b by ANALMETR:40;
then p,a // p,b by ANALMETR:def 10;
then a,p _|_ p,a by A2,A3,ANALMETR:62;
then a,p _|_ a,p by ANALMETR:61;
hence contradiction by A1,ANALMETR:39;
end;
