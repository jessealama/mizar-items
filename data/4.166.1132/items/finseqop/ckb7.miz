theorem
Th8: (for F being Function holds (for X being set holds (for x1,x2 being set holds ([ x1,x2 ] in ( dom F ) implies ( F .: (( X --> x1 ),( X --> x2 )) ) = ( X --> ( F . (x1,x2) ) )))))
proof
let F being Function;
let X being set;
let x1 being set,x2 being set;
assume that
A1: [ x1,x2 ] in ( dom F );
set f = ( X --> x1 );
set g = ( X --> x2 );
thus ( F .: (f,g) ) = ( F * ( X --> [ x1,x2 ] ) ) by CKB6:1
.= ( X --> ( F . (x1,x2) ) ) by A1,FUNCOP_1:17;
end;
