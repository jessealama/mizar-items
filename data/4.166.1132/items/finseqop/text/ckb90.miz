theorem
Th82: (for F,f,g being Function holds ([ x,y ] in ( dom ( F * (f,g) ) ) implies ( ( F * (f,g) ) . (x,y) ) = ( F . (( f . x ),( g . y )) )))
proof
let F being Function;
let f being Function;
let g being Function;
assume that
A1: [ x,y ] in ( dom ( F * (f,g) ) );
[ x,y ] in ( dom [: f,g :] ) by A1,FUNCT_1:11;
then [ x,y ] in [: ( dom f ),( dom g ) :] by FUNCT_3:def 8;
then ( [: f,g :] . (x,y) ) = [ ( f . x ),( g . y ) ] by FUNCT_3:65;
hence thesis by A1,FUNCT_1:12;
end;
