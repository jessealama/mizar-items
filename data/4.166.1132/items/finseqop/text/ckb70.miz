begin
reserve D for non  empty set;
reserve d for (Element of D);
reserve F for (BinOp of D);
definition
let D;
let F;
assume that
A1: F is  having_a_unity
and
A2: F is  associative
and
A3: F is  having_an_inverseOp;
func the_inverseOp_wrt F -> (UnOp of D) means 
:Def3: it is_an_inverseOp_wrt F;
existence by A3,CKB69:def 1;
uniqueness
proof
set e = ( the_unity_wrt F );
let u1 being (UnOp of D);
let u2 being (UnOp of D);
assume that
A4: (for d holds (( F . (d,( u1 . d )) ) = e & ( F . (( u1 . d ),d) ) = e))
and
A5: (for d holds (( F . (d,( u2 . d )) ) = e & ( F . (( u2 . d ),d) ) = e));
now
let d;
set d1 = ( u1 . d );
set d2 = ( u2 . d );
thus ( u1 . d ) = ( F . (d1,e) ) by A1,SETWISEO:15
.= ( F . (d1,( F . (d,d2) )) ) by A5
.= ( F . (( F . (d1,d) ),d2) ) by A2,BINOP_1:def 3
.= ( F . (e,d2) ) by A4
.= ( u2 . d ) by A1,SETWISEO:15;
end;
hence thesis by FUNCT_2:63;
end;
