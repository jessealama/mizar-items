requirements SUBSET,BOOLE;
begin
reserve UA for Universal_Algebra;
Lm1:now
let UA;
set F = ( UAEndMonoid UA );
let x being (Element of F);
let e being (Element of F);
reconsider i = e,y = x as (Element of ( UAEnd UA )) by CKB6:def 1;
assume A1: e = ( id (the carrier of UA) );
thus ( x * e ) = ( ( UAEndComp UA ) . (y,i) ) by CKB6:def 1
.= ( i * y ) by CKB5:def 1
.= x by A1,FUNCT_2:17;
thus ( e * x ) = ( ( UAEndComp UA ) . (i,y) ) by CKB6:def 1
.= ( y * i ) by CKB5:def 1
.= x by A1,FUNCT_2:17;
end;
registration
let UA;
cluster ( UAEndMonoid UA ) ->  well-unital  associative;
coherence
proof
reconsider i = ( id (the carrier of UA) ) as (Element of ( UAEnd UA )) by CKB3:1;
set H = multLoopStr (# ( UAEnd UA ),( UAEndComp UA ),i #);
set F = ( UAEndMonoid UA );
thus F is  well-unital
proof
let x being (Element of ( UAEndMonoid UA ));
( 1. ( UAEndMonoid UA ) ) = i by CKB6:def 1;
hence thesis by Lm1;
end;

(for f,g,h being (Element of H) holds ( ( f * g ) * h ) = ( f * ( g * h ) ))
proof
let f being (Element of H);
let g being (Element of H);
let h being (Element of H);
reconsider A = f,B = g,C = h as (Element of ( UAEnd UA ));
A1: ( g * h ) = ( C * B ) by CKB5:def 1;
( f * g ) = ( B * A ) by CKB5:def 1;
hence ( ( f * g ) * h ) = ( C * ( B * A ) ) by CKB5:def 1
.= ( ( C * B ) * A ) by RELAT_1:36
.= ( f * ( g * h ) ) by A1,CKB5:def 1;
end;
then (( 1. H ) = i & H is  associative) by GROUP_1:def 3;
hence thesis by CKB6:def 1;
end;
end;
