begin
reserve i for (Element of ( NAT ));
reserve j for (Element of ( NAT ));
reserve n for (Element of ( NAT ));
definition
let n;
let i;
redefine func n |-> i -> (FinSequence of ( NAT )) means 
:Def3: (( len it ) = n & (for j holds ((1 <= j & j <= n) implies ( it . j ) = i)));
coherence by FINSEQ_2:63;
compatibility
proof
let a being (FinSequence of ( NAT ));
hereby
assume A1: a = ( n |-> i );
hence ( len a ) = n by CARD_1:def 7;
thus (for j holds ((1 <= j & j <= n) implies ( a . j ) = i))
proof
let j;
assume (1 <= j & j <= n);
then j in ( Seg n ) by FINSEQ_1:1;
hence thesis by A1,FUNCOP_1:7;
end;

end;
assume that
A2: ( len a ) = n
and
A3: (for j holds ((1 <= j & j <= n) implies ( a . j ) = i));
A4: ( dom a ) = ( Seg n ) by A2,FINSEQ_1:def 3;
now
let z being set;
assume A5: z in ( dom a );
then reconsider z9 = z as (Element of ( NAT ));
(1 <= z9 & z9 <= n) by A4,A5,FINSEQ_1:1;
hence ( a . z ) = i by A3;
end;
hence thesis by A4,FUNCOP_1:11;
end;
end;
