environ
vocabularies LATTICES,ROBBINS1,XBOOLE_0,SUBSET_1,ARYTM_3,EQREL_1,ZFMISC_1,PBOOLE,STRUCT_0,BINOP_1,FUNCT_5,RELAT_1,FUNCT_1,SHEFFER1,CARD_1;
notations TARSKI,XBOOLE_0,FUNCT_5,CARD_1,STRUCT_0,LATTICES,BINOP_1,ROBBINS1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB17;
definitions LATTICES,ROBBINS1,STRUCT_0,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB17;
theorems STRUCT_0,ROBBINS1,LATTICES,CARD_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31;
registrations LATTICES,LATTICE6,ROBBINS1,XBOOLE_0,ZFMISC_1,CARD_1,ORDINAL1,STRUCT_0,CKB10;
constructors BINOP_1,ROBBINS1,FUNCT_5,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB17;
requirements SUBSET,NUMERALS;
begin
theorem
(for L being  complemented'  lower-bounded'  upper-bounded'  join-commutative  meet-commutative  join-idempotent  distributive  distributive' non  empty LattStr holds (for x,y being (Element of L) holds (x is_a_complement'_of y iff x is_a_complement_of y)))
proof
let L being  complemented'  lower-bounded'  upper-bounded'  join-commutative  meet-commutative  join-idempotent  distributive  distributive' non  empty LattStr;
let x being (Element of L);
let y being (Element of L);
hereby
assume A1: x is_a_complement'_of y;
then ( x "/\" y ) = ( Bot' L ) by CKB7:def 1;
then A2: ( x "/\" y ) = ( Bottom L ) by CKB29:1;
( x "\/" y ) = ( Top' L ) by A1,CKB7:def 1;
then ( x "\/" y ) = ( Top L ) by CKB28:1;
hence x is_a_complement_of y by A2,LATTICES:def 18;
end;
assume A3: x is_a_complement_of y;
then ( x "/\" y ) = ( Bottom L ) by LATTICES:def 18;
then A4: ( x "/\" y ) = ( Bot' L ) by CKB29:1;
( x "\/" y ) = ( Top L ) by A3,LATTICES:def 18;
then ( x "\/" y ) = ( Top' L ) by CKB28:1;
hence thesis by A4,CKB7:def 1;
end;
