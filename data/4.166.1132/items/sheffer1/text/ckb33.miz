begin
theorem
Th24: (for L being  complemented'  join-commutative  meet-commutative  lower-bounded'  upper-bounded'  join-idempotent  distributive  distributive' non  empty LattStr holds L is  complemented)
proof
let L being  complemented'  join-commutative  meet-commutative  lower-bounded'  upper-bounded'  join-idempotent  distributive  distributive' non  empty LattStr;
(for b being (Element of L) holds (ex a being (Element of L) st a is_a_complement_of b))
proof
let b being (Element of L);
consider a being (Element of L) such that A1: a is_a_complement'_of b by CKB8:def 1;
( b "/\" a ) = ( Bot' L ) by A1,CKB7:def 1;
then A2: ( b "/\" a ) = ( Bottom L ) by CKB29:1;
( b "\/" a ) = ( Top' L ) by A1,CKB7:def 1;
then ( b "\/" a ) = ( Top L ) by CKB28:1;
then a is_a_complement_of b by A2,LATTICES:def 18;
hence thesis;
end;
hence thesis by LATTICES:def 19;
end;
