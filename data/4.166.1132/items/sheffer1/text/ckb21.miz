begin
theorem
Th12: (for L being  complemented'  join-commutative  upper-bounded'  meet-commutative  join-idempotent  distributive  distributive'  lower-bounded' non  empty LattStr holds L is  join-absorbing)
proof
let L being  complemented'  join-commutative  upper-bounded'  meet-commutative  join-idempotent  distributive  distributive'  lower-bounded' non  empty LattStr;
let x being (Element of L);
let y being (Element of L);
A1: L is  meet-idempotent by CKB18:1;
A2: L is  meet-absorbing by CKB20:1;
( x "/\" ( x "\/" y ) ) = ( ( x "/\" x ) "\/" ( x "/\" y ) ) by LATTICES:def 11
.= ( x "\/" ( x "/\" y ) ) by A1,CKB17:def 1
.= x by A2,LATTICES:def 8;
hence thesis;
end;
