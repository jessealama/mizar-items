theorems STRUCT_0,ROBBINS1,LATTICES,CARD_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB41,CKB45,CKB46,CKB47,CKB48,CKB49;
registrations LATTICES,LATTICE6,ROBBINS1,XBOOLE_0,ZFMISC_1,CARD_1,ORDINAL1,STRUCT_0,CKB10,CKB36,CKB37,CKB38,CKB39,CKB40,CKB42,CKB43,CKB44,CKB50,CKB51,CKB52;
constructors BINOP_1,ROBBINS1,FUNCT_5,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB17,CKB38,CKB39,CKB40,CKB41,CKB45,CKB46,CKB47,CKB48,CKB49;
requirements SUBSET,NUMERALS;
begin
registration
cluster  ->  join-absorbing  meet-absorbing  Boolean for 1 -element 1 -element LattStr;
coherence
proof
let L being 1 -element LattStr;
thus L is  join-absorbing
proof
let x being (Element of L),y being (Element of L);
thus thesis by STRUCT_0:def 10;
end;

A2: L is  upper-bounded
proof
set c = the (Element of L);
take c;
let a being (Element of L);
thus thesis by STRUCT_0:def 10;
end;
thus L is  meet-absorbing
proof
let x being (Element of L),y being (Element of L);
thus thesis by STRUCT_0:def 10;
end;

A3: L is  lower-bounded
proof
set c = the (Element of L);
take c;
let a being (Element of L);
thus thesis by STRUCT_0:def 10;
end;
A4: L is  complemented
proof
set a = the (Element of L);
let b being (Element of L);
take a;
A5: (( a "/\" b ) = ( Bottom L ) & ( b "/\" a ) = ( Bottom L )) by STRUCT_0:def 10;
(( a "\/" b ) = ( Top L ) & ( b "\/" a ) = ( Top L )) by STRUCT_0:def 10;
hence thesis by A5,LATTICES:def 18;
end;
L is  distributive
proof
let a being (Element of L),b being (Element of L),c being (Element of L);
thus thesis by STRUCT_0:def 10;
end;
hence thesis by A3,A2,A4;
end;
end;
