theorem
Th22: (for L being  Boolean  complemented  lower-bounded  upper-bounded  distributive  distributive'  Lattice-like non  empty LattStr holds ( Bottom L ) = ( Bot' L ))
proof
let L being  Boolean  complemented  lower-bounded  upper-bounded  distributive  distributive'  Lattice-like non  empty LattStr;
set Y = ( Bottom L );
(L is  lower-bounded' & (for a being (Element of L) holds (( Y "\/" a ) = a & ( a "\/" Y ) = a))) by CKB25:1,LATTICES:14;
hence thesis by CKB5:def 1;
end;
