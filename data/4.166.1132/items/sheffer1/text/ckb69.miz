environ
vocabularies LATTICES,ROBBINS1,XBOOLE_0,SUBSET_1,ARYTM_3,EQREL_1,ZFMISC_1,PBOOLE,STRUCT_0,BINOP_1,FUNCT_5,RELAT_1,FUNCT_1,SHEFFER1,CARD_1;
notations TARSKI,XBOOLE_0,FUNCT_5,CARD_1,STRUCT_0,LATTICES,BINOP_1,ROBBINS1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB17,CKB38,CKB39,CKB40,CKB41,CKB45,CKB46,CKB47,CKB48,CKB49,CKB60;
definitions LATTICES,ROBBINS1,STRUCT_0,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB17,CKB41,CKB45,CKB46,CKB47,CKB48,CKB49,CKB60;
theorems STRUCT_0,ROBBINS1,LATTICES,CARD_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB41,CKB45,CKB46,CKB47,CKB48,CKB49,CKB57,CKB58,CKB59,CKB60,CKB61,CKB62,CKB63,CKB64,CKB65,CKB66,CKB67,CKB68;
registrations LATTICES,LATTICE6,ROBBINS1,XBOOLE_0,ZFMISC_1,CARD_1,ORDINAL1,STRUCT_0,CKB10,CKB36,CKB37,CKB38,CKB39,CKB40,CKB42,CKB43,CKB44,CKB50,CKB51,CKB52,CKB53,CKB54,CKB55,CKB56;
constructors BINOP_1,ROBBINS1,FUNCT_5,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB17,CKB38,CKB39,CKB40,CKB41,CKB45,CKB46,CKB47,CKB48,CKB49,CKB60;
requirements SUBSET,NUMERALS;
begin
theorem
(for L being  satisfying_Sheffer_1  satisfying_Sheffer_2  satisfying_Sheffer_3  properly_defined non empty ShefferOrthoLattStr holds L is  Boolean  Boolean Lattice)
proof
let L being  satisfying_Sheffer_1  satisfying_Sheffer_2  satisfying_Sheffer_3  properly_defined non empty ShefferOrthoLattStr;
A1: (L is  distributive & L is  distributive') by CKB67:1,CKB68:1;
(ex c being (Element of L) st (for a being (Element of L) holds (( c "/\" a ) = a & ( a "/\" c ) = a)))
proof
set b = the (Element of L);
take c = ( ( b | b ) | ( ( b | b ) | ( b | b ) ) );
let a being (Element of L);
thus ( c "/\" a ) = a
proof
set X = ( b " );
( c "/\" a ) = ( ( ( ( b | b ) | ( X | X ) ) | a ) " ) by CKB46:def 1
.= ( ( a | ( X | ( X | X ) ) ) " ) by CKB63:1
.= ( ( a | a ) " ) by CKB48:def 1
.= a by CKB47:def 1;
hence thesis;
end;

thus ( a "/\" c ) = a
proof
set X = ( b " );
( a "/\" c ) = ( ( a | ( ( b | b ) | ( X | X ) ) ) " ) by CKB46:def 1
.= ( ( a | a ) " ) by CKB48:def 1
.= a by CKB47:def 1;
hence thesis;
end;

end;
then A2: L is  upper-bounded' by CKB2:def 1;
(ex c being (Element of L) st (for a being (Element of L) holds (( c "\/" a ) = a & ( a "\/" c ) = a)))
proof
set b = the (Element of L);
take c = ( ( b | ( b | b ) ) | ( b | ( b | b ) ) );
let a being (Element of L);
( c "\/" a ) = ( ( ( ( b | ( b | b ) ) " ) " ) | ( a | a ) ) by CKB46:def 1
.= ( ( ( ( a | ( a | a ) ) " ) " ) | ( a | a ) ) by CKB64:1
.= ( ( a | ( a | a ) ) | ( a | a ) ) by CKB47:def 1
.= ( ( a | a ) | ( a | ( a | a ) ) ) by CKB63:1
.= ( ( a | a ) | ( a | a ) ) by CKB48:def 1
.= a by CKB47:def 1;
hence ( c "\/" a ) = a;
( a "\/" c ) = ( ( a | a ) | ( ( ( b | ( b | b ) ) " ) " ) ) by CKB46:def 1
.= ( ( a | a ) | ( ( ( a | ( a | a ) ) " ) " ) ) by CKB64:1
.= ( ( a | a ) | ( a | ( a | a ) ) ) by CKB47:def 1
.= ( ( a | a ) | ( a | a ) ) by CKB48:def 1
.= a by CKB47:def 1;
hence ( a "\/" c ) = a;
end;
then A3: L is  lower-bounded' by CKB4:def 1;
(for b being (Element of L) holds (ex a being (Element of L) st a is_a_complement'_of b))
proof
let b being (Element of L);
set a = ( b | b );
take a;
A4: ( Top' L ) = ( ( b | b ) | ( ( b | b ) | ( b | b ) ) )
proof
set X = ( ( b | b ) | ( ( b | b ) | ( b | b ) ) );
(for a being (Element of L) holds (( X "/\" a ) = a & ( a "/\" X ) = a))
proof
let a being (Element of L);
set Y = ( b " );
thus ( X "/\" a ) = ( ( ( ( b | b ) | ( Y | Y ) ) | a ) " ) by CKB46:def 1
.= ( ( a | ( Y | ( Y | Y ) ) ) " ) by CKB63:1
.= ( ( a | a ) " ) by CKB48:def 1
.= a by CKB47:def 1;
thus ( a "/\" X ) = ( ( a | ( ( b | b ) | ( Y | Y ) ) ) " ) by CKB46:def 1
.= ( ( a | a ) " ) by CKB48:def 1
.= a by CKB47:def 1;
end;
hence thesis by A2,CKB3:def 1;
end;
then A5: ( b "\/" a ) = ( Top' L ) by CKB46:def 1;
A6: ( Bot' L ) = ( ( b | ( b | b ) ) | ( b | ( b | b ) ) )
proof
set X = ( ( b | ( b | b ) ) | ( b | ( b | b ) ) );
(for a being (Element of L) holds (( X "\/" a ) = a & ( a "\/" X ) = a))
proof
let a being (Element of L);
thus ( X "\/" a ) = ( ( ( ( b | ( b | b ) ) " ) " ) | ( a | a ) ) by CKB46:def 1
.= ( ( ( ( a | ( a | a ) ) " ) " ) | ( a | a ) ) by CKB64:1
.= ( ( a | ( a | a ) ) | ( a | a ) ) by CKB47:def 1
.= ( ( a | a ) | ( a | ( a | a ) ) ) by CKB63:1
.= ( ( a | a ) | ( a | a ) ) by CKB48:def 1
.= a by CKB47:def 1;
thus ( a "\/" X ) = ( ( a | a ) | ( ( ( b | ( b | b ) ) " ) " ) ) by CKB46:def 1
.= ( ( a | a ) | ( ( ( a | ( a | a ) ) " ) " ) ) by CKB64:1
.= ( ( a | a ) | ( a | ( a | a ) ) ) by CKB47:def 1
.= ( ( a | a ) | ( a | a ) ) by CKB48:def 1
.= a by CKB47:def 1;
end;
hence thesis by A3,CKB5:def 1;
end;
then A7: ( b "/\" a ) = ( Bot' L ) by CKB46:def 1;
A8: ( a "\/" b ) = ( ( ( b | b ) | ( b | b ) ) | ( b | b ) ) by CKB46:def 1
.= ( Top' L ) by A4,CKB63:1;
( a "/\" b ) = ( ( ( b | b ) | b ) | ( ( b | b ) | b ) ) by CKB46:def 1
.= ( ( b | ( b | b ) ) | ( ( b | b ) | b ) ) by CKB63:1
.= ( Bot' L ) by A6,CKB63:1;
hence thesis by A8,A5,A7,CKB7:def 1;
end;
then A9: L is  complemented' by CKB8:def 1;
(L is  join-commutative & L is  meet-commutative) by CKB65:1,CKB66:1;
hence thesis by A3,A2,A9,A1;
end;
