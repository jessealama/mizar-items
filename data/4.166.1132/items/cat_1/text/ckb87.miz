theorem
Th100: (for T being (Function of (the carrier' of C),(the carrier' of D)) holds (for F being (Function of (the carrier of C),(the carrier of D)) holds ((((for c being (Object of C) holds ( T . ( id c ) ) = ( id ( F . c ) )) & (for f being (Morphism of C) holds (( F . ( dom f ) ) = ( dom ( T . f ) ) & ( F . ( cod f ) ) = ( cod ( T . f ) )))) & (for f,g being (Morphism of C) holds (( dom g ) = ( cod f ) implies ( T . ( g * f ) ) = ( ( T . g ) * ( T . f ) )))) implies T is (Functor of C,D))))
proof
let T being (Function of (the carrier' of C),(the carrier' of D));
let F being (Function of (the carrier of C),(the carrier of D));
assume that
A1: (for c being (Object of C) holds ( T . ( id c ) ) = ( id ( F . c ) ))
and
A2: (for f being (Morphism of C) holds (( F . ( dom f ) ) = ( dom ( T . f ) ) & ( F . ( cod f ) ) = ( cod ( T . f ) )))
and
A3: (for f,g being (Morphism of C) holds (( dom g ) = ( cod f ) implies ( T . ( g * f ) ) = ( ( T . g ) * ( T . f ) )));
A4: (for c being (Object of C) holds (ex d being (Object of D) st ( T . ( id c ) ) = ( id d )))
proof
let c being (Object of C);
take ( F . c );
thus thesis by A1;
end;
(for f being (Morphism of C) holds (( T . ( id ( dom f ) ) ) = ( id ( dom ( T . f ) ) ) & ( T . ( id ( cod f ) ) ) = ( id ( cod ( T . f ) ) )))
proof
let f being (Morphism of C);
thus ( T . ( id ( dom f ) ) ) = ( id ( F . ( dom f ) ) ) by A1
.= ( id ( dom ( T . f ) ) ) by A2;
thus ( T . ( id ( cod f ) ) ) = ( id ( F . ( cod f ) ) ) by A1
.= ( id ( cod ( T . f ) ) ) by A2;
end;
hence thesis by A3,A4,CKB83:1;
end;
