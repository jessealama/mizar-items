begin
theorem
Th23: (for D being non empty set holds (for CR being (FinSequence of D) holds CR is_a_record_of ( <*> D ),CR))
proof
let D being non empty set;
let CR being (FinSequence of D);
A1: CR is_terminated_by CR by FINSEQ_8:28;
( addcr (( <*> D ),CR) ) = ( ovlcon (( <*> D ),CR) ) by FINSEQ_8:def 11
.= ( ( <*> D ) ^ ( CR /^ ( len ( ovlpart (( <*> D ),CR) ) ) ) ) by FINSEQ_8:def 3
.= ( ( <*> D ) ^ ( CR /^ ( len ( <*> D ) ) ) ) by CKB23:1
.= ( ( <*> D ) ^ ( CR /^ ( 0 ) ) )
.= ( CR /^ ( 0 ) ) by FINSEQ_1:34
.= CR by FINSEQ_5:28;
hence thesis by A1,CKB22:def 1;
end;
