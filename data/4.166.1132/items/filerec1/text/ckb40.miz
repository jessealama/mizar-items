proof
let D being non empty set;
let f being (File of D),g being (File of D);
let n being (Element of ( NAT ));
assume A1: (( 0 ) < n & n <= ( len f ));
assume A2: ( instr (n,f,g) ) > ( len f );
then ( instr (n,f,g) ) >= ( ( len f ) + 1 ) by NAT_1:13;
then ( ( instr (n,f,g) ) - 1 ) >= ( ( ( len f ) + 1 ) - 1 ) by XREAL_1:9;
then ( ( instr (n,f,g) ) -' 1 ) >= ( len f ) by XREAL_0:def 2;
then ( f /^ ( ( instr (n,f,g) ) -' 1 ) ) = ( {} ) by FINSEQ_5:32;
then A3: ( len ( f /^ ( ( instr (n,f,g) ) -' 1 ) ) ) < 1;
( instr (n,f,g) ) > ( 0 ) by A2,NAT_1:2;
then (( len g ) >= ( 0 ) & g is_preposition_of ( f /^ ( ( instr (n,f,g) ) -' 1 ) )) by FINSEQ_8:def 10,NAT_1:2;
then g = ( {} ) by A3,FINSEQ_8:def 8;
hence thesis by A1,A2,CKB39:1;
end;
