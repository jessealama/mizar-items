theorems NAT_1,NAT_2,REVROT_1,JORDAN4,FINSEQ_1,FINSEQ_2,FINSEQ_5,FINSEQ_6,RFINSEQ,FUNCT_2,FINSEQ_8,GENEALG1,TOPREAL3,CALCUL_1,FINSEQ_3,XREAL_1,CARD_1,XXREAL_0,PARTFUN1,XREAL_0,NAT_D,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15;
registrations XBOOLE_0,RELAT_1,FUNCT_1,XXREAL_0,XREAL_0,FINSEQ_1,FINSEQ_5,ORDINAL1,CARD_1;
constructors XXREAL_0,NAT_1,PARTFUN1,RFINSEQ,NAT_D,FINSEQ_4,FINSEQ_5,CQC_LANG,FINSEQ_6,FINSEQ_8,REAL_1,RELSET_1;
requirements NUMERALS,REAL,BOOLE,SUBSET,ARITHM;
begin
reserve a for set;
reserve b for set;
reserve c for set;
theorem
(for D being non  empty set holds (for f being (FinSequence of D) holds (f = <* a,b,c *> implies ( f /^ 2 ) = <* c *>)))
proof
let D being non  empty set;
let f being (FinSequence of D);
assume A1: f = <* a,b,c *>;
then reconsider a2 = a,b2 = b,c2 = c as (Element of D) by CKB10:1;
( f /^ 2 ) = ( ( <* a2 *> ^ <* b2,c2 *> ) /^ ( 1 + 1 ) ) by A1,FINSEQ_1:43
.= ( ( <* a2 *> ^ <* b2,c2 *> ) /^ ( ( len <* a2 *> ) + 1 ) ) by FINSEQ_1:40
.= ( <* b2,c2 *> /^ 1 ) by FINSEQ_5:36
.= <* c2 *> by FINSEQ_6:46;
hence thesis;
end;
