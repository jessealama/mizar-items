requirements NUMERALS,BOOLE,SUBSET;
begin
definition
let S being non void non empty ManySortedSign;
let X being  non-empty (ManySortedSet of (the carrier of S));
func FreeSort X -> (ManySortedSet of (the carrier of S))
means
:Def13: (for s being (SortSymbol of S) holds ( it . s ) = ( FreeSort (X,s) ));
existence
proof
deffunc F((Element of S)) = ( FreeSort (X,$1) );
consider f being Function such that A1: (( dom f ) = (the carrier of S) & (for d being (Element of S) holds ( f . d ) = F(d))) from FUNCT_1:sch 4;
reconsider f as (ManySortedSet of (the carrier of S)) by A1,PARTFUN1:def 2,RELAT_1:def 18;
take f;
thus thesis by A1;
end;
uniqueness
proof
let A being (ManySortedSet of (the carrier of S)),B being (ManySortedSet of (the carrier of S));
assume that
A2: (for s being (SortSymbol of S) holds ( A . s ) = ( FreeSort (X,s) ))
and
A3: (for s being (SortSymbol of S) holds ( B . s ) = ( FreeSort (X,s) ));
(for i being set holds (i in (the carrier of S) implies ( A . i ) = ( B . i )))
proof
let i being set;
assume i in (the carrier of S);
then reconsider s = i as (SortSymbol of S);
( A . s ) = ( FreeSort (X,s) ) by A2;
hence thesis by A3;
end;
hence thesis by PBOOLE:3;
end;
end;
