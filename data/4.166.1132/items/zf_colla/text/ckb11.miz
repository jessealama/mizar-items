begin
reserve X for set;
reserve Y for set;
reserve Z for set;
reserve x for set;
reserve y for set;
reserve f for Function;
definition
let f;
let X;
let Y;
pred f is_epsilon-isomorphism_of X,Y
means
(((( dom f ) = X & ( rng f ) = Y) & f is  one-to-one) & (for x holds (for y holds ((x in X & y in X) implies ((ex Z st (Z = y & x in Z)) iff (ex Z st (( f . y ) = Z & ( f . x ) in Z)))))))
;end;
