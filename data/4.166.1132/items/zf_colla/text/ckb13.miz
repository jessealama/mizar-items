requirements NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve Y for set;
reserve E for non  empty set;
reserve f for Function;
reserve d for (Element of E);
theorem
Th12: ((( dom f ) = E & (for d holds ( f . d ) = ( f .: d ))) implies ( rng f ) is  epsilon-transitive)
proof
assume that
A1: ( dom f ) = E
and
A2: (for d holds ( f . d ) = ( f .: d ));
let Y;
assume Y in ( rng f );
then consider b being set such that A3: b in ( dom f ) and A4: Y = ( f . b ) by FUNCT_1:def 3;
reconsider d = b as (Element of E) by A1,A3;
A5: ( f . d ) = ( f .: d ) by A2;
let a being set;
assume a in Y;
then (ex c being set st ((c in ( dom f ) & c in d) & a = ( f . c ))) by A4,A5,FUNCT_1:def 6;
hence thesis by FUNCT_1:def 3;
end;
