environ
vocabularies PRE_TOPC,STRUCT_0,TARSKI,SUBSET_1,RCOMP_1,XBOOLE_0,ZFMISC_1,TDLAT_3,NATTRA_1,SETFAM_1,TSP_1;
notations TARSKI,XBOOLE_0,SUBSET_1,DOMAIN_1,STRUCT_0,PRE_TOPC,BORSUK_1,TSEP_1,TDLAT_3,TEX_2,TEX_4,CKB1,CKB3,CKB5,CKB6;
definitions T_0TOPSP,SUBSET_1,STRUCT_0,CKB1,CKB3,CKB5,CKB6;
theorems ZFMISC_1,SUBSET_1,PRE_TOPC,TOPS_1,TSEP_1,TDLAT_3,TEX_1,TEX_2,TEX_4,TARSKI,T_0TOPSP,STRUCT_0,XBOOLE_0,XBOOLE_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6;
registrations XBOOLE_0,STRUCT_0,TOPS_1,TDLAT_3,TEX_1,TEX_2,SUBSET_1,ZFMISC_1;
constructors REALSET2,BORSUK_1,TSEP_1,TDLAT_3,TEX_2,TEX_4,T_0TOPSP;
requirements SUBSET,BOOLE;
begin
registration
cluster  trivial ->  T_0 for non  empty non  empty TopStruct;
coherence
proof
let Y being non  empty TopStruct;
assume Y is  trivial;
then consider a being (Point of Y) such that A1: (the carrier of Y) = { a } by TEX_1:def 1;
now
let x being (Point of Y);
let y being (Point of Y);
assume A2: x <> y;
x = a by A1,TARSKI:def 1;
hence ((ex V being (Subset of Y) st ((V is  open & x in V) & (not y in V))) or (ex W being (Subset of Y) st ((W is  open & (not x in W)) & y in W))) by A1,A2,TARSKI:def 1;
end;
hence thesis by CKB5:def 1;
end;
end;
