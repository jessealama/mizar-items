begin
definition
let X being non  empty TopSpace;
redefine attr X is  T_0
means
:Def6: (for x,y being (Point of X) holds (x <> y implies ((not x in ( Cl { y } )) or (not y in ( Cl { x } )))));
compatibility
proof
thus (X is  T_0 implies (for x,y being (Point of X) holds (x <> y implies ((not x in ( Cl { y } )) or (not y in ( Cl { x } ))))))
proof
assume A1: X is  T_0;
hereby
let x being (Point of X);
let y being (Point of X);
assume A2: x <> y;
assume that
A3: x in ( Cl { y } )
and
A4: y in ( Cl { x } );
{ y } c= ( Cl { x } ) by A4,ZFMISC_1:31;
then A5: ( Cl { y } ) c= ( Cl { x } ) by TOPS_1:5;
{ x } c= ( Cl { y } ) by A3,ZFMISC_1:31;
then ( Cl { x } ) c= ( Cl { y } ) by TOPS_1:5;
then ( Cl { x } ) = ( Cl { y } ) by A5,XBOOLE_0:def 10;
hence contradiction by A1,A2,CKB17:def 1;
end;
end;

assume A6: (for x,y being (Point of X) holds (x <> y implies ((not x in ( Cl { y } )) or (not y in ( Cl { x } )))));
(for x,y being (Point of X) holds (x <> y implies ( Cl { x } ) <> ( Cl { y } )))
proof
let x being (Point of X);
let y being (Point of X);
assume A7: x <> y;
assume A8: ( Cl { x } ) = ( Cl { y } );
now
per cases  by A6,A7;
suppose (not x in ( Cl { y } ));

hence contradiction by A8,CKB16:1;
end;
suppose (not y in ( Cl { x } ));

hence contradiction by A8,CKB16:1;
end;
end;
hence contradiction;
end;
hence thesis by CKB17:def 1;
end;
