environ
vocabularies PRE_TOPC,STRUCT_0,TARSKI,SUBSET_1,RCOMP_1,XBOOLE_0,ZFMISC_1,TDLAT_3,NATTRA_1,SETFAM_1,TSP_1;
notations TARSKI,XBOOLE_0,SUBSET_1,DOMAIN_1,STRUCT_0,PRE_TOPC,BORSUK_1,TSEP_1,TDLAT_3,TEX_2,TEX_4,CKB1,CKB3,CKB5,CKB6,CKB17,CKB18,CKB19,CKB23,CKB24,CKB27,CKB28;
definitions T_0TOPSP,SUBSET_1,STRUCT_0,CKB1,CKB3,CKB5,CKB6,CKB17,CKB18,CKB19,CKB27,CKB28;
theorems ZFMISC_1,SUBSET_1,PRE_TOPC,TOPS_1,TSEP_1,TDLAT_3,TEX_1,TEX_2,TEX_4,TARSKI,T_0TOPSP,STRUCT_0,XBOOLE_0,XBOOLE_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB8,CKB16,CKB17,CKB18,CKB19,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34;
registrations XBOOLE_0,STRUCT_0,TOPS_1,TDLAT_3,TEX_1,TEX_2,SUBSET_1,ZFMISC_1,CKB7,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB20,CKB21,CKB22,CKB25,CKB26;
constructors REALSET2,BORSUK_1,TSEP_1,TDLAT_3,TEX_2,TEX_4,T_0TOPSP,CKB27;
requirements SUBSET,BOOLE;
begin
reserve Y for non  empty TopStruct;
theorem
Th11: (for A being (Subset of Y) holds (A is  discrete implies A is  T_0))
proof
let A being (Subset of Y);
assume A1: A is  discrete;
now
let x being (Point of Y);
let y being (Point of Y);
assume that
A2: x in A
and
A3: y in A;
{ x } c= A by A2,ZFMISC_1:31;
then consider G being (Subset of Y) such that A4: G is  open and A5: ( A /\ G ) = { x } by A1,TEX_2:def 3;
assume A6: x <> y;
now
take G;
thus G is  open by A4;
x in { x } by TARSKI:def 1;
hence x in G by A5,XBOOLE_0:def 4;
now
assume y in G;
then y in { x } by A3,A5,XBOOLE_0:def 4;
hence contradiction by A6,TARSKI:def 1;
end;
hence (not y in G);
end;
hence ((ex V being (Subset of Y) st ((V is  open & x in V) & (not y in V))) or (ex W being (Subset of Y) st ((W is  open & (not x in W)) & y in W)));
end;
hence thesis by CKB27:def 1;
end;
