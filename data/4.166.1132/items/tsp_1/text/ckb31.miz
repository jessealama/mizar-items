begin
reserve Y for non  empty TopStruct;
theorem
Th7: (for A,B being (Subset of Y) holds (B c= A implies (A is  T_0 implies B is  T_0)))
proof
let A being (Subset of Y);
let B being (Subset of Y);
assume A1: B c= A;
assume A is  T_0;
then (for x,y being (Point of Y) holds (((x in B & y in B) & x <> y) implies ((ex V being (Subset of Y) st ((V is  open & x in V) & (not y in V))) or (ex W being (Subset of Y) st ((W is  open & (not x in W)) & y in W))))) by A1,CKB27:def 1;
hence thesis by CKB27:def 1;
end;
