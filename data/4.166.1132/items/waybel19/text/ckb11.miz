theorem
Th6: (for T being  transitive  lower non  empty TopRelStr holds (for A being (Subset of T) holds (A is  closed implies A is  upper)))
proof
let T being  transitive  lower non  empty TopRelStr;
let A being (Subset of T);
assume ( ( [#] T ) \ A ) in (the topology of T);
then ( A ` ) is  open by PRE_TOPC:def 2;
then A1: ( A ` ) is  lower by CKB10:1;
let x being (Element of T);
let y being (Element of T);
assume that
A2: x in A
and
A3: x <= y
and
A4: (not y in A);
A5: y in ( A ` ) by A4,XBOOLE_0:def 5;
(not x in ( A ` )) by A2,XBOOLE_0:def 5;
hence thesis by A5,A1,A3,WAYBEL_0:def 19;
end;
