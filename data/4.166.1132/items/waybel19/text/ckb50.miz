begin
theorem
Lm2: (for T being LATTICE holds (for F being (Subset-Family of T) holds ((for A being (Subset of T) holds (A in F implies A is  property(S))) implies ( union F ) is  property(S))))
proof
let T being LATTICE;
let F being (Subset-Family of T);
assume that
A1: (for A being (Subset of T) holds (A in F implies A is  property(S)));
let D being non  empty  directed (Subset of T);
assume ( sup D ) in ( union F );
then consider A being set such that A2: ( sup D ) in A and A3: A in F by TARSKI:def 4;
reconsider A as (Subset of T) by A3;
A is  property(S) by A1,A3;
then consider y being (Element of T) such that A4: y in D and A5: (for x being (Element of T) holds ((x in D & x >= y) implies x in A)) by A2,WAYBEL11:def 3;
take y;
thus y in D by A4;
let x being (Element of T);
assume that
A6: x in D
and
A7: x >= y;
x in A by A6,A7,A5;
hence thesis by A3,TARSKI:def 4;
end;
