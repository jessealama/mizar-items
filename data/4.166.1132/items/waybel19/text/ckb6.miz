reconsider y = x as (Element of LL) by A1;
a = ( ( uparrow y ) ` ) by A1,A3,WAYBEL_0:13;
hence thesis;
end;

end;
theorem
Th2: (for L1,L2 being  TopSpace-like  lower non  empty TopRelStr holds (( the RelStr of L1 ) = ( the RelStr of L2 ) implies (the topology of L1) = (the topology of L2)))
proof
let L1 being  TopSpace-like  lower non  empty TopRelStr;
let L2 being  TopSpace-like  lower non  empty TopRelStr;
assume that
A1: ( the RelStr of L1 ) = ( the RelStr of L2 );
set B2 = { ( ( uparrow x ) ` ) where x is (Element of L2): (not contradiction) };
set B1 = { ( ( uparrow x ) ` ) where x is (Element of L1): (not contradiction) };
A2: B1 is (prebasis of L1) by CKB1:def 1;
A3: B2 is (prebasis of L2) by CKB1:def 1;
B1 = B2 by A1,Lm1;
hence thesis by A1,A2,A3,YELLOW_9:26;
end;
