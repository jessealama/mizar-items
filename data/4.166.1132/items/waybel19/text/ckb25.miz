begin
theorem
(for T,T2 being  lower  complete TopLattice holds (T2 is (TopAugmentation of [: T,(T qua LATTICE) :]) implies (for f being (Function of T2,T) holds (f = ( inf_op T ) implies f is  continuous))))
proof
let T being  lower  complete TopLattice;
let T2 being  lower  complete TopLattice;
assume that
A1: ( the RelStr of T2 ) = ( the RelStr of [: T,T :] );
let f being (Function of T2,T);
assume that
A2: f = ( inf_op T );
f is  infs-preserving
proof
let X being (Subset of T2);
reconsider Y = X as (Subset of [: T,T :]) by A1;
assume A3:  ex_inf_of X,T2;
thus  ex_inf_of ( f .: X ),T by YELLOW_0:17;
A4: ( inf_op T ) preserves_inf_of Y by WAYBEL_0:def 32;
 ex_inf_of Y,[: T,T :] by A3,A1,YELLOW_0:14;
hence ( inf ( f .: X ) ) = ( ( inf_op T ) . ( inf Y ) ) by A2,A4,WAYBEL_0:def 30
.= ( f . ( inf X ) ) by A1,A2,A3,YELLOW_0:27;
end;
hence thesis by CKB14:1;
end;
