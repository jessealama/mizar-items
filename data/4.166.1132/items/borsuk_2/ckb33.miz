begin
theorem
Th11: (for S1,S2,T1,T2 being non empty TopSpace holds (for f being  continuous (Function of S1,T1) holds (for g being  continuous (Function of S2,T2) holds (for P2 being (Subset of [: T1,T2 :]) holds (P2 is  open implies ( [: f,g :] " P2 ) is  open)))))
proof
let S1 being non empty TopSpace,S2 being non empty TopSpace,T1 being non empty TopSpace,T2 being non empty TopSpace;
let f being  continuous (Function of S1,T1);
let g being  continuous (Function of S2,T2);
let P2 being (Subset of [: T1,T2 :]);
reconsider Kill = ( [: f,g :] " ( Base-Appr P2 ) ) as (Subset-Family of [: S1,S2 :]);
(for P being (Subset of [: S1,S2 :]) holds (P in Kill implies P is  open))
proof
let P being (Subset of [: S1,S2 :]);
assume P in Kill;
then (ex B being (Subset of [: T1,T2 :]) st (B in ( Base-Appr P2 ) & P = ( [: f,g :] " B ))) by FUNCT_2:def 9;
hence thesis by CKB32:1;
end;
then A1: Kill is  open by TOPS_2:def 1;
assume P2 is  open;
then P2 = ( union ( Base-Appr P2 ) ) by BORSUK_1:13;
then ( [: f,g :] " (P2 qua (Subset of [: T1,T2 :])) ) = ( union ( [: f,g :] " ( Base-Appr P2 ) ) ) by CKB30:1;
hence thesis by A1,TOPS_2:19;
end;
