begin
reserve T for non  empty TopSpace;
reserve S for non  empty TopSpace;
theorem
(for S,T being non  empty TopSpace holds (for f being (Function of S,T) holds (f is  being_homeomorphism implies ( f " ) is  open)))
proof
let S being non  empty TopSpace;
let T being non  empty TopSpace;
let f being (Function of S,T);
assume f is  being_homeomorphism;
then A1: (( rng f ) = ( [#] T ) & f is  one-to-one  continuous) by TOPS_2:def 5;
let P being (Subset of T);
( f " P ) = ( ( f " ) .: P ) by A1,TOPS_2:55;
hence thesis by A1,TOPS_2:43;
end;
