environ
vocabularies NUMBERS,XBOOLE_0,PRE_TOPC,XREAL_0,ORDINAL1,CARD_1,XXREAL_0,STRUCT_0,BORSUK_1,XXREAL_1,REAL_1,SUBSET_1,TARSKI,FUNCT_1,RELAT_1,RCOMP_1,ORDINAL2,FUNCT_4,TOPS_2,FUNCOP_1,GRAPH_1,RELAT_2,ARYTM_3,ARYTM_1,TOPMETR,TREAL_1,VALUED_1,SETFAM_1,ZFMISC_1,PCOMPS_1,MCART_1,CONNSP_2,TOPS_1,BORSUK_2;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,SETFAM_1,ORDINAL1,CARD_1,NUMBERS,XXREAL_0,XREAL_0,REAL_1,RELAT_1,FUNCT_1,RELSET_1,PARTFUN1,FUNCT_2,DOMAIN_1,STRUCT_0,PRE_TOPC,TOPS_2,MCART_1,RCOMP_1,PCOMPS_1,TOPS_1,COMPTS_1,CONNSP_1,CONNSP_2,TREAL_1,FUNCT_4,BORSUK_1,T_0TOPSP,TOPMETR,BINOP_1,FUNCT_3,CKB8,CKB9,CKB10,CKB12,CKB14,CKB20,CKB25,CKB31;
definitions TARSKI,PRE_TOPC,COMPTS_1,URYSOHN1,T_0TOPSP,XBOOLE_0,BINOP_1,STRUCT_0,BORSUK_1,CKB8,CKB10,CKB12,CKB14,CKB20,CKB25;
theorems BORSUK_1,FUNCOP_1,TOPS_2,TREAL_1,FUNCT_2,FUNCT_1,PRE_TOPC,RCOMP_1,TARSKI,RELAT_1,TOPS_1,URYSOHN1,TOPMETR,FUNCT_4,HEINE,PCOMPS_1,MCART_1,ZFMISC_1,CONNSP_2,FUNCT_3,COMPTS_1,T_0TOPSP,CARD_1,RELSET_1,XBOOLE_0,XBOOLE_1,XREAL_0,XREAL_1,CONNSP_1,XXREAL_1,XXREAL_2,CKB1,CKB3,CKB6,CKB7,CKB8,CKB10,CKB12,CKB14,CKB16,CKB17,CKB19,CKB20,CKB22,CKB23,CKB25,CKB26,CKB27,CKB28,CKB30,CKB32,CKB33;
schemes FUNCT_2,FUNCT_1,CKB2;
registrations XBOOLE_0,SUBSET_1,FUNCT_1,FUNCT_2,XREAL_0,MEMBERED,STRUCT_0,PRE_TOPC,COMPTS_1,METRIC_1,BORSUK_1,RELAT_1,XXREAL_2,TOPS_1,CONNSP_1,TOPMETR,RELSET_1,CKB4,CKB5,CKB11,CKB13,CKB15,CKB18,CKB21,CKB24,CKB29,CKB34,CKB35;
constructors SETFAM_1,FUNCT_3,FUNCT_4,REAL_1,MEMBERED,RCOMP_1,TOPS_1,CONNSP_1,TOPS_2,COMPTS_1,URYSOHN1,T_0TOPSP,TREAL_1,FUNCOP_1,PCOMPS_1,XXREAL_2,COMPLEX1,CKB8,CKB9,CKB10,CKB12,CKB20,CKB25,CKB31;
requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve T1 for non  empty TopSpace;
reserve T2 for non  empty TopSpace;
registration
let T1 being  T_0 non  empty TopSpace;
let T2 being  T_0 non  empty TopSpace;
cluster [: T1,T2 :] ->  T_0;
coherence
proof
set T = [: T1,T2 :];
now
let p being (Point of T);
let q being (Point of T);
assume A1: p <> q;
q in (the carrier of T);
then q in [: (the carrier of T1),(the carrier of T2) :] by BORSUK_1:def 2;
then consider z,v being set such that A2: z in (the carrier of T1) and A3: v in (the carrier of T2) and A4: q = [ z,v ] by ZFMISC_1:def 2;
p in (the carrier of T);
then p in [: (the carrier of T1),(the carrier of T2) :] by BORSUK_1:def 2;
then consider x,y being set such that A5: x in (the carrier of T1) and A6: y in (the carrier of T2) and A7: p = [ x,y ] by ZFMISC_1:def 2;
reconsider y,v as (Point of T2) by A6,A3;
reconsider x,z as (Point of T1) by A5,A2;
per cases ;
suppose x <> z;

then consider V1 being (Subset of T1) such that A8: V1 is  open and A9: ((x in V1 & (not z in V1)) or (z in V1 & (not x in V1))) by T_0TOPSP:def 7;
set X = [: V1,( [#] T2 ) :];
A10:now
per cases  by A9;
suppose (x in V1 & (not z in V1));

hence ((p in X & (not q in X)) or (q in X & (not p in X))) by A6,A7,A4,ZFMISC_1:87;
end;
suppose (z in V1 & (not x in V1));

hence ((p in X & (not q in X)) or (q in X & (not p in X))) by A7,A3,A4,ZFMISC_1:87;
end;
end;
X is  open by A8,BORSUK_1:6;
hence (ex X being (Subset of T) st (X is  open & ((p in X & (not q in X)) or (q in X & (not p in X))))) by A10;
end;
suppose x = z;

then consider V1 being (Subset of T2) such that A11: V1 is  open and A12: ((y in V1 & (not v in V1)) or (v in V1 & (not y in V1))) by A1,A7,A4,T_0TOPSP:def 7;
set X = [: ( [#] T1 ),V1 :];
A13:now
per cases  by A12;
suppose (y in V1 & (not v in V1));

hence ((p in X & (not q in X)) or (q in X & (not p in X))) by A5,A7,A4,ZFMISC_1:87;
end;
suppose (v in V1 & (not y in V1));

hence ((p in X & (not q in X)) or (q in X & (not p in X))) by A7,A2,A4,ZFMISC_1:87;
end;
end;
X is  open by A11,BORSUK_1:6;
hence (ex X being (Subset of T) st (X is  open & ((p in X & (not q in X)) or (q in X & (not p in X))))) by A13;
end;
end;
hence thesis by T_0TOPSP:def 7;
end;
end;
