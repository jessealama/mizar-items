begin
reserve T for non  empty TopSpace;
theorem
(for T being non  empty TopSpace holds (for a,b,c being (Point of T) holds (for G1 being (Path of a,b) holds (for G2 being (Path of b,c) holds ((((((G1 is  continuous & G2 is  continuous) & ( G1 . ( 0 ) ) = a) & ( G1 . 1 ) = b) & ( G2 . ( 0 ) ) = b) & ( G2 . 1 ) = c) implies ((( G1 + G2 ) is  continuous & ( ( G1 + G2 ) . ( 0 ) ) = a) & ( ( G1 + G2 ) . 1 ) = c))))))
proof
let T being non  empty TopSpace;
let a being (Point of T);
let b being (Point of T);
let c being (Point of T);
let G1 being (Path of a,b);
let G2 being (Path of b,c);
assume (((((G1 is  continuous & G2 is  continuous) & ( G1 . ( 0 ) ) = a) & ( G1 . 1 ) = b) & ( G2 . ( 0 ) ) = b) & ( G2 . 1 ) = c);
then (ex h being (Function of ( I[01] ),T) st (((h is  continuous & ( h . ( 0 ) ) = a) & ( h . 1 ) = c) & ( rng h ) c= ( ( rng G1 ) \/ ( rng G2 ) ))) by CKB19:1;
then a,c are_connected  by CKB8:def 1;
hence thesis by CKB10:def 1;
end;
