environ
vocabularies RELAT_1,SUBSET_1,ZFMISC_1,TARSKI,XBOOLE_0;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,RELAT_1,CKB1,CKB4,CKB5;
definitions TARSKI,XBOOLE_0,RELAT_1,CKB4,CKB5;
theorems TARSKI,ZFMISC_1,RELAT_1,XBOOLE_0,XBOOLE_1,CKB4,CKB5,CKB6,CKB7;
schemes RELAT_1;
registrations XBOOLE_0,RELAT_1,SUBSET_1,CKB2,CKB3;
constructors TARSKI,SUBSET_1,RELAT_1,CKB4,CKB5;
requirements SUBSET,BOOLE;
begin
reserve X for set;
reserve Y for set;
reserve x for set;
reserve y for set;
theorem
((x in X & y in Y) implies { [ x,y ] } is (Relation of X,Y))
proof
assume (x in X & y in Y);
then [ x,y ] in [: X,Y :] by ZFMISC_1:87;
hence thesis by ZFMISC_1:31;
end;
