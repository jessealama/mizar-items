environ
vocabularies RELAT_1,SUBSET_1,ZFMISC_1,TARSKI,XBOOLE_0;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,RELAT_1,CKB1,CKB4,CKB5,CKB19,CKB20;
definitions TARSKI,XBOOLE_0,RELAT_1,CKB4,CKB5;
theorems TARSKI,ZFMISC_1,RELAT_1,XBOOLE_0,XBOOLE_1,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB21;
schemes RELAT_1;
registrations XBOOLE_0,RELAT_1,SUBSET_1,CKB2,CKB3,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18;
constructors TARSKI,SUBSET_1,RELAT_1,CKB4,CKB5,CKB19,CKB20;
requirements SUBSET,BOOLE;
begin
reserve X for set;
reserve Y for set;
reserve x for set;
reserve y for set;
reserve R for (Relation of X,Y);
theorem
((for x holds (x in X implies (ex y st [ x,y ] in R))) iff ( dom R ) = X)
proof
thus ((for x holds (x in X implies (ex y st [ x,y ] in R))) implies ( dom R ) = X)
proof
assume A1: (for x holds (x in X implies (ex y st [ x,y ] in R)));
now
let x;
now
assume x in X;
then (ex y st [ x,y ] in R) by A1;
hence x in ( dom R ) by RELAT_1:6;
end;
hence (x in ( dom R ) iff x in X);
end;
hence ( dom R ) = X by TARSKI:1;
end;

thus thesis by RELAT_1:def 4;
end;
