environ
vocabularies RELAT_1,SUBSET_1,ZFMISC_1,TARSKI,XBOOLE_0;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,RELAT_1,CKB1;
definitions TARSKI,XBOOLE_0,RELAT_1;
theorems TARSKI,ZFMISC_1,RELAT_1,XBOOLE_0,XBOOLE_1;
schemes RELAT_1;
registrations XBOOLE_0,RELAT_1,SUBSET_1,CKB2;
constructors TARSKI,SUBSET_1,RELAT_1;
requirements SUBSET,BOOLE;
begin
reserve X for set;
reserve Y for set;
reserve x for set;
reserve y for set;
registration
let X;
let Y;
cluster  -> X -defined Y -valued for (Relation of X,Y);
coherence
proof
let R being (Relation of X,Y);
thus ( dom R ) c= X
proof
let x;
assume x in ( dom R );
then (ex y st [ x,y ] in R) by RELAT_1:def 4;
hence thesis by ZFMISC_1:87;
end;

let y;
assume y in ( rng R );
then (ex x st [ x,y ] in R) by RELAT_1:def 5;
hence thesis by ZFMISC_1:87;
end;
end;
