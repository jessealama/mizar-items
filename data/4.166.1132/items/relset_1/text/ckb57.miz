begin
theorem
(for N being set holds (for R,S being (Relation of N) holds ((for i being set holds (i in N implies ( Im (R,i) ) = ( Im (S,i) ))) implies R = S)))
proof
let N being set;
let R being (Relation of N),S being (Relation of N);
assume that
A1: (for i being set holds (i in N implies ( Im (R,i) ) = ( Im (S,i) )));
let a being (Element of N),b being (Element of N);
thus ([ a,b ] in R implies [ a,b ] in S)
proof
assume A2: [ a,b ] in R;
then A3: a in ( dom R ) by RELAT_1:def 4;
a in { a } by TARSKI:def 1;
then b in ( Im (R,a) ) by A2,RELAT_1:def 13;
then b in ( Im (S,a) ) by A1,A3;
then (ex e being set st ([ e,b ] in S & e in { a })) by RELAT_1:def 13;
hence thesis by TARSKI:def 1;
end;

assume A4: [ a,b ] in S;
then A5: a in ( dom S ) by RELAT_1:def 4;
a in { a } by TARSKI:def 1;
then b in ( Im (S,a) ) by A4,RELAT_1:def 13;
then b in ( Im (R,a) ) by A1,A5;
then (ex e being set st ([ e,b ] in R & e in { a })) by RELAT_1:def 13;
hence thesis by TARSKI:def 1;
end;
