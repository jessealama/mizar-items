environ
vocabularies RELAT_1,SUBSET_1,ZFMISC_1,TARSKI,XBOOLE_0;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,RELAT_1,CKB1,CKB4,CKB5,CKB19,CKB20;
definitions TARSKI,XBOOLE_0,RELAT_1,CKB4,CKB5;
theorems TARSKI,ZFMISC_1,RELAT_1,XBOOLE_0,XBOOLE_1,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB21,CKB22;
schemes RELAT_1;
registrations XBOOLE_0,RELAT_1,SUBSET_1,CKB2,CKB3,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18;
constructors TARSKI,SUBSET_1,RELAT_1,CKB4,CKB5,CKB19,CKB20;
requirements SUBSET,BOOLE;
begin
reserve X for set;
reserve Y for set;
reserve x for set;
reserve y for set;
reserve R for (Relation of X,Y);
theorem
((for y holds (y in Y implies (ex x st [ x,y ] in R))) iff ( rng R ) = Y)
proof
thus ((for y holds (y in Y implies (ex x st [ x,y ] in R))) implies ( rng R ) = Y)
proof
assume A1: (for y holds (y in Y implies (ex x st [ x,y ] in R)));
now
let y;
now
assume y in Y;
then (ex x st [ x,y ] in R) by A1;
hence y in ( rng R ) by RELAT_1:6;
end;
hence (y in ( rng R ) iff y in Y);
end;
hence ( rng R ) = Y by TARSKI:1;
end;

thus thesis by RELAT_1:def 5;
end;
