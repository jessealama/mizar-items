begin
scheme  { A,B() -> set,P[set,set],P,R() -> (Relation of A(),B()) } : P() = R()
provided
A1: (for p being (Element of A()) holds (for q being (Element of B()) holds ([ p,q ] in P() iff P[ p,q ])))
and
A2: (for p being (Element of A()) holds (for q being (Element of B()) holds ([ p,q ] in R() iff P[ p,q ])))
proof
let y being (Element of A());
let z being (Element of B());
([ y,z ] in P() iff P[ y,z ]) by A1;
hence thesis by A2;
end;
