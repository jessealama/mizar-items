constructors FUNCT_4,REAL_1,SQUARE_1,BINOP_2,COMPLEX1,RCOMP_1,TOPS_2,COMPTS_1,TBSP_1,MONOID_0,TOPMETR,PSCOMP_1,JORDAN2C,SEQ_1,FUNCSDOM,PCOMPS_1,RELSET_1;
requirements REAL,NUMERALS,BOOLE,SUBSET,ARITHM;
begin
reserve a for  real number;
reserve p for (Point of ( TOP-REAL 2 ));
theorem
Th6: (for X being non  empty TopStruct holds (for g being (Function of X,( R^1 )) holds (for B being (Subset of X) holds (for a being  real number holds ((g is  continuous & B = { p where p is (Point of X): ( g /. p ) > a }) implies B is  open)))))
proof
let X being non  empty TopStruct;
let g being (Function of X,( R^1 ));
let B being (Subset of X);
let a being  real number;
assume that
A1: g is  continuous
and
A2: B = { p where p is (Point of X): ( g /. p ) > a };
{ r where r is Real: r > a } c= (the carrier of ( R^1 ))
proof
let x being set;
assume x in { r where r is Real: r > a };
then (ex r being Real st (r = x & r > a));
hence thesis by TOPMETR:17;
end;
then reconsider D = { r where r is Real: r > a } as (Subset of ( R^1 ));
A3: ( g " D ) c= B
proof
let x being set;
assume A4: x in ( g " D );
then reconsider p = x as (Point of X);
( g . x ) in D by A4,FUNCT_1:def 7;
then A5: (ex r being Real st (r = ( g . x ) & r > a));
( g /. p ) = ( g . p );
hence thesis by A2,A5;
end;
A6: (( [#] ( R^1 ) ) <> ( {} ) & D is  open) by JORDAN2B:25;
B c= ( g " D )
proof
let x being set;
assume x in B;
then consider p being (Point of X) such that A7: p = x and A8: ( g /. p ) > a by A2;
( g /. p ) is Real by XREAL_0:def 1;
then (( dom g ) = (the carrier of X) & ( g . x ) in D) by A7,A8,FUNCT_2:def 1;
hence thesis by A7,FUNCT_1:def 7;
end;
then B = ( g " D ) by A3,XBOOLE_0:def 10;
hence thesis by A1,A6,TOPS_2:43;
end;
