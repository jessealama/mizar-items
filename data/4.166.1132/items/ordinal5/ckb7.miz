environ
vocabularies NUMBERS,ORDINAL1,ORDINAL2,TARSKI,MATROID0,XBOOLE_0,CARD_1,FINSET_1,AFINSQ_1,ORDINAL4,RELAT_1,FUNCT_1,VALUED_0,CARD_3,SUBSET_1,ARYTM_3,NAT_1,XXREAL_0,NEWTON,ORDINAL3,FINSEQ_1,ORDINAL5;
notations XBOOLE_0,TARSKI,RELAT_1,FUNCT_1,SUBSET_1,FINSET_1,NUMBERS,MATROID0,ORDINAL1,NAT_1,XXREAL_0,NEWTON,ORDINAL2,AFINSQ_1,ORDINAL3,ORDINAL4,CARD_3;
definitions TARSKI,XBOOLE_0,ORDINAL2,CARD_3;
theorems TARSKI,FUNCT_1,FINSET_1,NAT_1,ORDERS_1,AFINSQ_1,RELAT_1,ORDINAL1,ORDINAL2,ORDINAL3,ORDINAL4,CARD_2,CARD_3,CARD_5,XBOOLE_0,XBOOLE_1,ZFMISC_1,NEWTON,XXREAL_0,NAT_4,PEPIN,CARD_1,CKB1;
schemes NAT_1,ORDINAL1,ORDINAL2,AFINSQ_1;
registrations XBOOLE_0,RELAT_1,FUNCT_1,ORDINAL1,NAT_1,XXREAL_0,XREAL_0,ORDINAL2,ORDINAL4,CARD_1,CARD_5,CARD_LAR,NEWTON,AFINSQ_1,FINSET_1,CKB2,CKB3,CKB4,CKB5,CKB6;
constructors NEWTON,NAT_1,XXREAL_0,AFINSQ_1,ORDINAL3,CARD_3,ORDINAL4;
requirements SUBSET,BOOLE,NUMERALS,ARITHM,REAL;
begin
reserve a for  ordinal number;
reserve b for  ordinal number;
reserve c for  ordinal number;
reserve S for T-Sequence;
reserve S1 for T-Sequence;
reserve S2 for T-Sequence;
theorem
Th2: (( dom S ) = ( a +^ b ) implies (ex S1 st (ex S2 st ((S = ( S1 ^ S2 ) & ( dom S1 ) = a) & ( dom S2 ) = b))))
proof
assume A1: ( dom S ) = ( a +^ b );
set S1 = ( S | a );
a c= ( a +^ b ) by ORDINAL3:24;
then A2: ( dom S1 ) = a by A1,RELAT_1:62;
deffunc F(Ordinal) = ( S . ( a +^ $1 ) );
consider S2 such that A3: (( dom S2 ) = b & (for c holds (c in b implies ( S2 . c ) = F(c)))) from ORDINAL2:sch 2;
take S1;
take S2;
set s = ( S1 ^ S2 );
A4: ( dom S ) = ( dom s ) by A1,A2,A3,ORDINAL4:def 1;
now
let x being set;
assume A5: x in ( dom S );
then reconsider z = x as Ordinal;
per cases  by A1,A5,ORDINAL3:38;
suppose A6: z in a;

hence ( S . x ) = ( S1 . z ) by FUNCT_1:49
.= ( s . x ) by A6,A2,ORDINAL4:def 1;
end;
suppose (ex c st (c in b & z = ( a +^ c )));

then consider c such that A7: (c in b & z = ( a +^ c ));
thus ( S . x ) = ( S2 . c ) by A3,A7
.= ( s . x ) by A7,A2,A3,ORDINAL4:def 1;
end;
end;
hence thesis by A2,A3,A4,FUNCT_1:2;
end;
