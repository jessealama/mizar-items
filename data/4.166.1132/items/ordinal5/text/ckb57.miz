begin
reserve a for  ordinal number;
reserve b for  ordinal number;
reserve c for  ordinal number;
definition
let a being Ordinal;
func first_epsilon_greater_than a ->  epsilon Ordinal means 
:Def6: (a in it & (for b being  epsilon Ordinal holds (a in b implies it c= b)));
existence
proof
defpred E[ Ordinal ]
 means
(a in $1 & $1 is  epsilon);
A1: (ex c st E[ c ]) by CKB55:1;
consider c such that A2: (E[ c ] & (for b holds (E[ b ] implies c c= b))) from ORDINAL1:sch 1(A1);
reconsider c as  epsilon Ordinal by A2;
take c;
thus thesis by A2;
end;
uniqueness
proof
let b1 being  epsilon Ordinal;
let b2 being  epsilon Ordinal;
assume that
A3: (a in b1 & (for b being  epsilon Ordinal holds (a in b implies b1 c= b)))
and
A4: (a in b2 & (for b being  epsilon Ordinal holds (a in b implies b2 c= b)));
thus (b1 c= b2 & b2 c= b1) by A3,A4;
end;
end;
