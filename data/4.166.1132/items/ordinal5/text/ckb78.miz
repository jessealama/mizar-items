theorem
(a is  epsilon implies (ex b st a = ( epsilon_ b )))
proof
defpred N[ set ]
 means
(ex b st $1 = ( epsilon_ b ));
defpred Q[ Ordinal ]
 means
(for e being  epsilon Ordinal holds (e in ( epsilon_ $1 ) implies N[ e ]));
A1: Q[ ( {} ) ]
proof
let e being  epsilon Ordinal;
( 0 ) in e by ORDINAL3:8;
then ( first_epsilon_greater_than ( 0 ) ) c= e by CKB57:def 1;
then (e in ( epsilon_ ( 0 ) ) implies e in e) by CKB60:1,CKB67:1;
hence thesis;
end;
A2: (Q[ c ] implies Q[ ( succ c ) ])
proof
assume A3: Q[ c ];
let e being  epsilon Ordinal;
assume that
A4: e in ( epsilon_ ( succ c ) );
A5: ( epsilon_ ( succ c ) ) = ( ( epsilon_ c ) |^|^ ( omega ) ) by CKB68:1
.= ( first_epsilon_greater_than ( epsilon_ c ) ) by CKB65:1;
per cases  by ORDINAL1:14;
suppose e in ( epsilon_ c );

hence N[ e ] by A3;
end;
suppose e = ( epsilon_ c );

hence N[ e ];
end;
suppose ( epsilon_ c ) in e;

then ( epsilon_ ( succ c ) ) c= e by A5,CKB57:def 1;
then e in e by A4;
hence N[ e ];
end;
end;
A6: (((b <> ( {} ) & b is  limit_ordinal) & (for c holds (c in b implies Q[ c ]))) implies Q[ b ])
proof
assume that
A7: (b <> ( {} ) & b is  limit_ordinal)
and
A8: (for c holds (c in b implies Q[ c ]));
let e being  epsilon Ordinal;
assume e in ( epsilon_ b );
then (ex c st (c in b & e in ( epsilon_ c ))) by A7,CKB73:1;
hence N[ e ] by A8;
end;
A9: Q[ b ] from ORDINAL2:sch 1(A1,A2,A6);
(a c= ( epsilon_ a ) & ( epsilon_ a ) in ( epsilon_ ( succ a ) )) by CKB70:1,CKB74:1,ORDINAL1:6;
hence thesis by A9,ORDINAL1:12;
end;
