begin
theorem
(for V being RealLinearSpace holds (for M1,M2,M3 being (Subset of V) holds (for r1,r2,r3 being Real holds (((M1 is  circled & M2 is  circled) & M3 is  circled) implies ( ( ( r1 * M1 ) + ( r2 * M2 ) ) + ( r3 * M3 ) ) is  circled))))
proof
let V being RealLinearSpace;
let M1 being (Subset of V),M2 being (Subset of V),M3 being (Subset of V);
let r1 being Real,r2 being Real,r3 being Real;
assume that
A1: (M1 is  circled & M2 is  circled)
and
A2: M3 is  circled;
( ( r1 * M1 ) + ( r2 * M2 ) ) is  circled by A1,CKB6:1;
then ( ( 1 * ( ( r1 * M1 ) + ( r2 * M2 ) ) ) + ( r3 * M3 ) ) is  circled by A2,CKB6:1;
hence thesis by CONVEX1:32;
end;
