begin
theorem
(for V being RealLinearSpace holds (for M1,M2 being (Subset of V) holds (M1 c= M2 implies ( Cir M1 ) c= ( Cir M2 ))))
proof
let V being RealLinearSpace;
let M1 being (Subset of V);
let M2 being (Subset of V);
assume M1 c= M2;
then ( Circled-Family M2 ) c= ( Circled-Family M1 ) by CKB15:1;
then A1: ( meet ( Circled-Family M1 ) ) c= ( meet ( Circled-Family M2 ) ) by SETFAM_1:6;
let x being set;
assume x in ( Cir M1 );
hence thesis by A1;
end;
