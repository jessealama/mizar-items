begin
definition
let V being RealLinearSpace;
let M being (Subset of V);
func Cir M ->  circled (Subset of V)
equals
( meet ( Circled-Family M ) );
coherence
proof
(for N being (Subset of V) holds (N in ( Circled-Family M ) implies N is  circled)) by CKB12:def 1;
then ( Circled-Family M ) is  circled-membered by RLTOPSP1:def 7;
hence thesis by RLTOPSP1:30;
end;
