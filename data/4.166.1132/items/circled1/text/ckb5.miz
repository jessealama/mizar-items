begin
theorem
Th2: (for V being RealLinearSpace holds (for M being (Subset of V) holds (for r being Real holds (M is  circled implies ( r * M ) is  circled))))
proof
let V being RealLinearSpace;
let M being (Subset of V);
let r being Real;
assume A1: M is  circled;
(for u being (VECTOR of V) holds (for p being Real holds ((( abs p ) <= 1 & u in ( r * M )) implies ( p * u ) in ( r * M ))))
proof
let u being (VECTOR of V);
let p being Real;
assume that
A2: ( abs p ) <= 1
and
A3: u in ( r * M );
u in { ( r * w ) where w is (Element of V): w in M } by A3,CONVEX1:def 1;
then consider u9 being (Element of V) such that A4: u = ( r * u9 ) and A5: u9 in M;
A6: ( p * u ) = ( ( r * p ) * u9 ) by A4,RLVECT_1:def 7
.= ( r * ( p * u9 ) ) by RLVECT_1:def 7;
( p * u9 ) in M by A1,A2,A5,CKB4:def 1;
hence thesis by A6,RLTOPSP1:1;
end;
hence thesis by CKB4:def 1;
end;
