constructors SETFAM_1,DOMAIN_1,XXREAL_0,REAL_1,BINOP_2,COMPLEX1,FINSOP_1,RVSUM_1,RUSUB_5,CONVEX1,RLTOPSP1,RELSET_1,CKB12,CKB13,CKB22,CKB30;
requirements BOOLE,REAL,NUMERALS,SUBSET,ARITHM;
begin
definition
let V being RealLinearSpace;
let M being non  empty (Subset of V);
func circledComb M means 
(for L being set holds (L in it iff L is (circled_Combination of M)));
existence
proof
defpred P[ set ]
 means
$1 is (circled_Combination of M);
consider A being set such that A1: (for x being set holds (x in A iff (x in ( Funcs ((the carrier of V),( REAL )) ) & P[ x ]))) from XBOOLE_0:sch 1;
take A;
let L being set;
thus (L in A implies L is (circled_Combination of M)) by A1;
assume L is (circled_Combination of M);
hence thesis by A1;
end;
uniqueness
proof
defpred P[ set ]
 means
$1 is (circled_Combination of M);
thus (for X1,X2 being set holds (((for x being set holds (x in X1 iff P[ x ])) & (for x being set holds (x in X2 iff P[ x ]))) implies X1 = X2)) from XBOOLE_0:sch 3;
end;
end;
