begin
reserve Y for TopStruct;
theorem
Th13: (for Y0 being (SubSpace of Y) holds (for A being (Subset of Y) holds (A = (the carrier of Y0) implies (A is  proper iff Y0 is  proper))))
proof
let Y0 being (SubSpace of Y);
let A being (Subset of Y);
assume A1: A = (the carrier of Y0);
hereby
assume A is  proper;
then (for A being (Subset of Y) holds (A = (the carrier of Y0) implies A is  proper)) by A1;
hence Y0 is  proper by CKB23:def 1;
end;
thus thesis by A1,CKB23:def 1;
end;
