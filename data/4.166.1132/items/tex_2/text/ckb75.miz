environ
vocabularies XBOOLE_0,ZFMISC_1,SUBSET_1,TARSKI,STRUCT_0,PRE_TOPC,RELAT_1,NATTRA_1,TDLAT_3,RCOMP_1,TOPS_3,TOPS_1,SETFAM_1,FUNCT_1,ORDINAL2,COMPTS_1,TEX_1,BORSUK_1,TEX_2,CARD_1;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,SETFAM_1,DOMAIN_1,RELAT_1,FUNCT_1,RELSET_1,PARTFUN1,FUNCT_2,CARD_1,STRUCT_0,PRE_TOPC,TOPS_1,TOPS_2,BORSUK_1,TSEP_1,TDLAT_3,TOPS_3,COMPTS_1,TEX_1,CKB23,CKB47,CKB74;
definitions PRE_TOPC,XBOOLE_0,COMPTS_1,SUBSET_1,STRUCT_0,CKB23,CKB47,CKB74;
theorems TARSKI,SUBSET_1,ZFMISC_1,FUNCT_2,PRE_TOPC,TOPS_1,TOPS_2,BORSUK_1,TSEP_1,TDLAT_3,TOPS_3,TEX_1,RELAT_1,RELSET_1,XBOOLE_0,XBOOLE_1,STRUCT_0,CKB1,CKB2,CKB5,CKB6,CKB12,CKB13,CKB22,CKB23,CKB24,CKB25,CKB26,CKB32,CKB37,CKB38,CKB43,CKB47,CKB50,CKB51,CKB54,CKB55,CKB74;
