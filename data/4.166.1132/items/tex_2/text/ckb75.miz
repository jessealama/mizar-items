registrations XBOOLE_0,SUBSET_1,RELAT_1,FUNCT_1,FUNCT_2,ZFMISC_1,STRUCT_0,PRE_TOPC,TOPS_1,COMPTS_1,BORSUK_1,TDLAT_3,TEX_1,CARD_1,ORDINAL1,CKB3,CKB4,CKB7,CKB8,CKB9,CKB10,CKB11,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB27,CKB28,CKB29,CKB30,CKB31,CKB33,CKB34,CKB35,CKB36,CKB39,CKB40,CKB41,CKB42,CKB44,CKB45,CKB46,CKB48,CKB49,CKB52,CKB53,CKB56,CKB57,CKB58,CKB59,CKB60,CKB61,CKB62,CKB63,CKB64,CKB65,CKB66,CKB67,CKB68,CKB69,CKB70,CKB71,CKB72,CKB73;
constructors SETFAM_1,TOPS_1,TOPS_2,COMPTS_1,REALSET2,BORSUK_1,TSEP_1,TDLAT_3,TOPS_3,TEX_1,CKB23,CKB47,CKB74;
requirements BOOLE,SUBSET,NUMERALS;
begin
reserve Y for TopStruct;
definition
let Y being TopStruct;
let A being (Subset of Y);
redefine attr A is  discrete
means
:Def6: (for D being (Subset of Y) holds (D c= A implies (ex F being (Subset of Y) st (F is  closed & ( A /\ F ) = D))));
compatibility
proof
hereby
assume A1: A is  discrete;
let D being (Subset of Y);
( A \ D ) c= A by XBOOLE_1:36;
then consider G being (Subset of Y) such that A2: G is  open and A3: ( A /\ G ) = ( A \ D ) by A1,CKB74:def 1;
assume A4: D c= A;
now
take F = ( ( [#] Y ) \ G );
G = ( ( [#] Y ) \ F ) by PRE_TOPC:3;
hence F is  closed by A2,PRE_TOPC:def 3;
( A /\ ( [#] Y ) ) = A by XBOOLE_1:28;
then ( A /\ F ) = ( A \ G ) by XBOOLE_1:49;
then ( A /\ F ) = ( A \ ( A \ D ) ) by A3,XBOOLE_1:47;
then ( A /\ F ) = ( A /\ D ) by XBOOLE_1:48;
hence ( A /\ F ) = D by A4,XBOOLE_1:28;
end;
hence (ex F being (Subset of Y) st (F is  closed & ( A /\ F ) = D));
end;
hereby
assume A5: (for D being (Subset of Y) holds (D c= A implies (ex F being (Subset of Y) st (F is  closed & ( A /\ F ) = D))));
now
let D being (Subset of Y);
consider F being (Subset of Y) such that A6: F is  closed and A7: ( A /\ F ) = ( A \ D ) by A5,XBOOLE_1:36;
assume A8: D c= A;
now
take G = ( ( [#] Y ) \ F );
thus G is  open by A6,PRE_TOPC:def 3;
( A /\ ( [#] Y ) ) = A by XBOOLE_1:28;
then ( A /\ G ) = ( A \ F ) by XBOOLE_1:49;
then ( A /\ G ) = ( A \ ( A \ D ) ) by A7,XBOOLE_1:47;
then ( A /\ G ) = ( A /\ D ) by XBOOLE_1:48;
hence ( A /\ G ) = D by A8,XBOOLE_1:28;
end;
hence (ex G being (Subset of Y) st (G is  open & ( A /\ G ) = D));
end;
hence A is  discrete by CKB74:def 1;
end;
end;
end;
