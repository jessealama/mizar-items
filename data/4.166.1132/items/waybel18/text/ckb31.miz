begin
registration
cluster ( Sierpinski_Space ) -> non  empty  TopSpace-like;
coherence
proof
set IT = ( Sierpinski_Space );
thus IT is non  empty by CKB30:def 1;
{ ( 0 ),1 } in { ( {} ),{ 1 },{ ( 0 ),1 } } by ENUMSET1:def 1;
then (the carrier of IT) in { ( {} ),{ 1 },{ ( 0 ),1 } } by CKB30:def 1;
hence (the carrier of IT) in (the topology of IT) by CKB30:def 1;
thus (for a being (Subset-Family of IT) holds (a c= (the topology of IT) implies ( union a ) in (the topology of IT)))
proof
let a being (Subset-Family of IT);
assume a c= (the topology of IT);
then A1: a c= { ( {} ),{ 1 },{ ( 0 ),1 } } by CKB30:def 1;
per cases  by A1,ZFMISC_1:118;
suppose a = ( {} );

then ( union a ) in { ( {} ),{ 1 },{ ( 0 ),1 } } by ENUMSET1:def 1,ZFMISC_1:2;
hence thesis by CKB30:def 1;
end;
suppose a = { ( {} ) };

then ( union a ) = ( {} ) by ZFMISC_1:25;
then ( union a ) in { ( {} ),{ 1 },{ ( 0 ),1 } } by ENUMSET1:def 1;
hence thesis by CKB30:def 1;
end;
suppose a = { { 1 } };

then ( union a ) = { 1 } by ZFMISC_1:25;
then ( union a ) in { ( {} ),{ 1 },{ ( 0 ),1 } } by ENUMSET1:def 1;
hence thesis by CKB30:def 1;
end;
suppose a = { { ( 0 ),1 } };

then ( union a ) = { ( 0 ),1 } by ZFMISC_1:25;
then ( union a ) in { ( {} ),{ 1 },{ ( 0 ),1 } } by ENUMSET1:def 1;
hence thesis by CKB30:def 1;
end;
suppose a = { ( {} ),{ 1 } };

then ( union a ) = ( ( {} ) \/ { 1 } ) by ZFMISC_1:75;
then ( union a ) in { ( {} ),{ 1 },{ ( 0 ),1 } } by ENUMSET1:def 1;
hence thesis by CKB30:def 1;
end;
suppose a = { { 1 },{ ( 0 ),1 } };

then ( union a ) = ( { 1 } \/ { ( 0 ),1 } ) by ZFMISC_1:75
.= { ( 0 ),1 } by ZFMISC_1:9;
then ( union a ) in { ( {} ),{ 1 },{ ( 0 ),1 } } by ENUMSET1:def 1;
hence thesis by CKB30:def 1;
end;
suppose a = { ( {} ),{ ( 0 ),1 } };

then ( union a ) = ( ( {} ) \/ { ( 0 ),1 } ) by ZFMISC_1:75;
then ( union a ) in { ( {} ),{ 1 },{ ( 0 ),1 } } by ENUMSET1:def 1;
hence thesis by CKB30:def 1;
end;
suppose a = { ( {} ),{ 1 },{ ( 0 ),1 } };

then a = ( { ( {} ) } \/ { { 1 },{ ( 0 ),1 } } ) by ENUMSET1:2;
then ( union a ) = ( ( union { ( {} ) } ) \/ ( union { { 1 },{ ( 0 ),1 } } ) ) by ZFMISC_1:78
.= ( ( {} ) \/ ( union { { 1 },{ ( 0 ),1 } } ) ) by ZFMISC_1:25
.= ( { 1 } \/ { ( 0 ),1 } ) by ZFMISC_1:75
.= { ( 0 ),1 } by ZFMISC_1:9;
then ( union a ) in { ( {} ),{ 1 },{ ( 0 ),1 } } by ENUMSET1:def 1;
hence thesis by CKB30:def 1;
end;
end;

let a being (Subset of IT);
let b being (Subset of IT);
assume (a in (the topology of IT) & b in (the topology of IT));
then A2: (a in { ( {} ),{ 1 },{ ( 0 ),1 } } & b in { ( {} ),{ 1 },{ ( 0 ),1 } }) by CKB30:def 1;
per cases  by A2,ENUMSET1:def 1;
suppose (a = ( {} ) & b = ( {} ));

then ( a /\ b ) in { ( {} ),{ 1 },{ ( 0 ),1 } } by ENUMSET1:def 1;
hence thesis by CKB30:def 1;
end;
suppose (a = ( {} ) & b = { 1 });

then ( a /\ b ) in { ( {} ),{ 1 },{ ( 0 ),1 } } by ENUMSET1:def 1;
hence thesis by CKB30:def 1;
end;
suppose (a = ( {} ) & b = { ( 0 ),1 });

then ( a /\ b ) in { ( {} ),{ 1 },{ ( 0 ),1 } } by ENUMSET1:def 1;
hence thesis by CKB30:def 1;
end;
suppose (a = { 1 } & b = ( {} ));

then ( a /\ b ) in { ( {} ),{ 1 },{ ( 0 ),1 } } by ENUMSET1:def 1;
hence thesis by CKB30:def 1;
end;
suppose (a = { 1 } & b = { 1 });

then ( a /\ b ) in { ( {} ),{ 1 },{ ( 0 ),1 } } by ENUMSET1:def 1;
hence thesis by CKB30:def 1;
end;
suppose (a = { 1 } & b = { ( 0 ),1 });

then ( a /\ b ) = { 1 } by ZFMISC_1:13;
then ( a /\ b ) in { ( {} ),{ 1 },{ ( 0 ),1 } } by ENUMSET1:def 1;
hence thesis by CKB30:def 1;
end;
suppose (a = { ( 0 ),1 } & b = ( {} ));

then ( a /\ b ) in { ( {} ),{ 1 },{ ( 0 ),1 } } by ENUMSET1:def 1;
hence thesis by CKB30:def 1;
end;
suppose (a = { ( 0 ),1 } & b = { 1 });

then ( a /\ b ) = { 1 } by ZFMISC_1:13;
then ( a /\ b ) in { ( {} ),{ 1 },{ ( 0 ),1 } } by ENUMSET1:def 1;
hence thesis by CKB30:def 1;
end;
suppose (a = { ( 0 ),1 } & b = { ( 0 ),1 });

then ( a /\ b ) in { ( {} ),{ 1 },{ ( 0 ),1 } } by ENUMSET1:def 1;
hence thesis by CKB30:def 1;
end;
end;
