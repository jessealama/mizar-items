begin
theorem
Th11: (for S being IncProjStr holds (for L being (LINE of S) holds (for A,B being (POINT of S) holds ({ A,B } on L iff (A on L & B on L)))))
proof
let S being IncProjStr;
let L being (LINE of S);
let A being (POINT of S),B being (POINT of S);
thus ({ A,B } on L implies (A on L & B on L))
proof
A1: (A in { A,B } & B in { A,B }) by TARSKI:def 2;
assume { A,B } on L;
hence thesis by A1,CKB9:def 1;
end;

assume A2: (A on L & B on L);
let C being (POINT of S);
assume C in { A,B };
hence thesis by A2,TARSKI:def 2;
end;
