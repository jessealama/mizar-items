environ
vocabularies XBOOLE_0,RELAT_1,SUBSET_1,FDIFF_1,TARSKI,CARD_1,ZFMISC_1,INCSP_1;
notations TARSKI,XBOOLE_0,ENUMSET1,ZFMISC_1,SUBSET_1,DOMAIN_1,RELSET_1,NUMBERS,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB39,CKB55,CKB56,CKB57,CKB58;
definitions TARSKI,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB55,CKB56,CKB57,CKB58;
theorems ENUMSET1,TARSKI,ZFMISC_1,XBOOLE_0,XBOOLE_1,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB40,CKB41,CKB42,CKB43,CKB44,CKB45,CKB46,CKB47,CKB48,CKB49,CKB50,CKB51,CKB52,CKB53,CKB54,CKB55,CKB56,CKB57,CKB58,CKB59,CKB60,CKB61,CKB62,CKB63,CKB64,CKB65,CKB66,CKB67,CKB68,CKB69,CKB70,CKB71,CKB72,CKB73,CKB74,CKB75,CKB76,CKB77,CKB78,CKB79,CKB80,CKB81;
registrations XBOOLE_0,SUBSET_1,CKB1,CKB2,CKB37,CKB38;
constructors RELSET_1,DOMAIN_1,CKB1,CKB2,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB55,CKB56,CKB57,CKB58;
requirements SUBSET,BOOLE;
begin
reserve S for IncSpace;
reserve A for (POINT of S);
reserve B for (POINT of S);
reserve C for (POINT of S);
reserve D for (POINT of S);
reserve L for (LINE of S);
reserve L1 for (LINE of S);
reserve L2 for (LINE of S);
reserve P for (PLANE of S);
theorem
Th80: (A on P implies (ex L st (ex L1 st (ex L2 st ((((((L1 <> L2 & L1 on P) & L2 on P) & (not L on P)) & A on L) & A on L1) & A on L2)))))
proof
consider B,C such that A1: { B,C } on P and A2: (not { A,B,C } is  linear) by CKB78:1;
consider D such that A3: (not { A,B,C,D } is  planar) by A2,CKB77:1;
assume A on P;
then A4: ( { A } \/ { B,C } ) on P by A1,CKB21:1;
then A5: { A,B,C } on P by ENUMSET1:2;
take L3 = ( Line (A,D) );
take L1 = ( Line (A,B) );
take L2 = ( Line (A,C) );
A6: A <> B by A2,CKB41:1;
then A7: { A,B } on L1 by CKB55:def 1;
A8: (not { A,C,B } is  linear) by A2,ENUMSET1:57;
then A9: A <> C by CKB41:1;
then A10: { A,C } on L2 by CKB55:def 1;
then (B on L2 implies ( { A,C } \/ { B } ) on L2) by CKB20:1;
then (B on L2 implies { A,C,B } on L2) by ENUMSET1:3;
hence L1 <> L2 by A8,A7,CKB11:def 1,CKB13:1;
( { A,B } \/ { C } ) on P by A5,ENUMSET1:3;
then { A,B } on P by CKB23:1;
hence L1 on P by A6,A7,CKB32:def 1;
{ A,C,B } on P by A4,ENUMSET1:2;
then ( { A,C } \/ { B } ) on P by ENUMSET1:3;
then { A,C } on P by CKB21:1;
hence L2 on P by A9,A10,CKB32:def 1;
(not { A,D,B,C } is  planar) by A3,ENUMSET1:63;
then A <> D by CKB42:1;
then A11: { A,D } on L3 by CKB55:def 1;
then (L3 on P implies { A,D } on P) by CKB40:1;
then (L3 on P implies D on P) by CKB15:1;
then (L3 on P implies ( { A,B,C } \/ { D } ) on P) by A5,CKB21:1;
then (L3 on P implies { A,B,C,D } on P) by ENUMSET1:6;
hence (not L3 on P) by A3,CKB12:def 1;
thus thesis by A10,A7,A11,CKB13:1;
end;
