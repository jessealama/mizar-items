environ
vocabularies XBOOLE_0,RELAT_1,SUBSET_1,FDIFF_1,TARSKI,CARD_1,ZFMISC_1,INCSP_1;
notations TARSKI,XBOOLE_0,ENUMSET1,ZFMISC_1,SUBSET_1,DOMAIN_1,RELSET_1,NUMBERS,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB39,CKB55,CKB56,CKB57,CKB58;
definitions TARSKI,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB55,CKB56,CKB57,CKB58;
theorems ENUMSET1,TARSKI,ZFMISC_1,XBOOLE_0,XBOOLE_1,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB40,CKB41,CKB42,CKB43,CKB44,CKB45,CKB46,CKB47,CKB48,CKB49,CKB50,CKB51,CKB52,CKB53,CKB54,CKB55,CKB56,CKB57,CKB58,CKB59,CKB60,CKB61,CKB62,CKB63,CKB64,CKB65,CKB66,CKB67,CKB68,CKB69,CKB70,CKB71;
registrations XBOOLE_0,SUBSET_1,CKB1,CKB2,CKB37,CKB38;
constructors RELSET_1,DOMAIN_1,CKB1,CKB2,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB55,CKB56,CKB57,CKB58;
requirements SUBSET,BOOLE;
begin
reserve S for IncSpace;
reserve A for (POINT of S);
reserve B for (POINT of S);
reserve C for (POINT of S);
reserve D for (POINT of S);
reserve P for (PLANE of S);
theorem
Lm2: (ex A st (ex B st (ex C st (ex D st (A on P & (not { A,B,C,D } is  planar))))))
proof
consider A such that A1: A on P by CKB29:def 1;
consider A1,B1,C1,D1 being (POINT of S) such that A2: (not { A1,B1,C1,D1 } is  planar) by CKB34:def 1;
now
assume A3: (not A1 on P);
A4:now
A5: A1 <> B1 by A2,CKB42:1;
then A6: { A1,B1 } on ( Line (A1,B1) ) by CKB55:def 1;
{ A1,B1 } on ( Line (A1,B1) ) by A5,CKB55:def 1;
then (C1 on ( Line (A1,B1) ) implies ( { A1,B1 } \/ { C1 } ) on ( Line (A1,B1) )) by CKB20:1;
then A7: (C1 on ( Line (A1,B1) ) implies { A1,B1,C1 } on ( Line (A1,B1) )) by ENUMSET1:3;
set Q = ( Plane (A1,B1,C1) );
assume A8: A on ( Line (A1,B1) );
A9: (not { A1,B1,C1 } is  linear) by A2,CKB43:1;
then { A1,B1,C1 } on Q by CKB56:def 1;
then A10: (A1 on Q & C1 on Q) by CKB16:1;
A11: { A1,B1,C1 } on Q by A9,CKB56:def 1;
then (D1 on Q implies ( { A1,B1,C1 } \/ { D1 } ) on Q) by CKB21:1;
then A12: (D1 on Q implies { A1,B1,C1,D1 } on Q) by ENUMSET1:6;
( { A1,B1 } \/ { C1 } ) on Q by A11,ENUMSET1:3;
then { A1,B1 } on Q by CKB23:1;
then ( Line (A1,B1) ) on Q by A5,A6,CKB32:def 1;
then A on Q by A8,CKB35:def 1;
then A13: { A,A1,C1 } on ( Plane (A1,B1,C1) ) by A10,CKB16:1;
A1 on ( Line (A1,B1) ) by A6,CKB13:1;
then { A,A1 } on ( Line (A1,B1) ) by A8,CKB13:1;
then (not { A,A1,C1 } is  linear) by A1,A3,A9,A7,CKB11:def 1,CKB44:1;
then (not { A,A1,C1,D1 } is  planar) by A2,A12,A13,CKB12:def 1,CKB45:1;
hence thesis by A1;
end;
now
set Q = ( Plane (A1,B1,A) );
assume A14: (not A on ( Line (A1,B1) ));
A15: A1 <> B1 by A2,CKB42:1;
then A16: { A1,B1 } on ( Line (A1,B1) ) by CKB55:def 1;
then (not { A1,B1,A } is  linear) by A14,A15,CKB44:1;
then A17: { A1,B1,A } on Q by CKB56:def 1;
then ( { A1,B1 } \/ { A } ) on Q by ENUMSET1:3;
then { A1,B1 } on Q by CKB21:1;
then ({ C1,D1 } on Q implies ( { A1,B1 } \/ { C1,D1 } ) on Q) by CKB23:1;
then ({ C1,D1 } on Q implies { A1,B1,C1,D1 } on Q) by ENUMSET1:5;
then ((not C1 on Q) or (not D1 on Q)) by A2,CKB12:def 1,CKB15:1;
then ((not { A1,B1,A,C1 } is  planar) or (not { A1,B1,A,D1 } is  planar)) by A14,A15,A16,A17,CKB44:1,CKB45:1;
then ((not { A,A1,B1,C1 } is  planar) or (not { A,A1,B1,D1 } is  planar)) by ENUMSET1:67;
hence thesis by A1;
end;
hence thesis by A4;
end;
hence thesis by A2;
end;
