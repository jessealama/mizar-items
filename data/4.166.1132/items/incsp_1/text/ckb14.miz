environ
vocabularies XBOOLE_0,RELAT_1,SUBSET_1,FDIFF_1,TARSKI,CARD_1,ZFMISC_1,INCSP_1;
notations TARSKI,XBOOLE_0,ENUMSET1,ZFMISC_1,SUBSET_1,DOMAIN_1,RELSET_1,NUMBERS,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12;
definitions TARSKI,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12;
theorems ENUMSET1,TARSKI,ZFMISC_1,XBOOLE_0,XBOOLE_1,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13;
registrations XBOOLE_0,SUBSET_1,CKB1,CKB2;
constructors RELSET_1,DOMAIN_1,CKB1,CKB2,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12;
requirements SUBSET,BOOLE;
begin
reserve S for IncStruct;
reserve A for (POINT of S);
reserve B for (POINT of S);
reserve C for (POINT of S);
reserve D for (POINT of S);
reserve L for (LINE of S);
theorem
Th12: (for S being IncProjStr holds (for L being (LINE of S) holds (for A,B,C being (POINT of S) holds ({ A,B,C } on L iff ((A on L & B on L) & C on L)))))
proof
let S being IncProjStr;
let L being (LINE of S);
let A being (POINT of S);
let B being (POINT of S);
let C being (POINT of S);
thus ({ A,B,C } on L implies ((A on L & B on L) & C on L))
proof
A1: C in { A,B,C } by ENUMSET1:def 1;
A2: (A in { A,B,C } & B in { A,B,C }) by ENUMSET1:def 1;
assume { A,B,C } on L;
hence thesis by A2,A1,CKB9:def 1;
end;

assume A3: ((A on L & B on L) & C on L);
let D being (POINT of S);
assume D in { A,B,C };
hence thesis by A3,ENUMSET1:def 1;
end;
