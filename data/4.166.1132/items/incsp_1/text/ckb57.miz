environ
vocabularies XBOOLE_0,RELAT_1,SUBSET_1,FDIFF_1,TARSKI,CARD_1,ZFMISC_1,INCSP_1;
notations TARSKI,XBOOLE_0,ENUMSET1,ZFMISC_1,SUBSET_1,DOMAIN_1,RELSET_1,NUMBERS,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB39,CKB55,CKB56;
definitions TARSKI,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB55,CKB56;
theorems ENUMSET1,TARSKI,ZFMISC_1,XBOOLE_0,XBOOLE_1,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB40,CKB41,CKB42,CKB43,CKB44,CKB45,CKB46,CKB47,CKB48,CKB49,CKB50,CKB51,CKB52,CKB53,CKB54,CKB55,CKB56;
registrations XBOOLE_0,SUBSET_1,CKB1,CKB2,CKB37,CKB38;
constructors RELSET_1,DOMAIN_1,CKB1,CKB2,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB55,CKB56;
requirements SUBSET,BOOLE;
begin
reserve S for IncSpace;
reserve A for (POINT of S);
reserve B for (POINT of S);
reserve C for (POINT of S);
reserve L for (LINE of S);
reserve P for (PLANE of S);
reserve P1 for (PLANE of S);
reserve P2 for (PLANE of S);
reserve Q for (PLANE of S);
definition
let S;
let A;
let L;
assume A1: (not A on L);
func Plane (A,L) -> (PLANE of S) means 
:Def21: (A on it & L on it);
existence
proof
consider B,C such that A1: (B <> C & { B,C } on L) by CKB26:def 1;
consider P such that A2: { B,C,A } on P by CKB30:def 1;
take P;
thus A on P by A2,CKB16:1;
( { B,C } \/ { A } ) on P by A2,ENUMSET1:3;
then { B,C } on P by CKB21:1;
hence thesis by A1,CKB32:def 1;
end;
uniqueness
proof
let P;
let Q;
assume that
A2: (A on P & L on P)
and
A3: (A on Q & L on Q);
consider P1 such that A4: (for P2 holds ((A on P2 & L on P2) iff P1 = P2)) by A1,CKB53:1;
P1 = P by A2,A4;
hence thesis by A3,A4;
end;
end;
