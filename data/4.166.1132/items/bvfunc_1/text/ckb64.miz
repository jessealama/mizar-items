environ
vocabularies NUMBERS,XBOOLEAN,XXREAL_0,CARD_1,XBOOLE_0,SUBSET_1,FUNCT_2,MARGREL1,FUNCT_1,RELAT_1,TARSKI,FUNCOP_1,PARTIT1,EQREL_1,ZFMISC_1,SETFAM_1,BVFUNC_1;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,XBOOLEAN,MARGREL1,RELAT_1,FUNCT_1,FUNCT_2,SETFAM_1,EQREL_1,NUMBERS,PARTIT1,XXREAL_0,CKB1,CKB3,CKB4,CKB5,CKB6,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB36,CKB41,CKB42,CKB55,CKB58,CKB59,CKB60,CKB61;
definitions TARSKI,XBOOLE_0,MARGREL1,XBOOLEAN,CKB1,CKB5,CKB6,CKB8,CKB9,CKB12,CKB13,CKB16,CKB17,CKB20,CKB21,CKB36,CKB41,CKB42,CKB55,CKB60,CKB61;
theorems TARSKI,FUNCT_1,FUNCT_2,MARGREL1,EQREL_1,SETFAM_1,PARTIT1,XBOOLE_0,FUNCOP_1,XBOOLEAN,CKB1,CKB5,CKB6,CKB8,CKB9,CKB12,CKB13,CKB16,CKB17,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB37,CKB38,CKB39,CKB40,CKB41,CKB42,CKB43,CKB44,CKB48,CKB49,CKB50,CKB51,CKB52,CKB53,CKB54,CKB55,CKB56,CKB57,CKB60,CKB61,CKB62,CKB63;
schemes DOMAIN_1,FUNCT_2,FUNCT_1,CKB2;
registrations SUBSET_1,XREAL_0,XBOOLEAN,EQREL_1,MARGREL1,FUNCT_1,FUNCT_2,CKB7,CKB14,CKB15,CKB45,CKB46,CKB47;
constructors SETFAM_1,XXREAL_0,XREAL_0,PARTIT1,BINARITH,RELSET_1,CKB3,CKB4,CKB5,CKB6,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB36,CKB41,CKB42,CKB55,CKB58,CKB59,CKB60,CKB61;
requirements REAL,SUBSET,BOOLE,ARITHM;
begin
reserve Y for non  empty set;
theorem
(for a being (Element of ( Funcs (Y,( BOOLEAN )) )) holds (for PA being (a_partition of Y) holds ( B_INF (a,PA) ) '<' a))
proof
let a being (Element of ( Funcs (Y,( BOOLEAN )) ));
let PA being (a_partition of Y);
consider k3 being Function such that A1: ( ( B_INF (a,PA) ) 'imp' a ) = k3 and A2: ( dom k3 ) = Y and ( rng k3 ) c= ( BOOLEAN ) by FUNCT_2:def 2;
consider k4 being Function such that A3: ( I_el Y ) = k4 and A4: ( dom k4 ) = Y and ( rng k4 ) c= ( BOOLEAN ) by FUNCT_2:def 2;
(for y being (Element of Y) holds ( ( ( B_INF (a,PA) ) 'imp' a ) . y ) = ( ( I_el Y ) . y ))
proof
let y being (Element of Y);
per cases ;
suppose A5: (for x being (Element of Y) holds (x in ( EqClass (y,PA) ) implies ( a . x ) = ( TRUE )));

y in ( EqClass (y,PA) ) by EQREL_1:def 6;
then A6: ( a . y ) = ( TRUE ) by A5;
( 'not' ( ( B_INF (a,PA) ) . y ) ) = ( ( 'not' ( B_INF (a,PA) ) ) . y ) by MARGREL1:def 19;
then ( ( 'not' ( ( B_INF (a,PA) ) . y ) ) 'or' ( a . y ) ) = ( ( ( 'not' ( B_INF (a,PA) ) ) . y ) 'or' ( ( I_el Y ) . y ) ) by A6,CKB21:def 1
.= ( ( ( 'not' ( B_INF (a,PA) ) ) 'or' ( I_el Y ) ) . y ) by CKB8:def 1
.= ( ( I_el Y ) . y ) by CKB31:1;
hence thesis by CKB16:def 1;
end;
suppose (not (for x being (Element of Y) holds (x in ( EqClass (y,PA) ) implies ( a . x ) = ( TRUE ))));

then ( ( B_INF (a,PA) ) . y ) = ( FALSE ) by CKB60:def 1;
then ( ( 'not' ( ( B_INF (a,PA) ) . y ) ) 'or' ( a . y ) ) = ( ( ( I_el Y ) . y ) 'or' ( a . y ) ) by CKB21:def 1
.= ( ( ( I_el Y ) 'or' a ) . y ) by CKB8:def 1
.= ( ( I_el Y ) . y ) by CKB31:1;
hence thesis by CKB16:def 1;
end;
end;
then (for u being set holds (u in Y implies ( k3 . u ) = ( k4 . u ))) by A1,A3;
then k3 = k4 by A2,A4,FUNCT_1:2;
hence thesis by A1,A3,CKB38:1;
end;
