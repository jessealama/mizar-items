constructors SETFAM_1,XXREAL_0,XREAL_0,PARTIT1,BINARITH,RELSET_1,CKB3,CKB4,CKB5,CKB6,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB36,CKB41,CKB42,CKB55;
requirements REAL,SUBSET,BOOLE,ARITHM;
begin
reserve Y for non empty set;
reserve B for (Subset of Y);
theorem
(for a being (Element of ( Funcs (Y,( BOOLEAN )) )) holds a is_dependent_of ( %I Y ))
proof
let a being (Element of ( Funcs (Y,( BOOLEAN )) ));
(for F being set holds (F in ( %I Y ) implies (for x1,x2 being set holds ((x1 in F & x2 in F) implies ( a . x1 ) = ( a . x2 )))))
proof
let F being set;
assume F in ( %I Y );
then F in { B: (ex z being set st (B = { z } & z in Y)) } by PARTIT1:31;
then (ex B st (F = B & (ex z being set st (B = { z } & z in Y))));
then consider z being set such that A1: F = { z } and z in Y;
let x1 being set,x2 being set;
assume that
A2: x1 in F
and
A3: x2 in F;
x1 = z by A1,A2,TARSKI:def 1;
hence thesis by A1,A3,TARSKI:def 1;
end;
hence thesis by CKB55:def 1;
end;
