requirements REAL,SUBSET,BOOLE,ARITHM;
begin
reserve Y for non  empty set;
theorem
(for a,b being (Element of ( Funcs (Y,( BOOLEAN )) )) holds (for PA being (a_partition of Y) holds ( B_SUP (( a 'or' b ),PA) ) = ( ( B_SUP (a,PA) ) 'or' ( B_SUP (b,PA) ) )))
proof
let a being (Element of ( Funcs (Y,( BOOLEAN )) ));
let b being (Element of ( Funcs (Y,( BOOLEAN )) ));
let PA being (a_partition of Y);
consider k3 being Function such that A1: ( B_SUP (( a 'or' b ),PA) ) = k3 and A2: ( dom k3 ) = Y and ( rng k3 ) c= ( BOOLEAN ) by FUNCT_2:def 2;
consider k4 being Function such that A3: ( ( B_SUP (a,PA) ) 'or' ( B_SUP (b,PA) ) ) = k4 and A4: ( dom k4 ) = Y and ( rng k4 ) c= ( BOOLEAN ) by FUNCT_2:def 2;
(for y being (Element of Y) holds ( ( B_SUP (( a 'or' b ),PA) ) . y ) = ( ( ( B_SUP (a,PA) ) 'or' ( B_SUP (b,PA) ) ) . y ))
proof
let y being (Element of Y);
A5:now
assume A6: (ex x being (Element of Y) st (x in ( EqClass (y,PA) ) & ( ( a 'or' b ) . x ) = ( TRUE )));
then consider x1 being (Element of Y) such that A7: x1 in ( EqClass (y,PA) ) and A8: ( ( a 'or' b ) . x1 ) = ( TRUE );
A9: ((((( a . x1 ) = ( FALSE ) & ( b . x1 ) = ( FALSE )) or (( a . x1 ) = ( FALSE ) & ( b . x1 ) = ( TRUE ))) or (( a . x1 ) = ( TRUE ) & ( b . x1 ) = ( FALSE ))) or (( a . x1 ) = ( TRUE ) & ( b . x1 ) = ( TRUE ))) by XBOOLEAN:def 3;
A10: ( ( a . x1 ) 'or' ( b . x1 ) ) = ( TRUE ) by A8,CKB8:def 1;
now
per cases  by A10,A9;
case (( a . x1 ) = ( FALSE ) & ( b . x1 ) = ( TRUE ));
then ( ( B_SUP (b,PA) ) . y ) = ( TRUE ) by A7,CKB61:def 1;
then ( ( ( B_SUP (a,PA) ) . y ) 'or' ( ( B_SUP (b,PA) ) . y ) ) = ( TRUE );
then ( ( ( B_SUP (a,PA) ) 'or' ( B_SUP (b,PA) ) ) . y ) = ( TRUE ) by CKB8:def 1;
hence thesis by A6,CKB61:def 1;
end;
case (( a . x1 ) = ( TRUE ) & ( b . x1 ) = ( FALSE ));
then ( ( B_SUP (a,PA) ) . y ) = ( TRUE ) by A7,CKB61:def 1;
then ( ( ( B_SUP (a,PA) ) . y ) 'or' ( ( B_SUP (b,PA) ) . y ) ) = ( TRUE );
then ( ( ( B_SUP (a,PA) ) 'or' ( B_SUP (b,PA) ) ) . y ) = ( TRUE ) by CKB8:def 1;
hence thesis by A6,CKB61:def 1;
end;
case (( a . x1 ) = ( TRUE ) & ( b . x1 ) = ( TRUE ));
then ( ( B_SUP (b,PA) ) . y ) = ( TRUE ) by A7,CKB61:def 1;
then ( ( ( B_SUP (a,PA) ) . y ) 'or' ( ( B_SUP (b,PA) ) . y ) ) = ( TRUE );
then ( ( ( B_SUP (a,PA) ) 'or' ( B_SUP (b,PA) ) ) . y ) = ( TRUE ) by CKB8:def 1;
hence thesis by A6,CKB61:def 1;
end;
end;
hence thesis;
end;
now
assume A11: (not (ex x being (Element of Y) st (x in ( EqClass (y,PA) ) & ( ( a 'or' b ) . x ) = ( TRUE ))));
now
assume (ex x being (Element of Y) st (x in ( EqClass (y,PA) ) & ( b . x ) = ( TRUE )));
then consider x1 being (Element of Y) such that A12: x1 in ( EqClass (y,PA) ) and A13: ( b . x1 ) = ( TRUE );
( ( a . x1 ) 'or' ( b . x1 ) ) = ( TRUE ) by A13;
then ( ( a 'or' b ) . x1 ) = ( TRUE ) by CKB8:def 1;
hence contradiction by A11,A12;
end;
then A14: ( ( B_SUP (b,PA) ) . y ) = ( FALSE ) by CKB61:def 1;
now
assume (ex x being (Element of Y) st (x in ( EqClass (y,PA) ) & ( a . x ) = ( TRUE )));
then consider x1 being (Element of Y) such that A15: x1 in ( EqClass (y,PA) ) and A16: ( a . x1 ) = ( TRUE );
( ( a . x1 ) 'or' ( b . x1 ) ) = ( TRUE ) by A16;
then ( ( a 'or' b ) . x1 ) = ( TRUE ) by CKB8:def 1;
hence contradiction by A11,A15;
end;
then ( ( ( B_SUP (a,PA) ) . y ) 'or' ( ( B_SUP (b,PA) ) . y ) ) = ( ( FALSE ) 'or' ( FALSE ) ) by A14,CKB61:def 1;
then ( ( ( B_SUP (a,PA) ) 'or' ( B_SUP (b,PA) ) ) . y ) = ( FALSE ) by CKB8:def 1;
hence thesis by A11,CKB61:def 1;
end;
hence thesis by A5;
end;
then (for u being set holds (u in Y implies ( k3 . u ) = ( k4 . u ))) by A1,A3;
hence thesis by A1,A2,A3,A4,FUNCT_1:2;
