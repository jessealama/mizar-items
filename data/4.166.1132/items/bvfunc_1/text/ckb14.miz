requirements REAL,SUBSET,BOOLE,ARITHM;
begin
registration
let p being  boolean-valued Function;
let q being  boolean-valued Function;
cluster ( p 'imp' q ) ->  boolean-valued;
coherence
proof
let x being set;
assume x in ( rng ( p 'imp' q ) );
then consider y being set such that A1: (y in ( dom ( p 'imp' q ) ) & x = ( ( p 'imp' q ) . y )) by FUNCT_1:def 3;
x = ( ( p . y ) => ( q . y ) ) by A1,CKB12:def 1
.= ( ( 'not' ( p . y ) ) 'or' ( q . y ) );
then (x = ( FALSE ) or x = ( TRUE )) by XBOOLEAN:def 3;
hence thesis;
end;
end;
