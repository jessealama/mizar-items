constructors SETFAM_1,XXREAL_0,XREAL_0,PARTIT1,BINARITH,RELSET_1,CKB3,CKB4,CKB5,CKB6,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB36,CKB41,CKB42,CKB55,CKB58,CKB59,CKB60,CKB61;
requirements REAL,SUBSET,BOOLE,ARITHM;
begin
reserve Y for non empty set;
theorem
(for a being (Element of ( Funcs (Y,( BOOLEAN )) )) holds (for PA being (a_partition of Y) holds ( 'not' ( B_INF (a,PA) ) ) = ( B_SUP (( 'not' a ),PA) )))
proof
let a being (Element of ( Funcs (Y,( BOOLEAN )) ));
let PA being (a_partition of Y);
consider k3 being Function such that A1: ( 'not' ( B_INF (a,PA) ) ) = k3 and A2: ( dom k3 ) = Y and ( rng k3 ) c= ( BOOLEAN ) by FUNCT_2:def 2;
consider k4 being Function such that A3: ( B_SUP (( 'not' a ),PA) ) = k4 and A4: ( dom k4 ) = Y and ( rng k4 ) c= ( BOOLEAN ) by FUNCT_2:def 2;
(for y being (Element of Y) holds ( ( 'not' ( B_INF (a,PA) ) ) . y ) = ( ( B_SUP (( 'not' a ),PA) ) . y ))
proof
let y being (Element of Y);
A5:now
assume that
A6: (for x being (Element of Y) holds (x in ( EqClass (y,PA) ) implies ( a . x ) = ( TRUE )))
and
A7: (ex x being (Element of Y) st (x in ( EqClass (y,PA) ) & ( ( 'not' a ) . x ) = ( TRUE )));
consider x1 being (Element of Y) such that A8: x1 in ( EqClass (y,PA) ) and A9: ( ( 'not' a ) . x1 ) = ( TRUE ) by A7;
( ( 'not' ( 'not' a ) ) . x1 ) = ( 'not' ( TRUE ) ) by A9,MARGREL1:def 19;
hence contradiction by A6,A8;
end;
A10:now
assume that
A11: (not (for x being (Element of Y) holds (x in ( EqClass (y,PA) ) implies ( a . x ) = ( TRUE ))))
and
A12: (not (ex x being (Element of Y) st (x in ( EqClass (y,PA) ) & ( ( 'not' a ) . x ) = ( TRUE ))));
consider x1 being (Element of Y) such that A13: x1 in ( EqClass (y,PA) ) and A14: ( a . x1 ) <> ( TRUE ) by A11;
( a . x1 ) = ( FALSE ) by A14,XBOOLEAN:def 3;
then ( ( 'not' a ) . x1 ) = ( 'not' ( FALSE ) ) by MARGREL1:def 19;
hence contradiction by A12,A13;
end;
A15:now
assume that
A16: (not (for x being (Element of Y) holds (x in ( EqClass (y,PA) ) implies ( a . x ) = ( TRUE ))))
and
A17: (ex x being (Element of Y) st (x in ( EqClass (y,PA) ) & ( ( 'not' a ) . x ) = ( TRUE )));
( ( B_INF (a,PA) ) . y ) = ( FALSE ) by A16,CKB60:def 1;
then ( ( 'not' ( B_INF (a,PA) ) ) . y ) = ( 'not' ( FALSE ) ) by MARGREL1:def 19;
hence thesis by A17,CKB61:def 1;
end;
now
assume that
A18: (for x being (Element of Y) holds (x in ( EqClass (y,PA) ) implies ( a . x ) = ( TRUE )))
and
A19: (not (ex x being (Element of Y) st (x in ( EqClass (y,PA) ) & ( ( 'not' a ) . x ) = ( TRUE ))));
( ( B_INF (a,PA) ) . y ) = ( TRUE ) by A18,CKB60:def 1;
then ( ( 'not' ( B_INF (a,PA) ) ) . y ) = ( 'not' ( TRUE ) ) by MARGREL1:def 19;
hence thesis by A19,CKB61:def 1;
end;
hence thesis by A5,A15,A10;
end;
then (for u being set holds (u in Y implies ( k3 . u ) = ( k4 . u ))) by A1,A3;
hence thesis by A1,A2,A3,A4,FUNCT_1:2;
end;
