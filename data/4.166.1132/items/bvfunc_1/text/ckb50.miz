environ
vocabularies NUMBERS,XBOOLEAN,XXREAL_0,CARD_1,XBOOLE_0,SUBSET_1,FUNCT_2,MARGREL1,FUNCT_1,RELAT_1,TARSKI,FUNCOP_1,PARTIT1,EQREL_1,ZFMISC_1,SETFAM_1,BVFUNC_1;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,XBOOLEAN,MARGREL1,RELAT_1,FUNCT_1,FUNCT_2,SETFAM_1,EQREL_1,NUMBERS,PARTIT1,XXREAL_0,CKB1,CKB3,CKB4,CKB5,CKB6,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB36,CKB41,CKB42;
definitions TARSKI,XBOOLE_0,MARGREL1,XBOOLEAN,CKB1,CKB5,CKB6,CKB8,CKB9,CKB12,CKB13,CKB16,CKB17,CKB20,CKB21,CKB36,CKB41,CKB42;
theorems TARSKI,FUNCT_1,FUNCT_2,MARGREL1,EQREL_1,SETFAM_1,PARTIT1,XBOOLE_0,FUNCOP_1,XBOOLEAN,CKB1,CKB5,CKB6,CKB8,CKB9,CKB12,CKB13,CKB16,CKB17,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB37,CKB38,CKB39,CKB40,CKB41,CKB42,CKB43,CKB44,CKB48,CKB49;
schemes DOMAIN_1,FUNCT_2,FUNCT_1,CKB2;
registrations SUBSET_1,XREAL_0,XBOOLEAN,EQREL_1,MARGREL1,FUNCT_1,FUNCT_2,CKB7,CKB14,CKB15,CKB45,CKB46,CKB47;
constructors SETFAM_1,XXREAL_0,XREAL_0,PARTIT1,BINARITH,RELSET_1,CKB3,CKB4,CKB5,CKB6,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB36,CKB41,CKB42;
requirements REAL,SUBSET,BOOLE,ARITHM;
begin
reserve Y for non empty set;
theorem
(for a,b being (Element of ( Funcs (Y,( BOOLEAN )) )) holds (( B_INF ( a '&' b ) ) = ( ( B_INF a ) '&' ( B_INF b ) ) & ( B_SUP ( a 'or' b ) ) = ( ( B_SUP a ) 'or' ( B_SUP b ) )))
proof
let a being (Element of ( Funcs (Y,( BOOLEAN )) )),b being (Element of ( Funcs (Y,( BOOLEAN )) ));
A1:now
assume A2: (for x being (Element of Y) holds ( ( a '&' b ) . x ) = ( TRUE ));
A3: (for x being (Element of Y) holds ( a . x ) = ( TRUE ))
proof
let x being (Element of Y);
( ( a '&' b ) . x ) = ( ( a . x ) '&' ( b . x ) ) by MARGREL1:def 20;
then ( ( a . x ) '&' ( b . x ) ) = ( TRUE ) by A2;
hence thesis by XBOOLEAN:101;
end;
(not (for x being (Element of Y) holds ( a . x ) = ( FALSE )))
proof
now
assume (for x being (Element of Y) holds ( a . x ) = ( FALSE ));
let x being (Element of Y);
( a . x ) = ( TRUE ) by A3;
hence thesis;
end;
hence thesis;
end;
then A4: ( B_SUP a ) = ( I_el Y ) by CKB42:def 1;
A5: (for x being (Element of Y) holds ( b . x ) = ( TRUE ))
proof
let x being (Element of Y);
( ( a '&' b ) . x ) = ( TRUE ) by A2;
then ( ( a . x ) '&' ( b . x ) ) = ( TRUE ) by MARGREL1:def 20;
hence thesis by XBOOLEAN:101;
end;
(not (for x being (Element of Y) holds ( b . x ) = ( FALSE )))
proof
now
assume (for x being (Element of Y) holds ( b . x ) = ( FALSE ));
let x being (Element of Y);
( b . x ) = ( TRUE ) by A5;
hence thesis;
end;
hence thesis;
end;
then A6: ( ( B_SUP a ) 'or' ( B_SUP b ) ) = ( ( I_el Y ) 'or' ( I_el Y ) ) by A4,CKB42:def 1;
A7: (not (for x being (Element of Y) holds ( ( a 'or' b ) . x ) = ( FALSE )))
proof
now
assume (for x being (Element of Y) holds ( ( a 'or' b ) . x ) = ( FALSE ));
let x being (Element of Y);
(( ( a 'or' b ) . x ) = ( ( a . x ) 'or' ( b . x ) ) & ( a . x ) = ( TRUE )) by A3,CKB8:def 1;
hence thesis;
end;
hence thesis;
end;
( ( B_INF a ) '&' ( B_INF b ) ) = ( ( B_INF a ) '&' ( I_el Y ) ) by A5,CKB41:def 1
.= ( ( I_el Y ) '&' ( I_el Y ) ) by A3,CKB41:def 1;
hence thesis by A2,A7,A6,CKB41:def 1,CKB42:def 1;
end;
A8:now
assume A9: (for x being (Element of Y) holds ( ( a 'or' b ) . x ) = ( FALSE ));
A10: (for x being (Element of Y) holds ( a . x ) = ( FALSE ))
proof
let x being (Element of Y);
( ( a 'or' b ) . x ) = ( FALSE ) by A9;
then A11: ( ( a . x ) 'or' ( b . x ) ) = ( FALSE ) by CKB8:def 1;
(( a . x ) = ( TRUE ) or ( a . x ) = ( FALSE )) by XBOOLEAN:def 3;
hence thesis by A11;
end;
A12: (not (for x being (Element of Y) holds ( ( a '&' b ) . x ) = ( TRUE )))
proof
now
assume (for x being (Element of Y) holds ( ( a '&' b ) . x ) = ( TRUE ));
let x being (Element of Y);
(( ( a '&' b ) . x ) = ( ( a . x ) '&' ( b . x ) ) & ( a . x ) = ( FALSE )) by A10,MARGREL1:def 20;
hence thesis;
end;
hence thesis;
end;
A13: (for x being (Element of Y) holds ( b . x ) = ( FALSE ))
proof
let x being (Element of Y);
( ( a 'or' b ) . x ) = ( FALSE ) by A9;
then A14: ( ( a . x ) 'or' ( b . x ) ) = ( FALSE ) by CKB8:def 1;
(( b . x ) = ( TRUE ) or ( b . x ) = ( FALSE )) by XBOOLEAN:def 3;
hence thesis by A14;
end;
then ( B_SUP b ) = ( O_el Y ) by CKB42:def 1;
then A15: ( ( B_SUP a ) 'or' ( B_SUP b ) ) = ( ( O_el Y ) 'or' ( O_el Y ) ) by A10,CKB42:def 1;
(not (for x being (Element of Y) holds ( a . x ) = ( TRUE )))
proof
now
assume (for x being (Element of Y) holds ( a . x ) = ( TRUE ));
let x being (Element of Y);
( a . x ) = ( FALSE ) by A10;
hence thesis;
end;
hence thesis;
end;
then A16: ( B_INF a ) = ( O_el Y ) by CKB41:def 1;
(not (for x being (Element of Y) holds ( b . x ) = ( TRUE )))
proof
now
assume (for x being (Element of Y) holds ( b . x ) = ( TRUE ));
let x being (Element of Y);
( b . x ) = ( FALSE ) by A13;
hence thesis;
end;
hence thesis;
end;
then ( ( B_INF a ) '&' ( B_INF b ) ) = ( ( O_el Y ) '&' ( O_el Y ) ) by A16,CKB41:def 1;
hence thesis by A9,A15,A12,CKB41:def 1,CKB42:def 1;
end;
now
assume that
A17: (not (for x being (Element of Y) holds ( ( a '&' b ) . x ) = ( TRUE )))
and
A18: (not (for x being (Element of Y) holds ( ( a 'or' b ) . x ) = ( FALSE )));
(((for x being (Element of Y) holds ( a . x ) = ( FALSE )) & (for x being (Element of Y) holds ( b . x ) = ( FALSE ))) implies (for x being (Element of Y) holds ( ( a 'or' b ) . x ) = ( FALSE )))
proof
assume that
A19: (for x being (Element of Y) holds ( a . x ) = ( FALSE ))
and
A20: (for x being (Element of Y) holds ( b . x ) = ( FALSE ));
let x being (Element of Y);
( a . x ) = ( FALSE ) by A19;
then ( ( a . x ) 'or' ( b . x ) ) = ( FALSE ) by A20;
hence thesis by CKB8:def 1;
end;
then (( B_SUP a ) = ( I_el Y ) or ( B_SUP b ) = ( I_el Y )) by A18,CKB42:def 1;
then A21: ( ( B_SUP a ) 'or' ( B_SUP b ) ) = ( I_el Y ) by CKB31:1;
(((for x being (Element of Y) holds ( a . x ) = ( TRUE )) & (for x being (Element of Y) holds ( b . x ) = ( TRUE ))) implies (for x being (Element of Y) holds ( ( a '&' b ) . x ) = ( TRUE )))
proof
assume that
A22: (for x being (Element of Y) holds ( a . x ) = ( TRUE ))
and
A23: (for x being (Element of Y) holds ( b . x ) = ( TRUE ));
let x being (Element of Y);
( a . x ) = ( TRUE ) by A22;
then ( ( a . x ) '&' ( b . x ) ) = ( TRUE ) by A23;
hence thesis by MARGREL1:def 20;
end;
then (( B_INF a ) = ( O_el Y ) or ( B_INF b ) = ( O_el Y )) by A17,CKB41:def 1;
then ( ( B_INF a ) '&' ( B_INF b ) ) = ( O_el Y ) by CKB26:1;
hence thesis by A17,A18,A21,CKB41:def 1,CKB42:def 1;
end;
hence thesis by A1,A8;
end;
