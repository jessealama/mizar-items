environ
vocabularies NUMBERS,XBOOLEAN,XXREAL_0,CARD_1,XBOOLE_0,SUBSET_1,FUNCT_2,MARGREL1,FUNCT_1,RELAT_1,TARSKI,FUNCOP_1,PARTIT1,EQREL_1,ZFMISC_1,SETFAM_1,BVFUNC_1;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,XBOOLEAN,MARGREL1,RELAT_1,FUNCT_1,FUNCT_2,SETFAM_1,EQREL_1,NUMBERS,PARTIT1,XXREAL_0,CKB1,CKB3,CKB4;
definitions TARSKI,XBOOLE_0,MARGREL1,XBOOLEAN,CKB1;
theorems TARSKI,FUNCT_1,FUNCT_2,MARGREL1,EQREL_1,SETFAM_1,PARTIT1,XBOOLE_0,FUNCOP_1,XBOOLEAN,CKB1;
schemes DOMAIN_1,FUNCT_2,FUNCT_1,CKB2;
registrations SUBSET_1,XREAL_0,XBOOLEAN,EQREL_1,MARGREL1,FUNCT_1,FUNCT_2;
constructors SETFAM_1,XXREAL_0,XREAL_0,PARTIT1,BINARITH,RELSET_1,CKB3,CKB4;
requirements REAL,SUBSET,BOOLE,ARITHM;
begin
definition
let p being  boolean-valued Function;
let q being  boolean-valued Function;
func p 'or' q ->  boolean-valued Function
means
:Def5: (( dom it ) = ( ( dom p ) /\ ( dom q ) ) & (for x being set holds (x in ( dom it ) implies ( it . x ) = ( ( p . x ) 'or' ( q . x ) ))));
existence
proof
deffunc F(set) = ( ( p . $1 ) 'or' ( q . $1 ) );
consider s being Function such that A1: (( dom s ) = ( ( dom p ) /\ ( dom q ) ) & (for x being set holds (x in ( ( dom p ) /\ ( dom q ) ) implies ( s . x ) = F(x)))) from FUNCT_1:sch 3;
s is  boolean-valued
proof
let x being set;
assume x in ( rng s );
then consider y being set such that A2: (y in ( dom s ) & x = ( s . y )) by FUNCT_1:def 3;
x = ( ( p . y ) 'or' ( q . y ) ) by A1,A2;
then (x = ( FALSE ) or x = ( TRUE )) by XBOOLEAN:def 3;
hence thesis;
end;
hence thesis by A1;
end;
uniqueness
proof
let s1 being  boolean-valued Function,s2 being  boolean-valued Function;
assume that
A3: ( dom s1 ) = ( ( dom p ) /\ ( dom q ) )
and
A4: (for x being set holds (x in ( dom s1 ) implies ( s1 . x ) = ( ( p . x ) 'or' ( q . x ) )))
and
A5: ( dom s2 ) = ( ( dom p ) /\ ( dom q ) )
and
A6: (for x being set holds (x in ( dom s2 ) implies ( s2 . x ) = ( ( p . x ) 'or' ( q . x ) )));
(for x being set holds (x in ( dom s1 ) implies ( s1 . x ) = ( s2 . x )))
proof
let x being set;
assume A7: x in ( dom s1 );
then ( s1 . x ) = ( ( p . x ) 'or' ( q . x ) ) by A4;
hence thesis by A3,A5,A6,A7;
end;
hence thesis by A3,A5,FUNCT_1:2;
end;
commutativity;
idempotence;
end;
