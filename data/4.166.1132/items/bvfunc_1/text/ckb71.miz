requirements REAL,SUBSET,BOOLE,ARITHM;
begin
reserve Y for non  empty set;
theorem
(for a,b being (Element of ( Funcs (Y,( BOOLEAN )) )) holds (for PA being (a_partition of Y) holds ( B_INF (( a '&' b ),PA) ) = ( ( B_INF (a,PA) ) '&' ( B_INF (b,PA) ) )))
proof
let a being (Element of ( Funcs (Y,( BOOLEAN )) ));
let b being (Element of ( Funcs (Y,( BOOLEAN )) ));
let PA being (a_partition of Y);
consider k3 being Function such that A1: ( B_INF (( a '&' b ),PA) ) = k3 and A2: ( dom k3 ) = Y and ( rng k3 ) c= ( BOOLEAN ) by FUNCT_2:def 2;
consider k4 being Function such that A3: ( ( B_INF (a,PA) ) '&' ( B_INF (b,PA) ) ) = k4 and A4: ( dom k4 ) = Y and ( rng k4 ) c= ( BOOLEAN ) by FUNCT_2:def 2;
(for y being (Element of Y) holds ( ( B_INF (( a '&' b ),PA) ) . y ) = ( ( ( B_INF (a,PA) ) '&' ( B_INF (b,PA) ) ) . y ))
proof
let y being (Element of Y);
A5:now
assume that
(for x being (Element of Y) holds (x in ( EqClass (y,PA) ) implies ( a . x ) = ( TRUE )))
and
A6: (not (for x being (Element of Y) holds (x in ( EqClass (y,PA) ) implies ( b . x ) = ( TRUE ))));
( ( B_INF (b,PA) ) . y ) = ( FALSE ) by A6,CKB60:def 1;
then ( ( ( B_INF (a,PA) ) . y ) '&' ( ( B_INF (b,PA) ) . y ) ) = ( FALSE );
then A7: ( ( ( B_INF (a,PA) ) '&' ( B_INF (b,PA) ) ) . y ) = ( FALSE ) by MARGREL1:def 20;
consider x1 being (Element of Y) such that A8: x1 in ( EqClass (y,PA) ) and A9: ( b . x1 ) <> ( TRUE ) by A6;
( b . x1 ) = ( FALSE ) by A9,XBOOLEAN:def 3;
then ( ( a . x1 ) '&' ( b . x1 ) ) = ( FALSE );
then ( ( a '&' b ) . x1 ) <> ( TRUE ) by MARGREL1:def 20;
hence thesis by A8,A7,CKB60:def 1;
end;
A10:now
assume that
A11: (not (for x being (Element of Y) holds (x in ( EqClass (y,PA) ) implies ( a . x ) = ( TRUE ))))
and
A12: (not (for x being (Element of Y) holds (x in ( EqClass (y,PA) ) implies ( b . x ) = ( TRUE ))));
( ( B_INF (b,PA) ) . y ) = ( FALSE ) by A12,CKB60:def 1;
then ( ( ( B_INF (a,PA) ) . y ) '&' ( ( B_INF (b,PA) ) . y ) ) = ( FALSE );
then A13: ( ( ( B_INF (a,PA) ) '&' ( B_INF (b,PA) ) ) . y ) = ( FALSE ) by MARGREL1:def 20;
consider xa being (Element of Y) such that A14: xa in ( EqClass (y,PA) ) and A15: ( a . xa ) <> ( TRUE ) by A11;
( a . xa ) = ( FALSE ) by A15,XBOOLEAN:def 3;
then ( ( a . xa ) '&' ( b . xa ) ) = ( FALSE );
then ( ( a '&' b ) . xa ) <> ( TRUE ) by MARGREL1:def 20;
hence thesis by A14,A13,CKB60:def 1;
end;
A16:now
assume that
A17: (for x being (Element of Y) holds (x in ( EqClass (y,PA) ) implies ( a . x ) = ( TRUE )))
and
A18: (for x being (Element of Y) holds (x in ( EqClass (y,PA) ) implies ( b . x ) = ( TRUE )));
A19: (for x being (Element of Y) holds (x in ( EqClass (y,PA) ) implies ( ( a '&' b ) . x ) = ( TRUE )))
proof
let x being (Element of Y);
assume A20: x in ( EqClass (y,PA) );
then ( b . x ) = ( TRUE ) by A18;
then ( ( a . x ) '&' ( b . x ) ) = ( ( TRUE ) '&' ( TRUE ) ) by A17,A20;
hence thesis by MARGREL1:def 20;
end;
( ( B_INF (b,PA) ) . y ) = ( TRUE ) by A18,CKB60:def 1;
then ( ( ( B_INF (a,PA) ) . y ) '&' ( ( B_INF (b,PA) ) . y ) ) = ( ( TRUE ) '&' ( TRUE ) ) by A17,CKB60:def 1;
then ( ( ( B_INF (a,PA) ) '&' ( B_INF (b,PA) ) ) . y ) = ( TRUE ) by MARGREL1:def 20;
hence thesis by A19,CKB60:def 1;
end;
now
assume that
A21: (not (for x being (Element of Y) holds (x in ( EqClass (y,PA) ) implies ( a . x ) = ( TRUE ))))
and
A22: (for x being (Element of Y) holds (x in ( EqClass (y,PA) ) implies ( b . x ) = ( TRUE )));
( ( B_INF (b,PA) ) . y ) = ( TRUE ) by A22,CKB60:def 1;
then ( ( ( B_INF (a,PA) ) . y ) '&' ( ( B_INF (b,PA) ) . y ) ) = ( ( FALSE ) '&' ( TRUE ) ) by A21,CKB60:def 1;
then A23: ( ( ( B_INF (a,PA) ) '&' ( B_INF (b,PA) ) ) . y ) = ( FALSE ) by MARGREL1:def 20;
consider x1 being (Element of Y) such that A24: x1 in ( EqClass (y,PA) ) and A25: ( a . x1 ) <> ( TRUE ) by A21;
( a . x1 ) = ( FALSE ) by A25,XBOOLEAN:def 3;
then ( ( a . x1 ) '&' ( b . x1 ) ) = ( FALSE );
then ( ( a '&' b ) . x1 ) <> ( TRUE ) by MARGREL1:def 20;
hence thesis by A24,A23,CKB60:def 1;
end;
hence thesis by A16,A5,A10;
end;
then (for u being set holds (u in Y implies ( k3 . u ) = ( k4 . u ))) by A1,A3;
hence thesis by A1,A2,A3,A4,FUNCT_1:2;
end;
