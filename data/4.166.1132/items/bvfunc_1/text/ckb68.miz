constructors SETFAM_1,XXREAL_0,XREAL_0,PARTIT1,BINARITH,RELSET_1,CKB3,CKB4,CKB5,CKB6,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB36,CKB41,CKB42,CKB55,CKB58,CKB59,CKB60,CKB61;
requirements REAL,SUBSET,BOOLE,ARITHM;
begin
reserve Y for non empty set;
theorem
(for a being (Element of ( Funcs (Y,( BOOLEAN )) )) holds ( B_SUP (a,( %O Y )) ) = ( B_SUP a ))
proof
let a being (Element of ( Funcs (Y,( BOOLEAN )) ));
consider k3 being Function such that A1: ( B_SUP (a,( %O Y )) ) = k3 and A2: ( dom k3 ) = Y and ( rng k3 ) c= ( BOOLEAN ) by FUNCT_2:def 2;
consider k4 being Function such that A3: ( B_SUP a ) = k4 and A4: ( dom k4 ) = Y and ( rng k4 ) c= ( BOOLEAN ) by FUNCT_2:def 2;
(for y being (Element of Y) holds ( ( B_SUP (a,( %O Y )) ) . y ) = ( ( B_SUP a ) . y ))
proof
let y being (Element of Y);
( EqClass (y,( %O Y )) ) in ( %O Y );
then ( EqClass (y,( %O Y )) ) in { Y } by PARTIT1:def 8;
then A5: ( EqClass (y,( %O Y )) ) = Y by TARSKI:def 1;
A6:now
assume that
A7: (not (ex x being (Element of Y) st (x in ( EqClass (y,( %O Y )) ) & ( a . x ) = ( TRUE ))))
and
A8: (not (for x being (Element of Y) holds ( a . x ) = ( FALSE )));
consider x1 being (Element of Y) such that A9: ( a . x1 ) <> ( FALSE ) by A8;
( a . x1 ) = ( TRUE ) by A9,XBOOLEAN:def 3;
hence contradiction by A5,A7;
end;
A10:now
assume that
A11: (not (ex x being (Element of Y) st (x in ( EqClass (y,( %O Y )) ) & ( a . x ) = ( TRUE ))))
and
A12: (for x being (Element of Y) holds ( a . x ) = ( FALSE ));
( B_SUP a ) = ( O_el Y ) by A12,CKB42:def 1;
then ( ( B_SUP a ) . y ) = ( FALSE ) by CKB20:def 1;
hence thesis by A11,CKB61:def 1;
end;
now
assume that
A13: (ex x being (Element of Y) st (x in ( EqClass (y,( %O Y )) ) & ( a . x ) = ( TRUE )))
and
(not (for x being (Element of Y) holds ( a . x ) = ( FALSE )));
( B_SUP a ) = ( I_el Y ) by A13,CKB42:def 1;
then ( ( B_SUP a ) . y ) = ( TRUE ) by CKB21:def 1;
hence thesis by A13,CKB61:def 1;
end;
hence thesis by A10,A6;
end;
then (for u being set holds (u in Y implies ( k3 . u ) = ( k4 . u ))) by A1,A3;
hence thesis by A1,A2,A3,A4,FUNCT_1:2;
end;
