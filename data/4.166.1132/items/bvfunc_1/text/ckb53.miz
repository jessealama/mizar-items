theorem
(for a being (Element of ( Funcs (Y,( BOOLEAN )) )) holds (for x being (Element of Y) holds ( ( B_INF a ) . x ) <= ( a . x )))
proof
let a being (Element of ( Funcs (Y,( BOOLEAN )) ));
let x being (Element of Y);
A1:now
assume (not (for x being (Element of Y) holds ( a . x ) = ( TRUE )));
then ( B_INF a ) = ( O_el Y ) by CKB41:def 1;
then ( ( B_INF a ) . x ) = ( FALSE ) by CKB20:def 1;
then ( ( ( B_INF a ) . x ) => ( a . x ) ) = ( TRUE );
hence thesis by CKB1:def 1;
end;
now
assume (for x being (Element of Y) holds ( a . x ) = ( TRUE ));
then ( a . x ) = ( TRUE );
then ( ( ( B_INF a ) . x ) => ( a . x ) ) = ( TRUE );
hence thesis by CKB1:def 1;
end;
hence thesis by A1;
end;
