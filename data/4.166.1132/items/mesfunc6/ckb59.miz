begin
reserve X for non empty set;
reserve f for (PartFunc of X,( REAL ));
reserve r for Real;
theorem
(f is  nonnegative implies ((( 0 ) <= r implies ( r (#) f ) is  nonnegative) & (r <= ( 0 ) implies ( r (#) f ) is  nonpositive)))
proof
assume A1: f is  nonnegative;
hereby
assume A2: ( 0 ) <= r;
now
let x being set;
assume that
A3: x in ( dom ( r (#) f ) );
( 0 ) <= ( f . x ) by A1,CKB53:1;
then ( ( 0 ) * r ) <= ( r * ( f . x ) ) by A2;
hence ( 0 ) <= ( ( r (#) f ) . x ) by A3,VALUED_1:def 5;
end;
hence ( r (#) f ) is  nonnegative by CKB54:1;
end;
assume A4: r <= ( 0 );
now
let x being set;
assume that
A5: x in ( dom ( r (#) f ) );
( 0 ) <= ( f . x ) by A1,CKB53:1;
then ( r * ( f . x ) ) <= ( r * ( 0 ) ) by A4;
hence ( ( r (#) f ) . x ) <= ( 0 ) by A5,VALUED_1:def 5;
end;
hence thesis by CKB56:1;
end;
