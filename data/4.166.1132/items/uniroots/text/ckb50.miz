begin
definition
let n being non empty Nat;
let L being  left_unital non empty doubleLoopStr;
func unital_poly (L,n) -> (Polynomial of L)
equals
( ( ( 0_. L ) +* (( 0 ),( - ( 1_ L ) )) ) +* (n,( 1_ L )) );
coherence
proof
set p = ( ( ( 0_. L ) +* (( 0 ),( - ( 1_ L ) )) ) +* (n,( 1_ L )) );
A1: (for i being Nat holds ((i <> ( 0 ) & i <> n) implies ( p . i ) = ( 0. L )))
proof
set q = ( ( 0_. L ) +* (( 0 ),( - ( 1_ L ) )) );
let i being Nat;
assume that
A2: i <> ( 0 )
and
A3: i <> n;
A4: i in ( NAT ) by ORDINAL1:def 12;
( ( q +* (n,( 1_ L )) ) . i ) = ( q . i ) by A3,FUNCT_7:32
.= ( ( 0_. L ) . i ) by A2,FUNCT_7:32
.= ( 0. L ) by A4,FUNCOP_1:7;
hence thesis;
end;
(for i being Nat holds (i >= ( n + 1 ) implies ( p . i ) = ( 0. L )))
proof
let i being Nat;
assume that
A5: i >= ( n + 1 );
now
A6: ( n + ( 0 ) ) < ( n + 1 ) by XREAL_1:8;
assume i = n;
hence contradiction by A5,A6;
end;
hence thesis by A1,A5;
end;
hence thesis by ALGSEQ_1:def 1;
end;
end;
