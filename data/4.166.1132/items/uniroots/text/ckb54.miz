theorem
Th44: (for L being non  degenerated  well-unital non  empty doubleLoopStr holds (for n being non  empty (Element of ( NAT )) holds ( len ( unital_poly (L,n) ) ) = ( n + 1 )))
proof
let L being non  degenerated  well-unital non  empty doubleLoopStr;
let n being non  empty (Element of ( NAT ));
A1: (for m being Nat holds (m is_at_least_length_of ( unital_poly (L,n) ) implies ( n + 1 ) <= m))
proof
let m being Nat;
assume that
A2: m is_at_least_length_of ( unital_poly (L,n) );
now
assume m < ( n + 1 );
then m <= n by NAT_1:13;
then ( ( unital_poly (L,n) ) . n ) = ( 0. L ) by A2,ALGSEQ_1:def 2;
hence contradiction by CKB52:1;
end;
hence thesis;
end;
(for i being Nat holds (i >= ( n + 1 ) implies ( ( unital_poly (L,n) ) . i ) = ( 0. L )))
proof
let i being Nat;
assume that
A3: i >= ( n + 1 );
now
A4: ( n + ( 0 ) ) < ( n + 1 ) by XREAL_1:8;
assume i = n;
hence contradiction by A3,A4;
end;
hence thesis by A3,CKB53:1;
end;
then ( n + 1 ) is_at_least_length_of ( unital_poly (L,n) ) by ALGSEQ_1:def 2;
hence thesis by A1,ALGSEQ_1:def 3;
end;
