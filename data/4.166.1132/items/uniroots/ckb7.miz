assume A1: (for i being (Element of ( NAT )) holds (i in ( dom f ) implies ( f . i ) is  integer));
defpred P[ (Element of ( NAT )) ]
 means
(for f being (FinSequence of FC) holds ((( len f ) = $1 & (for i being (Element of ( NAT )) holds (i in ( dom f ) implies ( f . i ) is  integer))) implies ( Sum f ) is  integer));
A2: (for n being (Element of ( NAT )) holds (P[ n ] implies P[ ( n + 1 ) ]))
proof
let n being (Element of ( NAT ));
assume that
A3: P[ n ];
let f being (FinSequence of FC);
assume that
A4: ( len f ) = ( n + 1 )
and
A5: (for i being (Element of ( NAT )) holds (i in ( dom f ) implies ( f . i ) is  integer));
consider g being (FinSequence of FC),c being (Element of FC) such that A6: f = ( g ^ <* c *> ) by A4,FINSEQ_2:19;
A7:now
