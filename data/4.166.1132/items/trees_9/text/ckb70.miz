begin
Lm2:now
let p being FinSequence;
let q being FinSequence;
assume that
A1: ( len p ) = ( len q )
and
A2: (for i being (Element of ( NAT )) holds (i < ( len p ) implies ( p . ( i + 1 ) ) = ( q . ( i + 1 ) )));
A3:now
