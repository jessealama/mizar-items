begin
reserve k for Nat;
reserve n for Nat;
reserve f for (Function of n,k);
scheme Sch1 { n,k() -> Nat,P[set] } : { f where f is (Function of n(),k()): P[ f ] } is  finite
proof
set F = { f where f is (Function of n(),k()): P[ f ] };
now
per cases ;
suppose A1: k() = ( 0 );

F c= { ( {} ) }
proof
let x being set;
assume x in F;
then consider f being (Function of n(),k()) such that A2: x = f and P[ f ];
f = ( {} ) by A1;
hence thesis by A2,TARSKI:def 1;
end;
hence thesis;
end;
suppose A3: k() > ( 0 );

A4: F c= ( Funcs (n(),k()) )
proof
let x being set;
assume x in F;
then (ex f being (Function of n(),k()) st (x = f & P[ f ]));
hence thesis by A3,FUNCT_2:8;
end;
( Funcs (n(),k()) ) is  finite by FRAENKEL:6;
hence thesis by A4;
end;
end;
hence thesis;
end;
