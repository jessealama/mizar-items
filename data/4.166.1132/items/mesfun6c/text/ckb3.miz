environ
vocabularies NUMBERS,XREAL_0,ORDINAL1,MEASURE6,ARYTM_3,ARYTM_1,RELAT_1,REAL_1,XBOOLE_0,PARTFUN1,COMPLEX1,SUBSET_1,FUNCT_1,PROB_1,MEASURE1,XCMPLX_0,MESFUNC1,VALUED_1,CARD_1,TARSKI,INTEGRA5,MESFUNC5,XXREAL_0,RFUNCT_3,SUPINF_2,POWER,SQUARE_1,PREPOWER,VALUED_0;
notations TARSKI,XBOOLE_0,SUBSET_1,ORDINAL1,NUMBERS,XXREAL_0,XCMPLX_0,XREAL_0,COMPLEX1,REAL_1,RELAT_1,FUNCT_1,RELSET_1,PARTFUN1,VALUED_1,SUPINF_2,SEQ_1,PREPOWER,POWER,SQUARE_1,MEASURE6,MEASURE1,EXTREAL1,COMSEQ_3,VALUED_0,MESFUNC1,MESFUNC2,PROB_1,MESFUNC5,MESFUNC6,CKB2;
definitions MESFUNC5,MEASURE6,VALUED_1,MESFUNC6,COMPLEX1,XCMPLX_0,VALUED_0,SUPINF_2,CKB2;
theorems TARSKI,PARTFUN1,FUNCT_1,SUPINF_2,EXTREAL1,EXTREAL2,MESFUNC1,XBOOLE_0,XBOOLE_1,XCMPLX_1,HOLDER_1,PREPOWER,MESFUNC2,XREAL_1,MESFUNC5,XXREAL_0,VALUED_1,MESFUNC6,SUBSET_1,COMPLEX1,XREAL_0,RELAT_1,POWER,SQUARE_1,RELSET_1,FINSUB_1,COMSEQ_3,CKB1,CKB2;
schemes SEQ_1;
registrations XBOOLE_0,SUBSET_1,NUMBERS,XXREAL_0,XREAL_0,MEMBERED,MEASURE1,VALUED_0,XCMPLX_0,RELAT_1,FUNCT_1,COMSEQ_3,RELSET_1;
constructors REAL_1,SEQ_1,PREPOWER,POWER,SQUARE_1,COMPLEX1,MEASURE6,EXTREAL1,MESFUNC1,MESFUNC2,MESFUNC5,MESFUNC6,EXTREAL2,SUPINF_1,RELSET_1,ABIAN,BINOP_2,VALUED_0,COMSEQ_3,CKB2;
requirements NUMERALS,REAL,BOOLE,SUBSET,ARITHM;
begin
reserve X for non  empty set;
reserve f for (PartFunc of X,( COMPLEX ));
reserve r for Real;
theorem
Th2: (( r (#) ( Re f ) ) = ( Re ( r (#) f ) ) & ( r (#) ( Im f ) ) = ( Im ( r (#) f ) ))
proof
A1: ( dom ( r (#) ( Re f ) ) ) = ( dom ( Re f ) ) by VALUED_1:def 5;
A2: ( Im r ) = ( 0 ) by COMPLEX1:def 2;
A3: ( dom ( Re f ) ) = ( dom f ) by COMSEQ_3:def 3;
A4: ( Re r ) = r by COMPLEX1:def 1;
A5: ( dom ( r (#) f ) ) = ( dom f ) by VALUED_1:def 5;
A6: ( dom ( Re ( r (#) f ) ) ) = ( dom ( r (#) f ) ) by COMSEQ_3:def 3;
now
let x being set;
A7: ( Re ( r * ( f . x ) ) ) = ( ( ( Re r ) * ( Re ( f . x ) ) ) - ( ( Im r ) * ( Im ( f . x ) ) ) ) by COMPLEX1:9;
assume A8: x in ( dom ( r (#) ( Re f ) ) );
then A9: ( ( Re f ) . x ) = ( Re ( f . x ) ) by A1,COMSEQ_3:def 3;
( ( Re ( r (#) f ) ) . x ) = ( Re ( ( r (#) f ) . x ) ) by A1,A5,A6,A3,A8,COMSEQ_3:def 3;
then ( ( Re ( r (#) f ) ) . x ) = ( r * ( Re ( f . x ) ) ) by A1,A5,A3,A4,A2,A8,A7,VALUED_1:def 5;
hence ( ( r (#) ( Re f ) ) . x ) = ( ( Re ( r (#) f ) ) . x ) by A8,A9,VALUED_1:def 5;
end;
hence ( r (#) ( Re f ) ) = ( Re ( r (#) f ) ) by A1,A5,A6,A3,FUNCT_1:2;
A10: ( dom ( r (#) ( Im f ) ) ) = ( dom ( Im f ) ) by VALUED_1:def 5;
A11: ( dom ( Im f ) ) = ( dom f ) by COMSEQ_3:def 4;
A12: ( dom ( Im ( r (#) f ) ) ) = ( dom ( r (#) f ) ) by COMSEQ_3:def 4;
now
let x being set;
A13: ( Im ( r * ( f . x ) ) ) = ( ( ( Im r ) * ( Re ( f . x ) ) ) + ( ( Re r ) * ( Im ( f . x ) ) ) ) by COMPLEX1:9;
assume A14: x in ( dom ( r (#) ( Im f ) ) );
then A15: ( ( Im f ) . x ) = ( Im ( f . x ) ) by A10,COMSEQ_3:def 4;
( ( Im ( r (#) f ) ) . x ) = ( Im ( ( r (#) f ) . x ) ) by A10,A5,A12,A11,A14,COMSEQ_3:def 4;
then ( ( Im ( r (#) f ) ) . x ) = ( r * ( Im ( f . x ) ) ) by A10,A5,A11,A4,A2,A14,A13,VALUED_1:def 5;
hence ( ( r (#) ( Im f ) ) . x ) = ( ( Im ( r (#) f ) ) . x ) by A14,A15,VALUED_1:def 5;
end;
hence thesis by A10,A5,A12,A11,FUNCT_1:2;
end;
