environ
vocabularies NUMBERS,XREAL_0,ORDINAL1,MEASURE6,ARYTM_3,ARYTM_1,RELAT_1,REAL_1,XBOOLE_0,PARTFUN1,COMPLEX1,SUBSET_1,FUNCT_1,PROB_1,MEASURE1,XCMPLX_0,MESFUNC1,VALUED_1,CARD_1,TARSKI,INTEGRA5,MESFUNC5,XXREAL_0,RFUNCT_3,SUPINF_2,POWER,SQUARE_1,PREPOWER,VALUED_0;
notations TARSKI,XBOOLE_0,SUBSET_1,ORDINAL1,NUMBERS,XXREAL_0,XCMPLX_0,XREAL_0,COMPLEX1,REAL_1,RELAT_1,FUNCT_1,RELSET_1,PARTFUN1,VALUED_1,SUPINF_2,SEQ_1,PREPOWER,POWER,SQUARE_1,MEASURE6,MEASURE1,EXTREAL1,COMSEQ_3,VALUED_0,MESFUNC1,MESFUNC2,PROB_1,MESFUNC5,MESFUNC6,CKB2,CKB20,CKB21,CKB30;
definitions MESFUNC5,MEASURE6,VALUED_1,MESFUNC6,COMPLEX1,XCMPLX_0,VALUED_0,SUPINF_2,CKB2,CKB20,CKB21,CKB30;
theorems TARSKI,PARTFUN1,FUNCT_1,SUPINF_2,EXTREAL1,EXTREAL2,MESFUNC1,XBOOLE_0,XBOOLE_1,XCMPLX_1,HOLDER_1,PREPOWER,MESFUNC2,XREAL_1,MESFUNC5,XXREAL_0,VALUED_1,MESFUNC6,SUBSET_1,COMPLEX1,XREAL_0,RELAT_1,POWER,SQUARE_1,RELSET_1,FINSUB_1,COMSEQ_3,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB33,CKB34,CKB35,CKB36,CKB37;
schemes SEQ_1;
registrations XBOOLE_0,SUBSET_1,NUMBERS,XXREAL_0,XREAL_0,MEMBERED,MEASURE1,VALUED_0,XCMPLX_0,RELAT_1,FUNCT_1,COMSEQ_3,RELSET_1,CKB31,CKB32;
constructors REAL_1,SEQ_1,PREPOWER,POWER,SQUARE_1,COMPLEX1,MEASURE6,EXTREAL1,MESFUNC1,MESFUNC2,MESFUNC5,MESFUNC6,EXTREAL2,SUPINF_1,RELSET_1,ABIAN,BINOP_2,VALUED_0,COMSEQ_3,CKB2,CKB20,CKB21,CKB30;
requirements NUMERALS,REAL,BOOLE,SUBSET,ARITHM;
begin
reserve X for non  empty set;
reserve S for (SigmaField of X);
reserve M for (sigma_Measure of S);
reserve f for (PartFunc of X,( COMPLEX ));
reserve A for (Element of S);
theorem
Th31: ((ex A being (Element of S) st (A = ( dom f ) & f is_measurable_on A)) implies (f is_integrable_on M iff |. f .| is_integrable_on M))
proof
A1: ( dom ( |. ( Re f ) .| + |. ( Im f ) .| ) ) = ( ( dom |. ( Re f ) .| ) /\ ( dom |. ( Im f ) .| ) ) by VALUED_1:def 1;
A2: ( dom |. ( Re f ) .| ) = ( dom ( Re f ) ) by VALUED_1:def 11;
A3: ( dom |. ( Im f ) .| ) = ( dom ( Im f ) ) by VALUED_1:def 11;
A4: ( dom |. f .| ) = ( dom f ) by VALUED_1:def 11;
assume (ex A being (Element of S) st (A = ( dom f ) & f is_measurable_on A));
then consider A being (Element of S) such that A5: A = ( dom f ) and A6: f is_measurable_on A;
A7: ( dom ( Re f ) ) = A by A5,COMSEQ_3:def 3;
A8: ( Im f ) is_measurable_on A by A6,CKB2:def 1;
A9: ( Re f ) is_measurable_on A by A6,CKB2:def 1;
A10: ( dom ( Im f ) ) = A by A5,COMSEQ_3:def 4;
A11: |. f .| is_measurable_on A by A5,A6,CKB37:1;
hereby
A12:now
let x being (Element of X);
assume A13: x in ( dom |. f .| );
then A14: ( |. f .| . x ) = |. ( f . x ) .| by VALUED_1:def 11;
A15: |. ( ( Im f ) . x ) .| = ( |. ( Im f ) .| . x ) by A5,A10,A3,A4,A13,VALUED_1:def 11;
A16: |. ( ( Re f ) . x ) .| = ( |. ( Re f ) .| . x ) by A5,A7,A2,A4,A13,VALUED_1:def 11;
A17: ( ( Im f ) . x ) = ( Im ( f . x ) ) by A5,A10,A4,A13,COMSEQ_3:def 4;
A18: ( ( Re f ) . x ) = ( Re ( f . x ) ) by A5,A7,A4,A13,COMSEQ_3:def 3;
( ( |. ( Re f ) .| + |. ( Im f ) .| ) . x ) = ( ( |. ( Re f ) .| . x ) + ( |. ( Im f ) .| . x ) ) by A5,A7,A10,A1,A2,A3,A4,A13,VALUED_1:def 1;
hence |. ( |. f .| . x ) .| <= ( ( |. ( Re f ) .| + |. ( Im f ) .| ) . x ) by A18,A17,A14,A16,A15,COMPLEX1:78;
end;
assume A19: f is_integrable_on M;
then ( Im f ) is_integrable_on M by CKB20:def 1;
then A20: |. ( Im f ) .| is_integrable_on M by A10,A8,MESFUNC6:94;
( Re f ) is_integrable_on M by A19,CKB20:def 1;
then |. ( Re f ) .| is_integrable_on M by A7,A9,MESFUNC6:94;
then ( |. ( Re f ) .| + |. ( Im f ) .| ) is_integrable_on M by A20,MESFUNC6:100;
hence |. f .| is_integrable_on M by A5,A7,A10,A1,A2,A3,A4,A11,A12,MESFUNC6:96;
end;
hereby
assume A21: |. f .| is_integrable_on M;
now
let x being (Element of X);
A22: |. ( Im ( f . x ) ) .| <= |. ( f . x ) .| by COMPLEX1:79;
assume A23: x in ( dom ( Im f ) );
then ( |. f .| . x ) = |. ( f . x ) .| by A5,A10,A4,VALUED_1:def 11;
hence |. ( ( Im f ) . x ) .| <= ( |. f .| . x ) by A23,A22,COMSEQ_3:def 4;
end;
then A24: ( Im f ) is_integrable_on M by A5,A10,A8,A4,A21,MESFUNC6:96;
now
let x being (Element of X);
A25: |. ( Re ( f . x ) ) .| <= |. ( f . x ) .| by COMPLEX1:79;
assume A26: x in ( dom ( Re f ) );
then ( |. f .| . x ) = |. ( f . x ) .| by A5,A7,A4,VALUED_1:def 11;
hence |. ( ( Re f ) . x ) .| <= ( |. f .| . x ) by A26,A25,COMSEQ_3:def 3;
end;
then ( Re f ) is_integrable_on M by A5,A7,A9,A4,A21,MESFUNC6:96;
hence f is_integrable_on M by A24,CKB20:def 1;
end;
end;
