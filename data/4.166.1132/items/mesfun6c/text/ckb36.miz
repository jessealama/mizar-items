environ
vocabularies NUMBERS,XREAL_0,ORDINAL1,MEASURE6,ARYTM_3,ARYTM_1,RELAT_1,REAL_1,XBOOLE_0,PARTFUN1,COMPLEX1,SUBSET_1,FUNCT_1,PROB_1,MEASURE1,XCMPLX_0,MESFUNC1,VALUED_1,CARD_1,TARSKI,INTEGRA5,MESFUNC5,XXREAL_0,RFUNCT_3,SUPINF_2,POWER,SQUARE_1,PREPOWER,VALUED_0;
notations TARSKI,XBOOLE_0,SUBSET_1,ORDINAL1,NUMBERS,XXREAL_0,XCMPLX_0,XREAL_0,COMPLEX1,REAL_1,RELAT_1,FUNCT_1,RELSET_1,PARTFUN1,VALUED_1,SUPINF_2,SEQ_1,PREPOWER,POWER,SQUARE_1,MEASURE6,MEASURE1,EXTREAL1,COMSEQ_3,VALUED_0,MESFUNC1,MESFUNC2,PROB_1,MESFUNC5,MESFUNC6,CKB2,CKB20,CKB21,CKB30;
definitions MESFUNC5,MEASURE6,VALUED_1,MESFUNC6,COMPLEX1,XCMPLX_0,VALUED_0,SUPINF_2,CKB2,CKB20,CKB21,CKB30;
theorems TARSKI,PARTFUN1,FUNCT_1,SUPINF_2,EXTREAL1,EXTREAL2,MESFUNC1,XBOOLE_0,XBOOLE_1,XCMPLX_1,HOLDER_1,PREPOWER,MESFUNC2,XREAL_1,MESFUNC5,XXREAL_0,VALUED_1,MESFUNC6,SUBSET_1,COMPLEX1,XREAL_0,RELAT_1,POWER,SQUARE_1,RELSET_1,FINSUB_1,COMSEQ_3,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB33,CKB34,CKB35;
schemes SEQ_1;
registrations XBOOLE_0,SUBSET_1,NUMBERS,XXREAL_0,XREAL_0,MEMBERED,MEASURE1,VALUED_0,XCMPLX_0,RELAT_1,FUNCT_1,COMSEQ_3,RELSET_1,CKB31,CKB32;
constructors REAL_1,SEQ_1,PREPOWER,POWER,SQUARE_1,COMPLEX1,MEASURE6,EXTREAL1,MESFUNC1,MESFUNC2,MESFUNC5,MESFUNC6,EXTREAL2,SUPINF_1,RELSET_1,ABIAN,BINOP_2,VALUED_0,COMSEQ_3,CKB2,CKB20,CKB21,CKB30;
requirements NUMERALS,REAL,BOOLE,SUBSET,ARITHM;
begin
reserve X for non empty set;
reserve S for (SigmaField of X);
reserve E for (Element of S);
theorem
Th29: (for k being  real number holds (for X holds (for S holds (for E holds (for f being (PartFunc of X,( REAL )) holds ((((f is  nonnegative & ( 0 ) <= k) & E c= ( dom f )) & f is_measurable_on E) implies ( f to_power k ) is_measurable_on E))))))
proof
let k being  real number;
let X;
let S;
let E;
let f being (PartFunc of X,( REAL ));
reconsider k1 = k as Real by XREAL_0:def 1;
assume that
A1: f is  nonnegative
and
A2: ( 0 ) <= k
and
A3: E c= ( dom f )
and
A4: f is_measurable_on E;
A5: ( dom ( f to_power k ) ) = ( dom f ) by CKB30:def 1;
per cases  by A2;
suppose A6: k = ( 0 );

A7: E c= ( dom ( f to_power k ) ) by A3,CKB30:def 1;
now
let r being  real number;
reconsider r1 = r as Real by XREAL_0:def 1;
per cases ;
suppose A8: r <= 1;

now
let x being set;
assume A9: x in E;
then ( ( f to_power k ) . x ) = ( ( f . x ) to_power k ) by A3,A5,CKB30:def 1;
then r <= ( ( f to_power k ) . x ) by A6,A8,POWER:24;
hence x in ( great_eq_dom (( f to_power k ),r1) ) by A3,A5,A9,MESFUNC6:6;
end;
then E c= ( great_eq_dom (( f to_power k ),r) ) by TARSKI:def 3;
then ( E /\ ( great_eq_dom (( f to_power k ),r) ) ) = E by XBOOLE_1:28;
then ( E /\ ( less_dom (( f to_power k ),r1) ) ) = ( E \ E ) by A7,CKB35:1;
hence ( E /\ ( less_dom (( f to_power k ),r) ) ) in S;
end;
suppose A10: 1 < r;

now
let x being set;
assume A11: x in E;
then ( ( f to_power k ) . x ) = ( ( f . x ) to_power k ) by A3,A5,CKB30:def 1;
then ( ( f to_power k ) . x ) < r by A6,A10,POWER:24;
hence x in ( less_dom (( f to_power k ),r) ) by A3,A5,A11,MESFUNC6:3;
end;
then E c= ( less_dom (( f to_power k ),r) ) by TARSKI:def 3;
then ( E /\ ( less_dom (( f to_power k ),r) ) ) = E by XBOOLE_1:28;
hence ( E /\ ( less_dom (( f to_power k ),r) ) ) in S;
end;
end;
hence thesis by MESFUNC6:12;
end;
suppose A12: k > ( 0 );

(for r being  real number holds ( E /\ ( great_eq_dom (( f to_power k ),r) ) ) in S)
proof
let r being  real number;
reconsider r1 = r as Real by XREAL_0:def 1;
per cases ;
suppose A13: r1 <= ( 0 );

now
let x being set;
assume x in E;
then x in ( dom f ) by A3;
then A14: x in ( dom ( f to_power k ) ) by CKB30:def 1;
( 0 ) <= ( ( f to_power k ) . x ) by A1,A12,MESFUNC6:51;
hence x in ( great_eq_dom (( f to_power k ),r1) ) by A13,A14,MESFUNC6:6;
end;
then E c= ( great_eq_dom (( f to_power k ),r) ) by TARSKI:def 3;
then ( E /\ ( great_eq_dom (( f to_power k ),r) ) ) = E by XBOOLE_1:28;
hence thesis;
end;
suppose A15: ( 0 ) < r1;

A16:now
set R = ( r to_power ( 1 / k ) );
let x being set;
A17: ( 0 ) < ( r to_power ( 1 / k ) ) by A15,POWER:34;
assume A18: x in ( great_eq_dom (f,( r1 to_power ( 1 / k ) )) );
then A19: x in ( dom ( f to_power k ) ) by A5,MESFUNC6:6;
( R to_power k ) = ( r to_power ( ( 1 / k ) * k ) ) by A15,POWER:33;
then A20: ( R to_power k ) = ( r to_power 1 ) by A12,XCMPLX_1:87;
(ex y being Real st (y = ( f . x ) & ( r1 to_power ( 1 / k ) ) <= y)) by A18,MESFUNC6:6;
then ( r1 to_power 1 ) <= ( ( f . x ) to_power k1 ) by A12,A17,A20,HOLDER_1:3;
then r <= ( ( f . x ) to_power k ) by POWER:25;
then r <= ( ( f to_power k ) . x ) by A19,CKB30:def 1;
hence x in ( great_eq_dom (( f to_power k ),r1) ) by A19,MESFUNC6:6;
end;
now
let x being set;
assume A21: x in ( great_eq_dom (( f to_power k ),r1) );
then A22: x in ( dom ( f to_power k ) ) by MESFUNC6:6;
( 0 ) <= ( f . x ) by A1,MESFUNC6:51;
then ( ( ( f . x ) to_power k1 ) to_power ( 1 / k1 ) ) = ( ( f . x ) to_power ( ( k1 * 1 ) / k1 ) ) by A12,HOLDER_1:2;
then ( ( ( f . x ) to_power k1 ) to_power ( 1 / k1 ) ) = ( ( f . x ) to_power 1 ) by A12,XCMPLX_1:87;
then A23: ( ( ( f . x ) to_power k ) to_power ( 1 / k ) ) = ( f . x ) by POWER:25;
(ex y2 being Real st (y2 = ( ( f to_power k ) . x ) & r1 <= y2)) by A21,MESFUNC6:6;
then r <= ( ( f . x ) to_power k ) by A22,CKB30:def 1;
then ( r to_power ( 1 / k1 ) ) <= ( ( ( f . x ) to_power k1 ) to_power ( 1 / k1 ) ) by A12,A15,HOLDER_1:3;
hence x in ( great_eq_dom (f,( r1 to_power ( 1 / k ) )) ) by A5,A22,A23,MESFUNC6:6;
end;
then ( E /\ ( great_eq_dom (( f to_power k ),r1) ) ) = ( E /\ ( great_eq_dom (f,( r1 to_power ( 1 / k ) )) ) ) by A16,TARSKI:1;
hence thesis by A3,A4,MESFUNC6:13;
end;
end;
hence thesis by A3,A5,MESFUNC6:13;
end;
end;
