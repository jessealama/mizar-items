environ
vocabularies NUMBERS,XREAL_0,ORDINAL1,MEASURE6,ARYTM_3,ARYTM_1,RELAT_1,REAL_1,XBOOLE_0,PARTFUN1,COMPLEX1,SUBSET_1,FUNCT_1,PROB_1,MEASURE1,XCMPLX_0,MESFUNC1,VALUED_1,CARD_1,TARSKI,INTEGRA5,MESFUNC5,XXREAL_0,RFUNCT_3,SUPINF_2,POWER,SQUARE_1,PREPOWER,VALUED_0;
notations TARSKI,XBOOLE_0,SUBSET_1,ORDINAL1,NUMBERS,XXREAL_0,XCMPLX_0,XREAL_0,COMPLEX1,REAL_1,RELAT_1,FUNCT_1,RELSET_1,PARTFUN1,VALUED_1,SUPINF_2,SEQ_1,PREPOWER,POWER,SQUARE_1,MEASURE6,MEASURE1,EXTREAL1,COMSEQ_3,VALUED_0,MESFUNC1,MESFUNC2,PROB_1,MESFUNC5,MESFUNC6,CKB2;
definitions MESFUNC5,MEASURE6,VALUED_1,MESFUNC6,COMPLEX1,XCMPLX_0,VALUED_0,SUPINF_2,CKB2;
theorems TARSKI,PARTFUN1,FUNCT_1,SUPINF_2,EXTREAL1,EXTREAL2,MESFUNC1,XBOOLE_0,XBOOLE_1,XCMPLX_1,HOLDER_1,PREPOWER,MESFUNC2,XREAL_1,MESFUNC5,XXREAL_0,VALUED_1,MESFUNC6,SUBSET_1,COMPLEX1,XREAL_0,RELAT_1,POWER,SQUARE_1,RELSET_1,FINSUB_1,COMSEQ_3,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16;
schemes SEQ_1;
registrations XBOOLE_0,SUBSET_1,NUMBERS,XXREAL_0,XREAL_0,MEMBERED,MEASURE1,VALUED_0,XCMPLX_0,RELAT_1,FUNCT_1,COMSEQ_3,RELSET_1;
constructors REAL_1,SEQ_1,PREPOWER,POWER,SQUARE_1,COMPLEX1,MEASURE6,EXTREAL1,MESFUNC1,MESFUNC2,MESFUNC5,MESFUNC6,EXTREAL2,SUPINF_1,RELSET_1,ABIAN,BINOP_2,VALUED_0,COMSEQ_3,CKB2;
requirements NUMERALS,REAL,BOOLE,SUBSET,ARITHM;
begin
reserve X for non empty set;
reserve S for (SigmaField of X);
reserve f for (PartFunc of X,( COMPLEX ));
reserve A for (Element of S);
reserve B for (Element of S);
theorem
(( dom f ) = A implies (f is_measurable_on B iff f is_measurable_on ( A /\ B )))
proof
assume A1: ( dom f ) = A;
then A2: ( dom ( Re f ) ) = A by COMSEQ_3:def 3;
A3: ( dom ( Im f ) ) = A by A1,COMSEQ_3:def 4;
hereby
assume A4: f is_measurable_on B;
then ( Im f ) is_measurable_on B by CKB2:def 1;
then A5: ( Im f ) is_measurable_on ( A /\ B ) by A3,MESFUNC6:80;
( Re f ) is_measurable_on B by A4,CKB2:def 1;
then ( Re f ) is_measurable_on ( A /\ B ) by A2,MESFUNC6:80;
hence f is_measurable_on ( A /\ B ) by A5,CKB2:def 1;
end;
hereby
assume A6: f is_measurable_on ( A /\ B );
then ( Im f ) is_measurable_on ( A /\ B ) by CKB2:def 1;
then A7: ( Im f ) is_measurable_on B by A3,MESFUNC6:80;
( Re f ) is_measurable_on ( A /\ B ) by A6,CKB2:def 1;
then ( Re f ) is_measurable_on B by A2,MESFUNC6:80;
hence f is_measurable_on B by A7,CKB2:def 1;
end;
end;
