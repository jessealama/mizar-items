begin
reserve n for (Element of ( NAT ));
reserve m for (Element of ( NAT ));
reserve seq for Real_Sequence;
theorem
Th9: (seq is  divergent_to+infty implies ( 2 to_power seq ) is  divergent_to+infty)
proof
assume A1: seq is  divergent_to+infty;
let r being Real;
consider p being (Element of ( NAT )) such that A2: p > r by SEQ_4:3;
consider n such that A3: (for m holds (n <= m implies p < ( seq . m ))) by A1,LIMFUNC1:def 4;
take n;
let m;
assume m >= n;
then p < ( seq . m ) by A3;
then A4: ( 2 to_power p ) < ( 2 to_power ( seq . m ) ) by POWER:39;
( 2 to_power p ) = ( 2 |^ p ) by POWER:41;
then p < ( 2 to_power ( seq . m ) ) by A4,NEWTON:86,XXREAL_0:2;
then r < ( 2 to_power ( seq . m ) ) by A2,XXREAL_0:2;
hence thesis by CKB3:def 1;
end;
