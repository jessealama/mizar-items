begin
definition
let F being (ManySortedSet of ( NAT ));
func F .Lifespan() -> (Element of ( NAT ))
means
(( F . it ) = ( F . ( it + 1 ) ) & (for n being Nat holds (( F . n ) = ( F . ( n + 1 ) ) implies it <= n))) if F is  halting otherwise it = ( 0 );
existence
proof
defpred P[ Nat ]
 means
( F . $1 ) = ( F . ( $1 + 1 ) );
hereby
assume F is  halting;
then A1: (ex n being Nat st P[ n ]) by CKB115:def 1;
(ex IT being Nat st (P[ IT ] & (for n being Nat holds (P[ n ] implies IT <= n)))) from NAT_1:sch 5(A1);
then consider IT being Nat such that A2: (P[ IT ] & (for n being Nat holds (P[ n ] implies IT <= n)));
IT in ( NAT ) by ORDINAL1:def 12;
hence (ex IT being (Element of ( NAT )) st (P[ IT ] & (for n being Nat holds (P[ n ] implies IT <= n)))) by A2;
end;
thus thesis;
end;
uniqueness
proof
let IT1 being (Element of ( NAT )),IT2 being (Element of ( NAT ));
hereby
assume F is  halting;
assume A3: (( F . IT1 ) = ( F . ( IT1 + 1 ) ) & (for n being Nat holds (( F . n ) = ( F . ( n + 1 ) ) implies IT1 <= n)));
assume (( F . IT2 ) = ( F . ( IT2 + 1 ) ) & (for n being Nat holds (( F . n ) = ( F . ( n + 1 ) ) implies IT2 <= n)));
then (IT1 <= IT2 & IT2 <= IT1) by A3;
hence IT1 = IT2 by XXREAL_0:1;
end;
thus thesis;
end;
consistency;
end;
