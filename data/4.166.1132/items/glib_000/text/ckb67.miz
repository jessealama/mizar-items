theorem
Lm5: (for G1 being _Graph holds (for G2 being (Subgraph of G1) holds (for x,y,e being set holds (e Joins x,y,G2 implies e Joins x,y,G1))))
proof
let G1 being _Graph;
let G2 being (Subgraph of G1);
let x being set,y being set,e being set;
assume A1: e Joins x,y,G2;
then A2: e in ( the_Edges_of G2 ) by CKB22:def 1;
((( ( the_Source_of G2 ) . e ) = x & ( ( the_Target_of G2 ) . e ) = y) or (( ( the_Source_of G2 ) . e ) = y & ( ( the_Target_of G2 ) . e ) = x)) by A1,CKB22:def 1;
then ((( ( the_Source_of G1 ) . e ) = x & ( ( the_Target_of G1 ) . e ) = y) or (( ( the_Source_of G1 ) . e ) = y & ( ( the_Target_of G1 ) . e ) = x)) by A2,CKB62:def 1;
hence thesis by A2,CKB22:def 1;
end;
