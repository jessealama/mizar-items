begin
definition
let S being non void non empty ManySortedSign;
let X being  non-empty (ManySortedSet of (the carrier of S));
let v being (SortSymbol of S);
let e being (Element of ( (the Sorts of ( FreeMSA X )) . v ));
func depth e -> Nat
means
(ex dt being  finite DecoratedTree st (ex t being  finite Tree st ((dt = e & t = ( dom dt )) & it = ( height t ))));
existence
proof
reconsider dt = e as  finite DecoratedTree;
reconsider domdt = ( dom dt ) as  finite Tree;
take ( height domdt );
take dt;
take domdt;
thus thesis;
end;
uniqueness;
end;
