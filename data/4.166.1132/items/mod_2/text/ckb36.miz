requirements SUBSET,BOOLE,NUMERALS;
begin
reserve R for Ring;
reserve G1 for (LeftMod of R);
reserve G2 for (LeftMod of R);
reserve G3 for (LeftMod of R);
reserve f for LModMorphismStr over R;
theorem
Th24: (for G1,G2,G3,G4 being (LeftMod of R) holds (for f being  strict (Morphism of G1,G2) holds (for g being  strict (Morphism of G2,G3) holds (for h being  strict (Morphism of G3,G4) holds ( h * ( g * f ) ) = ( ( h * g ) * f )))))
proof
let G1 being (LeftMod of R);
let G2 being (LeftMod of R);
let G3 being (LeftMod of R);
let G4 being (LeftMod of R);
let f being  strict (Morphism of G1,G2);
let g being  strict (Morphism of G2,G3);
let h being  strict (Morphism of G3,G4);
consider f0 being (Function of G1,G2) such that A1: f = LModMorphismStr (# G1,G2,f0 #) by CKB26:1;
consider g0 being (Function of G2,G3) such that A2: g = LModMorphismStr (# G2,G3,g0 #) by CKB26:1;
consider h0 being (Function of G3,G4) such that A3: h = LModMorphismStr (# G3,G4,h0 #) by CKB26:1;
A4: ( h *' g ) = LModMorphismStr (# G2,G4,( h0 * g0 ) #) by A2,A3,CKB33:1;
( g *' f ) = LModMorphismStr (# G1,G3,( g0 * f0 ) #) by A1,A2,CKB33:1;
then ( h * ( g * f ) ) = LModMorphismStr (# G1,G4,( h0 * ( g0 * f0 ) ) #) by A3,CKB33:1
.= LModMorphismStr (# G1,G4,( ( h0 * g0 ) * f0 ) #) by RELAT_1:36
.= ( ( h * g ) * f ) by A1,A4,CKB33:1;
hence thesis;
end;
