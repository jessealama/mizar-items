begin
reserve R for Ring;
reserve f for LModMorphismStr over R;
theorem
(for f,g being  strict (LModMorphism of R) holds (( dom g ) = ( cod f ) implies (( dom ( g * f ) ) = ( dom f ) & ( cod ( g * f ) ) = ( cod g ))))
proof
let f being  strict (LModMorphism of R);
let g being  strict (LModMorphism of R);
assume ( dom g ) = ( cod f );
then A1: (ex G1,G2,G3 being (LeftMod of R) st (ex f0 being (Function of G1,G2) st (ex g0 being (Function of G2,G3) st ((f = LModMorphismStr (# G1,G2,f0 #) & g = LModMorphismStr (# G2,G3,g0 #)) & ( g * f ) = LModMorphismStr (# G1,G3,( g0 * f0 ) #))))) by CKB34:1;
hence ( dom ( g * f ) ) = ( dom f );
thus thesis by A1;
end;
