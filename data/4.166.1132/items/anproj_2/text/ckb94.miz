environ
vocabularies RLVECT_1,SUBSET_1,REAL_1,RELAT_1,ARYTM_3,SUPINF_2,CARD_1,XCMPLX_0,ANPROJ_1,ARYTM_1,XBOOLE_0,FUNCT_2,NUMBERS,FUNCT_1,FUNCSDOM,ZFMISC_1,INCSP_1,COLLSP,RELAT_2,PARSP_2,VECTSP_1,AFF_2,ANALOAF,ANPROJ_2;
notations XBOOLE_0,SUBSET_1,NUMBERS,FUNCT_1,FUNCT_2,DOMAIN_1,STRUCT_0,RLVECT_1,REAL_1,FUNCSDOM,COLLSP,ANPROJ_1,CKB13,CKB14,CKB15,CKB27,CKB28,CKB59,CKB62,CKB63,CKB64,CKB65,CKB81,CKB82,CKB83,CKB84,CKB85,CKB86,CKB87;
definitions COLLSP,ANPROJ_1,BINOP_1,FUNCSDOM,RLVECT_1,STRUCT_0,ALGSTR_0,CKB13,CKB14,CKB15,CKB27,CKB28,CKB59,CKB62,CKB63,CKB64,CKB65,CKB82,CKB83,CKB84,CKB85,CKB87;
theorems RLVECT_1,FUNCT_2,FUNCSDOM,COLLSP,ANPROJ_1,ENUMSET1,XCMPLX_0,XCMPLX_1,FUNCOP_1,STRUCT_0,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB37,CKB38,CKB39,CKB40,CKB41,CKB42,CKB43,CKB44,CKB45,CKB46,CKB47,CKB48,CKB49,CKB50,CKB51,CKB52,CKB53,CKB54,CKB55,CKB56,CKB57,CKB58,CKB59,CKB62,CKB63,CKB64,CKB65,CKB66,CKB67,CKB68,CKB70,CKB71,CKB72,CKB73,CKB75,CKB77,CKB78,CKB82,CKB83,CKB84,CKB85,CKB87,CKB88,CKB89,CKB90,CKB91,CKB93;
schemes FUNCT_2;
registrations XBOOLE_0,SUBSET_1,RELSET_1,NUMBERS,STRUCT_0,COLLSP,REAL_1,ANPROJ_1,XREAL_0,ORDINAL1,CKB60,CKB61,CKB69,CKB74,CKB76,CKB79,CKB80,CKB92;
constructors DOMAIN_1,REAL_1,FUNCSDOM,BINOP_2,ANPROJ_1,RELSET_1,CKB13,CKB14,CKB15,CKB27,CKB28,CKB59,CKB64,CKB65,CKB82,CKB83,CKB84,CKB85,CKB87;
requirements NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve A for non empty set;
reserve x1 for (Element of A);
reserve x2 for (Element of A);
reserve V for non trivial RealLinearSpace;
reserve p for (Element of ( ProjectiveSpace V ));
reserve p1 for (Element of ( ProjectiveSpace V ));
reserve q for (Element of ( ProjectiveSpace V ));
reserve q1 for (Element of ( ProjectiveSpace V ));
reserve r for (Element of ( ProjectiveSpace V ));
reserve r1 for (Element of ( ProjectiveSpace V ));
reserve r2 for (Element of ( ProjectiveSpace V ));
theorem
Th30: ((ex x1,x2 being (Element of ( ProjectiveSpace V )) st (x1 <> x2 & (for r1 holds (for r2 holds (ex q st (x1,x2,q is_collinear  & r1,r2,q is_collinear )))))) implies (for p holds (for p1 holds (for q holds (for q1 holds (ex r st (p,p1,r is_collinear  & q,q1,r is_collinear )))))))
proof
given x1 being (Element of ( ProjectiveSpace V )),x2 being (Element of ( ProjectiveSpace V )) such that
A1: x1 <> x2
and
A2: (for r1 holds (for r2 holds (ex q st (x1,x2,q is_collinear  & r1,r2,q is_collinear ))));

let p;
let p1;
let q;
let q1;
consider p3 being (Element of ( ProjectiveSpace V )) such that A3: x1,x2,p3 is_collinear  and A4: p,p1,p3 is_collinear  by A2;
consider q3 being (Element of ( ProjectiveSpace V )) such that A5: x1,x2,q3 is_collinear  and A6: q,q1,q3 is_collinear  by A2;
consider s2 being (Element of ( ProjectiveSpace V )) such that A7: x1,x2,s2 is_collinear  and A8: p,q,s2 is_collinear  by A2;
A9: s2,p,q is_collinear  by A8,CKB70:1;
consider s4 being (Element of ( ProjectiveSpace V )) such that A10: x1,x2,s4 is_collinear  and A11: p,q1,s4 is_collinear  by A2;
A12: s4,q1,p is_collinear  by A11,CKB70:1;
p3,s2,q3 is_collinear  by A1,A3,A5,A7,CKB63:def 1;
then consider s3 being (Element of ( ProjectiveSpace V )) such that A13: p3,p,s3 is_collinear  and A14: q3,q,s3 is_collinear  by A9,CKB64:def 1;
consider s being (Element of ( ProjectiveSpace V )) such that A15: x1,x2,s is_collinear  and A16: p1,q1,s is_collinear  by A2;
q3,s4,p3 is_collinear  by A1,A3,A5,A10,CKB63:def 1;
then consider s5 being (Element of ( ProjectiveSpace V )) such that A17: q3,q1,s5 is_collinear  and A18: p3,p,s5 is_collinear  by A12,CKB64:def 1;
A19: p1,s,q1 is_collinear  by A16,CKB70:1;
consider s6 being (Element of ( ProjectiveSpace V )) such that A20: x1,x2,s6 is_collinear  and A21: p1,q,s6 is_collinear  by A2;
A22: s6,p1,q is_collinear  by A21,CKB70:1;
p3,s6,q3 is_collinear  by A1,A3,A5,A20,CKB63:def 1;
then consider s7 being (Element of ( ProjectiveSpace V )) such that A23: p3,p1,s7 is_collinear  and A24: q3,q,s7 is_collinear  by A22,CKB64:def 1;
s,p3,q3 is_collinear  by A1,A3,A5,A15,CKB63:def 1;
then consider s1 being (Element of ( ProjectiveSpace V )) such that A25: p1,p3,s1 is_collinear  and A26: q1,q3,s1 is_collinear  by A19,CKB64:def 1;
A27:now
A28:now
A29: q3,q1,s1 is_collinear  by A26,CKB70:1;
assume that
A30: p3 <> p1
and
A31: q3 <> q1;
(q3,q1,q is_collinear  & q3,q1,q1 is_collinear ) by A6,CKB62:def 1,CKB70:1;
then A32: q,q1,s1 is_collinear  by A31,A29,CKB63:def 1;
take s1;
A33: p3,p1,s1 is_collinear  by A25,CKB70:1;
(p3,p1,p is_collinear  & p3,p1,p1 is_collinear ) by A4,CKB62:def 1,CKB70:1;
then p,p1,s1 is_collinear  by A30,A33,CKB63:def 1;
hence thesis by A32;
end;
A34:now
assume that
A35: p3 <> p
and
A36: q3 <> q;
take s3;
(q3,q,q is_collinear  & q3,q,q1 is_collinear ) by A6,CKB62:def 1,CKB70:1;
then A37: q,q1,s3 is_collinear  by A14,A36,CKB63:def 1;
(p3,p,p is_collinear  & p3,p,p1 is_collinear ) by A4,CKB62:def 1,CKB70:1;
then p,p1,s3 is_collinear  by A13,A35,CKB63:def 1;
hence thesis by A37;
end;
A38:now
assume that
A39: p3 <> p1
and
A40: q3 <> q;
take s7;
(q3,q,q is_collinear  & q3,q,q1 is_collinear ) by A6,CKB62:def 1,CKB70:1;
then A41: q,q1,s7 is_collinear  by A24,A40,CKB63:def 1;
(p3,p1,p is_collinear  & p3,p1,p1 is_collinear ) by A4,CKB62:def 1,CKB70:1;
then p,p1,s7 is_collinear  by A23,A39,CKB63:def 1;
hence thesis by A41;
end;
A42:now
assume that
A43: p3 <> p
and
A44: q3 <> q1;
take s5;
(q3,q1,q is_collinear  & q3,q1,q1 is_collinear ) by A6,CKB62:def 1,CKB70:1;
then A45: q,q1,s5 is_collinear  by A17,A44,CKB63:def 1;
(p3,p,p is_collinear  & p3,p,p1 is_collinear ) by A4,CKB62:def 1,CKB70:1;
then p,p1,s5 is_collinear  by A18,A43,CKB63:def 1;
hence thesis by A45;
end;
assume (p <> p1 & q <> q1);
hence thesis by A34,A42,A38,A28;
end;
now
A46:now
assume A47: p = p1;
take q3;
p,p1,q3 is_collinear  by A47,CKB62:def 1;
hence thesis by A6;
end;
A48:now
assume A49: q = q1;
take p3;
q,q1,p3 is_collinear  by A49,CKB62:def 1;
hence thesis by A4;
end;
assume (p = p1 or q = q1);
hence thesis by A48,A46;
end;
hence thesis by A27;
end;
