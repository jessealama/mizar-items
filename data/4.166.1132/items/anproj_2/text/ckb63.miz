begin
definition
let CS being non empty CollStr;
redefine attr CS is  transitive
means
:Def8: (for p,q,r,r1,r2 being (Element of CS) holds ((((p <> q & p,q,r is_collinear ) & p,q,r1 is_collinear ) & p,q,r2 is_collinear ) implies r,r1,r2 is_collinear ));
compatibility
proof
hereby
assume A4: CS is  transitive;
let p being (Element of CS),q being (Element of CS),r being (Element of CS),r1 being (Element of CS),r2 being (Element of CS);
assume that
A5: p <> q
and
A6: (p,q,r is_collinear  & p,q,r1 is_collinear )
and
A7: p,q,r2 is_collinear ;
A8: [ p,q,r2 ] in (the Collinearity of CS) by A7,COLLSP:def 2;
([ p,q,r ] in (the Collinearity of CS) & [ p,q,r1 ] in (the Collinearity of CS)) by A6,COLLSP:def 2;
then [ r,r1,r2 ] in (the Collinearity of CS) by A4,A5,A8,COLLSP:def 4;
hence r,r1,r2 is_collinear  by COLLSP:def 2;
end;
assume A9: (for p,q,r,r1,r2 being (Element of CS) holds ((((p <> q & p,q,r is_collinear ) & p,q,r1 is_collinear ) & p,q,r2 is_collinear ) implies r,r1,r2 is_collinear ));
let p being (Element of CS),q being (Element of CS),r being (Element of CS),r1 being (Element of CS),r2 being (Element of CS);
assume that
A10: p <> q
and
A11: ([ p,q,r ] in (the Collinearity of CS) & [ p,q,r1 ] in (the Collinearity of CS))
and
A12: [ p,q,r2 ] in (the Collinearity of CS);
A13: p,q,r2 is_collinear  by A12,COLLSP:def 2;
(p,q,r is_collinear  & p,q,r1 is_collinear ) by A11,COLLSP:def 2;
then r,r1,r2 is_collinear  by A9,A10,A13;
hence thesis by COLLSP:def 2;
end;
