scheme MaxFinSetElem { X() ->  finite set,P[set,set] } : (ex x st (x in X() & (for y holds (y in X() implies P[ x,y ]))))
provided
A1: X() <> ( {} )
and
A2: (for x holds (for y holds (P[ x,y ] or P[ y,x ])))
and
A3: (for x holds (for y holds (for z holds ((P[ x,y ] & P[ y,z ]) implies P[ x,z ]))))
proof
defpred Q[ Nat ]
 means
(for X being  finite set holds ((( card X ) = $1 & X <> ( {} )) implies (ex x st (x in X & (for y holds (y in X implies P[ x,y ]))))));
A4: Q[ ( 0 ) ];
A5: (Q[ n ] implies Q[ ( n + 1 ) ])
proof
assume A6: (for X being  finite set holds ((( card X ) = n & X <> ( {} )) implies (ex x st (x in X & (for y holds (y in X implies P[ x,y ]))))));
let X being  finite set;
assume that
A7: ( card X ) = ( n + 1 )
and
A8: X <> ( {} );
set x = the (Element of X);
A9:now
assume ( X \ { x } ) = ( {} );
then A10: X c= { x } by XBOOLE_1:37;
thus thesis
proof
take x;
thus x in X by A8;
let y;
assume y in X;
then y = x by A10,TARSKI:def 1;
hence thesis by A2;
end;

end;
now
assume A11: ( X \ { x } ) <> ( {} );
{ x } c= X by A8,ZFMISC_1:31;
then A12: ( card ( X \ { x } ) ) = ( ( n + 1 ) - ( card { x } ) ) by A7,CKB52:1;
( card { x } ) = 1 by CARD_1:30;
then consider y such that A13: y in ( X \ { x } ) and A14: (for z holds (z in ( X \ { x } ) implies P[ y,z ])) by A6,A11,A12;
A15: (P[ x,y ] or P[ y,x ]) by A2;
A16: P[ x,x ] by A2;
P[ y,y ] by A2;
then consider z such that A17: (z = x or z = y) and A18: P[ z,x ] and A19: P[ z,y ] by A15,A16;
thus thesis
proof
take z;
thus z in X by A13,A17;
let u;
A20: (u in { x } or (not u in { x }));
assume u in X;
then (u = x or u in ( X \ { x } )) by A20,TARSKI:def 1,XBOOLE_0:def 5;
then (P[ z,u ] or P[ y,u ]) by A14,A18;
hence thesis by A3,A19;
end;

end;
hence thesis by A9;
end;
