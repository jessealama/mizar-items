requirements NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve x for set;
reserve X for set;
reserve Y for set;
reserve f for Function;
deffunc g(set) = ( $1 `1 );
deffunc plus(set,set) = ( [: $1,{ ( 0 ) } :] \/ [: $2,{ 1 } :] );
deffunc plus(set,set) = ( [: $1,{ ( 0 ) } :] \/ [: $2,{ 1 } :] );
deffunc plus(set,set,set,set) = ( [: $1,{ $3 } :] \/ [: $2,{ $4 } :] );
theorem
Th47: ( card ( X \/ Y ) ) c= ( ( card X ) +` ( card Y ) )
proof
consider f such that A1: (( dom f ) = plus(X,Y) & (for x holds (x in plus(X,Y) implies ( f . x ) = g(x)))) from FUNCT_1:sch 3;
( X \/ Y ) c= ( rng f )
proof
let x;
assume x in ( X \/ Y );
then A2: (x in X or x in Y) by XBOOLE_0:def 3;
per cases ;
suppose x in X;

then [ x,( 0 ) ] in [: X,{ ( 0 ) } :] by ZFMISC_1:106;
then A3: [ x,( 0 ) ] in plus(X,Y) by XBOOLE_0:def 3;
( [ x,( 0 ) ] `1 ) = x by MCART_1:7;
then x = ( f . [ x,( 0 ) ] ) by A1,A3;
hence thesis by A1,A3,FUNCT_1:def 3;
end;
suppose (not x in X);

then [ x,1 ] in [: Y,{ 1 } :] by A2,ZFMISC_1:106;
then A4: [ x,1 ] in plus(X,Y) by XBOOLE_0:def 3;
( [ x,1 ] `1 ) = x by MCART_1:7;
then x = ( f . [ x,1 ] ) by A1,A4;
hence thesis by A1,A4,FUNCT_1:def 3;
end;
end;
then ( card ( X \/ Y ) ) c= ( card plus(X,Y) ) by A1,CARD_1:12;
hence thesis by CKB24:1;
end;
