begin
reserve M for Cardinal;
reserve N for Cardinal;
deffunc plus(set,set) = ( [: $1,{ ( 0 ) } :] \/ [: $2,{ 1 } :] );
definition
let M;
let N;
func M +` N -> Cardinal equals 
( card ( M +^ N ) );
coherence;
commutativity
proof
let C being Cardinal;
let M;
let N;
assume C = ( card ( M +^ N ) );
hence C = ( card plus(N,M) ) by CKB4:1
.= ( card ( N +^ M ) ) by CKB4:1;
end;
end;
