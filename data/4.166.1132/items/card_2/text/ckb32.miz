requirements NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve K for Cardinal;
reserve M for Cardinal;
reserve N for Cardinal;
deffunc plus(set,set) = ( [: $1,{ ( 0 ) } :] \/ [: $2,{ 1 } :] );
deffunc plus(set,set) = ( [: $1,{ ( 0 ) } :] \/ [: $2,{ 1 } :] );
deffunc plus(set,set,set,set) = ( [: $1,{ $3 } :] \/ [: $2,{ $4 } :] );
theorem
Th37: ( K *` ( M +` N ) ) = ( ( K *` M ) +` ( K *` N ) )
proof
A1: [: ( card [: K,M :] ),{ ( 0 ) } :],[: [: K,M :],{ ( 0 ) } :] are_equipotent  by CKB14:1;
M,[: M,{ ( 0 ) } :] are_equipotent  by CKB12:1;
then A2: [: K,M :],[: K,[: M,{ ( 0 ) } :] :] are_equipotent  by CKB15:1;
[: [: K,M :],{ ( 0 ) } :],[: K,M :] are_equipotent  by CKB12:1;
then [: [: K,M :],{ ( 0 ) } :],[: K,[: M,{ ( 0 ) } :] :] are_equipotent  by A2,WELLORD2:15;
then A3: [: ( card [: K,M :] ),{ ( 0 ) } :],[: K,[: M,{ ( 0 ) } :] :] are_equipotent  by A1,WELLORD2:15;
A4: [: ( card [: K,N :] ),{ 1 } :],[: [: K,N :],{ 1 } :] are_equipotent  by CKB14:1;
[: M,{ ( 0 ) } :] misses [: N,{ 1 } :] by CKB26:1;
then A5: [: K,[: M,{ ( 0 ) } :] :] misses [: K,[: N,{ 1 } :] :] by ZFMISC_1:104;
N,[: N,{ 1 } :] are_equipotent  by CKB12:1;
then A6: [: K,N :],[: K,[: N,{ 1 } :] :] are_equipotent  by CKB15:1;
A7: ( K *` ( M +` N ) ) = ( card [: K,( card plus(M,N) ) :] ) by CKB17:1
.= ( card [: K,plus(M,N) :] ) by CKB14:1
.= ( card ( [: K,[: M,{ ( 0 ) } :] :] \/ [: K,[: N,{ 1 } :] :] ) ) by ZFMISC_1:97;
[: [: K,N :],{ 1 } :],[: K,N :] are_equipotent  by CKB12:1;
then [: [: K,N :],{ 1 } :],[: K,[: N,{ 1 } :] :] are_equipotent  by A6,WELLORD2:15;
then A8: [: ( card [: K,N :] ),{ 1 } :],[: K,[: N,{ 1 } :] :] are_equipotent  by A4,WELLORD2:15;
[: ( card [: K,M :] ),{ ( 0 ) } :] misses [: ( card [: K,N :] ),{ 1 } :] by CKB26:1;
then ( [: ( card [: K,M :] ),{ ( 0 ) } :] \/ [: ( card [: K,N :] ),{ 1 } :] ),( [: K,[: M,{ ( 0 ) } :] :] \/ [: K,[: N,{ 1 } :] :] ) are_equipotent  by A3,A8,A5,CARD_1:31;
hence ( K *` ( M +` N ) ) = ( card ( [: ( card [: K,M :] ),{ ( 0 ) } :] \/ [: ( card [: K,N :] ),{ 1 } :] ) ) by A7,CARD_1:5
.= ( ( K *` M ) +` ( K *` N ) ) by CKB17:1;
end;
