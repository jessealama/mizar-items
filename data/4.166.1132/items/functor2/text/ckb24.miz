begin
definition
let A being category;
let B being category;
let F being  covariant (Functor of A,B);
let F1 being  covariant (Functor of A,B);
let F2 being  covariant (Functor of A,B);
let t1 being (natural_transformation of F,F1);
let t2 being (natural_transformation of F1,F2);
assume that
A1: (F is_naturally_transformable_to F1 & F1 is_naturally_transformable_to F2);
func t2 `*` t1 -> (natural_transformation of F,F2)
means
:Def8: it = ( t2 `*` t1 );
existence
proof
A2: F is_naturally_transformable_to F2 by A1,CKB21:1;
A3: ((for a,b being (object of A) holds (<^ a,b ^> <> ( {} ) implies (for f being (Morphism of a,b) holds ( ( t1 ! b ) * ( F . f ) ) = ( ( F1 . f ) * ( t1 ! a ) )))) & (for a,b being (object of A) holds (<^ a,b ^> <> ( {} ) implies (for f being (Morphism of a,b) holds ( ( t2 ! b ) * ( F1 . f ) ) = ( ( F2 . f ) * ( t2 ! a ) ))))) by A1,CKB22:def 1;
(F is_transformable_to F1 & F1 is_transformable_to F2) by A1,CKB18:def 1;
then (for a,b being (object of A) holds (<^ a,b ^> <> ( {} ) implies (for f being (Morphism of a,b) holds ( ( ( t2 `*` t1 ) ! b ) * ( F . f ) ) = ( ( F2 . f ) * ( ( t2 `*` t1 ) ! a ) )))) by A3,CKB20:1;
then ( t2 `*` t1 ) is (natural_transformation of F,F2) by A2,CKB22:def 1;
hence thesis;
end;
correctness;
end;
