begin
theorem
Lm1: (for A,B being  transitive  with_units non empty AltCatStr holds (for F,G being  covariant (Functor of A,B) holds (for a,b being (object of A) holds (<^ a,b ^> <> ( {} ) implies (for f being (Morphism of a,b) holds ( ( ( idt F ) ! b ) * ( F . f ) ) = ( ( F . f ) * ( ( idt F ) ! a ) ))))))
proof
let A being  transitive  with_units non empty AltCatStr,B being  transitive  with_units non empty AltCatStr;
let F being  covariant (Functor of A,B),G being  covariant (Functor of A,B);
let a being (object of A),b being (object of A);
assume that
A1: <^ a,b ^> <> ( {} );
let f being (Morphism of a,b);
A2: <^ a,a ^> <> ( {} ) by ALTCAT_2:def 7;
A3: <^ b,b ^> <> ( {} ) by ALTCAT_2:def 7;
thus ( ( ( idt F ) ! b ) * ( F . f ) ) = ( ( idm ( F . b ) ) * ( F . f ) ) by CKB14:1
.= ( ( F . ( idm b ) ) * ( F . f ) ) by CKB6:1
.= ( F . ( ( idm b ) * f ) ) by A1,A3,FUNCTOR0:def 23
.= ( F . f ) by A1,ALTCAT_1:20
.= ( F . ( f * ( idm a ) ) ) by A1,ALTCAT_1:def 17
.= ( ( F . f ) * ( F . ( idm a ) ) ) by A1,A2,FUNCTOR0:def 23
.= ( ( F . f ) * ( idm ( F . a ) ) ) by CKB6:1
.= ( ( F . f ) * ( ( idt F ) ! a ) ) by CKB14:1;
end;
