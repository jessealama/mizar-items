begin
theorem
(for A,B being  transitive  with_units non empty AltCatStr holds (for F,F1,F2 being  covariant (Functor of A,B) holds ((F is_naturally_transformable_to F1 & F1 is_naturally_transformable_to F2) implies (for t1 being (natural_transformation of F,F1) holds (for t2 being (natural_transformation of F1,F2) holds (for a being (object of A) holds ( ( t2 `*` t1 ) ! a ) = ( ( t2 ! a ) * ( t1 ! a ) )))))))
proof
let A being  transitive  with_units non empty AltCatStr,B being  transitive  with_units non empty AltCatStr;
let F being  covariant (Functor of A,B),F1 being  covariant (Functor of A,B),F2 being  covariant (Functor of A,B);
assume A1: (F is_naturally_transformable_to F1 & F1 is_naturally_transformable_to F2);
let t1 being (natural_transformation of F,F1);
let t2 being (natural_transformation of F1,F2);
let a being (object of A);
(F is_transformable_to F1 & F1 is_transformable_to F2) by A1,CKB18:def 1;
hence thesis by CKB12:def 1;
end;
