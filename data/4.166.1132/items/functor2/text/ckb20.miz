begin
theorem
Lm2: (for A,B being category holds (for F,F1,F2 being  covariant (Functor of A,B) holds ((F is_transformable_to F1 & F1 is_transformable_to F2) implies (for t1 being (transformation of F,F1) holds ((for a,b being (object of A) holds (<^ a,b ^> <> ( {} ) implies (for f being (Morphism of a,b) holds ( ( t1 ! b ) * ( F . f ) ) = ( ( F1 . f ) * ( t1 ! a ) )))) implies (for t2 being (transformation of F1,F2) holds ((for a,b being (object of A) holds (<^ a,b ^> <> ( {} ) implies (for f being (Morphism of a,b) holds ( ( t2 ! b ) * ( F1 . f ) ) = ( ( F2 . f ) * ( t2 ! a ) )))) implies (for a,b being (object of A) holds (<^ a,b ^> <> ( {} ) implies (for f being (Morphism of a,b) holds ( ( ( t2 `*` t1 ) ! b ) * ( F . f ) ) = ( ( F2 . f ) * ( ( t2 `*` t1 ) ! a ) )))))))))))
proof
let A being category,B being category;
let F being  covariant (Functor of A,B),F1 being  covariant (Functor of A,B),F2 being  covariant (Functor of A,B);
assume that
A1: F is_transformable_to F1
and
A2: F1 is_transformable_to F2;
let t1 being (transformation of F,F1);
assume that
A3: (for a,b being (object of A) holds (<^ a,b ^> <> ( {} ) implies (for f being (Morphism of a,b) holds ( ( t1 ! b ) * ( F . f ) ) = ( ( F1 . f ) * ( t1 ! a ) ))));
let t2 being (transformation of F1,F2);
assume that
A4: (for a,b being (object of A) holds (<^ a,b ^> <> ( {} ) implies (for f being (Morphism of a,b) holds ( ( t2 ! b ) * ( F1 . f ) ) = ( ( F2 . f ) * ( t2 ! a ) ))));
let a being (object of A),b being (object of A);
A5: <^ ( F . b ),( F1 . b ) ^> <> ( {} ) by A1,CKB7:def 1;
A6: <^ ( F . a ),( F1 . a ) ^> <> ( {} ) by A1,CKB7:def 1;
A7: <^ ( F1 . b ),( F2 . b ) ^> <> ( {} ) by A2,CKB7:def 1;
A8: <^ ( F1 . a ),( F2 . a ) ^> <> ( {} ) by A2,CKB7:def 1;
assume A9: <^ a,b ^> <> ( {} );
then A10: <^ ( F . a ),( F . b ) ^> <> ( {} ) by FUNCTOR0:def 18;
let f being (Morphism of a,b);
A11: <^ ( F2 . a ),( F2 . b ) ^> <> ( {} ) by A9,FUNCTOR0:def 18;
A12: <^ ( F1 . a ),( F1 . b ) ^> <> ( {} ) by A9,FUNCTOR0:def 18;
thus ( ( ( t2 `*` t1 ) ! b ) * ( F . f ) ) = ( ( ( t2 ! b ) * ( t1 ! b ) ) * ( F . f ) ) by A1,A2,CKB12:def 1
.= ( ( t2 ! b ) * ( ( t1 ! b ) * ( F . f ) ) ) by A10,A5,A7,ALTCAT_1:21
.= ( ( t2 ! b ) * ( ( F1 . f ) * ( t1 ! a ) ) ) by A3,A9
.= ( ( ( t2 ! b ) * ( F1 . f ) ) * ( t1 ! a ) ) by A6,A7,A12,ALTCAT_1:21
.= ( ( ( F2 . f ) * ( t2 ! a ) ) * ( t1 ! a ) ) by A4,A9
.= ( ( F2 . f ) * ( ( t2 ! a ) * ( t1 ! a ) ) ) by A6,A8,A11,ALTCAT_1:21
.= ( ( F2 . f ) * ( ( t2 `*` t1 ) ! a ) ) by A1,A2,CKB12:def 1;
end;
