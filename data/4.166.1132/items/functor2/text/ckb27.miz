begin
theorem
(for A,B being category holds (for F,F1,F2,F3 being  covariant (Functor of A,B) holds (for t being (natural_transformation of F,F1) holds (for t1 being (natural_transformation of F1,F2) holds (((F is_naturally_transformable_to F1 & F1 is_naturally_transformable_to F2) & F2 is_naturally_transformable_to F3) implies (for t3 being (natural_transformation of F2,F3) holds ( ( t3 `*` t1 ) `*` t ) = ( t3 `*` ( t1 `*` t ) )))))))
proof
let A being category,B being category;
let F being  covariant (Functor of A,B),F1 being  covariant (Functor of A,B),F2 being  covariant (Functor of A,B),F3 being  covariant (Functor of A,B);
let t being (natural_transformation of F,F1);
let t1 being (natural_transformation of F1,F2);
assume that
A1: F is_naturally_transformable_to F1
and
A2: F1 is_naturally_transformable_to F2
and
A3: F2 is_naturally_transformable_to F3;
A4: F is_naturally_transformable_to F2 by A1,A2,CKB21:1;
let t3 being (natural_transformation of F2,F3);
A5: F2 is_transformable_to F3 by A3,CKB18:def 1;
A6: (F is_transformable_to F1 & F1 is_transformable_to F2) by A1,A2,CKB18:def 1;
F1 is_naturally_transformable_to F3 by A2,A3,CKB21:1;
hence ( ( t3 `*` t1 ) `*` t ) = ( ( t3 `*` t1 ) `*` (t qua (transformation of F,F1)) ) by A1,CKB24:def 1
.= ( ( (t3 qua (transformation of F2,F3)) `*` t1 ) `*` t ) by A2,A3,CKB24:def 1
.= ( t3 `*` ( t1 `*` (t qua (transformation of F,F1)) ) ) by A6,A5,CKB16:1
.= ( (t3 qua (transformation of F2,F3)) `*` ( t1 `*` t ) ) by A1,A2,CKB24:def 1
.= ( t3 `*` ( t1 `*` t ) ) by A3,A4,CKB24:def 1;
end;
