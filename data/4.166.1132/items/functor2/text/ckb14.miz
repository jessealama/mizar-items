theorem
Th6: (for A,B being  transitive  with_units non empty AltCatStr holds (for F being (Functor of A,B) holds (for a being (object of A) holds ( ( idt F ) ! a ) = ( idm ( F . a ) ))))
proof
let A being  transitive  with_units non empty AltCatStr,B being  transitive  with_units non empty AltCatStr;
let F being (Functor of A,B);
let a being (object of A);
thus ( ( idt F ) ! a ) = ( (( idt F ) qua (ManySortedSet of (the carrier of A))) . a ) by CKB11:def 1
.= ( idm ( F . a ) ) by CKB10:def 1;
end;
