begin
definition
let R being RelStr;
let X being (Subset of R);
redefine attr X is  lower
means
:Def1: (for x,y being set holds ((x in X & [ y,x ] in (the InternalRel of R)) implies y in X));
compatibility
proof
hereby
assume A1: X is  lower;
let x being set;
let y being set;
assume that
A2: x in X
and
A3: [ y,x ] in (the InternalRel of R);
reconsider x9 = x,y9 = y as (Element of R) by A3,ZFMISC_1:87;
y9 <= x9 by A3,ORDERS_2:def 5;
hence y in X by A1,A2,WAYBEL_0:def 19;
end;
assume A4: (for x,y being set holds ((x in X & [ y,x ] in (the InternalRel of R)) implies y in X));
let x being (Element of R);
let y being (Element of R);
assume that
A5: x in X;
assume y <= x;
then [ y,x ] in (the InternalRel of R) by ORDERS_2:def 5;
hence thesis by A4,A5;
end;
end;
