begin
theorem
Th10: (for R being non  empty RelStr holds (for x being (Element of R) holds (( (the InternalRel of R) -Seg x ) c= ( well_founded-Part R ) implies x in ( well_founded-Part R ))))
proof
let R being non  empty RelStr;
let x being (Element of R);
set wfp = ( well_founded-Part R );
set IT = { S where S is (Subset of R): S is  well_founded  lower };
set xwfp = ( wfp \/ { x } );
A1: wfp = ( union IT ) by CKB14:def 1;
x in { x } by TARSKI:def 1;
then A2: x in xwfp by XBOOLE_0:def 3;
reconsider xwfp as (Subset of R);
{ x } is  well_founded  well_founded (Subset of R) by CKB16:1;
then A3: xwfp is  well_founded by CKB17:1;
assume ( (the InternalRel of R) -Seg x ) c= wfp;
then xwfp is  lower by CKB9:1;
then xwfp in IT by A3;
hence thesis by A1,A2,TARSKI:def 4;
end;
