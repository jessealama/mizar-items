begin
reserve x for set;
theorem
Th8: (for Y being set holds (for X being (Subset of ( EqRelLatt Y )) holds (for R being (Relation of Y) holds (R = ( union X ) implies ( "\/" X ) = ( EqCl R )))))
proof
let Y being set;
let X being (Subset of ( EqRelLatt Y ));
let R being (Relation of Y);
reconsider X1 = ( "\/" X ) as (Equivalence_Relation of Y) by MSUALG_5:21;
assume A1: R = ( union X );
A2:now
let EqR being (Equivalence_Relation of Y);
reconsider EqR1 = EqR as (Element of ( EqRelLatt Y )) by MSUALG_5:21;
assume A3: R c= EqR;
now
let q being (Element of ( EqRelLatt Y ));
reconsider q1 = q as (Equivalence_Relation of Y) by MSUALG_5:21;
assume A4: q in X;
now
let x;
assume x in q1;
then x in ( union X ) by A4,TARSKI:def 4;
hence x in EqR by A1,A3;
end;
then q1 c= EqR by TARSKI:def 3;
hence q [= EqR1 by CKB3:1;
end;
then X is_less_than EqR1 by LATTICE3:def 17;
then ( "\/" X ) [= EqR1 by LATTICE3:def 21;
hence X1 c= EqR by CKB3:1;
end;
R c= ( "\/" X ) by A1,CKB10:1;
hence thesis by A2,MSUALG_5:def 1;
end;
