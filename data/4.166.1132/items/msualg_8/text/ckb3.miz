requirements NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve Y for set;
reserve S for non  void non  empty ManySortedSign;
reserve A for  non-empty MSAlgebra over S;
theorem
Th2: (for a,b being (Element of ( EqRelLatt Y )) holds (for A,B being (Equivalence_Relation of Y) holds ((a = A & b = B) implies (a [= b iff A c= B))))
proof
let a being (Element of ( EqRelLatt Y ));
let b being (Element of ( EqRelLatt Y ));
let A being (Equivalence_Relation of Y);
let B being (Equivalence_Relation of Y);
assume that
A1: a = A
and
A2: b = B;
thus (a [= b implies A c= B)
proof
assume A3: a [= b;
( A /\ B ) = ( (the L_meet of ( EqRelLatt Y )) . (A,B) ) by MSUALG_5:def 2
.= ( a "/\" b ) by A1,A2,LATTICES:def 2
.= A by A1,A3,LATTICES:4;
hence thesis by XBOOLE_1:17;
end;

thus (A c= B implies a [= b)
proof
assume A4: A c= B;
( a "/\" b ) = ( (the L_meet of ( EqRelLatt Y )) . (A,B) ) by A1,A2,LATTICES:def 2
.= ( A /\ B ) by MSUALG_5:def 2
.= a by A1,A4,XBOOLE_1:28;
hence thesis by LATTICES:4;
end;

end;
