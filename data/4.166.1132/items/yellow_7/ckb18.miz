theorem
Th13: (for L being non empty RelStr holds (for X being set holds (( ex_inf_of X,L or  ex_sup_of X,( L opp )) implies ( "/\" (X,L) ) = ( "\/" (X,( L opp )) ))))
proof
let L being non empty RelStr;
let X being set;
assume A1: ( ex_inf_of X,L or  ex_sup_of X,( L opp ));
then A2:  ex_inf_of X,L by CKB16:1;
then ( "/\" (X,L) ) is_<=_than X by YELLOW_0:def 10;
then A3: ( ( "/\" (X,L) ) ~ ) is_>=_than X by CKB13:1;
A4:now
let x being (Element of ( L opp ));
assume x is_>=_than X;
then ( ~ x ) is_<=_than X by CKB14:1;
then ( ~ x ) <= ( "/\" (X,L) ) by A2,YELLOW_0:def 10;
hence x >= ( ( "/\" (X,L) ) ~ ) by CKB3:1;
end;
 ex_sup_of X,( L opp ) by A1,CKB16:1;
hence thesis by A3,A4,YELLOW_0:def 9;
end;
