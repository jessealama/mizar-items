theorem
Th6: (for L being RelStr holds (L is  transitive iff ( L opp ) is  transitive))
proof
let L being RelStr;
thus (L is  transitive implies ( L opp ) is  transitive)
proof
assume A1: L is  transitive;
let x being (Element of ( L opp )),y being (Element of ( L opp )),z being (Element of ( L opp ));
assume (x <= y & z >= y);
then (( ~ x ) >= ( ~ y ) & ( ~ z ) <= ( ~ y )) by CKB2:1;
then ( ~ x ) >= ( ~ z ) by A1,YELLOW_0:def 2;
hence thesis by CKB2:1;
end;

assume A2: ( L opp ) is  transitive;
let x being (Element of L),y being (Element of L),z being (Element of L);
assume (x <= y & z >= y);
then (( x ~ ) >= ( y ~ ) & ( z ~ ) <= ( y ~ )) by LATTICE3:9;
then ( x ~ ) >= ( z ~ ) by A2,YELLOW_0:def 2;
hence thesis by LATTICE3:9;
end;
