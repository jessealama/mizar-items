begin
theorem
Th10: (for L being RelStr holds (for X being set holds ( ex_sup_of X,L iff  ex_inf_of X,( L opp ))))
proof
let L being RelStr;
let X being set;
hereby
assume  ex_sup_of X,L;
then consider a being (Element of L) such that A1: X is_<=_than a and A2: (for b being (Element of L) holds (X is_<=_than b implies b >= a)) and A3: (for c being (Element of L) holds ((X is_<=_than c & (for b being (Element of L) holds (X is_<=_than b implies b >= c))) implies c = a)) by YELLOW_0:def 7;
thus  ex_inf_of X,( L opp )
proof
take ( a ~ );
thus X is_>=_than ( a ~ ) by A1,CKB13:1;
hereby
let b being (Element of ( L opp ));
assume X is_>=_than b;
then X is_<=_than ( ~ b ) by CKB14:1;
then ( ~ b ) >= a by A2;
hence b <= ( a ~ ) by CKB3:1;
end;
let c being (Element of ( L opp ));
assume X is_>=_than c;
then A4: X is_<=_than ( ~ c ) by CKB14:1;
assume A5: (for b being (Element of ( L opp )) holds (X is_>=_than b implies b <= c));
now
let b being (Element of L);
assume X is_<=_than b;
then X is_>=_than ( b ~ ) by CKB13:1;
then ( b ~ ) <= c by A5;
hence b >= ( ~ c ) by CKB3:1;
end;
hence thesis by A3,A4;
end;

end;
assume  ex_inf_of X,( L opp );
then consider a being (Element of ( L opp )) such that A6: X is_>=_than a and A7: (for b being (Element of ( L opp )) holds (X is_>=_than b implies b <= a)) and A8: (for c being (Element of ( L opp )) holds ((X is_>=_than c & (for b being (Element of ( L opp )) holds (X is_>=_than b implies b <= c))) implies c = a)) by YELLOW_0:def 8;
take ( ~ a );
thus X is_<=_than ( ~ a ) by A6,CKB14:1;
hereby
let b being (Element of L);
assume X is_<=_than b;
then X is_>=_than ( b ~ ) by CKB13:1;
then ( b ~ ) <= a by A7;
hence b >= ( ~ a ) by CKB3:1;
end;
let c being (Element of L);
assume X is_<=_than c;
then A9: X is_>=_than ( c ~ ) by CKB13:1;
assume A10: (for b being (Element of L) holds (X is_<=_than b implies b >= c));
now
let b being (Element of ( L opp ));
assume X is_>=_than b;
then X is_<=_than ( ~ b ) by CKB14:1;
then ( ~ b ) >= c by A10;
hence b <= ( c ~ ) by CKB3:1;
end;
hence thesis by A8,A9;
end;
