begin
theorem
Th42: (for S,T being non  empty RelStr holds (for f being (Function of S,T) holds (for g being (Function of ( S opp ),( T opp )) holds (f = g implies ((f is  monotone iff g is  monotone) & (f is  antitone iff g is  antitone))))))
proof
let S being non  empty RelStr;
let T being non  empty RelStr;
let f being (Function of S,T);
let g being (Function of ( S ~ ),( T ~ ));
assume that
A1: f = g;
thus (f is  monotone implies g is  monotone)
proof
assume A2: (for x,y being (Element of S) holds (x <= y implies ( f . x ) <= ( f . y )));
let x being (Element of ( S ~ ));
let y being (Element of ( S ~ ));
assume x <= y;
then ( ~ y ) <= ( ~ x ) by CKB2:1;
then A3: ( f . ( ~ y ) ) <= ( f . ( ~ x ) ) by A2;
(( ( f . ( ~ x ) ) ~ ) = ( f . ( ~ x ) ) & ( ( f . ( ~ y ) ) ~ ) = ( f . ( ~ y ) ));
hence thesis by A1,A3,LATTICE3:9;
end;

thus (g is  monotone implies f is  monotone)
proof
assume A4: (for x,y being (Element of ( S ~ )) holds (x <= y implies ( g . x ) <= ( g . y )));
let x being (Element of S);
let y being (Element of S);
assume x <= y;
then ( y ~ ) <= ( x ~ ) by LATTICE3:9;
then A5: ( g . ( y ~ ) ) <= ( g . ( x ~ ) ) by A4;
(( ~ ( g . ( x ~ ) ) ) = ( g . ( x ~ ) ) & ( ~ ( g . ( y ~ ) ) ) = ( g . ( y ~ ) ));
hence thesis by A1,A5,CKB2:1;
end;

thus (f is  antitone implies g is  antitone)
proof
assume A6: (for x,y being (Element of S) holds (x <= y implies (for a,b being (Element of T) holds ((a = ( f . x ) & b = ( f . y )) implies a >= b))));
let x being (Element of ( S ~ ));
let y being (Element of ( S ~ ));
assume x <= y;
then ( ~ y ) <= ( ~ x ) by CKB2:1;
then A7: ( f . ( ~ y ) ) >= ( f . ( ~ x ) ) by A6;
(( ( f . ( ~ x ) ) ~ ) = ( f . ( ~ x ) ) & ( ( f . ( ~ y ) ) ~ ) = ( f . ( ~ y ) ));
hence thesis by A1,A7,LATTICE3:9;
end;

thus (g is  antitone implies f is  antitone)
proof
assume A8: (for x,y being (Element of ( S opp )) holds (x <= y implies (for a,b being (Element of ( T opp )) holds ((a = ( g . x ) & b = ( g . y )) implies a >= b))));
let x being (Element of S);
let y being (Element of S);
assume x <= y;
then ( y ~ ) <= ( x ~ ) by LATTICE3:9;
then A9: ( g . ( y ~ ) ) >= ( g . ( x ~ ) ) by A8;
(( ~ ( g . ( x ~ ) ) ) = ( g . ( x ~ ) ) & ( ~ ( g . ( y ~ ) ) ) = ( g . ( y ~ ) ));
hence thesis by A1,A9,CKB2:1;
end;

end;
