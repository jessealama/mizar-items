begin
theorem
Th4: (for L being RelStr holds (L is  reflexive iff ( L opp ) is  reflexive))
proof
let L being RelStr;
thus (L is  reflexive implies ( L opp ) is  reflexive)
proof
assume L is  reflexive;
then A1: (the InternalRel of L) is_reflexive_in (the carrier of L) by ORDERS_2:def 2;
let x being set;
assume x in (the carrier of ( L opp ));
then [ x,x ] in (the InternalRel of L) by A1,RELAT_2:def 1;
hence thesis by RELAT_1:def 7;
end;

assume ( L opp ) is  reflexive;
then A2: (the InternalRel of ( L opp )) is_reflexive_in (the carrier of ( L opp )) by ORDERS_2:def 2;
let x being set;
assume x in (the carrier of L);
then [ x,x ] in (the InternalRel of ( L opp )) by A2,RELAT_2:def 1;
hence thesis by RELAT_1:def 7;
end;
