environ
vocabularies INCPROJ,INCSP_1,SUBSET_1,AFF_2,ZFMISC_1,VECTSP_1,ANALOAF,PARTFUN1,RELAT_1,TARSKI,FUNCT_1,PROJRED1;
notations TARSKI,ZFMISC_1,SUBSET_1,DOMAIN_1,RELAT_1,RELSET_1,INCSP_1,INCPROJ,PARTFUN1,FUNCT_1,CKB20;
definitions TARSKI,CKB20;
theorems TARSKI,PARTFUN1,INCPROJ,FUNCT_1,RELAT_1,RELSET_1,XBOOLE_0,ZFMISC_1,INCSP_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20;
schemes PARTFUN1,XBOOLE_0;
registrations FUNCT_1,RELSET_1,INCPROJ;
constructors DOMAIN_1,INCPROJ,RELSET_1,CKB20;
requirements SUBSET,BOOLE;
begin
reserve IPP for  Desarguesian  2-dimensional IncProjSp;
reserve p for (POINT of IPP);
reserve x for (POINT of IPP);
reserve K for (LINE of IPP);
reserve X for (LINE of IPP);
theorem
Th21: ((not p on K) implies (for x holds (x on K implies ( ( IncProj (K,p,K) ) . x ) = x)))
proof
assume A1: (not p on K);
let x;
A2: (ex X st (p on X & x on X)) by INCPROJ:def 5;
assume x on K;
hence thesis by A1,A2,CKB20:def 1;
end;
