definitions SUPINF_2,XXREAL_3;
theorems MEASURE6,SUPINF_2,EXTREAL1,MESFUNC1,ABSVALUE,XCMPLX_1,COMPLEX1,XREAL_1,XXREAL_0,XXREAL_3,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22;
registrations NUMBERS,XXREAL_0,XREAL_0,MEMBERED,XBOOLE_0,XXREAL_3;
constructors REAL_1,COMPLEX1,SUPINF_2,MEASURE6,EXTREAL1,MESFUNC1,SUPINF_1,BINOP_2;
requirements NUMERALS,REAL,SUBSET,BOOLE,ARITHM;
begin
reserve x for R_eal;
reserve y for R_eal;
reserve w for R_eal;
reserve z for R_eal;
theorem
((|. x .| <= z & |. y .| <= w) implies |. ( x + y ) .| <= ( z + w ))
proof
assume A1: (|. x .| <= z & |. y .| <= w);
then (( - z ) <= x & ( - w ) <= y) by CKB12:1;
then A2: ( ( - z ) + ( - w ) ) <= ( x + y ) by XXREAL_3:36;
(x <= z & y <= w) by A1,CKB12:1;
then A3: ( x + y ) <= ( z + w ) by XXREAL_3:36;
( ( - z ) - w ) = ( - ( z + w ) ) by XXREAL_3:25;
hence thesis by A3,A2,CKB12:1;
end;
