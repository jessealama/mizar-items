environ
vocabularies NUMBERS,SUPINF_1,REAL_1,COMPLEX1,CARD_1,XXREAL_0,ARYTM_1,ARYTM_3,RELAT_1,MESFUNC1,MEASURE6;
notations NUMBERS,XXREAL_0,COMPLEX1,REAL_1,SUPINF_1,SUPINF_2,MEASURE6,EXTREAL1,MESFUNC1;
definitions SUPINF_2,XXREAL_3;
theorems MEASURE6,SUPINF_2,EXTREAL1,MESFUNC1,ABSVALUE,XCMPLX_1,COMPLEX1,XREAL_1,XXREAL_0,XXREAL_3,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16;
registrations NUMBERS,XXREAL_0,XREAL_0,MEMBERED,XBOOLE_0,XXREAL_3;
constructors REAL_1,COMPLEX1,SUPINF_2,MEASURE6,EXTREAL1,MESFUNC1,SUPINF_1,BINOP_2;
requirements NUMERALS,REAL,SUBSET,BOOLE,ARITHM;
begin
reserve x for R_eal;
reserve y for R_eal;
theorem
(((not ((x = ( -infty ) or x = ( +infty )) & (y = ( -infty ) or y = ( +infty )))) & y <> ( 0 )) implies |. ( x / y ) .| = ( |. x .| / |. y .| ))
proof
assume that
A1: (not ((x = ( -infty ) or x = ( +infty )) & (y = ( -infty ) or y = ( +infty ))))
and
A2: y <> ( 0 );
per cases ;
suppose A3: x = ( +infty );

then A4: y < ( +infty ) by A1,XXREAL_0:9,XXREAL_0:14;
A5: ( -infty ) < y by A1,A3,XXREAL_0:12,XXREAL_0:14;
now
per cases  by A2;
suppose A6: ( 0 ) < y;

then ( x / y ) = ( +infty ) by A3,A4,XXREAL_3:83;
then A7: |. ( x / y ) .| = ( +infty ) by EXTREAL1:3;
A8: |. x .| = ( +infty ) by A3,EXTREAL1:3;
|. y .| = y by A6,EXTREAL1:3;
hence thesis by A4,A6,A7,A8,XXREAL_3:83;
end;
suppose A9: y < ( 0 );

then |. y .| = ( - y ) by EXTREAL1:4;
then A10: |. y .| < ( +infty ) by A5,XXREAL_3:5,XXREAL_3:38;
( x / y ) = ( -infty ) by A3,A5,A9,XXREAL_3:85;
then A11: |. ( x / y ) .| = ( +infty ) by EXTREAL1:4,XXREAL_3:5;
(( 0 ) < |. y .| & |. x .| = ( +infty )) by A2,A3,CKB4:1,EXTREAL1:3;
hence thesis by A10,A11,XXREAL_3:83;
end;
end;
hence thesis;
end;
suppose A12: x = ( -infty );

then A13: y < ( +infty ) by A1,XXREAL_0:9,XXREAL_0:14;
A14: ( -infty ) < y by A1,A12,XXREAL_0:12,XXREAL_0:14;
now
per cases  by A2;
suppose A15: ( 0 ) < y;

then ( x / y ) = ( -infty ) by A12,A13,XXREAL_3:86;
then A16: |. ( x / y ) .| = ( +infty ) by EXTREAL1:4,XXREAL_3:5;
A17: |. x .| = ( +infty ) by A12,EXTREAL1:4,XXREAL_3:5;
|. y .| = y by A15,EXTREAL1:3;
hence thesis by A13,A15,A16,A17,XXREAL_3:83;
end;
suppose A18: y < ( 0 );

then |. y .| = ( - y ) by EXTREAL1:4;
then A19: |. y .| < ( +infty ) by A14,XXREAL_3:5,XXREAL_3:38;
( x / y ) = ( +infty ) by A12,A14,A18,XXREAL_3:84;
then A20: |. ( x / y ) .| = ( +infty ) by EXTREAL1:3;
(( 0 ) < |. y .| & |. x .| = ( +infty )) by A2,A12,CKB4:1,EXTREAL1:4,XXREAL_3:5;
hence thesis by A19,A20,XXREAL_3:83;
end;
end;
hence thesis;
end;
suppose (x <> ( +infty ) & x <> ( -infty ));

then reconsider a = x as Real by XXREAL_0:14;
now
per cases ;
suppose y = ( +infty );

then (|. ( x / y ) .| = ( 0 ) & |. y .| = ( +infty )) by EXTREAL1:def 1;
hence thesis;
end;
suppose y = ( -infty );

then (|. ( x / y ) .| = ( 0 ) & |. y .| = ( +infty )) by EXTREAL1:def 1,XXREAL_3:5;
hence thesis;
end;
suppose (y <> ( +infty ) & y <> ( -infty ));

then reconsider b = y as Real by XXREAL_0:14;
(|. x .| = ( abs a ) & |. y .| = ( abs b )) by CKB1:1;
then A21: ( |. x .| / |. y .| ) = ( ( abs a ) / ( abs b ) ) by EXTREAL1:2;
( x / y ) = ( a / b ) by EXTREAL1:2;
then |. ( x / y ) .| = ( abs ( a / b ) ) by CKB1:1;
hence thesis by A21,COMPLEX1:67;
end;
end;
hence thesis;
end;
end;
