begin
reserve x for R_eal;
theorem
Th66: |. x .| = |. ( - x ) .|
proof
per cases ;
suppose A1: ( 0 ) < x;

then |. ( - x ) .| = ( - ( - x ) ) by EXTREAL1:4
.= x;
hence thesis by A1,EXTREAL1:3;
end;
suppose A2: x < ( 0 );

then |. x .| = ( - x ) by EXTREAL1:4;
hence thesis by A2,EXTREAL1:3;
end;
suppose x = ( 0 );

hence thesis;
end;
end;
