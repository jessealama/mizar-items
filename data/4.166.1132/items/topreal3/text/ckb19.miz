begin
reserve n for (Element of ( NAT ));
reserve j for (Element of ( NAT ));
reserve x for set;
theorem
Th26: (for f being (FinSequence of ( TOP-REAL n )) holds (for i being Nat holds ( LSeg (f,i) ) c= ( L~ f )))
proof
let f being (FinSequence of ( TOP-REAL n ));
let i being Nat;
let x;
assume that
A1: x in ( LSeg (f,i) );
A2: i in ( NAT ) by ORDINAL1:def 12;
now
per cases ;
case i < 1;
hence contradiction by A1,TOPREAL1:def 3;
end;
case A3: i >= 1;
now
per cases ;
case ( i + 1 ) > ( len f );
hence contradiction by A1,TOPREAL1:def 3;
end;
case A4: ( i + 1 ) <= ( len f );
set M = { ( LSeg (f,j) ): (1 <= j & ( j + 1 ) <= ( len f )) };
( LSeg (f,i) ) in M by A2,A3,A4;
hence thesis by A1,TARSKI:def 4;
end;
end;
hence thesis;
end;
end;
hence thesis;
end;
