environ
vocabularies NUMBERS,PRE_TOPC,EUCLID,FINSEQ_1,XREAL_0,ORDINAL1,SUBSET_1,RELAT_1,ARYTM_3,MCART_1,ARYTM_1,FUNCT_1,SQUARE_1,BINOP_2,COMPLEX1,RVSUM_1,CARD_3,NAT_1,STRUCT_0,REAL_1,XXREAL_0,RLTOPSP1,TARSKI,CARD_1,XBOOLE_0,TOPREAL1,PARTFUN1,ORDINAL4,METRIC_1;
notations TARSKI,XBOOLE_0,SUBSET_1,ORDINAL1,NUMBERS,XCMPLX_0,XREAL_0,SQUARE_1,COMPLEX1,REAL_1,NAT_1,RELAT_1,FUNCT_1,PARTFUN1,FINSEQ_1,BINOP_1,PRE_TOPC,FINSEQ_2,STRUCT_0,METRIC_1,RVSUM_1,RLVECT_1,RLTOPSP1,EUCLID,TOPREAL1,XXREAL_0;
definitions TARSKI,TOPREAL1,FUNCT_1,XBOOLE_0,SQUARE_1,EUCLID,RVSUM_1,VALUED_1,ALGSTR_0,RLTOPSP1;
theorems TARSKI,NAT_1,ZFMISC_1,FUNCT_1,FINSEQ_1,ABSVALUE,SQUARE_1,FINSEQ_2,METRIC_1,RVSUM_1,EUCLID,TOPREAL1,FINSEQ_3,FINSEQ_4,PARTFUN2,TBSP_1,RELAT_1,INT_1,XBOOLE_0,XBOOLE_1,XCMPLX_1,XREAL_0,COMPLEX1,XREAL_1,BINOP_2,XXREAL_0,ORDINAL1,RLTOPSP1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27;
registrations XBOOLE_0,RELSET_1,NUMBERS,XXREAL_0,XREAL_0,SQUARE_1,NAT_1,MEMBERED,FINSEQ_1,STRUCT_0,EUCLID,VALUED_0;
constructors PARTFUN1,REAL_1,SQUARE_1,NAT_1,BINOP_2,COMPLEX1,FINSEQOP,FINSEQ_4,TOPREAL1,SEQ_1,RVSUM_1,RELSET_1,DOMAIN_1;
requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve p for (Point of ( TOP-REAL 2 ));
reserve p2 for (Point of ( TOP-REAL 2 ));
reserve q for (Point of ( TOP-REAL 2 ));
theorem
(for q holds (for p2 holds (for p holds ((( q `1 ) = ( p2 `1 ) & ( p `1 ) <> ( p2 `1 )) implies ( ( ( LSeg (p2,|[ ( p `1 ),( p2 `2 ) ]|) ) \/ ( LSeg (|[ ( p `1 ),( p2 `2 ) ]|,p) ) ) /\ ( LSeg (q,p2) ) ) = { p2 }))))
proof
let q;
let p2;
let p;
assume that
A1: ( q `1 ) = ( p2 `1 )
and
A2: ( p `1 ) <> ( p2 `1 );
set p3 = |[ ( p `1 ),( p2 `2 ) ]|;
set l23 = ( LSeg (p2,p3) );
set l3 = ( LSeg (p3,p) );
set l = ( LSeg (q,p2) );
A3: ( l3 /\ l ) = ( {} )
proof
set x = the (Element of ( l3 /\ l ));
assume A4: ( l3 /\ l ) <> ( {} );
then x in l3 by XBOOLE_0:def 4;
then consider d1 being Real such that A5: x = ( ( ( 1 - d1 ) * p3 ) + ( d1 * p ) ) and ( 0 ) <= d1 and d1 <= 1;
x in l by A4,XBOOLE_0:def 4;
then consider d2 being Real such that A6: x = ( ( ( 1 - d2 ) * q ) + ( d2 * p2 ) ) and ( 0 ) <= d2 and d2 <= 1;
A7: ( ( ( ( 1 - d1 ) * p3 ) + ( d1 * p ) ) `1 ) = ( ( ( ( 1 - d1 ) * p3 ) `1 ) + ( ( d1 * p ) `1 ) ) by CKB2:1
.= ( ( ( 1 - d1 ) * ( p3 `1 ) ) + ( ( d1 * p ) `1 ) ) by CKB4:1
.= ( ( ( 1 - d1 ) * ( p3 `1 ) ) + ( d1 * ( p `1 ) ) ) by CKB4:1
.= ( ( ( 1 - d1 ) * ( p `1 ) ) + ( d1 * ( p `1 ) ) ) by EUCLID:52
.= ( p `1 );
( ( ( ( 1 - d2 ) * q ) + ( d2 * p2 ) ) `1 ) = ( ( ( ( 1 - d2 ) * q ) `1 ) + ( ( d2 * p2 ) `1 ) ) by CKB2:1
.= ( ( ( 1 - d2 ) * ( q `1 ) ) + ( ( d2 * p2 ) `1 ) ) by CKB4:1
.= ( ( ( 1 - d2 ) * ( q `1 ) ) + ( d2 * ( p2 `1 ) ) ) by CKB4:1
.= ( p2 `1 ) by A1;
hence contradiction by A2,A5,A7,A6;
end;
A8: ( l23 /\ l ) = { p2 }
proof
thus ( l23 /\ l ) c= { p2 }
proof
let x being set;
assume A9: x in ( l23 /\ l );
then x in l23 by XBOOLE_0:def 4;
then consider s1 being Real such that A10: x = ( ( ( 1 - s1 ) * p2 ) + ( s1 * p3 ) ) and ( 0 ) <= s1 and s1 <= 1;
x in l by A9,XBOOLE_0:def 4;
then consider s2 being Real such that A11: x = ( ( ( 1 - s2 ) * q ) + ( s2 * p2 ) ) and ( 0 ) <= s2 and s2 <= 1;
A12: ( ( ( ( 1 - s2 ) * q ) + ( s2 * p2 ) ) `1 ) = ( ( ( ( 1 - s2 ) * q ) `1 ) + ( ( s2 * p2 ) `1 ) ) by CKB2:1
.= ( ( ( 1 - s2 ) * ( q `1 ) ) + ( ( s2 * p2 ) `1 ) ) by CKB4:1
.= ( ( ( 1 - s2 ) * ( q `1 ) ) + ( s2 * ( p2 `1 ) ) ) by CKB4:1
.= ( p2 `1 ) by A1;
set t = ( ( ( 1 - s1 ) * p2 ) + ( s1 * p3 ) );
A13: ( t `2 ) = ( ( ( ( 1 - s1 ) * p2 ) `2 ) + ( ( s1 * p3 ) `2 ) ) by CKB2:1
.= ( ( ( 1 - s1 ) * ( p2 `2 ) ) + ( ( s1 * p3 ) `2 ) ) by CKB4:1
.= ( ( ( 1 - s1 ) * ( p2 `2 ) ) + ( s1 * ( p3 `2 ) ) ) by CKB4:1
.= ( ( ( 1 - s1 ) * ( p2 `2 ) ) + ( s1 * ( p2 `2 ) ) ) by EUCLID:52
.= ( p2 `2 );
t = |[ ( t `1 ),( t `2 ) ]| by EUCLID:53
.= p2 by A10,A13,A11,A12,EUCLID:53;
hence thesis by A10,TARSKI:def 1;
end;

let x being set;
assume x in { p2 };
then A14: x = p2 by TARSKI:def 1;
(p2 in l23 & p2 in l) by RLTOPSP1:68;
hence thesis by A14,XBOOLE_0:def 4;
end;
thus ( ( l23 \/ l3 ) /\ l ) = ( ( l23 /\ l ) \/ ( l3 /\ l ) ) by XBOOLE_1:23
.= { p2 } by A8,A3;
end;
