environ
vocabularies XBOOLE_0,PRE_TOPC,SUBSET_1,STRUCT_0,TARSKI,TOPS_1,RCOMP_1,TSEP_1,CONNSP_1,SETFAM_1,TSEP_2;
notations TARSKI,XBOOLE_0,SUBSET_1,PRE_TOPC,STRUCT_0,TOPS_1,BORSUK_1,CONNSP_1,TSEP_1,CKB2,CKB3,CKB10;
definitions BORSUK_1,TSEP_1,XBOOLE_0,SUBSET_1,STRUCT_0,CKB2;
theorems PRE_TOPC,TOPS_1,CONNSP_1,BORSUK_1,TSEP_1,TDLAT_1,TDLAT_3,SUBSET_1,XBOOLE_0,XBOOLE_1,CKB1,CKB2,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB11,CKB12,CKB13;
registrations STRUCT_0,PRE_TOPC;
constructors TOPS_1,CONNSP_1,BORSUK_1,TSEP_1,CKB2,CKB10;
requirements BOOLE,SUBSET;
begin
reserve X for non empty TopSpace;
theorem
Th12: (for A1,A2 being (Subset of X) holds (A1,A2 constitute_a_decomposition  implies (A1 is  open iff A2 is  closed)))
proof
let A1 being (Subset of X),A2 being (Subset of X);
assume A1: A1,A2 constitute_a_decomposition ;
then A2 = ( A1 ` ) by CKB5:1;
hence (A1 is  open implies A2 is  closed) by TOPS_1:4;
assume A2: A2 is  closed;
A1 = ( A2 ` ) by A1,CKB5:1;
hence thesis by A2,TOPS_1:3;
end;
