environ
vocabularies XBOOLE_0,PRE_TOPC,SUBSET_1,STRUCT_0,TARSKI,TOPS_1,RCOMP_1,TSEP_1,CONNSP_1,SETFAM_1,TSEP_2;
notations TARSKI,XBOOLE_0,SUBSET_1,PRE_TOPC,STRUCT_0,TOPS_1,BORSUK_1,CONNSP_1,TSEP_1,CKB2,CKB3,CKB10;
definitions BORSUK_1,TSEP_1,XBOOLE_0,SUBSET_1,STRUCT_0,CKB2;
theorems PRE_TOPC,TOPS_1,CONNSP_1,BORSUK_1,TSEP_1,TDLAT_1,TDLAT_3,SUBSET_1,XBOOLE_0,XBOOLE_1,CKB1,CKB2,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB11;
registrations STRUCT_0,PRE_TOPC;
constructors TOPS_1,CONNSP_1,BORSUK_1,TSEP_1,CKB2,CKB10;
requirements BOOLE,SUBSET;
begin
reserve X for non empty TopSpace;
reserve A1 for (Subset of X);
reserve A2 for (Subset of X);
theorem
Th10: (A1,A2 constitute_a_decomposition  implies (( Cl A1 ),( Int A2 ) constitute_a_decomposition  & ( Int A1 ),( Cl A2 ) constitute_a_decomposition ))
proof
assume A1: A1,A2 constitute_a_decomposition ;
( Cl A1 ) = ( ( Int ( A1 ` ) ) ` ) by TDLAT_3:1;
then ( Cl A1 ) = ( ( Int A2 ) ` ) by A1,CKB5:1;
hence ( Cl A1 ),( Int A2 ) constitute_a_decomposition  by CKB6:1;
( Int A1 ) = ( ( Cl ( A1 ` ) ) ` ) by TOPS_1:def 1;
then ( Int A1 ) = ( ( Cl A2 ) ` ) by A1,CKB5:1;
hence ( Int A1 ),( Cl A2 ) constitute_a_decomposition  by CKB6:1;
end;
