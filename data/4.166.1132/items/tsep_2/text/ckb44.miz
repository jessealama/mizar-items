begin
reserve X for non empty TopSpace;
theorem
(for X1,X2,Y1,Y2 being non empty (SubSpace of X) holds ((X1,Y1 constitute_a_decomposition  & X2,Y2 constitute_a_decomposition ) implies (X1,X2 are_separated  implies Y1,Y2 are_weakly_separated )))
proof
let X1 being non empty (SubSpace of X),X2 being non empty (SubSpace of X),Y1 being non empty (SubSpace of X),Y2 being non empty (SubSpace of X);
assume A1: (X1,Y1 constitute_a_decomposition  & X2,Y2 constitute_a_decomposition );
assume X1,X2 are_separated ;
then X1,X2 are_weakly_separated  by TSEP_1:78;
hence thesis by A1,CKB43:1;
end;
