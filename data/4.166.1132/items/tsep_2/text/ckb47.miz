begin
reserve X for non empty TopSpace;
theorem
(for X1,X2 being non empty (SubSpace of X) holds (X1,X2 constitute_a_decomposition  implies (X1,X2 are_weakly_separated  iff X1,X2 are_separated )))
proof
let X1 being non empty (SubSpace of X),X2 being non empty (SubSpace of X);
assume X1,X2 constitute_a_decomposition ;
then X1 misses X2 by CKB34:1;
hence thesis by TSEP_1:78;
end;
