environ
vocabularies XBOOLE_0,PRE_TOPC,SUBSET_1,STRUCT_0,TARSKI,TOPS_1,RCOMP_1,TSEP_1,CONNSP_1,SETFAM_1,TSEP_2;
notations TARSKI,XBOOLE_0,SUBSET_1,PRE_TOPC,STRUCT_0,TOPS_1,BORSUK_1,CONNSP_1,TSEP_1,CKB2,CKB3,CKB10,CKB32,CKB33,CKB36;
definitions BORSUK_1,TSEP_1,XBOOLE_0,SUBSET_1,STRUCT_0,CKB2,CKB32;
theorems PRE_TOPC,TOPS_1,CONNSP_1,BORSUK_1,TSEP_1,TDLAT_1,TDLAT_3,SUBSET_1,XBOOLE_0,XBOOLE_1,CKB1,CKB2,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB34,CKB35,CKB37,CKB38,CKB39,CKB40,CKB41,CKB42,CKB43,CKB44,CKB45,CKB46,CKB47,CKB48,CKB49,CKB50,CKB51,CKB52;
registrations STRUCT_0,PRE_TOPC;
constructors TOPS_1,CONNSP_1,BORSUK_1,TSEP_1,CKB2,CKB10,CKB32,CKB36;
requirements BOOLE,SUBSET;
begin
reserve X for non empty TopSpace;
reserve X0 for non empty (SubSpace of X);
theorem
(for X1,X2 being non empty (SubSpace of X) holds ((X1 meets X0 & X2 meets X0) implies (for Y1,Y2 being (SubSpace of X0) holds ((Y1 = ( X1 meet X0 ) & Y2 = ( X2 meet X0 )) implies (X1,X2 are_weakly_separated  implies Y1,Y2 are_weakly_separated )))))
proof
let X1 being non empty (SubSpace of X),X2 being non empty (SubSpace of X);
reconsider A1 = (the carrier of X1),A2 = (the carrier of X2) as (Subset of X) by TSEP_1:1;
assume A1: (X1 meets X0 & X2 meets X0);
let Y1 being (SubSpace of X0),Y2 being (SubSpace of X0);
assume A2: (Y1 = ( X1 meet X0 ) & Y2 = ( X2 meet X0 ));
assume X1,X2 are_weakly_separated ;
then A3: A1,A2 are_weakly_separated  by TSEP_1:def 7;
now
let C1 being (Subset of X0),C2 being (Subset of X0);
assume (C1 = (the carrier of Y1) & C2 = (the carrier of Y2));
then (C1 = ( (the carrier of X0) /\ A1 ) & C2 = ( (the carrier of X0) /\ A2 )) by A1,A2,TSEP_1:def 4;
hence C1,C2 are_weakly_separated  by A3,CKB31:1;
end;
hence thesis by TSEP_1:def 7;
end;
