environ
vocabularies XBOOLE_0,PRE_TOPC,SUBSET_1,STRUCT_0,TARSKI,TOPS_1,RCOMP_1,TSEP_1,CONNSP_1,SETFAM_1,TSEP_2;
notations TARSKI,XBOOLE_0,SUBSET_1,PRE_TOPC,STRUCT_0,TOPS_1,BORSUK_1,CONNSP_1,TSEP_1,CKB2,CKB3;
definitions BORSUK_1,TSEP_1,XBOOLE_0,SUBSET_1,STRUCT_0,CKB2;
theorems PRE_TOPC,TOPS_1,CONNSP_1,BORSUK_1,TSEP_1,TDLAT_1,TDLAT_3,SUBSET_1,XBOOLE_0,XBOOLE_1,CKB1,CKB2,CKB4,CKB5,CKB6,CKB7,CKB8;
registrations STRUCT_0,PRE_TOPC;
constructors TOPS_1,CONNSP_1,BORSUK_1,TSEP_1,CKB2;
requirements BOOLE,SUBSET;
begin
reserve X for non empty TopSpace;
reserve A for (Subset of X);
theorem
Th8: A,A do_not_constitute_a_decomposition 
proof
assume A1: A,A constitute_a_decomposition ;
per cases ;
suppose A2: A is non empty;

A = ( A ` ) by A1,CKB5:1;
then A misses A by SUBSET_1:23;
then ( A /\ A ) = ( {} ) by XBOOLE_0:def 7;
hence contradiction by A2;
end;
suppose A is  empty;

then ( {} ) = ( A \/ A )
.= (the carrier of X) by A1,CKB2:def 1;
hence contradiction;
end;
end;
