environ
vocabularies NUMBERS,STRUCT_0,TOPMETR,ZFMISC_1,INT_1,XREAL_0,ORDINAL1,ARYTM_3,ARYTM_1,XXREAL_0,XXREAL_1,XBOOLE_0,CARD_1,FUNCT_1,RELAT_1,FINSEQ_1,CARD_3,FUNCOP_1,MONOID_0,PRE_TOPC,PCOMPS_1,EUCLID,SUBSET_1,RCOMP_1,CONNSP_2,TOPS_1,TARSKI,NATTRA_1,TDLAT_3,ORDINAL2,FUNCT_2,T_0TOPSP,TOPS_2,PARTFUN1,REAL_1,FCONT_1,TMAP_1,JGRAPH_6,MCART_1,TOPREALA;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,ORDINAL1,RELAT_1,FUNCT_1,RELSET_1,PARTFUN1,FUNCT_2,NUMBERS,XCMPLX_0,XXREAL_0,XREAL_0,REAL_1,FUNCOP_1,INT_1,CARD_3,FINSEQ_1,FUNCT_4,MONOID_0,RCOMP_1,FCONT_1,STRUCT_0,PRE_TOPC,PCOMPS_1,TOPS_1,BORSUK_1,TSEP_1,TOPS_2,TDLAT_3,CONNSP_2,T_0TOPSP,TMAP_1,TOPMETR,EUCLID,JGRAPH_6;
definitions TARSKI,XBOOLE_0,FUNCT_2,PRE_TOPC,T_0TOPSP,TMAP_1,STRUCT_0,RCOMP_1,FCONT_1,MONOID_0,EUCLID,PARTFUN2;
theorems PRE_TOPC,XREAL_0,FUNCT_2,BORSUK_1,INT_1,EUCLID,TOPS_2,BORSUK_3,FUNCT_1,TOPMETR,RELAT_1,FUNCOP_1,TOPREAL6,JORDAN16,BORSUK_6,TARSKI,ZFMISC_1,FUNCT_4,TSEP_1,HILBERT3,FINSEQ_1,TMAP_1,XBOOLE_1,JORDAN6,TOPS_3,FCONT_1,CONNSP_2,FRECHET,XBOOLE_0,FINSEQ_3,NAT_1,RCOMP_1,TEX_2,DOMAIN_1,TOPS_1,TOPGRP_1,T_0TOPSP,TREAL_1,CARD_3,JGRAPH_6,TDLAT_3,ENUMSET1,TOPALG_3,XREAL_1,XXREAL_0,XXREAL_1,CKB17,CKB18,CKB19,CKB20,CKB23,CKB26,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB37,CKB38;
registrations RELSET_1,FUNCT_2,FUNCT_4,NUMBERS,XXREAL_0,XREAL_0,NAT_1,INT_1,MEMBERED,FINSEQ_1,STRUCT_0,PRE_TOPC,TOPS_1,BORSUK_1,TSEP_1,TDLAT_3,MONOID_0,EUCLID,TOPMETR,TOPGRP_1,BORSUK_6,VALUED_0,FCONT_1,PCOMPS_1,XBOOLE_0,FUNCT_1,RELAT_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB21,CKB22,CKB24,CKB25,CKB27;
constructors SETFAM_1,FUNCT_4,REAL_1,NAT_1,CARD_3,RCOMP_1,FINSEQ_4,PARTFUN2,FCONT_1,TOPS_1,TOPS_2,TDLAT_3,TMAP_1,T_0TOPSP,MONOID_0,BORSUK_4,JGRAPH_6,SEQ_1,FUNCSDOM,PCOMPS_1;
requirements NUMERALS,BOOLE,SUBSET,ARITHM,REAL;
begin
theorem
(for f,f1,f2 being (PartFunc of ( REAL ),( REAL )) holds (((((((( dom f ) = ( ( dom f1 ) \/ ( dom f2 ) ) & ( dom f1 ) is  open) & ( dom f2 ) is  open) & ( f1 | ( dom f1 ) ) is  continuous) & ( f2 | ( dom f2 ) ) is  continuous) & (for z being set holds (z in ( dom f1 ) implies ( f . z ) = ( f1 . z )))) & (for z being set holds (z in ( dom f2 ) implies ( f . z ) = ( f2 . z )))) implies ( f | ( dom f ) ) is  continuous))
proof
let f being (PartFunc of ( REAL ),( REAL ));
let f1 being (PartFunc of ( REAL ),( REAL ));
let f2 being (PartFunc of ( REAL ),( REAL ));
set X1 = ( dom f1 );
set X2 = ( dom f2 );
assume that
A1: ( dom f ) = ( X1 \/ X2 )
and
A2: X1 is  open
and
A3: X2 is  open
and
A4: ( f1 | X1 ) is  continuous
and
A5: ( f2 | X2 ) is  continuous
and
A6: (for z being set holds (z in X1 implies ( f . z ) = ( f1 . z )))
and
A7: (for z being set holds (z in X2 implies ( f . z ) = ( f2 . z )));
A8: ( ( dom f ) /\ X1 ) = X1 by A1,XBOOLE_1:7,XBOOLE_1:28;
A9: ( dom ( f | X2 ) ) = ( ( dom f ) /\ X2 ) by RELAT_1:61;
let x being  real number;
assume x in ( dom ( f | ( dom f ) ) );
then A10: x in ( dom f ) by RELAT_1:57;
then A11: ( ( f | ( X1 \/ X2 ) ) . x ) = ( f . x ) by A1,FUNCT_1:49;
A12: ( ( dom f ) /\ X2 ) = X2 by A1,XBOOLE_1:7,XBOOLE_1:28;
A13: ( dom ( f | X1 ) ) = ( ( dom f ) /\ X1 ) by RELAT_1:61;
per cases  by A1,A10,XBOOLE_0:def 3;
suppose A14: x in X1;

then A15: ( ( f | ( X1 \/ X2 ) ) . x ) = ( f1 . x ) by A6,A11
.= ( ( f1 | X1 ) . x ) by A14,FUNCT_1:49;
(for N1 being (Neighbourhood of ( ( f | ( X1 \/ X2 ) ) . x )) holds (ex N being (Neighbourhood of x) st (for x1 being  real number holds ((x1 in ( dom ( f | ( X1 \/ X2 ) ) ) & x1 in N) implies ( ( f | ( X1 \/ X2 ) ) . x1 ) in N1))))
proof
let N1 being (Neighbourhood of ( ( f | ( X1 \/ X2 ) ) . x ));
consider N2 being (Neighbourhood of x) such that A16: N2 c= X1 by A2,A14,RCOMP_1:18;
x in ( dom ( f1 | X1 ) ) by A14,RELAT_1:57;
then ( f1 | X1 ) is_continuous_in x by A4,FCONT_1:def 2;
then consider N being (Neighbourhood of x) such that A17: (for x1 being  real number holds ((x1 in ( dom ( f1 | X1 ) ) & x1 in N) implies ( ( f1 | X1 ) . x1 ) in N1)) by A15,FCONT_1:4;
consider N3 being (Neighbourhood of x) such that A18: N3 c= N and A19: N3 c= N2 by RCOMP_1:17;
take N3;
let x1 being  real number;
assume that
A20: x1 in ( dom ( f | ( X1 \/ X2 ) ) )
and
A21: x1 in N3;
per cases ;
suppose A22: x1 in ( dom ( f | X1 ) );

A23: ( dom ( f | X1 ) ) = ( X1 /\ X1 ) by A1,A13,XBOOLE_1:7,XBOOLE_1:28
.= ( dom ( f1 | X1 ) ) by RELAT_1:61;
A24: x1 in X1 by A13,A22,XBOOLE_0:def 4;
( ( f | ( X1 \/ X2 ) ) . x1 ) = ( f . x1 ) by A20,FUNCT_1:47
.= ( f1 . x1 ) by A6,A24
.= ( ( f1 | X1 ) . x1 ) by A24,FUNCT_1:49;
hence thesis by A17,A18,A21,A22,A23;
end;
suppose A25: (not x1 in ( dom ( f | X1 ) ));

x1 in N2 by A19,A21;
hence thesis by A13,A8,A16,A25;
end;
end;
hence thesis by A1,FCONT_1:4;
end;
suppose A26: x in X2;

then A27: ( ( f | ( X1 \/ X2 ) ) . x ) = ( f2 . x ) by A7,A11
.= ( ( f2 | X2 ) . x ) by A26,FUNCT_1:49;
(for N1 being (Neighbourhood of ( ( f | ( X1 \/ X2 ) ) . x )) holds (ex N being (Neighbourhood of x) st (for x1 being  real number holds ((x1 in ( dom ( f | ( X1 \/ X2 ) ) ) & x1 in N) implies ( ( f | ( X1 \/ X2 ) ) . x1 ) in N1))))
proof
let N1 being (Neighbourhood of ( ( f | ( X1 \/ X2 ) ) . x ));
consider N2 being (Neighbourhood of x) such that A28: N2 c= X2 by A3,A26,RCOMP_1:18;
x in ( dom ( f2 | X2 ) ) by A26,RELAT_1:57;
then ( f2 | X2 ) is_continuous_in x by A5,FCONT_1:def 2;
then consider N being (Neighbourhood of x) such that A29: (for x1 being  real number holds ((x1 in ( dom ( f2 | X2 ) ) & x1 in N) implies ( ( f2 | X2 ) . x1 ) in N1)) by A27,FCONT_1:4;
consider N3 being (Neighbourhood of x) such that A30: N3 c= N and A31: N3 c= N2 by RCOMP_1:17;
take N3;
let x1 being  real number;
assume that
A32: x1 in ( dom ( f | ( X1 \/ X2 ) ) )
and
A33: x1 in N3;
per cases ;
suppose A34: x1 in ( dom ( f | X2 ) );

A35: ( dom ( f | X2 ) ) = ( X2 /\ X2 ) by A1,A9,XBOOLE_1:7,XBOOLE_1:28
.= ( dom ( f2 | X2 ) ) by RELAT_1:61;
A36: x1 in X2 by A9,A34,XBOOLE_0:def 4;
( ( f | ( X1 \/ X2 ) ) . x1 ) = ( f . x1 ) by A32,FUNCT_1:47
.= ( f2 . x1 ) by A7,A36
.= ( ( f2 | X2 ) . x1 ) by A36,FUNCT_1:49;
hence thesis by A29,A30,A33,A34,A35;
end;
suppose A37: (not x1 in ( dom ( f | X2 ) ));

x1 in N2 by A31,A33;
hence thesis by A9,A12,A28,A37;
end;
end;
hence thesis by A1,FCONT_1:4;
end;
end;
