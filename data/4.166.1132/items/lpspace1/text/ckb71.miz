reserve S for (SigmaField of X);
reserve M for (sigma_Measure of S);
reserve f for (PartFunc of X,( REAL ));
reserve f1 for (PartFunc of X,( REAL ));
definition
let X being non  empty set;
let S being (SigmaField of X);
let M being (sigma_Measure of S);
func addCoset M -> (BinOp of ( CosetSet M )) means 
:Def15: (for A,B being (Element of ( CosetSet M )) holds (for a,b being (PartFunc of X,( REAL )) holds ((a in A & b in B) implies ( it . (A,B) ) = ( a.e-eq-class (( a + b ),M) ))));
existence
proof
defpred P[ set,set,set ]
 means
(for a,b being (PartFunc of X,( REAL )) holds ((a in $1 & b in $2) implies $3 = ( a.e-eq-class (( a + b ),M) )));
set C = ( CosetSet M );
A1:now
let A being (Element of C);
let B being (Element of C);
A in C;
then consider a being (PartFunc of X,( REAL )) such that A2: A = ( a.e-eq-class (a,M) ) and A3: a in ( L1_Functions M );
B in C;
then consider b being (PartFunc of X,( REAL )) such that A4: B = ( a.e-eq-class (b,M) ) and A5: b in ( L1_Functions M );
set z = ( a.e-eq-class (( a + b ),M) );
A6: ( a + b ) in ( L1_Functions M ) by A3,A5,CKB39:1;
then z in C;
then reconsider z as (Element of C);
take z;
now
let a1 being (PartFunc of X,( REAL ));
let b1 being (PartFunc of X,( REAL ));
assume A7: (a1 in A & b1 in B);
then (a1 a.e.= a,M & b1 a.e.= b,M) by A2,A3,A4,A5,CKB64:1;
then A8: ( a1 + b1 ) a.e.= ( a + b ),M by CKB52:1;
( a1 + b1 ) in ( L1_Functions M ) by A7,CKB39:1;
hence z = ( a.e-eq-class (( a1 + b1 ),M) ) by A6,A8,CKB66:1;
end;
hence P[ A,B,z ];
end;
consider f being (Function of [: C,C :],C) such that A9: (for A,B being (Element of C) holds P[ A,B,( f . (A,B) ) ]) from BINOP_1:sch 3(A1);
reconsider f as (BinOp of C);
take f;
let A being (Element of C);
let B being (Element of C);
let a being (PartFunc of X,( REAL ));
let b being (PartFunc of X,( REAL ));
assume (a in A & b in B);
hence thesis by A9;
end;
uniqueness
proof
set C = ( CosetSet M );
let f1 being (BinOp of ( CosetSet M ));
let f2 being (BinOp of ( CosetSet M ));
assume that
A10: (for A,B being (Element of ( CosetSet M )) holds (for a,b being (PartFunc of X,( REAL )) holds ((a in A & b in B) implies ( f1 . (A,B) ) = ( a.e-eq-class (( a + b ),M) ))))
and
A11: (for A,B being (Element of ( CosetSet M )) holds (for a,b being (PartFunc of X,( REAL )) holds ((a in A & b in B) implies ( f2 . (A,B) ) = ( a.e-eq-class (( a + b ),M) ))));
now
let A being (Element of ( CosetSet M ));
let B being (Element of ( CosetSet M ));
A in C;
then consider a1 being (PartFunc of X,( REAL )) such that A12: (A = ( a.e-eq-class (a1,M) ) & a1 in ( L1_Functions M ));
B in C;
then consider b1 being (PartFunc of X,( REAL )) such that A13: (B = ( a.e-eq-class (b1,M) ) & b1 in ( L1_Functions M ));
A14: b1 in B by A13,CKB65:1;
A15: a1 in A by A12,CKB65:1;
then ( f1 . (A,B) ) = ( a.e-eq-class (( a1 + b1 ),M) ) by A10,A14;
hence ( f1 . (A,B) ) = ( f2 . (A,B) ) by A11,A15,A14;
end;
hence thesis by BINOP_1:2;
end;
