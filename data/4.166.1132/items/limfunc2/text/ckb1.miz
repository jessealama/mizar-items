reserve g for Real;
theorem
Lm1: (for r,g,r1 being  real number holds ((( 0 ) < g & r <= r1) implies (( r - g ) < r1 & r < ( r1 + g ))))
proof
let r being  real number;
let g being  real number;
let r1 being  real number;
assume that
A1: ( 0 ) < g
and
A2: r <= r1;
( r - g ) < ( r1 - ( 0 ) ) by A1,A2,XREAL_1:15;
hence ( r - g ) < r1;
( r + ( 0 ) ) < ( r1 + g ) by A1,A2,XREAL_1:8;
hence thesis;
end;
