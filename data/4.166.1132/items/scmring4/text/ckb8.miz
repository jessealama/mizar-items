begin
reserve R for non  trivial  good Ring;
reserve a for (Data-Location of R);
definition
let R;
let a being (Data-Location of R);
let r being (Element of R);
redefine func a .--> r -> (FinPartState of ( SCM R ));

coherence
proof
set k = ( a .--> r );
set f = (the Object-Kind of ( SCM R ));
reconsider b = a as (Element of ( SCM-Memory )) by SCMRING2:def 1;
A1: ( dom k ) = { a } by FUNCOP_1:13;
(for x being set holds (x in ( dom k ) implies ( k . x ) in ( f . x )))
proof
let x being set;
assume A2: x in ( dom k );
then x = a by A1,TARSKI:def 1;
then A3: ( k . x ) = r by FUNCOP_1:72;
( f . x ) = ( ObjectKind a ) by A1,A2,TARSKI:def 1
.= (the carrier of R) by SCMRING3:1;
hence thesis by A3;
end;
hence thesis by FUNCT_1:def 14;
end;
end;
