begin
reserve I for set;
reserve A for (ManySortedSet of I);
reserve M for (ManySortedSet of I);
theorem
Th34: (for D being  properly-upper-bound (MSSubsetFamily of M) holds (for A being (Element of ( bool M )) holds (for J being (MSSetOp of M) holds ((A in D & (for X being (Element of ( bool M )) holds (for SF being  non-empty (MSSubsetFamily of M) holds ((for Y being (ManySortedSet of I) holds (Y in SF iff (Y in D & X c= Y))) implies ( J .. X ) = ( meet SF ))))) implies ( J .. A ) = A))))
proof
let D being  properly-upper-bound (MSSubsetFamily of M);
let A being (Element of ( bool M ));
let J being (MSSetOp of M);
assume that
A1: A in D
and
A2: (for X being (Element of ( bool M )) holds (for SF being  non-empty (MSSubsetFamily of M) holds ((for Y being (ManySortedSet of I) holds (Y in SF iff (Y in D & X c= Y))) implies ( J .. X ) = ( meet SF ))));
consider SF being  non-empty (MSSubsetFamily of M) such that A3: (for Y being (ManySortedSet of I) holds (Y in SF iff (Y in D & A c= Y))) by CKB76:1;
A in SF by A1,A3;
then ( meet SF ) c= A by MSSUBFAM:43;
then A4: ( J .. A ) c= A by A2,A3;
A5: (for Z1 being (ManySortedSet of I) holds (Z1 in SF implies A c= Z1)) by A3;
( J .. A ) = ( meet SF ) by A2,A3;
then A c= ( J .. A ) by A5,MSSUBFAM:45;
hence ( J .. A ) = A by A4,PBOOLE:def 10;
end;
