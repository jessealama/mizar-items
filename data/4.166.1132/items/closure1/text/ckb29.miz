environ
vocabularies PBOOLE,FUNCT_1,RELAT_1,TARSKI,FUNCT_4,FUNCOP_1,XBOOLE_0,SUBSET_1,FINSEQ_4,PARTFUN1,FUNCT_6,MEMBER_1,MSUALG_3,FINSET_1,ZFMISC_1,RELAT_2,MSAFREE2,BINOP_1,YELLOW_6,STRUCT_0,MSUALG_1,CLOSURE2,MSSUBFAM,SETFAM_1,CLOSURE1;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,STRUCT_0,SETFAM_1,RELAT_1,FUNCT_1,FINSET_1,PARTFUN1,FUNCT_2,FUNCOP_1,FUNCT_4,PBOOLE,FUNCT_6,MSUALG_1,MSUALG_3,PRALG_1,MSSUBFAM,CKB5,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17;
definitions XBOOLE_0,FUNCT_1,MSSUBFAM,MSUALG_1,MSUALG_3,PBOOLE,FINSET_1,TARSKI,FUNCOP_1,CKB14,CKB15,CKB16,CKB17;
theorems FUNCOP_1,FUNCT_1,FUNCT_2,FUNCT_4,MBOOLEAN,MSSUBFAM,MSUALG_1,MSUALG_3,PBOOLE,PZFMISC1,TARSKI,ZFMISC_1,PRALG_1,XBOOLE_0,XBOOLE_1,PARTFUN1,RELAT_1,CKB2,CKB3,CKB6,CKB7,CKB8,CKB9,CKB10,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28;
schemes MSSUBFAM,PBOOLE,XBOOLE_0,CKB1;
registrations SUBSET_1,FUNCT_1,RELSET_1,FUNCOP_1,FINSET_1,PBOOLE,MSSUBFAM,MSUALG_1,RELAT_1,PRE_CIRC,PRALG_1,CKB4,CKB11,CKB22;
constructors SETFAM_1,PZFMISC1,MSSUBFAM,PRALG_1,MSUALG_3,RELSET_1,FUNCT_4,FUNCT_6,CKB5,CKB13,CKB14,CKB15,CKB16,CKB17;
requirements SUBSET,BOOLE;
begin
reserve I for set;
reserve M for (ManySortedSet of I);
reserve P for (MSSetOp of M);
reserve E for (Element of ( bool M ));
reserve T for (Element of ( bool M ));
theorem
(P is  monotonic implies ( P .. ( E /\ T ) ) c= ( ( P .. E ) /\ ( P .. T ) ))
proof
assume A1: P is  monotonic;
E in ( bool M ) by MSSUBFAM:12;
then E c= M by MBOOLEAN:1;
then ( E /\ T ) c= M by MBOOLEAN:14;
then ( E /\ T ) in ( bool M ) by MBOOLEAN:1;
then A2: ( E /\ T ) is (Element of ( bool M )) by MSSUBFAM:11;
( E /\ T ) c= T by PBOOLE:15;
then A3: ( P .. ( E /\ T ) ) c= ( P .. T ) by A1,A2,CKB15:def 1;
( E /\ T ) c= E by PBOOLE:15;
then ( P .. ( E /\ T ) ) c= ( P .. E ) by A1,A2,CKB15:def 1;
hence thesis by A3,PBOOLE:17;
end;
