begin
reserve i for set;
reserve x for set;
reserve I for set;
reserve M for (ManySortedSet of I);
theorem
Th36: (for D being  properly-upper-bound (MSSubsetFamily of M) holds (for J being (MSSetOp of M) holds ((for X being (Element of ( bool M )) holds (for SF being  non-empty (MSSubsetFamily of M) holds ((for Y being (ManySortedSet of I) holds (Y in SF iff (Y in D & X c= Y))) implies ( J .. X ) = ( meet SF )))) implies J is  reflexive  monotonic)))
proof
let D being  properly-upper-bound (MSSubsetFamily of M);
let J being (MSSetOp of M);
assume that
A1: (for X being (Element of ( bool M )) holds (for SF being  non-empty (MSSubsetFamily of M) holds ((for Y being (ManySortedSet of I) holds (Y in SF iff (Y in D & X c= Y))) implies ( J .. X ) = ( meet SF ))));
thus J is  reflexive
proof
let X being (Element of ( bool M ));
consider SF being  non-empty (MSSubsetFamily of M) such that A2: (for Y being (ManySortedSet of I) holds (Y in SF iff (Y in D & X c= Y))) by CKB76:1;
(( J .. X ) = ( meet SF ) & (for Z1 being (ManySortedSet of I) holds (Z1 in SF implies X c= Z1))) by A1,A2;
hence thesis by MSSUBFAM:45;
end;

thus J is  monotonic
proof
let x being (Element of ( bool M ));
let y being (Element of ( bool M ));
assume that
A3: x c= y;
consider SFx being  non-empty (MSSubsetFamily of M) such that A4: (for Y being (ManySortedSet of I) holds (Y in SFx iff (Y in D & x c= Y))) by CKB76:1;
consider SFy being  non-empty (MSSubsetFamily of M) such that A5: (for Y being (ManySortedSet of I) holds (Y in SFy iff (Y in D & y c= Y))) by CKB76:1;
SFy c= SFx
proof
let i;
assume A6: i in I;
then consider Fi being non  empty set such that A7: Fi = ( D . i );
A8: ( x . i ) c= ( y . i ) by A3,A6,PBOOLE:def 2;
(( SFx . i ) = { t where t is (Element of Fi): ( x . i ) c= t } & ( SFy . i ) = { z where z is (Element of Fi): ( y . i ) c= z }) by A4,A5,A6,A7,CKB77:1;
hence thesis by A8,CKB2:1;
end;
then A9: ( meet SFx ) c= ( meet SFy ) by MSSUBFAM:46;
( J .. x ) = ( meet SFx ) by A1,A4;
hence thesis by A1,A5,A9;
end;

end;
