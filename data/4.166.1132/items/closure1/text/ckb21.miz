environ
vocabularies PBOOLE,FUNCT_1,RELAT_1,TARSKI,FUNCT_4,FUNCOP_1,XBOOLE_0,SUBSET_1,FINSEQ_4,PARTFUN1,FUNCT_6,MEMBER_1,MSUALG_3,FINSET_1,ZFMISC_1,RELAT_2,MSAFREE2,BINOP_1,YELLOW_6,STRUCT_0,MSUALG_1,CLOSURE2,MSSUBFAM,SETFAM_1,CLOSURE1;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,STRUCT_0,SETFAM_1,RELAT_1,FUNCT_1,FINSET_1,PARTFUN1,FUNCT_2,FUNCOP_1,FUNCT_4,PBOOLE,FUNCT_6,MSUALG_1,MSUALG_3,PRALG_1,MSSUBFAM,CKB5,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17;
definitions XBOOLE_0,FUNCT_1,MSSUBFAM,MSUALG_1,MSUALG_3,PBOOLE,FINSET_1,TARSKI,FUNCOP_1,CKB14,CKB15,CKB16,CKB17;
theorems FUNCOP_1,FUNCT_1,FUNCT_2,FUNCT_4,MBOOLEAN,MSSUBFAM,MSUALG_1,MSUALG_3,PBOOLE,PZFMISC1,TARSKI,ZFMISC_1,PRALG_1,XBOOLE_0,XBOOLE_1,PARTFUN1,RELAT_1,CKB2,CKB3,CKB6,CKB7,CKB8,CKB9,CKB10,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20;
schemes MSSUBFAM,PBOOLE,XBOOLE_0,CKB1;
registrations SUBSET_1,FUNCT_1,RELSET_1,FUNCOP_1,FINSET_1,PBOOLE,MSSUBFAM,MSUALG_1,RELAT_1,PRE_CIRC,PRALG_1,CKB4,CKB11;
constructors SETFAM_1,PZFMISC1,MSSUBFAM,PRALG_1,MSUALG_3,RELSET_1,FUNCT_4,FUNCT_6,CKB5,CKB13,CKB14,CKB15,CKB16,CKB17;
requirements SUBSET,BOOLE;
begin
reserve i for set;
reserve x for set;
reserve I for set;
reserve M for (ManySortedSet of I);
theorem
(for X being (Element of ( bool M )) holds (for i,x being set holds ((i in I & x in ( ( ( id ( bool M ) ) .. X ) . i )) implies (ex Y being  finite-yielding (Element of ( bool M )) st (Y c= X & x in ( ( ( id ( bool M ) ) .. Y ) . i ))))))
proof
let X being (Element of ( bool M ));
let i being set;
let x being set;
assume that
A1: i in I
and
A2: x in ( ( ( id ( bool M ) ) .. X ) . i );
A3: x in ( X . i ) by A2,CKB18:1;
set Y = ( ( I --> ( {} ) ) +* ( i .--> { x } ) );
( dom Y ) = I by A1,PZFMISC1:1;
then reconsider Y as (ManySortedSet of I) by PARTFUN1:def 2,RELAT_1:def 18;
A4: ( dom ( i .--> { x } ) ) = { i } by FUNCOP_1:13;
i in { i } by TARSKI:def 1;
then A5: ( Y . i ) = ( ( i .--> { x } ) . i ) by A4,FUNCT_4:13
.= { x } by FUNCOP_1:72;
X in ( bool M ) by MSSUBFAM:12;
then X c= M by MBOOLEAN:18;
then A6: ( X . i ) c= ( M . i ) by A1,PBOOLE:def 2;
Y is (Element of ( bool M ))
proof
let j being set;
assume that
A7: j in I;
now
per cases ;
case A8: j = i;
then { x } c= ( M . j ) by A3,A6,ZFMISC_1:31;
hence thesis by A5,A7,A8,MBOOLEAN:def 1;
end;
case j <> i;
then (not j in ( dom ( i .--> { x } ) )) by A4,TARSKI:def 1;
then ( Y . j ) = ( ( I --> ( {} ) ) . j ) by FUNCT_4:11;
then ( Y . j ) = ( {} ) by A7,FUNCOP_1:7;
then ( Y . j ) c= ( M . j ) by XBOOLE_1:2;
hence thesis by A7,MBOOLEAN:def 1;
end;
end;
hence thesis;
end;
then reconsider Y as (Element of ( bool M ));
Y is  finite-yielding
proof
let j being set;
assume that
A9: j in I;
now
per cases ;
case j = i;
hence thesis by A5;
end;
case j <> i;
then (not j in ( dom ( i .--> { x } ) )) by A4,TARSKI:def 1;
then ( Y . j ) = ( ( I --> ( {} ) ) . j ) by FUNCT_4:11;
hence thesis by A9,FUNCOP_1:7;
end;
end;
hence thesis;
end;
then reconsider Y as  finite-yielding (Element of ( bool M ));
take Y;
thus Y c= X
proof
let j being set;
assume that
A10: j in I;
now
per cases ;
case j = i;
hence thesis by A3,A5,ZFMISC_1:31;
end;
case j <> i;
then (not j in ( dom ( i .--> { x } ) )) by A4,TARSKI:def 1;
then ( Y . j ) = ( ( I --> ( {} ) ) . j ) by FUNCT_4:11;
then ( Y . j ) = ( {} ) by A10,FUNCOP_1:7;
hence thesis by XBOOLE_1:2;
end;
end;
hence thesis;
end;

x in ( Y . i ) by A5,TARSKI:def 1;
hence thesis by CKB18:1;
end;
