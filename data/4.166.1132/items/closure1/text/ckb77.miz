constructors SETFAM_1,PZFMISC1,MSSUBFAM,PRALG_1,MSUALG_3,RELSET_1,FUNCT_4,FUNCT_6,CKB5,CKB13,CKB14,CKB15,CKB16,CKB17,CKB32,CKB41,CKB42,CKB43,CKB44,CKB45,CKB46,CKB47,CKB48,CKB71;
requirements SUBSET,BOOLE;
begin
reserve i for set;
reserve x for set;
reserve I for set;
reserve M for (ManySortedSet of I);
theorem
Th32: (for D being  properly-upper-bound (MSSubsetFamily of M) holds (for X being (Element of ( bool M )) holds (for SF being  non-empty (MSSubsetFamily of M) holds ((for Y being (ManySortedSet of I) holds (Y in SF iff (Y in D & X c= Y))) implies (for i being set holds (for Di being non  empty set holds ((i in I & Di = ( D . i )) implies ( SF . i ) = { z where z is (Element of Di): ( X . i ) c= z })))))))
proof
let D being  properly-upper-bound (MSSubsetFamily of M);
let X being (Element of ( bool M ));
let SF being  non-empty (MSSubsetFamily of M);
assume that
A1: (for Y being (ManySortedSet of I) holds (Y in SF iff (Y in D & X c= Y)));
let i being set;
let Di being non  empty set;
assume that
A2: i in I
and
A3: Di = ( D . i );
thus ( SF . i ) c= { z where z is (Element of Di): ( X . i ) c= z }
proof
consider K being (ManySortedSet of I) such that A4: K in SF by PBOOLE:134;
let x;
assume that
A5: x in ( SF . i );
( dom ( K +* ( i .--> x ) ) ) = I by A2,PZFMISC1:1;
then reconsider L = ( K +* ( i .--> x ) ) as (ManySortedSet of I) by PARTFUN1:def 2,RELAT_1:def 18;
A6: ( dom ( i .--> x ) ) = { i } by FUNCOP_1:13;
i in { i } by TARSKI:def 1;
then A7: ( L . i ) = ( ( i .--> x ) . i ) by A6,FUNCT_4:13
.= x by FUNCOP_1:72;
A8: L in SF
proof
let s being set;
assume that
A9: s in I;
per cases ;
suppose s = i;

hence thesis by A5,A7;
end;
suppose s <> i;

then (not s in ( dom ( i .--> x ) )) by A6,TARSKI:def 1;
then ( L . s ) = ( K . s ) by FUNCT_4:11;
hence thesis by A4,A9,PBOOLE:def 1;
end;
end;
then X c= L by A1;
then A10: ( X . i ) c= ( L . i ) by A2,PBOOLE:def 2;
L in D by A1,A8;
then ( L . i ) in ( D . i ) by A2,PBOOLE:def 1;
hence thesis by A3,A7,A10;
end;

let x;
assume x in { z where z is (Element of Di): ( X . i ) c= z };
then consider g being (Element of Di) such that A11: g = x and A12: ( X . i ) c= g;
( dom ( M +* ( i .--> g ) ) ) = I by A2,PZFMISC1:1;
then reconsider L1 = ( M +* ( i .--> g ) ) as (ManySortedSet of I) by PARTFUN1:def 2,RELAT_1:def 18;
A13: ( dom ( i .--> g ) ) = { i } by FUNCOP_1:13;
i in { i } by TARSKI:def 1;
then A14: ( L1 . i ) = ( ( i .--> g ) . i ) by A13,FUNCT_4:13
.= g by FUNCOP_1:72;
A15: X c= L1
proof
let s being set;
assume that
A16: s in I;
per cases ;
suppose s = i;

hence thesis by A12,A14;
end;
suppose A17: s <> i;

X in ( bool M ) by MSSUBFAM:12;
then A18: X c= M by MBOOLEAN:18;
(not s in ( dom ( i .--> g ) )) by A13,A17,TARSKI:def 1;
then ( L1 . s ) = ( M . s ) by FUNCT_4:11;
hence thesis by A16,A18,PBOOLE:def 2;
end;
end;
A19: M in D by MSSUBFAM:def 6;
L1 in D
proof
let s being set;
assume that
A20: s in I;
per cases ;
suppose s = i;

hence thesis by A3,A14;
end;
suppose s <> i;

then (not s in ( dom ( i .--> g ) )) by A13,TARSKI:def 1;
then ( L1 . s ) = ( M . s ) by FUNCT_4:11;
hence thesis by A19,A20,PBOOLE:def 1;
end;
end;
then L1 in SF by A1,A15;
hence thesis by A2,A11,A14,PBOOLE:def 1;
end;
