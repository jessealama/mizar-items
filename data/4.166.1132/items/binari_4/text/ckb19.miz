begin
reserve n for non  empty Nat;
theorem
(for x,y being (Tuple of n,( BOOLEAN )) holds ((x = ( 0* n ) & y = ( 0* n )) implies x,y are_summable ))
proof
let x being (Tuple of n,( BOOLEAN ));
let y being (Tuple of n,( BOOLEAN ));
assume that
A1: x = ( 0* n )
and
A2: y = ( 0* n );
A3: 1 <= n by NAT_1:14;
then A4: n in ( Seg n ) by FINSEQ_1:1;
( len x ) = n by CARD_1:def 7;
then ( x /. n ) = ( ( 0* n ) . n ) by A1,A3,FINSEQ_4:15
.= ( FALSE ) by A4,FUNCOP_1:7;
then ( add_ovfl (x,y) ) = ( ( ( ( FALSE ) '&' ( FALSE ) ) 'or' ( ( FALSE ) '&' ( ( carry (x,y) ) /. n ) ) ) 'or' ( ( FALSE ) '&' ( ( carry (x,y) ) /. n ) ) ) by A1,A2,BINARITH:def 6
.= ( FALSE );
hence thesis by BINARITH:def 7;
end;
