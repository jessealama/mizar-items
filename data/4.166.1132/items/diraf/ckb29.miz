environ
vocabularies XBOOLE_0,SUBSET_1,RELAT_1,ZFMISC_1,ANALOAF,STRUCT_0,PARSP_1,PARSP_2,DIRAF;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,DOMAIN_1,RELSET_1,STRUCT_0,ANALOAF,CKB1,CKB2,CKB11,CKB23;
definitions ANALOAF,STRUCT_0,CKB1,CKB2,CKB11,CKB23;
theorems ANALOAF,RELAT_1,ZFMISC_1,DOMAIN_1,STRUCT_0,CKB1,CKB2,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28;
schemes RELSET_1;
registrations XBOOLE_0,SUBSET_1,RELSET_1,STRUCT_0,ANALOAF,CKB3;
constructors DOMAIN_1,ANALOAF,CKB1,CKB2,CKB11,CKB23;
requirements SUBSET,BOOLE;
begin
reserve S for OAffinSpace;
reserve x for (Element of S);
reserve y for (Element of S);
reserve z for (Element of S);
reserve t for (Element of S);
theorem
Th27: (x,y '||' z,t implies ((((((x,y '||' t,z & y,x '||' z,t) & y,x '||' t,z) & z,t '||' x,y) & z,t '||' y,x) & t,z '||' x,y) & t,z '||' y,x))
proof
assume x,y '||' z,t;
then A1: (x,y // z,t or x,y // t,z) by CKB23:def 1;
hence x,y '||' t,z by CKB23:def 1;
(y,x // t,z or y,x // z,t) by A1,ANALOAF:def 5;
hence (y,x '||' z,t & y,x '||' t,z) by CKB23:def 1;
(z,t // x,y or z,t // y,x) by A1,CKB6:1;
hence (z,t '||' x,y & z,t '||' y,x) by CKB23:def 1;
(t,z // y,x or t,z // x,y) by A1,CKB6:1;
hence thesis by CKB23:def 1;
end;
