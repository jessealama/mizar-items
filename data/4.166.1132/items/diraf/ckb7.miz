environ
vocabularies XBOOLE_0,SUBSET_1,RELAT_1,ZFMISC_1,ANALOAF,STRUCT_0,PARSP_1,PARSP_2,DIRAF;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,DOMAIN_1,RELSET_1,STRUCT_0,ANALOAF,CKB1,CKB2;
definitions ANALOAF,STRUCT_0,CKB1,CKB2;
theorems ANALOAF,RELAT_1,ZFMISC_1,DOMAIN_1,STRUCT_0,CKB1,CKB2,CKB4,CKB5,CKB6;
schemes RELSET_1;
registrations XBOOLE_0,SUBSET_1,RELSET_1,STRUCT_0,ANALOAF,CKB3;
constructors DOMAIN_1,ANALOAF,CKB1,CKB2;
requirements SUBSET,BOOLE;
begin
reserve S for OAffinSpace;
reserve x for (Element of S);
reserve y for (Element of S);
reserve z for (Element of S);
reserve t for (Element of S);
reserve u for (Element of S);
reserve w for (Element of S);
theorem
Th6: (((z <> t & x,y // z,t) & z,t // u,w) implies x,y // u,w)
proof
assume A1: z <> t;
assume that
A2: x,y // z,t
and
A3: z,t // u,w;
z,t // x,y by A2,CKB6:1;
hence thesis by A1,A3,ANALOAF:def 5;
end;
