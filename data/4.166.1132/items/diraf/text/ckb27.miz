environ
vocabularies XBOOLE_0,SUBSET_1,RELAT_1,ZFMISC_1,ANALOAF,STRUCT_0,PARSP_1,PARSP_2,DIRAF;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,DOMAIN_1,RELSET_1,STRUCT_0,ANALOAF,CKB1,CKB2,CKB11,CKB23;
definitions ANALOAF,STRUCT_0,CKB1,CKB2,CKB11,CKB23;
theorems ANALOAF,RELAT_1,ZFMISC_1,DOMAIN_1,STRUCT_0,CKB1,CKB2,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26;
schemes RELSET_1;
registrations XBOOLE_0,SUBSET_1,RELSET_1,STRUCT_0,ANALOAF,CKB3;
constructors DOMAIN_1,ANALOAF,CKB1,CKB2,CKB11,CKB23;
requirements SUBSET,BOOLE;
begin
reserve S for OAffinSpace;
reserve x for (Element of S);
reserve y for (Element of S);
reserve z for (Element of S);
reserve t for (Element of S);
reserve u for (Element of S);
reserve w for (Element of S);
theorem
Lm2: (((x <> y & x,y '||' z,t) & x,y '||' u,w) implies z,t '||' u,w)
proof
assume that
A1: x <> y
and
A2: x,y '||' z,t
and
A3: x,y '||' u,w;
A4: (x,y // u,w or x,y // w,u) by A3,CKB23:def 1;
A5:now
assume x,y // t,z;
then (t,z // u,w or t,z // w,u) by A1,A4,ANALOAF:def 5;
then (z,t // w,u or z,t // u,w) by ANALOAF:def 5;
hence thesis by CKB23:def 1;
end;
now
assume x,y // z,t;
then (z,t // u,w or z,t // w,u) by A1,A4,ANALOAF:def 5;
hence thesis by CKB23:def 1;
end;
hence thesis by A2,A5,CKB23:def 1;
end;
