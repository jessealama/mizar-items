environ
vocabularies XBOOLE_0,SUBSET_1,RELAT_1,ZFMISC_1,ANALOAF,STRUCT_0,PARSP_1,PARSP_2,DIRAF;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,DOMAIN_1,RELSET_1,STRUCT_0,ANALOAF,CKB1,CKB2,CKB11,CKB23;
definitions ANALOAF,STRUCT_0,CKB1,CKB2,CKB11,CKB23;
theorems ANALOAF,RELAT_1,ZFMISC_1,DOMAIN_1,STRUCT_0,CKB1,CKB2,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27;
schemes RELSET_1;
registrations XBOOLE_0,SUBSET_1,RELSET_1,STRUCT_0,ANALOAF,CKB3;
constructors DOMAIN_1,ANALOAF,CKB1,CKB2,CKB11,CKB23;
requirements SUBSET,BOOLE;
begin
reserve S for OAffinSpace;
reserve x for (Element of S);
reserve y for (Element of S);
reserve z for (Element of S);
theorem
Th26: (x,y '||' x,z implies y,x '||' y,z)
proof
A1:now
assume x,y // z,x;
then y,x // x,z by ANALOAF:def 5;
then y,x // y,z by ANALOAF:def 5;
hence thesis by CKB23:def 1;
end;
A2:now
A3:now
assume x,z // z,y;
then y,z // z,x by CKB6:1;
then y,z // y,x by ANALOAF:def 5;
then y,x // y,z by CKB6:1;
hence thesis by CKB23:def 1;
end;
A4:now
assume x,y // y,z;
then y,x // z,y by ANALOAF:def 5;
hence thesis by CKB23:def 1;
end;
assume x,y // x,z;
hence thesis by A4,A3,ANALOAF:def 5;
end;
assume x,y '||' x,z;
hence thesis by A2,A1,CKB23:def 1;
end;
