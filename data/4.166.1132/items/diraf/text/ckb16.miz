environ
vocabularies XBOOLE_0,SUBSET_1,RELAT_1,ZFMISC_1,ANALOAF,STRUCT_0,PARSP_1,PARSP_2,DIRAF;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,DOMAIN_1,RELSET_1,STRUCT_0,ANALOAF,CKB1,CKB2,CKB11;
definitions ANALOAF,STRUCT_0,CKB1,CKB2,CKB11;
theorems ANALOAF,RELAT_1,ZFMISC_1,DOMAIN_1,STRUCT_0,CKB1,CKB2,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15;
schemes RELSET_1;
registrations XBOOLE_0,SUBSET_1,RELSET_1,STRUCT_0,ANALOAF,CKB3;
constructors DOMAIN_1,ANALOAF,CKB1,CKB2,CKB11;
requirements SUBSET,BOOLE;
begin
reserve S for OAffinSpace;
reserve a for (Element of S);
reserve b for (Element of S);
reserve c for (Element of S);
reserve d for (Element of S);
theorem
(( Mid a,b,c &  Mid a,c,d) implies  Mid b,c,d)
proof
assume that
A1:  Mid a,b,c
and
A2:  Mid a,c,d;
now
A3: a,b // b,c by A1,CKB11:def 1;
then a,b // a,c by ANALOAF:def 5;
then A4: (b,c // a,c or a = b) by A3,ANALOAF:def 5;
assume A5: a <> c;
a,c // c,d by A2,CKB11:def 1;
then b,c // c,d by A5,A4,CKB7:1;
hence thesis by CKB11:def 1;
end;
hence thesis by A1,A2,CKB13:1;
end;
