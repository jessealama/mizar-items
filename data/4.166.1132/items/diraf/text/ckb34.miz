environ
vocabularies XBOOLE_0,SUBSET_1,RELAT_1,ZFMISC_1,ANALOAF,STRUCT_0,PARSP_1,PARSP_2,DIRAF;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,DOMAIN_1,RELSET_1,STRUCT_0,ANALOAF,CKB1,CKB2,CKB11,CKB23;
definitions ANALOAF,STRUCT_0,CKB1,CKB2,CKB11,CKB23;
theorems ANALOAF,RELAT_1,ZFMISC_1,DOMAIN_1,STRUCT_0,CKB1,CKB2,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33;
schemes RELSET_1;
registrations XBOOLE_0,SUBSET_1,RELSET_1,STRUCT_0,ANALOAF,CKB3;
constructors DOMAIN_1,ANALOAF,CKB1,CKB2,CKB11,CKB23;
requirements SUBSET,BOOLE;
begin
reserve S for OAffinSpace;
reserve p for (Element of S);
reserve q for (Element of S);
reserve r for (Element of S);
reserve x for (Element of S);
reserve y for (Element of S);
reserve z for (Element of S);
reserve t for (Element of S);
reserve u for (Element of S);
theorem
Th32: ((z,x '||' x,t & x <> z) implies (ex u st (y,x '||' x,u & y,z '||' t,u)))
proof
assume that
A1: z,x '||' x,t
and
A2: x <> z;
A3:now
consider p such that A4:  Mid z,x,p and A5: x <> p by CKB18:1;
A6: z,x // x,p by A4,CKB11:def 1;
then consider q such that A7: y,x // x,q and A8: y,z // p,q by A2,ANALOAF:def 5;
assume A9: z,x // t,x;
then x,p // t,x by A2,A6,ANALOAF:def 5;
then p,x // x,t by CKB6:1;
then consider r such that A10: q,x // x,r and A11: q,p // t,r by A5,ANALOAF:def 5;
A12:now
assume q = p;
then A13: x,p // y,x by A7,CKB6:1;
x,p // z,x by A6,CKB6:1;
then z,x // y,x by A5,A13,ANALOAF:def 5;
then y,x // t,x by A2,A9,ANALOAF:def 5;
then A14: y,x '||' x,t by CKB23:def 1;
y,z // t,t by ANALOAF:def 5;
then y,z '||' t,t by CKB23:def 1;
hence thesis by A14;
end;
A15:now
A16: x,z // x,t by A9,CKB6:1;
assume A17: q = x;
p,x // x,z by A6,CKB6:1;
then y,z // x,z by A5,A8,A17,CKB7:1;
then y,z // x,t by A2,A16,CKB7:1;
then A18: y,z '||' t,x by CKB23:def 1;
y,x // x,x by CKB8:1;
then y,x '||' x,x by CKB23:def 1;
hence thesis by A18;
end;
now
assume that
A19: q <> p
and
A20: q <> x;
x,q // r,x by A10,CKB6:1;
then y,x // r,x by A7,A20,CKB7:1;
then A21: y,x '||' x,r by CKB23:def 1;
p,q // r,t by A11,CKB6:1;
then y,z // r,t by A8,A19,CKB7:1;
then y,z '||' t,r by CKB23:def 1;
hence thesis by A21;
end;
hence thesis by A12,A15;
end;
now
assume z,x // x,t;
then consider u such that A22: (y,x // x,u & y,z // t,u) by A2,ANALOAF:def 5;
(y,x '||' x,u & y,z '||' t,u) by A22,CKB23:def 1;
hence thesis;
end;
hence thesis by A1,A3,CKB23:def 1;
end;
