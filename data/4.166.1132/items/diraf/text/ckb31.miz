environ
vocabularies XBOOLE_0,SUBSET_1,RELAT_1,ZFMISC_1,ANALOAF,STRUCT_0,PARSP_1,PARSP_2,DIRAF;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,DOMAIN_1,RELSET_1,STRUCT_0,ANALOAF,CKB1,CKB2,CKB11,CKB23;
definitions ANALOAF,STRUCT_0,CKB1,CKB2,CKB11,CKB23;
theorems ANALOAF,RELAT_1,ZFMISC_1,DOMAIN_1,STRUCT_0,CKB1,CKB2,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30;
schemes RELSET_1;
registrations XBOOLE_0,SUBSET_1,RELSET_1,STRUCT_0,ANALOAF,CKB3;
constructors DOMAIN_1,ANALOAF,CKB1,CKB2,CKB11,CKB23;
requirements SUBSET,BOOLE;
begin
reserve S for OAffinSpace;
reserve x for (Element of S);
reserve y for (Element of S);
reserve z for (Element of S);
reserve t for (Element of S);
theorem
Th29: (ex x st (ex y st (ex z st (not x,y '||' x,z))))
proof
consider x,y,z,t such that A1: (not x,y // z,t) and A2: (not x,y // t,z) by ANALOAF:def 5;
A3: x <> y by A1,CKB8:1;
now
assume A4: x,y '||' x,z;
thus (not x,y '||' x,t)
proof
assume A5: x,y '||' x,t;
then x,z '||' x,t by A3,A4,CKB30:1;
then z,x '||' z,t by CKB28:1;
then x,z '||' z,t by CKB29:1;
then (x,y '||' z,t or x = z) by A4,CKB30:1;
hence contradiction by A1,A2,A5,CKB23:def 1;
end;

end;
hence thesis;
end;
