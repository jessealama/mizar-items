environ
vocabularies XBOOLE_0,SUBSET_1,RELAT_1,ZFMISC_1,ANALOAF,STRUCT_0,PARSP_1,PARSP_2,DIRAF;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,DOMAIN_1,RELSET_1,STRUCT_0,ANALOAF,CKB1,CKB2,CKB11,CKB23,CKB35,CKB36;
definitions ANALOAF,STRUCT_0,CKB1,CKB2,CKB11,CKB23,CKB35;
theorems ANALOAF,RELAT_1,ZFMISC_1,DOMAIN_1,STRUCT_0,CKB1,CKB2,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB37,CKB38,CKB39,CKB40,CKB41,CKB42,CKB43,CKB44,CKB45,CKB46,CKB47,CKB48;
schemes RELSET_1;
registrations XBOOLE_0,SUBSET_1,RELSET_1,STRUCT_0,ANALOAF,CKB3;
constructors DOMAIN_1,ANALOAF,CKB1,CKB2,CKB11,CKB23,CKB35;
requirements SUBSET,BOOLE;
begin
reserve S for OAffinSpace;
reserve x for (Element of S);
reserve y for (Element of S);
reserve z for (Element of S);
reserve t for (Element of S);
reserve u for (Element of S);
reserve w for (Element of S);
reserve AS for non  empty AffinStruct;
theorem
Th46: (AS = ( Lambda S ) implies ((((((ex x,y being (Element of AS) st x <> y) & (for x,y,z,t,u,w being (Element of AS) holds (((x,y // y,x & x,y // z,z) & (((x <> y & x,y // z,t) & x,y // u,w) implies z,t // u,w)) & (x,y // x,z implies y,x // y,z)))) & (ex x,y,z being (Element of AS) st (not x,y // x,z))) & (for x,y,z being (Element of AS) holds (ex t being (Element of AS) st (x,z // y,t & y <> t)))) & (for x,y,z being (Element of AS) holds (ex t being (Element of AS) st (x,y // z,t & x,z // y,t)))) & (for x,y,z,t being (Element of AS) holds ((z,x // x,t & x <> z) implies (ex u being (Element of AS) st (y,x // x,u & y,z // t,u))))))
proof
assume A1: AS = ( Lambda S );
hence (ex x,y being (Element of AS) st x <> y) by STRUCT_0:def 10;
thus (for x,y,z,t,u,w being (Element of AS) holds (((x,y // y,x & x,y // z,z) & (((x <> y & x,y // z,t) & x,y // u,w) implies z,t // u,w)) & (x,y // x,z implies y,x // y,z)))
proof
let x being (Element of AS);
let y being (Element of AS);
let z being (Element of AS);
let t being (Element of AS);
let u being (Element of AS);
let w being (Element of AS);
reconsider x9 = x,y9 = y,z9 = z,t9 = t,u9 = u,w9 = w as (Element of S) by A1;
(x9,y9 '||' y9,x9 & x9,y9 '||' z9,z9) by CKB25:1,CKB26:1;
hence (x,y // y,x & x,y // z,z) by A1,CKB48:1;
(((x9 <> y9 & x9,y9 '||' z9,t9) & x9,y9 '||' u9,w9) implies z9,t9 '||' u9,w9) by CKB27:1;
hence (((x <> y & x,y // z,t) & x,y // u,w) implies z,t // u,w) by A1,CKB48:1;
(x9,y9 '||' x9,z9 implies y9,x9 '||' y9,z9) by CKB28:1;
hence thesis by A1,CKB48:1;
end;

thus (ex x,y,z being (Element of AS) st (not x,y // x,z))
proof
consider x9,y9,z9 being (Element of S) such that A2: (not x9,y9 '||' x9,z9) by CKB31:1;
reconsider x = x9,y = y9,z = z9 as (Element of AS) by A1;
(not x,y // x,z) by A1,A2,CKB48:1;
hence thesis;
end;

thus (for x,y,z being (Element of AS) holds (ex t being (Element of AS) st (x,z // y,t & y <> t)))
proof
let x being (Element of AS);
let y being (Element of AS);
let z being (Element of AS);
reconsider x9 = x,y9 = y,z9 = z as (Element of S) by A1;
consider t9 being (Element of S) such that A3: x9,z9 '||' y9,t9 and A4: y9 <> t9 by CKB32:1;
reconsider t = t9 as (Element of AS) by A1;
x,z // y,t by A1,A3,CKB48:1;
hence thesis by A4;
end;

thus (for x,y,z being (Element of AS) holds (ex t being (Element of AS) st (x,y // z,t & x,z // y,t)))
proof
let x being (Element of AS);
let y being (Element of AS);
let z being (Element of AS);
reconsider x9 = x,y9 = y,z9 = z as (Element of S) by A1;
consider t9 being (Element of S) such that A5: (x9,y9 '||' z9,t9 & x9,z9 '||' y9,t9) by CKB33:1;
reconsider t = t9 as (Element of AS) by A1;
(x,y // z,t & x,z // y,t) by A1,A5,CKB48:1;
hence thesis;
end;

thus (for x,y,z,t being (Element of AS) holds ((z,x // x,t & x <> z) implies (ex u being (Element of AS) st (y,x // x,u & y,z // t,u))))
proof
let x being (Element of AS);
let y being (Element of AS);
let z being (Element of AS);
let t being (Element of AS);
assume that
A6: z,x // x,t
and
A7: x <> z;
reconsider x9 = x,y9 = y,z9 = z,t9 = t as (Element of S) by A1;
z9,x9 '||' x9,t9 by A1,A6,CKB48:1;
then consider u9 being (Element of S) such that A8: (y9,x9 '||' x9,u9 & y9,z9 '||' t9,u9) by A7,CKB34:1;
reconsider u = u9 as (Element of AS) by A1;
(y,x // x,u & y,z // t,u) by A1,A8,CKB48:1;
hence thesis;
end;

end;
