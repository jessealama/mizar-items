environ
vocabularies XBOOLE_0,SUBSET_1,RELAT_1,ZFMISC_1,ANALOAF,STRUCT_0,PARSP_1,PARSP_2,DIRAF;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,DOMAIN_1,RELSET_1,STRUCT_0,ANALOAF,CKB1,CKB2,CKB11;
definitions ANALOAF,STRUCT_0,CKB1,CKB2,CKB11;
theorems ANALOAF,RELAT_1,ZFMISC_1,DOMAIN_1,STRUCT_0,CKB1,CKB2,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18;
schemes RELSET_1;
registrations XBOOLE_0,SUBSET_1,RELSET_1,STRUCT_0,ANALOAF,CKB3;
constructors DOMAIN_1,ANALOAF,CKB1,CKB2,CKB11;
requirements SUBSET,BOOLE;
begin
reserve S for OAffinSpace;
reserve x for (Element of S);
reserve y for (Element of S);
reserve z for (Element of S);
theorem
(( Mid x,y,z &  Mid y,x,z) implies x = y)
proof
assume that
A1:  Mid x,y,z
and
A2:  Mid y,x,z;
x,y // y,z by A1,CKB11:def 1;
then A3: x,y // x,z by ANALOAF:def 5;
y,x // x,z by A2,CKB11:def 1;
then A4: x,y // z,x by ANALOAF:def 5;
(x = z implies x = y) by A1,CKB13:1;
hence thesis by A3,A4,CKB9:1;
end;
