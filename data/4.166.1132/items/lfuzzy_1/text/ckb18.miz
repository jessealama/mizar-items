begin
reserve X for non  empty set;
reserve Y for non  empty set;
theorem
Lm1: (for X,Y,Z being non  empty set holds (for R being (RMembership_Func of X,Y) holds (for S being (RMembership_Func of Y,Z) holds (for x being (Element of X) holds (for z being (Element of Z) holds (( rng ( min (R,S,x,z) ) ) = { ( ( R . [ x,y ] ) "/\" ( S . [ y,z ] ) ) where y is (Element of Y): (not contradiction) } & ( rng ( min (R,S,x,z) ) ) <> ( {} )))))))
proof
let X being non  empty set;
let Y being non  empty set;
let Z being non  empty set;
let R being (RMembership_Func of X,Y);
let S being (RMembership_Func of Y,Z);
let x being (Element of X);
let z being (Element of Z);
set L = { ( ( R . [ x,y ] ) "/\" ( S . [ y,z ] ) ) where y is (Element of Y): (not contradiction) };
A1: (Y = ( dom ( min (R,S,x,z) ) ) & ( min (R,S,x,z) ) is (PartFunc of ( dom ( min (R,S,x,z) ) ),( rng ( min (R,S,x,z) ) ))) by FUNCT_2:def 1,RELSET_1:4;
(for c being set holds (c in L iff c in ( rng ( min (R,S,x,z) ) )))
proof
let c being set;
hereby
assume c in L;
then consider y being (Element of Y) such that A2: c = ( ( R . [ x,y ] ) "/\" ( S . [ y,z ] ) );
c = ( ( min (R,S,x,z) ) . y ) by A2,FUZZY_4:def 2;
hence c in ( rng ( min (R,S,x,z) ) ) by A1,PARTFUN1:4;
end;
assume c in ( rng ( min (R,S,x,z) ) );
then consider y being (Element of Y) such that y in ( dom ( min (R,S,x,z) ) ) and A3: c = ( ( min (R,S,x,z) ) . y ) by PARTFUN1:3;
c = ( ( R . [ x,y ] ) "/\" ( S . [ y,z ] ) ) by A3,FUZZY_4:def 2;
hence thesis;
end;
hence ( rng ( min (R,S,x,z) ) ) = L by TARSKI:1;
thus ( rng ( min (R,S,x,z) ) ) <> ( {} );
end;
