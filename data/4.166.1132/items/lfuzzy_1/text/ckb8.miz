begin
reserve X for non  empty set;
reserve Y for non  empty set;
theorem
Th6: (for X,Y,Z being non  empty set holds (for R,S being (RMembership_Func of X,Y) holds (for T,U being (RMembership_Func of Y,Z) holds ((R c= S & T c= U) implies ( R (#) T ) c= ( S (#) U )))))
proof
let X being non  empty set;
let Y being non  empty set;
let Z being non  empty set;
let R being (RMembership_Func of X,Y);
let S being (RMembership_Func of X,Y);
let T being (RMembership_Func of Y,Z);
let U being (RMembership_Func of Y,Z);
assume A1: (R c= S & T c= U);
(for c being (Element of [: X,Z :]) holds ( ( R (#) T ) . c ) <= ( ( S (#) U ) . c ))
proof
let c being (Element of [: X,Z :]);
consider x,z being set such that A2: [ x,z ] = c by RELAT_1:def 1;
A3: (x in X & z in Z) by A2,ZFMISC_1:87;
(for y being set holds (y in Y implies (( R . [ x,y ] ) <= ( S . [ x,y ] ) & ( T . [ y,z ] ) <= ( U . [ y,z ] ))))
proof
let y being set;
assume y in Y;
then ([ x,y ] in [: X,Y :] & [ y,z ] in [: Y,Z :]) by A3,ZFMISC_1:87;
hence thesis by A1,FUZZY_1:def 2;
end;
hence thesis by A2,A3,FUZZY_4:18;
end;
hence thesis by FUZZY_1:def 2;
