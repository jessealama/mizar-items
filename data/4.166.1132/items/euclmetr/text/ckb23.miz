notations SUBSET_1,NUMBERS,STRUCT_0,REAL_1,RLVECT_1,ANALOAF,DIRAF,AFF_1,AFF_2,ANALMETR,GEOMTRAP,PAPDESAF,CONMETR,CONAFFM,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7;
