begin
reserve MS for OrtAfPl;
theorem
(MS is  translation iff MS is  satisfying_des)
proof
set AS = ( Af MS );
A1:now
assume MS is  satisfying_des;
then AS is  translational by CKB22:1;
hence MS is  translation by CKB6:def 1;
end;
now
assume MS is  translation;
then AS is  translational by CKB6:def 1;
hence MS is  satisfying_des by CKB22:1;
end;
hence thesis by A1;
end;
