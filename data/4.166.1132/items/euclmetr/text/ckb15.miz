environ
vocabularies ANALMETR,SUBSET_1,SYMSP_1,AFF_2,VECTSP_1,TRANSGEO,ANALOAF,DIRAF,STRUCT_0,AFF_1,INCSP_1,CONAFFM,CONMETR,RLVECT_1,ARYTM_1,ARYTM_3,REAL_1,RELAT_1,CARD_1,MCART_1,SUPINF_2,PARSP_1,EUCLMETR;
notations SUBSET_1,NUMBERS,STRUCT_0,REAL_1,RLVECT_1,ANALOAF,DIRAF,AFF_1,AFF_2,ANALMETR,GEOMTRAP,PAPDESAF,CONMETR,CONAFFM,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7;
definitions RLVECT_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7;
theorems RLVECT_1,AFF_1,AFF_4,AFPROJ,ANALOAF,ANALMETR,GEOMTRAP,PAPDESAF,CONMETR,CONAFFM,AFF_2,RLSUB_2,XCMPLX_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14;
registrations ANALMETR,PAPDESAF,REAL_1,ORDINAL1,XREAL_0,STRUCT_0;
constructors REAL_1,AFF_1,AFF_2,TRANSLAC,GEOMTRAP,CONAFFM,CONMETR,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7;
requirements NUMERALS,SUBSET,ARITHM,BOOLE;
begin
reserve MS for OrtAfPl;
theorem
Th8: (MS is  Euclidean iff MS is  satisfying_3H)
proof
A1:now
assume A2: MS is  satisfying_3H;
now
let a being (Element of MS);
let b being (Element of MS);
let c being (Element of MS);
let d being (Element of MS);
assume that
A3: a,b _|_ c,d
and
A4: b,c _|_ a,d;
A5:now
A6: (d,a _|_ c,b & d,c _|_ a,b) by A3,A4,ANALMETR:61;
assume A7: (not  LIN a,b,c);
then consider d1 being (Element of MS) such that A8: d1,a _|_ b,c and A9: (d1,b _|_ a,c & d1,c _|_ a,b) by A2,CONAFFM:def 3;
A10: (not  LIN a,c,b) by A7,CKB11:1;
d1,a _|_ c,b by A8,ANALMETR:61;
then d,b _|_ a,c by A9,A6,A10,CKB13:1;
hence b,d _|_ a,c by ANALMETR:61;
end;
now
A11: (a = c implies b,d _|_ a,c) by ANALMETR:58;
A12: (b = c implies b,d _|_ a,c) by A3,ANALMETR:61;
assume A13:  LIN a,b,c;
(a = b implies b,d _|_ a,c) by A4,ANALMETR:61;
hence b,d _|_ a,c by A3,A4,A13,A11,A12,CKB14:1;
end;
hence b,d _|_ a,c by A5;
end;
hence MS is  Euclidean by CKB1:def 1;
end;
now
assume A14: MS is  Euclidean;
now
let a being (Element of MS);
let b being (Element of MS);
let c being (Element of MS);
assume (not  LIN a,b,c);
then consider d being (Element of MS) such that A15: (d,a _|_ b,c & d,b _|_ a,c) by CKB12:1;
take d;
(b,c _|_ a,d & c,a _|_ b,d) by A15,ANALMETR:61;
then c,d _|_ b,a by A14,CKB1:def 1;
hence ((d,a _|_ b,c & d,b _|_ a,c) & d,c _|_ a,b) by A15,ANALMETR:61;
end;
hence MS is  satisfying_3H by CONAFFM:def 3;
end;
hence thesis by A1;
end;
