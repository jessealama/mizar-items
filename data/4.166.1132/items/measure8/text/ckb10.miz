begin
reserve X for set;
reserve F for (Field_Subset of X);
reserve A for (Subset of X);
reserve n for  natural number;
definition
let X;
let A;
let F;
mode Covering of A,F
 -> (Set_Sequence of F)
means :Def3: A c= ( union ( rng it ) );
existence
proof
X in ( bool X ) by ZFMISC_1:def 1;
then reconsider IT = ( ( NAT ) --> X ) as (SetSequence of X) by FUNCOP_1:45;
(for n being Nat holds ( IT . n ) in F)
proof
let n being Nat;
n in ( NAT ) by ORDINAL1:def 12;
then ( IT . n ) = X by FUNCOP_1:7;
hence ( IT . n ) in F by PROB_1:5;
end;
then reconsider IT as (Set_Sequence of F) by CKB9:def 1;
take IT;
( rng IT ) = { X } by FUNCOP_1:8;
then X c= ( union ( rng IT ) ) by ZFMISC_1:25;
hence thesis by XBOOLE_1:1;
end;
end;
