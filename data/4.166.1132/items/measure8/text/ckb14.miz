begin
reserve X for set;
reserve F for (Field_Subset of X);
reserve M for (Measure of F);
reserve n for  natural number;
reserve FSets for (Set_Sequence of F);
definition
let X;
let F;
let M;
let FSets;
func vol (M,FSets) -> ExtREAL_sequence means 
:Def5: (for n holds ( it . n ) = ( M . ( FSets . n ) ));
existence
proof
deffunc H((Element of ( NAT ))) = ( M . ( FSets . $1 ) );
consider IT being (Function of ( NAT ),( ExtREAL )) such that A1: (for n being (Element of ( NAT )) holds ( IT . n ) = H(n)) from FUNCT_2:sch 4;
take IT;
hereby
let n being Nat;
n in ( NAT ) by ORDINAL1:def 12;
hence ( IT . n ) = ( M . ( FSets . n ) ) by A1;
end;
end;
uniqueness
proof
let V1 being (Function of ( NAT ),( ExtREAL ));
let V2 being (Function of ( NAT ),( ExtREAL ));
assume that
A2: (for n being Nat holds ( V1 . n ) = ( M . ( FSets . n ) ))
and
A3: (for n being Nat holds ( V2 . n ) = ( M . ( FSets . n ) ));
now
let x being set;
assume x in ( NAT );
then reconsider n = x as Nat;
thus ( V1 . x ) = ( M . ( FSets . n ) ) by A2
.= ( V2 . x ) by A3;
end;
hence thesis by FUNCT_2:12;
end;
end;
