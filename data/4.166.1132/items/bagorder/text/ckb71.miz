begin
theorem
Th41: (for R being non  empty RelStr holds (for s being (sequence of R) holds (for j being Nat holds (s is  descending implies ( s ^\ j ) is  descending))))
proof
let R being non  empty RelStr;
let s1 being (sequence of R);
let j being Nat;
assume that
A1: s1 is  descending;
set s2 = ( s1 ^\ j );
set IR = (the InternalRel of R);
now
let n being Nat;
set nj = ( n + j );
A2: ( s2 . n ) = ( s1 . nj ) by NAT_1:def 3;
A3: ( s2 . ( n + 1 ) ) = ( s1 . ( ( n + 1 ) + j ) ) by NAT_1:def 3
.= ( s1 . ( ( n + j ) + 1 ) );
hence ( s2 . ( n + 1 ) ) <> ( s2 . n ) by A1,A2,WELLFND1:def 6;
thus [ ( s2 . ( n + 1 ) ),( s2 . n ) ] in IR by A1,A2,A3,WELLFND1:def 6;
end;
hence thesis by WELLFND1:def 6;
end;
