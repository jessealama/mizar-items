begin
reserve i for (Element of ( NAT ));
reserve n for (Element of ( NAT ));
reserve X for set;
reserve f for Function;
reserve g for Function;
definition
let F being  with_the_same_arity Relation;
func arity F -> (Element of ( NAT )) means 
:Def7: (for f being  homogeneous Function holds (f in ( rng F ) implies it = ( arity f ))) if (ex f being  homogeneous Function st f in ( rng F )) otherwise it = ( 0 );
existence
proof
hereby
given f being  homogeneous Function such that
A1: f in ( rng F );

take i = ( arity f );
let g being  homogeneous Function;
assume A2: g in ( rng F );
per cases ;
suppose A3: f is  empty;

thus i = ( arity g )
proof
per cases  by A1,A2,A3,CKB45:def 1;
suppose g is  empty;

hence thesis by A3;
end;
suppose ( dom g ) = { ( {} ) };

hence thesis by A3,CKB36:1,CKB37:1;
end;
end;

end;
suppose A4: g is  empty;

thus i = ( arity g )
proof
per cases  by A1,A2,A4,CKB45:def 1;
suppose f is  empty;

hence thesis by A4;
end;
suppose ( dom f ) = { ( {} ) };

hence thesis by A4,CKB36:1,CKB37:1;
end;
end;

end;
suppose A5: (f is non  empty & g is non  empty);

set a = the (Element of ( dom f ));
( dom f ) <> ( {} ) by A5;
then A6: a in ( dom f );
consider n being (Element of ( NAT )),X being non  empty set such that A7: ( dom f ) c= ( n -tuples_on X ) and A8: ( dom g ) c= ( n -tuples_on X ) by A1,A2,A5,CKB45:def 1;
reconsider a as (Element of ( n -tuples_on X )) by A7,A6;
A9: ( arity f ) = ( len a ) by A5,MARGREL1:def 25
.= n by CARD_1:def 7;
set a = the (Element of ( dom g ));
( dom g ) <> ( {} ) by A5;
then a in ( dom g );
then reconsider a as (Element of ( n -tuples_on X )) by A8;
( arity g ) = ( len a ) by A5,MARGREL1:def 25;
hence i = ( arity g ) by A9,CARD_1:def 7;
end;
end;
thus thesis;
end;
uniqueness
proof
let i1 being (Element of ( NAT ));
let i2 being (Element of ( NAT ));
hereby
given f being  homogeneous Function such that
A10: f in ( rng F );

assume (for f being  homogeneous Function holds (f in ( rng F ) implies i1 = ( arity f )));
then i1 = ( arity f ) by A10;
hence ((for f being  homogeneous Function holds (f in ( rng F ) implies i2 = ( arity f ))) implies i1 = i2) by A10;
end;
thus thesis;
end;
correctness;
