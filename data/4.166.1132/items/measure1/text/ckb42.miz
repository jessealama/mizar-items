requirements NUMERALS,REAL,SUBSET,BOOLE,ARITHM;
begin
reserve X for set;
registration
let X;
cluster  compl-closed  sigma-multiplicative ->  sigma-additive for (Subset-Family of X);
coherence
proof
let F being (Subset-Family of X);
assume that
A1: F is  compl-closed
and
A2: F is  sigma-multiplicative;
let M being non empty  countable (Subset-Family of X);
assume that
A3: M c= F;
consider f being (SetSequence of X) such that A4: M = ( rng f ) by SUPINF_2:def 8;
(for n being Nat holds ( ( Complement f ) . n ) in F)
proof
let n being Nat;
reconsider n as (Element of ( NAT )) by ORDINAL1:def 12;
(( f . n ) in M & ( ( Complement f ) . n ) = ( ( f . n ) ` )) by A4,FUNCT_2:4,PROB_1:def 2;
hence thesis by A1,A3,PROB_1:def 1;
end;
then ( rng ( Complement f ) ) c= F by NAT_1:52;
then A5: ( Intersection ( Complement f ) ) in F by A2,PROB_1:def 6;
( ( Intersection ( Complement f ) ) ` ) = ( union M ) by A4,CARD_3:def 4;
hence thesis by A1,A5,PROB_1:def 1;
end;
end;
