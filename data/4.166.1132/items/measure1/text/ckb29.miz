constructors ENUMSET1,PARTFUN1,FUNCOP_1,FINSUB_1,REAL_1,NAT_1,PROB_2,SUPINF_2,SUPINF_1,RELSET_1,FUNCT_7,CKB15,CKB16,CKB22,CKB23,CKB24,CKB27;
requirements NUMERALS,REAL,SUBSET,BOOLE,ARITHM;
begin
reserve X for set;
theorem
(for S being (Field_Subset of X) holds (for M being (Measure of S) holds (for A,B being (measure_zero of M) holds ((( A \/ B ) is (measure_zero of M) & ( A /\ B ) is (measure_zero of M)) & ( A \ B ) is (measure_zero of M)))))
proof
let S being (Field_Subset of X);
let M being (Measure of S);
let A being (measure_zero of M);
let B being (measure_zero of M);
A1: ( 0. ) <= ( M . ( A /\ B ) ) by CKB15:def 1;
A2: ( 0. ) <= ( M . ( A \ B ) ) by CKB15:def 1;
A3: ( M . A ) = ( 0. ) by CKB27:def 1;
then ( M . ( A \ B ) ) <= ( 0. ) by CKB19:1,XBOOLE_1:36;
then A4: ( M . ( A \ B ) ) = ( 0. ) by A2,XXREAL_0:1;
( M . B ) = ( 0. ) by CKB27:def 1;
then ( M . ( A \/ B ) ) <= ( ( 0. ) + ( 0. ) ) by A3,CKB25:1;
then A5: ( M . ( A \/ B ) ) <= ( 0. ) by XXREAL_3:4;
( 0. ) <= ( M . ( A \/ B ) ) by CKB15:def 1;
then A6: ( M . ( A \/ B ) ) = ( 0. ) by A5,XXREAL_0:1;
( M . ( A /\ B ) ) <= ( 0. ) by A3,CKB19:1,XBOOLE_1:17;
then ( M . ( A /\ B ) ) = ( 0. ) by A1,XXREAL_0:1;
hence thesis by A6,A4,CKB27:def 1;
end;
