begin
reserve X for set;
theorem
(for S being (Field_Subset of X) holds (for M being (Measure of S) holds (for A being (Element of S) holds (for B being (measure_zero of M) holds (A c= B implies A is (measure_zero of M))))))
proof
let S being (Field_Subset of X);
let M being (Measure of S);
let A being (Element of S);
let B being (measure_zero of M);
assume A c= B;
then ( M . A ) <= ( M . B ) by CKB19:1;
then A1: ( M . A ) <= ( 0. ) by CKB27:def 1;
( 0. ) <= ( M . A ) by CKB15:def 1;
then ( M . A ) = ( 0. ) by A1,XXREAL_0:1;
hence thesis by CKB27:def 1;
end;
