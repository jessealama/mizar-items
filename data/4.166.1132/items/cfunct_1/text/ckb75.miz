begin
reserve Y for set;
reserve C for non empty set;
reserve c for (Element of C);
reserve f for (PartFunc of C,( COMPLEX ));
theorem
Th81: (( f | Y ) is  bounded iff (ex p being  real number st (for c holds (c in ( Y /\ ( dom f ) ) implies |. ( f /. c ) .| <= p))))
proof
A1: ( dom |. f .| ) = ( dom f ) by VALUED_1:def 11;
A2: ( dom ( |. f .| | Y ) ) = ( Y /\ ( dom |. f .| ) ) by RELAT_1:61;
A3: ( |. f .| | Y ) = |. ( f | Y ) .| by RFUNCT_1:46;
hereby
assume ( f | Y ) is  bounded;
then ( |. f .| | Y ) is  bounded by A3,CKB74:1;
then consider p being  real number such that A4: (for c being set holds (c in ( dom ( |. f .| | Y ) ) implies ( abs ( ( |. f .| | Y ) . c ) ) <= p)) by RFUNCT_1:72;
now
let c;
assume that
A5: c in ( Y /\ ( dom f ) );
c in ( dom f ) by A5,XBOOLE_0:def 4;
then A6: ( f . c ) = ( f /. c ) by PARTFUN1:def 6;
( abs ( ( |. f .| | Y ) . c ) ) = ( abs ( |. f .| . c ) ) by A1,A2,A5,FUNCT_1:47
.= ( abs |. ( f . c ) .| ) by VALUED_1:18;
hence |. ( f /. c ) .| <= p by A1,A2,A4,A5,A6;
end;
hence (ex p being  real number st (for c holds (c in ( Y /\ ( dom f ) ) implies |. ( f /. c ) .| <= p)));
end;
given p being  real number such that
A7: (for c holds (c in ( Y /\ ( dom f ) ) implies |. ( f /. c ) .| <= p));

A8: ( dom |. f .| ) = ( dom f ) by VALUED_1:def 11;
now
let c being set;
assume that
A9: c in ( dom ( |. f .| | Y ) );
A10: c in ( dom |. f .| ) by A9,RELAT_1:57;
( abs ( ( |. f .| | Y ) . c ) ) = ( abs ( |. f .| . c ) ) by A9,FUNCT_1:47
.= ( abs |. ( f . c ) .| ) by VALUED_1:18
.= ( abs |. ( f /. c ) .| ) by A1,A10,PARTFUN1:def 6;
hence ( abs ( ( |. f .| | Y ) . c ) ) <= p by A2,A7,A8,A9;
end;
then ( |. f .| | Y ) is  bounded by RFUNCT_1:72;
hence thesis by A3,CKB74:1;
end;
