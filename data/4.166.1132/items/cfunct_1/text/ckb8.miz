begin
reserve C for non  empty set;
reserve c for (Element of C);
reserve f for (PartFunc of C,( COMPLEX ));
theorem
Th9: (( dom ( - f ) ) = ( dom f ) & (for c holds (c in ( dom ( - f ) ) implies ( ( - f ) /. c ) = ( - ( f /. c ) ))))
proof
thus A1: ( dom ( - f ) ) = ( dom f ) by VALUED_1:8;
now
let c;
assume A2: c in ( dom ( - f ) );
hence ( ( - f ) /. c ) = ( ( - f ) . c ) by PARTFUN1:def 6
.= ( - ( f . c ) ) by VALUED_1:8
.= ( - ( f /. c ) ) by A1,A2,PARTFUN1:def 6;
end;
hence thesis;
end;
