begin
reserve C for non  empty set;
reserve c for (Element of C);
reserve f for (PartFunc of C,( COMPLEX ));
reserve r for (Element of ( COMPLEX ));
theorem
Th7: (( dom ( r (#) f ) ) = ( dom f ) & (for c holds (c in ( dom ( r (#) f ) ) implies ( ( r (#) f ) /. c ) = ( r * ( f /. c ) ))))
proof
thus A1: ( dom ( r (#) f ) ) = ( dom f ) by VALUED_1:def 5;
now
let c;
assume A2: c in ( dom ( r (#) f ) );
hence ( ( r (#) f ) /. c ) = ( ( r (#) f ) . c ) by PARTFUN1:def 6
.= ( r * ( f . c ) ) by VALUED_1:6
.= ( r * ( f /. c ) ) by A1,A2,PARTFUN1:def 6;
end;
hence thesis;
end;
