begin
reserve C for non empty set;
reserve c for (Element of C);
reserve f for (PartFunc of C,( COMPLEX ));
definition
let C;
let f;
func f ^ -> (PartFunc of C,( COMPLEX ))
means
:Def2: (( dom it ) = ( ( dom f ) \ ( f " { ( 0 ) } ) ) & (for c holds (c in ( dom it ) implies ( it /. c ) = ( ( f /. c ) " ))));
existence
proof
deffunc F(set) = ( ( f /. $1 ) " );
defpred P[ set ]
 means
$1 in ( ( dom f ) \ ( f " { ( 0c ) } ) );
consider F being (PartFunc of C,( COMPLEX )) such that A1: (for c holds (c in ( dom F ) iff P[ c ])) and A2: (for c holds (c in ( dom F ) implies ( F /. c ) = F(c))) from PARTFUN2:sch 2;
take F;
thus ( dom F ) = ( ( dom f ) \ ( f " { ( 0 ) } ) ) by A1,SUBSET_1:3;
let c;
assume c in ( dom F );
hence thesis by A2;
end;
uniqueness
proof
deffunc F(set) = ( ( f /. $1 ) " );
set X = ( ( dom f ) \ ( f " { ( 0 ) } ) );
thus (for f,g being (PartFunc of C,( COMPLEX )) holds (((( dom f ) = X & (for c being (Element of C) holds (c in ( dom f ) implies ( f /. c ) = F(c)))) & (( dom g ) = X & (for c being (Element of C) holds (c in ( dom g ) implies ( g /. c ) = F(c))))) implies f = g)) from PARTFUN2:sch 3;
end;
end;
