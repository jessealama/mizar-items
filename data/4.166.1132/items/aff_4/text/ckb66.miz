environ
vocabularies DIRAF,SUBSET_1,ANALOAF,AFF_1,INCSP_1,STRUCT_0,XBOOLE_0,TARSKI,RELAT_1,PARSP_1,AFF_2,ARYTM_3,AFF_4;
notations TARSKI,XBOOLE_0,SUBSET_1,STRUCT_0,ANALOAF,DIRAF,AFF_1,AFF_2,CKB15,CKB16,CKB36,CKB44,CKB57;
definitions CKB15,CKB16,CKB36,CKB44,CKB57;
theorems AFF_1,DIRAF,TARSKI,AFF_2,XBOOLE_0,XBOOLE_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB37,CKB38,CKB39,CKB40,CKB41,CKB42,CKB43,CKB44,CKB45,CKB46,CKB47,CKB48,CKB49,CKB50,CKB51,CKB52,CKB53,CKB54,CKB55,CKB56,CKB57,CKB58,CKB59,CKB60,CKB61,CKB62,CKB63,CKB64,CKB65;
registrations XBOOLE_0,STRUCT_0;
constructors AFF_1,AFF_2,CKB15,CKB16,CKB36,CKB44,CKB57;
requirements SUBSET,BOOLE;
begin
reserve AS for AffinSpace;
reserve a for (Element of AS);
reserve b for (Element of AS);
reserve c for (Element of AS);
reserve a9 for (Element of AS);
reserve b9 for (Element of AS);
reserve c9 for (Element of AS);
reserve p for (Element of AS);
reserve x for (Element of AS);
reserve y for (Element of AS);
reserve A for (Subset of AS);
reserve C for (Subset of AS);
reserve P for (Subset of AS);
reserve X for (Subset of AS);
reserve Y for (Subset of AS);
theorem
Lm11: ((((((((((((((A // P & A // C) & (not A,P,C is_coplanar )) & a in A) & a9 in A) & b in P) & b9 in P) & c in C) & c9 in C) & A is  being_line) & A <> P) & A <> C) & a,b // a9,b9) & a,c // a9,c9) implies b,c // b9,c9)
proof
assume that
A1: A // P
and
A2: A // C
and
A3: (not A,P,C is_coplanar )
and
A4: a in A
and
A5: a9 in A
and
A6: b in P
and
A7: b9 in P
and
A8: c in C
and
A9: c9 in C
and
A10: A is  being_line
and
A11: A <> P
and
A12: A <> C
and
A13: a,b // a9,b9
and
A14: a,c // a9,c9;
A15: c <> a by A2,A4,A8,A12,AFF_1:45;
A16: P // C by A1,A2,AFF_1:44;
then consider X such that A17: (P c= X & C c= X) and A18: X is  being_plane by CKB52:1;
consider Y such that A19: A c= Y and A20: C c= Y and A21: Y is  being_plane by A2,CKB52:1;
A22: P <> C by A3,A19,A20,A21,CKB57:def 1;
then A23: b <> c by A6,A8,A16,AFF_1:45;
A24: a <> b by A1,A4,A6,A11,AFF_1:45;
A25:now
set BC = ( Line (b,c) );
set BC9 = ( Line (b9,c9) );
set AB = ( Line (a,b) );
set AB9 = ( Line (a9,b9) );
set AC = ( Line (a,c) );
set AC9 = ( Line (a9,c9) );
assume A26: a <> a9;
assume A27: (not b,c // b9,c9);
A28: (b9 in BC9 & c9 in BC9) by AFF_1:15;
A29: BC c= X by A6,A8,A17,A18,A23,CKB26:1;
A30: c in BC by AFF_1:15;
A31: (BC is  being_line & b in BC) by A23,AFF_1:15,AFF_1:def 3;
A32: c9 <> b9 by A7,A9,A16,A22,AFF_1:45;
then A33: BC9 is  being_line by AFF_1:def 3;
BC9 c= X by A7,A9,A17,A18,A32,CKB26:1;
then consider p such that A34: p in BC and A35: p in BC9 by A18,A27,A33,A31,A30,A28,A29,CKB30:1,AFF_1:39;
A36: a9 in AC9 by AFF_1:15;
 LIN c9,b9,p by A33,A28,A35,AFF_1:21;
then consider y such that A37:  LIN c9,a9,y and A38: b9,a9 // p,y by A32,CKB2:1;
A39: c in AC by AFF_1:15;
 LIN c,b,p by A31,A30,A34,AFF_1:21;
then consider x such that A40:  LIN c,a,x and A41: b,a // p,x by A23,CKB2:1;
A42: a in AB by AFF_1:15;
A43: (AC is  being_line & a in AC) by A15,AFF_1:15,AFF_1:def 3;
then A44: x in AC by A15,A39,A40,AFF_1:25;
set K = ( p * AB );
A45: b in AB by AFF_1:15;
A46: AB is  being_line by A24,AFF_1:def 3;
then A47: AB // K by CKB36:def 1;
A48: p in K by A46,CKB36:def 1;
p,x // a,b by A41,AFF_1:4;
then p,x // AB by A24,AFF_1:def 4;
then p,x // K by A47,CKB4:1;
then A49: x in K by A48,CKB3:1;
A50: a9 <> b9 by A1,A5,A7,A11,AFF_1:45;
p,y // a9,b9 by A38,AFF_1:4;
then A51: p,y // AB9 by A50,AFF_1:def 4;
AB // AB9 by A13,A24,A50,AFF_1:37;
then AB9 // K by A47,AFF_1:44;
then p,y // K by A51,CKB4:1;
then A52: y in K by A48,CKB3:1;
A53: AC c= Y by A4,A8,A19,A20,A21,A15,CKB26:1;
A54: c9 in AC9 by AFF_1:15;
A55: a9 <> c9 by A2,A5,A9,A12,AFF_1:45;
then AC9 is  being_line by AFF_1:def 3;
then A56: y in AC9 by A55,A36,A54,A37,AFF_1:25;
A57: AC9 c= Y by A5,A9,A19,A20,A21,A55,CKB26:1;
A58:now
assume A59: x <> y;
then K = ( Line (x,y) ) by A46,A49,A52,CKB37:1,AFF_1:57;
then K c= Y by A21,A53,A57,A44,A56,A59,CKB26:1;
then A60: AB c= Y by A4,A19,A21,A42,A47,CKB31:1;
P = ( b * A ) by A1,A6,A10,CKB36:def 1;
then P c= Y by A10,A19,A21,A45,A60,CKB38:1;
hence contradiction by A3,A19,A20,A21,CKB57:def 1;
end;
A61: AC // AC9 by A14,A15,A55,AFF_1:37;
now
assume x = y;
then a9 in AC by A36,A61,A44,A56,AFF_1:45;
then c in A by A4,A5,A10,A26,A43,A39,AFF_1:18;
hence contradiction by A2,A8,A12,AFF_1:45;
end;
hence contradiction by A58;
end;
now
assume a = a9;
then (b = b9 & c = c9) by A1,A2,A4,A6,A7,A8,A9,A11,A12,A13,A14,CKB12:1;
hence thesis by AFF_1:2;
end;
hence thesis by A25;
end;
