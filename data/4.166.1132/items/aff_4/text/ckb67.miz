environ
vocabularies DIRAF,SUBSET_1,ANALOAF,AFF_1,INCSP_1,STRUCT_0,XBOOLE_0,TARSKI,RELAT_1,PARSP_1,AFF_2,ARYTM_3,AFF_4;
notations TARSKI,XBOOLE_0,SUBSET_1,STRUCT_0,ANALOAF,DIRAF,AFF_1,AFF_2,CKB15,CKB16,CKB36,CKB44,CKB57;
definitions CKB15,CKB16,CKB36,CKB44,CKB57;
theorems AFF_1,DIRAF,TARSKI,AFF_2,XBOOLE_0,XBOOLE_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB37,CKB38,CKB39,CKB40,CKB41,CKB42,CKB43,CKB44,CKB45,CKB46,CKB47,CKB48,CKB49,CKB50,CKB51,CKB52,CKB53,CKB54,CKB55,CKB56,CKB57,CKB58,CKB59,CKB60,CKB61,CKB62,CKB63,CKB64,CKB65,CKB66;
registrations XBOOLE_0,STRUCT_0;
constructors AFF_1,AFF_2,CKB15,CKB16,CKB36,CKB44,CKB57;
requirements SUBSET,BOOLE;
begin
reserve AS for AffinSpace;
reserve a for (Element of AS);
reserve b for (Element of AS);
reserve c for (Element of AS);
reserve d for (Element of AS);
reserve a9 for (Element of AS);
reserve b9 for (Element of AS);
reserve c9 for (Element of AS);
reserve d9 for (Element of AS);
reserve A for (Subset of AS);
reserve C for (Subset of AS);
reserve P for (Subset of AS);
reserve X for (Subset of AS);
theorem
Th51: (((((((((((((((((not AS is AffinPlane) & A // P) & A // C) & a in A) & a9 in A) & b in P) & b9 in P) & c in C) & c9 in C) & A is  being_line) & P is  being_line) & C is  being_line) & A <> P) & A <> C) & a,b // a9,b9) & a,c // a9,c9) implies b,c // b9,c9)
proof
assume that
A1: (not AS is AffinPlane)
and
A2: A // P
and
A3: A // C
and
A4: (a in A & a9 in A)
and
A5: (b in P & b9 in P)
and
A6: (c in C & c9 in C)
and
A7: A is  being_line
and
A8: P is  being_line
and
A9: C is  being_line
and
A10: A <> P
and
A11: A <> C
and
A12: a,b // a9,b9
and
A13: a,c // a9,c9;
now
assume A,P,C is_coplanar ;
then consider X such that A14: A c= X and A15: P c= X and A16: C c= X and A17: X is  being_plane by CKB57:def 1;
consider d such that A18: (not d in X) by A1,A17,CKB62:1;
set K = ( d * A );
A19: d in K by A7,CKB36:def 1;
then A20: (not K c= X) by A18;
A21: A // K by A7,CKB36:def 1;
(ex d9 st (d9 in K & a,d // a9,d9))
proof
A22:now
assume A23: a <> a9;
consider d9 such that A24: a,a9 // d,d9 and A25: a,d // a9,d9 by DIRAF:40;
d,d9 // a,a9 by A24,AFF_1:4;
then d,d9 // A by A4,A7,A23,AFF_1:27;
then d,d9 // K by A21,CKB4:1;
then d9 in K by A19,CKB3:1;
hence thesis by A25;
end;
now
assume A26: a = a9;
take d9 = d;
thus d9 in K by A7,CKB36:def 1;
thus a,d // a9,d9 by A26,AFF_1:2;
end;
hence thesis by A22;
end;
then consider d9 such that A27: d9 in K and A28: a,d // a9,d9;
A29: (K // P & K // C) by A2,A3,A21,AFF_1:44;
now
assume A30: P <> C;
A31: (not K,P,C is_coplanar )
proof
assume K,P,C is_coplanar ;
then P,C,K is_coplanar  by CKB58:1;
hence contradiction by A8,A9,A15,A16,A17,A20,A30,CKB60:1;
end;
A32: K <> A by A14,A18,A19;
(not A,K,P is_coplanar )
proof
assume A,K,P is_coplanar ;
then A,P,K is_coplanar  by CKB58:1;
hence contradiction by A7,A8,A10,A14,A15,A17,A20,CKB60:1;
end;
then A33: d,b // d9,b9 by A2,A4,A5,A7,A10,A12,A19,A21,A27,A28,A32,CKB66:1;
A34: (K <> P & K <> C) by A15,A16,A18,A19;
(not A,K,C is_coplanar )
proof
assume A,K,C is_coplanar ;
then A,C,K is_coplanar  by CKB58:1;
hence contradiction by A7,A9,A11,A14,A16,A17,A20,CKB60:1;
end;
then d,c // d9,c9 by A3,A4,A6,A7,A11,A13,A19,A21,A27,A28,A32,CKB66:1;
hence thesis by A5,A6,A7,A19,A29,A27,A34,A31,A33,CKB66:1,CKB37:1;
end;
hence thesis by A5,A6,A8,AFF_1:51;
end;
hence thesis by A2,A3,A4,A5,A6,A7,A10,A11,A12,A13,CKB66:1;
end;
