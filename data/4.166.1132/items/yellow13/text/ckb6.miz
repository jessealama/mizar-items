theorem
Th2: (for T being  discrete non  empty TopSpace holds T is  normal)
proof
let T being  discrete non  empty TopSpace;
let W being (Subset of T);
let V being (Subset of T);
assume that
W <> ( {} )
and
V <> ( {} )
and
W is  closed
and
V is  closed
and
A1: ( W /\ V ) = ( {} );
take P = W;
take Q = V;
thus (P is  open & Q is  open) by TDLAT_3:15;
thus ((W c= P & V c= Q) & ( P /\ Q ) = ( {} )) by A1;
end;
