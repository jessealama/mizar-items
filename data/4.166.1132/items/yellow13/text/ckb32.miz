requirements SUBSET,BOOLE,NUMERALS;
begin
definition
let T being non  empty TopSpace;
let p being (Point of T);
redefine mode basis of p
means (for A being (a_neighborhood of p) holds (ex P being (a_neighborhood of p) st (P in it & P c= A)));
compatibility
proof
let F being (Subset-Family of T);
hereby
assume A1: F is (basis of p);
let A being (a_neighborhood of p);
p in ( Int A ) by CONNSP_2:def 1;
then consider P being (Subset of T) such that A2: P in F and A3: p in ( Int P ) and A4: P c= A by A1,CKB31:def 1;
reconsider P as (a_neighborhood of p) by A3,CONNSP_2:def 1;
take P;
thus (P in F & P c= A) by A2,A4;
end;
assume A5: (for A being (a_neighborhood of p) holds (ex P being (a_neighborhood of p) st (P in F & P c= A)));
let A being (Subset of T);
assume p in ( Int A );
then A is (a_neighborhood of p) by CONNSP_2:def 1;
then consider P being (a_neighborhood of p) such that A6: (P in F & P c= A) by A5;
take P;
thus thesis by A6,CONNSP_2:def 1;
end;
end;
