begin
theorem
Lm4: (for n being Ordinal holds (for T being  connected (TermOrder of n) holds (for L being  add-associative  right_complementable  right_zeroed  commutative  associative  well-unital  distributive  almost_left_invertible non  trivial doubleLoopStr holds (for P being (Subset of ( Polynom-Ring (n,L) )) holds (for R being (RedSequence of ( PolyRedRel (P,T) )) holds (for i being (Element of ( NAT )) holds (((1 <= i & i <= ( len R )) & ( len R ) > 1) implies ( R . i ) is (Polynomial of n,L))))))))
proof
let n being Ordinal;
let T being  connected (TermOrder of n);
let L being  add-associative  right_complementable  right_zeroed  commutative  associative  well-unital  distributive  almost_left_invertible non  trivial doubleLoopStr;
let P being (Subset of ( Polynom-Ring (n,L) ));
let R being (RedSequence of ( PolyRedRel (P,T) ));
let i being (Element of ( NAT ));
assume that
A1: 1 <= i
and
A2: i <= ( len R )
and
A3: 1 < ( len R );
A4: i in ( dom R ) by A1,A2,FINSEQ_3:25;
now
per cases ;
case i <> ( len R );
then i < ( len R ) by A2,XXREAL_0:1;
then (1 <= ( i + 1 ) & ( i + 1 ) <= ( len R )) by NAT_1:11,NAT_1:13;
then ( i + 1 ) in ( dom R ) by FINSEQ_3:25;
then [ ( R . i ),( R . ( i + 1 ) ) ] in ( PolyRedRel (P,T) ) by A4,REWRITE1:def 2;
then ( R . i ) in ( dom ( PolyRedRel (P,T) ) ) by RELAT_1:def 4;
then ( R . i ) in (the carrier of ( Polynom-Ring (n,L) )) by XBOOLE_0:def 5;
hence thesis by POLYNOM1:def 10;
end;
case A5: i = ( len R );
A6: ( i - 1 ) is (Element of ( NAT )) by A1,INT_1:5;
( 1 + ( - 1 ) ) < ( i + ( - 1 ) ) by A3,A5,XREAL_1:8;
then A7: 1 <= ( i - 1 ) by A6,NAT_1:14;
A8: i = ( ( i - 1 ) + 1 );
( i - 1 ) <= ( len R ) by A5,XREAL_1:146;
then ( i - 1 ) in ( dom R ) by A6,A7,FINSEQ_3:25;
then [ ( R . ( i - 1 ) ),( R . i ) ] in ( PolyRedRel (P,T) ) by A4,A8,REWRITE1:def 2;
then ( R . i ) in ( rng ( PolyRedRel (P,T) ) ) by RELAT_1:def 5;
hence thesis by POLYNOM1:def 10;
end;
end;
hence thesis;
end;
