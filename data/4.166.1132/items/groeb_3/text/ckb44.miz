constructors XXREAL_0,REWRITE1,VECTSP_2,WAYBEL_4,POLYNOM2,BAGORDER,TERMORD,POLYRED,GROEB_1,GROEB_2,RELSET_1,PBOOLE,FUNCT_7,VFUNCT_1,CKB18,CKB19,CKB25,CKB26,CKB33,CKB34;
requirements NUMERALS,REAL,SUBSET,BOOLE,ARITHM;
begin
theorem
Th33: (for n being Ordinal holds (for T being  connected (TermOrder of n) holds (for L being  add-associative  right_zeroed  right_complementable non  empty addLoopStr holds (for p being (Polynomial of n,L) holds (for i being (Element of ( NAT )) holds (i <= ( card ( Support p ) ) implies ( ( Up (p,T,i) ) + ( Low (p,T,i) ) ) = p))))))
proof
let n being Ordinal;
let T being  connected (TermOrder of n);
let L being  add-associative  right_zeroed  right_complementable non  empty addLoopStr;
let p being (Polynomial of n,L);
let i being (Element of ( NAT ));
set u = ( ( Up (p,T,i) ) + ( Low (p,T,i) ) );
assume A1: i <= ( card ( Support p ) );
A2:now
let x being set;
assume A3: x in ( Support p );
then reconsider x9 = x as (Element of ( Bags n ));
A4: ( u . x9 ) = ( ( ( Up (p,T,i) ) . x9 ) + ( ( Low (p,T,i) ) . x9 ) ) by POLYNOM1:15;
A5:now
per cases  by A1,A3,CKB39:1;
case A6: x9 in ( Support ( Up (p,T,i) ) );
hence ( u . x9 ) = ( ( ( Up (p,T,i) ) . x9 ) + ( 0. L ) ) by A1,A4,CKB43:1
.= ( ( Up (p,T,i) ) . x9 ) by RLVECT_1:def 4
.= ( p . x9 ) by A1,A6,CKB43:1;
end;
case A7: x9 in ( Support ( Low (p,T,i) ) );
hence ( u . x9 ) = ( ( 0. L ) + ( ( Low (p,T,i) ) . x9 ) ) by A1,A4,CKB42:1
.= ( ( Low (p,T,i) ) . x9 ) by ALGSTR_1:def 2
.= ( p . x9 ) by A1,A7,CKB42:1;
end;
end;
( p . x9 ) <> ( 0. L ) by A3,POLYNOM1:def 3;
hence x in ( Support u ) by A5,POLYNOM1:def 3;
end;
now
let x being set;
(( Support ( Up (p,T,i) ) ) c= ( Support p ) & ( Support ( Low (p,T,i) ) ) c= ( Support p )) by A1,CKB37:1;
then (( Support u ) c= ( ( Support ( Up (p,T,i) ) ) \/ ( Support ( Low (p,T,i) ) ) ) & ( ( Support ( Up (p,T,i) ) ) \/ ( Support ( Low (p,T,i) ) ) ) c= ( Support p )) by POLYNOM1:20,XBOOLE_1:8;
then A8: ( Support u ) c= ( Support p ) by XBOOLE_1:1;
assume x in ( Support u );
hence x in ( Support p ) by A8;
end;
then A9: ( Support u ) = ( Support p ) by A2,TARSKI:1;
A10:now
let x being set;
assume x in ( dom p );
then reconsider x9 = x as (Element of ( Bags n ));
A11: ( u . x9 ) = ( ( ( Up (p,T,i) ) . x9 ) + ( ( Low (p,T,i) ) . x9 ) ) by POLYNOM1:15;
now
per cases ;
case A12: x9 in ( Support p );
now
per cases  by A1,A12,CKB39:1;
case A13: x9 in ( Support ( Up (p,T,i) ) );
hence ( u . x9 ) = ( ( ( Up (p,T,i) ) . x9 ) + ( 0. L ) ) by A1,A11,CKB43:1
.= ( ( Up (p,T,i) ) . x9 ) by RLVECT_1:def 4
.= ( p . x9 ) by A1,A13,CKB43:1;
end;
case A14: x9 in ( Support ( Low (p,T,i) ) );
hence ( u . x9 ) = ( ( 0. L ) + ( ( Low (p,T,i) ) . x9 ) ) by A1,A11,CKB42:1
.= ( ( Low (p,T,i) ) . x9 ) by ALGSTR_1:def 2
.= ( p . x9 ) by A1,A14,CKB42:1;
end;
end;
hence ( p . x9 ) = ( u . x9 );
end;
case A15: (not x9 in ( Support p ));
hence ( p . x9 ) = ( 0. L ) by POLYNOM1:def 3
.= ( u . x9 ) by A9,A15,POLYNOM1:def 3;
end;
end;
hence ( p . x ) = ( u . x );
end;
( dom p ) = ( Bags n ) by FUNCT_2:def 1
.= ( dom u ) by FUNCT_2:def 1;
hence thesis by A10,FUNCT_1:2;
end;
