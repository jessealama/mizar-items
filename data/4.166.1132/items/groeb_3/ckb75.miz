begin
theorem
(for n being (Element of ( NAT )) holds (for T being  connected  admissible (TermOrder of n) holds (for L being  add-associative  right_complementable  right_zeroed  commutative  associative  well-unital  distributive  Abelian  almost_left_invertible non degenerated non empty doubleLoopStr holds (for G being (Subset of ( Polynom-Ring (n,L) )) holds ((not ( 0_ (n,L) ) in G) implies ((for g1,g2,h being (Polynomial of n,L) holds ((((g1 in G & g2 in G) & (not ( HT (g1,T) ),( HT (g2,T) ) are_disjoint )) & h is_a_normal_form_of ( S-Poly (g1,g2,T) ),( PolyRedRel (G,T) )) implies h = ( 0_ (n,L) ))) implies G is_Groebner_basis_wrt T))))))
proof
let n being (Element of ( NAT ));
let T being  connected  admissible (TermOrder of n);
let L being  add-associative  right_complementable  right_zeroed  commutative  associative  well-unital  distributive  Abelian  almost_left_invertible non degenerated non empty doubleLoopStr;
let G being (Subset of ( Polynom-Ring (n,L) ));
assume A1: (not ( 0_ (n,L) ) in G);
assume A2: (for g1,g2,h being (Polynomial of n,L) holds ((((g1 in G & g2 in G) & (not ( HT (g1,T) ),( HT (g2,T) ) are_disjoint )) & h is_a_normal_form_of ( S-Poly (g1,g2,T) ),( PolyRedRel (G,T) )) implies h = ( 0_ (n,L) )));
(for g1,g2 being (Polynomial of n,L) holds ((g1 in G & g2 in G) implies ( PolyRedRel (G,T) ) reduces ( S-Poly (g1,g2,T) ),( 0_ (n,L) )))
proof
let g1 being (Polynomial of n,L),g2 being (Polynomial of n,L);
assume that
A3: g1 in G
and
A4: g2 in G;
now
per cases ;
case A5: ( HT (g1,T) ),( HT (g2,T) ) are_disjoint ;
now
let u being set;
assume A6: u in { g1,g2 };
now
per cases  by A6,TARSKI:def 2;
case u = g1;
hence u in G by A3;
end;
case u = g2;
hence u in G by A4;
end;
end;
hence u in G;
end;
then A7: { g1,g2 } c= G by TARSKI:def 3;
( PolyRedRel ({ g1,g2 },T) ) reduces ( S-Poly (g1,g2,T) ),( 0_ (n,L) ) by A5,CKB72:1;
hence thesis by A7,GROEB_1:4,REWRITE1:22;
end;
case A8: (not ( HT (g1,T) ),( HT (g2,T) ) are_disjoint );
( S-Poly (g1,g2,T) ) has_a_normal_form_wrt ( PolyRedRel (G,T) )
proof
now
per cases ;
case (not ( S-Poly (g1,g2,T) ) in ( field ( PolyRedRel (G,T) ) ));
hence thesis by REWRITE1:46;
end;
case ( S-Poly (g1,g2,T) ) in ( field ( PolyRedRel (G,T) ) );
hence thesis by REWRITE1:def 14;
end;
end;
hence thesis;
end;
then consider h being set such that A9: h is_a_normal_form_of ( S-Poly (g1,g2,T) ),( PolyRedRel (G,T) ) by REWRITE1:def 11;
( PolyRedRel (G,T) ) reduces ( S-Poly (g1,g2,T) ),h by A9,REWRITE1:def 6;
then reconsider h as (Polynomial of n,L) by CKB11:1;
h = ( 0_ (n,L) ) by A2,A3,A4,A8,A9;
hence thesis by A9,REWRITE1:def 6;
end;
end;
hence thesis;
end;
hence thesis by A1,GROEB_2:25;
end;
