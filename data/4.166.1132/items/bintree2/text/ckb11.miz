theorem
Th8: (for T being Tree holds (T = ( { ( 0 ),1 } * ) iff (for t being (Element of T) holds ( succ t ) = { ( t ^ <* ( 0 ) *> ),( t ^ <* 1 *> ) })))
proof
let T being Tree;
thus (T = ( { ( 0 ),1 } * ) implies (for t being (Element of T) holds ( succ t ) = { ( t ^ <* ( 0 ) *> ),( t ^ <* 1 *> ) }))
proof
assume A1: T = ( { ( 0 ),1 } * );
let t being (Element of T);
(T is  binary & (not t in ( Leaves T ))) by A1,CKB6:1,CKB7:1;
hence thesis by BINTREE1:def 2;
end;

assume A2: (for t being (Element of T) holds ( succ t ) = { ( t ^ <* ( 0 ) *> ),( t ^ <* 1 *> ) });
thus T = ( { ( 0 ),1 } * )
proof
thus T c= ( { ( 0 ),1 } * )
proof
let x being set;
assume A3: x in T;
T is  binary by A2,CKB10:1;
then x is (FinSequence of { ( 0 ),1 }) by A3,CKB4:1;
hence thesis by FINSEQ_1:def 11;
end;

defpred P[ FinSequence ]
 means
$1 in T;
let x being set;
assume x in ( { ( 0 ),1 } * );
then A4: x is (FinSequence of { ( 0 ),1 }) by FINSEQ_1:def 11;
A5: (for p being (FinSequence of { ( 0 ),1 }) holds (for n being (Element of { ( 0 ),1 }) holds (P[ p ] implies P[ ( p ^ <* n *> ) ])))
proof
let p being (FinSequence of { ( 0 ),1 });
let n being (Element of { ( 0 ),1 });
A6: (n = ( 0 ) or n = 1) by TARSKI:def 2;
assume p in T;
then reconsider p1 = p as (Element of T);
( succ p1 ) = { ( p1 ^ <* ( 0 ) *> ),( p1 ^ <* 1 *> ) } by A2;
then ( p ^ <* n *> ) in ( succ p1 ) by A6,TARSKI:def 2;
hence thesis;
end;
A7: P[ ( <*> { ( 0 ),1 } ) ] by TREES_1:22;
(for p being (FinSequence of { ( 0 ),1 }) holds P[ p ]) from FINSEQ_2:sch 2(A7,A5);
hence thesis by A4;
end;

