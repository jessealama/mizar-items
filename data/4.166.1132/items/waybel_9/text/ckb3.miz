begin
theorem
Th1: (for S,T being RelStr holds (for K,L being non empty RelStr holds (for f being (Function of S,T) holds (for g being (Function of K,L) holds ((((( the RelStr of S ) = ( the RelStr of K ) & ( the RelStr of T ) = ( the RelStr of L )) & f = g) & f is  monotone) implies g is  monotone)))))
proof
let S being RelStr,T being RelStr;
let K being non empty RelStr,L being non empty RelStr;
let f being (Function of S,T);
let g being (Function of K,L);
assume that
A1: ( the RelStr of S ) = ( the RelStr of K )
and
A2: ( the RelStr of T ) = ( the RelStr of L )
and
A3: f = g
and
A4: f is  monotone;
reconsider S1 = S,T1 = T as non empty RelStr by A1,A2;
let x being (Element of K),y being (Element of K);
assume that
A5: x <= y;
reconsider x1 = x,y1 = y as (Element of S1) by A1;
reconsider f1 = f as (Function of S1,T1);
x1 <= y1 by A1,A5,YELLOW_0:1;
then ( f1 . x1 ) <= ( f1 . y1 ) by A4,WAYBEL_1:def 2;
hence ( g . x ) <= ( g . y ) by A2,A3,YELLOW_0:1;
end;
