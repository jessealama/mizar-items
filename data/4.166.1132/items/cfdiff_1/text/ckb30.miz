theorem
Th6: (for g being  real number holds (( 0 ) < g implies { y where y is Complex: |. ( y - z0 ) .| < g } is (Neighbourhood of z0)))
proof
let g being  real number;
assume that
A1: g > ( 0 );
set N = { y where y is Complex: |. ( y - z0 ) .| < g };
A2: N c= ( COMPLEX )
proof
let z being set;
assume z in { y where y is Complex: |. ( y - z0 ) .| < g };
then (ex y being Complex st (z = y & |. ( y - z0 ) .| < g));
hence thesis;
end;
g in ( REAL ) by XREAL_0:def 1;
hence thesis by A1,A2,CKB29:def 1;
end;
