requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve X for set;
reserve y for set;
reserve x for Complex;
reserve f for (PartFunc of ( COMPLEX ),( COMPLEX ));
definition
let f;
let X;
assume A1: f is_differentiable_on X;
func f `| X -> (PartFunc of ( COMPLEX ),( COMPLEX )) means 
:Def12: (( dom it ) = X & (for x holds (x in X implies ( it /. x ) = ( diff (f,x) ))));
existence
proof
deffunc F((Element of ( COMPLEX ))) = ( diff (f,$1) );
defpred P[ set ]
 means
$1 in X;
consider F being (PartFunc of ( COMPLEX ),( COMPLEX )) such that A2: ((for x being (Element of ( COMPLEX )) holds (x in ( dom F ) iff P[ x ])) & (for x being (Element of ( COMPLEX )) holds (x in ( dom F ) implies ( F . x ) = F(x)))) from SEQ_1:sch 3;
take F;
now
A3: X is (Subset of ( COMPLEX )) by A1,CKB37:1;
let y;
assume y in X;
hence y in ( dom F ) by A2,A3;
end;
then A4: X c= ( dom F ) by TARSKI:def 3;
(for y holds (y in ( dom F ) implies y in X)) by A2;
then ( dom F ) c= X by TARSKI:def 3;
hence ( dom F ) = X by A4,XBOOLE_0:def 10;
now
let x;
assume x in X;
then A5: x in ( dom F ) by A2;
then ( F . x ) = ( diff (f,x) ) by A2;
hence ( F /. x ) = ( diff (f,x) ) by A5,PARTFUN1:def 6;
end;
hence thesis;
end;
uniqueness
proof
let F being (PartFunc of ( COMPLEX ),( COMPLEX ));
let G being (PartFunc of ( COMPLEX ),( COMPLEX ));
assume that
A6: ( dom F ) = X
and
A7: (for x holds (x in X implies ( F /. x ) = ( diff (f,x) )))
and
A8: ( dom G ) = X
and
A9: (for x holds (x in X implies ( G /. x ) = ( diff (f,x) )));
now
let x;
assume A10: x in ( dom F );
then ( F /. x ) = ( diff (f,x) ) by A6,A7;
hence ( F /. x ) = ( G /. x ) by A6,A9,A10;
end;
hence thesis by A6,A8,PARTFUN2:1;
end;
end;
