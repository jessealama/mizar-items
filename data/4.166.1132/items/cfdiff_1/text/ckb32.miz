constructors PARTFUN1,REAL_1,NAT_1,VALUED_0,VALUED_1,SEQ_1,SEQ_2,FINSEQ_4,SEQM_3,PARTFUN2,SQUARE_1,COMSEQ_2,COMSEQ_3,CFCONT_1,XXREAL_0,CFUNCT_1,RELSET_1,CKB1,CKB3,CKB13,CKB16,CKB29;
requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve y for set;
reserve a1 for Complex;
reserve a2 for Complex;
reserve z for Complex;
reserve z0 for Complex;
theorem
Lm1: (for z0 being Complex holds (for N1,N2 being (Neighbourhood of z0) holds (ex N being (Neighbourhood of z0) st (N c= N1 & N c= N2))))
proof
let z0 being Complex;
let N1 being (Neighbourhood of z0);
let N2 being (Neighbourhood of z0);
consider a1 being Real such that A1: ( 0 ) < a1 and A2: { y where y is Complex: |. ( y - z0 ) .| < a1 } c= N1 by CKB29:def 1;
consider a2 being Real such that A3: ( 0 ) < a2 and A4: { y where y is Complex: |. ( y - z0 ) .| < a2 } c= N2 by CKB29:def 1;
set g = ( min (a1,a2) );
take { y where y is Complex: |. ( y - z0 ) .| < g };
A5: g <= a2 by XXREAL_0:17;
A6: { y where y is Complex: |. ( y - z0 ) .| < g } c= { y where y is Complex: |. ( y - z0 ) .| < a2 }
proof
let z being set;
assume z in { y where y is Complex: |. ( y - z0 ) .| < g };
then consider y being Complex such that A7: z = y and A8: |. ( y - z0 ) .| < g;
|. ( y - z0 ) .| < a2 by A5,A8,XXREAL_0:2;
hence thesis by A7;
end;
A9: g <= a1 by XXREAL_0:17;
A10: { y where y is Complex: |. ( y - z0 ) .| < g } c= { y where y is Complex: |. ( y - z0 ) .| < a1 }
proof
let z being set;
assume z in { y where y is Complex: |. ( y - z0 ) .| < g };
then consider y being Complex such that A11: z = y and A12: |. ( y - z0 ) .| < g;
|. ( y - z0 ) .| < a1 by A9,A12,XXREAL_0:2;
hence thesis by A11;
end;
( 0 ) < g by A1,A3,XXREAL_0:15;
hence thesis by A2,A4,A10,A6,CKB30:1,XBOOLE_1:1;
end;
