constructors PARTFUN1,REAL_1,NAT_1,VALUED_0,VALUED_1,SEQ_1,SEQ_2,FINSEQ_4,SEQM_3,PARTFUN2,SQUARE_1,COMSEQ_2,COMSEQ_3,CFCONT_1,XXREAL_0,CFUNCT_1,RELSET_1,CKB1,CKB3,CKB13,CKB16,CKB29,CKB33,CKB34,CKB35,CKB36,CKB38,CKB39,CKB49;
requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve n for (Element of ( NAT ));
reserve a1 for Complex;
reserve x for Complex;
reserve x0 for Complex;
reserve z for Complex;
reserve f for (PartFunc of ( COMPLEX ),( COMPLEX ));
reserve h for  convergent_to_0 Complex_Sequence;
reconsider cf = ( ( COMPLEX ) --> ( 0c ) ) as (Function of ( COMPLEX ),( COMPLEX ));
reserve R for C_REST;
reserve L for C_LINEAR;
reserve Z for  open (Subset of ( COMPLEX ));
theorem
(for f holds (for Z holds ((Z c= ( dom f ) & (ex a1 st ( rng f ) = { a1 })) implies (f is_differentiable_on Z & (for x holds (x in Z implies ( ( f `| Z ) /. x ) = ( 0c )))))))
proof
set R = cf;
A1: ( dom R ) = ( COMPLEX ) by FUNCOP_1:13;
now
let h;
now
let n being Nat;
A2: (( rng h ) c= ( dom R ) & n in ( NAT )) by A1,ORDINAL1:def 12;
thus ( ( ( h " ) (#) ( R /* h ) ) . n ) = ( ( ( h " ) . n ) * ( ( R /* h ) . n ) ) by VALUED_1:5
.= ( ( ( h " ) . n ) * ( R /. ( h . n ) ) ) by A2,FUNCT_2:109
.= ( ( ( h " ) . n ) * ( 0c ) ) by FUNCOP_1:7
.= ( 0c );
end;
then (( ( h " ) (#) ( R /* h ) ) is  constant & ( ( ( h " ) (#) ( R /* h ) ) . ( 0 ) ) = ( 0c )) by VALUED_0:def 18;
hence (( ( h " ) (#) ( R /* h ) ) is  convergent & ( lim ( ( h " ) (#) ( R /* h ) ) ) = ( 0c )) by CFCONT_1:26,CFCONT_1:27;
end;
then reconsider R as C_REST by CKB13:def 1;
set L = cf;
( L /. z ) = ( ( 0c ) * z ) by FUNCOP_1:7;
then reconsider L as C_LINEAR by CKB16:def 1;
let f;
let Z;
assume that
A3: Z c= ( dom f );
given a1 such that
A4: ( rng f ) = { a1 };

A5:now
let x0;
assume A6: x0 in ( dom f );
then ( f . x0 ) in { a1 } by A4,FUNCT_1:def 3;
then ( f /. x0 ) in { a1 } by A6,PARTFUN1:def 6;
hence ( f /. x0 ) = a1 by TARSKI:def 1;
end;
A7:now
let x0;
assume A8: x0 in Z;
then consider N being (Neighbourhood of x0) such that A9: N c= Z by CKB40:1;
A10: N c= ( dom f ) by A3,A9,XBOOLE_1:1;
(for x holds (x in N implies ( ( f /. x ) - ( f /. x0 ) ) = ( ( L /. ( x - x0 ) ) + ( R /. ( x - x0 ) ) )))
proof
let x;
assume x in N;
hence ( ( f /. x ) - ( f /. x0 ) ) = ( a1 - ( f /. x0 ) ) by A5,A10
.= ( a1 - a1 ) by A3,A5,A8
.= ( ( L /. ( x - x0 ) ) + ( 0c ) ) by FUNCOP_1:7
.= ( ( L /. ( x - x0 ) ) + ( R /. ( x - x0 ) ) ) by FUNCOP_1:7;
end;
hence f is_differentiable_in x0 by A10,CKB33:def 1;
end;
hence A11: f is_differentiable_on Z by A3,CKB47:1;
let x0;
assume A12: x0 in Z;
then A13: f is_differentiable_in x0 by A7;
then (ex N being (Neighbourhood of x0) st (N c= ( dom f ) & (ex L st (ex R st (for x holds (x in N implies ( ( f /. x ) - ( f /. x0 ) ) = ( ( L /. ( x - x0 ) ) + ( R /. ( x - x0 ) ) ))))))) by CKB33:def 1;
then consider N being (Neighbourhood of x0) such that A14: N c= ( dom f );
A15: (for x holds (x in N implies ( ( f /. x ) - ( f /. x0 ) ) = ( ( L /. ( x - x0 ) ) + ( R /. ( x - x0 ) ) )))
proof
let x;
assume x in N;
hence ( ( f /. x ) - ( f /. x0 ) ) = ( a1 - ( f /. x0 ) ) by A5,A14
.= ( a1 - a1 ) by A3,A5,A12
.= ( ( L /. ( x - x0 ) ) + ( 0c ) ) by FUNCOP_1:7
.= ( ( L /. ( x - x0 ) ) + ( R /. ( x - x0 ) ) ) by FUNCOP_1:7;
end;
thus ( ( f `| Z ) /. x0 ) = ( diff (f,x0) ) by A11,A12,CKB49:def 1
.= ( L /. ( 1r ) ) by A13,A14,A15,CKB34:def 1
.= ( 0c ) by FUNCOP_1:7;
end;
