vocabularies NUMBERS,SUBSET_1,REAL_1,XREAL_0,ORDINAL1,CFUNCT_1,COMSEQ_1,PARTFUN1,ORDINAL2,NAT_1,FDIFF_1,VALUED_0,SEQ_2,CARD_1,SEQ_1,FUNCT_1,COMPLEX1,RELAT_1,ARYTM_3,XXREAL_0,ARYTM_1,FUNCOP_1,VALUED_1,FUNCT_2,TARSKI,RCOMP_1,XBOOLE_0,FCONT_1,CFCONT_1,XXREAL_2,CFDIFF_1;
notations TARSKI,SUBSET_1,ORDINAL1,FUNCT_1,NUMBERS,XCMPLX_0,XREAL_0,COMPLEX1,REAL_1,NAT_1,PARTFUN1,FUNCT_2,FUNCOP_1,VALUED_0,VALUED_1,SEQ_1,RELSET_1,SEQ_2,CFUNCT_1,COMSEQ_1,COMSEQ_2,COMSEQ_3,CFCONT_1,XXREAL_0,CKB1,CKB3,CKB13,CKB15,CKB16,CKB18,CKB29,CKB33,CKB34,CKB35,CKB36,CKB38,CKB39,CKB49;
definitions XCMPLX_0,XBOOLE_0,TARSKI,RELAT_1,COMSEQ_2,SEQM_3,CFCONT_1,SUBSET_1,FINSEQ_4,VALUED_0,VALUED_1,SEQ_2,COMPLEX1,FUNCT_2,CKB1,CKB3,CKB13,CKB16,CKB29,CKB33,CKB34,CKB35,CKB36,CKB38,CKB39,CKB49;
theorems TARSKI,NAT_1,FUNCT_1,FUNCT_2,ZFMISC_1,COMPLEX1,SEQ_1,SEQ_2,SEQ_4,NUMBERS,COMSEQ_1,COMSEQ_2,COMSEQ_3,CFUNCT_1,CFCONT_1,RELAT_1,XBOOLE_0,XBOOLE_1,PARTFUN1,PARTFUN2,XCMPLX_0,XCMPLX_1,XREAL_1,XXREAL_0,ORDINAL1,ABSVALUE,XREAL_0,FUNCOP_1,SUBSET_1,VALUED_1,VALUED_0,CKB1,CKB2,CKB3,CKB4,CKB6,CKB11,CKB12,CKB13,CKB16,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB37,CKB38,CKB39,CKB40,CKB41,CKB42,CKB43,CKB44,CKB45,CKB47,CKB48,CKB49,CKB50,CKB53,CKB54,CKB55,CKB56,CKB57,CKB58,CKB59,CKB60,CKB61;
schemes SEQ_1,COMSEQ_1,FUNCT_2;
registrations FUNCT_1,RELSET_1,FUNCT_2,MEMBERED,NUMBERS,ORDINAL1,XREAL_0,XXREAL_0,COMSEQ_2,NAT_1,VALUED_0,SEQ_4,VALUED_1,FUNCOP_1,CKB5,CKB7,CKB8,CKB9,CKB10,CKB14,CKB17,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB46,CKB51,CKB52;
constructors PARTFUN1,REAL_1,NAT_1,VALUED_0,VALUED_1,SEQ_1,SEQ_2,FINSEQ_4,SEQM_3,PARTFUN2,SQUARE_1,COMSEQ_2,COMSEQ_3,CFCONT_1,XXREAL_0,CFUNCT_1,RELSET_1,CKB1,CKB3,CKB13,CKB16,CKB29,CKB33,CKB34,CKB35,CKB36,CKB38,CKB39,CKB49;
requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
reserve n for (Element of ( NAT ));
reserve b for Complex;
reserve x for Complex;
reserve x0 for Complex;
reserve f for (PartFunc of ( COMPLEX ),( COMPLEX ));
reserve h for  convergent_to_0 Complex_Sequence;
reconsider cf = ( ( COMPLEX ) --> ( 0c ) ) as (Function of ( COMPLEX ),( COMPLEX ));
reserve R for C_REST;
reserve L for C_LINEAR;
reserve Z for  open (Subset of ( COMPLEX ));
theorem
(for f holds (for Z holds ((Z c= ( dom f ) & ( f | Z ) = ( id Z )) implies (f is_differentiable_on Z & (for x holds (x in Z implies ( ( f `| Z ) /. x ) = ( 1r )))))))
proof
set R = cf;
reconsider L = ( id ( COMPLEX ) ) as (PartFunc of ( COMPLEX ),( COMPLEX ));
A1: ( COMPLEX ) c= ( COMPLEX );
then (for b holds ( L /. b ) = ( ( 1r ) * b )) by PARTFUN2:6;
then reconsider L as C_LINEAR by CKB16:def 1;
A2: ( dom R ) = ( COMPLEX ) by FUNCOP_1:13;
now
let h;
now
let n being Nat;
A3: (n in ( NAT ) & ( rng h ) c= ( dom R )) by A2,ORDINAL1:def 12;
thus ( ( ( h " ) (#) ( R /* h ) ) . n ) = ( ( ( h " ) . n ) * ( ( R /* h ) . n ) ) by VALUED_1:5
.= ( ( ( h " ) . n ) * ( R /. ( h . n ) ) ) by A3,FUNCT_2:109
.= ( ( ( h " ) . n ) * ( 0c ) ) by FUNCOP_1:7
.= ( 0c );
end;
then (( ( h " ) (#) ( R /* h ) ) is  constant & ( ( ( h " ) (#) ( R /* h ) ) . ( 0 ) ) = ( 0c )) by VALUED_0:def 18;
hence (( ( h " ) (#) ( R /* h ) ) is  convergent & ( lim ( ( h " ) (#) ( R /* h ) ) ) = ( 0c )) by CFCONT_1:26,CFCONT_1:27;
end;
then reconsider R as C_REST by CKB13:def 1;
let f;
let Z;
assume that
A4: Z c= ( dom f )
and
A5: ( f | Z ) = ( id Z );
A6:now
let x being Complex;
assume A7: x in Z;
then ( ( f | Z ) . x ) = x by A5,FUNCT_1:18;
then ( f . x ) = x by A7,FUNCT_1:49;
hence ( f /. x ) = x by A4,A7,PARTFUN1:def 6;
end;
A8:now
let x0;
assume A9: x0 in Z;
then consider N being (Neighbourhood of x0) such that A10: N c= Z by CKB40:1;
A11: (for x holds (x in N implies ( ( f /. x ) - ( f /. x0 ) ) = ( ( L /. ( x - x0 ) ) + ( R /. ( x - x0 ) ) )))
proof
let x;
assume x in N;
hence ( ( f /. x ) - ( f /. x0 ) ) = ( x - ( f /. x0 ) ) by A6,A10
.= ( x - x0 ) by A6,A9
.= ( ( L /. ( x - x0 ) ) + ( 0c ) ) by A1,PARTFUN2:6
.= ( ( L /. ( x - x0 ) ) + ( R /. ( x - x0 ) ) ) by FUNCOP_1:7;
end;
N c= ( dom f ) by A4,A10,XBOOLE_1:1;
hence f is_differentiable_in x0 by A11,CKB33:def 1;
end;
hence A12: f is_differentiable_on Z by A4,CKB47:1;
let x0;
assume A13: x0 in Z;
then consider N1 being (Neighbourhood of x0) such that A14: N1 c= Z by CKB40:1;
A15: f is_differentiable_in x0 by A8,A13;
then (ex N being (Neighbourhood of x0) st (N c= ( dom f ) & (ex L st (ex R st (for x holds (x in N implies ( ( f /. x ) - ( f /. x0 ) ) = ( ( L /. ( x - x0 ) ) + ( R /. ( x - x0 ) ) ))))))) by CKB33:def 1;
then consider N being (Neighbourhood of x0) such that A16: N c= ( dom f );
consider N2 being (Neighbourhood of x0) such that A17: N2 c= N1 and A18: N2 c= N by CKB32:1;
A19: N2 c= ( dom f ) by A16,A18,XBOOLE_1:1;
A20: (for x holds (x in N2 implies ( ( f /. x ) - ( f /. x0 ) ) = ( ( L /. ( x - x0 ) ) + ( R /. ( x - x0 ) ) )))
proof
let x;
assume x in N2;
then x in N1 by A17;
hence ( ( f /. x ) - ( f /. x0 ) ) = ( x - ( f /. x0 ) ) by A6,A14
.= ( x - x0 ) by A6,A13
.= ( ( L /. ( x - x0 ) ) + ( 0c ) ) by A1,PARTFUN2:6
.= ( ( L /. ( x - x0 ) ) + ( R /. ( x - x0 ) ) ) by FUNCOP_1:7;
end;
thus ( ( f `| Z ) /. x0 ) = ( diff (f,x0) ) by A12,A13,CKB49:def 1
.= ( L /. ( 1r ) ) by A15,A19,A20,CKB34:def 1
.= ( 1r ) by A1,PARTFUN2:6;
end;
