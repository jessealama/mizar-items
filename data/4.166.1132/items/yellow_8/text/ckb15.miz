begin
theorem
Th21: (for T being non  empty TopStruct holds (for x being (Point of T) holds (for B being (Basis of x) holds (for V being (Subset of T) holds (V in B implies (V is  open & x in V))))))
proof
let T being non  empty TopStruct;
let x being (Point of T);
let B being (Basis of x);
let V being (Subset of T);
assume that
A1: V in B;
B c= (the topology of T) by TOPS_2:64;
hence V is  open by A1,PRE_TOPC:def 2;
x in ( Intersect B ) by CKB12:def 1;
hence thesis by A1,SETFAM_1:43;
end;
