theorem
Th1: (for X,A,B being set holds ((A in ( Fin X ) & B c= A) implies B in ( Fin X )))
proof
let X being set;
let A being set;
let B being set;
assume that
A1: A in ( Fin X )
and
A2: B c= A;
A c= X by A1,FINSUB_1:def 5;
then B c= X by A2,XBOOLE_1:1;
hence thesis by A1,A2,FINSUB_1:def 5;
end;
