begin
theorem
Th2: (for X being set holds (for F being (Subset-Family of X) holds (F c= ( Fin X ) implies ( meet F ) in ( Fin X ))))
proof
let X being set;
let F being (Subset-Family of X);
assume that
A1: F c= ( Fin X );
per cases ;
suppose F = ( {} );

hence thesis by FINSUB_1:7,SETFAM_1:1;
end;
suppose F <> ( {} );

then consider A being set such that A2: A in F by XBOOLE_0:def 1;
( meet F ) c= A by A2,SETFAM_1:3;
hence thesis by A1,A2,CKB1:1;
end;
end;
