begin
registration
let X being  infinite set;
cluster ( CofinTop X ) -> non sober;
coherence
proof
set T = ( CofinTop X );
reconsider S = ( [#] X ) as (Subset of T) by CKB38:def 1;
S is  irreducible
proof
X = ( [#] T ) by CKB38:def 1;
hence S is non empty  closed;
let S1 being (Subset of T),S2 being (Subset of T);
assume that
A1: (S1 is  closed & S2 is  closed)
and
A2: S = ( S1 \/ S2 );
assume (S1 <> S & S2 <> S);
then reconsider S19 = S1,S29 = S2 as  finite set by A1,CKB41:1;
S = ( S19 \/ S29 ) by A2;
hence contradiction;
end;
then reconsider S as  irreducible (Subset of T);
take S;
let p being (Point of T);
now
assume p is_dense_point_of S;
then S c= ( Cl { p } ) by CKB22:def 1;
then ( Cl { p } ) is  infinite;
hence contradiction by CKB42:1;
end;
hence thesis;
end;
