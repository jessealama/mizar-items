begin
theorem
(for T being non empty TopSpace holds (for B being (Basis of T) holds (for V being (Subset of T) holds ( Int V ) = ( union { G where G is (Subset of T): (G in B & G c= V) } ))))
proof
let T being non empty TopSpace;
let B being (Basis of T);
let V being (Subset of T);
set X = { G where G is (Subset of T): (G in B & G c= V) };
set Y = { G where G is (Subset of T): (G in B & G c= ( Int V )) };
X = Y
proof
thus X c= Y
proof
let e being set;
assume e in X;
then consider G being (Subset of T) such that A1: e = G and A2: G in B and A3: G c= V;
G c= ( Int V ) by A2,A3,CKB10:1,TOPS_1:24;
hence thesis by A1,A2;
end;

let e being set;
assume e in Y;
then consider G being (Subset of T) such that A4: (e = G & G in B) and A5: G c= ( Int V );
( Int V ) c= V by TOPS_1:16;
then G c= V by A5,XBOOLE_1:1;
hence thesis by A4;
end;
hence thesis by CKB9:1;
end;
