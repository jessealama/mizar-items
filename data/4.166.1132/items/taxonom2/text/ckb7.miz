environ
vocabularies ORDERS_2,XBOOLE_0,SUBSET_1,ORDERS_1,XXREAL_0,YELLOW_1,RELAT_2,WELLORD2,RELAT_1,TARSKI,TREES_1,EQREL_1,TAXONOM1,SETFAM_1,ZFMISC_1,ABIAN,PRE_TOPC,LATTICES,ORDINAL1,PARTIT1,TAXONOM2;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,SETFAM_1,RELAT_1,ORDINAL1,RELSET_1,STRUCT_0,ORDERS_2,EQREL_1,ABIAN,PARTIT1,ORDERS_1,YELLOW_1,TAXONOM1,CKB1,CKB2,CKB5;
definitions TARSKI,EQREL_1,TAXONOM1,SETFAM_1,XBOOLE_0,CKB1,CKB2;
theorems TARSKI,TAXONOM1,PARTIT1,EQREL_1,ZFMISC_1,ORDERS_2,ORDERS_1,RELAT_1,RELSET_1,SETFAM_1,REALSET1,ABIAN,YELLOW_1,WELLORD2,ORDINAL1,XBOOLE_0,XBOOLE_1,CKB1,CKB2,CKB3,CKB6;
schemes XBOOLE_0;
registrations XBOOLE_0,SUBSET_1,RELSET_1,EQREL_1,YELLOW_1,CKB4;
constructors SETFAM_1,REALSET1,ABIAN,YELLOW_1,TAXONOM1,RELSET_1,CKB1,CKB2;
requirements BOOLE,SUBSET;
begin
reserve X for non  empty set;
reserve x for set;
reserve y for set;
theorem
Th3: (for P being (a_partition of X) holds (for x,y,z being set holds ((((x in P & y in P) & z in x) & z in y) implies x = y)))
proof
let P being (a_partition of X);
let x being set;
let y being set;
let z being set;
assume that
A1: x in P
and
A2: y in P
and
A3: z in x
and
A4: z in y;
consider EqR being (Equivalence_Relation of X) such that A5: P = ( Class EqR ) by EQREL_1:34;
A6: (ex y9 being (Element of X) st y = ( Class (EqR,y9) )) by A2,A5,EQREL_1:36;
(ex x9 being (Element of X) st x = ( Class (EqR,x9) )) by A1,A5,EQREL_1:36;
hence thesis by A3,A4,A6,CKB6:1;
end;
