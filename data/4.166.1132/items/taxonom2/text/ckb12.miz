environ
vocabularies ORDERS_2,XBOOLE_0,SUBSET_1,ORDERS_1,XXREAL_0,YELLOW_1,RELAT_2,WELLORD2,RELAT_1,TARSKI,TREES_1,EQREL_1,TAXONOM1,SETFAM_1,ZFMISC_1,ABIAN,PRE_TOPC,LATTICES,ORDINAL1,PARTIT1,TAXONOM2;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,SETFAM_1,RELAT_1,ORDINAL1,RELSET_1,STRUCT_0,ORDERS_2,EQREL_1,ABIAN,PARTIT1,ORDERS_1,YELLOW_1,TAXONOM1,CKB1,CKB2,CKB5,CKB10;
definitions TARSKI,EQREL_1,TAXONOM1,SETFAM_1,XBOOLE_0,CKB1,CKB2,CKB10;
theorems TARSKI,TAXONOM1,PARTIT1,EQREL_1,ZFMISC_1,ORDERS_2,ORDERS_1,RELAT_1,RELSET_1,SETFAM_1,REALSET1,ABIAN,YELLOW_1,WELLORD2,ORDINAL1,XBOOLE_0,XBOOLE_1,CKB1,CKB2,CKB3,CKB6,CKB7,CKB8,CKB9,CKB10;
schemes XBOOLE_0;
registrations XBOOLE_0,SUBSET_1,RELSET_1,EQREL_1,YELLOW_1,CKB4,CKB11;
constructors SETFAM_1,REALSET1,ABIAN,YELLOW_1,TAXONOM1,RELSET_1,CKB1,CKB2,CKB10;
requirements BOOLE,SUBSET;
begin
reserve x for set;
reserve y for set;
registration
cluster non  trivial  hierarchic for set;
existence
proof
set B = the  empty set;
set A = the non  empty set;
consider C being set such that A1: C = { A,B };
A2: C is  hierarchic
proof
let x being set;
let y being set;
assume that
A3: x in C
and
A4: y in C;
per cases  by A1,A3,TARSKI:def 2;
suppose A5: x = A;

per cases  by A1,A4,TARSKI:def 2;
suppose y = A;

hence thesis by A5;
end;
suppose y = B;

hence thesis by XBOOLE_1:65;
end;
end;
suppose x = B;

hence thesis by XBOOLE_1:65;
end;
end;
take C;
now
assume A6: C is  trivial;
per cases  by A6,ZFMISC_1:131;
suppose C is  empty;

hence contradiction by A1;
end;
suppose (ex x being set st C = { x });

hence contradiction by A1,ZFMISC_1:5;
end;
end;
hence thesis by A2;
end;
end;
