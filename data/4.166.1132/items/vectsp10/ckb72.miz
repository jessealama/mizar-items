requirements SUBSET,BOOLE;
begin
registration
let K being  add-associative  right_zeroed  right_complementable  Abelian  associative  well-unital  distributive non empty doubleLoopStr;
let V being (VectSp of K);
let f being (linear-Functional of V);
cluster ( CQFunctional f ) -> non degenerated;
coherence
proof
set qf = ( CQFunctional f );
set W = ( Ker f );
set qV = ( VectQuot (V,W) );
A1: (the carrier of W) = ( ker f ) by CKB66:def 1;
A2: (the carrier of qV) = ( CosetSet (V,W) ) by CKB37:def 1;
thus ( ker qf ) c= { ( 0. qV ) }
proof
let x being set;
assume x in ( ker qf );
then consider w being (Vector of qV) such that A3: x = w and A4: ( qf . w ) = ( 0. K );
w in ( CosetSet (V,W) ) by A2;
then consider A being (Coset of W) such that A5: w = A;
consider v being (Vector of V) such that A6: A = ( v + W ) by VECTSP_4:def 6;
( f . v ) = ( 0. K ) by A1,A4,A5,A6,CKB67:def 1;
then v in ( ker f );
then v in W by A1,STRUCT_0:def 5;
then w = ( zeroCoset (V,W) ) by A5,A6,VECTSP_4:49
.= ( 0. qV ) by CKB38:1;
hence thesis by A3,TARSKI:def 1;
end;

thus { ( 0. qV ) } c= ( ker qf )
proof
let x being set;
assume x in { ( 0. qV ) };
then A7: x = ( 0. qV ) by TARSKI:def 1;
( qf . ( 0. qV ) ) = ( 0. K ) by HAHNBAN1:def 9;
hence thesis by A7;
end;

end;
end;
