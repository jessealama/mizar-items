begin
definition
let K being  add-associative  right_zeroed  right_complementable non  empty doubleLoopStr;
let V being  right_zeroed non  empty VectSpStr over K;
let f being  0-preserving (Functional of V);
redefine attr f is  constant
means
:Def7: f = ( 0Functional V );
compatibility
proof
A1: (( f . ( 0. V ) ) = ( 0. K ) & (the carrier of V) = ( dom f )) by FUNCT_2:def 1,HAHNBAN1:def 9;
hereby
assume A2: f is  constant;
now
let v being (Vector of V);
thus ( f . v ) = ( 0. K ) by A1,A2,FUNCT_1:def 10
.= ( ( 0Functional V ) . v ) by HAHNBAN1:14;
end;
hence f = ( 0Functional V ) by FUNCT_2:63;
end;
assume A3: f = ( 0Functional V );
now
let x being set;
let y being set;
assume (x in ( dom f ) & y in ( dom f ));
then reconsider v = x,w = y as (Vector of V);
thus ( f . x ) = ( ( 0Functional V ) . v ) by A3
.= ( 0. K ) by HAHNBAN1:14
.= ( ( 0Functional V ) . w ) by HAHNBAN1:14
.= ( f . y ) by A3;
end;
hence thesis by FUNCT_1:def 10;
end;
