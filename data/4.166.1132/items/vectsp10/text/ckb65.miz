requirements SUBSET,BOOLE;
begin
registration
let K being non degenerated non empty doubleLoopStr;
let V being non trivial VectSpStr over K;
cluster non degenerated  0-preserving -> non constant for (Functional of V);
coherence
proof
let f being (Functional of V);
assume that
A1: f is non degenerated
and
A2: f is  0-preserving
and
A3: f is  constant;
A4: ( f . ( 0. V ) ) = ( 0. K ) by A2,HAHNBAN1:def 9;
A5:now
assume A6: (for v being (Vector of V) holds (v <> ( 0. V ) implies ( f . v ) = ( 0. K )));
(the carrier of V) c= ( ker f )
proof
let x being set;
assume x in (the carrier of V);
then reconsider v = x as (Vector of V);
per cases ;
suppose v = ( 0. V );

hence thesis by A4;
end;
suppose v <> ( 0. V );

then ( f . v ) = ( 0. K ) by A6;
hence thesis;
end;
end;
then (the carrier of V) = ( ker f ) by XBOOLE_0:def 10
.= { ( 0. V ) } by A1,CKB64:def 1;
hence contradiction by ZFMISC_1:131;
end;
( dom f ) = (the carrier of V) by FUNCT_2:def 1;
hence contradiction by A3,A4,A5,FUNCT_1:def 10;
end;
end;
