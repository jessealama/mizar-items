begin
theorem
(for K being  add-associative  right_zeroed  right_complementable  Abelian  associative  well-unital  distributive non  empty doubleLoopStr holds (for V being (VectSp of K) holds (for S,T being (Subspace of V) holds (for v being (Vector of V) holds (((( S /\ T ) = ( (0). V ) & v in S) & v in T) implies v = ( 0. V ))))))
proof
let K being  add-associative  right_zeroed  right_complementable  Abelian  associative  well-unital  distributive non  empty doubleLoopStr;
let V being (VectSp of K);
let S being (Subspace of V);
let T being (Subspace of V);
let v being (Vector of V);
assume that
A1: ( S /\ T ) = ( (0). V )
and
A2: (v in S & v in T);
(v in (the carrier of S) & v in (the carrier of T)) by A2,STRUCT_0:def 5;
then v in ( (the carrier of S) /\ (the carrier of T) ) by XBOOLE_0:def 4;
then v in (the carrier of ( S /\ T )) by VECTSP_5:def 2;
then v in { ( 0. V ) } by A1,VECTSP_4:def 3;
hence thesis by TARSKI:def 1;
end;
