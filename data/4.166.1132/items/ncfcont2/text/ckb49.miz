begin
theorem
Lm3: (for X being ComplexNormSpace holds (for x being (Point of X) holds ((for e being Real holds (e > ( 0 ) implies ||. x .|| < e)) implies x = ( 0. X ))))
proof
let X being ComplexNormSpace;
let x being (Point of X);
assume that
A1: (for e being Real holds (e > ( 0 ) implies ||. x .|| < e));
now
assume x <> ( 0. X );
then ( 0 ) <> ||. x .|| by NORMSP_0:def 5;
then ( 0 ) < ||. x .|| by CLVECT_1:105;
hence contradiction by A1;
end;
hence thesis;
end;
