begin
reserve X for set;
reserve CNS1 for ComplexNormSpace;
reserve CNS2 for ComplexNormSpace;
theorem
Th25: (for f being (PartFunc of CNS1,CNS2) holds (f is_Lipschitzian_on X implies f is_uniformly_continuous_on X))
proof
let f being (PartFunc of CNS1,CNS2);
assume A1: f is_Lipschitzian_on X;
hence X c= ( dom f ) by NCFCONT1:def 17;
consider r being Real such that A2: ( 0 ) < r and A3: (for x1,x2 being (Point of CNS1) holds ((x1 in X & x2 in X) implies ||. ( ( f /. x1 ) - ( f /. x2 ) ) .|| <= ( r * ||. ( x1 - x2 ) .|| ))) by A1,NCFCONT1:def 17;
let p being Real;
assume that
A4: ( 0 ) < p;
take s = ( p / r );
thus ( 0 ) < s by A2,A4,XREAL_1:139;
let x1 being (Point of CNS1),x2 being (Point of CNS1);
assume ((x1 in X & x2 in X) & ||. ( x1 - x2 ) .|| < s);
then (( r * ||. ( x1 - x2 ) .|| ) < ( s * r ) & ||. ( ( f /. x1 ) - ( f /. x2 ) ) .|| <= ( r * ||. ( x1 - x2 ) .|| )) by A2,A3,XREAL_1:68;
then ||. ( ( f /. x1 ) - ( f /. x2 ) ) .|| < ( ( p / r ) * r ) by XXREAL_0:2;
hence thesis by A2,XCMPLX_1:87;
end;
