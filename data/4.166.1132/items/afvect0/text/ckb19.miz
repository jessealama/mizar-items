environ
vocabularies XBOOLE_0,ANALOAF,SUBSET_1,STRUCT_0,ZFMISC_1,TDGROUP,DIRAF,BINOP_1,FUNCT_1,ALGSTR_0,SUPINF_2,ARYTM_3,RLVECT_1,ARYTM_1,VECTSP_1,MCART_1,PBOOLE,RELAT_1,TARSKI,AFVECT0;
notations TARSKI,ZFMISC_1,SUBSET_1,STRUCT_0,ALGSTR_0,ANALOAF,TDGROUP,FUNCT_1,FUNCT_2,MCART_1,BINOP_1,RELAT_1,VECTSP_1,RLVECT_1,CKB1,CKB3,CKB17;
definitions STRUCT_0,RLVECT_1,BINOP_1,ALGSTR_0,CKB1,CKB17;
theorems DOMAIN_1,TDGROUP,FUNCT_1,FUNCT_2,MCART_1,RELAT_1,TARSKI,RLVECT_1,ANALOAF,XBOOLE_0,VECTSP_1,STRUCT_0,CKB1,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18;
schemes BINOP_1,FUNCT_2;
registrations XBOOLE_0,SUBSET_1,RELSET_1,STRUCT_0,VECTSP_1,TDGROUP,RELAT_1,CKB2,CKB4;
constructors BINOP_1,DOMAIN_1,TDGROUP,RELSET_1,CKB1,CKB17;
requirements SUBSET,BOOLE;
begin
reserve AFV for WeakAffVect;
reserve a for (Element of AFV);
reserve b for (Element of AFV);
reserve c for (Element of AFV);
reserve d for (Element of AFV);
theorem
(( MDist a,b &  MDist a,c) implies (b = c or  MDist b,c))
proof
assume that
A1:  MDist a,b
and
A2:  MDist a,c;
A3: a,b // b,a by A1,CKB17:def 1;
A4: a,c // c,a by A2,CKB17:def 1;
consider d such that A5: c,a // b,d by CKB1:def 1;
b,d // c,a by A5,CKB7:1;
then a,c // b,d by A4,CKB1:def 1;
then A6: b,c // a,d by A3,CKB1:def 1;
c,b // a,d by A5,CKB1:def 1;
then b,c // c,b by A6,CKB1:def 1;
hence thesis by CKB17:def 1;
end;
