environ
vocabularies XBOOLE_0,SUBSET_1,FUNCT_2,MARGREL1,PARTIT1,BVFUNC_1,XBOOLEAN,FUNCT_1,RELAT_1,TARSKI,EQREL_1,BVFUNC_2;
notations TARSKI,XBOOLE_0,SUBSET_1,XBOOLEAN,MARGREL1,RELAT_1,FUNCT_1,FUNCT_2,EQREL_1,BVFUNC_1,BVFUNC_2;
definitions BVFUNC_1,XBOOLEAN,BVFUNC_2;
theorems FUNCT_1,FUNCT_2,BVFUNC_1,BVFUNC_2,MARGREL1,XBOOLEAN,EQREL_1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15;
registrations XBOOLE_0,XBOOLEAN,MARGREL1;
constructors BINARITH,BVFUNC_1,BVFUNC_2;
requirements SUBSET,BOOLE,ARITHM;
begin
reserve Y for non empty set;
theorem
(for a,b being (Element of ( Funcs (Y,( BOOLEAN )) )) holds (for G being (Subset of ( PARTITIONS Y )) holds (for PA being (a_partition of Y) holds ( All (( a 'eqv' b ),PA,G) ) = ( ( All (( a 'imp' b ),PA,G) ) '&' ( All (( b 'imp' a ),PA,G) ) ))))
proof
let a being (Element of ( Funcs (Y,( BOOLEAN )) )),b being (Element of ( Funcs (Y,( BOOLEAN )) ));
let G being (Subset of ( PARTITIONS Y ));
let PA being (a_partition of Y);
consider k3 being Function such that A1: ( All (( a 'eqv' b ),PA,G) ) = k3 and A2: ( dom k3 ) = Y and ( rng k3 ) c= ( BOOLEAN ) by FUNCT_2:def 2;
consider k4 being Function such that A3: ( ( All (( a 'imp' b ),PA,G) ) '&' ( All (( b 'imp' a ),PA,G) ) ) = k4 and A4: ( dom k4 ) = Y and ( rng k4 ) c= ( BOOLEAN ) by FUNCT_2:def 2;
(for z being (Element of Y) holds ( ( All (( a 'eqv' b ),PA,G) ) . z ) = ( ( ( All (( a 'imp' b ),PA,G) ) '&' ( All (( b 'imp' a ),PA,G) ) ) . z ))
proof
let z being (Element of Y);
( ( All (( a 'eqv' b ),PA,G) ) . z ) = ( ( All (( ( a 'imp' b ) '&' ( b 'imp' a ) ),PA,G) ) . z ) by CKB7:1
.= ( ( ( All (( a 'imp' b ),PA,G) ) '&' ( All (( b 'imp' a ),PA,G) ) ) . z ) by BVFUNC_1:39;
hence thesis;
end;
then (for u being set holds (u in Y implies ( k3 . u ) = ( k4 . u ))) by A1,A3;
hence thesis by A1,A2,A3,A4,FUNCT_1:2;
end;
