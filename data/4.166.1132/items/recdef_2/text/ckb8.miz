environ
vocabularies NUMBERS,ZFMISC_1,XBOOLE_0,SUBSET_1,FUNCT_1,RELAT_1,FUNCT_4,TARSKI,CARD_1,ARYTM_3,MCART_1,NAT_1,ARYTM_1,XXREAL_0,FUNCT_7,RECDEF_2;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,MCART_1,MCART_2,DOMAIN_1,NUMBERS,XCMPLX_0,NAT_1,NAT_D,RELAT_1,FUNCT_1,FUNCT_2,FUNCT_4,FUNCT_7,XXREAL_0,CKB1,CKB2,CKB3,CKB7;
definitions TARSKI,XBOOLE_0,CKB1,CKB2,CKB3,CKB7;
theorems MCART_1,NAT_1,FUNCT_1,FUNCT_2,RELSET_1,INT_1,FUNCT_4,XBOOLE_0,XBOOLE_1,DOMAIN_1,MCART_2,FUNCT_7,XREAL_1,XXREAL_0,XREAL_0,NAT_D,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7;
schemes NAT_1,RECDEF_1,FUNCT_1,XBOOLE_0,PARTFUN1;
registrations SUBSET_1,ORDINAL1,RELSET_1,XREAL_0,NAT_1,INT_1;
constructors DOMAIN_1,FUNCT_4,XXREAL_0,NAT_1,INT_1,MCART_2,BINARITH,FUNCT_7,NAT_D,RELSET_1,CKB1,CKB2,CKB3,CKB7;
requirements BOOLE,SUBSET,NUMERALS,ARITHM,REAL;
begin
definition
let x being set;
given x1 being set,x2 being set,x3 being set,x4 being set such that
A1: x = [ x1,x2,x3,x4 ];

func x `2_4
means
:Def5: (for y1,y2,y3,y4 being set holds (x = [ y1,y2,y3,y4 ] implies it = y2));
existence
proof
take x2;
thus thesis by A1,MCART_1:29;
end;
uniqueness
proof
let z1 being set,z2 being set;
assume that
A4: (for y1,y2,y3,y4 being set holds (x = [ y1,y2,y3,y4 ] implies z1 = y2))
and
A5: (for y1,y2,y3,y4 being set holds (x = [ y1,y2,y3,y4 ] implies z2 = y2));
z1 = x2 by A1,A4;
hence thesis by A1,A5;
end;
end;
