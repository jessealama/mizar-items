begin
definition
let x being set;
given x1 being set,x2 being set,x3 being set such that
A1: x = [ x1,x2,x3 ];

func x `1_3
means
:Def1: (for y1,y2,y3 being set holds (x = [ y1,y2,y3 ] implies it = y1));
existence
proof
take x1;
thus thesis by A1,MCART_1:25;
end;
uniqueness
proof
let z1 being set,z2 being set;
assume that
A2: (for y1,y2,y3 being set holds (x = [ y1,y2,y3 ] implies z1 = y1))
and
A3: (for y1,y2,y3 being set holds (x = [ y1,y2,y3 ] implies z2 = y1));
z1 = x1 by A1,A2;
hence thesis by A1,A3;
end;
end;
