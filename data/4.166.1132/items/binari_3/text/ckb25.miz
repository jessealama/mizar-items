begin
definition
let n being Nat;
let k being Nat;
func n -BinarySequence k -> (Tuple of n,( BOOLEAN )) means 
:Def1: (for i being Nat holds (i in ( Seg n ) implies ( it /. i ) = ( IFEQ (( ( k div ( 2 to_power ( i -' 1 ) ) ) mod 2 ),( 0 ),( FALSE ),( TRUE )) )));
existence
proof
reconsider n9 = n as Nat;
deffunc F(Nat) = ( IFEQ (( ( k div ( 2 to_power ( $1 -' 1 ) ) ) mod 2 ),( 0 ),( FALSE ),( TRUE )) );
consider p being (FinSequence of ( BOOLEAN )) such that A1: ( len p ) = n9 and A2: (for i being Nat holds (i in ( dom p ) implies ( p . i ) = F(i))) from FINSEQ_2:sch 1;
A3: ( dom p ) = ( Seg n9 ) by A1,FINSEQ_1:def 3;
reconsider p as (Element of ( n -tuples_on ( BOOLEAN ) )) by A1,FINSEQ_2:92;
take p;
let i being Nat;
assume A4: i in ( Seg n );
then i in ( dom p ) by A1,FINSEQ_1:def 3;
hence ( p /. i ) = ( p . i ) by PARTFUN1:def 6
.= ( IFEQ (( ( k div ( 2 to_power ( i -' 1 ) ) ) mod 2 ),( 0 ),( FALSE ),( TRUE )) ) by A2,A3,A4;
end;
uniqueness
proof
let p1 being (Tuple of n,( BOOLEAN ));
let p2 being (Tuple of n,( BOOLEAN ));
assume that
A5: (for i being Nat holds (i in ( Seg n ) implies ( p1 /. i ) = ( IFEQ (( ( k div ( 2 to_power ( i -' 1 ) ) ) mod 2 ),( 0 ),( FALSE ),( TRUE )) )))
and
A6: (for i being Nat holds (i in ( Seg n ) implies ( p2 /. i ) = ( IFEQ (( ( k div ( 2 to_power ( i -' 1 ) ) ) mod 2 ),( 0 ),( FALSE ),( TRUE )) )));
A7: ( len p1 ) = n by CARD_1:def 7;
then A8: ( dom p1 ) = ( Seg n ) by FINSEQ_1:def 3;
A9: ( len p2 ) = n by CARD_1:def 7;
now
let i being Nat;
assume A10: i in ( dom p1 );
then A11: i in ( dom p2 ) by A9,A8,FINSEQ_1:def 3;
thus ( p1 . i ) = ( p1 /. i ) by A10,PARTFUN1:def 6
.= ( IFEQ (( ( k div ( 2 to_power ( i -' 1 ) ) ) mod 2 ),( 0 ),( FALSE ),( TRUE )) ) by A5,A8,A10
.= ( p2 /. i ) by A6,A8,A10
.= ( p2 . i ) by A11,PARTFUN1:def 6;
end;
hence thesis by A7,A9,FINSEQ_2:9;
end;
