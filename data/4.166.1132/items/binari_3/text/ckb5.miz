constructors XXREAL_0,NAT_1,NAT_D,FINSEQOP,SERIES_1,ZFMISC_1,BINARITH,FINSEQ_5,BINARI_2,ABIAN,EUCLID,BINOP_2,RELSET_1;
requirements REAL,NUMERALS,SUBSET,BOOLE,ARITHM;
begin
theorem
Th6: (for n being Nat holds (for y being (Tuple of n,( BOOLEAN )) holds (y = ( 0* n ) implies ( 'not' y ) = ( n |-> 1 ))))
proof
let n being Nat;
let y being (Tuple of n,( BOOLEAN ));
assume A1: y = ( 0* n );
A2:now
A3: ( len y ) = n by CARD_1:def 7;
let i being Nat;
assume that
A4: 1 <= i
and
A5: i <= ( len ( 'not' y ) );
A6: ( len ( 'not' y ) ) = n by CARD_1:def 7;
then A7: i in ( Seg n ) by A4,A5,FINSEQ_1:1;
then A8: ( y . i ) = ( FALSE ) by A1,FUNCOP_1:7;
thus ( ( 'not' y ) . i ) = ( ( 'not' y ) /. i ) by A4,A5,FINSEQ_4:15
.= ( 'not' ( y /. i ) ) by A7,BINARITH:def 1
.= ( 'not' ( FALSE ) ) by A4,A5,A6,A3,A8,FINSEQ_4:15
.= ( ( n |-> 1 ) . i ) by A7,FUNCOP_1:7;
end;
( len ( 'not' y ) ) = n by CARD_1:def 7
.= ( len ( n |-> 1 ) ) by CARD_1:def 7;
hence thesis by A2,FINSEQ_1:14;
end;
