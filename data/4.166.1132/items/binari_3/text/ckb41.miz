theorem
(for n being non  empty Nat holds (for x being (Tuple of n,( BOOLEAN )) holds ( n -BinarySequence ( Absval x ) ) = x))
proof
let n being non  empty Nat;
let x being (Tuple of n,( BOOLEAN ));
( Absval x ) < ( 2 to_power n ) by CKB1:1;
then ( Absval ( n -BinarySequence ( Absval x ) ) ) = ( Absval x ) by CKB40:1;
hence thesis by CKB2:1;
end;
