begin
reserve n for (Element of ( NAT ));
reserve D for non  empty set;
reserve y1 for set;
reserve y2 for set;
scheme DefRecD { D() -> non  empty set,A() -> (Element of D()),n() -> Nat,P[set,set,set] } : ((ex y being (Element of D()) st (ex f being (Function of ( NAT ),D()) st ((y = ( f . n() ) & ( f . ( 0 ) ) = A()) & (for n holds P[ n,( f . n ),( f . ( n + 1 ) ) ])))) & (for y1,y2 being (Element of D()) holds (((ex f being (Function of ( NAT ),D()) st ((y1 = ( f . n() ) & ( f . ( 0 ) ) = A()) & (for n holds P[ n,( f . n ),( f . ( n + 1 ) ) ]))) & (ex f being (Function of ( NAT ),D()) st ((y2 = ( f . n() ) & ( f . ( 0 ) ) = A()) & (for n holds P[ n,( f . n ),( f . ( n + 1 ) ) ])))) implies y1 = y2)))
provided
A1: (for n being (Element of ( NAT )) holds (for x being (Element of D()) holds (ex y being (Element of D()) st P[ n,x,y ])))
and
A2: (for n being (Element of ( NAT )) holds (for x,y1,y2 being (Element of D()) holds ((P[ n,x,y1 ] & P[ n,x,y2 ]) implies y1 = y2)))
proof
A3: (for n being (Element of ( NAT )) holds (for x being (Element of D()) holds (ex y being (Element of D()) st P[ n,x,y ]))) by A1;
consider f being (Function of ( NAT ),D()) such that A4: (( f . ( 0 ) ) = A() & (for n being (Element of ( NAT )) holds P[ n,( f . n ),( f . ( n + 1 ) ) ])) from CKB4:sch 1(A3);
A5: (for n being Nat holds (for x,y1,y2 being (Element of D()) holds ((P[ n,x,y1 ] & P[ n,x,y2 ]) implies y1 = y2)))
proof
let n being Nat;
n in ( NAT ) by ORDINAL1:def 12;
hence thesis by A2;
end;
thus (ex y being (Element of D()) st (ex f being (Function of ( NAT ),D()) st ((y = ( f . n() ) & ( f . ( 0 ) ) = A()) & (for n holds P[ n,( f . n ),( f . ( n + 1 ) ) ]))))
proof
reconsider n = n() as (Element of ( NAT )) by ORDINAL1:def 12;
take ( f . n );
take f;
thus thesis by A4;
end;

let y1 being (Element of D());
let y2 being (Element of D());
given f1 being (Function of ( NAT ),D()) such that
A6: y1 = ( f1 . n() )
and
A7: ( f1 . ( 0 ) ) = A()
and
A8: (for n holds P[ n,( f1 . n ),( f1 . ( n + 1 ) ) ]);

A9: ( f1 . ( 0 ) ) = A() by A7;
A10: (for n being Nat holds P[ n,( f1 . n ),( f1 . ( n + 1 ) ) ])
proof
let n being Nat;
n in ( NAT ) by ORDINAL1:def 12;
hence thesis by A8;
end;
given f2 being (Function of ( NAT ),D()) such that
A11: y2 = ( f2 . n() )
and
A12: ( f2 . ( 0 ) ) = A()
and
A13: (for n holds P[ n,( f2 . n ),( f2 . ( n + 1 ) ) ]);

A14: (for n being Nat holds P[ n,( f2 . n ),( f2 . ( n + 1 ) ) ])
proof
let n being Nat;
n in ( NAT ) by ORDINAL1:def 12;
hence thesis by A13;
end;
A15: ( f2 . ( 0 ) ) = A() by A12;
f1 = f2 from NAT_1:sch 14(A9,A10,A15,A14,A5);
hence thesis by A6,A11;
end;
