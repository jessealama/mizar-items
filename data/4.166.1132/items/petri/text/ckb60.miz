registrations XBOOLE_0,SUBSET_1,RELSET_1,STRUCT_0,PARTIT_2,RELAT_1,CKB2,CKB6,CKB7,CKB8,CKB9,CKB39,CKB42,CKB45;
constructors RELSET_1,DOMAIN_1,STRUCT_0,PARTIT_2,CKB1,CKB2,CKB3,CKB4,CKB5,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB27,CKB28,CKB37,CKB38,CKB40,CKB41,CKB43,CKB44,CKB48,CKB49,CKB50,CKB51,CKB52,CKB53,CKB54,CKB55;
requirements SUBSET,BOOLE;
begin
reserve PTN for Petri_net;
reserve S0 for (Subset of (the carrier of PTN));
theorem
(for PTN being Petri_net holds (for t being (transition of PTN) holds (for S0 being (Subset of (the carrier of PTN)) holds (t in ( S0 *' ) iff ( *' { t } ) meets S0))))
proof
let PTN being Petri_net;
let t being (transition of PTN);
let S0 being (Subset of (the carrier of PTN));
thus (t in ( S0 *' ) implies ( *' { t } ) meets S0)
proof
assume t in ( S0 *' );
then consider f being (S-T_arc of PTN),s being (place of PTN) such that A1: s in S0 and A2: f = [ s,t ] by CKB26:1;
t in { t } by TARSKI:def 1;
then s in ( *' { t } ) by A2;
hence ( ( *' { t } ) /\ S0 ) <> ( {} ) by A1,XBOOLE_0:def 4;
end;

assume ( ( *' { t } ) /\ S0 ) <> ( {} );
then consider s being (place of PTN) such that A3: s in ( ( *' { t } ) /\ S0 ) by SUBSET_1:4;
A4: s in S0 by A3,XBOOLE_0:def 4;
s in ( *' { t } ) by A3,XBOOLE_0:def 4;
then consider f being (S-T_arc of PTN),t0 being (transition of PTN) such that A5: t0 in { t } and A6: f = [ s,t0 ] by CKB30:1;
t0 = t by A5,TARSKI:def 1;
hence thesis by A4,A6;
end;
