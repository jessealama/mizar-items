theorem
Th29: (for X being non empty set holds (for Y being ComplexNormSpace holds (Y is  complete implies (for seq being (sequence of ( C_NormSpace_of_BoundedFunctions (X,Y) )) holds (seq is  Cauchy_sequence_by_Norm implies seq is  convergent)))))
proof
let X being non empty set;
let Y being ComplexNormSpace;
assume that
A1: Y is  complete;
let vseq being (sequence of ( C_NormSpace_of_BoundedFunctions (X,Y) ));
assume that
A2: vseq is  Cauchy_sequence_by_Norm;
defpred P[ set,set ]
