constructors PARTFUN1,REAL_1,NAT_1,COMPLEX1,SEQM_3,COMSEQ_2,REALSET1,MEASURE6,INTEGRA2,LOPBAN_1,CSSPACE3,CLOPBAN1,SEQ_1,XXREAL_2,RVSUM_1,SEQ_4,RELSET_1,BINOP_2,SEQ_2,SERIES_1,CKB3,CKB11,CKB18,CKB25;
requirements SUBSET,REAL,BOOLE,NUMERALS,ARITHM;
begin
definition
let X being non empty set;
let Y being ComplexNormSpace;
func ComplexBoundedFunctions (X,Y) -> (Subset of ( ComplexVectSpace (X,Y) ))
means
:Def5: (for x being set holds (x in it iff x is  bounded  bounded (Function of X,(the carrier of Y))));
existence
proof
defpred P[ set ]
 means
$1 is  bounded  bounded (Function of X,(the carrier of Y));
consider IT being set such that A1: (for x being set holds (x in IT iff (x in ( Funcs (X,(the carrier of Y)) ) & P[ x ]))) from XBOOLE_0:sch 1;
take IT;
(for x being set holds (x in IT implies x in ( Funcs (X,(the carrier of Y)) ))) by A1;
hence IT is (Subset of ( ComplexVectSpace (X,Y) )) by TARSKI:def 3;
let x being set;
thus (x in IT implies x is  bounded  bounded (Function of X,(the carrier of Y))) by A1;
assume A2: x is  bounded  bounded (Function of X,(the carrier of Y));
then x in ( Funcs (X,(the carrier of Y)) ) by FUNCT_2:8;
hence thesis by A1,A2;
end;
uniqueness
proof
let X1 being (Subset of ( ComplexVectSpace (X,Y) )),X2 being (Subset of ( ComplexVectSpace (X,Y) ));
assume that
A3: (for x being set holds (x in X1 iff x is  bounded  bounded (Function of X,(the carrier of Y))))
and
A4: (for x being set holds (x in X2 iff x is  bounded  bounded (Function of X,(the carrier of Y))));
(for x being set holds (x in X2 implies x in X1))
proof
let x being set;
assume x in X2;
then x is  bounded  bounded (Function of X,(the carrier of Y)) by A4;
hence thesis by A3;
end;
then A5: X2 c= X1 by TARSKI:def 3;
(for x being set holds (x in X1 implies x in X2))
proof
let x being set;
assume x in X1;
then x is  bounded  bounded (Function of X,(the carrier of Y)) by A3;
hence thesis by A4;
end;
then X1 c= X2 by TARSKI:def 3;
hence thesis by A5,XBOOLE_0:def 10;
end;
end;
