proof
let A1 being Ordinal,A2 being Ordinal;
assume that
A2: ((A1 = ( {} ) & (ex B st (B in ( dom fi ) & (for C holds ((B c= C & C in ( dom fi )) implies ( fi . C ) = ( {} )))))) or (A1 <> ( {} ) & (for B holds (for C holds ((B in A1 & A1 in C) implies (ex D st (D in ( dom fi ) & (for E being Ordinal holds ((D c= E & E in ( dom fi )) implies (B in ( fi . E ) & ( fi . E ) in C))))))))))
and
A3: ((A2 = ( {} ) & (ex B st (B in ( dom fi ) & (for C holds ((B c= C & C in ( dom fi )) implies ( fi . C ) = ( {} )))))) or (A2 <> ( {} ) & (for B holds (for C holds ((B in A2 & A2 in C) implies (ex D st (D in ( dom fi ) & (for E being Ordinal holds ((D c= E & E in ( dom fi )) implies (B in ( fi . E ) & ( fi . E ) in C))))))))));
A4:now
set x = the (Element of A1);
reconsider x as Ordinal;
assume A1 in A2;
then consider D such that A5: D in ( dom fi ) and A6: (for A holds ((D c= A & A in ( dom fi )) implies (A1 in ( fi . A ) & ( fi . A ) in ( succ A2 )))) by A3,ORDINAL1:6;
now
assume A1 = ( {} );
then consider B such that A7: B in ( dom fi ) and A8: (for C holds ((B c= C & C in ( dom fi )) implies ( fi . C ) = ( {} ))) by A2;
(B c= D or D c= B);
then consider E being Ordinal such that A9: ((E = D & B c= D) or (E = B & D c= B));
( fi . E ) = ( {} ) by A5,A7,A8,A9;
hence contradiction by A5,A6,A7,A9;
end;
then consider C such that A10: C in ( dom fi ) and A11: (for A holds ((C c= A & A in ( dom fi )) implies (x in ( fi . A ) & ( fi . A ) in ( succ A1 )))) by A2,ORDINAL1:6;
(C c= D or D c= C);
then consider E being Ordinal such that A12: ((E = D & C c= D) or (E = C & D c= C));
( fi . E ) in ( succ A1 ) by A5,A10,A11,A12;
then A13: (( fi . E ) in A1 or ( fi . E ) = A1) by ORDINAL1:8;
A1 in ( fi . E ) by A5,A6,A10,A12;
hence contradiction by A13;
end;
set x = the (Element of A2);
reconsider x as Ordinal;
assume A1 <> A2;
then (A1 in A2 or A2 in A1) by ORDINAL1:14;
then consider D such that A14: D in ( dom fi ) and A15: (for A holds ((D c= A & A in ( dom fi )) implies (A2 in ( fi . A ) & ( fi . A ) in ( succ A1 )))) by A2,A4,ORDINAL1:6;
now
assume A2 = ( {} );
then consider B such that A16: B in ( dom fi ) and A17: (for C holds ((B c= C & C in ( dom fi )) implies ( fi . C ) = ( {} ))) by A3;
(B c= D or D c= B);
then consider E being Ordinal such that A18: ((E = D & B c= D) or (E = B & D c= B));
( fi . E ) = ( {} ) by A14,A16,A17,A18;
hence contradiction by A14,A15,A16,A18;
end;
then consider C such that A19: C in ( dom fi ) and A20: (for A holds ((C c= A & A in ( dom fi )) implies (x in ( fi . A ) & ( fi . A ) in ( succ A2 )))) by A3,ORDINAL1:6;
(C c= D or D c= C);
then consider E being Ordinal such that A21: ((E = D & C c= D) or (E = C & D c= C));
( fi . E ) in ( succ A2 ) by A14,A19,A20,A21;
