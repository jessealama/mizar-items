definition
let L;
func lim_sup L -> Ordinal
means
(ex fi st ((it = ( inf fi ) & ( dom fi ) = ( dom L )) & (for A holds (A in ( dom L ) implies ( fi . A ) = ( sup ( rng ( L | ( ( dom L ) \ A ) ) ) )))));
existence
proof
deffunc F(Ordinal) = ( sup ( rng ( L | ( ( dom L ) \ $1 ) ) ) );
consider fi such that A1: (( dom fi ) = ( dom L ) & (for A holds (A in ( dom L ) implies ( fi . A ) = F(A)))) from CKB38:sch 1;
take ( inf fi );
take fi;
thus thesis by A1;
end;
uniqueness
proof
let A1 being Ordinal,A2 being Ordinal;
given fi such that
A2: (A1 = ( inf fi ) & ( dom fi ) = ( dom L ))
and
A3: (for A holds (A in ( dom L ) implies ( fi . A ) = ( sup ( rng ( L | ( ( dom L ) \ A ) ) ) )));

given psi such that
A4: (A2 = ( inf psi ) & ( dom psi ) = ( dom L ))
and
A5: (for A holds (A in ( dom L ) implies ( psi . A ) = ( sup ( rng ( L | ( ( dom L ) \ A ) ) ) )));

now
let x;
assume A6: x in ( dom L );
then reconsider A = x as Ordinal;
( fi . A ) = ( sup ( rng ( L | ( ( dom L ) \ A ) ) ) ) by A3,A6;
hence ( fi . x ) = ( psi . x ) by A5,A6;
end;
hence thesis by A2,A4,FUNCT_1:2;
end;
