environ
vocabularies ORDINAL1,FUNCT_1,XBOOLE_0,TARSKI,ZFMISC_1,RELAT_1,SETFAM_1,SUBSET_1,FUNCOP_1,ORDINAL2;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,RELAT_1,FUNCT_1,ORDINAL1,SETFAM_1,FUNCOP_1;
definitions ORDINAL1,TARSKI,XBOOLE_0;
theorems TARSKI,XBOOLE_0,ORDINAL1,SETFAM_1,ZFMISC_1,FUNCT_1,RELAT_1,XBOOLE_1,FUNCOP_1;
schemes XBOOLE_0,FUNCT_1,ORDINAL1;
registrations XBOOLE_0,SUBSET_1,FUNCT_1,ORDINAL1,FUNCOP_1;
constructors SETFAM_1,ORDINAL1,FUNCOP_1;
requirements SUBSET,BOOLE,NUMERALS;
begin
reserve A for Ordinal;
reserve B for Ordinal;
scheme OrdinalInd { P[Ordinal] } : (for A holds P[ A ])
provided
A1: P[ ( {} ) ]
and
A2: (for A holds (P[ A ] implies P[ ( succ A ) ]))
and
A3: (for A holds (((A <> ( {} ) & A is  limit_ordinal) & (for B holds (B in A implies P[ B ]))) implies P[ A ]))
proof
A4: (for A holds ((for B holds (B in A implies P[ B ])) implies P[ A ]))
proof
let A;
assume that
A5: (for B holds (B in A implies P[ B ]));
A6:now
assume that
A7: A <> ( {} )
and
A8: (for B holds A <> ( succ B ));
A is  limit_ordinal by A8,ORDINAL1:29;
hence thesis by A3,A5,A7;
end;
now
given B such that
A9: A = ( succ B );

B in A by A9,ORDINAL1:6;
hence thesis by A2,A5,A9;
end;
hence thesis by A1,A6;
end;
thus (for A holds P[ A ]) from ORDINAL1:sch 2(A4);
end;
