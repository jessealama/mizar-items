environ
vocabularies ORDINAL1,FUNCT_1,XBOOLE_0,TARSKI,ZFMISC_1,RELAT_1,SETFAM_1,SUBSET_1,FUNCOP_1,ORDINAL2;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,RELAT_1,FUNCT_1,ORDINAL1,SETFAM_1,FUNCOP_1,CKB7,CKB17;
definitions ORDINAL1,TARSKI,XBOOLE_0,CKB7,CKB17;
theorems TARSKI,XBOOLE_0,ORDINAL1,SETFAM_1,ZFMISC_1,FUNCT_1,RELAT_1,XBOOLE_1,FUNCOP_1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB17;
schemes XBOOLE_0,FUNCT_1,ORDINAL1,CKB1;
registrations XBOOLE_0,SUBSET_1,FUNCT_1,ORDINAL1,FUNCOP_1,CKB16;
constructors SETFAM_1,ORDINAL1,FUNCOP_1,CKB7,CKB17;
requirements SUBSET,BOOLE,NUMERALS;
begin
reserve A for Ordinal;
reserve B for Ordinal;
reserve C for Ordinal;
reserve X for set;
reserve x for set;
definition
let X;
func sup X -> Ordinal means 
:Def7: (( On X ) c= it & (for A holds (( On X ) c= A implies it c= A)));
existence
proof
defpred P[ Ordinal ]
 means
( On X ) c= $1;
(x in ( On X ) implies x is Ordinal) by ORDINAL1:def 9;
then reconsider A = ( union ( On X ) ) as Ordinal by ORDINAL1:23;
( On X ) c= ( succ A )
proof
let x;
assume A1: x in ( On X );
then reconsider B = x as Ordinal by ORDINAL1:def 9;
B c= A by A1,ZFMISC_1:74;
hence thesis by ORDINAL1:22;
end;
then A2: (ex A st P[ A ]);
thus (ex F being Ordinal st (P[ F ] & (for A holds (P[ A ] implies F c= A)))) from ORDINAL1:sch 1(A2);
end;
uniqueness
proof
let B;
let C;
assume ((( On X ) c= B & (for A holds (( On X ) c= A implies B c= A))) & (( On X ) c= C & (for A holds (( On X ) c= A implies C c= A))));
hence (B c= C & C c= B);
end;
end;
