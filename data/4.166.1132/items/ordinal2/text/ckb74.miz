requirements SUBSET,BOOLE,NUMERALS;
begin
reserve A for Ordinal;
reserve B for Ordinal;
reserve C for Ordinal;
reserve D for Ordinal;
reserve x for set;
reserve fi for Ordinal-Sequence;
theorem
Th51: (A c= B implies ( A +^ C ) c= ( B +^ C ))
proof
defpred P[ Ordinal ]
 means
( A +^ $1 ) c= ( B +^ $1 );
assume A1: A c= B;
A2: (for C holds ((for D holds (D in C implies P[ D ])) implies P[ C ]))
proof
let C;
assume that
A3: (for D holds (D in C implies ( A +^ D ) c= ( B +^ D )));
A4:now
given D such that
A5: C = ( succ D );

A6: ( B +^ C ) = ( succ ( B +^ D ) ) by A5,CKB67:1;
(( A +^ D ) c= ( B +^ D ) & ( A +^ C ) = ( succ ( A +^ D ) )) by A3,A5,CKB67:1,ORDINAL1:6;
hence thesis by A6,CKB2:1;
end;
A7:now
deffunc F(Ordinal) = ( A +^ $1 );
assume that
A8: C <> ( {} )
and
A9: (for D holds C <> ( succ D ));
consider fi such that A10: (( dom fi ) = C & (for D holds (D in C implies ( fi . D ) = F(D)))) from CKB38:sch 1;
A11:now
let D;
assume D in ( rng fi );
then consider x such that A12: x in ( dom fi ) and A13: D = ( fi . x ) by FUNCT_1:def 3;
reconsider x as Ordinal by A12;
A14: ( B +^ x ) in ( B +^ C ) by A10,A12,CKB72:1;
(D = ( A +^ x ) & ( A +^ x ) c= ( B +^ x )) by A3,A10,A12,A13;
hence D in ( B +^ C ) by A14,ORDINAL1:12;
end;
C is  limit_ordinal by A9,ORDINAL1:29;
then ( A +^ C ) = ( sup fi ) by A8,A10,CKB68:1
.= ( sup ( rng fi ) );
hence thesis by A11,CKB25:1;
end;
now
assume A15: C = ( {} );
then ( A +^ C ) = A by CKB66:1;
hence thesis by A1,A15,CKB66:1;
end;
hence thesis by A4,A7;
end;
(for C holds P[ C ]) from ORDINAL1:sch 2(A2);
hence thesis;
end;
