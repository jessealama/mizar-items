scheme OSResultL { fi() -> Ordinal-Sequence,A() -> Ordinal,F(Ordinal) -> Ordinal,B() -> Ordinal,C(Ordinal,Ordinal) -> Ordinal,D(Ordinal,T-Sequence) -> Ordinal } : F(A()) = D(A(),fi())
provided
A1: (for A holds (for B holds (B = F(A) iff (ex fi st ((((B = ( last fi ) & ( dom fi ) = ( succ A )) & ( fi . ( {} ) ) = B()) & (for C holds (( succ C ) in ( succ A ) implies ( fi . ( succ C ) ) = C(C,( fi . C ))))) & (for C holds (((C in ( succ A ) & C <> ( {} )) & C is  limit_ordinal) implies ( fi . C ) = D(C,( fi | C )))))))))
and
A2: (A() <> ( {} ) & A() is  limit_ordinal)
and
A3: ( dom fi() ) = A()
and
A4: (for A holds (A in A() implies ( fi() . A ) = F(A)))
proof
A5: A() in ( succ A() ) by ORDINAL1:6;
consider fi such that A6: ( dom fi ) = ( succ A() ) and A7: (( {} ) in ( succ A() ) implies ( fi . ( {} ) ) = B()) and A8: (for C holds (( succ C ) in ( succ A() ) implies ( fi . ( succ C ) ) = C(C,( fi . C )))) and A9: (for C holds (((C in ( succ A() ) & C <> ( {} )) & C is  limit_ordinal) implies ( fi . C ) = D(C,( fi | C )))) from CKB46:sch 1;
set psi = ( fi | A() );
A10: (for A holds (for B holds (B = F(A) iff (ex fi st ((((B = ( last fi ) & ( dom fi ) = ( succ A )) & ( fi . ( {} ) ) = B()) & (for C holds (( succ C ) in ( succ A ) implies ( fi . ( succ C ) ) = C(C,( fi . C ))))) & (for C holds (((C in ( succ A ) & C <> ( {} )) & C is  limit_ordinal) implies ( fi . C ) = D(C,( fi | C ))))))))) by A1;
A11: (for B holds (B in ( dom fi ) implies ( fi . B ) = F(B))) from CKB47:sch 1(A10,A6,A7,A8,A9);
A12:now
let x;
assume A13: x in A();
then reconsider x9 = x as Ordinal;
thus ( psi . x ) = ( fi . x9 ) by A13,FUNCT_1:49
.= F(x9) by A6,A11,A5,A13,ORDINAL1:10
.= ( fi() . x ) by A4,A13;
end;
A() c= ( dom fi ) by A6,A5,ORDINAL1:def 2;
then ( dom psi ) = A() by RELAT_1:62;
then psi = fi() by A3,A12,FUNCT_1:2;
then ( fi . A() ) = D(A(),fi()) by A2,A9,ORDINAL1:6;
hence thesis by A6,A11,ORDINAL1:6;
