environ
vocabularies ORDINAL1,FUNCT_1,XBOOLE_0,TARSKI,ZFMISC_1,RELAT_1,SETFAM_1,SUBSET_1,FUNCOP_1,ORDINAL2;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,RELAT_1,FUNCT_1,ORDINAL1,SETFAM_1,FUNCOP_1;
definitions ORDINAL1,TARSKI,XBOOLE_0;
theorems TARSKI,XBOOLE_0,ORDINAL1,SETFAM_1,ZFMISC_1,FUNCT_1,RELAT_1,XBOOLE_1,FUNCOP_1,CKB2,CKB3,CKB4,CKB5;
schemes XBOOLE_0,FUNCT_1,ORDINAL1,CKB1;
registrations XBOOLE_0,SUBSET_1,FUNCT_1,ORDINAL1,FUNCOP_1;
constructors SETFAM_1,ORDINAL1,FUNCOP_1;
requirements SUBSET,BOOLE,NUMERALS;
begin
reserve A for Ordinal;
reserve Y for set;
reserve x for set;
theorem
Th5: ( union A ) c= A
proof
let x;
assume x in ( union A );
then consider Y such that A1: x in Y and A2: Y in A by TARSKI:def 4;
Y c= A by A2,ORDINAL1:def 2;
hence thesis by A1;
end;
