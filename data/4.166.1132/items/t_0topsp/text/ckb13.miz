begin
theorem
Th10: (for T being non empty TopSpace holds (for A being (Subset of T) holds (A is  open implies (for p,q being (Point of T) holds ((p in A & ( ( T_0-canonical_map T ) . p ) = ( ( T_0-canonical_map T ) . q )) implies q in A)))))
proof
let T being non empty TopSpace;
let A being (Subset of T);
assume that
A1: A is  open;
set F = ( T_0-canonical_map T );
let p being (Point of T),q being (Point of T);
assume that
A2: p in A
and
A3: ( F . p ) = ( F . q );
A4: ( F . p ) = ( Class (( Indiscernibility T ),p) ) by CKB11:1;
q in ( F . p ) by A3,BORSUK_1:28;
then [ q,p ] in ( Indiscernibility T ) by A4,EQREL_1:19;
hence thesis by A1,A2,CKB4:def 1;
end;
