begin
theorem
Th11: (for T being non empty TopSpace holds (for A being (Subset of T) holds (A is  open implies (for C being (Subset of T) holds ((C in ( Indiscernible T ) & C meets A) implies C c= A)))))
proof
let T being non empty TopSpace;
let A being (Subset of T);
assume that
A1: A is  open;
set R = ( Indiscernibility T );
let C being (Subset of T);
assume that
A2: C in ( Indiscernible T )
and
A3: C meets A;
consider y being set such that A4: y in C and A5: y in A by A3,XBOOLE_0:3;
consider x being set such that x in (the carrier of T) and A6: C = ( Class (R,x) ) by A2,EQREL_1:def 3;
(for p being set holds (p in C implies p in A))
proof
let p being set;
[ y,x ] in R by A6,A4,EQREL_1:19;
then A7: [ x,y ] in R by EQREL_1:6;
assume A8: p in C;
then [ p,x ] in R by A6,EQREL_1:19;
then [ p,y ] in R by A7,EQREL_1:7;
hence thesis by A1,A5,A8,CKB4:def 1;
end;
hence thesis by TARSKI:def 3;
end;
