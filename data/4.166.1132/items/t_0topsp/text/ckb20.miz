begin
theorem
(for T being non  empty TopSpace holds ( T_0-reflex T ) is T_0-TopSpace)
proof
let T being non  empty TopSpace;
(for x,y being (Point of ( T_0-reflex T )) holds ((not x = y) implies (ex A being (Subset of ( T_0-reflex T )) st (A is  open & ((x in A & (not y in A)) or (y in A & (not x in A))))))) by CKB16:1;
hence thesis by CKB17:def 1;
end;
