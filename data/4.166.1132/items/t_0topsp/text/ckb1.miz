begin
theorem
Th2: (for X,Y being non  empty set holds (for f being (Function of X,Y) holds (for A being (Subset of X) holds ((for x1,x2 being (Element of X) holds ((x1 in A & ( f . x1 ) = ( f . x2 )) implies x2 in A)) implies ( f " ( f .: A ) ) = A))))
proof
let X being non  empty set;
let Y being non  empty set;
let f being (Function of X,Y);
let A being (Subset of X);
assume A1: (for x1,x2 being (Element of X) holds ((x1 in A & ( f . x1 ) = ( f . x2 )) implies x2 in A));
(for x being set holds (x in ( f " ( f .: A ) ) implies x in A))
proof
let x being set;
assume A2: x in ( f " ( f .: A ) );
then ( f . x ) in ( f .: A ) by FUNCT_1:def 7;
then (ex x0 being set st ((x0 in X & x0 in A) & ( f . x ) = ( f . x0 ))) by FUNCT_2:64;
hence thesis by A1,A2;
end;
then (A c= ( f " ( f .: A ) ) & ( f " ( f .: A ) ) c= A) by FUNCT_2:42,TARSKI:def 3;
hence thesis by XBOOLE_0:def 10;
end;
