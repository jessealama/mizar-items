begin
theorem
Th12: (for T being non  empty TopSpace holds ( T_0-canonical_map T ) is  open)
proof
let T being non  empty TopSpace;
set F = ( T_0-canonical_map T );
(for A being (Subset of T) holds (A is  open implies ( F .: A ) is  open))
proof
set D = ( Indiscernible T );
A1: F = ( proj D ) by BORSUK_1:def 8;
let A being (Subset of T);
assume that
A2: A is  open;
A3: (for C being (Subset of T) holds ((C in D & C meets A) implies C c= A)) by A2,CKB14:1;
set A9 = ( F .: A );
A9 is (Subset of D) by BORSUK_1:def 7;
then ( F " A9 ) = ( union A9 ) by A1,EQREL_1:67;
then A = ( union A9 ) by A1,A3,EQREL_1:69;
then ( union A9 ) in (the topology of T) by A2,PRE_TOPC:def 2;
hence thesis by CKB9:1;
end;
hence thesis by CKB3:def 1;
end;
