environ
vocabularies SUBSET_1,FINSUB_1,PARTFUN1,FINSET_1,TARSKI,XBOOLE_0,NORMFORM,FUNCT_1,ORDINAL4,FUNCT_4,STRUCT_0,LATTICES,BINOP_1,EQREL_1,XXREAL_2,SUBSTLAT;
notations TARSKI,XBOOLE_0,SUBSET_1,FINSET_1,FINSUB_1,BINOP_1,STRUCT_0,FUNCT_1,PARTFUN1,LATTICES,FUNCT_4,CKB1,CKB6,CKB9,CKB11;
definitions TARSKI,LATTICES,XBOOLE_0,CKB1,CKB9,CKB11;
theorems ZFMISC_1,TARSKI,FINSUB_1,FINSET_1,FUNCT_4,PARTFUN1,BINOP_1,LATTICES,LATTICE2,XBOOLE_0,XBOOLE_1,RELSET_1,CKB1,CKB2,CKB3,CKB4,CKB9,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35;
schemes FRAENKEL,BINOP_1;
registrations XBOOLE_0,RELSET_1,PARTFUN1,FINSET_1,FINSUB_1,LATTICES,FUNCT_1,CKB5,CKB7,CKB8,CKB10,CKB17;
constructors PARTFUN1,BINOP_1,FUNCT_4,FINSET_1,FINSUB_1,LATTICES,RELSET_1,CKB1,CKB6,CKB9,CKB11;
requirements SUBSET,BOOLE;
begin
reserve V for set;
reserve C for set;
theorem
Lm5: (for K,L being (Element of ( Fin ( PFuncs (V,C) ) )) holds ( mi ( ( K ^ L ) \/ L ) ) = ( mi L ))
proof
let K being (Element of ( Fin ( PFuncs (V,C) ) )),L being (Element of ( Fin ( PFuncs (V,C) ) ));
now
let a being set;
assume A1: a in ( mi ( ( K ^ L ) \/ L ) );
then a in ( ( K ^ L ) \/ L ) by CKB15:1;
then A2: (a in ( K ^ L ) or a in L) by XBOOLE_0:def 3;
A3:now
let b being  finite set;
assume b in L;
then b in ( ( K ^ L ) \/ L ) by XBOOLE_0:def 3;
hence (b c= a implies b = a) by A1,CKB15:1;
end;
A4:now
assume a in ( K ^ L );
then consider b being set such that A5: b in L and A6: b c= a by CKB35:1;
b in ( ( K ^ L ) \/ L ) by A5,XBOOLE_0:def 3;
hence a in L by A1,A5,A6,CKB15:1;
end;
a is  finite by A1,CKB2:1;
hence a in ( mi L ) by A2,A4,A3,CKB18:1;
end;
hence ( mi ( ( K ^ L ) \/ L ) ) c= ( mi L ) by CKB16:1;
now
let a being set;
assume A7: a in ( mi L );
then A8: a in L by CKB15:1;
then A9: a in ( ( K ^ L ) \/ L ) by XBOOLE_0:def 3;
A10:now
let b being  finite set;
assume b in ( ( K ^ L ) \/ L );
then A11: (b in ( K ^ L ) or b in L) by XBOOLE_0:def 3;
assume A12: b c= a;
now
assume b in ( K ^ L );
then consider c being set such that A13: c in L and A14: c c= b by CKB35:1;
c c= a by A12,A14,XBOOLE_1:1;
then c = a by A7,A13,CKB15:1;
hence b in L by A8,A12,A14,XBOOLE_0:def 10;
end;
hence b = a by A7,A11,A12,CKB15:1;
end;
a is  finite by A7,CKB2:1;
hence a in ( mi ( ( K ^ L ) \/ L ) ) by A9,A10,CKB18:1;
end;
hence thesis by CKB16:1;
end;
