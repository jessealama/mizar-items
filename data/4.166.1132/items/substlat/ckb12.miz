environ
vocabularies SUBSET_1,FINSUB_1,PARTFUN1,FINSET_1,TARSKI,XBOOLE_0,NORMFORM,FUNCT_1,ORDINAL4,FUNCT_4,STRUCT_0,LATTICES,BINOP_1,EQREL_1,XXREAL_2,SUBSTLAT;
notations TARSKI,XBOOLE_0,SUBSET_1,FINSET_1,FINSUB_1,BINOP_1,STRUCT_0,FUNCT_1,PARTFUN1,LATTICES,FUNCT_4,CKB1,CKB6,CKB9,CKB11;
definitions TARSKI,LATTICES,XBOOLE_0,CKB1,CKB9,CKB11;
theorems ZFMISC_1,TARSKI,FINSUB_1,FINSET_1,FUNCT_4,PARTFUN1,BINOP_1,LATTICES,LATTICE2,XBOOLE_0,XBOOLE_1,RELSET_1,CKB1,CKB2,CKB3,CKB4,CKB9,CKB11;
schemes FRAENKEL,BINOP_1;
registrations XBOOLE_0,RELSET_1,PARTFUN1,FINSET_1,FINSUB_1,LATTICES,FUNCT_1,CKB5,CKB7,CKB8,CKB10;
constructors PARTFUN1,BINOP_1,FUNCT_4,FINSET_1,FINSUB_1,LATTICES,RELSET_1,CKB1,CKB6,CKB9,CKB11;
requirements SUBSET,BOOLE;
begin
reserve V for set;
reserve C for set;
reserve A for (Element of ( Fin ( PFuncs (V,C) ) ));
reserve B for (Element of ( Fin ( PFuncs (V,C) ) ));
theorem
Th3: ( A ^ B ) = ( B ^ A )
proof
deffunc U((Element of ( PFuncs (V,C) )),(Element of ( PFuncs (V,C) ))) = ( $1 \/ $2 );
defpred X[ Function,Function ]
 means
(($1 in A & $2 in B) & $1 tolerates $2);
defpred Y[ Function,Function ]
 means
(($2 in B & $1 in A) & $2 tolerates $1);
set X1 = { U(s,t) where s is (Element of ( PFuncs (V,C) )),t is (Element of ( PFuncs (V,C) )): X[ s,t ] };
set X2 = { U(t,s) where s is (Element of ( PFuncs (V,C) )),t is (Element of ( PFuncs (V,C) )): Y[ s,t ] };
A1:now
let x being set;
(x in X2 iff (ex s,t being (Element of ( PFuncs (V,C) )) st (x = U(t,s) & Y[ s,t ])));
then (x in X2 iff (ex t,s being (Element of ( PFuncs (V,C) )) st (((x = ( t \/ s ) & t in B) & s in A) & t tolerates s)));
hence (x in X2 iff x in { ( t \/ s ) where t is (Element of ( PFuncs (V,C) )),s is (Element of ( PFuncs (V,C) )): ((t in B & s in A) & t tolerates s) });
end;
A2: (for s,t being (Element of ( PFuncs (V,C) )) holds U(s,t) = U(t,s));
A3: (for s,t being (Element of ( PFuncs (V,C) )) holds (X[ s,t ] iff Y[ s,t ]));
X1 = X2 from FRAENKEL:sch 8(A3,A2);
hence thesis by A1,TARSKI:1;
end;
