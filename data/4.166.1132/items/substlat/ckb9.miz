environ
vocabularies SUBSET_1,FINSUB_1,PARTFUN1,FINSET_1,TARSKI,XBOOLE_0,NORMFORM,FUNCT_1,ORDINAL4,FUNCT_4,STRUCT_0,LATTICES,BINOP_1,EQREL_1,XXREAL_2,SUBSTLAT;
notations TARSKI,XBOOLE_0,SUBSET_1,FINSET_1,FINSUB_1,BINOP_1,STRUCT_0,FUNCT_1,PARTFUN1,LATTICES,FUNCT_4,CKB1,CKB6;
definitions TARSKI,LATTICES,XBOOLE_0,CKB1;
theorems ZFMISC_1,TARSKI,FINSUB_1,FINSET_1,FUNCT_4,PARTFUN1,BINOP_1,LATTICES,LATTICE2,XBOOLE_0,XBOOLE_1,RELSET_1,CKB1,CKB2,CKB3,CKB4;
schemes FRAENKEL,BINOP_1;
registrations XBOOLE_0,RELSET_1,PARTFUN1,FINSET_1,FINSUB_1,LATTICES,FUNCT_1,CKB5,CKB7,CKB8;
constructors PARTFUN1,BINOP_1,FUNCT_4,FINSET_1,FINSUB_1,LATTICES,RELSET_1,CKB1,CKB6;
requirements SUBSET,BOOLE;
begin
reserve V for set;
reserve C for set;
definition
let V;
let C;
let A being (Element of ( Fin ( PFuncs (V,C) ) ));
func mi A -> (Element of ( SubstitutionSet (V,C) ))
equals
{ t where t is (Element of ( PFuncs (V,C) )): (t is  finite & (for s being (Element of ( PFuncs (V,C) )) holds ((s in A & s c= t) iff s = t))) };
coherence
proof
set M = { t where t is (Element of ( PFuncs (V,C) )): (t is  finite & (for s being (Element of ( PFuncs (V,C) )) holds ((s in A & s c= t) iff s = t))) };
A1: M c= ( PFuncs (V,C) )
proof
let a being set;
assume a in M;
then (ex t9 being (Element of ( PFuncs (V,C) )) st ((a = t9 & t9 is  finite) & (for s being (Element of ( PFuncs (V,C) )) holds ((s in A & s c= t9) iff s = t9))));
hence thesis;
end;
A2:now
let x being set;
assume x in M;
then (ex t being (Element of ( PFuncs (V,C) )) st ((x = t & t is  finite) & (for s being (Element of ( PFuncs (V,C) )) holds ((s in A & s c= t) iff s = t))));
hence x in A;
end;
then A3: M c= A by TARSKI:def 3;
reconsider M as set;
reconsider M9 = M as (Element of ( Fin ( PFuncs (V,C) ) )) by A1,A3,FINSUB_1:def 5;
A4: (for u being set holds (u in M9 implies u is  finite))
proof
let u being set;
assume u in M9;
then (ex t9 being (Element of ( PFuncs (V,C) )) st ((u = t9 & t9 is  finite) & (for s being (Element of ( PFuncs (V,C) )) holds ((s in A & s c= t9) iff s = t9))));
hence thesis;
end;
(for s,t being (Element of ( PFuncs (V,C) )) holds (((s in M9 & t in M9) & s c= t) implies s = t))
proof
let s being (Element of ( PFuncs (V,C) )),t being (Element of ( PFuncs (V,C) ));
assume that
A5: (s in M9 & t in M9)
and
A6: s c= t;
(s in A & (ex tt being (Element of ( PFuncs (V,C) )) st ((t = tt & tt is  finite) & (for ss being (Element of ( PFuncs (V,C) )) holds ((ss in A & ss c= tt) iff ss = tt))))) by A2,A5;
hence thesis by A6;
end;
then M in { A1 where A1 is (Element of ( Fin ( PFuncs (V,C) ) )): ((for u being set holds (u in A1 implies u is  finite)) & (for s,t being (Element of ( PFuncs (V,C) )) holds (((s in A1 & t in A1) & s c= t) implies s = t))) } by A4;
hence thesis;
end;
end;
