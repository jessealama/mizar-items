begin
reserve V for set;
reserve C for set;
theorem
Lm1: (for a,b being set holds ((b in ( SubstitutionSet (V,C) ) & a in b) implies a is  finite))
proof
let a being set;
let b being set;
assume that
A1: b in ( SubstitutionSet (V,C) )
and
A2: a in b;
(ex A being (Element of ( Fin ( PFuncs (V,C) ) )) st ((A = b & (for u being set holds (u in A implies u is  finite))) & (for s,t being (Element of ( PFuncs (V,C) )) holds (((s in A & t in A) & s c= t) implies s = t)))) by A1;
hence thesis by A2;
end;
