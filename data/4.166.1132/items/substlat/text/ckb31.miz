environ
vocabularies SUBSET_1,FINSUB_1,PARTFUN1,FINSET_1,TARSKI,XBOOLE_0,NORMFORM,FUNCT_1,ORDINAL4,FUNCT_4,STRUCT_0,LATTICES,BINOP_1,EQREL_1,XXREAL_2,SUBSTLAT;
notations TARSKI,XBOOLE_0,SUBSET_1,FINSET_1,FINSUB_1,BINOP_1,STRUCT_0,FUNCT_1,PARTFUN1,LATTICES,FUNCT_4,CKB1,CKB6,CKB9,CKB11;
definitions TARSKI,LATTICES,XBOOLE_0,CKB1,CKB9,CKB11;
theorems ZFMISC_1,TARSKI,FINSUB_1,FINSET_1,FUNCT_4,PARTFUN1,BINOP_1,LATTICES,LATTICE2,XBOOLE_0,XBOOLE_1,RELSET_1,CKB1,CKB2,CKB3,CKB4,CKB9,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30;
schemes FRAENKEL,BINOP_1;
registrations XBOOLE_0,RELSET_1,PARTFUN1,FINSET_1,FINSUB_1,LATTICES,FUNCT_1,CKB5,CKB7,CKB8,CKB10,CKB17;
constructors PARTFUN1,BINOP_1,FUNCT_4,FINSET_1,FINSUB_1,LATTICES,RELSET_1,CKB1,CKB6,CKB9,CKB11;
requirements SUBSET,BOOLE;
begin
reserve V for set;
reserve C for set;
reserve A for (Element of ( Fin ( PFuncs (V,C) ) ));
reserve B for (Element of ( Fin ( PFuncs (V,C) ) ));
theorem
Th19: ( mi ( ( mi A ) ^ B ) ) = ( mi ( A ^ B ) )
proof
A1: ( ( mi A ) ^ B ) c= ( A ^ B ) by CKB19:1,CKB25:1;
now
let a being set;
assume A2: a in ( mi ( ( mi A ) ^ B ) );
A3:now
let b being  finite set;
assume b in ( A ^ B );
then consider c being  finite set such that A4: c c= b and A5: c in ( ( mi A ) ^ B ) by CKB28:1;
assume A6: b c= a;
then c c= a by A4,XBOOLE_1:1;
then c = a by A2,A5,CKB15:1;
hence b = a by A4,A6,XBOOLE_0:def 10;
end;
(a in ( ( mi A ) ^ B ) & a is  finite) by A2,CKB2:1,CKB15:1;
hence a in ( mi ( A ^ B ) ) by A1,A3,CKB18:1;
end;
hence ( mi ( ( mi A ) ^ B ) ) c= ( mi ( A ^ B ) ) by CKB16:1;
A7: ( mi ( A ^ B ) ) c= ( ( mi A ) ^ B ) by CKB29:1;
now
let a being set;
assume A8: a in ( mi ( A ^ B ) );
then (a is  finite & (for b being  finite set holds (b in ( ( mi A ) ^ B ) implies (b c= a implies b = a)))) by A1,CKB2:1,CKB15:1;
hence a in ( mi ( ( mi A ) ^ B ) ) by A7,A8,CKB18:1;
end;
hence thesis by CKB16:1;
end;
