begin
theorem
Th20: (for K being  Abelian non empty addLoopStr holds (for V being non empty VectSpStr over K holds (for f,g being (Functional of V) holds ( f + g ) = ( g + f ))))
proof
let K being  Abelian non empty addLoopStr;
let V being non empty VectSpStr over K;
let f being (Functional of V),g being (Functional of V);
now
let x being (Element of V);
thus ( ( f + g ) . x ) = ( ( f . x ) + ( g . x ) ) by CKB16:def 1
.= ( ( g + f ) . x ) by CKB16:def 1;
end;
hence thesis by FUNCT_2:63;
end;
