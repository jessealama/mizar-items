begin
reserve x for set;
theorem
(for T being non empty TopSpace holds (for A being (Subset of T) holds (A is  closed iff (for F being  proper (Filter of ( BoolePoset ( [#] T ) )) holds (A in F implies (for x being (Point of T) holds (x is_a_cluster_point_of F,T implies x in A)))))))
proof
let T being non empty TopSpace;
let A being (Subset of T);
(A is  closed iff ( Cl A ) = A) by PRE_TOPC:22;
hence (A is  closed implies (for F being  proper (Filter of ( BoolePoset ( [#] T ) )) holds (A in F implies (for x being (Point of T) holds (x is_a_cluster_point_of F,T implies x in A))))) by CKB35:1;
assume A1: (for F being  proper (Filter of ( BoolePoset ( [#] T ) )) holds (A in F implies (for x being (Point of T) holds (x is_a_cluster_point_of F,T implies x in A))));
A = ( Cl A )
proof
thus A c= ( Cl A ) by PRE_TOPC:18;
let x;
assume A2: x in ( Cl A );
then reconsider y = x as (Element of T);
(ex F being  proper (Filter of ( BoolePoset ( [#] T ) )) st (A in F & y is_a_cluster_point_of F,T)) by A2,CKB35:1;
hence thesis by A1;
end;
hence thesis;
end;
