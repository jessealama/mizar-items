theorem
Th21: (for T being non  empty 1-sorted holds (for A being set holds (for N being (net of T) holds (N is_eventually_in A implies (for S being (subnet of N) holds S is_eventually_in A)))))
proof
let T being non  empty 1-sorted;
let A being set;
let N being (net of T);
given i being (Element of N) such that
A1: (for j being (Element of N) holds (i <= j implies ( N . j ) in A));

let S being (subnet of N);
consider f being (Function of S,N) such that A2: (the mapping of S) = ( (the mapping of N) * f ) and A3: (for m being (Element of N) holds (ex n being (Element of S) st (for p being (Element of S) holds (n <= p implies m <= ( f . p ))))) by YELLOW_6:def 9;
consider n being (Element of S) such that A4: (for p being (Element of S) holds (n <= p implies i <= ( f . p ))) by A3;
take n;
let p being (Element of S);
assume n <= p;
then ( N . ( f . p ) ) in A by A1,A4;
hence thesis by A2,FUNCT_2:15;
end;
