begin
reserve x for set;
theorem
(for T being non  empty TopSpace holds (T is  compact iff (for F being  proper (Filter of ( BoolePoset ( [#] T ) )) holds (ex x being (Point of T) st x is_a_cluster_point_of F,T))))
proof
let T being non  empty TopSpace;
hereby
assume A1: T is  compact;
let F being  proper (Filter of ( BoolePoset ( [#] T ) ));
reconsider G = F as  proper (Filter of ( BoolePoset ( [#] T ) ));
consider x being (Point of T) such that A2: x is_a_cluster_point_of ( a_net G ) by A1,CKB41:1;
take x;
thus x is_a_cluster_point_of F,T by A2,CKB26:1;
end;
assume A3: (for F being  proper (Filter of ( BoolePoset ( [#] T ) )) holds (ex x being (Point of T) st x is_a_cluster_point_of F,T));
now
let N being (net of T);
assume that
N in ( NetUniv T );
reconsider F = ( a_filter N ) as  proper (Filter of ( BoolePoset (the carrier of T) ));
consider x being (Point of T) such that A4: x is_a_cluster_point_of F,T by A3;
take x;
thus x is_a_cluster_point_of N by A4,CKB18:1;
end;
hence thesis by CKB42:1;
end;
