begin
reserve V for RealLinearSpace;
reserve W1 for (Subspace of V);
reserve W2 for (Subspace of V);
reserve W3 for (Subspace of V);
reserve v for (VECTOR of V);
theorem
Lm4: ((W1 is (Subspace of W2) & W1 is (Subspace of W3)) implies W1 is (Subspace of ( W2 /\ W3 )))
proof
assume A1: (W1 is (Subspace of W2) & W1 is (Subspace of W3));
now
let v;
assume v in W1;
then (v in W2 & v in W3) by A1,RLSUB_1:8;
hence v in ( W2 /\ W3 ) by RLSUB_2:3;
end;
hence thesis by RLSUB_1:29;
end;
