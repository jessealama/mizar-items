begin
reserve y for set;
theorem
Th18: (for N being Pnet holds (for x being (Element of ( Elements N )) holds ( Post (N,x) ) c= ( Elements N )))
proof
let N being Pnet;
let x being (Element of ( Elements N ));
(for y holds (y in ( Post (N,x) ) implies y in ( Elements N ))) by CKB20:def 1;
hence thesis by TARSKI:def 3;
end;
