begin
reserve n for (Element of ( NAT ));
reserve r for Real;
reserve S for RealNormSpace;
reserve T for RealNormSpace;
theorem
Th20: (for h being (PartFunc of S,T) holds (for seq being (sequence of S) holds (for r being Real holds (( rng seq ) c= ( dom h ) implies ( ( r (#) h ) /* seq ) = ( r * ( h /* seq ) )))))
proof
let h being (PartFunc of S,T);
let seq being (sequence of S);
let r being Real;
assume A1: ( rng seq ) c= ( dom h );
then A2: ( rng seq ) c= ( dom ( r (#) h ) ) by VFUNCT_1:def 4;
now
let n;
A3: ( seq . n ) in ( dom ( r (#) h ) ) by A2,CKB11:1;
thus ( ( ( r (#) h ) /* seq ) . n ) = ( ( r (#) h ) /. ( seq . n ) ) by A2,FUNCT_2:109
.= ( r * ( h /. ( seq . n ) ) ) by A3,VFUNCT_1:def 4
.= ( r * ( ( h /* seq ) . n ) ) by A1,FUNCT_2:109;
end;
hence thesis by NORMSP_1:def 5;
end;
