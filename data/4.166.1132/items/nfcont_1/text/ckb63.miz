begin
reserve S for RealNormSpace;
reserve x0 for (Point of S);
reserve x1 for (Point of S);
reserve x2 for (Point of S);
theorem
Th60: (for f being (PartFunc of (the carrier of S),( REAL )) holds ((for x0 holds (x0 in ( dom f ) implies ( f /. x0 ) = ||. x0 .||)) implies f is_continuous_on ( dom f )))
proof
let f being (PartFunc of (the carrier of S),( REAL ));
assume A1: (for x0 holds (x0 in ( dom f ) implies ( f /. x0 ) = ||. x0 .||));
now
let x1;
let x2;
assume (x1 in ( dom f ) & x2 in ( dom f ));
then (( f /. x1 ) = ||. x1 .|| & ( f /. x2 ) = ||. x2 .||) by A1;
hence ( abs ( ( f /. x1 ) - ( f /. x2 ) ) ) <= ( 1 * ||. ( x1 - x2 ) .|| ) by NORMSP_1:9;
end;
then f is_Lipschitzian_on ( dom f ) by CKB47:def 1;
hence thesis by CKB56:1;
end;
