begin
reserve X for set;
reserve r for Real;
reserve S for RealNormSpace;
reserve T for RealNormSpace;
reserve f for (PartFunc of S,T);
theorem
Th29: (for f being (PartFunc of (the carrier of S),( REAL )) holds (f is_continuous_on X iff ( f | X ) is_continuous_on X))
proof
let f being (PartFunc of (the carrier of S),( REAL ));
thus (f is_continuous_on X implies ( f | X ) is_continuous_on X)
proof
assume A1: f is_continuous_on X;
then X c= ( dom f ) by CKB25:def 1;
then X c= ( ( dom f ) /\ X ) by XBOOLE_1:28;
hence X c= ( dom ( f | X ) ) by RELAT_1:61;
let r being (Point of S);
assume r in X;
then ( f | X ) is_continuous_in r by A1,CKB25:def 1;
hence thesis by RELAT_1:72;
end;

assume A2: ( f | X ) is_continuous_on X;
then X c= ( dom ( f | X ) ) by CKB25:def 1;
then (( ( dom f ) /\ X ) c= ( dom f ) & X c= ( ( dom f ) /\ X )) by RELAT_1:61,XBOOLE_1:17;
hence X c= ( dom f ) by XBOOLE_1:1;
let r being (Point of S);
assume r in X;
then ( ( f | X ) | X ) is_continuous_in r by A2,CKB25:def 1;
hence thesis by RELAT_1:72;
end;
