notations TARSKI,XBOOLE_0,SUBSET_1,RELAT_1,FUNCT_1,RELSET_1,PARTFUN1,PARTFUN2,FUNCT_2,PRE_TOPC,VALUED_0,STRUCT_0,ALGSTR_0,ORDINAL1,NUMBERS,COMPLEX1,REAL_1,XREAL_0,RLVECT_1,SEQ_1,SEQ_2,SEQ_4,RCOMP_1,VFUNCT_1,NORMSP_0,NORMSP_1,BHSP_1,XXREAL_0,CKB3,CKB6,CKB7,CKB8,CKB9,CKB10,CKB24,CKB25;
definitions TARSKI,SEQM_3,BHSP_3,RLVECT_1,RELAT_1,VALUED_0,CKB3,CKB6,CKB7,CKB8,CKB9,CKB10,CKB24,CKB25;
theorems TARSKI,XBOOLE_0,XBOOLE_1,RLVECT_1,XCMPLX_0,XCMPLX_1,ZFMISC_1,RCOMP_1,XREAL_0,SEQ_2,NAT_1,RELAT_1,FUNCT_1,VFUNCT_1,LOPBAN_1,FUNCT_2,SEQ_4,NORMSP_1,PARTFUN1,PARTFUN2,XREAL_1,COMPLEX1,XXREAL_0,BHSP_1,VALUED_0,NORMSP_0,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35;
