theorem
(x0 in ( dom f ) implies f is_continuous_on { x0 })
proof
assume A1: x0 in ( dom f );
thus { x0 } c= ( dom f )
proof
let x;
assume x in { x0 };
hence thesis by A1,TARSKI:def 1;
end;

let p being (Point of S);
assume that
A2: p in { x0 };
thus ( f | { x0 } ) is_continuous_in p
proof
p in ( dom f ) by A1,A2,TARSKI:def 1;
then p in ( ( dom f ) /\ { x0 } ) by A2,XBOOLE_0:def 4;
hence p in ( dom ( f | { x0 } ) ) by RELAT_1:61;
let s1;
assume that
A3: ( rng s1 ) c= ( dom ( f | { x0 } ) )
and
s1 is  convergent
and
( lim s1 ) = p;
A4: ( ( dom f ) /\ { x0 } ) c= { x0 } by XBOOLE_1:17;
( rng s1 ) c= ( ( dom f ) /\ { x0 } ) by A3,RELAT_1:61;
then A5: ( rng s1 ) c= { x0 } by A4,XBOOLE_1:1;
A6:now
let n;
( dom s1 ) = ( NAT ) by FUNCT_2:def 1;
then ( s1 . n ) in ( rng s1 ) by FUNCT_1:3;
hence ( s1 . n ) = x0 by A5,TARSKI:def 1;
end;
A7: p = x0 by A2,TARSKI:def 1;
A8:now
let g being Real;
assume that
A9: ( 0 ) < g;
take n = ( 0 );
let m;
assume that
n <= m;
||. ( ( ( ( f | { x0 } ) /* s1 ) . m ) - ( ( f | { x0 } ) /. p ) ) .|| = ||. ( ( ( f | { x0 } ) /. ( s1 . m ) ) - ( ( f | { x0 } ) /. x0 ) ) .|| by A7,A3,FUNCT_2:109
.= ||. ( ( ( f | { x0 } ) /. x0 ) - ( ( f | { x0 } ) /. x0 ) ) .|| by A6
.= ||. ( 0. T ) .|| by RLVECT_1:15
.= ( 0 ) by NORMSP_1:1;
hence ||. ( ( ( ( f | { x0 } ) /* s1 ) . m ) - ( ( f | { x0 } ) /. p ) ) .|| < g by A9;
end;
hence ( ( f | { x0 } ) /* s1 ) is  convergent by NORMSP_1:def 6;
hence thesis by A8,NORMSP_1:def 7;
end;

