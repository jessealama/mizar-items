theorem
Th53: (for f being (PartFunc of (the carrier of S),( REAL )) holds (f is_Lipschitzian_on X implies f is_continuous_on X))
proof
let f being (PartFunc of (the carrier of S),( REAL ));
assume A1: f is_Lipschitzian_on X;
then consider r such that A2: ( 0 ) < r and A3: (for x1 holds (for x2 holds ((x1 in X & x2 in X) implies ( abs ( ( f /. x1 ) - ( f /. x2 ) ) ) <= ( r * ||. ( x1 - x2 ) .|| )))) by CKB47:def 1;
A4: X c= ( dom f ) by A1,CKB47:def 1;
then A5: ( dom ( f | X ) ) = X by RELAT_1:62;
now
let x0;
assume that
A6: x0 in X;
now
let g;
assume that
A7: ( 0 ) < g;
set s = ( g / r );
take s9 = s;
A8:now
let x1;
assume that
A9: x1 in ( dom ( f | X ) )
and
A10: ||. ( x1 - x0 ) .|| < s;
( r * ||. ( x1 - x0 ) .|| ) < ( ( g / r ) * r ) by A2,A10,XREAL_1:68;
then A11: ( r * ||. ( x1 - x0 ) .|| ) < g by A2,XCMPLX_1:87;
( abs ( ( f /. x1 ) - ( f /. x0 ) ) ) <= ( r * ||. ( x1 - x0 ) .|| ) by A3,A5,A6,A9;
then ( abs ( ( f /. x1 ) - ( f /. x0 ) ) ) < g by A11,XXREAL_0:2;
then ( abs ( ( ( f | X ) /. x1 ) - ( f /. x0 ) ) ) < g by A9,PARTFUN2:15;
hence ( abs ( ( ( f | X ) /. x1 ) - ( ( f | X ) /. x0 ) ) ) < g by A5,A6,PARTFUN2:15;
end;
(( 0 ) < ( r " ) & s9 = ( g * ( r " ) )) by A2,XCMPLX_0:def 9,XREAL_1:122;
hence (( 0 ) < s9 & (for x1 holds ((x1 in ( dom ( f | X ) ) & ||. ( x1 - x0 ) .|| < s9) implies ( abs ( ( ( f | X ) /. x1 ) - ( ( f | X ) /. x0 ) ) ) < g))) by A7,A8,XREAL_1:129;
end;
hence ( f | X ) is_continuous_in x0 by A5,A6,CKB14:1;
end;
hence thesis by A4,CKB25:def 1;
end;
