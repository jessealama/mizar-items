environ
vocabularies NUMBERS,XBOOLE_0,PROB_1,MEASURE1,PARTFUN1,SUBSET_1,RELAT_1,FUNCT_1,XXREAL_0,ARYTM_1,SUPINF_2,SUPINF_1,CARD_1,REAL_1,VALUED_1,MEASURE6,INTEGRA5,MESFUNC5,ARYTM_3,MESFUNC1,COMPLEX1,TARSKI,MESFUNC2,VALUED_0,FINSEQ_1,BINOP_1,SETWISEO,CARD_3,ORDINAL1,FINSEQ_2,NEWTON,ORDINAL4,NAT_1,XREAL_0,POWER,SQUARE_1,XXREAL_2,ORDINAL2,RFUNCT_3,FUNCT_3,MESFUNC7;
notations TARSKI,XBOOLE_0,SUBSET_1,ORDINAL1,NUMBERS,VALUED_0,XREAL_0,COMPLEX1,REAL_1,XXREAL_0,XXREAL_2,MEASURE1,NAT_1,SUPINF_1,RELAT_1,RELSET_1,SETWISEO,PARTFUN1,FINSEQ_1,FINSEQ_2,SETWOP_2,BINOP_1,FUNCT_2,NEWTON,SQUARE_1,PROB_1,SUPINF_2,EXTREAL1,POWER,MESFUNC5,MESFUNC1,MESFUNC2,MEASURE6,CKB7,CKB9,CKB14,CKB16,CKB17,CKB26;
definitions TARSKI,MESFUNC1,MEASURE6,MESFUNC5,XCMPLX_0,SQUARE_1,CKB9,CKB14,CKB16,CKB26;
theorems ABSVALUE,SETWISEO,POWER,HOLDER_1,TARSKI,PARTFUN1,FUNCT_1,SUPINF_2,EXTREAL1,EXTREAL2,BINOP_1,MESFUNC1,FINSEQ_1,XREAL_0,XBOOLE_0,XBOOLE_1,XCMPLX_1,MESFUNC2,NEWTON,XREAL_1,COMPLEX1,XXREAL_0,FINSEQ_2,MESFUNC5,FINSOP_1,NAT_1,RELAT_1,FUNCT_3,MEASURE1,MESFUNC6,ORDINAL1,VALUED_0,XXREAL_2,XXREAL_3,CKB1,CKB2,CKB3,CKB6,CKB9,CKB11,CKB12,CKB14,CKB16,CKB20,CKB21,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB37,CKB38,CKB39,CKB40,CKB41;
schemes FUNCT_2,NAT_1,PARTFUN2,BINOP_1,BINOP_2;
registrations SUBSET_1,NAT_1,RELSET_1,XREAL_0,MEMBERED,ORDINAL1,FINSEQ_1,MEASURE1,NUMBERS,XXREAL_0,XBOOLE_0,VALUED_0,POWER,XXREAL_3,CKB4,CKB5,CKB8,CKB10,CKB13,CKB15,CKB18,CKB19,CKB22;
constructors REAL_1,FINSOP_1,EXTREAL1,BINOP_1,NEWTON,POWER,MESFUNC1,MEASURE6,MESFUNC2,MEASURE3,SETWISEO,EXTREAL2,SQUARE_1,MESFUNC5,SUPINF_1,RELSET_1,BINOP_2,CKB9,CKB14,CKB16,CKB17,CKB26;
requirements NUMERALS,REAL,BOOLE,SUBSET,ARITHM;
begin
reserve X for non empty set;
reserve S for (SigmaField of X);
reserve M for (sigma_Measure of S);
reserve f for (PartFunc of X,( ExtREAL ));
reserve E for (Element of S);
reserve a for  real number;
reserve b for  real number;
theorem
((((f is_integrable_on M & E c= ( dom f )) & ( M . E ) < ( +infty )) & (for x being (Element of X) holds (x in E implies (a <= ( f . x ) & ( f . x ) <= b)))) implies (( ( R_EAL a ) * ( M . E ) ) <= ( Integral (M,( f | E )) ) & ( Integral (M,( f | E )) ) <= ( ( R_EAL b ) * ( M . E ) )))
proof
reconsider a1 = a,b1 = b as (Element of ( REAL )) by XREAL_0:def 1;
assume that
A1: f is_integrable_on M
and
A2: E c= ( dom f )
and
A3: ( M . E ) < ( +infty )
and
A4: (for x being (Element of X) holds (x in E implies (a <= ( f . x ) & ( f . x ) <= b)));
set C = ( chi (E,X) );
A5: ( f | E ) is_integrable_on M by A1,MESFUNC5:97;
(for x being (Element of X) holds (x in ( dom ( a1 (#) ( C | E ) ) ) implies ( ( a1 (#) ( C | E ) ) . x ) <= ( ( f | E ) . x )))
proof
let x being (Element of X);
assume A6: x in ( dom ( a1 (#) ( C | E ) ) );
then A7: x in ( dom ( C | E ) ) by MESFUNC1:def 6;
then x in ( ( dom C ) /\ E ) by RELAT_1:61;
then A8: x in E by XBOOLE_0:def 4;
then a <= ( f . x ) by A4;
then A9: a <= ( ( f | E ) . x ) by A8,FUNCT_1:49;
( ( a1 (#) ( C | E ) ) . x ) = ( ( R_EAL a ) * ( ( C | E ) . x ) ) by A6,MESFUNC1:def 6
.= ( ( R_EAL a ) * ( C . x ) ) by A7,FUNCT_1:47
.= ( ( R_EAL a ) * ( 1. ) ) by A8,FUNCT_3:def 3;
hence thesis by A9,XXREAL_3:81;
end;
then A10: ( ( f | E ) - ( a1 (#) ( C | E ) ) ) is  nonnegative by CKB1:1;
( chi (E,X) ) is_integrable_on M by A3,CKB40:1;
then A11: ( C | E ) is_integrable_on M by MESFUNC5:97;
then ( a1 (#) ( C | E ) ) is_integrable_on M by MESFUNC5:110;
then consider E1 being (Element of S) such that A12: E1 = ( ( dom ( f | E ) ) /\ ( dom ( a1 (#) ( C | E ) ) ) ) and A13: ( Integral (M,( ( a1 (#) ( C | E ) ) | E1 )) ) <= ( Integral (M,( ( f | E ) | E1 )) ) by A5,A10,CKB3:1;
( dom ( f | E ) ) = ( ( dom f ) /\ E ) by RELAT_1:61;
then A14: ( dom ( f | E ) ) = E by A2,XBOOLE_1:28;
( dom ( a1 (#) ( C | E ) ) ) = ( dom ( C | E ) ) by MESFUNC1:def 6;
then ( dom ( a1 (#) ( C | E ) ) ) = ( ( dom C ) /\ E ) by RELAT_1:61;
then ( dom ( a1 (#) ( C | E ) ) ) = ( X /\ E ) by FUNCT_3:def 3;
then A15: ( dom ( a1 (#) ( C | E ) ) ) = E by XBOOLE_1:28;
then A16: ( ( f | E ) | E1 ) = ( f | E ) by A12,A14,RELAT_1:69;
( dom ( b1 (#) ( C | E ) ) ) = ( dom ( C | E ) ) by MESFUNC1:def 6;
then ( dom ( b1 (#) ( C | E ) ) ) = ( ( dom C ) /\ E ) by RELAT_1:61;
then ( dom ( b1 (#) ( C | E ) ) ) = ( X /\ E ) by FUNCT_3:def 3;
then A17: ( dom ( b1 (#) ( C | E ) ) ) = E by XBOOLE_1:28;
(for x being (Element of X) holds (x in ( dom ( f | E ) ) implies ( ( f | E ) . x ) <= ( ( b1 (#) ( C | E ) ) . x )))
proof
let x being (Element of X);
assume A18: x in ( dom ( f | E ) );
then A19: x in ( dom ( C | E ) ) by A14,A15,MESFUNC1:def 6;
then x in ( ( dom C ) /\ E ) by RELAT_1:61;
then A20: x in E by XBOOLE_0:def 4;
then ( f . x ) <= b by A4;
then A21: ( ( f | E ) . x ) <= b by A20,FUNCT_1:49;
( ( b1 (#) ( C | E ) ) . x ) = ( ( R_EAL b ) * ( ( C | E ) . x ) ) by A14,A17,A18,MESFUNC1:def 6
.= ( ( R_EAL b ) * ( C . x ) ) by A19,FUNCT_1:47
.= ( ( R_EAL b ) * ( 1. ) ) by A20,FUNCT_3:def 3;
hence thesis by A21,XXREAL_3:81;
end;
then A22: ( ( b1 (#) ( C | E ) ) - ( f | E ) ) is  nonnegative by CKB1:1;
( b1 (#) ( C | E ) ) is_integrable_on M by A11,MESFUNC5:110;
then consider E2 being (Element of S) such that A23: E2 = ( ( dom ( f | E ) ) /\ ( dom ( b1 (#) ( C | E ) ) ) ) and A24: ( Integral (M,( ( f | E ) | E2 )) ) <= ( Integral (M,( ( b1 (#) ( C | E ) ) | E2 )) ) by A5,A22,CKB3:1;
A25: ( ( b1 (#) ( C | E ) ) | E2 ) = ( b1 (#) ( C | E ) ) by A14,A17,A23,RELAT_1:69;
E = ( E /\ E );
then A26: ( Integral (M,( C | E )) ) = ( M . E ) by A3,CKB41:1;
A27: ( ( f | E ) | E2 ) = ( f | E ) by A14,A17,A23,RELAT_1:69;
( ( a1 (#) ( C | E ) ) | E1 ) = ( a1 (#) ( C | E ) ) by A12,A14,A15,RELAT_1:69;
hence thesis by A11,A13,A24,A25,A16,A27,A26,MESFUNC5:110;
end;
