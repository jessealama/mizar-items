environ
vocabularies NUMBERS,XBOOLE_0,PROB_1,MEASURE1,PARTFUN1,SUBSET_1,RELAT_1,FUNCT_1,XXREAL_0,ARYTM_1,SUPINF_2,SUPINF_1,CARD_1,REAL_1,VALUED_1,MEASURE6,INTEGRA5,MESFUNC5,ARYTM_3,MESFUNC1,COMPLEX1,TARSKI,MESFUNC2,VALUED_0,FINSEQ_1,BINOP_1,SETWISEO,CARD_3,ORDINAL1,FINSEQ_2,NEWTON,ORDINAL4,NAT_1,XREAL_0,POWER,SQUARE_1,XXREAL_2,ORDINAL2,RFUNCT_3,FUNCT_3,MESFUNC7;
notations TARSKI,XBOOLE_0,SUBSET_1,ORDINAL1,NUMBERS,VALUED_0,XREAL_0,COMPLEX1,REAL_1,XXREAL_0,XXREAL_2,MEASURE1,NAT_1,SUPINF_1,RELAT_1,RELSET_1,SETWISEO,PARTFUN1,FINSEQ_1,FINSEQ_2,SETWOP_2,BINOP_1,FUNCT_2,NEWTON,SQUARE_1,PROB_1,SUPINF_2,EXTREAL1,POWER,MESFUNC5,MESFUNC1,MESFUNC2,MEASURE6,CKB7,CKB9,CKB14,CKB16,CKB17,CKB26;
definitions TARSKI,MESFUNC1,MEASURE6,MESFUNC5,XCMPLX_0,SQUARE_1,CKB9,CKB14,CKB16,CKB26;
theorems ABSVALUE,SETWISEO,POWER,HOLDER_1,TARSKI,PARTFUN1,FUNCT_1,SUPINF_2,EXTREAL1,EXTREAL2,BINOP_1,MESFUNC1,FINSEQ_1,XREAL_0,XBOOLE_0,XBOOLE_1,XCMPLX_1,MESFUNC2,NEWTON,XREAL_1,COMPLEX1,XXREAL_0,FINSEQ_2,MESFUNC5,FINSOP_1,NAT_1,RELAT_1,FUNCT_3,MEASURE1,MESFUNC6,ORDINAL1,VALUED_0,XXREAL_2,XXREAL_3,CKB1,CKB2,CKB3,CKB6,CKB9,CKB11,CKB12,CKB14,CKB16,CKB20,CKB21,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29;
schemes FUNCT_2,NAT_1,PARTFUN2,BINOP_1,BINOP_2;
registrations SUBSET_1,NAT_1,RELSET_1,XREAL_0,MEMBERED,ORDINAL1,FINSEQ_1,MEASURE1,NUMBERS,XXREAL_0,XBOOLE_0,VALUED_0,POWER,XXREAL_3,CKB4,CKB5,CKB8,CKB10,CKB13,CKB15,CKB18,CKB19,CKB22;
constructors REAL_1,FINSOP_1,EXTREAL1,BINOP_1,NEWTON,POWER,MESFUNC1,MEASURE6,MESFUNC2,MEASURE3,SETWISEO,EXTREAL2,SQUARE_1,MESFUNC5,SUPINF_1,RELSET_1,BINOP_2,CKB9,CKB14,CKB16,CKB17,CKB26;
requirements NUMERALS,REAL,BOOLE,SUBSET,ARITHM;
begin
reserve X for non empty set;
reserve S for (SigmaField of X);
reserve f for (PartFunc of X,( ExtREAL ));
reserve E for (Element of S);
theorem
Th14: (for k being  natural number holds (for X holds (for S holds (for f holds (for E holds ((E c= ( dom f ) & f is_measurable_on E) implies ( |. f .| |^ k ) is_measurable_on E))))))
proof
let k being  natural number;
let X;
let S;
let f;
let E;
reconsider k1 = k as (Element of ( NAT )) by ORDINAL1:def 12;
assume that
A1: E c= ( dom f )
and
A2: f is_measurable_on E;
A3: ( dom ( |. f .| |^ k ) ) = ( dom |. f .| ) by CKB26:def 1;
then A4: ( dom ( |. f .| |^ k ) ) = ( dom f ) by MESFUNC1:def 10;
per cases ;
suppose A5: k = ( 0 );

A6: (for r being  real number holds (1 < r implies ( E /\ ( less_dom (( |. f .| |^ k ),( R_EAL r )) ) ) in S))
proof
let r being  real number;
assume A7: 1 < r;
E c= ( less_dom (( |. f .| |^ k ),( R_EAL r )) )
proof
let x being set;
assume A8: x in E;
then ( ( |. f .| |^ k ) . x ) = ( ( |. f .| . x ) |^ k ) by A1,A4,CKB26:def 1;
then ( ( |. f .| |^ k ) . x ) < r by A5,A7,CKB20:1,FINSEQ_2:58;
hence thesis by A1,A4,A8,MESFUNC1:def 11;
end;
then ( E /\ ( less_dom (( |. f .| |^ k ),( R_EAL r )) ) ) = E by XBOOLE_1:28;
hence thesis;
end;
A9: E c= ( dom ( |. f .| |^ k ) ) by A1,A3,MESFUNC1:def 10;
(for r being  real number holds ( E /\ ( less_dom (( |. f .| |^ k ),( R_EAL r )) ) ) in S)
proof
let r being  real number;
now
assume A10: r <= 1;
E c= ( great_eq_dom (( |. f .| |^ k ),( R_EAL r )) )
proof
let x being set;
assume A11: x in E;
then ( ( |. f .| |^ k ) . x ) = ( ( |. f .| . x ) |^ k ) by A1,A4,CKB26:def 1;
then r <= ( ( |. f .| |^ k ) . x ) by A5,A10,CKB20:1,FINSEQ_2:58;
hence thesis by A1,A4,A11,MESFUNC1:def 14;
end;
then ( E /\ ( great_eq_dom (( |. f .| |^ k ),( R_EAL r )) ) ) = E by XBOOLE_1:28;
then ( E /\ ( less_dom (( |. f .| |^ k ),( R_EAL r )) ) ) = ( E \ E ) by A9,MESFUNC1:17;
hence thesis;
end;
hence thesis by A6;
end;
hence thesis by MESFUNC1:def 16;
end;
suppose A12: k <> ( 0 );

then A13: ( ( 1 / k ) * k ) = 1 by XCMPLX_1:87;
A14: (for r being  real number holds (( 0 ) < r implies ( great_eq_dom (( |. f .| |^ k ),( R_EAL r )) ) = ( great_eq_dom (|. f .|,( R_EAL ( r to_power ( 1 / k ) ) )) )))
proof
let r being  real number;
assume A15: ( 0 ) < r;
A16: ( great_eq_dom (( |. f .| |^ k ),( R_EAL r )) ) c= ( great_eq_dom (|. f .|,( R_EAL ( r to_power ( 1 / k ) ) )) )
proof
let x being set;
assume A17: x in ( great_eq_dom (( |. f .| |^ k ),( R_EAL r )) );
then A18: x in ( dom ( |. f .| |^ k ) ) by MESFUNC1:def 14;
then A19: ( |. f .| . x ) = |. ( f . x ) .| by A3,MESFUNC1:def 10;
then A20: ( 0 ) <= ( |. f .| . x ) by EXTREAL2:3;
per cases ;
suppose ( |. f .| . x ) = ( +infty );

then ( R_EAL ( r to_power ( 1 / k ) ) ) <= ( |. f .| . x ) by XXREAL_0:3;
hence thesis by A3,A18,MESFUNC1:def 14;
end;
suppose ( |. f .| . x ) <> ( +infty );

then reconsider fx = ( |. f .| . x ) as (Element of ( REAL )) by A20,XXREAL_0:14;
A21: r <= ( ( |. f .| |^ k ) . x ) by A17,MESFUNC1:def 14;
( ( |. f .| |^ k ) . x ) = ( ( |. f .| . x ) |^ k ) by A18,CKB26:def 1;
then A22: r <= ( fx to_power k1 ) by A21,CKB27:1;
( ( fx to_power k ) to_power ( 1 / k ) ) = ( fx to_power ( ( k * 1 ) / k ) ) by A12,A19,EXTREAL2:3,HOLDER_1:2;
then A23: ( ( fx to_power k ) to_power ( 1 / k ) ) = fx by A13,POWER:25;
r is Real by XREAL_0:def 1;
then ( r to_power ( 1 / k ) ) <= ( ( fx to_power k ) to_power ( 1 / k ) ) by A12,A15,A22,HOLDER_1:3;
hence thesis by A3,A18,A23,MESFUNC1:def 14;
end;
end;
( great_eq_dom (|. f .|,( R_EAL ( r to_power ( 1 / k ) ) )) ) c= ( great_eq_dom (( |. f .| |^ k ),( R_EAL r )) )
proof
let x being set;
assume A24: x in ( great_eq_dom (|. f .|,( R_EAL ( r to_power ( 1 / k ) ) )) );
then A25: x in ( dom |. f .| ) by MESFUNC1:def 14;
then A26: ( ( |. f .| |^ k ) . x ) = ( ( |. f .| . x ) |^ k ) by A3,CKB26:def 1;
( |. f .| . x ) = |. ( f . x ) .| by A25,MESFUNC1:def 10;
then A27: ( 0 ) <= ( |. f .| . x ) by EXTREAL2:3;
A28:now
assume ( |. f .| . x ) <> ( +infty );
then reconsider fx = ( |. f .| . x ) as (Element of ( REAL )) by A27,XXREAL_0:14;
reconsider R = ( r to_power ( 1 / k ) ) as Real by XREAL_0:def 1;
A29: ( 0 ) < ( r to_power ( 1 / k ) ) by A15,POWER:34;
A30: ( R to_power k1 ) = ( r to_power ( ( 1 / k ) * k ) ) by A15,POWER:33;
A31: ( ( |. f .| |^ k ) . x ) = ( fx |^ k ) by A26,CKB27:1;
( R_EAL ( r to_power ( 1 / k ) ) ) <= ( |. f .| . x ) by A24,MESFUNC1:def 14;
then ( r to_power 1 ) <= ( fx to_power k ) by A12,A13,A29,A30,HOLDER_1:3;
hence r <= ( ( |. f .| |^ k ) . x ) by A31,POWER:25;
end;
now
assume ( |. f .| . x ) = ( +infty );
then ( ( |. f .| . x ) |^ k ) = ( +infty ) by A12,CKB29:1,NAT_1:14;
hence r <= ( ( |. f .| |^ k ) . x ) by A26,XXREAL_0:3;
end;
hence thesis by A3,A25,A28,MESFUNC1:def 14;
end;
hence thesis by A16,XBOOLE_0:def 10;
end;
A32: |. f .| is_measurable_on E by A1,A2,MESFUNC2:27;
(for r being  real number holds ( E /\ ( great_eq_dom (( |. f .| |^ k ),( R_EAL r )) ) ) in S)
proof
let r being  real number;
per cases ;
suppose A33: r <= ( 0 );

E c= ( great_eq_dom (( |. f .| |^ k ),( R_EAL r )) )
proof
let x being set;
assume A34: x in E;
then A35: ( |. f .| . x ) = |. ( f . x ) .| by A1,A3,A4,MESFUNC1:def 10;
( ( |. f .| |^ k ) . x ) = ( ( |. f .| . x ) |^ k ) by A1,A4,A34,CKB26:def 1;
then r <= ( ( |. f .| |^ k ) . x ) by A33,A35,CKB28:1,EXTREAL2:3;
hence thesis by A1,A4,A34,MESFUNC1:def 14;
end;
then ( E /\ ( great_eq_dom (( |. f .| |^ k ),( R_EAL r )) ) ) = E by XBOOLE_1:28;
hence thesis;
end;
suppose ( 0 ) < r;

then ( E /\ ( great_eq_dom (( |. f .| |^ k ),( R_EAL r )) ) ) = ( E /\ ( great_eq_dom (|. f .|,( R_EAL ( r to_power ( 1 / k ) ) )) ) ) by A14;
hence thesis by A1,A3,A4,A32,MESFUNC1:27;
end;
end;
hence thesis by A1,A4,MESFUNC1:27;
end;
end;
