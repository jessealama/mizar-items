requirements SUBSET;
begin
reserve x for set;
reserve y for set;
reserve R for Relation;
theorem
(R is  strongly_connected implies R is  connected  reflexive)
proof
assume R is  strongly_connected;
then A1: R is_strongly_connected_in ( field R ) by CKB15:def 1;
then (for x holds (for y holds (((x in ( field R ) & y in ( field R )) & x <> y) implies ([ x,y ] in R or [ y,x ] in R)))) by CKB7:def 1;
then R is_connected_in ( field R ) by CKB6:def 1;
hence R is  connected by CKB14:def 1;
thus R is  reflexive
proof
let x;
assume x in ( field R );
hence thesis by A1,CKB7:def 1;
end;

end;
