begin
reserve X for set;
reserve Y for set;
reserve Z for set;
theorem
(for f being (Function of X,Y) holds (Y <> ( {} ) implies (( rng f ) = Y iff (for Z holds (Z <> ( {} ) implies (for g,h being (Function of Y,Z) holds (( g * f ) = ( h * f ) implies g = h)))))))
proof
let f being (Function of X,Y);
assume A1: Y <> ( {} );
thus (( rng f ) = Y implies (for Z holds (Z <> ( {} ) implies (for g,h being (Function of Y,Z) holds (( g * f ) = ( h * f ) implies g = h)))))
proof
assume A2: ( rng f ) = Y;
let Z;
assume that
A3: Z <> ( {} );
let g being (Function of Y,Z),h being (Function of Y,Z);
(( dom g ) = Y & ( dom h ) = Y) by A3,CKB1:def 1;
hence thesis by A2,FUNCT_1:86;
end;

assume A4: (for Z holds (Z <> ( {} ) implies (for g,h being (Function of Y,Z) holds (( g * f ) = ( h * f ) implies g = h))));
(for g,h being Function holds (((( dom g ) = Y & ( dom h ) = Y) & ( g * f ) = ( h * f )) implies g = h))
proof
let g being Function,h being Function;
assume that
A5: ( dom g ) = Y
and
A6: ( dom h ) = Y;
A7: ( rng g ) <> ( {} ) by A1,A5,RELAT_1:42;
A8: g is (Function of Y,( ( rng g ) \/ ( rng h ) )) by A5,CKB6:1,XBOOLE_1:7;
h is (Function of Y,( ( rng g ) \/ ( rng h ) )) by A6,CKB6:1,XBOOLE_1:7;
hence thesis by A4,A7,A8;
end;
hence thesis by FUNCT_1:16;
end;
