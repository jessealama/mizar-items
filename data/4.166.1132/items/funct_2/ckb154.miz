begin
reserve y for set;
theorem
(for X,Y being non empty set holds (for f being (Function of X,Y) holds (f is  constant iff (ex y being (Element of Y) st ( rng f ) = { y }))))
proof
let X being non empty set,Y being non empty set;
let f being (Function of X,Y);
hereby
consider x being set such that A1: x in ( dom f ) by XBOOLE_0:def 1;
set y = ( f . x );
reconsider y as (Element of Y) by A1,CKB9:1;
assume A2: f is  constant;
take y;
(for y9 being set holds (y9 in ( rng f ) iff y9 = y))
proof
let y9 being set;
hereby
assume y9 in ( rng f );
then (ex x9 being set st (x9 in ( dom f ) & y9 = ( f . x9 ))) by FUNCT_1:def 3;
hence y9 = y by A2,A1,FUNCT_1:def 10;
end;
assume y9 = y;
hence thesis by A1,CKB8:1;
end;
hence ( rng f ) = { y } by TARSKI:def 1;
end;
given y being (Element of Y) such that
A3: ( rng f ) = { y };

let x being set,x9 being set;
assume x in ( dom f );
then A4: ( f . x ) in ( rng f ) by CKB8:1;
assume x9 in ( dom f );
then A5: ( f . x9 ) in ( rng f ) by CKB8:1;
thus ( f . x ) = y by A3,A4,TARSKI:def 1
.= ( f . x9 ) by A3,A5,TARSKI:def 1;
end;
