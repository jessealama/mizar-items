theorem
(for A1,A2,B1,B2 being set holds (for f being (Function of A1,A2) holds (for g being (Function of B1,B2) holds (f tolerates g implies ( f /\ g ) is (Function of ( A1 /\ B1 ),( A2 /\ B2 ))))))
proof
let A1 being set,A2 being set,B1 being set,B2 being set;
let f being (Function of A1,A2);
let g being (Function of B1,B2);
assume that
A1: f tolerates g;
A2: (( dom ( f /\ g ) ) c= ( ( dom f ) /\ ( dom g ) ) & ( ( dom f ) /\ ( dom g ) ) c= ( A1 /\ B1 )) by RELAT_1:2,XBOOLE_1:27;
A3:now
set a = the (Element of ( A1 /\ B1 ));
assume that
A4: ( dom f ) = A1
and
A1 <> ( {} )
and
A5: ( dom g ) = B1
and
B1 <> ( {} );
hereby
assume A6: ( A1 /\ B1 ) <> ( {} );
then a in A1 by XBOOLE_0:def 4;
then A7: ( f . a ) in ( rng f ) by A4,FUNCT_1:def 3;
(( f . a ) = ( g . a ) & a in B1) by A1,A4,A5,A6,PARTFUN1:def 4,XBOOLE_0:def 4;
then ( f . a ) in ( rng g ) by A5,FUNCT_1:def 3;
hence ( A2 /\ B2 ) <> ( {} ) by A7,XBOOLE_0:def 4;
end;
thus ( dom ( f /\ g ) ) = ( A1 /\ B1 )
proof
thus ( dom ( f /\ g ) ) c= ( A1 /\ B1 ) by A2,XBOOLE_1:1;
let a being set;
assume A8: a in ( A1 /\ B1 );
then a in A1 by XBOOLE_0:def 4;
then A9: [ a,( f . a ) ] in f by A4,FUNCT_1:def 2;
(( f . a ) = ( g . a ) & a in B1) by A1,A4,A5,A8,PARTFUN1:def 4,XBOOLE_0:def 4;
then [ a,( f . a ) ] in g by A5,FUNCT_1:def 2;
then [ a,( f . a ) ] in ( f /\ g ) by A9,XBOOLE_0:def 4;
hence thesis by RELAT_1:def 4;
end;

end;
(( rng ( f /\ g ) ) c= ( ( rng f ) /\ ( rng g ) ) & ( ( rng f ) /\ ( rng g ) ) c= ( A2 /\ B2 )) by RELAT_1:13,XBOOLE_1:27;
then A10: ( rng ( f /\ g ) ) c= ( A2 /\ B2 ) by XBOOLE_1:1;
(A2 = ( {} ) or A2 <> ( {} ));
then A11: ((f = ( {} ) & (A1 = ( {} ) or A2 = ( {} ))) or ((( dom f ) = A1 & A1 <> ( {} )) & A2 <> ( {} ))) by CKB1:def 1;
A12: (B2 = ( {} ) or B2 <> ( {} ));
A13:now
per cases ;
case ( A2 /\ B2 ) <> ( {} );
hence ( dom ( f /\ g ) ) = ( A1 /\ B1 ) by A11,A12,A3,CKB1:def 1;
end;
case ( A1 /\ B1 ) = ( {} );
hence ( dom ( f /\ g ) ) = ( A1 /\ B1 ) by A2;
end;
case (( A2 /\ B2 ) = ( {} ) & ( A1 /\ B1 ) <> ( {} ));
hence ( f /\ g ) = ( {} ) by A11,A12,A3,CKB1:def 1;
end;
end;
( dom ( f /\ g ) ) c= ( A1 /\ B1 ) by A2,XBOOLE_1:1;
hence thesis by A10,A13,CKB1:def 1,RELSET_1:4;
end;
