begin
theorem
(for X being set holds (for Y,Z being non empty set holds (for f being (Function of X,Y) holds (for g being (Function of Y,Z) holds ((f is  onto & g is  onto) implies ( g * f ) is  onto)))))
proof
let X being set;
let Y being non empty set,Z being non empty set;
let f being (Function of X,Y);
let g being (Function of Y,Z);
assume that
A1: f is  onto
and
A2: g is  onto;
( rng f ) = Y by A1,CKB33:def 1
.= ( dom g ) by CKB1:def 1;
then ( rng ( g * f ) ) = ( rng g ) by RELAT_1:28
.= Z by A2,CKB33:def 1;
hence thesis by CKB33:def 1;
end;
