begin
reserve X for set;
reserve x for set;
reserve y for set;
theorem
Th33: (for x holds (x in X implies ( Class (( id X ),x) ) = { x }))
proof
let x;
A1:now
let y;
assume y in ( Class (( id X ),x) );
then [ y,x ] in ( id X ) by CKB32:1;
hence y = x by RELAT_1:def 10;
end;
assume x in X;
then (for y holds (y in ( Class (( id X ),x) ) iff y = x)) by A1,CKB33:1;
hence thesis by TARSKI:def 1;
end;
