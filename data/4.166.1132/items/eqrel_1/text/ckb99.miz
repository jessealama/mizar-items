begin
reserve e for set;
reserve u for set;
reserve X for non empty set;
definition
let X;
let D being non empty (a_partition of X);
func proj D -> (Function of X,D)
means
:Def12: (for p being (Element of X) holds p in ( it . p ));
existence
proof
defpred P[ set,set ]
 means
$1 in $2;
A1:now
A2: ( union D ) = X by CKB47:def 1;
let e;
assume e in X;
then (ex u st (e in u & u in D)) by A2,TARSKI:def 4;
hence (ex u st (u in D & P[ e,u ]));
end;
consider F being (Function of X,D) such that A3: (for e holds (e in X implies P[ e,( F . e ) ])) from FUNCT_2:sch 1(A1);
take F;
let p being (Element of X);
thus thesis by A3;
end;
correctness
proof
let F being (Function of X,D),G being (Function of X,D);
assume that
A4: ((for p being (Element of X) holds p in ( F . p )) & (for p being (Element of X) holds p in ( G . p )));
now
let x being (Element of X);
(x in ( F . x ) & x in ( G . x )) by A4;
then A5: (not ( F . x ) misses ( G . x )) by XBOOLE_0:3;
(( F . x ) is (Subset of X) & ( G . x ) is (Subset of X)) by TARSKI:def 3;
hence ( F . x ) = ( G . x ) by A5,CKB47:def 1;
end;
hence F = G by FUNCT_2:63;
end;
