reserve Y for set;
theorem
Th59: (for X1 being (Subset of X) holds (for Y1 being (Subset of Y) holds ([: X1,Y1 :] <> ( {} ) implies (( ( .: ( pr1 (X,Y) ) ) . [: X1,Y1 :] ) = X1 & ( ( .: ( pr2 (X,Y) ) ) . [: X1,Y1 :] ) = Y1))))
proof
let X1 being (Subset of X);
let Y1 being (Subset of Y);
assume A1: [: X1,Y1 :] <> ( {} );
thus ( ( .: ( pr1 (X,Y) ) ) . [: X1,Y1 :] ) = ( ( pr1 (X,Y) ) .: [: X1,Y1 :] ) by CKB80:1
.= X1 by A1,CKB82:1;
thus ( ( .: ( pr2 (X,Y) ) ) . [: X1,Y1 :] ) = ( ( pr2 (X,Y) ) .: [: X1,Y1 :] ) by CKB81:1
.= Y1 by A1,CKB82:1;
end;
