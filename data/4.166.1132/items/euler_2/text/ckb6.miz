constructors REAL_1,SQUARE_1,NAT_1,NAT_D,NEWTON,RFINSEQ,WSIERP_1,EULER_1,SEQ_1,RECDEF_1,CLASSES1,BINOP_2,RELSET_1;
requirements REAL,NUMERALS,BOOLE,SUBSET,ARITHM;
begin
reserve m for Nat;
reserve z for Integer;
theorem
Lm4: (((( 1 - m ) <= z & z <= ( m - 1 )) & m divides z) implies z = ( 0 ))
proof
assume that
A1: ( 1 - m ) <= z
and
A2: z <= ( m - 1 )
and
A3: m divides z;
consider t being Integer such that A4: z = ( m * t ) by A3,INT_1:def 3;
assume A5: z <> ( 0 );
now
per cases  by A5,A4;
suppose A6: t > ( 0 );

then reconsider t as (Element of ( NAT )) by INT_1:3;
( m * t ) >= m by A6,NAT_1:14,NEWTON:33;
then ( ( m * t ) + 1 ) > m by NAT_1:13;
hence contradiction by A2,A4,XREAL_1:19;
end;
suppose A7: t < ( 0 );

reconsider r = ( t + 1 ) as Integer;
1 <= ( ( m * t ) + ( m * 1 ) ) by A1,A4,XREAL_1:20;
then 1 <= ( m * ( t + 1 ) );
then r >= 1 by CKB5:1;
then ( 1 - 1 ) <= t by XREAL_1:20;
hence contradiction by A7;
end;
end;
hence contradiction;
end;
