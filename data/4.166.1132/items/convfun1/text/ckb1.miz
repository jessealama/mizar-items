requirements REAL,NUMERALS,BOOLE,SUBSET,ARITHM;
begin
definition
let X being non  empty RLSStruct;
let Y being non  empty RLSStruct;
func Add_in_Prod_of_RLS (X,Y) -> (BinOp of [: (the carrier of X),(the carrier of Y) :]) means 
:Def1: (for z1,z2 being (Element of [: (the carrier of X),(the carrier of Y) :]) holds (for x1,x2 being (VECTOR of X) holds (for y1,y2 being (VECTOR of Y) holds ((z1 = [ x1,y1 ] & z2 = [ x2,y2 ]) implies ( it . (z1,z2) ) = [ ( (the addF of X) . [ x1,x2 ] ),( (the addF of Y) . [ y1,y2 ] ) ]))));
existence
proof
defpred P[ set,set,set ]
 means
(for x1,x2 being (VECTOR of X) holds (for y1,y2 being (VECTOR of Y) holds (($1 = [ x1,y1 ] & $2 = [ x2,y2 ]) implies $3 = [ ( (the addF of X) . [ x1,x2 ] ),( (the addF of Y) . [ y1,y2 ] ) ])));
set A = [: (the carrier of X),(the carrier of Y) :];
A1: (for z1,z2 being (Element of A) holds (ex z3 being (Element of A) st P[ z1,z2,z3 ]))
proof
let z1 being (Element of A);
let z2 being (Element of A);
consider x19 being set,y19 being set such that A2: x19 in (the carrier of X) and A3: y19 in (the carrier of Y) and A4: z1 = [ x19,y19 ] by ZFMISC_1:def 2;
consider x29 being set,y29 being set such that A5: x29 in (the carrier of X) and A6: y29 in (the carrier of Y) and A7: z2 = [ x29,y29 ] by ZFMISC_1:def 2;
reconsider y19,y29 as (VECTOR of Y) by A3,A6;
reconsider x19,x29 as (VECTOR of X) by A2,A5;
reconsider z3 = [ ( (the addF of X) . [ x19,x29 ] ),( (the addF of Y) . [ y19,y29 ] ) ] as (Element of A);
take z3;
let x1 being (VECTOR of X);
let x2 being (VECTOR of X);
let y1 being (VECTOR of Y);
let y2 being (VECTOR of Y);
assume that
A8: z1 = [ x1,y1 ]
and
A9: z2 = [ x2,y2 ];
A10: x2 = x29 by A7,A9,ZFMISC_1:27;
A11: y1 = y19 by A4,A8,ZFMISC_1:27;
x1 = x19 by A4,A8,ZFMISC_1:27;
hence thesis by A7,A9,A10,A11,ZFMISC_1:27;
end;
thus (ex o being (BinOp of A) st (for a,b being (Element of A) holds P[ a,b,( o . (a,b) ) ])) from BINOP_1:sch 3(A1);
end;
uniqueness
proof
let A1 being (BinOp of [: (the carrier of X),(the carrier of Y) :]);
let A2 being (BinOp of [: (the carrier of X),(the carrier of Y) :]);
assume A12: (for z1,z2 being (Element of [: (the carrier of X),(the carrier of Y) :]) holds (for x1,x2 being (VECTOR of X) holds (for y1,y2 being (VECTOR of Y) holds ((z1 = [ x1,y1 ] & z2 = [ x2,y2 ]) implies ( A1 . (z1,z2) ) = [ ( (the addF of X) . [ x1,x2 ] ),( (the addF of Y) . [ y1,y2 ] ) ]))));
assume A13: (for z1,z2 being (Element of [: (the carrier of X),(the carrier of Y) :]) holds (for x1,x2 being (VECTOR of X) holds (for y1,y2 being (VECTOR of Y) holds ((z1 = [ x1,y1 ] & z2 = [ x2,y2 ]) implies ( A2 . (z1,z2) ) = [ ( (the addF of X) . [ x1,x2 ] ),( (the addF of Y) . [ y1,y2 ] ) ]))));
(for z1,z2 being (Element of [: (the carrier of X),(the carrier of Y) :]) holds ( A1 . (z1,z2) ) = ( A2 . (z1,z2) ))
proof
let z1 being (Element of [: (the carrier of X),(the carrier of Y) :]);
let z2 being (Element of [: (the carrier of X),(the carrier of Y) :]);
consider x1 being set,y1 being set such that A14: x1 in (the carrier of X) and A15: y1 in (the carrier of Y) and A16: z1 = [ x1,y1 ] by ZFMISC_1:def 2;
consider x2 being set,y2 being set such that A17: x2 in (the carrier of X) and A18: y2 in (the carrier of Y) and A19: z2 = [ x2,y2 ] by ZFMISC_1:def 2;
reconsider y1,y2 as (VECTOR of Y) by A15,A18;
reconsider x1,x2 as (VECTOR of X) by A14,A17;
( A1 . (z1,z2) ) = [ ( (the addF of X) . [ x1,x2 ] ),( (the addF of Y) . [ y1,y2 ] ) ] by A12,A16,A19;
hence thesis by A13,A16,A19;
end;
hence thesis by BINOP_1:2;
end;
end;
