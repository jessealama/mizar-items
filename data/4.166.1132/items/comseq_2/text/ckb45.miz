begin
reserve n for (Element of ( NAT ));
reserve n1 for (Element of ( NAT ));
reserve n2 for (Element of ( NAT ));
reserve m for (Element of ( NAT ));
reserve g1 for (Element of ( COMPLEX ));
reserve g2 for (Element of ( COMPLEX ));
reserve g for (Element of ( COMPLEX ));
reserve R for Real;
reserve s for Complex_Sequence;
reserve s9 for Complex_Sequence;
theorem
Th29: ((s is  convergent  convergent Complex_Sequence & s9 is  convergent  convergent Complex_Sequence) implies ( s (#) s9 ) is  convergent)
proof
assume that
A1: s is  convergent  convergent Complex_Sequence
and
A2: s9 is  convergent  convergent Complex_Sequence;
consider g1 such that A3: (for p being Real holds (( 0 ) < p implies (ex n st (for m holds (n <= m implies |. ( ( s . m ) - g1 ) .| < p))))) by A1,CKB14:def 1;
consider g2 such that A4: (for p being Real holds (( 0 ) < p implies (ex n st (for m holds (n <= m implies |. ( ( s9 . m ) - g2 ) .| < p))))) by A2,CKB14:def 1;
take g = ( g1 * g2 );
let p being Real;
consider R such that A5: ( 0 ) < R and A6: (for n holds |. ( s . n ) .| < R) by A1,CKB13:1;
A7: ( ( 0 ) + ( 0 ) ) < ( |. g2 .| + R ) by A5,COMPLEX1:46,XREAL_1:8;
assume A8: ( 0 ) < p;
then consider n1 such that A9: (for m holds (n1 <= m implies |. ( ( s . m ) - g1 ) .| < ( p / ( |. g2 .| + R ) ))) by A3,A7,XREAL_1:139;
consider n2 such that A10: (for m holds (n2 <= m implies |. ( ( s9 . m ) - g2 ) .| < ( p / ( |. g2 .| + R ) ))) by A4,A7,A8,XREAL_1:139;
take n = ( n1 + n2 );
let m;
assume that
A11: n <= m;
n1 <= ( n1 + n2 ) by NAT_1:12;
then n1 <= m by A11,XXREAL_0:2;
then A12: |. ( ( s . m ) - g1 ) .| <= ( p / ( |. g2 .| + R ) ) by A9;
(( 0 ) <= |. g2 .| & |. ( ( ( s . m ) - g1 ) * g2 ) .| = ( |. g2 .| * |. ( ( s . m ) - g1 ) .| )) by COMPLEX1:46,COMPLEX1:65;
then A13: |. ( ( ( s . m ) - g1 ) * g2 ) .| <= ( |. g2 .| * ( p / ( |. g2 .| + R ) ) ) by A12,XREAL_1:64;
|. ( ( ( s (#) s9 ) . m ) - g ) .| = |. ( ( ( ( ( s . m ) * ( s9 . m ) ) - ( ( s . m ) * g2 ) ) + ( ( s . m ) * g2 ) ) - ( g1 * g2 ) ) .| by VALUED_1:5
.= |. ( ( ( s . m ) * ( ( s9 . m ) - g2 ) ) + ( ( ( s . m ) - g1 ) * g2 ) ) .|;
then A14: |. ( ( ( s (#) s9 ) . m ) - g ) .| <= ( |. ( ( s . m ) * ( ( s9 . m ) - g2 ) ) .| + |. ( ( ( s . m ) - g1 ) * g2 ) .| ) by COMPLEX1:56;
n2 <= n by NAT_1:12;
then n2 <= m by A11,XXREAL_0:2;
then A15: |. ( ( s9 . m ) - g2 ) .| < ( p / ( |. g2 .| + R ) ) by A10;
A16: (( 0 ) <= |. ( s . m ) .| & ( 0 ) <= |. ( ( s9 . m ) - g2 ) .|) by COMPLEX1:46;
|. ( s . m ) .| < R by A6;
then ( |. ( s . m ) .| * |. ( ( s9 . m ) - g2 ) .| ) < ( R * ( p / ( |. g2 .| + R ) ) ) by A16,A15,XREAL_1:96;
then A17: |. ( ( s . m ) * ( ( s9 . m ) - g2 ) ) .| < ( R * ( p / ( |. g2 .| + R ) ) ) by COMPLEX1:65;
( ( R * ( p / ( |. g2 .| + R ) ) ) + ( |. g2 .| * ( p / ( |. g2 .| + R ) ) ) ) = ( ( p / ( |. g2 .| + R ) ) * ( |. g2 .| + R ) )
.= p by A7,XCMPLX_1:87;
then ( |. ( ( s . m ) * ( ( s9 . m ) - g2 ) ) .| + |. ( ( ( s . m ) - g1 ) * g2 ) .| ) < p by A17,A13,XREAL_1:8;
hence thesis by A14,XXREAL_0:2;
end;
