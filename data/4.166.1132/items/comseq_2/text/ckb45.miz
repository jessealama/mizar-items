registrations XBOOLE_0,ORDINAL1,RELSET_1,FUNCT_2,NUMBERS,XXREAL_0,XREAL_0,MEMBERED,VALUED_1,VALUED_0,CKB5,CKB12,CKB18,CKB19,CKB21,CKB24,CKB28,CKB33,CKB39,CKB43,CKB44;
constructors PARTFUN1,FUNCOP_1,REAL_1,SQUARE_1,NAT_1,COMPLEX1,SEQ_2,VALUED_1,SEQ_1,RELSET_1,CKB3,CKB14,CKB15;
