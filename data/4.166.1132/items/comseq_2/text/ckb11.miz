definition
let s;
redefine attr s is  bounded
means
:Def3: (ex r being Real st (for n holds |. ( s . n ) .| < r));
compatibility
proof
thus (s is  bounded implies (ex r being Real st (for n holds |. ( s . n ) .| < r)))
proof
given r being  real number such that
A1: (for y being set holds (y in ( dom s ) implies ( abs ( s . y ) ) < r));

reconsider r as (Element of ( REAL )) by XREAL_0:def 1;
take r;
let n;
n in ( NAT );
then n in ( dom s ) by FUNCT_2:def 1;
hence thesis by A1;
end;

given r being Real such that
A2: (for n holds |. ( s . n ) .| < r);

take r;
let y being set;
assume y in ( dom s );
hence thesis by A2;
