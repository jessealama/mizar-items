constructors PARTFUN1,BINOP_1,DOMAIN_1,FUNCOP_1,XXREAL_0,REAL_1,NAT_1,FINSEQ_4,RLSUB_1,VALUED_1,RELSET_1,VECTSP_1,PRE_POLY,RLVECT_1,CKB1;
requirements REAL,NUMERALS,BOOLE,SUBSET,ARITHM;
begin
reserve p for FinSequence;
reserve V for RealLinearSpace;
definition
let V being non  empty addLoopStr;
let T being  finite (Subset of V);
assume A1: V is  Abelian  add-associative  right_zeroed;
func Sum T -> (Element of V) means 
:Def4: (ex F being (FinSequence of (the carrier of V)) st ((( rng F ) = T & F is  one-to-one) & it = ( Sum F )));
existence
proof
consider p such that A2: ( rng p ) = T and A3: p is  one-to-one by FINSEQ_4:58;
reconsider p as (FinSequence of (the carrier of V)) by A2,FINSEQ_1:def 4;
take ( Sum p );
take p;
thus thesis by A2,A3;
end;
uniqueness by A1,RLVECT_1:42;
end;
