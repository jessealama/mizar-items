reserve V for RealLinearSpace;
reserve u for (VECTOR of V);
reserve v for (VECTOR of V);
reserve a for Real;
reserve A for (Subset of V);
reserve l for (Linear_Combination of A);
reserve l1 for (Linear_Combination of A);
reserve l2 for (Linear_Combination of A);
definition
let V;
let A;
func LC_RLSpace A ->  strict (Subspace of ( LC_RLSpace V )) means 
(the carrier of it) = { l: (not contradiction) };
existence
proof
set X = { l: (not contradiction) };
X c= (the carrier of ( LC_RLSpace V ))
proof
let x;
assume x in X;
then (ex l st x = l);
hence thesis by CKB74:def 1;
end;
then reconsider X as (Subset of ( LC_RLSpace V ));
A1: X is  linearly-closed
proof
thus (for v,u being (VECTOR of ( LC_RLSpace V )) holds ((v in X & u in X) implies ( v + u ) in X))
proof
let v being (VECTOR of ( LC_RLSpace V ));
let u being (VECTOR of ( LC_RLSpace V ));
assume that
A2: v in X
and
A3: u in X;
consider l1 such that A4: v = l1 by A2;
consider l2 such that A5: u = l2 by A3;
A6: u = ( vector (( LC_RLSpace V ),l2) ) by A5,CKB1:def 1,RLVECT_1:1;
v = ( vector (( LC_RLSpace V ),l1) ) by A4,CKB1:def 1,RLVECT_1:1;
then ( v + u ) = ( l1 + l2 ) by A6,CKB87:1;
then ( v + u ) is (Linear_Combination of A) by CKB51:1;
hence thesis;
end;

let a;
let v being (VECTOR of ( LC_RLSpace V ));
assume v in X;
then consider l such that A7: v = l;
( a * v ) = ( a * ( vector (( LC_RLSpace V ),l) ) ) by A7,CKB1:def 1,RLVECT_1:1
.= ( a * l ) by CKB88:1;
then ( a * v ) is (Linear_Combination of A) by CKB58:1;
hence thesis;
end;
( ZeroLC V ) is (Linear_Combination of A) by CKB30:1;
then ( ZeroLC V ) in X;
hence thesis by A1,RLSUB_1:35;
end;
uniqueness by RLSUB_1:30;
end;
