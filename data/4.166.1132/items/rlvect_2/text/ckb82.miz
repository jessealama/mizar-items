registrations SUBSET_1,FUNCT_1,RELSET_1,FUNCT_2,FINSET_1,NUMBERS,XREAL_0,STRUCT_0,RLVECT_1,RLSUB_1,VALUED_1,VALUED_0,MEMBERED,FINSEQ_1,CARD_1,VECTSP_1,VECTSP_2,CKB24,CKB75,CKB81;
constructors PARTFUN1,BINOP_1,DOMAIN_1,FUNCOP_1,XXREAL_0,REAL_1,NAT_1,FINSEQ_4,RLSUB_1,VALUED_1,RELSET_1,VECTSP_1,PRE_POLY,RLVECT_1,CKB1,CKB9,CKB21,CKB23,CKB26,CKB28,CKB32,CKB38,CKB48,CKB49,CKB55,CKB63,CKB69,CKB74,CKB76,CKB77,CKB78,CKB79,CKB80;
requirements REAL,NUMERALS,BOOLE,SUBSET,ARITHM;
begin
reserve V for RealLinearSpace;
reserve u for (VECTOR of V);
reserve v for (VECTOR of V);
reserve w for (VECTOR of V);
reserve a for Real;
reserve b for Real;
reserve K for (Linear_Combination of V);
reserve L for (Linear_Combination of V);
registration
let V;
cluster ( LC_RLSpace V ) ->  Abelian  add-associative  right_zeroed  right_complementable  vector-distributive  scalar-distributive  scalar-associative  scalar-unital;
coherence
proof
set S = ( LC_RLSpace V );
A1:now
let v being (VECTOR of S);
let u being (VECTOR of S);
let K;
let L;
A2: (( @ ( @ K ) ) = K & ( @ ( @ L ) ) = L);
assume (v = K & u = L);
hence ( v + u ) = ( K + L ) by A2,CKB78:def 1;
end;
thus S is  Abelian
proof
let u being (Element of S);
let v being (Element of S);
reconsider K = u,L = v as (Linear_Combination of V) by CKB74:def 1;
thus ( u + v ) = ( L + K ) by A1
.= ( v + u ) by A1;
end;

thus S is  add-associative
proof
let u being (Element of S);
let v being (Element of S);
let w being (Element of S);
reconsider L = u,K = v,M = w as (Linear_Combination of V) by CKB74:def 1;
A3: ( v + w ) = ( K + M ) by A1;
( u + v ) = ( L + K ) by A1;
hence ( ( u + v ) + w ) = ( ( L + K ) + M ) by A1
.= ( L + ( K + M ) ) by CKB53:1
.= ( u + ( v + w ) ) by A1,A3;
end;

thus S is  right_zeroed
proof
let v being (Element of S);
reconsider K = v as (Linear_Combination of V) by CKB74:def 1;
thus ( v + ( 0. S ) ) = ( K + ( ZeroLC V ) ) by A1
.= v by CKB54:1;
end;

thus S is  right_complementable
proof
let v being (Element of S);
reconsider K = v as (Linear_Combination of V) by CKB74:def 1;
( - K ) in (the carrier of S) by CKB74:def 1;
then ( - K ) in S by STRUCT_0:def 5;
then ( - K ) = ( vector (S,( - K )) ) by CKB1:def 1;
then ( v + ( vector (S,( - K )) ) ) = ( K - K ) by A1
.= ( 0. S ) by CKB73:1;
hence (ex w being (VECTOR of S) st ( v + w ) = ( 0. S ));
end;

A4:now
let v being (VECTOR of S);
let L;
let a;
A5: ( @ ( @ L ) ) = L;
assume v = L;
hence ( a * v ) = ( a * L ) by A5,CKB79:def 1;
end;
thus (for a being  real number holds (for v,w being (VECTOR of S) holds ( a * ( v + w ) ) = ( ( a * v ) + ( a * w ) )))
proof
let a being  real number;
let v being (VECTOR of S);
let w being (VECTOR of S);
reconsider K = v,M = w as (Linear_Combination of V) by CKB74:def 1;
reconsider a as Real by XREAL_0:def 1;
A6: (( a * v ) = ( a * K ) & ( a * w ) = ( a * M )) by A4;
( v + w ) = ( K + M ) by A1;
then ( a * ( v + w ) ) = ( a * ( K + M ) ) by A4
.= ( ( a * K ) + ( a * M ) ) by CKB60:1
.= ( ( a * v ) + ( a * w ) ) by A1,A6;
hence thesis;
end;

thus (for a,b being  real number holds (for v being (VECTOR of S) holds ( ( a + b ) * v ) = ( ( a * v ) + ( b * v ) )))
proof
let a being  real number;
let b being  real number;
let v being (VECTOR of S);
reconsider K = v as (Linear_Combination of V) by CKB74:def 1;
reconsider a,b as Real by XREAL_0:def 1;
A7: (( a * v ) = ( a * K ) & ( b * v ) = ( b * K )) by A4;
( ( a + b ) * v ) = ( ( a + b ) * K ) by A4
.= ( ( a * K ) + ( b * K ) ) by CKB59:1
.= ( ( a * v ) + ( b * v ) ) by A1,A7;
hence thesis;
end;

thus (for a,b being  real number holds (for v being (VECTOR of S) holds ( ( a * b ) * v ) = ( a * ( b * v ) )))
proof
let a being  real number;
let b being  real number;
let v being (VECTOR of S);
reconsider K = v as (Linear_Combination of V) by CKB74:def 1;
reconsider a,b as Real by XREAL_0:def 1;
A8: ( b * v ) = ( b * K ) by A4;
( ( a * b ) * v ) = ( ( a * b ) * K ) by A4
.= ( a * ( b * K ) ) by CKB61:1
.= ( a * ( b * v ) ) by A4,A8;
hence thesis;
end;

let v being (VECTOR of S);
reconsider K = v as (Linear_Combination of V) by CKB74:def 1;
thus ( 1 * v ) = ( 1 * K ) by A4
.= v by CKB62:1;
end;
end;
