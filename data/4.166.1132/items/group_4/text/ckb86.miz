constructors PARTFUN1,SETFAM_1,BINOP_1,SETWISEO,XXREAL_0,NAT_1,NAT_D,FINSEQ_3,FINSEQ_4,FINSOP_1,REALSET1,REAL_1,LATTICES,GROUP_3,RELSET_1,FINSEQ_2,BINOP_2,CKB1,CKB4,CKB9,CKB25,CKB35,CKB44,CKB46,CKB48,CKB54,CKB61,CKB84,CKB85;
requirements NUMERALS,REAL,BOOLE,SUBSET,ARITHM;
begin
reserve G for Group;
reserve A for (Subset of G);
reserve B for (Subset of G);
reserve C for (Subset of G);
reserve H1 for (Subgroup of G);
reserve H2 for (Subgroup of G);
theorem
Lm6: ((LattStr (# ( Subgroups G ),( SubJoin G ),( SubMeet G ) #) is Lattice & LattStr (# ( Subgroups G ),( SubJoin G ),( SubMeet G ) #) is 0_Lattice) & LattStr (# ( Subgroups G ),( SubJoin G ),( SubMeet G ) #) is 1_Lattice)
proof
set L = LattStr (# ( Subgroups G ),( SubJoin G ),( SubMeet G ) #);
A1:now
let A being (Element of L);
let B being (Element of L);
let H1 being  strict (Subgroup of G);
let H2 being  strict (Subgroup of G);
assume A2: (A = H1 & B = H2);
thus ( A "\/" B ) = ( ( SubJoin G ) . (A,B) ) by LATTICES:def 1
.= ( H1 "\/" H2 ) by A2,CKB84:def 1;
end;
A3:now
let A being (Element of L);
let B being (Element of L);
let H1 being  strict (Subgroup of G);
let H2 being  strict (Subgroup of G);
assume A4: (A = H1 & B = H2);
thus ( A "/\" B ) = ( ( SubMeet G ) . (A,B) ) by LATTICES:def 2
.= ( H1 /\ H2 ) by A4,CKB85:def 1;
end;
now
let A being (Element of L);
let B being (Element of L);
let C being (Element of L);
reconsider H1 = A,H2 = B,H3 = C as  strict (Subgroup of G) by GROUP_3:def 1;
A5: ( H2 "\/" H3 ) = ( B "\/" C ) by A1;
thus ( A "\/" B ) = ( H1 "\/" H2 ) by A1
.= ( H2 "\/" H1 )
.= ( B "\/" A ) by A1;
A6: ( H1 "\/" H2 ) = ( A "\/" B ) by A1;
hence ( ( A "\/" B ) "\/" C ) = ( ( H1 "\/" H2 ) "\/" H3 ) by A1
.= ( H1 "\/" ( H2 "\/" H3 ) ) by CKB72:1
.= ( A "\/" ( B "\/" C ) ) by A1,A5;
A7: ( H1 /\ H2 ) = ( A "/\" B ) by A3;
hence ( ( A "/\" B ) "\/" B ) = ( ( H1 /\ H2 ) "\/" H2 ) by A1
.= B by CKB81:1;
A8: ( H2 /\ H3 ) = ( B "/\" C ) by A3;
thus ( A "/\" B ) = ( H2 /\ H1 ) by A3
.= ( B "/\" A ) by A3;
thus ( ( A "/\" B ) "/\" C ) = ( ( H1 /\ H2 ) /\ H3 ) by A3,A7
.= ( H1 /\ ( H2 /\ H3 ) ) by GROUP_2:84
.= ( A "/\" ( B "/\" C ) ) by A3,A8;
thus ( A "/\" ( A "\/" B ) ) = ( H1 /\ ( H1 "\/" H2 ) ) by A3,A6
.= A by CKB82:1;
end;
then A9: L is  join-commutative  join-associative  meet-absorbing  meet-commutative  meet-associative  join-absorbing by LATTICES:def 4,LATTICES:def 5,LATTICES:def 6,LATTICES:def 7,LATTICES:def 8,LATTICES:def 9;
hence L is Lattice;
reconsider L as Lattice by A9;
reconsider E = ( (1). G ) as (Element of L) by GROUP_3:def 1;
now
let A being (Element of L);
reconsider H = A as  strict (Subgroup of G) by GROUP_3:def 1;
thus ( E "/\" A ) = ( ( (1). G ) /\ H ) by A3
.= E by GROUP_2:85;
hence ( A "/\" E ) = E;
end;
hence LattStr (# ( Subgroups G ),( SubJoin G ),( SubMeet G ) #) is 0_Lattice by LATTICES:def 13;
reconsider F = ( (Omega). G ) as (Element of L) by GROUP_3:def 1;
now
let A being (Element of L);
reconsider H = A as  strict (Subgroup of G) by GROUP_3:def 1;
thus ( F "\/" A ) = ( ( (Omega). G ) "\/" H ) by A1
.= F by CKB74:1;
hence ( A "\/" F ) = F;
end;
hence thesis by LATTICES:def 14;
end;
