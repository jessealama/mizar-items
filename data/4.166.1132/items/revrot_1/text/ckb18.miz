begin
reserve i for (Element of ( NAT ));
theorem
Th16: (for D being non empty set holds (for f being  circular (FinSequence of D) holds (for p being (Element of D) holds ((for i holds ((1 < i & i < ( len f )) implies ( f /. i ) <> ( f /. 1 ))) implies ( Rotate (( Rotate (f,p) ),( f /. 1 )) ) = f))))
proof
let D being non empty set;
let f being  circular (FinSequence of D);
let p being (Element of D);
assume that
A1: (for i holds ((1 < i & i < ( len f )) implies ( f /. i ) <> ( f /. 1 )));
per cases ;
suppose (not p in ( rng f ));

hence ( Rotate (( Rotate (f,p) ),( f /. 1 )) ) = ( Rotate (f,( f /. 1 )) ) by FINSEQ_6:def 2
.= f by FINSEQ_6:89;
end;
suppose p = ( f /. 1 );

hence ( Rotate (( Rotate (f,p) ),( f /. 1 )) ) = ( Rotate (f,( f /. 1 )) ) by FINSEQ_6:93
.= f by FINSEQ_6:89;
end;
suppose that A2: p in ( rng f )
and
A3: p <> ( f /. 1 );
A4: ( f /. 1 ) = ( ( f -: p ) /. 1 ) by A2,FINSEQ_5:44;
A5: ( f /. 1 ) = ( f /. ( len f ) ) by FINSEQ_6:def 1;
then A6: ( f /. 1 ) = ( ( f :- p ) /. ( len ( f :- p ) ) ) by A2,FINSEQ_5:54;
then A7: ( f /. 1 ) in ( rng ( f :- p ) ) by CKB4:1;
A8: ( f :- p ) just_once_values ( f /. 1 )
proof
(( p .. f ) <> 1 & ( p .. f ) >= 1) by A2,A3,FINSEQ_4:21,FINSEQ_5:38;
then A9: ( p .. f ) > 1 by XXREAL_0:1;
take ( len ( f :- p ) );
thus ( len ( f :- p ) ) in ( dom ( f :- p ) ) by FINSEQ_5:6;
thus ( f /. 1 ) = ( ( f :- p ) /. ( len ( f :- p ) ) ) by A2,A5,FINSEQ_5:54;
let z being set;
assume A10: z in ( dom ( f :- p ) );
then reconsider k = z as (Element of ( NAT ));
k <> ( 0 ) by A10,FINSEQ_3:25;
then consider i being Nat such that A11: k = ( i + 1 ) by NAT_1:6;
assume A12: z <> ( len ( f :- p ) );
reconsider i as (Element of ( NAT )) by ORDINAL1:def 12;
k <= ( len ( f :- p ) ) by A10,FINSEQ_3:25;
then k < ( len ( f :- p ) ) by A12,XXREAL_0:1;
then ( i + 1 ) < ( ( ( len f ) - ( p .. f ) ) + 1 ) by A2,A11,FINSEQ_5:50;
then i < ( ( len f ) - ( p .. f ) ) by XREAL_1:6;
then A13: ( i + ( p .. f ) ) < ( len f ) by XREAL_1:20;
( p .. f ) <= ( i + ( p .. f ) ) by NAT_1:11;
then A14: 1 < ( i + ( p .. f ) ) by A9,XXREAL_0:2;
( ( f :- p ) /. ( i + 1 ) ) = ( f /. ( i + ( p .. f ) ) ) by A2,A10,A11,FINSEQ_5:52;
hence thesis by A1,A11,A14,A13;
end;
( f /. 1 ) in ( rng f ) by A2,FINSEQ_6:42,RELAT_1:38;
then A15: ( f /. 1 ) in ( rng ( Rotate (f,p) ) ) by A2,FINSEQ_6:90;
( Rotate (f,p) ) = ( ( f :- p ) ^ ( ( f -: p ) /^ 1 ) ) by A2,FINSEQ_6:def 2;
hence ( Rotate (( Rotate (f,p) ),( f /. 1 )) ) = ( ( ( ( f :- p ) ^ ( ( f -: p ) /^ 1 ) ) :- ( f /. 1 ) ) ^ ( ( ( ( f :- p ) ^ ( ( f -: p ) /^ 1 ) ) -: ( f /. 1 ) ) /^ 1 ) ) by A15,FINSEQ_6:def 2
.= ( ( ( ( f :- p ) :- ( f /. 1 ) ) ^ ( ( f -: p ) /^ 1 ) ) ^ ( ( ( ( f :- p ) ^ ( ( f -: p ) /^ 1 ) ) -: ( f /. 1 ) ) /^ 1 ) ) by A7,FINSEQ_6:64
.= ( ( ( ( f :- p ) :- ( f /. 1 ) ) ^ ( ( f -: p ) /^ 1 ) ) ^ ( ( ( f :- p ) -: ( f /. 1 ) ) /^ 1 ) ) by A7,FINSEQ_6:66
.= ( ( <* ( f /. 1 ) *> ^ ( ( f -: p ) /^ 1 ) ) ^ ( ( ( f :- p ) -: ( f /. 1 ) ) /^ 1 ) ) by A6,A8,CKB7:1
.= ( ( f -: p ) ^ ( ( ( f :- p ) -: ( f /. 1 ) ) /^ 1 ) ) by A2,A4,FINSEQ_5:29,FINSEQ_5:47
.= ( ( f -: p ) ^ ( ( f :- p ) /^ 1 ) ) by A6,A8,CKB6:1
.= f by A2,FINSEQ_6:76;
end;
end;
