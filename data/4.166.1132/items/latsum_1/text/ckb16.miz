theorem
Th12: (for R,S being non empty RelStr holds (for x being set holds (x in ( (the carrier of R) /\ (the carrier of S) ) implies x is (Element of R))))
proof
let R being non empty RelStr,S being non empty RelStr;
let x being set;
assume A1: x in ( (the carrier of R) /\ (the carrier of S) );
( (the carrier of R) /\ (the carrier of S) ) c= (the carrier of R) by XBOOLE_1:17;
hence thesis by A1;
end;
