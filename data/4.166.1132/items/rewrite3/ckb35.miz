begin
reserve x1 for set;
reserve x2 for set;
reserve y1 for set;
reserve y2 for set;
reserve z1 for set;
reserve z2 for set;
reserve E for non empty set;
reserve u9 for (Element of ( E ^omega ));
reserve v1 for (Element of ( E ^omega ));
reserve v2 for (Element of ( E ^omega ));
reserve w1 for (Element of ( E ^omega ));
reserve w2 for (Element of ( E ^omega ));
reserve F for (Subset of ( E ^omega ));
theorem
Th30: (for TS being  deterministic transition-system over F holds ((x1,x2 ==>. y1,z1,TS & x1,x2 ==>. y2,z2,TS) implies (y1 = y2 & z1 = z2)))
proof
let TS being  deterministic transition-system over F;
assume that
A1: x1,x2 ==>. y1,z1,TS
and
A2: x1,x2 ==>. y2,z2,TS;
consider v2,w2 such that A3: v2 = z2 and A4: x1,w2 -->. y2,TS and A5: x2 = ( w2 ^ v2 ) by A2,CKB24:def 1;
consider v1,w1 such that A6: v1 = z1 and A7: x1,w1 -->. y1,TS and A8: x2 = ( w1 ^ v1 ) by A1,CKB24:def 1;
A9: (the Tran of TS) is Function by CKB15:def 1;
((ex u9 st (( w1 ^ u9 ) = w2 & v1 = ( u9 ^ v2 ))) or (ex u9 st (( w2 ^ u9 ) = w1 & v2 = ( u9 ^ v1 )))) by A8,A5,CKB13:1;
then w1 = w2 by A7,A4,CKB23:1;
then v1 = v2 by A8,A5,AFINSQ_1:28;
hence thesis by A1,A2,A6,A3,A9,CKB32:1;
end;
