begin
reserve x for set;
reserve y for set;
reserve E for non  empty set;
reserve u for (Element of ( E ^omega ));
reserve v for (Element of ( E ^omega ));
reserve w for (Element of ( E ^omega ));
reserve F for (Subset of ( E ^omega ));
theorem
Th29: (for TS being transition-system over F holds ((not ( <%> E ) in ( rng ( dom (the Tran of TS) ) )) implies (x,u ==>. y,v,TS implies ( len u ) > ( len v ))))
proof
let TS being transition-system over F;
assume that
A1: (not ( <%> E ) in ( rng ( dom (the Tran of TS) ) ));
assume A2: x,u ==>. y,v,TS;
then consider w such that A3: x,w -->. y,TS and A4: u = ( w ^ v ) by CKB27:1;
A5: w in ( rng ( dom (the Tran of TS) ) ) by A3,CKB19:1;
per cases ;
suppose A6: v = ( <%> E );

then u <> ( <%> E ) by A1,A2,CKB33:1;
then ( len u ) > ( 0 );
hence thesis by A6;
end;
suppose v <> ( <%> E );

hence thesis by A1,A4,A5,CKB10:1;
end;
end;
