begin
reserve x for set;
reserve y for set;
reserve z for set;
reserve E for non  empty set;
reserve v for (Element of ( E ^omega ));
reserve w for (Element of ( E ^omega ));
reserve F for (Subset of ( E ^omega ));
theorem
Th23: (for TS being transition-system over F holds (x,y -->. z,TS iff x,y ==>. z,( <%> E ),TS))
proof
let TS being transition-system over F;
thus (x,y -->. z,TS implies x,y ==>. z,( <%> E ),TS)
proof
assume A1: x,y -->. z,TS;
then y in F by CKB19:1;
then reconsider w = y as (Element of ( E ^omega ));
w = ( w ^ ( <%> E ) ) by AFINSQ_1:29;
hence thesis by A1,CKB24:def 1;
end;

assume x,y ==>. z,( <%> E ),TS;
then (ex v st (ex w st ((v = ( <%> E ) & x,w -->. z,TS) & y = ( w ^ v )))) by CKB24:def 1;
hence thesis by AFINSQ_1:29;
end;
