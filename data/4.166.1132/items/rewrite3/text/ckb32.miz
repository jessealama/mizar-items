begin
reserve x1 for set;
reserve x2 for set;
reserve y1 for set;
reserve y2 for set;
reserve z for set;
reserve E for non  empty set;
reserve v1 for (Element of ( E ^omega ));
reserve v2 for (Element of ( E ^omega ));
reserve w1 for (Element of ( E ^omega ));
reserve w2 for (Element of ( E ^omega ));
reserve F for (Subset of ( E ^omega ));
theorem
Th27: (for TS being transition-system over F holds ((the Tran of TS) is Function implies ((x1,x2 ==>. y1,z,TS & x1,x2 ==>. y2,z,TS) implies y1 = y2)))
proof
let TS being transition-system over F;
assume that
A1: (the Tran of TS) is Function;
assume that
A2: x1,x2 ==>. y1,z,TS
and
A3: x1,x2 ==>. y2,z,TS;
consider v1,w1 such that A4: v1 = z and A5: x1,w1 -->. y1,TS and A6: x2 = ( w1 ^ v1 ) by A2,CKB24:def 1;
consider v2,w2 such that A7: v2 = z and A8: x1,w2 -->. y2,TS and A9: x2 = ( w2 ^ v2 ) by A3,CKB24:def 1;
w1 = w2 by A4,A6,A7,A9,AFINSQ_1:28;
hence thesis by A1,A5,A8,CKB21:1;
end;
