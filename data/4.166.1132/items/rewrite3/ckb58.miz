registrations CARD_1,RELSET_1,NAT_1,XREAL_0,XBOOLE_0,MEMBERED,XXREAL_0,STRUCT_0,SUBSET_1,REWRITE1,AFINSQ_1,RELAT_1,FUNCT_1,FINSEQ_1,CATALAN2,CKB14,CKB17;
constructors XXREAL_0,NAT_1,FSM_1,MEMBERED,REWRITE1,FLANG_1,XREAL_0,RELSET_1,CKB14,CKB15,CKB18,CKB24,CKB36,CKB53;
requirements NUMERALS,REAL,BOOLE,SUBSET,ARITHM;
begin
reserve x for set;
reserve x1 for set;
reserve y1 for set;
reserve E for non empty set;
reserve u for (Element of ( E ^omega ));
reserve F for (Subset of ( E ^omega ));
reserve k for Nat;
reserve l for Nat;
reserve TS for non empty transition-system over F;
theorem
Th51: (for P being (RedSequence of ( ==>.-relation TS )) holds ((ex x st (ex u st ( P . 1 ) = [ x,u ])) implies (for k holds (k in ( dom P ) implies ( dim2 (( P . k ),E) ) = ( ( P . k ) `2 )))))
proof
let P being (RedSequence of ( ==>.-relation TS ));
given x,u such that
A1: ( P . 1 ) = [ x,u ];

let k;
assume that
A2: k in ( dom P );
per cases ;
suppose A3: k > 1;

A4: k <= ( len P ) by A2,FINSEQ_3:25;
consider l such that A5: k = ( l + 1 ) by A3,NAT_1:6;
l <= k by A5,NAT_1:11;
then A6: l <= ( len P ) by A4,XXREAL_0:2;
l >= 1 by A3,A5,NAT_1:19;
then l in ( dom P ) by A6,FINSEQ_3:25;
then [ ( P . l ),( P . k ) ] in ( ==>.-relation TS ) by A2,A5,REWRITE1:def 2;
then A7: ( P . k ) in ( rng ( ==>.-relation TS ) ) by RELAT_1:6;
( rng ( ==>.-relation TS ) ) c= [: (the carrier of TS),( E ^omega ) :] by RELAT_1:def 19;
then (ex x1 st (ex y1 st ((x1 in (the carrier of TS) & y1 in ( E ^omega )) & ( P . k ) = [ x1,y1 ]))) by A7,ZFMISC_1:def 2;
hence thesis by CKB53:def 1;
end;
suppose A8: k <= 1;

k >= 1 by A2,FINSEQ_3:25;
then k = 1 by A8,XXREAL_0:1;
hence thesis by A1,CKB53:def 1;
end;
end;
