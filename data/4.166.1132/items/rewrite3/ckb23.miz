registrations CARD_1,RELSET_1,NAT_1,XREAL_0,XBOOLE_0,MEMBERED,XXREAL_0,STRUCT_0,SUBSET_1,REWRITE1,AFINSQ_1,RELAT_1,FUNCT_1,FINSEQ_1,CATALAN2,CKB14,CKB17;
constructors XXREAL_0,NAT_1,FSM_1,MEMBERED,REWRITE1,FLANG_1,XREAL_0,RELSET_1,CKB14,CKB15,CKB18;
requirements NUMERALS,REAL,BOOLE,SUBSET,ARITHM;
begin
reserve x for set;
reserve z1 for set;
reserve z2 for set;
reserve E for non empty set;
reserve u for (Element of ( E ^omega ));
reserve v for (Element of ( E ^omega ));
reserve w for (Element of ( E ^omega ));
reserve F for (Subset of ( E ^omega ));
theorem
Th19: (for TS being  deterministic transition-system over F holds (((u <> v & x,u -->. z1,TS) & x,v -->. z2,TS) implies (not (ex w st (( u ^ w ) = v or ( v ^ w ) = u)))))
proof
let TS being  deterministic transition-system over F;
assume that
A1: u <> v
and
A2: x,u -->. z1,TS
and
A3: x,v -->. z2,TS;
x in TS by A2,CKB19:1;
then reconsider x as (Element of TS) by STRUCT_0:def 5;
[ [ x,v ],z2 ] in (the Tran of TS) by A3,CKB18:def 1;
then A4: [ x,v ] in ( dom (the Tran of TS) ) by RELAT_1:6;
[ [ x,u ],z1 ] in (the Tran of TS) by A2,CKB18:def 1;
then [ x,u ] in ( dom (the Tran of TS) ) by RELAT_1:6;
hence thesis by A1,A4,CKB15:def 1;
end;
