begin
reserve s1 for Real;
reserve t1 for Real;
reserve s2 for Real;
reserve t2 for Real;
reserve s for Real;
reserve t for Real;
theorem
Th29: (for s1 holds (for t1 holds (for s2 holds (for t2 holds (for P being (Subset of ( TOP-REAL 2 )) holds (P = { |[ s,t ]|: (((s1 < s & s < s2) & t1 < t) & t < t2) } implies P is  open))))))
proof
let s1;
let t1;
let s2;
let t2;
let P being (Subset of ( TOP-REAL 2 ));
assume A1: P = { |[ s,t ]|: (((s1 < s & s < s2) & t1 < t) & t < t2) };
reconsider P1 = { |[ s,t ]|: s1 < s } as (Subset of ( TOP-REAL 2 )) by CKB10:1,CKB13:1;
reconsider P2 = { |[ s,t ]|: s < s2 } as (Subset of ( TOP-REAL 2 )) by CKB10:1,CKB11:1;
reconsider P3 = { |[ s,t ]|: t1 < t } as (Subset of ( TOP-REAL 2 )) by CKB10:1,CKB14:1;
reconsider P4 = { |[ s,t ]|: t < t2 } as (Subset of ( TOP-REAL 2 )) by CKB10:1,CKB12:1;
A2: P = ( ( ( P1 /\ P2 ) /\ P3 ) /\ P4 ) by A1,CKB17:1;
A3: P1 is  open by CKB31:1;
P2 is  open by CKB32:1;
then A4: ( P1 /\ P2 ) is  open by A3,TOPS_1:11;
A5: P3 is  open by CKB33:1;
A6: P4 is  open by CKB34:1;
( ( P1 /\ P2 ) /\ P3 ) is  open by A4,A5,TOPS_1:11;
hence thesis by A2,A6,TOPS_1:11;
end;
