begin
reserve S for non  void non  empty ManySortedSign;
reserve A for  non-empty MSAlgebra over S;
reserve V for (Variables of A);
reserve t for (c-Term of A,V);
reserve f for (ManySortedFunction of V,(the Sorts of A));
theorem
Th32: (for s being (SortSymbol of S) holds (for v being (Element of ( V . s )) holds (t = ( root-tree [ v,s ] ) implies ( root-tree ( ( f . s ) . v ) ) is_an_evaluation_of t,f)))
proof
let s being (SortSymbol of S);
let x being (Element of ( V . s ));
assume that
A1: t = ( root-tree [ x,s ] );
set vt = ( root-tree ( ( f . s ) . x ) );
A2: ( dom vt ) = ( elementary_tree ( 0 ) ) by TREES_4:3;
hence ( dom vt ) = ( dom t ) by A1,TREES_4:3;
let p being (Node of vt);
reconsider e = p as  empty (FinSequence of ( NAT )) by A2,TARSKI:def 1,TREES_1:29;
A3: ( t . ( {} ) ) = [ x,s ] by A1,TREES_4:3;
hereby
let s1 being (SortSymbol of S);
let x1 being (Element of ( V . s1 ));
A4: e = p;
assume ( t . p ) = [ x1,s1 ];
then A5: [ x1,s1 ] = ( t . e );
then A6: s = s1 by A3,ZFMISC_1:27;
x = x1 by A3,A5,ZFMISC_1:27;
hence ( vt . p ) = ( ( f . s1 ) . x1 ) by A6,A4,TREES_4:3;
end;
hereby
let s1 being (SortSymbol of S);
let v being (Element of ( (the Sorts of A) . s1 ));
assume ( t . p ) = [ v,s1 ];
then A7: [ v,s1 ] = ( t . e )
.= [ x,s ] by A1,TREES_4:3;
then A8: x = v by ZFMISC_1:27;
s = s1 by A7,ZFMISC_1:27;
hence ( vt . p ) = v by A8,CKB54:1;
end;
let o being (OperSymbol of S);
assume ( t . p ) = [ o,(the carrier of S) ];
then (the carrier of S) = ( ( t . e ) `2 ) by MCART_1:7
.= s by A3,MCART_1:7;
hence ( vt . p ) = ( ( Den (o,A) ) . ( succ (vt,p) ) ) by CKB30:1;
end;
