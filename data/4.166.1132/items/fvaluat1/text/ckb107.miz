begin
reserve x for ExtInt;
reserve y for ExtInt;
registration
let R being Ring;
let M1 being (RightMod of R);
let M2 being (Submodule of M1);
cluster ( incl (M2,M1) ) ->  additive  scalar-linear;
coherence
proof
set h = ( incl (M2,M1) );
(the carrier of M2) c= (the carrier of M1) by RMOD_2:def 2;
then A1: h = ( id (the carrier of M2) ) by YELLOW_9:def 1;
thus ( incl (M2,M1) ) is  additive
proof
let x being (Element of M2);
let y being (Element of M2);
((( h . x ) = x & ( h . y ) = y) & ( h . ( x + y ) ) = ( x + y )) by A1,FUNCT_1:17;
hence thesis by RMOD_2:13;
end;

let x being (Element of M2);
let r being (Element of R);
(( h . x ) = x & ( h . ( x * r ) ) = ( x * r )) by A1,FUNCT_1:17;
hence thesis by RMOD_2:14;
end;
end;
