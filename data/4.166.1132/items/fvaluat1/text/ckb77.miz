theorem
Th51: (for K being non  empty doubleLoopStr holds (for v being (Valuation of K) holds ( NonNegElements v ) c= (the carrier of K)))
proof
let K being non  empty doubleLoopStr;
let v being (Valuation of K);
let a being set;
assume a in ( NonNegElements v );
then (ex x being (Element of K) st (a = x & ( 0 ) <= ( v . x )));
hence thesis;
end;
