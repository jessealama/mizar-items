begin
reserve i for Integer;
definition
let X being  ext-real-membered set;
given i0 being  positive ExtInt such that
A1: i0 in X;

func least-positive X ->  positive ExtInt means 
:Def4: (it in X & (for i being  positive ExtInt holds (i in X implies it <= i)));
existence
proof
defpred P[ Integer ]
 means
($1 in X & $1 is  positive);
per cases ;
suppose (ex i being  positive Integer st i in X);

then A2: (ex i being Integer st P[ i ]);
A3: (for i being Integer holds (P[ i ] implies ( 0 ) <= i));
consider j0 being Integer such that A4: P[ j0 ] and A5: (for i being Integer holds (P[ i ] implies j0 <= i)) from INT_1:sch 5(A3,A2);
reconsider j = j0 as  positive ExtInt by A4;
take j;
thus j in X by A4;
let i being  positive ExtInt;
assume A6: i in X;
per cases ;
suppose i is Integer;

then reconsider i1 = i as Integer;
j0 <= i1 by A6,A5;
hence j <= i;
end;
suppose (not i is Integer);

then i = ( +infty ) by CKB10:def 1;
hence thesis by XXREAL_0:3;
end;
end;
suppose A7: (not (ex i being  positive Integer st i in X));

take i0;
thus i0 in X by A1;
let i being  positive ExtInt;
assume i in X;
then ((not i is  positive  positive Integer) & ( +infty ) <= ( +infty )) by A7;
then i = ( +infty ) by CKB10:def 1;
hence i0 <= i by XXREAL_0:3;
end;
end;
uniqueness
proof
let a being  positive ExtInt;
let b being  positive ExtInt;
assume (((a in X & (for i being  positive ExtInt holds (i in X implies a <= i))) & b in X) & (for i being  positive ExtInt holds (i in X implies b <= i)));
then (a <= b & b <= a);
hence thesis by XXREAL_0:1;
end;
end;
