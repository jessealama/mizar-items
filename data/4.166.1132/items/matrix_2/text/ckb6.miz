constructors RELAT_2,PARTFUN1,BINOP_1,FINSUB_1,XXREAL_0,NAT_1,NAT_D,FINSEQOP,FINSEQ_4,GROUP_4,MATRIX_1,VECTSP_1,RELSET_1,CKB1,CKB2,CKB5;
requirements NUMERALS,REAL,BOOLE,SUBSET,ARITHM;
begin
reserve x1 for set;
reserve x2 for set;
reserve y1 for set;
reserve y2 for set;
theorem
Th3: ((( len ( (x1,x2) ][ (y1,y2) ) ) = 2 & ( width ( (x1,x2) ][ (y1,y2) ) ) = 2) & ( Indices ( (x1,x2) ][ (y1,y2) ) ) = [: ( Seg 2 ),( Seg 2 ) :])
proof
set M = ( (x1,x2) ][ (y1,y2) );
thus A1: ( len M ) = 2 by FINSEQ_1:44;
( rng M ) = { <* x1,x2 *>,<* y1,y2 *> } by FINSEQ_2:127;
then A2: <* x1,x2 *> in ( rng M ) by TARSKI:def 2;
( len <* x1,x2 *> ) = 2 by FINSEQ_1:44;
hence ( width M ) = 2 by A1,A2,MATRIX_1:def 3;
hence thesis by A1,FINSEQ_1:def 3;
end;
