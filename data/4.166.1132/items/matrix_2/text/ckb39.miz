begin
reserve y for set;
reserve n for Nat;
definition
let P being  permutational non empty set;
redefine mode Element of P
 -> (Permutation of ( Seg ( len P ) ));
coherence
proof
let x being (Element of P);
consider n such that A1: (for y holds (y in P implies y is (Permutation of ( Seg n )))) by CKB35:def 1;
reconsider n as (Element of ( NAT )) by ORDINAL1:def 12;
reconsider q = x as (Permutation of ( Seg n )) by A1;
A2: ( dom q ) = ( Seg n ) by FUNCT_2:52;
then reconsider q as FinSequence by FINSEQ_1:def 2;
( len q ) = n by A2,FINSEQ_1:def 3;
then ( Seg ( len P ) ) = ( Seg n ) by CKB37:def 1;
hence thesis by A1;
end;
end;
