begin
definition
let i being (Element of ( NAT ));
let x being (FinSequence of ( REAL ));
func reproj (i,x) -> Function means 
:Def5: (( dom it ) = ( REAL ) & (for r being (Element of ( REAL )) holds ( it . r ) = ( Replace (x,i,r) )));
existence
proof
deffunc F((Element of ( REAL ))) = ( Replace (x,i,$1) );
consider f being Function such that A1: (( dom f ) = ( REAL ) & (for r being (Element of ( REAL )) holds (r in ( REAL ) implies ( f . r ) = F(r)))) from GRAPH_5:sch 1;
take f;
thus thesis by A1;
end;
uniqueness
proof
let f being Function;
let g being Function;
assume that
A2: ( dom f ) = ( REAL )
and
A3: (for r being (Element of ( REAL )) holds ( f . r ) = ( Replace (x,i,r) ))
and
A4: ( dom g ) = ( REAL )
and
A5: (for r being (Element of ( REAL )) holds ( g . r ) = ( Replace (x,i,r) ));
now
let p being set;
assume p in ( dom f );
then reconsider r = p as (Element of ( REAL )) by A2;
( f . r ) = ( Replace (x,i,r) ) by A3;
hence ( f . p ) = ( g . p ) by A5;
end;
hence thesis by A2,A4,FUNCT_1:2;
end;
