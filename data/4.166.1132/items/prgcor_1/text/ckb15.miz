theorem
(for n,m being Integer holds ( idiv_prg (n,m) ) = ( n div m ))
proof
let n being Integer,m being Integer;
per cases ;
suppose A1: m = ( 0 );

then ( idiv_prg (n,m) ) = ( 0 ) by CKB14:def 1;
hence thesis by A1,INT_1:48;
end;
suppose A2: m <> ( 0 );

now
per cases ;
case A3: n >= ( 0 );
now
per cases  by A2;
case A4: m > ( 0 );
hence ( idiv_prg (n,m) ) = ( idiv1_prg (n,m) ) by A3,CKB14:def 1
.= ( n div m ) by A3,A4,CKB12:1;
end;
case A5: m < ( 0 );
now
per cases ;
case A6: ( ( - m ) * ( idiv1_prg (n,( - m )) ) ) = n;
reconsider m2 = ( - m ),n2 = n as (Element of ( NAT )) by A3,A5,INT_1:3;
( - m ) > ( 0 ) by A5,XREAL_1:58;
then A7: ( idiv1_prg (n,( - m )) ) = ( n2 div m2 ) by CKB11:1;
( idiv_prg (n,m) ) = ( - ( idiv1_prg (n,( - m )) ) ) by A3,A5,A6,CKB14:def 1;
hence thesis by A5,A6,A7,CKB13:1;
end;
case A8: ( ( - m ) * ( idiv1_prg (n,( - m )) ) ) <> n;
reconsider m2 = ( - m ),n2 = n as (Element of ( NAT )) by A3,A5,INT_1:3;
( - m ) > ( 0 ) by A5,XREAL_1:58;
then A9: ( idiv1_prg (n,( - m )) ) = ( n2 div m2 ) by CKB11:1;
( idiv_prg (n,m) ) = ( ( - ( idiv1_prg (n,( - m )) ) ) - 1 ) by A3,A5,A8,CKB14:def 1;
hence thesis by A5,A8,A9,CKB13:1;
end;
end;
hence thesis;
end;
end;
hence thesis;
end;
case A10: n < ( 0 );
now
per cases  by A2;
case A11: m < ( 0 );
then A12: ( - m ) > ( 0 ) by XREAL_1:58;
A13: ( n div m ) = [\ ( n / m ) /] by INT_1:def 9
.= [\ ( ( - n ) / ( - m ) ) /] by XCMPLX_1:191
.= ( ( - n ) div ( - m ) ) by INT_1:def 9;
( idiv_prg (n,m) ) = ( idiv1_prg (( - n ),( - m )) ) by A10,A11,CKB14:def 1
.= ( ( - n ) div ( - m ) ) by A10,A12,CKB12:1;
hence thesis by A13;
end;
case A14: m > ( 0 );
now
per cases ;
case A15: ( m * ( idiv1_prg (( - n ),m) ) ) = ( - n );
reconsider m2 = m,n2 = ( - n ) as (Element of ( NAT )) by A10,A14,INT_1:3;
A16: ( idiv1_prg (( - n ),m) ) = ( n2 div m2 ) by A14,CKB11:1;
( idiv_prg (n,m) ) = ( - ( idiv1_prg (( - n ),m) ) ) by A10,A14,A15,CKB14:def 1;
hence thesis by A10,A14,A15,A16,CKB13:1;
end;
case A17: ( m * ( idiv1_prg (( - n ),m) ) ) <> ( - n );
reconsider m2 = m,n2 = ( - n ) as (Element of ( NAT )) by A10,A14,INT_1:3;
A18: ( idiv1_prg (( - n ),m) ) = ( n2 div m2 ) by A14,CKB11:1;
( idiv_prg (n,m) ) = ( ( - ( idiv1_prg (( - n ),m) ) ) - 1 ) by A10,A14,A17,CKB14:def 1;
hence thesis by A10,A14,A17,A18,CKB13:1;
end;
end;
hence thesis;
end;
end;
hence thesis;
