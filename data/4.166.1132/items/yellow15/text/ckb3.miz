begin
theorem
Th2: (for i being Nat holds (for D being  finite set holds ( i -tuples_on D ) is  finite))
proof
let i being Nat;
let D being  finite set;
A1: i is (Element of ( NAT )) by ORDINAL1:def 12;
per cases ;
suppose A2: D is non  empty;

A3:now
let x being set;
assume x in ( dom ( i |-> D ) );
then x in ( Seg i ) by FUNCOP_1:13;
hence ( ( i |-> D ) . x ) is  finite by FUNCOP_1:7;
end;
( product ( i |-> D ) ) = ( i -tuples_on D ) by A1,A2,FINSEQ_3:131;
hence thesis by A3,MSSCYC_1:1;
end;
suppose A4: D is  empty;

now
per cases ;
suppose i <> ( 0 );

hence thesis by A4,FINSEQ_3:119;
end;
suppose i = ( 0 );

then ( i -tuples_on D ) = { ( <*> D ) } by FINSEQ_2:94;
hence thesis;
end;
end;
hence thesis;
end;
end;
