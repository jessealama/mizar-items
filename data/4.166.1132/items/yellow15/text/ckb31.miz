begin
theorem
(for X being non  empty set holds (for Y being  finite (Subset-Family of X) holds (Y is  in_general_position implies ( Components Y ) is (a_partition of X))))
proof
let X being non  empty set;
let Y being  finite (Subset-Family of X);
assume Y is  in_general_position;
then A1: (for A being (Subset of X) holds (A in ( Components Y ) implies (A <> ( {} ) & (for B being (Subset of X) holds (B in ( Components Y ) implies (A = B or A misses B)))))) by CKB28:def 1,CKB27:1;
( union ( Components Y ) ) = X by CKB26:1;
hence thesis by A1,EQREL_1:def 4;
end;
