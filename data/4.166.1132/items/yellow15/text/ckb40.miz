begin
theorem
(for T being  T_0 TopSpace holds (T is  infinite implies (for B being (Basis of T) holds B is  infinite)))
proof
let T being  T_0 TopSpace;
assume A1: T is  infinite;
let B being (Basis of T);
assume B is  finite;
then reconsider B1 = B as  finite (Subset-Family of T);
( union ( Components B1 ) ) = (the carrier of T) by CKB26:1;
then consider X being set such that A2: X in ( Components B1 ) and A3: X is  infinite by A1,FINSET_1:7;
reconsider X as  infinite set by A3;
consider x being set such that A4: x in X by XBOOLE_0:def 1;
consider y being set such that A5: y in X and A6: x <> y by A4,CKB7:1;
reconsider x1 = x,y1 = y as (Element of T) by A2,A4,A5;
consider Y being (Subset of T) such that A7: Y is  open and A8: ((x1 in Y & (not y1 in Y)) or (y1 in Y & (not x1 in Y))) by A1,A6,T_0TOPSP:def 7;
now
per cases  by A8;
suppose A9: (x in Y & (not y in Y));

then x in ( Y /\ X ) by A4,XBOOLE_0:def 4;
then Y meets X by XBOOLE_0:4;
then X c= Y by A2,A7,CKB39:1;
hence contradiction by A5,A9;
end;
suppose A10: (y in Y & (not x in Y));

then y in ( Y /\ X ) by A5,XBOOLE_0:def 4;
then Y meets X by XBOOLE_0:4;
then X c= Y by A2,A7,CKB39:1;
hence contradiction by A4,A10;
end;
end;
hence contradiction;
end;
