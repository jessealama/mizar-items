begin
reserve X for non  empty set;
reserve Y for ComplexLinearSpace;
reserve f for (Element of ( Funcs (X,(the carrier of Y)) ));
reserve g for (Element of ( Funcs (X,(the carrier of Y)) ));
reserve h for (Element of ( Funcs (X,(the carrier of Y)) ));
theorem
Th44: (for X,Y being ComplexNormSpace holds (for f,g,h being (Point of ( C_NormSpace_of_BoundedLinearOperators (X,Y) )) holds (h = ( f - g ) iff (for x being (VECTOR of X) holds ( h . x ) = ( ( f . x ) - ( g . x ) )))))
proof
let X being ComplexNormSpace;
let Y being ComplexNormSpace;
let f being (Point of ( C_NormSpace_of_BoundedLinearOperators (X,Y) ));
let g being (Point of ( C_NormSpace_of_BoundedLinearOperators (X,Y) ));
let h being (Point of ( C_NormSpace_of_BoundedLinearOperators (X,Y) ));
reconsider f9 = f,g9 = g,h9 = h as  bounded (LinearOperator of X,Y) by CKB40:def 1;
hereby
assume h = ( f - g );
then ( h + g ) = ( f - ( g - g ) ) by RLVECT_1:29;
then ( h + g ) = ( f - ( 0. ( C_NormSpace_of_BoundedLinearOperators (X,Y) ) ) ) by RLVECT_1:15;
then A1: ( h + g ) = f by RLVECT_1:13;
now
let x being (VECTOR of X);
( f9 . x ) = ( ( h9 . x ) + ( g9 . x ) ) by A1,CKB66:1;
then ( ( f9 . x ) - ( g9 . x ) ) = ( ( h9 . x ) + ( ( g9 . x ) - ( g9 . x ) ) ) by RLVECT_1:def 3;
then ( ( f9 . x ) - ( g9 . x ) ) = ( ( h9 . x ) + ( 0. Y ) ) by RLVECT_1:15;
hence ( ( f9 . x ) - ( g9 . x ) ) = ( h9 . x ) by RLVECT_1:4;
end;
hence (for x being (VECTOR of X) holds ( h . x ) = ( ( f . x ) - ( g . x ) ));
end;
assume A2: (for x being (VECTOR of X) holds ( h . x ) = ( ( f . x ) - ( g . x ) ));
now
let x being (VECTOR of X);
( h9 . x ) = ( ( f9 . x ) - ( g9 . x ) ) by A2;
then ( ( h9 . x ) + ( g9 . x ) ) = ( ( f9 . x ) - ( ( g9 . x ) - ( g9 . x ) ) ) by RLVECT_1:29;
then ( ( h9 . x ) + ( g9 . x ) ) = ( ( f9 . x ) - ( 0. Y ) ) by RLVECT_1:15;
hence ( ( h9 . x ) + ( g9 . x ) ) = ( f9 . x ) by RLVECT_1:13;
end;
then f = ( h + g ) by CKB66:1;
then ( f - g ) = ( h + ( g - g ) ) by RLVECT_1:def 3;
then ( f - g ) = ( h + ( 0. ( C_NormSpace_of_BoundedLinearOperators (X,Y) ) ) ) by RLVECT_1:15;
hence thesis by RLVECT_1:4;
end;
