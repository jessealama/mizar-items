environ
vocabularies NUMBERS,PRE_TOPC,EUCLID,FINSEQ_1,REAL_1,SUBSET_1,GOBOARD1,NAT_1,XBOOLE_0,CARD_1,ARYTM_3,XXREAL_0,TARSKI,ARYTM_1,RELAT_1,ORDINAL4,PARTFUN1,FUNCT_1,RLTOPSP1,TOPREAL1,MCART_1,MATRIX_1,INCSP_1,ZFMISC_1,TREES_1,ORDINAL2,COMPLEX1,STRUCT_0,GOBOARD2;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,ORDINAL1,CARD_1,NUMBERS,XCMPLX_0,XREAL_0,COMPLEX1,REAL_1,NAT_1,RELAT_1,FUNCT_1,PARTFUN1,FINSEQ_1,SEQ_1,VALUED_0,STRUCT_0,PRE_TOPC,SEQ_4,MATRIX_1,RLTOPSP1,EUCLID,TOPREAL1,GOBOARD1,XXREAL_0,CKB15;
definitions TARSKI,TOPREAL1,GOBOARD1,XBOOLE_0,CKB15;
theorems TARSKI,NAT_1,ZFMISC_1,FUNCT_1,FINSEQ_1,ABSVALUE,SEQ_4,FINSEQ_2,MATRIX_1,EUCLID,TOPREAL1,TOPREAL3,GOBOARD1,FINSEQ_4,FINSEQ_3,PARTFUN2,INT_1,XBOOLE_0,XBOOLE_1,XREAL_1,COMPLEX1,XXREAL_0,ORDINAL1,PARTFUN1,SEQM_3,RLTOPSP1,CKB1,CKB2,CKB3,CKB4,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB13,CKB14,CKB15;
schemes NAT_1,MATRIX_1,FINSEQ_4;
registrations RELSET_1,FINSET_1,NUMBERS,XXREAL_0,XREAL_0,NAT_1,MEMBERED,FINSEQ_1,STRUCT_0,EUCLID,GOBOARD1,VALUED_0,CARD_1,XXREAL_2,RELAT_1,FUNCT_1,SEQM_3,SEQ_4,CKB11,CKB12;
constructors PARTFUN1,SQUARE_1,NAT_1,COMPLEX1,SEQ_4,TOPREAL1,GOBOARD1,SEQ_1,XXREAL_2,SEQM_3,RELSET_1,DOMAIN_1,BINOP_2,RVSUM_1,CKB15;
requirements NUMERALS,REAL,BOOLE,SUBSET,ARITHM;
begin
reserve n for (Element of ( NAT ));
reserve i for (Element of ( NAT ));
reserve j for (Element of ( NAT ));
registration
let v1 being non  empty (FinSequence of ( REAL ));
let v2 being non  empty (FinSequence of ( REAL ));
cluster ( GoB (v1,v2) ) -> non  empty-yielding  X_equal-in-line  Y_equal-in-column;
coherence
proof
set M = ( GoB (v1,v2) );
A1: ( len M ) = ( len v1 ) by CKB15:def 1;
then A2: ( dom M ) = ( dom v1 ) by FINSEQ_3:29;
A3: ( width M ) = ( len v2 ) by CKB15:def 1;
hence M is non  empty-yielding by A1,GOBOARD1:def 3;
A4: ( Indices M ) = [: ( dom v1 ),( Seg ( len v2 ) ) :] by A3,A2,MATRIX_1:def 4;
thus M is  X_equal-in-line
proof
let n;
reconsider l = ( Line (M,n) ) as (FinSequence of ( TOP-REAL 2 ));
set x = ( X_axis l );
assume A5: n in ( dom M );
A6: (( len l ) = ( width M ) & ( dom x ) = ( Seg ( len x ) )) by FINSEQ_1:def 3,MATRIX_1:def 7;
A7: ( len x ) = ( len l ) by GOBOARD1:def 1;
then A8: ( dom x ) = ( dom l ) by FINSEQ_3:29;
now
let i;
let j;
assume that
A9: i in ( dom x )
and
A10: j in ( dom x );
reconsider r = ( v1 . n ),s1 = ( v2 . i ),s2 = ( v2 . j ) as Real;
[ n,i ] in ( Indices M ) by A3,A2,A4,A5,A7,A6,A9,ZFMISC_1:87;
then ( M * (n,i) ) = |[ r,s1 ]| by CKB15:def 1;
then A11: ( ( M * (n,i) ) `1 ) = r by EUCLID:52;
( l /. i ) = ( l . i ) by A8,A9,PARTFUN1:def 6;
then ( l /. i ) = ( M * (n,i) ) by A7,A6,A9,MATRIX_1:def 7;
then A12: ( x . i ) = r by A9,A11,GOBOARD1:def 1;
[ n,j ] in ( Indices M ) by A3,A2,A4,A5,A7,A6,A10,ZFMISC_1:87;
then ( M * (n,j) ) = |[ r,s2 ]| by CKB15:def 1;
then A13: ( ( M * (n,j) ) `1 ) = r by EUCLID:52;
( l /. j ) = ( l . j ) by A8,A10,PARTFUN1:def 6;
then ( l /. j ) = ( M * (n,j) ) by A7,A6,A10,MATRIX_1:def 7;
hence ( x . i ) = ( x . j ) by A10,A13,A12,GOBOARD1:def 1;
end;
hence thesis by SEQM_3:def 10;
end;

thus M is  Y_equal-in-column
proof
let n;
reconsider c = ( Col (M,n) ) as (FinSequence of ( TOP-REAL 2 ));
set y = ( Y_axis c );
( len y ) = ( len c ) by GOBOARD1:def 2;
then A14: ( dom y ) = ( dom c ) by FINSEQ_3:29;
( len c ) = ( len M ) by MATRIX_1:def 8;
then A15: ( dom c ) = ( dom M ) by FINSEQ_3:29;
assume A16: n in ( Seg ( width M ) );
now
let i;
let j;
assume that
A17: i in ( dom y )
and
A18: j in ( dom y );
reconsider r = ( v2 . n ),s1 = ( v1 . i ),s2 = ( v1 . j ) as Real;
[ i,n ] in ( Indices M ) by A3,A2,A4,A16,A14,A15,A17,ZFMISC_1:87;
then ( M * (i,n) ) = |[ s1,r ]| by CKB15:def 1;
then A19: ( ( M * (i,n) ) `2 ) = r by EUCLID:52;
( c /. i ) = ( c . i ) by A14,A17,PARTFUN1:def 6;
then ( c /. i ) = ( M * (i,n) ) by A14,A15,A17,MATRIX_1:def 8;
then A20: ( y . i ) = r by A17,A19,GOBOARD1:def 2;
[ j,n ] in ( Indices M ) by A3,A2,A4,A16,A14,A15,A18,ZFMISC_1:87;
then ( M * (j,n) ) = |[ s2,r ]| by CKB15:def 1;
then A21: ( ( M * (j,n) ) `2 ) = r by EUCLID:52;
( c /. j ) = ( c . j ) by A14,A18,PARTFUN1:def 6;
then ( c /. j ) = ( M * (j,n) ) by A14,A15,A18,MATRIX_1:def 8;
hence ( y . i ) = ( y . j ) by A18,A21,A20,GOBOARD1:def 2;
end;
hence thesis by SEQM_3:def 10;
end;

end;
end;
