environ
vocabularies FUNCT_1,RELAT_1,XBOOLE_0,TARSKI,SUBSET_1,ZFMISC_1,RELAT_2,PARTFUN1;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,RELAT_1,RELAT_2,RELSET_1,FUNCT_1,CKB5,CKB11,CKB18,CKB29;
definitions TARSKI,FUNCT_1,XBOOLE_0,RELAT_1,RELAT_2,CKB29;
theorems TARSKI,FUNCT_1,GRFUNC_1,ZFMISC_1,RELAT_1,RELSET_1,XBOOLE_0,XBOOLE_1,RELAT_2,CKB1,CKB2,CKB6,CKB7,CKB8,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB37,CKB38,CKB39,CKB40,CKB41,CKB42,CKB43,CKB44,CKB45;
schemes FUNCT_1,XBOOLE_0,CKB3,CKB9,CKB10;
registrations XBOOLE_0,SUBSET_1,RELAT_1,FUNCT_1,RELSET_1,CKB4;
constructors FUNCT_1,RELAT_2,RELSET_1,CKB11,CKB18,CKB29;
requirements SUBSET,BOOLE;
begin
reserve x for set;
reserve y for set;
reserve X for set;
reserve Y for set;
theorem
(for f being Function holds (f is  one-to-one implies ( <: f,X,Y :> " ) = <: ( f " ),Y,X :>))
proof
let f being Function;
assume A1: f is  one-to-one;
then A2: <: f,X,Y :> is  one-to-one by CKB45:1;
(y in ( dom ( <: f,X,Y :> " ) ) iff y in ( dom <: ( f " ),Y,X :> ))
proof
thus (y in ( dom ( <: f,X,Y :> " ) ) implies y in ( dom <: ( f " ),Y,X :> ))
proof
assume y in ( dom ( <: f,X,Y :> " ) );
then A3: y in ( rng <: f,X,Y :> ) by A2,FUNCT_1:33;
then consider x such that A4: x in ( dom <: f,X,Y :> ) and A5: y = ( <: f,X,Y :> . x ) by FUNCT_1:def 3;
A6: ( f . x ) = y by A4,A5,CKB34:1;
then A7: y in Y by A4,CKB32:1;
( rng <: f,X,Y :> ) c= ( rng f ) by CKB31:1;
then y in ( rng f ) by A3;
then A8: y in ( dom ( f " ) ) by A1,FUNCT_1:32;
( dom <: f,X,Y :> ) c= ( dom f ) by CKB31:1;
then ( ( f " ) . y ) = x by A1,A4,A6,FUNCT_1:32;
hence thesis by A4,A8,A7,CKB32:1;
end;

assume A9: y in ( dom <: ( f " ),Y,X :> );
( dom <: ( f " ),Y,X :> ) c= ( dom ( f " ) ) by CKB31:1;
then y in ( dom ( f " ) ) by A9;
then y in ( rng f ) by A1,FUNCT_1:33;
then consider x such that A10: x in ( dom f ) and A11: y = ( f . x ) by FUNCT_1:def 3;
x = ( ( f " ) . ( f . x ) ) by A1,A10,FUNCT_1:34;
then x in X by A9,A11,CKB32:1;
then x in ( dom <: f,X,Y :> ) by A9,A10,A11,CKB32:1;
then (( <: f,X,Y :> . x ) in ( rng <: f,X,Y :> ) & ( <: f,X,Y :> . x ) = ( f . x )) by CKB34:1,FUNCT_1:def 3;
hence thesis by A2,A11,FUNCT_1:33;
end;
then A12: ( dom ( <: f,X,Y :> " ) ) = ( dom <: ( f " ),Y,X :> ) by TARSKI:1;
(for y holds (y in ( dom <: ( f " ),Y,X :> ) implies ( <: ( f " ),Y,X :> . y ) = ( ( <: f,X,Y :> " ) . y )))
proof
let y;
A13: ( rng <: f,X,Y :> ) c= ( rng f ) by CKB31:1;
assume A14: y in ( dom <: ( f " ),Y,X :> );
then y in ( rng <: f,X,Y :> ) by A2,A12,FUNCT_1:33;
then consider x such that A15: x in ( dom f ) and A16: y = ( f . x ) by A13,FUNCT_1:def 3;
A17: x = ( ( f " ) . ( f . x ) ) by A1,A15,FUNCT_1:34;
then x in X by A14,A16,CKB32:1;
then x in ( dom <: f,X,Y :> ) by A14,A15,A16,CKB32:1;
then (( ( <: f,X,Y :> " ) . ( <: f,X,Y :> . x ) ) = x & ( <: f,X,Y :> . x ) = ( f . x )) by A2,CKB34:1,FUNCT_1:34;
hence thesis by A14,A16,A17,CKB34:1;
end;
hence thesis by A12,FUNCT_1:2;
end;
