theorem
Th136: (for f,g being Function holds ((( dom f ) = ( dom g ) & f tolerates g) implies f = g))
proof
let f being Function;
let g being Function;
assume that
A1: ( dom f ) = ( dom g )
and
A2: f tolerates g;
(for x holds (x in ( dom f ) implies ( f . x ) = ( g . x ))) by A1,A2,CKB67:1;
hence thesis by A1,FUNCT_1:2;
end;
