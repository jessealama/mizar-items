environ
vocabularies FUNCT_1,RELAT_1,XBOOLE_0,TARSKI,SUBSET_1,ZFMISC_1,RELAT_2,PARTFUN1;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,RELAT_1,RELAT_2,RELSET_1,FUNCT_1,CKB5,CKB11,CKB18;
definitions TARSKI,FUNCT_1,XBOOLE_0,RELAT_1,RELAT_2;
theorems TARSKI,FUNCT_1,GRFUNC_1,ZFMISC_1,RELAT_1,RELSET_1,XBOOLE_0,XBOOLE_1,RELAT_2,CKB1,CKB2,CKB6,CKB7,CKB8,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25;
schemes FUNCT_1,XBOOLE_0,CKB3,CKB9,CKB10;
registrations XBOOLE_0,SUBSET_1,RELAT_1,FUNCT_1,RELSET_1,CKB4;
constructors FUNCT_1,RELAT_2,RELSET_1,CKB11,CKB18;
requirements SUBSET,BOOLE;
begin
reserve x for set;
reserve X for set;
reserve Y for set;
theorem
(for f being Function holds (((( dom f ) = { x } & x in X) & ( f . x ) in Y) implies f is (PartFunc of X,Y)))
proof
let f being Function;
assume that
A1: ( dom f ) = { x }
and
A2: x in X
and
A3: ( f . x ) in Y;
( rng f ) = { ( f . x ) } by A1,FUNCT_1:4;
then A4: ( rng f ) c= Y by A3,ZFMISC_1:31;
( dom f ) c= X by A1,A2,ZFMISC_1:31;
hence thesis by A4,RELSET_1:4;
end;
