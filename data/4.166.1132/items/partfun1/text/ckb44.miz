environ
vocabularies FUNCT_1,RELAT_1,XBOOLE_0,TARSKI,SUBSET_1,ZFMISC_1,RELAT_2,PARTFUN1;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,RELAT_1,RELAT_2,RELSET_1,FUNCT_1,CKB5,CKB11,CKB18,CKB29;
definitions TARSKI,FUNCT_1,XBOOLE_0,RELAT_1,RELAT_2,CKB29;
theorems TARSKI,FUNCT_1,GRFUNC_1,ZFMISC_1,RELAT_1,RELSET_1,XBOOLE_0,XBOOLE_1,RELAT_2,CKB1,CKB2,CKB6,CKB7,CKB8,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB37,CKB38,CKB39,CKB40,CKB41,CKB42,CKB43;
schemes FUNCT_1,XBOOLE_0,CKB3,CKB9,CKB10;
registrations XBOOLE_0,SUBSET_1,RELAT_1,FUNCT_1,RELSET_1,CKB4;
constructors FUNCT_1,RELAT_2,RELSET_1,CKB11,CKB18,CKB29;
requirements SUBSET,BOOLE;
begin
reserve x for set;
reserve X for set;
reserve Y for set;
reserve Z for set;
theorem
(for f,g being Function holds (( ( rng f ) /\ ( dom g ) ) c= Y implies ( <: g,Y,Z :> * <: f,X,Y :> ) = <: ( g * f ),X,Z :>))
proof
let f being Function;
let g being Function;
assume that
A1: ( ( rng f ) /\ ( dom g ) ) c= Y;
A2: ( dom <: ( g * f ),X,Z :> ) c= ( dom ( <: g,Y,Z :> * <: f,X,Y :> ) )
proof
let x;
assume A3: x in ( dom <: ( g * f ),X,Z :> );
then A4: x in ( dom ( g * f ) ) by CKB32:1;
then A5: ( f . x ) in ( dom g ) by FUNCT_1:11;
A6: x in ( dom f ) by A4,FUNCT_1:11;
then ( f . x ) in ( rng f ) by FUNCT_1:def 3;
then A7: ( f . x ) in ( ( rng f ) /\ ( dom g ) ) by A5,XBOOLE_0:def 4;
( ( g * f ) . x ) in Z by A3,CKB32:1;
then ( g . ( f . x ) ) in Z by A4,FUNCT_1:12;
then A8: ( f . x ) in ( dom <: g,Y,Z :> ) by A1,A5,A7,CKB32:1;
(x in ( dom <: f,X,Y :> ) & ( <: f,X,Y :> . x ) = ( f . x )) by A1,A3,A6,A7,CKB32:1,CKB33:1;
hence thesis by A8,FUNCT_1:11;
end;
(for x holds (x in ( dom <: ( g * f ),X,Z :> ) implies ( <: ( g * f ),X,Z :> . x ) = ( ( <: g,Y,Z :> * <: f,X,Y :> ) . x )))
proof
let x;
assume A9: x in ( dom <: ( g * f ),X,Z :> );
then A10: x in ( dom ( g * f ) ) by CKB32:1;
then A11: ( f . x ) in ( dom g ) by FUNCT_1:11;
x in ( dom f ) by A10,FUNCT_1:11;
then ( f . x ) in ( rng f ) by FUNCT_1:def 3;
then A12: ( f . x ) in ( ( rng f ) /\ ( dom g ) ) by A11,XBOOLE_0:def 4;
( ( g * f ) . x ) in Z by A9,CKB32:1;
then ( g . ( f . x ) ) in Z by A10,FUNCT_1:12;
then A13: ( f . x ) in ( dom <: g,Y,Z :> ) by A1,A11,A12,CKB32:1;
x in ( dom f ) by A10,FUNCT_1:11;
then A14: x in ( dom <: f,X,Y :> ) by A1,A9,A12,CKB32:1;
thus ( <: ( g * f ),X,Z :> . x ) = ( ( g * f ) . x ) by A9,CKB34:1
.= ( g . ( f . x ) ) by A10,FUNCT_1:12
.= ( <: g,Y,Z :> . ( f . x ) ) by A13,CKB34:1
.= ( <: g,Y,Z :> . ( <: f,X,Y :> . x ) ) by A14,CKB34:1
.= ( ( <: g,Y,Z :> * <: f,X,Y :> ) . x ) by A2,A9,FUNCT_1:12;
end;
then A15: <: ( g * f ),X,Z :> c= ( <: g,Y,Z :> * <: f,X,Y :> ) by A2,GRFUNC_1:2;
( <: g,Y,Z :> * <: f,X,Y :> ) c= <: ( g * f ),X,Z :> by CKB43:1;
hence thesis by A15,XBOOLE_0:def 10;
end;
