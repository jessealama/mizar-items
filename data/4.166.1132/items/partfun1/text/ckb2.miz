environ
vocabularies FUNCT_1,RELAT_1,XBOOLE_0,TARSKI,SUBSET_1,ZFMISC_1,RELAT_2,PARTFUN1;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,RELAT_1,RELAT_2,RELSET_1,FUNCT_1;
definitions TARSKI,FUNCT_1,XBOOLE_0,RELAT_1,RELAT_2;
theorems TARSKI,FUNCT_1,GRFUNC_1,ZFMISC_1,RELAT_1,RELSET_1,XBOOLE_0,XBOOLE_1,RELAT_2,CKB1;
schemes FUNCT_1,XBOOLE_0;
registrations XBOOLE_0,SUBSET_1,RELAT_1,FUNCT_1,RELSET_1;
constructors FUNCT_1,RELAT_2,RELSET_1;
requirements SUBSET,BOOLE;
begin
reserve x for set;
theorem
Th3: (for f,g,h being Function holds (( f \/ g ) = h implies (for x holds (x in ( ( dom f ) /\ ( dom g ) ) implies ( f . x ) = ( g . x )))))
proof
let f being Function;
let g being Function;
let h being Function;
assume that
A1: ( f \/ g ) = h;
let x;
assume A2: x in ( ( dom f ) /\ ( dom g ) );
then x in ( dom f ) by XBOOLE_0:def 4;
then A3: ( h . x ) = ( f . x ) by A1,GRFUNC_1:15;
x in ( dom g ) by A2,XBOOLE_0:def 4;
hence thesis by A1,A3,GRFUNC_1:15;
end;
