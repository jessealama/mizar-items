vocabularies FUNCT_1,RELAT_1,XBOOLE_0,TARSKI,SUBSET_1,ZFMISC_1,RELAT_2,PARTFUN1;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,RELAT_1,RELAT_2,RELSET_1,FUNCT_1,CKB5;
definitions TARSKI,FUNCT_1,XBOOLE_0,RELAT_1,RELAT_2;
theorems TARSKI,FUNCT_1,GRFUNC_1,ZFMISC_1,RELAT_1,RELSET_1,XBOOLE_0,XBOOLE_1,RELAT_2,CKB1,CKB2,CKB6;
schemes FUNCT_1,XBOOLE_0,CKB3;
registrations XBOOLE_0,SUBSET_1,RELAT_1,FUNCT_1,RELSET_1,CKB4;
constructors FUNCT_1,RELAT_2,RELSET_1;
requirements SUBSET,BOOLE;
begin
reserve x for set;
reserve Y for set;
theorem
Th27: (for f being Y -valued Function holds (x in ( dom f ) implies ( f . x ) in Y))
proof
let f being Y -valued Function;
assume x in ( dom f );
then A1: ( f . x ) in ( rng f ) by FUNCT_1:def 3;
( rng f ) c= Y by RELAT_1:def 19;
hence thesis by A1;
end;
