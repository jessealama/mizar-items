begin
reserve x for set;
reserve X for set;
reserve Y for set;
reserve Z for set;
theorem
Th82: (for f being Function holds (Z c= X implies <: f,Z,Y :> c= <: f,X,Y :>))
proof
let f being Function;
assume that
A1: Z c= X;
A2: ( dom <: f,Z,Y :> ) c= ( dom <: f,X,Y :> )
proof
let x;
assume A3: x in ( dom <: f,Z,Y :> );
then A4: ( f . x ) in Y by CKB32:1;
(x in Z & x in ( dom f )) by A3,CKB32:1;
hence thesis by A1,A4,CKB32:1;
end;
now
let x;
assume A5: x in ( dom <: f,Z,Y :> );
then ( <: f,Z,Y :> . x ) = ( f . x ) by CKB34:1;
hence ( <: f,Z,Y :> . x ) = ( <: f,X,Y :> . x ) by A2,A5,CKB34:1;
end;
hence thesis by A2,GRFUNC_1:2;
end;
