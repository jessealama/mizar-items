environ
vocabularies FUNCT_1,RELAT_1,XBOOLE_0,TARSKI,SUBSET_1,ZFMISC_1,RELAT_2,PARTFUN1;
notations TARSKI,XBOOLE_0,ZFMISC_1,SUBSET_1,RELAT_1,RELAT_2,RELSET_1,FUNCT_1,CKB5,CKB11,CKB18,CKB29;
definitions TARSKI,FUNCT_1,XBOOLE_0,RELAT_1,RELAT_2,CKB29;
theorems TARSKI,FUNCT_1,GRFUNC_1,ZFMISC_1,RELAT_1,RELSET_1,XBOOLE_0,XBOOLE_1,RELAT_2,CKB1,CKB2,CKB6,CKB7,CKB8,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27,CKB28,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB37,CKB38,CKB39,CKB40,CKB41,CKB42;
schemes FUNCT_1,XBOOLE_0,CKB3,CKB9,CKB10;
registrations XBOOLE_0,SUBSET_1,RELAT_1,FUNCT_1,RELSET_1,CKB4;
constructors FUNCT_1,RELAT_2,RELSET_1,CKB11,CKB18,CKB29;
requirements SUBSET,BOOLE;
begin
reserve x for set;
reserve X for set;
reserve Y for set;
reserve Z for set;
theorem
Th92: (for f,g being Function holds ( <: g,Y,Z :> * <: f,X,Y :> ) c= <: ( g * f ),X,Z :>)
proof
let f being Function,g being Function;
A1: (for x holds (x in ( dom ( <: g,Y,Z :> * <: f,X,Y :> ) ) implies ( ( <: g,Y,Z :> * <: f,X,Y :> ) . x ) = ( <: ( g * f ),X,Z :> . x )))
proof
let x;
assume A2: x in ( dom ( <: g,Y,Z :> * <: f,X,Y :> ) );
then A3: x in ( dom <: f,X,Y :> ) by FUNCT_1:11;
then A4: x in ( dom f ) by CKB32:1;
( <: f,X,Y :> . x ) in ( dom <: g,Y,Z :> ) by A2,FUNCT_1:11;
then A5: ( f . x ) in ( dom <: g,Y,Z :> ) by A3,CKB34:1;
then ( g . ( f . x ) ) in Z by CKB32:1;
then A6: ( ( g * f ) . x ) in Z by A4,FUNCT_1:13;
( f . x ) in ( dom g ) by A5,CKB32:1;
then x in ( dom ( g * f ) ) by A4,FUNCT_1:11;
then A7: x in ( dom <: ( g * f ),X,Z :> ) by A2,A6,CKB32:1;
thus ( ( <: g,Y,Z :> * <: f,X,Y :> ) . x ) = ( <: g,Y,Z :> . ( <: f,X,Y :> . x ) ) by A2,FUNCT_1:12
.= ( <: g,Y,Z :> . ( f . x ) ) by A3,CKB34:1
.= ( g . ( f . x ) ) by A5,CKB34:1
.= ( ( g * f ) . x ) by A4,FUNCT_1:13
.= ( <: ( g * f ),X,Z :> . x ) by A7,CKB34:1;
end;
( dom ( <: g,Y,Z :> * <: f,X,Y :> ) ) c= ( dom <: ( g * f ),X,Z :> )
proof
let x;
assume A8: x in ( dom ( <: g,Y,Z :> * <: f,X,Y :> ) );
then A9: x in ( dom <: f,X,Y :> ) by FUNCT_1:11;
then A10: x in ( dom f ) by CKB32:1;
( <: f,X,Y :> . x ) in ( dom <: g,Y,Z :> ) by A8,FUNCT_1:11;
then A11: ( f . x ) in ( dom <: g,Y,Z :> ) by A9,CKB34:1;
then ( g . ( f . x ) ) in Z by CKB32:1;
then A12: ( ( g * f ) . x ) in Z by A10,FUNCT_1:13;
( f . x ) in ( dom g ) by A11,CKB32:1;
then x in ( dom ( g * f ) ) by A10,FUNCT_1:11;
hence thesis by A8,A12,CKB32:1;
end;
hence thesis by A1,GRFUNC_1:2;
end;
