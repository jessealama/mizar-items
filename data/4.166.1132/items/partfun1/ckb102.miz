begin
reserve x for set;
reserve y for set;
reserve z for set;
reserve A for set;
theorem
((A is  functional & (for f,g being Function holds ((f in A & g in A) implies f tolerates g))) implies ( union A ) is Function)
proof
assume that
A1: (for x holds (x in A implies x is Function))
and
A2: (for f,g being Function holds ((f in A & g in A) implies f tolerates g));
A3:now
let x;
let y;
let z;
assume that
A4: [ x,y ] in ( union A )
and
A5: [ x,z ] in ( union A );
consider p being set such that A6: [ x,y ] in p and A7: p in A by A4,TARSKI:def 4;
consider q being set such that A8: [ x,z ] in q and A9: q in A by A5,TARSKI:def 4;
reconsider p,q as Function by A1,A7,A9;
p tolerates q by A2,A7,A9;
hence y = z by A6,A8,CKB101:1;
end;
now
let z;
assume z in ( union A );
then consider p being set such that A10: z in p and A11: p in A by TARSKI:def 4;
reconsider p as Function by A1,A11;
p = p;
hence (ex x st (ex y st [ x,y ] = z)) by A10,RELAT_1:def 1;
end;
hence thesis by A3,FUNCT_1:def 1,RELAT_1:def 1;
end;
