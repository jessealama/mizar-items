begin
reserve x for set;
reserve X for set;
reserve Y for set;
theorem
Th80: (for f being Function holds (x in ( dom <: f,X,Y :> ) implies ( <: f,X,Y :> . x ) = ( f . x )))
proof
let f being Function;
assume A1: x in ( dom <: f,X,Y :> );
then (x in ( dom f ) & ( f . x ) in Y) by CKB32:1;
hence thesis by A1,CKB33:1;
end;
