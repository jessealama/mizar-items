vocabularies NUMBERS,SUBSET_1,MEMBERED,XCMPLX_0,ORDINAL1,XXREAL_0,XREAL_0,ARYTM_1,RELAT_1,TARSKI,XBOOLE_0,ARYTM_3,RAT_1,INT_1,CARD_1,MEMBER_1;
notations TARSKI,XBOOLE_0,SUBSET_1,ENUMSET1,NUMBERS,XCMPLX_0,XREAL_0,RAT_1,INT_1,ORDINAL1,MEMBERED,BINOP_2,XXREAL_3,XXREAL_0,CKB1,CKB2,CKB3,CKB6;
definitions TARSKI,XBOOLE_0,XCMPLX_0,MEMBERED,XXREAL_3,CKB6;
theorems XCMPLX_0,TARSKI,XBOOLE_0,ENUMSET1,XCMPLX_1,XXREAL_0,XXREAL_3,XBOOLE_1,CKB6;
registrations XREAL_0,INT_1,RAT_1,ORDINAL1,MEMBERED,XCMPLX_0,NAT_1,XXREAL_3,XBOOLE_0,CKB4,CKB5;
constructors XCMPLX_0,RAT_1,MEMBERED,ENUMSET1,BINOP_2,XXREAL_3,CKB1,CKB2,CKB3,CKB6;
requirements BOOLE,SUBSET,NUMERALS,ARITHM,REAL;
begin
reserve w for (Element of ( ExtREAL ));
reserve F for  ext-real-membered set;
reserve f for  ext-real number;
theorem
Th1: (f in F iff ( - f ) in ( -- F ))
proof
f in ( ExtREAL ) by XXREAL_0:def 1;
hence (f in F implies ( - f ) in ( -- F ));
assume ( - f ) in ( -- F );
then A1: (ex w st (( - w ) = ( - f ) & w in F));
( - ( - f ) ) = f;
hence thesis by A1;
end;
