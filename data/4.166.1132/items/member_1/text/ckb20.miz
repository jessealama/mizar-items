begin
reserve c for (Element of ( COMPLEX ));
reserve A for  complex-membered set;
reserve B for  complex-membered set;
reserve z for  complex number;
reserve e for set;
definition
let A being  complex-membered set;
func -- A ->  complex-membered set equals 
{ ( - c ): c in A };
coherence
proof
{ ( - c ): c in A } is  complex-membered
proof
let e;
assume e in { ( - c ): c in A };
then (ex c st (e = ( - c ) & c in A));
hence thesis;
end;
hence thesis;
end;
involutiveness
proof
let A;
let B;
assume A1: A = { ( - c ): c in B };
thus B c= { ( - c ): c in A }
proof
let z;
A2: z in ( COMPLEX ) by XCMPLX_0:def 2;
A3: (( - z ) in ( COMPLEX ) & z = ( - ( - z ) )) by XCMPLX_0:def 2;
assume z in B;
then ( - z ) in A by A1,A2;
hence thesis by A3;
end;

let e;
assume e in { ( - c ): c in A };
then consider r0 being (Element of ( COMPLEX )) such that A4: ( - r0 ) = e and A5: r0 in A;
(ex c st (( - c ) = r0 & c in B)) by A1,A5;
hence thesis by A4;
end;
end;
