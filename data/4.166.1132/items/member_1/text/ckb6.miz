environ
vocabularies NUMBERS,SUBSET_1,MEMBERED,XCMPLX_0,ORDINAL1,XXREAL_0,XREAL_0,ARYTM_1,RELAT_1,TARSKI,XBOOLE_0,ARYTM_3,RAT_1,INT_1,CARD_1,MEMBER_1;
notations TARSKI,XBOOLE_0,SUBSET_1,ENUMSET1,NUMBERS,XCMPLX_0,XREAL_0,RAT_1,INT_1,ORDINAL1,MEMBERED,BINOP_2,XXREAL_3,XXREAL_0,CKB1,CKB2,CKB3;
definitions TARSKI,XBOOLE_0,XCMPLX_0,MEMBERED,XXREAL_3;
theorems XCMPLX_0,TARSKI,XBOOLE_0,ENUMSET1,XCMPLX_1,XXREAL_0,XXREAL_3,XBOOLE_1;
registrations XREAL_0,INT_1,RAT_1,ORDINAL1,MEMBERED,XCMPLX_0,NAT_1,XXREAL_3,XBOOLE_0,CKB4,CKB5;
constructors XCMPLX_0,RAT_1,MEMBERED,ENUMSET1,BINOP_2,XXREAL_3,CKB1,CKB2,CKB3;
requirements BOOLE,SUBSET,NUMERALS,ARITHM,REAL;
begin
reserve w for (Element of ( ExtREAL ));
reserve w1 for (Element of ( ExtREAL ));
reserve A for  complex-membered set;
reserve B for  complex-membered set;
reserve F for  ext-real-membered set;
reserve z for  complex number;
reserve e for set;
definition
let F being  ext-real-membered set;
func -- F ->  ext-real-membered set equals 
{ ( - w ): w in F };
coherence
proof
{ ( - w ): w in F } is  ext-real-membered
proof
let e;
assume e in { ( - w ): w in F };
then (ex w st (e = ( - w ) & w in F));
hence thesis;
end;
hence thesis;
end;
involutiveness
proof
let A being  ext-real-membered set;
let B being  ext-real-membered set;
assume A1: A = { ( - w ): w in B };
thus B c= { ( - w ): w in A }
proof
let z being  ext-real number;
A2: z in ( ExtREAL ) by XXREAL_0:def 1;
A3: (( - z ) in ( ExtREAL ) & z = ( - ( - z ) )) by XXREAL_0:def 1;
assume z in B;
then ( - z ) in A by A1,A2;
hence thesis by A3;
end;

let e;
assume e in { ( - w ): w in A };
then consider w1 such that A4: ( - w1 ) = e and A5: w1 in A;
(ex w st (( - w ) = w1 & w in B)) by A1,A5;
hence thesis by A4;
end;
end;
