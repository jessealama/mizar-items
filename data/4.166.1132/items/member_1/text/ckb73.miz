theorems XCMPLX_0,TARSKI,XBOOLE_0,ENUMSET1,XCMPLX_1,XXREAL_0,XXREAL_3,XBOOLE_1,CKB6,CKB7,CKB8,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB37,CKB38,CKB39,CKB42,CKB43,CKB44,CKB45,CKB46,CKB47,CKB48,CKB49,CKB50,CKB56,CKB57,CKB58,CKB59,CKB60,CKB61,CKB62,CKB63,CKB64,CKB65,CKB66,CKB67,CKB72;
registrations XREAL_0,INT_1,RAT_1,ORDINAL1,MEMBERED,XCMPLX_0,NAT_1,XXREAL_3,XBOOLE_0,CKB4,CKB5,CKB9,CKB10,CKB23,CKB24,CKB25,CKB26,CKB27,CKB40,CKB41,CKB51,CKB52,CKB53,CKB54,CKB68,CKB69,CKB70,CKB71,CKB28,CKB55;
constructors XCMPLX_0,RAT_1,MEMBERED,ENUMSET1,BINOP_2,XXREAL_3,CKB1,CKB2,CKB3,CKB6,CKB20,CKB38,CKB48,CKB66;
requirements BOOLE,SUBSET,NUMERALS,ARITHM,REAL;
begin
reserve w for (Element of ( ExtREAL ));
reserve w1 for (Element of ( ExtREAL ));
reserve F for  ext-real-membered set;
reserve G for  ext-real-membered set;
reserve H for  ext-real-membered set;
reserve j for  ext-real number;
theorem
Th41: ( F ++ ( G \/ H ) ) = ( ( F ++ G ) \/ ( F ++ H ) )
proof
let j;
hereby
assume j in ( F ++ ( G \/ H ) );
then consider w,w1 such that A1: j = ( w + w1 ) and A2: w in F and A3: w1 in ( G \/ H );
(w1 in G or w1 in H) by A3,XBOOLE_0:def 3;
then (( w + w1 ) in ( F ++ G ) or ( w + w1 ) in ( F ++ H )) by A2;
hence j in ( ( F ++ G ) \/ ( F ++ H ) ) by A1,XBOOLE_0:def 3;
end;
assume A4: j in ( ( F ++ G ) \/ ( F ++ H ) );
per cases  by A4,XBOOLE_0:def 3;
suppose j in ( F ++ G );

then consider w,w1 such that A5: (j = ( w + w1 ) & w in F) and A6: w1 in G;
w1 in ( G \/ H ) by A6,XBOOLE_0:def 3;
hence thesis by A5;
end;
suppose j in ( F ++ H );

then consider w,w1 such that A7: (j = ( w + w1 ) & w in F) and A8: w1 in H;
w1 in ( G \/ H ) by A8,XBOOLE_0:def 3;
hence thesis by A7;
end;
end;
