begin
reserve w for (Element of ( ExtREAL ));
reserve w1 for (Element of ( ExtREAL ));
reserve F for  ext-real-membered set;
reserve G for  ext-real-membered set;
reserve H for  ext-real-membered set;
reserve I for  ext-real-membered set;
reserve i for  ext-real number;
theorem
((F c= G & H c= I) implies ( F ++ H ) c= ( G ++ I ))
proof
assume A1: (F c= G & H c= I);
let i;
assume i in ( F ++ H );
then (ex w st (ex w1 st ((i = ( w + w1 ) & w in F) & w1 in H)));
hence thesis by A1;
end;
