environ
vocabularies NUMBERS,SUBSET_1,MEMBERED,XCMPLX_0,ORDINAL1,XXREAL_0,XREAL_0,ARYTM_1,RELAT_1,TARSKI,XBOOLE_0,ARYTM_3,RAT_1,INT_1,CARD_1,MEMBER_1;
notations TARSKI,XBOOLE_0,SUBSET_1,ENUMSET1,NUMBERS,XCMPLX_0,XREAL_0,RAT_1,INT_1,ORDINAL1,MEMBERED,BINOP_2,XXREAL_3,XXREAL_0,CKB1,CKB2,CKB3,CKB6,CKB20,CKB38,CKB48,CKB66,CKB78;
definitions TARSKI,XBOOLE_0,XCMPLX_0,MEMBERED,XXREAL_3,CKB6,CKB20,CKB38,CKB48,CKB66,CKB78;
theorems XCMPLX_0,TARSKI,XBOOLE_0,ENUMSET1,XCMPLX_1,XXREAL_0,XXREAL_3,XBOOLE_1,CKB6,CKB7,CKB8,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB29,CKB30,CKB31,CKB32,CKB33,CKB34,CKB35,CKB36,CKB37,CKB38,CKB39,CKB42,CKB43,CKB44,CKB45,CKB46,CKB47,CKB48,CKB49,CKB50,CKB56,CKB57,CKB58,CKB59,CKB60,CKB61,CKB62,CKB63,CKB64,CKB65,CKB66,CKB67,CKB72,CKB73,CKB74,CKB75,CKB76,CKB77,CKB78,CKB79;
registrations XREAL_0,INT_1,RAT_1,ORDINAL1,MEMBERED,XCMPLX_0,NAT_1,XXREAL_3,XBOOLE_0,CKB4,CKB5,CKB9,CKB10,CKB23,CKB24,CKB25,CKB26,CKB27,CKB40,CKB41,CKB51,CKB52,CKB53,CKB54,CKB68,CKB69,CKB70,CKB71,CKB80,CKB81,CKB82,CKB83,CKB84,CKB85,CKB86,CKB87,CKB28,CKB55;
constructors XCMPLX_0,RAT_1,MEMBERED,ENUMSET1,BINOP_2,XXREAL_3,CKB1,CKB2,CKB3,CKB6,CKB20,CKB38,CKB48,CKB66,CKB78;
requirements BOOLE,SUBSET,NUMERALS,ARITHM,REAL;
begin
reserve w for (Element of ( ExtREAL ));
reserve w1 for (Element of ( ExtREAL ));
reserve c for (Element of ( COMPLEX ));
reserve c1 for (Element of ( COMPLEX ));
reserve A for  complex-membered set;
reserve B for  complex-membered set;
reserve F for  ext-real-membered set;
reserve G for  ext-real-membered set;
reserve a for  complex number;
registration
let A being  real-membered set;
let B being  real-membered set;
let F being  ext-real-membered set;
let G being  ext-real-membered set;
identify A ++ B with F ++ G when A = F,B = G;
compatibility
proof
assume A1: (A = F & B = G);
let a being  ext-real number;
hereby
assume a in ( A ++ B );
then consider c,c1 such that A2: a = ( c + c1 ) and A3: (c in A & c1 in B);
reconsider d1 = c,d2 = c1 as (Element of ( ExtREAL )) by A3,XXREAL_0:def 1;
a = ( d1 + d2 ) by A2,A3,XXREAL_3:def 2;
hence a in ( F ++ G ) by A1,A3;
end;
assume a in ( F ++ G );
then consider w,w1 such that A4: a = ( w + w1 ) and A5: (w in F & w1 in G);
reconsider d1 = w,d2 = w1 as (Element of ( COMPLEX )) by A1,A5,XCMPLX_0:def 2;
a = ( d1 + d2 ) by A1,A4,A5,XXREAL_3:def 2;
hence thesis by A1,A5;
end;
end;
