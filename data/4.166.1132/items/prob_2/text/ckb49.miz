theorem
(for A holds (for B holds (for P holds (( 0 ) < ( P . B ) implies ( 1 - ( ( P . ( ( [#] Sigma ) \ A ) ) / ( P . B ) ) ) <= ( ( P .|. B ) . A )))))
proof
let A;
let B;
let P;
assume A1: ( 0 ) < ( P . B );
( ( ( P . B ) + ( P . A ) ) - 1 ) <= ( P . ( A /\ B ) ) by CKB22:1;
then ( ( P . B ) + ( - ( 1 - ( P . A ) ) ) ) <= ( P . ( A /\ B ) );
then ( ( P . B ) + ( - ( P . ( ( [#] Sigma ) \ A ) ) ) ) <= ( P . ( A /\ B ) ) by PROB_1:32;
then ( ( ( P . B ) + ( - ( P . ( ( [#] Sigma ) \ A ) ) ) ) / ( P . B ) ) <= ( ( P . ( A /\ B ) ) / ( P . B ) ) by A1,XREAL_1:72;
then ( ( ( P . B ) - ( P . ( ( [#] Sigma ) \ A ) ) ) / ( P . B ) ) <= ( ( P .|. B ) . A ) by A1,CKB38:def 1;
then ( ( ( P . B ) / ( P . B ) ) - ( ( P . ( ( [#] Sigma ) \ A ) ) / ( P . B ) ) ) <= ( ( P .|. B ) . A ) by XCMPLX_1:120;
hence thesis by A1,XCMPLX_1:60;
end;
