begin
reserve i for Nat;
reserve n for Nat;
reserve x0 for  real-valued FinSequence;
definition
let n being Nat;
let x0 being (Element of ( REAL n ));
func ProjFinSeq x0 -> (FinSequence of ( REAL n )) means 
:Def12: (( len it ) = n & (for i being Nat holds ((1 <= i & i <= n) implies ( it . i ) = ( |( x0,( Base_FinSeq (n,i) ) )| * ( Base_FinSeq (n,i) ) ))));
existence
proof
defpred P[ set,set ]
 means
(for i being Nat holds (((i = $1 & 1 <= i) & i <= n) implies $2 = ( |( x0,( Base_FinSeq (n,i) ) )| * ( Base_FinSeq (n,i) ) )));
A1: (for k being Nat holds (k in ( Seg n ) implies (ex x being (Element of ( REAL n )) st P[ k,x ])))
proof
reconsider n0 = n as (Element of ( NAT )) by ORDINAL1:def 12;
let k being Nat;
assume k in ( Seg n );
reconsider k0 = k as (Element of ( NAT )) by ORDINAL1:def 12;
reconsider x00 = ( |( x0,( Base_FinSeq (n0,k0) ) )| * ( Base_FinSeq (n0,k0) ) ) as (Element of ( REAL n ));
(for i being Nat holds (((i = k & 1 <= i) & i <= n) implies x00 = ( |( x0,( Base_FinSeq (n,i) ) )| * ( Base_FinSeq (n,i) ) )));
hence (ex x being (Element of ( REAL n )) st P[ k,x ]);
end;
consider p being (FinSequence of ( REAL n )) such that A2: (( dom p ) = ( Seg n ) & (for k being Nat holds (k in ( Seg n ) implies P[ k,( p . k ) ]))) from FINSEQ_1:sch 5(A1);
A3: (for i being Nat holds ((1 <= i & i <= n) implies ( p . i ) = ( |( x0,( Base_FinSeq (n,i) ) )| * ( Base_FinSeq (n,i) ) )))
proof
let i being Nat;
assume that
A4: 1 <= i
and
A5: i <= n;
i in ( Seg n ) by A4,A5,FINSEQ_1:1;
hence ( p . i ) = ( |( x0,( Base_FinSeq (n,i) ) )| * ( Base_FinSeq (n,i) ) ) by A2,A4,A5;
end;
( Seg n ) = ( Seg ( len p ) ) by A2,FINSEQ_1:def 3;
hence thesis by A3,FINSEQ_1:6;
end;
uniqueness
proof
let r1 being (FinSequence of ( REAL n ));
let r2 being (FinSequence of ( REAL n ));
assume that
A6: ( len r1 ) = n
and
A7: (for i being Nat holds ((1 <= i & i <= n) implies ( r1 . i ) = ( |( x0,( Base_FinSeq (n,i) ) )| * ( Base_FinSeq (n,i) ) )))
and
A8: ( len r2 ) = n
and
A9: (for i being Nat holds ((1 <= i & i <= n) implies ( r2 . i ) = ( |( x0,( Base_FinSeq (n,i) ) )| * ( Base_FinSeq (n,i) ) )));
(for k being Nat holds ((1 <= k & k <= n) implies ( r1 . k ) = ( r2 . k )))
proof
reconsider n22 = n as (Element of ( NAT )) by ORDINAL1:def 12;
let k being Nat;
assume that
A10: 1 <= k
and
A11: k <= n;
reconsider k0 = k as (Element of ( NAT )) by ORDINAL1:def 12;
( r1 . k0 ) = ( |( x0,( Base_FinSeq (n22,k0) ) )| * ( Base_FinSeq (n22,k0) ) ) by A7,A10,A11;
hence ( r1 . k ) = ( r2 . k ) by A9,A10,A11;
end;
hence r1 = r2 by A6,A8,FINSEQ_1:14;
end;
