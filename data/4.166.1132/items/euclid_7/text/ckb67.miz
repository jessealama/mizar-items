requirements REAL,SUBSET,NUMERALS,ARITHM,BOOLE;
begin
reserve i for Nat;
reserve n for Nat;
registration
let n;
cluster ( RN_Base n ) ->  orthogonal_basis;
coherence
proof
set B = ( RN_Base n );
A1: { x where x is (Element of ( REAL n )): (ex i being (Element of ( NAT )) st ((1 <= i & i <= n) & x = ( Base_FinSeq (n,i) ))) } c= B
proof
let y being set;
assume y in { x where x is (Element of ( REAL n )): (ex i being (Element of ( NAT )) st ((1 <= i & i <= n) & x = ( Base_FinSeq (n,i) ))) };
then (ex x being (Element of ( REAL n )) st (y = x & (ex i being (Element of ( NAT )) st ((1 <= i & i <= n) & x = ( Base_FinSeq (n,i) )))));
hence y in B;
end;
B c= { x where x is (Element of ( REAL n )): (ex i being (Element of ( NAT )) st ((1 <= i & i <= n) & x = ( Base_FinSeq (n,i) ))) }
proof
let y being set;
assume y in B;
then (ex i2 being (Element of ( NAT )) st ((y = ( Base_FinSeq (n,i2) ) & 1 <= i2) & i2 <= n));
hence y in { x where x is (Element of ( REAL n )): (ex i being (Element of ( NAT )) st ((1 <= i & i <= n) & x = ( Base_FinSeq (n,i) ))) };
end;
then A2: B = { x where x is (Element of ( REAL n )): (ex i being (Element of ( NAT )) st ((1 <= i & i <= n) & x = ( Base_FinSeq (n,i) ))) } by A1,XBOOLE_0:def 10;
thus B is  R-orthogonal
proof
let x being  real-valued FinSequence;
let y being  real-valued FinSequence;
assume that
A3: x in B
and
A4: y in B
and
A5: x <> y;
A6: (ex y0 being (Element of ( REAL n )) st (y = y0 & (ex i being (Element of ( NAT )) st ((1 <= i & i <= n) & y0 = ( Base_FinSeq (n,i) ))))) by A2,A4;
(ex x0 being (Element of ( REAL n )) st (x = x0 & (ex i being (Element of ( NAT )) st ((1 <= i & i <= n) & x0 = ( Base_FinSeq (n,i) ))))) by A2,A3;
hence |( x,y )| = ( 0 ) by A5,A6,CKB59:1;
end;

thus B is  R-normal
proof
let x being  real-valued FinSequence;
assume x in B;
then (ex x0 being (Element of ( REAL n )) st (x = x0 & (ex i being (Element of ( NAT )) st ((1 <= i & i <= n) & x0 = ( Base_FinSeq (n,i) ))))) by A2;
hence |. x .| = 1 by CKB58:1;
end;

let B2 being  R-orthonormal (Subset of ( REAL n ));
assume A7: B c= B2;
now
assume (not B2 c= B);
then consider x being set such that A8: x in B2 and A9: (not x in B) by TARSKI:def 3;
reconsider rx = x as (Element of ( REAL n )) by A8;
A10:now
A11: n in ( NAT ) by ORDINAL1:def 12;
assume rx <> ( 0* n );
then consider i being (Element of ( NAT )) such that A12: 1 <= i and A13: i <= n and A14: ( rx . i ) <> ( 0 ) by A11,JORDAN2C:46;
( Base_FinSeq (n,i) ) in B by A12,A13;
then |( rx,( Base_FinSeq (n,i) ) )| = ( 0 ) by A7,A8,A9,CKB21:def 1;
hence contradiction by A12,A13,A14,CKB60:1;
end;
|. ( 0* n ) .| = ( 0 ) by EUCLID:7;
hence contradiction by A8,A10,CKB24:def 1;
end;
hence thesis by A7,XBOOLE_0:def 10;
end;
end;
