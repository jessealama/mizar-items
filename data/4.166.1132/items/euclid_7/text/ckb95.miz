begin
reserve i for Nat;
reserve n for Nat;
reserve f for  real-valued FinSequence;
Lm4:now
let n;
thus (( RN_Base n ) is  finite & ( card ( RN_Base n ) ) = n)
proof
per cases ;
suppose n is  empty;

hence thesis;
end;
suppose n is non  empty;

then reconsider n as non  empty Nat;
defpred P[ set,set ]
 means
(for i being (Element of ( NAT )) holds (i = $1 implies $2 = ( Base_FinSeq (n,i) )));
A1: (for x being set holds (x in ( Seg n ) implies (ex y being set st P[ x,y ])))
proof
let x being set;
assume x in ( Seg n );
then reconsider j = x as (Element of ( NAT ));
(for i being (Element of ( NAT )) holds (i = j implies ( Base_FinSeq (n,j) ) = ( Base_FinSeq (n,i) )));
hence (ex y being set st P[ x,y ]);
end;
consider f being Function such that A2: (( dom f ) = ( Seg n ) & (for x2 being set holds (x2 in ( Seg n ) implies P[ x2,( f . x2 ) ]))) from CLASSES1:sch 1(A1);
A3: ( rng f ) c= ( RN_Base n )
proof
let y being set;
assume y in ( rng f );
then consider x being set such that A4: x in ( dom f ) and A5: y = ( f . x ) by FUNCT_1:def 3;
reconsider nx = x as (Element of ( NAT )) by A2,A4;
A6: nx <= n by A2,A4,FINSEQ_1:1;
A7: ( f . x ) = ( Base_FinSeq (n,nx) ) by A2,A4;
1 <= nx by A2,A4,FINSEQ_1:1;
hence y in ( RN_Base n ) by A5,A6,A7;
end;
then reconsider f2 = f as (Function of ( Seg n ),( RN_Base n )) by A2,FUNCT_2:2;
(for x1,x2 being set holds (((x1 in ( dom f ) & x2 in ( dom f )) & ( f . x1 ) = ( f . x2 )) implies x1 = x2))
proof
let x1 being set;
let x2 being set;
assume that
A8: x1 in ( dom f )
and
A9: x2 in ( dom f )
and
A10: ( f . x1 ) = ( f . x2 );
reconsider nx1 = x1,nx2 = x2 as (Element of ( NAT )) by A2,A8,A9;
A11: nx1 <= n by A2,A8,FINSEQ_1:1;
A12: ( f . x2 ) = ( Base_FinSeq (n,nx2) ) by A2,A9;
A13: ( f . x1 ) = ( Base_FinSeq (n,nx1) ) by A2,A8;
1 <= nx1 by A2,A8,FINSEQ_1:1;
hence x1 = x2 by A10,A11,A13,A12,CKB55:1;
end;
then A14: f2 is  one-to-one by FUNCT_1:def 4;
( RN_Base n ) c= ( rng f )
proof
let y being set;
assume y in ( RN_Base n );
then consider i being (Element of ( NAT )) such that A15: y = ( Base_FinSeq (n,i) ) and A16: 1 <= i and A17: i <= n;
A18: i in ( Seg n ) by A16,A17,FINSEQ_1:1;
then ( f . i ) = ( Base_FinSeq (n,i) ) by A2;
hence y in ( rng f ) by A2,A15,A18,FUNCT_1:def 3;
end;
then ( rng f ) = ( RN_Base n ) by A3,XBOOLE_0:def 10;
then f2 is  onto by FUNCT_2:def 3;
then ( card ( Seg n ) ) = ( card ( RN_Base n ) ) by A14,CKB3:1;
hence thesis by FINSEQ_1:57;
end;
end;

end;
registration
let n;
