theorem
(for n being (Element of ( NAT )) holds ( PFArt (1,n) ) = { [ 1,n ],[ 2,n ] })
proof
let n being (Element of ( NAT ));
thus ( PFArt (1,n) ) c= { [ 1,n ],[ 2,n ] }
proof
let x being set;
assume A1: x in ( PFArt (1,n) );
per cases  by A1,CKB22:def 1;
suppose (ex m being  odd (Element of ( NAT )) st (m <= ( 2 * 1 ) & [ m,n ] = x));

then x = [ 1,n ] by NAT_1:26;
hence thesis by TARSKI:def 2;
end;
suppose [ ( 2 * 1 ),n ] = x;

hence thesis by TARSKI:def 2;
end;
end;

let x being set;
assume A2: x in { [ 1,n ],[ 2,n ] };
per cases  by A2,TARSKI:def 2;
suppose A3: x = [ 1,n ];

1 <= ( 2 * 1 );
hence thesis by A3,CKB22:def 1,CKB4:1,CKB5:1;
end;
suppose x = [ 2,n ];

then x = [ ( 2 * 1 ),n ];
hence thesis by CKB22:def 1;
end;
end;
