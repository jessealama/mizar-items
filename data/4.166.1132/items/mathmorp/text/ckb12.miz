begin
reserve T for non empty  Abelian  add-associative  right_zeroed  right_complementable RLSStruct;
reserve X for (Subset of T);
reserve x for (Point of T);
theorem
( X (+) { x } ) = ( X + x )
proof
thus ( X (+) { x } ) c= ( X + x )
proof
let p being set;
assume p in ( X (+) { x } );
then consider y being (Point of T),z being (Point of T) such that A1: p = ( y + z ) and A2: y in X and A3: z in { x };
{ z } c= { x } by A3,ZFMISC_1:31;
then p = ( y + x ) by A1,ZFMISC_1:18;
hence thesis by A2;
end;

let p being set;
assume p in ( X + x );
then A4: (ex q being (Point of T) st (p = ( q + x ) & q in X));
x in { x } by TARSKI:def 1;
hence thesis by A4;
end;
