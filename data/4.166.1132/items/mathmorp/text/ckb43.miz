begin
reserve T for non empty  Abelian  add-associative  right_zeroed  right_complementable RLSStruct;
reserve X for (Subset of T);
reserve Y for (Subset of T);
reserve B for (Subset of T);
theorem
( ( B (-) X ) \/ ( B (-) Y ) ) c= ( B (-) ( X /\ Y ) )
proof
let x being set;
assume x in ( ( B (-) X ) \/ ( B (-) Y ) );
then (x in ( B (-) X ) or x in ( B (-) Y )) by XBOOLE_0:def 3;
then consider y being (Point of T) such that A1: ((x = y & ( X + y ) c= B) or (x = y & ( Y + y ) c= B));
( ( X + y ) /\ ( Y + y ) ) c= B
proof
let a being set;
assume A2: a in ( ( X + y ) /\ ( Y + y ) );
then A3: a in ( X + y ) by XBOOLE_0:def 4;
A4: a in ( Y + y ) by A2,XBOOLE_0:def 4;
per cases  by A1;
suppose ( X + y ) c= B;

hence thesis by A3;
end;
suppose ( Y + y ) c= B;

hence thesis by A4;
end;
end;
then ( ( X /\ Y ) + y ) c= B by CKB35:1;
hence thesis by A1;
end;
