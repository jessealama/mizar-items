environ
vocabularies NUMBERS,SUBSET_1,PRE_TOPC,EUCLID,ARYTM_3,STRUCT_0,REALSET1,ARYTM_1,VALUED_2,TARSKI,SUPINF_2,XBOOLE_0,XREAL_0,ORDINAL1,RELAT_1,CARD_1,MONOID_0,RLVECT_1,CONVEX1,XXREAL_0,REAL_1,RLTOPSP1,MATHMORP,ALGSTR_0;
notations TARSKI,ORDINAL1,XBOOLE_0,SUBSET_1,NUMBERS,XCMPLX_0,XXREAL_0,DOMAIN_1,STRUCT_0,ALGSTR_0,PRE_TOPC,XREAL_0,REAL_1,RLVECT_1,RLTOPSP1,EUCLID,RUSUB_4,CKB1,CKB2,CKB3,CKB4;
definitions TARSKI,XBOOLE_0,SUBSET_1,ALGSTR_0,RUSUB_4,CKB1,CKB2,CKB3;
theorems XBOOLE_1,XCMPLX_1,TARSKI,SUBSET_1,ZFMISC_1,EUCLID,XBOOLE_0,JORDAN1,XREAL_0,XREAL_1,RLTOPSP1,RLVECT_1,CKB1,CKB2,CKB3,CKB5,CKB6,CKB7,CKB8,CKB9,CKB10,CKB11,CKB12,CKB13,CKB14,CKB15,CKB16,CKB17,CKB18,CKB19,CKB20,CKB21,CKB22,CKB23,CKB24,CKB25,CKB26,CKB27;
registrations XBOOLE_0,XXREAL_0,XREAL_0,MEMBERED,STRUCT_0,EUCLID,RLTOPSP1,ALGSTR_0,RLVECT_1;
constructors DOMAIN_1,XXREAL_0,REAL_1,JORDAN1,CONVEX1,BINOP_2,RUSUB_4,CKB1,CKB2,CKB3;
requirements REAL,BOOLE,SUBSET,NUMERALS,ARITHM;
begin
reserve T for non empty  Abelian  add-associative  right_zeroed  right_complementable RLSStruct;
reserve X for (Subset of T);
reserve x for (Point of T);
theorem
( X (-) { x } ) = ( X + ( - x ) )
proof
thus ( X (-) { x } ) c= ( X + ( - x ) )
proof
let y being set;
assume y in ( X (-) { x } );
then consider p being (Point of T) such that A1: p = y and A2: ( { x } + p ) c= X;
( { p } + x ) c= X by A2,CKB10:1;
then ( ( { p } + x ) + ( - x ) ) c= ( X + ( - x ) ) by CKB7:1;
then ( { p } + ( x + ( - x ) ) ) c= ( X + ( - x ) ) by CKB22:1;
then ( { p } + ( 0. T ) ) c= ( X + ( - x ) ) by RLVECT_1:5;
then { p } c= ( X + ( - x ) ) by CKB27:1;
hence thesis by A1,ZFMISC_1:31;
end;

let y being set;
assume y in ( X + ( - x ) );
then consider p being (Point of T) such that A3: y = ( p + ( - x ) ) and A4: p in X;
reconsider y as (Point of T) by A3;
y = ( p - x ) by A3;
then A5: ( y + x ) = p by CKB18:1;
( { x } + y ) c= X
proof
let q being set;
assume q in ( { x } + y );
then consider qq being (Point of T) such that A6: q = ( qq + y ) and A7: qq in { x };
{ qq } c= { x } by A7,ZFMISC_1:31;
hence thesis by A4,A5,A6,ZFMISC_1:18;
end;
hence thesis;
end;
