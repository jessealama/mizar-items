stylesheet "1.0";
output method="text";

tpl [/] {
  if [tstp] {
    apply [tstp];
  } else {
    msg terminate="yes" "Error: the tstp document element is missing.";
  }
}

tpl [tstp] {
  for-each [formula] {
    $n = `@name`;
    for-each [*[position() = 3]] {
      if [descendant::function[starts-with (@name, "a_")]] {
        $n;
      }
    }
  }
}
